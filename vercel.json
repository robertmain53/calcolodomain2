{
  "cleanUrls": true,
  "routes": [
    { "handle": "filesystem" },
    { "src": ".*\\.bak$", "status": 404 },
    { "src": ".*\\.(csv|xlsx)$", "status": 404 },
    { "src": "^/out(?:/.*)?$", "status": 404 },
    { "src": "^/tools(?:/.*)?$", "status": 404 },
    { "src": "^/reports(?:/.*)?$", "status": 404 },
    { "src": "^/\\.claude(?:/.*)?$", "status": 404 },
    { "src": "^/README\\.md$", "status": 404 },
    { "src": "^/sitemap_data_.*\\.json$", "status": 404 },
    { "src": "^/calculators-data\\.json$", "status": 404 },
    { "src": "^/.*\\.page\\..*\\.json$", "status": 404 }
  ]
}