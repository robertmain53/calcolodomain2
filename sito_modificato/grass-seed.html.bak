    <script>
      // Default seeding rates (lbs per 1000 sq ft) for new / overseed by grass type
      const defaultRates = {
        tall_fescue: { new: 7, overseed: 4, patch: 6 },
        kentucky_bluegrass: { new: 2.5, overseed: 1.5, patch: 2.5 },
        perennial_ryegrass: { new: 7, overseed: 4, patch: 6 },
        fine_fescue: { new: 4, overseed: 2.5, patch: 4 },
        bermudagrass: { new: 2, overseed: 1, patch: 2 },
        zoysia: { new: 2, overseed: 1, patch: 2 },
        mix_cool: { new: 5, overseed: 3, patch: 4.5 },
        mix_warm: { new: 3, overseed: 1.5, patch: 2.5 },
      };

      const unitSystemEl = document.getElementById("unit-system");
      const projectTypeEl = document.getElementById("project-type");
      const grassTypeEl = document.getElementById("grass-type");
      const areaEl = document.getElementById("area");
      const areaUnitEl = document.getElementById("area-unit");
      const rateEl = document.getElementById("rate");
      const rateUnitEl = document.getElementById("rate-unit");
      const coverageFactorEl = document.getElementById("coverage-factor");
      const wastageEl = document.getElementById("wastage");
      const rateHintEl = document.getElementById("rate-hint");
      const errorEl = document.getElementById("error-message");
      const resultsSectionEl = document.getElementById("results-section");
      const resultMainEl = document.getElementById("result-main");
      const resultSecondaryEl = document.getElementById("result-secondary");
      const resultRateSummaryEl = document.getElementById(
        "result-rate-summary"
      );
      const bagSuggestionsEl = document.getElementById("bag-suggestions");

      function updateRateUnitForSystem() {
        if (unitSystemEl.value === "imperial") {
          rateUnitEl.value = "lbs_per_1000_sqft";
          areaUnitEl.value = "sqft";
        } else {
          // metric
          rateUnitEl.value = "kg_per_100_sqm";
          areaUnitEl.value = "sqm";
        }
      }

      function setDefaultRate() {
        const grass = grassTypeEl.value;
        const project = projectTypeEl.value;

        if (grass === "custom") {
          rateHintEl.textContent =
            "Enter the seeding rate printed on your seed bag.";
          return;
        }

        const base = defaultRates[grass];
        if (!base) return;

        const lbsPer1000 = base[project] || base.new;

        if (unitSystemEl.value === "imperial") {
          rateUnitEl.value = "lbs_per_1000_sqft";
          rateEl.value = lbsPer1000.toFixed(2);
          rateHintEl.textContent = `Typical rate for ${
            grassTypeEl.options[grassTypeEl.selectedIndex].text
          } (${projectTypeEl.options[
            projectTypeEl.selectedIndex
          ].text.toLowerCase()}): about ${lbsPer1000.toFixed(
            1
          )} lbs per 1000 ft².`;
        } else {
          // Convert lbs/1000 ft² to kg/100 m²
          // 1 lb/1000 ft² ≈ 0.0488 kg/100 m²
          const kgPer100sqm = lbsPer1000 * 0.0488;
          rateUnitEl.value = "kg_per_100_sqm";
          rateEl.value = kgPer100sqm.toFixed(2);
          const gPerSqm = kgPer100sqm * 10; // 1 kg/100 m² = 10 g/m²
          rateHintEl.textContent = `Typical rate: ≈ ${kgPer100sqm.toFixed(
            2
          )} kg per 100 m² (≈ ${gPerSqm.toFixed(0)} g/m²).`;
        }
      }

      unitSystemEl.addEventListener("change", () => {
        updateRateUnitForSystem();
        setDefaultRate();
      });

      projectTypeEl.addEventListener("change", setDefaultRate);
      grassTypeEl.addEventListener("change", setDefaultRate);

      document
        .getElementById("toggle-dimension-helper")
        .addEventListener("click", () => {
          const helper = document.getElementById("dimension-helper");
          helper.classList.toggle("hidden");
        });

      document
        .getElementById("apply-dimensions")
        .addEventListener("click", () => {
          const length = parseFloat(document.getElementById("length").value);
          const width = parseFloat(document.getElementById("width").value);
          if (isNaN(length) || isNaN(width) || length <= 0 || width <= 0) {
            alert("Please enter valid positive values for length and width.");
            return;
          }
          const area = length * width;
          areaEl.value = area.toFixed(2);
        });

      function formatNumber(value, decimals = 2) {
        return Number(value).toLocaleString(undefined, {
          minimumFractionDigits: decimals,
          maximumFractionDigits: decimals,
        });
      }

      function suggestBags(totalLbs, totalKg) {
        bagSuggestionsEl.innerHTML = "";

        if (unitSystemEl.value === "imperial") {
          const bagSizes = [5, 10, 20, 25, 40, 50];
          const liExact = document.createElement("li");
          liExact.textContent = `Exact requirement: ${formatNumber(
            totalLbs,
            2
          )} lbs`;
          bagSuggestionsEl.appendChild(liExact);

          bagSizes.forEach((size) => {
            const bags = Math.ceil(totalLbs / size);
            if (bags <= 0) return;
            const li = document.createElement("li");
            li.textContent = `${bags} × ${size} lb bag${
              bags > 1 ? "s" : ""
            } (total ${formatNumber(bags * size, 1)} lbs)`;
            bagSuggestionsEl.appendChild(li);
          });
        } else {
          const bagSizes = [2, 5, 10, 20];
          const liExact = document.createElement("li");
          liExact.textContent = `Exact requirement: ${formatNumber(
            totalKg,
            2
          )} kg`;
          bagSuggestionsEl.appendChild(liExact);

          bagSizes.forEach((size) => {
            const bags = Math.ceil(totalKg / size);
            if (bags <= 0) return;
            const li = document.createElement("li");
            li.textContent = `${bags} × ${size} kg bag${
              bags > 1 ? "s" : ""
            } (total ${formatNumber(bags * size, 1)} kg)`;
            bagSuggestionsEl.appendChild(li);
          });
        }
      }

      document.getElementById("calculate-btn").addEventListener("click", () => {
        errorEl.classList.add("hidden");
        errorEl.textContent = "";
        resultsSectionEl.classList.add("hidden");

        const area = parseFloat(areaEl.value);
        const rate = parseFloat(rateEl.value);
        const coverageFactor = parseFloat(coverageFactorEl.value);
        const wastage = parseFloat(wastageEl.value);

        if (isNaN(area) || area <= 0) {
          errorEl.textContent =
            "Please enter a valid lawn area greater than zero.";
          errorEl.classList.remove("hidden");
          return;
        }
        if (isNaN(rate) || rate <= 0) {
          errorEl.textContent =
            "Please enter a valid seeding rate greater than zero.";
          errorEl.classList.remove("hidden");
          return;
        }

        let seedLbs = 0;
        let seedKg = 0;

        if (rateUnitEl.value === "lbs_per_1000_sqft") {
          const baseLbs = (area / 1000) * rate;
          seedLbs = baseLbs * coverageFactor * (1 + wastage);
          seedKg = seedLbs * 0.45359237;
        } else if (rateUnitEl.value === "kg_per_100_sqm") {
          const baseKg = (area / 100) * rate;
          seedKg = baseKg * coverageFactor * (1 + wastage);
          seedLbs = seedKg / 0.45359237;
        } else if (rateUnitEl.value === "g_per_sqm") {
          const baseKg = (area * rate) / 1000; // g/m² * m² / 1000 = kg
          seedKg = baseKg * coverageFactor * (1 + wastage);
          seedLbs = seedKg / 0.45359237;
        }

        if (unitSystemEl.value === "imperial") {
          resultMainEl.textContent = `${formatNumber(seedLbs, 2)} lbs of seed`;
          resultSecondaryEl.textContent = `≈ ${formatNumber(seedKg, 2)} kg`;
        } else {
          resultMainEl.textContent = `${formatNumber(seedKg, 2)} kg of seed`;
          resultSecondaryEl.textContent = `≈ ${formatNumber(seedLbs, 2)} lbs`;
        }

        let rateSummary = "";
        if (rateUnitEl.value === "lbs_per_1000_sqft") {
          rateSummary = `Based on ${formatNumber(
            rate,
            2
          )} lbs / 1000 ft², coverage factor ${coverageFactor} and ${Math.round(
            wastage * 100
          )}% extra.`;
        } else if (rateUnitEl.value === "kg_per_100_sqm") {
          rateSummary = `Based on ${formatNumber(
            rate,
            2
          )} kg / 100 m², coverage factor ${coverageFactor} and ${Math.round(
            wastage * 100
          )}% extra.`;
        } else {
          rateSummary = `Based on ${formatNumber(
            rate,
            2
          )} g / m², coverage factor ${coverageFactor} and ${Math.round(
            wastage * 100
          )}% extra.`;
        }
        resultRateSummaryEl.textContent = rateSummary;

        suggestBags(seedLbs, seedKg);
        resultsSectionEl.classList.remove("hidden");
      });

      document.getElementById("reset-btn").addEventListener("click", () => {
        unitSystemEl.value = "imperial";
        projectTypeEl.value = "new";
        grassTypeEl.value = "tall_fescue";
        areaEl.value = "";
        document.getElementById("length").value = "";
        document.getElementById("width").value = "";
        coverageFactorEl.value = "1";
        wastageEl.value = "0.1";
        updateRateUnitForSystem();
        setDefaultRate();
        errorEl.textContent = "";
        errorEl.classList.add("hidden");
        resultsSectionEl.classList.add("hidden");
      });

      // Initialize defaults on load
      updateRateUnitForSystem();
      setDefaultRate();
    </script>

