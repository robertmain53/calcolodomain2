    <script>
      // Utility: parse date safely
      function parseDate(value) {
        if (!value) return null;
        const d = new Date(value + "T00:00:00");
        return isNaN(d.getTime()) ? null : d;
      }

      function daysBetweenInclusive(start, end) {
        if (!start || !end) return 0;
        const ms = end.getTime() - start.getTime();
        return ms < 0 ? 0 : Math.floor(ms / 86400000) + 1;
      }

      function formatDays(n) {
        if (n === null || n === undefined || isNaN(n)) return "0 days";
        const v = Math.round(n);
        return v + (v === 1 ? " day" : " days");
      }

      // Project metrics
      const contractStartEl = document.getElementById("contract-start");
      const contractFinishEl = document.getElementById("contract-finish");
      const actualFinishEl = document.getElementById("actual-finish");
      const baselineDurationEl = document.getElementById("baseline-duration");
      const projectMetricsEl = document.getElementById("project-metrics");
      const totalProjectDelayEl = document.getElementById(
        "total-project-delay"
      );

      document
        .getElementById("calc-project-metrics")
        .addEventListener("click", () => {
          const cs = parseDate(contractStartEl.value);
          const cf = parseDate(contractFinishEl.value);
          const af = parseDate(actualFinishEl.value);

          if (!cs || !cf || !af) {
            projectMetricsEl.textContent =
              "Please enter contract start, contract completion, and actual completion dates.";
            projectMetricsEl.classList.remove("text-green-700");
            projectMetricsEl.classList.add("text-red-600");
            return;
          }

          let baseline = parseInt(baselineDurationEl.value, 10);
          if (!baseline || baseline <= 0) {
            baseline = daysBetweenInclusive(cs, cf);
          }
          const actualDuration = daysBetweenInclusive(cs, af);
          const delay = Math.max(0, actualDuration - baseline);

          projectMetricsEl.classList.remove("text-red-600");
          projectMetricsEl.classList.add("text-green-700");
          projectMetricsEl.textContent = `Baseline duration: ${baseline} days | Actual duration: ${actualDuration} days | Overall delay: ${delay} days.`;

          totalProjectDelayEl.textContent = formatDays(delay);
        });

      // Delay events table
      const delayTbody = document.getElementById("delay-tbody");
      let delayIdCounter = 1;

      const RESPONSIBLE_OPTIONS = [
        { value: "owner", label: "Owner" },
        { value: "contractor", label: "Contractor" },
        { value: "neutral", label: "Neutral / Force Majeure" },
      ];

      const TYPE_OPTIONS = [
        "Design / Approvals",
        "Change Order / Variation",
        "Access / Site Constraints",
        "Weather",
        "Labour / Resources",
        "Materials / Supply Chain",
        "Third Party / Authority",
        "Other",
      ];

      const METHOD_OPTIONS = [
        "Not specified",
        "Impacted As-Planned",
        "As-Planned vs. As-Built",
        "Time Impact Analysis",
        "Windows / Contemporaneous",
      ];

      function createSelect(options, className) {
        const sel = document.createElement("select");
        sel.className = className;
        options.forEach((opt) => {
          const o = document.createElement("option");
          if (typeof opt === "string") {
            o.value = opt;
            o.textContent = opt;
          } else {
            o.value = opt.value;
            o.textContent = opt.label;
          }
          sel.appendChild(o);
        });
        return sel;
      }

      function addDelayRow(prefill = {}) {
        const tr = document.createElement("tr");
        tr.dataset.id = delayIdCounter++;

        // Event name
        const tdName = document.createElement("td");
        tdName.className = "px-2 py-1 border-b align-top";
        const nameInput = document.createElement("input");
        nameInput.type = "text";
        nameInput.className =
          "w-32 md:w-40 border border-gray-300 rounded px-1 py-0.5 text-xs";
        nameInput.placeholder = "e.g. Late IFC drawings";
        nameInput.value = prefill.name || "";
        tdName.appendChild(nameInput);

        // Start
        const tdStart = document.createElement("td");
        tdStart.className = "px-2 py-1 border-b align-top";
        const startInput = document.createElement("input");
        startInput.type = "date";
        startInput.className =
          "border border-gray-300 rounded px-1 py-0.5 text-xs";
        startInput.value = prefill.start || "";
        tdStart.appendChild(startInput);

        // Finish
        const tdFinish = document.createElement("td");
        tdFinish.className = "px-2 py-1 border-b align-top";
        const finishInput = document.createElement("input");
        finishInput.type = "date";
        finishInput.className =
          "border border-gray-300 rounded px-1 py-0.5 text-xs";
        finishInput.value = prefill.finish || "";
        tdFinish.appendChild(finishInput);

        // Responsible
        const tdResp = document.createElement("td");
        tdResp.className = "px-2 py-1 border-b align-top";
        const respSelect = createSelect(
          RESPONSIBLE_OPTIONS,
          "border border-gray-300 rounded px-1 py-0.5 text-xs"
        );
        if (prefill.responsible) respSelect.value = prefill.responsible;
        tdResp.appendChild(respSelect);

        // Type
        const tdType = document.createElement("td");
        tdType.className = "px-2 py-1 border-b align-top";
        const typeSelect = createSelect(
          TYPE_OPTIONS,
          "border border-gray-300 rounded px-1 py-0.5 text-xs"
        );
        if (prefill.type) typeSelect.value = prefill.type;
        tdType.appendChild(typeSelect);

        // Critical?
        const tdCritical = document.createElement("td");
        tdCritical.className = "px-2 py-1 border-b align-top text-center";
        const criticalCheckbox = document.createElement("input");
        criticalCheckbox.type = "checkbox";
        criticalCheckbox.className = "h-4 w-4 text-blue-600";
        criticalCheckbox.checked = !!prefill.critical;
        tdCritical.appendChild(criticalCheckbox);

        // Method
        const tdMethod = document.createElement("td");
        tdMethod.className = "px-2 py-1 border-b align-top";
        const methodSelect = createSelect(
          METHOD_OPTIONS,
          "border border-gray-300 rounded px-1 py-0.5 text-xs"
        );
        if (prefill.method) methodSelect.value = prefill.method;
        tdMethod.appendChild(methodSelect);

        // Net delay
        const tdNet = document.createElement("td");
        tdNet.className = "px-2 py-1 border-b align-top text-right text-xs";
        tdNet.textContent = "0";

        // Classification
        const tdClass = document.createElement("td");
        tdClass.className = "px-2 py-1 border-b align-top text-xs";
        tdClass.textContent = "—";

        // Delete
        const tdDel = document.createElement("td");
        tdDel.className = "px-2 py-1 border-b align-top text-center";
        const delBtn = document.createElement("button");
        delBtn.type = "button";
        delBtn.className = "text-red-600 hover:text-red-800 text-xs";
        delBtn.textContent = "✕";
        delBtn.addEventListener("click", () => {
          tr.remove();
          recalcAnalysis();
        });
        tdDel.appendChild(delBtn);

        tr.appendChild(tdName);
        tr.appendChild(tdStart);
        tr.appendChild(tdFinish);
        tr.appendChild(tdResp);
        tr.appendChild(tdType);
        tr.appendChild(tdCritical);
        tr.appendChild(tdMethod);
        tr.appendChild(tdNet);
        tr.appendChild(tdClass);
        tr.appendChild(tdDel);

        delayTbody.appendChild(tr);
      }

      document.getElementById("add-delay").addEventListener("click", () => {
        addDelayRow();
      });

      document.getElementById("clear-delays").addEventListener("click", () => {
        delayTbody.innerHTML = "";
        recalcAnalysis();
      });

      document
        .getElementById("recalc-delays")
        .addEventListener("click", recalcAnalysis);

      function recalcAnalysis() {
        const cs = parseDate(contractStartEl.value);
        const cf = parseDate(actualFinishEl.value || contractFinishEl.value);
        const projectStart = cs;
        const projectEnd = cf;

        let ownerCritical = 0;
        let contractorCritical = 0;
        let neutralCritical = 0;
        let excusable = 0;
        let compensable = 0;
        let nonexcusable = 0;

        const events = [];

        delayTbody.querySelectorAll("tr").forEach((tr) => {
          const tds = tr.children;
          const name = tds[0].querySelector("input").value || "Delay event";
          const start = parseDate(tds[1].querySelector("input").value);
          const finish = parseDate(tds[2].querySelector("input").value);
          const responsible = tds[3].querySelector("select").value;
          const type = tds[4].querySelector("select").value;
          const critical = tds[5].querySelector("input").checked;
          const method = tds[6].querySelector("select").value;

          let net = 0;
          if (projectStart && projectEnd && start && finish) {
            const overlapStart = new Date(
              Math.max(projectStart.getTime(), start.getTime())
            );
            const overlapEnd = new Date(
              Math.min(projectEnd.getTime(), finish.getTime())
            );
            net = daysBetweenInclusive(overlapStart, overlapEnd);
          }

          let classification = "Not analysed";
          if (critical && net > 0) {
            if (responsible === "owner") {
              classification = "Compensable (owner-caused)";
              compensable += net;
              ownerCritical += net;
            } else if (responsible === "contractor") {
              classification = "Non-excusable (contractor-caused)";
              nonexcusable += net;
              contractorCritical += net;
            } else {
              classification = "Excusable (neutral / force majeure)";
              excusable += net;
              neutralCritical += net;
            }
          } else if (!critical && net > 0) {
            classification = "Non-critical (absorbed by float)";
          }

          tds[7].textContent = net.toString();
          tds[8].textContent = classification;

          events.push({
            name,
            start: start ? start.toISOString().slice(0, 10) : "",
            finish: finish ? finish.toISOString().slice(0, 10) : "",
            responsible,
            type,
            critical,
            method,
            net,
            classification,
          });
        });

        document.getElementById("owner-critical-delay").textContent =
          formatDays(ownerCritical);
        document.getElementById("contractor-critical-delay").textContent =
          formatDays(contractorCritical);
        document.getElementById("neutral-critical-delay").textContent =
          formatDays(neutralCritical);
        document.getElementById("excusable-delay").textContent =
          formatDays(excusable);
        document.getElementById("compensable-delay").textContent =
          formatDays(compensable);
        document.getElementById("nonexcusable-delay").textContent =
          formatDays(nonexcusable);

        updateNarrative(events);
      }

      function updateNarrative(events) {
        const cs = contractStartEl.value || "N/A";
        const cf = contractFinishEl.value || "N/A";
        const af = actualFinishEl.value || "N/A";
        const totalDelayText = totalProjectDelayEl.textContent || "0 days";

        let lines = [];
        lines.push(`Project delay analysis summary`);
        lines.push(`--------------------------------`);
        lines.push(`Contract start: ${cs}`);
        lines.push(`Contract completion: ${cf}`);
        lines.push(`Actual completion: ${af}`);
        lines.push(`Overall delay (indicative): ${totalDelayText}`);
        lines.push("");

        if (!events.length) {
          lines.push(
            "No delay events have been entered yet. Add events above to generate a detailed narrative."
          );
          document.getElementById("narrative-output").value = lines.join("\n");
          return;
        }

        lines.push("Key delay events:");
        events.forEach((e, idx) => {
          lines.push(
            `${idx + 1}. ${e.name} (${e.start} to ${e.finish}) – ` +
              `${e.net} day(s), ${
                e.critical ? "critical path" : "non-critical"
              }, ` +
              `${e.classification}.`
          );
        });

        lines.push("");
        lines.push("Methodology (high-level):");
        lines.push(
          "- The analysis is based on the contractual dates and the delay events recorded above. " +
            "Each event has been mapped to the project period and classified by responsibility and criticality."
        );
        lines.push(
          "- Net delay days per event represent the overlap between the event and the project period; " +
            "non-critical delays are treated as absorbed by available float."
        );
        lines.push(
          "- Classifications (excusable, compensable, non-excusable) are indicative only and should be " +
            "validated against the contract, governing law, and detailed CPM schedules."
        );

        document.getElementById("narrative-output").value = lines.join("\n");
      }

      // CSV export
      document.getElementById("export-csv").addEventListener("click", () => {
        const rows = [];
        rows.push([
          "Event Name",
          "Start Date",
          "Finish Date",
          "Responsible",
          "Type",
          "Critical Path?",
          "Method",
          "Net Delay (days)",
          "Classification",
        ]);

        delayTbody.querySelectorAll("tr").forEach((tr) => {
          const tds = tr.children;
          const name = tds[0].querySelector("input").value;
          const start = tds[1].querySelector("input").value;
          const finish = tds[2].querySelector("input").value;
          const responsible = tds[3].querySelector("select").value;
          const type = tds[4].querySelector("select").value;
          const critical = tds[5].querySelector("input").checked ? "Yes" : "No";
          const method = tds[6].querySelector("select").value;
          const net = tds[7].textContent;
          const classification = tds[8].textContent;

          rows.push([
            name,
            start,
            finish,
            responsible,
            type,
            critical,
            method,
            net,
            classification,
          ]);
        });

        if (rows.length === 1) {
          alert("No delay events to export.");
          return;
        }

        const csvContent = rows
          .map((r) =>
            r.map((v) => `"${(v || "").replace(/"/g, '""')}"`).join(",")
          )
          .join("\n");
        const blob = new Blob([csvContent], {
          type: "text/csv;charset=utf-8;",
        });
