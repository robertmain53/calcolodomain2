    <script>
      // Tab switching
      const tabEvents = document.getElementById("tab-events");
      const tabRisk = document.getElementById("tab-risk");
      const panelEvents = document.getElementById("panel-events");
      const panelRisk = document.getElementById("panel-risk");

      function activateTab(tab) {
        if (tab === "events") {
          tabEvents.classList.add("border-blue-600", "text-blue-600");
          tabEvents.classList.remove("border-transparent", "text-gray-600");
          tabRisk.classList.remove("border-blue-600", "text-blue-600");
          tabRisk.classList.add("border-transparent", "text-gray-600");
          panelEvents.classList.remove("hidden");
          panelRisk.classList.add("hidden");
        } else {
          tabRisk.classList.add("border-blue-600", "text-blue-600");
          tabRisk.classList.remove("border-transparent", "text-gray-600");
          tabEvents.classList.remove("border-blue-600", "text-blue-600");
          tabEvents.classList.add("border-transparent", "text-gray-600");
          panelRisk.classList.remove("hidden");
          panelEvents.classList.add("hidden");
        }
      }

      tabEvents.addEventListener("click", () => activateTab("events"));
      tabRisk.addEventListener("click", () => activateTab("risk"));

      // Risk mode switching
      const riskModeRadios = document.querySelectorAll(
        'input[name="risk-mode"]'
      );
      const riskInputsRisks = document.getElementById("risk-inputs-risks");
      const riskInputsAri = document.getElementById("risk-inputs-ari");

      riskModeRadios.forEach((r) => {
        r.addEventListener("change", () => {
          if (r.value === "risks" && r.checked) {
            riskInputsRisks.classList.remove("hidden");
            riskInputsAri.classList.add("hidden");
          } else if (r.value === "ari" && r.checked) {
            riskInputsAri.classList.remove("hidden");
            riskInputsRisks.classList.add("hidden");
          }
        });
      });

      // Utility functions
      function formatPercent(x) {
        if (!isFinite(x)) return "—";
        return (x * 100).toFixed(2) + "%";
      }

      function formatNNH(x) {
        if (!isFinite(x) || x <= 0) return "Not defined";
        return x.toFixed(2);
      }

      function ceilNNH(x) {
        if (!isFinite(x) || x <= 0) return "—";
        return Math.ceil(x);
      }

      // Z values for common alpha
      function zForAlpha(alpha) {
        const map = { "0.10": 1.6449, 0.05: 1.96, 0.01: 2.5758 };
        return map[alpha.toString()] || 1.96;
      }

      // Events mode calculation
      document.getElementById("calc-events").addEventListener("click", () => {
        const Et = parseFloat(document.getElementById("treat-events").value);
        const Nt = parseFloat(document.getElementById("treat-total").value);
        const Ec = parseFloat(document.getElementById("control-events").value);
        const Nc = parseFloat(document.getElementById("control-total").value);
        const alpha = parseFloat(document.getElementById("alpha").value);

        const resultsDiv = document.getElementById("results-events");
        const contentDiv = document.getElementById("results-events-content");
        contentDiv.innerHTML = "";

        if ([Et, Nt, Ec, Nc].some((v) => isNaN(v))) {
          contentDiv.innerHTML =
            '<p class="text-red-600">Please enter all four values.</p>';
          resultsDiv.classList.remove("hidden");
          return;
        }
        if (Nt <= 0 || Nc <= 0) {
          contentDiv.innerHTML =
            '<p class="text-red-600">Total patients must be greater than zero in both groups.</p>';
          resultsDiv.classList.remove("hidden");
          return;
        }
        if (Et < 0 || Ec < 0 || Et > Nt || Ec > Nc) {
          contentDiv.innerHTML =
            '<p class="text-red-600">Events must be between 0 and total patients in each group.</p>';
          resultsDiv.classList.remove("hidden");
          return;
        }

        const Rt = Et / Nt;
        const Rc = Ec / Nc;
        const ARI = Rt - Rc;

        let html = "";
        html += `<p><strong>Treatment risk (R<sub>T</sub>):</strong> ${formatPercent(
          Rt
        )} (${Et}/${Nt})</p>`;
        html += `<p><strong>Control risk (R<sub>C</sub>):</strong> ${formatPercent(
          Rc
        )} (${Ec}/${Nc})</p>`;
        html += `<p><strong>Absolute risk increase (ARI):</strong> ${formatPercent(
          ARI
        )}</p>`;

        if (Math.abs(ARI) < 1e-9) {
          html +=
            '<p class="text-amber-700 mt-2">The absolute risk increase is essentially zero, so NNH is not defined (no detectable difference in harm).</p>';
          contentDiv.innerHTML = html;
          resultsDiv.classList.remove("hidden");
          return;
        }

        const nnhExact = 1 / ARI;
        const nnhRounded = ceilNNH(Math.abs(nnhExact));

        if (ARI > 0) {
          html += `<p><strong>NNH (exact):</strong> ${formatNNH(
            nnhExact
          )} &nbsp; → &nbsp; <strong>NNH (rounded up):</strong> ${nnhRounded}</p>`;
          html += `<p class="text-gray-700">Interpretation: you would need to treat approximately <strong>${nnhRounded} patients</strong> for one additional adverse event to occur compared with control.</p>`;
        } else {
          const nntExact = 1 / Math.abs(ARI);
          const nntRounded = ceilNNH(nntExact);
          html += `<p class="text-amber-700"><strong>Note:</strong> ARI is negative, indicating a reduction in harm. This corresponds to a benefit rather than harm.</p>`;
          html += `<p><strong>NNT (for benefit, exact):</strong> ${nntExact.toFixed(
            2
          )} &nbsp; → &nbsp; <strong>NNT (rounded up):</strong> ${nntRounded}</p>`;
          html += `<p class="text-gray-700">Interpretation: you would need to treat approximately <strong>${nntRounded} patients</strong> to prevent one adverse event compared with control.</p>`;
        }

        // Confidence interval for ARI and NNH
        if (ARI !== 0) {
          const z = zForAlpha(alpha);
          const se = Math.sqrt((Rt * (1 - Rt)) / Nt + (Rc * (1 - Rc)) / Nc);
          const ariLow = ARI - z * se;
          const ariHigh = ARI + z * se;

          html += '<hr class="my-3">';
          html += `<p class="font-semibold">Approximate ${Math.round(
            (1 - alpha) * 100
          )}% confidence interval</p>`;
          html += `<p><strong>ARI CI:</strong> ${formatPercent(
            ariLow
          )} to ${formatPercent(ariHigh)}</p>`;

          if (ariLow <= 0 && ariHigh >= 0) {
            html += `<p class="text-amber-700 text-sm">The confidence interval for ARI crosses zero, so the corresponding NNH confidence interval is not well defined and may include both harm and benefit.</p>`;
          } else {
            const nnhLow = 1 / ariHigh;
            const nnhHigh = 1 / ariLow;
            html += `<p><strong>NNH CI (approx.):</strong> ${ceilNNH(
              Math.abs(nnhLow)
            )} to ${ceilNNH(Math.abs(nnhHigh))}</p>`;
          }
        }

        contentDiv.innerHTML = html;
        resultsDiv.classList.remove("hidden");
      });

      document.getElementById("reset-events").addEventListener("click", () => {
        [
          "treat-events",
          "treat-total",
          "control-events",
          "control-total",
        ].forEach((id) => {
          document.getElementById(id).value = "";
        });
        document.getElementById("results-events").classList.add("hidden");
        document.getElementById("results-events-content").innerHTML = "";
      });

      // Risk / ARI mode calculation
      document.getElementById("calc-risk").addEventListener("click", () => {
        const mode =
          Array.from(riskModeRadios).find((r) => r.checked)?.value || "risks";
        const resultsDiv = document.getElementById("results-risk");
        const contentDiv = document.getElementById("results-risk-content");
        contentDiv.innerHTML = "";

        let ARI;

        if (mode === "risks") {
          const RtPct = parseFloat(document.getElementById("risk-treat").value);
          const RcPct = parseFloat(
            document.getElementById("risk-control").value
          );

          if ([RtPct, RcPct].some((v) => isNaN(v))) {
            contentDiv.innerHTML =
              '<p class="text-red-600">Please enter both treatment and control risks.</p>';
            resultsDiv.classList.remove("hidden");
            return;
          }
          if (RtPct < 0 || RtPct > 100 || RcPct < 0 || RcPct > 100) {
            contentDiv.innerHTML =
              '<p class="text-red-600">Risks must be between 0% and 100%.</p>';
            resultsDiv.classList.remove("hidden");
            return;
          }

          const Rt = RtPct / 100;
          const Rc = RcPct / 100;
          ARI = Rt - Rc;

          let html = "";
          html += `<p><strong>Treatment risk (R<sub>T</sub>):</strong> ${formatPercent(
            Rt
          )}</p>`;
          html += `<p><strong>Control risk (R<sub>C</sub>):</strong> ${formatPercent(
            Rc
          )}</p>`;
          html += `<p><strong>Absolute risk increase (ARI):</strong> ${formatPercent(
            ARI
          )}</p>`;

          if (Math.abs(ARI) < 1e-9) {
            html +=
              '<p class="text-amber-700 mt-2">The absolute risk increase is essentially zero, so NNH is not defined.</p>';
            contentDiv.innerHTML = html;
            resultsDiv.classList.remove("hidden");
            return;
          }

          const nnhExact = 1 / ARI;
          const nnhRounded = ceilNNH(Math.abs(nnhExact));

          if (ARI > 0) {
            html += `<p><strong>NNH (exact):</strong> ${formatNNH(
              nnhExact
            )} &nbsp; → &nbsp; <strong>NNH (rounded up):</strong> ${nnhRounded}</p>`;
            html += `<p class="text-gray-700">Interpretation: about <strong>${nnhRounded} patients</strong> must be exposed for one additional adverse event to occur.</p>`;
          } else {
            const nntExact = 1 / Math.abs(ARI);
            const nntRounded = ceilNNH(nntExact);
            html += `<p class="text-amber-700"><strong>Note:</strong> ARI is negative, indicating reduced risk. Interpret this as NNT for benefit.</p>`;
            html += `<p><strong>NNT (exact):</strong> ${nntExact.toFixed(
              2
            )} &nbsp; → &nbsp; <strong>NNT (rounded up):</strong> ${nntRounded}</p>`;
          }

          contentDiv.innerHTML = html;
          resultsDiv.classList.remove("hidden");
        } else {
          const ariInput = parseFloat(
            document.getElementById("ari-input").value
          );
          if (isNaN(ariInput)) {
            contentDiv.innerHTML =
              '<p class="text-red-600">Please enter an absolute risk increase (ARI).</p>';
            resultsDiv.classList.remove("hidden");
            return;
          }
          ARI = ariInput;

          let html = "";
          html += `<p><strong>Absolute risk increase (ARI):</strong> ${ARI.toFixed(
            4
          )} (${formatPercent(ARI)})</p>`;

          if (Math.abs(ARI) < 1e-9) {
            html +=
              '<p class="text-amber-700 mt-2">The absolute risk increase is essentially zero, so NNH is not defined.</p>';
            contentDiv.innerHTML = html;
            resultsDiv.classList.remove("hidden");
            return;
          }

          const nnhExact = 1 / ARI;
          const nnhRounded = ceilNNH(Math.abs(nnhExact));

          if (ARI > 0) {
            html += `<p><strong>NNH (exact):</strong> ${formatNNH(
              nnhExact
            )} &nbsp; → &nbsp; <strong>NNH (rounded up):</strong> ${nnhRounded}</p>`;
            html += `<p class="text-gray-700">Interpretation: about <strong>${nnhRounded} patients</strong> must be exposed for one additional adverse event to occur.</p>`;
          } else {
            const nntExact = 1 / Math.abs(ARI);
            const nntRounded = ceilNNH(nntExact);
            html += `<p class="text-amber-700"><strong>Note:</strong> ARI is negative, indicating reduced risk. Interpret this as NNT for benefit.</p>`;
            html += `<p><strong>NNT (exact):</strong> ${nntExact.toFixed(
              2
            )} &nbsp; → &nbsp; <strong>NNT (rounded up):</strong> ${nntRounded}</p>`;
          }

          contentDiv.innerHTML = html;
          resultsDiv.classList.remove("hidden");
        }
      });

      document.getElementById("reset-risk").addEventListener("click", () => {
        ["risk-treat", "risk-control", "ari-input"].forEach((id) => {
          document.getElementById(id).value = "";
        });
        document.querySelector(
          'input[name="risk-mode"][value="risks"]'
        ).checked = true;
        riskInputsRisks.classList.remove("hidden");
        riskInputsAri.classList.add("hidden");
        document.getElementById("results-risk").classList.add("hidden");
        document.getElementById("results-risk-content").innerHTML = "";
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
