    <script>
      // --- Maclaurin Series Calculator Logic ---

      function factorial(n) {
        if (n < 0) return NaN;
        let res = 1;
        for (let i = 2; i <= n; i++) res *= i;
        return res;
      }

      // Predefined derivatives at 0 for supported functions
      // Returns array [f(0), f'(0), ..., f^{(n)}(0)]
      function derivativesAtZero(funcKey, n) {
        const arr = [];
        switch (funcKey) {
          case "exp":
            for (let k = 0; k <= n; k++) arr.push(1);
            break;
          case "sin":
            // pattern: 0,1,0,-1,0,1,...
            const sinPattern = [0, 1, 0, -1];
            for (let k = 0; k <= n; k++) arr.push(sinPattern[k % 4]);
            break;
          case "cos":
            // pattern: 1,0,-1,0,1,...
            const cosPattern = [1, 0, -1, 0];
            for (let k = 0; k <= n; k++) arr.push(cosPattern[k % 4]);
            break;
          case "ln1px":
            // f(x) = ln(1+x); f^(k)(0) = (-1)^{k-1} (k-1)! for k>=1, f(0)=0
            arr.push(0);
            for (let k = 1; k <= n; k++) {
              const sign = k % 2 === 1 ? 1 : -1; // (-1)^{k-1}
              arr.push(sign * factorial(k - 1));
            }
            break;
          case "arctan":
            // f(x) = arctan x; odd derivatives nonzero, pattern from series
            // f^(2m)(0) = 0; f^(2m+1)(0) = (-1)^m * (2m)! / (2m+1)
            for (let k = 0; k <= n; k++) {
              if (k % 2 === 0) {
                arr.push(0);
              } else {
                const m = (k - 1) / 2;
                const sign = m % 2 === 0 ? 1 : -1;
                arr.push((sign * factorial(2 * m)) / (2 * m + 1));
              }
            }
            break;
          case "sinh":
            // pattern like sin but with hyperbolic: sinh, cosh, sinh, cosh...
            // derivatives at 0: 0,1,0,1,0,1,...
            for (let k = 0; k <= n; k++) {
              if (k === 0) arr.push(0);
              else if (k % 2 === 1) arr.push(1);
              else arr.push(0);
            }
            break;
          case "cosh":
            // derivatives at 0: 1,0,1,0,1,0,...
            for (let k = 0; k <= n; k++) {
              if (k % 2 === 0) arr.push(1);
              else arr.push(0);
            }
            break;
          default:
            for (let k = 0; k <= n; k++) arr.push(0);
        }
        return arr;
      }

      // Evaluate base function numerically
      function evalFunction(funcKey, x) {
        switch (funcKey) {
          case "exp":
            return Math.exp(x);
          case "sin":
            return Math.sin(x);
          case "cos":
            return Math.cos(x);
          case "ln1px":
            return x > -1 ? Math.log(1 + x) : NaN;
          case "arctan":
            return Math.atan(x);
          case "sinh":
            return Math.sinh(x);
          case "cosh":
            return Math.cosh(x);
          default:
            return NaN;
        }
      }

      // Build human-readable polynomial string from coefficients a_k where term = a_k * x^k
      function polynomialToString(coeffs) {
        const terms = [];
        for (let k = 0; k < coeffs.length; k++) {
          const c = coeffs[k];
          if (!isFinite(c) || Math.abs(c) < 1e-14) continue;

          let sign = c >= 0 ? "+" : "−";
          let absC = Math.abs(c);

          let coeffStr = "";
          if (k === 0) {
            coeffStr = absC.toFixed(6).replace(/\.?0+$/, "");
          } else {
            if (Math.abs(absC - 1) < 1e-12) {
              coeffStr = "";
            } else {
              coeffStr = absC.toFixed(6).replace(/\.?0+$/, "");
            }
          }

          let xPart = "";
          if (k === 1) xPart = "x";
          else if (k > 1) xPart = "x^" + k;

          let term = "";
          if (coeffStr && xPart) term = coeffStr + " " + xPart;
          else if (!coeffStr && xPart) term = xPart;
          else term = coeffStr;

          terms.push({ sign, term });
        }

        if (terms.length === 0) return "0";

        let result = "";
        terms.forEach((t, idx) => {
          if (idx === 0) {
            result += (t.sign === "−" ? "− " : "") + t.term;
          } else {
            result += " " + t.sign + " " + t.term;
          }
        });
        return result;
      }

      function updateX0Label() {
        const x0 = document.getElementById("x0-input").value;
        document.getElementById("x0-label").textContent = x0;
      }

      document.addEventListener("DOMContentLoaded", function () {
        const funcSelect = document.getElementById("function-select");
        const customWrapper = document.getElementById(
          "custom-derivatives-wrapper"
        );
        const customInput = document.getElementById("custom-derivatives");
        const orderInput = document.getElementById("order-input");
        const x0Input = document.getElementById("x0-input");
        const computeBtn = document.getElementById("compute-btn");
        const resetBtn = document.getElementById("reset-btn");
        const errorBox = document.getElementById("error-box");

        const seriesOutput = document.getElementById("series-output");
        const orderLabel = document.getElementById("order-label");
        const fxValue = document.getElementById("fx-value");
        const pnValue = document.getElementById("pn-value");
        const absError = document.getElementById("abs-error");
        const relError = document.getElementById("rel-error");

        funcSelect.addEventListener("change", () => {
          if (funcSelect.value === "custom") {
            customWrapper.classList.remove("hidden");
          } else {
            customWrapper.classList.add("hidden");
          }
        });

        x0Input.addEventListener("input", updateX0Label);

        resetBtn.addEventListener("click", () => {
          funcSelect.value = "exp";
          customWrapper.classList.add("hidden");
          customInput.value = "";
          orderInput.value = 5;
          x0Input.value = 0.5;
          updateX0Label();
          errorBox.classList.add("hidden");
          seriesOutput.textContent =
            "Select a function and click “Compute Maclaurin Series”.";
          orderLabel.textContent = "(n)";
          fxValue.textContent = "–";
          pnValue.textContent = "–";
          absError.textContent = "–";
          relError.textContent = "–";
          if (window.MathJax && window.MathJax.typesetPromise) {
            MathJax.typesetPromise();
          }
        });

        computeBtn.addEventListener("click", () => {
          errorBox.classList.add("hidden");
          const funcKey = funcSelect.value;
          const n = parseInt(orderInput.value, 10);
          const x0 = parseFloat(x0Input.value);

          if (!Number.isInteger(n) || n < 0 || n > 20) {
            errorBox.textContent =
              "Order n must be an integer between 0 and 20.";
            errorBox.classList.remove("hidden");
            return;
          }
          if (!isFinite(x0)) {
            errorBox.textContent = "Please enter a valid numeric value for x.";
            errorBox.classList.remove("hidden");
            return;
          }

          let derivs;
          if (funcKey === "custom") {
            const raw = customInput.value.trim();
            if (!raw) {
              errorBox.textContent =
                "For custom functions, please enter at least one derivative value at 0.";
              errorBox.classList.remove("hidden");
              return;
            }
            const parts = raw
              .split(",")
              .map((s) => s.trim())
              .filter((s) => s.length > 0);
            derivs = parts.map(Number);
            if (derivs.some((v) => !isFinite(v))) {
              errorBox.textContent =
                "Custom derivatives must be comma-separated real numbers.";
              errorBox.classList.remove("hidden");
              return;
            }
            if (derivs.length <= n) {
              errorBox.textContent = `You entered only ${
                derivs.length
              } derivative(s). Please provide at least n+1 = ${
                n + 1
              } values or reduce the order.`;
              errorBox.classList.remove("hidden");
              return;
            }
          } else {
            derivs = derivativesAtZero(funcKey, n);
          }

          // Build coefficients a_k = f^{(k)}(0) / k!
          const coeffs = [];
          for (let k = 0; k <= n; k++) {
            coeffs.push(derivs[k] / factorial(k));
          }

          const polyStr = polynomialToString(coeffs);
          orderLabel.textContent = "(" + n + ")";
          seriesOutput.innerHTML =
            '<span class="font-mono">P_' + n + "(x)</span> = " + polyStr;

          // Numeric evaluation
          let fx = NaN;
          if (funcKey !== "custom") {
            fx = evalFunction(funcKey, x0);
          }
          let pn = 0;
          let xPow = 1;
          for (let k = 0; k <= n; k++) {
            pn += coeffs[k] * xPow;
            xPow *= x0;
          }

          if (isFinite(fx)) {
            fxValue.textContent = fx.toPrecision(10);
            pnValue.textContent = pn.toPrecision(10);
            const err = Math.abs(fx - pn);
            absError.textContent = err.toExponential(4);
            if (Math.abs(fx) > 1e-14) {
              relError.textContent = (err / Math.abs(fx)).toExponential(4);
            } else {
              relError.textContent = "n/a";
            }
          } else {
            fxValue.textContent = "n/a (not defined or custom function)";
            pnValue.textContent = pn.toPrecision(10);
            absError.textContent = "n/a";
            relError.textContent = "n/a";
          }

          if (window.MathJax && window.MathJax.typesetPromise) {
            MathJax.typesetPromise();
          }
        });

        updateX0Label();
      });
    </script>
  
