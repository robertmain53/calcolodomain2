    <script>
      // Core calculator logic
      function formatRatio(value) {
        if (!isFinite(value)) return "–";
        if (value === 0) return "0.0x";
        const abs = Math.abs(value);
        if (abs >= 1000) return value.toFixed(1) + "x";
        if (abs >= 100) return value.toFixed(1) + "x";
        if (abs >= 10) return value.toFixed(2) + "x";
        return value.toFixed(2) + "x";
      }

      function interpretCoverage(ratio) {
        if (!isFinite(ratio))
          return {
            label: "N/A",
            band: "No meaningful interpretation (check inputs).",
          };
        if (ratio < 0)
          return {
            label: "Negative coverage (distress)",
            band: "EBIT/EBITDA is negative while interest is positive. Very high default risk.",
          };
        if (ratio === 0)
          return {
            label: "No coverage",
            band: "No earnings available to cover interest expense.",
          };
        if (ratio < 1)
          return {
            label: "< 1x (unsustainable)",
            band: "Earnings do not fully cover interest. Typically unsustainable without external support.",
          };
        if (ratio < 1.5)
          return {
            label: "1.0x–1.5x (very weak)",
            band: "Very thin cushion. Highly vulnerable to downturns or rate increases.",
          };
        if (ratio < 3)
          return {
            label: "1.5x–3.0x (moderate)",
            band: "Acceptable only for stable, low‑volatility businesses. Risky for cyclical firms.",
          };
        if (ratio < 5)
          return {
            label: "3.0x–5.0x (comfortable)",
            band: "Generally healthy coverage for many investment‑grade borrowers.",
          };
        return {
          label: "5.0x+ (strong)",
          band: "Very strong coverage. Conservative leverage or unusually high profitability.",
        };
      }

      function getSelectedCoverageType() {
        const radios = document.querySelectorAll(".coverage-type");
        for (const r of radios) {
          if (r.checked) return r.value;
        }
        return "ebit";
      }

      function getPeriodLabel() {
        const sel = document.getElementById("period-select");
        if (!sel) return "";
        const v = sel.value;
        if (v === "annual") return "Annual figures";
        if (v === "ttm") return "Trailing 12 months (TTM)";
        if (v === "quarter") return "Quarterly figures";
        if (v === "custom") return "Custom period";
        return "";
      }

      function calculateCoverage() {
        const ebitInput = document.getElementById("ebit-input");
        const interestInput = document.getElementById("interest-input");
        const errorEl = document.getElementById("input-error");
        const resultsCard = document.getElementById("results-card");
        const coverageOut = document.getElementById("coverage-output");
        const coverageMultiple = document.getElementById("coverage-multiple");
        const coverageInterp = document.getElementById(
          "coverage-interpretation"
        );
        const coverageTraffic = document.getElementById("coverage-traffic");
        const coverageDef = document.getElementById("coverage-definition");
        const coveragePeriod = document.getElementById("coverage-period");
        const lowInterestWarn = document.getElementById("warning-low-interest");

        const ebit = parseFloat(ebitInput.value);
        const interest = parseFloat(interestInput.value);

        if (isNaN(ebit) || isNaN(interest) || interest <= 0) {
          errorEl.classList.remove("hidden");
          resultsCard.classList.add("hidden");
          return;
        }
        errorEl.classList.add("hidden");

        const ratio = ebit / interest;
        const formatted = formatRatio(ratio);
        const interp = interpretCoverage(ratio);

        coverageOut.textContent = formatted;
        coverageMultiple.textContent = "times interest covered";
        coverageInterp.textContent = interp.label;
        coverageTraffic.textContent = interp.band;

        const type = getSelectedCoverageType();
        coverageDef.textContent =
          type === "ebit"
            ? "EBIT / Interest expense"
            : "EBITDA / Interest expense";
        coveragePeriod.textContent = getPeriodLabel();

        if (interest > 0 && Math.abs(ebit / interest) > 20) {
          lowInterestWarn.classList.remove("hidden");
        } else {
          lowInterestWarn.classList.add("hidden");
        }

        resultsCard.classList.remove("hidden");
      }

      function runScenario() {
        const baseEbit = parseFloat(
          document.getElementById("ebit-input").value
        );
        const baseInterest = parseFloat(
          document.getElementById("interest-input").value
        );
        const deltaEbit = parseFloat(
          document.getElementById("delta-ebit").value
        );
        const deltaInterest = parseFloat(
          document.getElementById("delta-interest").value
        );
        const scenarioBox = document.getElementById("scenario-results");
        const scenarioText = document.getElementById("scenario-text");
        const scenarioInterp = document.getElementById(
          "scenario-interpretation"
        );

        if (
          !isFinite(baseEbit) ||
          !isFinite(baseInterest) ||
          baseInterest <= 0
        ) {
          scenarioBox.classList.remove("hidden");
          scenarioText.textContent =
            "Enter valid base EBIT/EBITDA and interest expense above before running a scenario.";
          scenarioInterp.textContent = "";
          return;
        }

        const ebitFactor = isNaN(deltaEbit) ? 0 : deltaEbit / 100;
        const intFactor = isNaN(deltaInterest) ? 0 : deltaInterest / 100;

        const newEbit = baseEbit * (1 + ebitFactor);
        const newInterest = baseInterest * (1 + intFactor);

        if (!isFinite(newEbit) || !isFinite(newInterest) || newInterest <= 0) {
          scenarioBox.classList.remove("hidden");
          scenarioText.textContent =
            "Scenario inputs lead to invalid values. Please adjust the percentage changes.";
          scenarioInterp.textContent = "";
          return;
        }

        const newRatio = newEbit / newInterest;
        const formatted = formatRatio(newRatio);
        const interp = interpretCoverage(newRatio);

        scenarioBox.classList.remove("hidden");
        scenarioText.textContent = `With EBIT/EBITDA changing by ${
          isNaN(deltaEbit) ? 0 : deltaEbit
        }% and interest expense changing by ${
          isNaN(deltaInterest) ? 0 : deltaInterest
        }%, the interest coverage ratio would be approximately ${formatted}.`;
        scenarioInterp.textContent = interp.label + " – " + interp.band;
      }

      document.addEventListener("DOMContentLoaded", function () {
        const calcBtn = document.getElementById("calculate-btn");
        const resetBtn = document.getElementById("reset-btn");
        const scenarioBtn = document.getElementById("scenario-btn");

        if (calcBtn) calcBtn.addEventListener("click", calculateCoverage);
        if (resetBtn)
          resetBtn.addEventListener("click", function () {
            document.getElementById("ebit-input").value = "";
            document.getElementById("interest-input").value = "";
            document.getElementById("delta-ebit").value = "";
            document.getElementById("delta-interest").value = "";
            document.getElementById("results-card").classList.add("hidden");
            document.getElementById("scenario-results").classList.add("hidden");
            document.getElementById("input-error").classList.add("hidden");
            document
              .getElementById("warning-low-interest")
              .classList.add("hidden");
          });
        if (scenarioBtn) scenarioBtn.addEventListener("click", runScenario);
      });

      // Mobile menu script (from template)
      document.addEventListener("DOMContentLoaded", function () {
        const toggle = document.getElementById("mobile-menu-toggle");
        const menu = document.getElementById("mobile-menu");
        if (toggle && menu) {
          toggle.addEventListener("click", () => {
            const isHidden = menu.classList.contains("hidden");
            menu.classList.toggle("hidden", !isHidden);
            toggle.setAttribute("aria-expanded", String(isHidden));
          });
        }
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
