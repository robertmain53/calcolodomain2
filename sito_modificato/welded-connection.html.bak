    <script>
      // Welded connection calculator logic
      function formatNumber(val, decimals = 2) {
        if (!isFinite(val)) return "–";
        return Number(val).toFixed(decimals);
      }

      function calculateWeld() {
        const designMethod = document.getElementById("designMethod").value;
        const weldType = document.getElementById("weldType").value;
        const weldConfig = document.getElementById("weldConfig").value;

        const FEXX = parseFloat(document.getElementById("fexx").value);
        const FuBase = parseFloat(document.getElementById("fuBase").value);
        const w = parseFloat(document.getElementById("weldSize").value);
        const L = parseFloat(document.getElementById("weldLength").value);
        const Vu = parseFloat(document.getElementById("Vu").value);
        const Tu = parseFloat(document.getElementById("Tu").value);
        const Mu = parseFloat(document.getElementById("Mu").value);
        const phiOmega = parseFloat(document.getElementById("phiOmega").value);
        const Cw = parseFloat(document.getElementById("Cw").value);

        const statusText = document.getElementById("statusText");
        const utilBadge = document.getElementById("utilizationBadge");
        const stepDetails = document.getElementById("stepDetails");

        // Basic validation
        if (
          [FEXX, FuBase, w, L, phiOmega, Cw].some((v) => !isFinite(v) || v <= 0)
        ) {
          statusText.textContent =
            "Please enter positive values for weld properties and factors.";
          utilBadge.textContent = "–";
          utilBadge.className = "badge bg-red-100 text-red-700";
          return;
        }

        // 1. Effective throat
        let te;
        if (weldType === "fillet") {
          te = 0.707 * w;
        } else {
          // Groove weld: approximate throat as weld size (user should input equivalent)
          te = w;
        }

        // 2. Effective length
        const Leff = weldConfig === "double" ? 2 * L : L;

        // 3. Effective area (in^2)
        const Aw = te * Leff;

        // 4. Nominal weld strength
        const Fnw = Cw * Math.min(FEXX, FuBase); // ksi
        const Rn = (Fnw * Aw) / 1.0; // kips (since ksi * in^2)

        // 5. Design strength
        let Rd;
        if (designMethod === "LRFD") {
          Rd = phiOmega * Rn;
        } else {
          Rd = Rn / phiOmega;
        }

        // 6. Demand
        const Vu_k = isFinite(Vu) ? Vu : 0;
        const Tu_k = isFinite(Tu) ? Tu : 0;
        const Mu_kipin = isFinite(Mu) ? Mu : 0;

        let FM = 0;
        if (Mu_kipin !== 0 && Leff > 0) {
          FM = (2 * Mu_kipin) / Leff; // kip
        }

        const Ru = Math.sqrt(Math.pow(Vu_k, 2) + Math.pow(Tu_k + FM, 2));

        // 7. Utilization
        const U = Rd > 0 ? Ru / Rd : NaN;

        // Update results
        document.getElementById("resThroat").textContent = formatNumber(te, 3);
        document.getElementById("resLeff").textContent = formatNumber(Leff, 2);
        document.getElementById("resRn").textContent = formatNumber(Rn, 2);
        document.getElementById("resRd").textContent = formatNumber(Rd, 2);
        document.getElementById("resRu").textContent = formatNumber(Ru, 2);

        if (!isFinite(U)) {
          statusText.textContent =
            "Unable to compute utilization. Check your inputs.";
          utilBadge.textContent = "–";
          utilBadge.className = "badge bg-red-100 text-red-700";
          return;
        }

        const Upercent = U * 100;
        utilBadge.textContent = formatNumber(Upercent, 1) + "%";

        if (U <= 1.0) {
          statusText.textContent =
            "Weld PASSES strength check for the given loads.";
          utilBadge.className = "badge bg-green-100 text-green-700";
        } else {
          statusText.textContent =
            "Weld FAILS strength check. Increase weld size or length.";
          utilBadge.className = "badge bg-red-100 text-red-700";
        }

        // Step-by-step explanation
        const lines = [];
        lines.push(
          `1. Effective throat t_e = ${
            weldType === "fillet" ? "0.707 × w" : "w (assumed)"
          } = ${formatNumber(te, 3)} in.`
        );
        lines.push(
          `2. Effective length L_eff = ${
            weldConfig === "double" ? "2 × L" : "L"
          } = ${formatNumber(Leff, 2)} in.`
        );
        lines.push(
          `3. Effective area A_w = t_e × L_eff = ${formatNumber(Aw, 3)} in².`
        );
        lines.push(
          `4. Nominal weld strength F_nw = C_w × min(F_EXX, F_u) = ${formatNumber(
            Cw,
            2
          )} × min(${formatNumber(FEXX, 1)}, ${formatNumber(
            FuBase,
            1
          )}) = ${formatNumber(Fnw, 2)} ksi.`
        );
        lines.push(
          `   ⇒ R_n = F_nw × A_w = ${formatNumber(Fnw, 2)} × ${formatNumber(
            Aw,
            3
          )} = ${formatNumber(Rn, 2)} kips.`
        );
        if (designMethod === "LRFD") {
          lines.push(
            `5. Design strength (LRFD): φR_n = ${formatNumber(
              phiOmega,
              2
            )} × ${formatNumber(Rn, 2)} = ${formatNumber(Rd, 2)} kips.`
          );
        } else {
          lines.push(
            `5. Design strength (ASD): R_n / Ω = ${formatNumber(
              Rn,
              2
            )} / ${formatNumber(phiOmega, 2)} = ${formatNumber(Rd, 2)} kips.`
          );
        }
        lines.push(
          `6. Applied loads: V_u = ${formatNumber(
            Vu_k,
            2
          )} kips, T_u = ${formatNumber(Tu_k, 2)} kips, M_u = ${formatNumber(
            Mu_kipin,
            2
          )} kip-in.`
        );
        if (Mu_kipin !== 0) {
          lines.push(
            `   Moment-induced force at extreme fiber F_M = 2 M_u / L_eff = 2 × ${formatNumber(
              Mu_kipin,
              2
            )} / ${formatNumber(Leff, 2)} = ${formatNumber(FM, 2)} kips.`
          );
        } else {
          lines.push("   No eccentric moment specified ⇒ F_M = 0 kips.");
        }
        lines.push(
          `7. Resultant demand R_u = √(V_u² + (T_u + F_M)²) = ${formatNumber(
            Ru,
            2
          )} kips.`
        );
        lines.push(
          `8. Utilization U = R_u / R_d = ${formatNumber(
            Ru,
            2
          )} / ${formatNumber(Rd, 2)} = ${formatNumber(U, 2)} (${formatNumber(
            Upercent,
            1
          )}%).`
        );

        stepDetails.innerHTML = lines.map((l) => `<p>${l}</p>`).join("");
      }

      document
        .getElementById("calcBtn")
        .addEventListener("click", calculateWeld);
    </script>

