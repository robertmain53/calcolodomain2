    <script>
      // Tab switching
      const tabBuilding = document.getElementById("tab-building");
      const tabBridge = document.getElementById("tab-bridge");
      const panelBuilding = document.getElementById("panel-building");
      const panelBridge = document.getElementById("panel-bridge");

      function setTab(active) {
        if (active === "building") {
          panelBuilding.classList.remove("hidden");
          panelBridge.classList.add("hidden");
          tabBuilding.classList.add("tab-active");
          tabBridge.classList.remove("tab-active");
        } else {
          panelBuilding.classList.add("hidden");
          panelBridge.classList.remove("hidden");
          tabBuilding.classList.remove("tab-active");
          tabBridge.classList.add("tab-active");
        }
      }

      tabBuilding.addEventListener("click", () => setTab("building"));
      tabBridge.addEventListener("click", () => setTab("bridge"));

      // Helpers
      function getAlpha(material) {
        switch (material) {
          case "steel":
            return 12e-6;
          case "concrete":
            return 10e-6;
          case "composite":
            return 11e-6;
          default:
            return 10e-6;
        }
      }

      function formatSci(x) {
        if (!isFinite(x)) return "–";
        const absx = Math.abs(x);
        if (absx !== 0 && (absx < 1e-5 || absx > 1e3)) {
          return x.toExponential(3);
        }
        return x.toFixed(6);
      }

      // Building calculations
      function calcBuilding(design) {
        const h = parseFloat(document.getElementById("b-thickness").value) || 0;
        let alpha = parseFloat(document.getElementById("b-alpha").value);
        const material = document.getElementById("b-material").value;
        if (isNaN(alpha)) alpha = getAlpha(material);

        let dTu = parseFloat(document.getElementById("b-dTu").value) || 0;
        let dTd = parseFloat(document.getElementById("b-dTd").value) || 0;

        const gammaQ = design ? 1.5 : 1.0;
        dTu *= gammaQ;
        dTd *= gammaQ;

        const eu = alpha * dTu;
        const theta = h > 0 ? dTd / h : 0;
        const kappa = alpha * theta;
        const Ttop = dTu + 0.5 * dTd;
        const Tbot = dTu - 0.5 * dTd;

        document.getElementById("b-eu").textContent =
          formatSci(eu) + " (dimensionless)";
        document.getElementById("b-theta").textContent = theta.toFixed(3);
        document.getElementById("b-kappa").textContent = formatSci(kappa);
        document.getElementById("b-Ttopbot").textContent =
          Ttop.toFixed(2) + " / " + Tbot.toFixed(2);
      }

      document
        .getElementById("b-btn-char")
        .addEventListener("click", () => calcBuilding(false));
      document
        .getElementById("b-btn-design")
        .addEventListener("click", () => calcBuilding(true));
      document.getElementById("b-btn-reset").addEventListener("click", () => {
        document.getElementById("b-element-type").value = "roof";
        document.getElementById("b-material").value = "concrete";
        document.getElementById("b-thickness").value = 0.25;
        document.getElementById("b-alpha").value = "";
        document.getElementById("b-kr").value = 1;
        document.getElementById("b-dTu").value = 30;
        document.getElementById("b-dTd").value = 15;
        document.getElementById("b-eu").textContent = "–";
        document.getElementById("b-theta").textContent = "–";
        document.getElementById("b-kappa").textContent = "–";
        document.getElementById("b-Ttopbot").textContent = "–";
      });

      // Bridge calculations
      function calcBridge(design) {
        const h = parseFloat(document.getElementById("br-h").value) || 0;
        let alpha = parseFloat(document.getElementById("br-alpha").value);
        const type = document.getElementById("br-type").value;
        if (isNaN(alpha)) alpha = getAlpha(type);

        let dTu = parseFloat(document.getElementById("br-dTu").value) || 0;
        let dTd = parseFloat(document.getElementById("br-dTd").value) || 0;

        const gammaQ = design ? 1.5 : 1.0;
        dTu *= gammaQ;
        dTd *= gammaQ;

        const eu = alpha * dTu;
        const theta = h > 0 ? dTd / h : 0;
        const kappa = alpha * theta;
        const Ttop = dTu + 0.5 * dTd;
        const Tbot = dTu - 0.5 * dTd;

        document.getElementById("br-eu").textContent =
          formatSci(eu) + " (dimensionless)";
        document.getElementById("br-theta").textContent = theta.toFixed(3);
        document.getElementById("br-kappa").textContent = formatSci(kappa);
        document.getElementById("br-Ttopbot").textContent =
          Ttop.toFixed(2) + " / " + Tbot.toFixed(2);
      }

      document
        .getElementById("br-btn-char")
        .addEventListener("click", () => calcBridge(false));
      document
        .getElementById("br-btn-design")
        .addEventListener("click", () => calcBridge(true));
      document.getElementById("br-btn-reset").addEventListener("click", () => {
        document.getElementById("br-type").value = "concrete";
        document.getElementById("br-exposure").value = "open";
        document.getElementById("br-h").value = 0.8;
        document.getElementById("br-alpha").value = "";
        document.getElementById("br-kr").value = 1;
        document.getElementById("br-dTu").value = 35;
        document.getElementById("br-dTd").value = 18;
        document.getElementById("br-eu").textContent = "–";
        document.getElementById("br-theta").textContent = "–";
        document.getElementById("br-kappa").textContent = "–";
        document.getElementById("br-Ttopbot").textContent = "–";
      });

      // Initial state
      setTab("building");
    </script>

