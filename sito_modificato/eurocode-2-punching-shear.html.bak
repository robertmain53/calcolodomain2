    <script>
      // Helper: format number with fixed decimals
      function fmt(value, decimals) {
        if (!isFinite(value)) return "–";
        return value.toFixed(decimals);
      }

      function updateStatusBadge(status) {
        const badge = document.getElementById("overallStatus");
        if (!badge) return;
        badge.classList.remove(
          "result-badge-ok",
          "result-badge-warn",
          "result-badge-fail"
        );
        if (status === "ok") {
          badge.textContent = "OK – concrete without shear reinforcement";
          badge.classList.add("result-badge-ok");
        } else if (status === "reinf") {
          badge.textContent = "Shear reinforcement required";
          badge.classList.add("result-badge-warn");
        } else if (status === "fail") {
          badge.textContent = "Not adequate – increase slab/column";
          badge.classList.add("result-badge-fail");
        } else {
          badge.textContent = "Awaiting input";
          badge.classList.add("result-badge-warn");
        }
      }

      function calculatePunchingShear() {
        const h = parseFloat(document.getElementById("slabThickness").value);
        const d = parseFloat(document.getElementById("effectiveDepth").value);
        const cx = parseFloat(document.getElementById("colWidth").value);
        const cy = parseFloat(document.getElementById("colLength").value);
        const fck = parseFloat(document.getElementById("fck").value);
        const rhoLperc = parseFloat(document.getElementById("rhoL").value);
        const gammaC = parseFloat(document.getElementById("gammaC").value);
        const VEd_kN = parseFloat(document.getElementById("ved").value);
        const beta = parseFloat(document.getElementById("beta").value);
        const CRdc = parseFloat(document.getElementById("crdc").value);
        const k1 = parseFloat(document.getElementById("k1").value);
        const vminFactor = parseFloat(
          document.getElementById("vminFactor").value
        );
        const sigmaCp = parseFloat(document.getElementById("sigmaCp").value);
        const fywd = parseFloat(document.getElementById("fywd").value);

        const statusMsg = document.getElementById("statusMessage");
        const shearReinfCard = document.getElementById("shearReinfCard");

        // Basic validation
        if (
          [h, d, cx, cy, fck, rhoLperc, gammaC, VEd_kN, beta, CRdc, k1].some(
            (v) => !isFinite(v) || v <= 0
          )
        ) {
          statusMsg.textContent =
            "Please enter valid positive values for all required fields.";
          updateStatusBadge("pending");
          return;
        }
        if (d >= h) {
          statusMsg.textContent =
            "Effective depth d should be less than slab thickness h.";
          updateStatusBadge("pending");
          return;
        }

        statusMsg.textContent = "";

        // 1. Control perimeter at 2d (internal column)
        const dist = 2 * d; // mm
        const u1 = 2 * (cx + cy + 4 * d); // mm

        // 2. k factor (EC2 6.2.2)
        let k = 1 + Math.sqrt(200 / d);
        if (k > 2.0) k = 2.0;

        // 3. Reinforcement ratio as decimal
        const rhoL = rhoLperc / 100.0;

        // 4. vmin
        const vmin = vminFactor * Math.pow(k, 1.5) * Math.sqrt(fck); // MPa

        // 5. vRdc
        const term1 = CRdc * k * Math.cbrt(100 * rhoL * fck);
        const term2 = vmin + k1 * sigmaCp;
        const vRdc = Math.max(term1, term2); // MPa

        // 6. vEd
        const VEd_N = VEd_kN * 1000.0; // kN -> kN, but we use mm & MPa: 1 MPa = 1 N/mm2, 1 kN = 1000 N
        const vEd = (beta * VEd_N) / (u1 * d); // N/mm2 = MPa

        // 7. vRd,max
        const alphaCC = 1.0;
        const fcd = (alphaCC * fck) / gammaC;
        const nu = 0.6 * (1 - fck / 250.0);
        const vRdmax = 0.5 * nu * fcd;

        // 8. Utilisations
        const utilRdc = vEd / vRdc;
        const utilRdmax = vEd / vRdmax;

        // Update UI
        document.getElementById("u1Dist").textContent = fmt(dist, 0);
        document.getElementById("u1Value").textContent = fmt(u1, 0);
        document.getElementById("kFactor").textContent = fmt(k, 2);

        document.getElementById("vEdValue").textContent = fmt(vEd, 3);
        document.getElementById("vRdcValue").textContent = fmt(vRdc, 3);
        document.getElementById("vRdmaxValue").textContent = fmt(vRdmax, 3);
        document.getElementById("utilRdc").textContent = fmt(utilRdc, 2);
        document.getElementById("utilRdmax").textContent = fmt(utilRdmax, 2);

        const overallSummary = document.getElementById("overallSummary");

        // Determine status
        if (vEd <= vRdc) {
          updateStatusBadge("ok");
          shearReinfCard.classList.add("hidden");
          overallSummary.textContent =
            "Punching shear is satisfied without shear reinforcement: vEd ≤ vRd,c. Check detailing and minimum reinforcement as per Eurocode 2.";
        } else if (vEd > vRdc && vEd <= vRdmax) {
          updateStatusBadge("reinf");
          // Estimate reinforcement demand
          const vEdRed = vEd - vRdc; // MPa
          const VEdRed_kN = (vEdRed * u1 * d) / 1000.0; // convert N to kN
          // Indicative reinforcement ratio (very simplified)
          const rhoW = (vEdRed / fywd) * 100.0; // %

          document.getElementById("vEdRed").textContent = fmt(VEdRed_kN, 1);
          document.getElementById("deltaV").textContent = fmt(vEdRed, 3);
          document.getElementById("rhoW").textContent = fmt(rhoW, 3);

          shearReinfCard.classList.remove("hidden");
          overallSummary.textContent =
            "Concrete alone is insufficient (vEd > vRd,c) but below vRd,max. Provide punching shear reinforcement and verify detailing to EN 1992-1-1, 6.4.5.";
        } else {
          updateStatusBadge("fail");
          shearReinfCard.classList.add("hidden");
          overallSummary.textContent =
            "Design not adequate: vEd exceeds vRd,max. Increasing shear reinforcement alone is not sufficient. Increase slab thickness, column size, or provide drop panels/column capitals.";
        }
      }

      document
        .getElementById("calcButton")
        .addEventListener("click", calculatePunchingShear);

      document.getElementById("resetButton").addEventListener("click", () => {
        document.getElementById("slabThickness").value = 250;
        document.getElementById("effectiveDepth").value = 210;
        document.getElementById("colWidth").value = 300;
        document.getElementById("colLength").value = 300;
        document.getElementById("fck").value = 25;
        document.getElementById("rhoL").value = 0.5;
        document.getElementById("gammaC").value = 1.5;
        document.getElementById("ved").value = 900;
        document.getElementById("beta").value = 1.15;
        document.getElementById("crdc").value = 0.18;
        document.getElementById("k1").value = 0.15;
        document.getElementById("vminFactor").value = 0.035;
        document.getElementById("sigmaCp").value = 0;
        document.getElementById("fywd").value = 500;

        document.getElementById("statusMessage").textContent = "";
        document.getElementById("overallSummary").textContent =
          "Enter geometry and loads, then click “Calculate punching shear” to see utilisation and required shear reinforcement.";
        document.getElementById("u1Dist").textContent = "–";
        document.getElementById("u1Value").textContent = "–";
        document.getElementById("kFactor").textContent = "–";
        document.getElementById("vEdValue").textContent = "–";
        document.getElementById("vRdcValue").textContent = "–";
        document.getElementById("vRdmaxValue").textContent = "–";
        document.getElementById("utilRdc").textContent = "–";
        document.getElementById("utilRdmax").textContent = "–";
        document.getElementById("vEdRed").textContent = "–";
        document.getElementById("deltaV").textContent = "–";
        document.getElementById("rhoW").textContent = "–";
        document.getElementById("shearReinfCard").classList.add("hidden");
        updateStatusBadge("pending");
      });

      // Initialize badge
      updateStatusBadge("pending");

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
