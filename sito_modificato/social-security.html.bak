    <script>
      // Utility: format currency
      function formatCurrency(value) {
        if (!isFinite(value)) return "$0";
        return (
          "$" + value.toLocaleString("en-US", { maximumFractionDigits: 0 })
        );
      }

      function clamp(val, min, max) {
        return Math.min(Math.max(val, min), max);
      }

      // Determine Full Retirement Age (approximate, in years as decimal)
      function getFRA(birthYear) {
        if (birthYear <= 1937) return 65;
        if (birthYear >= 1943 && birthYear <= 1954) return 66;
        if (birthYear >= 1960) return 67;

        // 1938–1942 and 1955–1959: increase by 2 months per year
        if (birthYear >= 1938 && birthYear <= 1942) {
          const extraMonths = (birthYear - 1937) * 2;
          return 65 + extraMonths / 12;
        }
        if (birthYear >= 1955 && birthYear <= 1959) {
          const extraMonths = (birthYear - 1954) * 2;
          return 66 + extraMonths / 12;
        }
        // Fallback
        return 67;
      }

      // Approximate PIA from AIME using 2024-style bend points
      function computePIA(aime) {
        const B1 = 1115; // first bend point
        const B2 = 6721; // second bend point

        const part1 = 0.9 * Math.min(aime, B1);
        const part2 = 0.32 * Math.min(Math.max(aime - B1, 0), B2 - B1);
        const part3 = 0.15 * Math.max(aime - B2, 0);

        return part1 + part2 + part3;
      }

      // Early/late retirement adjustment factor relative to FRA
      function adjustmentFactor(claimAge, fraAge) {
        const monthsDiff = Math.round((claimAge - fraAge) * 12);

        if (monthsDiff === 0) return 1;

        if (monthsDiff < 0) {
          // Early claiming
          const m = Math.abs(monthsDiff);
          const first36 = Math.min(m, 36);
          const additional = Math.max(m - 36, 0);
          const reduction = first36 * (5 / 900) + additional * (5 / 1200); // 5/9% and 5/12% per month
          return 1 - reduction;
        } else {
          // Delayed retirement credits up to age 70
          const cappedMonths = Math.min(monthsDiff, (70 - fraAge) * 12);
          const increase = cappedMonths * (2 / 300); // 2/3% per month
          return 1 + increase;
        }
      }

      function updateClaimAgeLabel() {
        const claimAgeInput = document.getElementById("claim-age");
        const claimAgeDisplay = document.getElementById("claim-age-display");
        const claimAgeLabel = document.getElementById("claim-age-label");
        const age = Number(claimAgeInput.value) || 67;
        claimAgeDisplay.textContent = age;
        claimAgeLabel.textContent = age;
      }

      function calculateSocialSecurity() {
        const birthYearInput = document.getElementById("birth-year");
        const aimeInput = document.getElementById("aime");
        const claimAgeInput = document.getElementById("claim-age");
        const benefitTypeInputs = document.querySelectorAll(
          'input[name="benefit-type"]'
        );

        let birthYear = parseInt(birthYearInput.value, 10);
        if (!birthYear || birthYear < 1900 || birthYear > 2007) {
          birthYear = 1960;
          birthYearInput.value = birthYear;
        }

        let aime = parseFloat(aimeInput.value);
        if (!isFinite(aime) || aime < 0) {
          aime = 0;
          aimeInput.value = "0";
        }

        let claimAge = parseFloat(claimAgeInput.value);
        claimAge = clamp(claimAge, 62, 70);
        claimAgeInput.value = claimAge;
        updateClaimAgeLabel();

        const fra = getFRA(birthYear);
        const piaRaw = computePIA(aime);
        const pia = Math.max(0, piaRaw);

        // Determine benefit type
        let benefitType = "retirement";
        benefitTypeInputs.forEach((r) => {
          if (r.checked) benefitType = r.value;
        });

        // Base PIA for retirement; for spousal, base is 50% of spouse's PIA (simplified)
        const basePIA = benefitType === "spousal" ? pia * 0.5 : pia;

        const adjFactor = adjustmentFactor(claimAge, fra);
        const monthlyBenefit = basePIA * adjFactor;

        // Update main displays
        document.getElementById("fra-display").textContent = fra
          .toFixed(1)
          .replace(".0", "");
        document.getElementById("pia-display").textContent = formatCurrency(
          Math.round(basePIA)
        );
        document.getElementById("benefit-display").textContent = formatCurrency(
          Math.round(monthlyBenefit)
        );

        const adjPercent = (adjFactor - 1) * 100;
        const adjDisplay = document.getElementById("adj-display");
        const sign = adjPercent > 0 ? "+" : "";
        adjDisplay.textContent = sign + adjPercent.toFixed(1) + "%";

        // Comparison table: ages 62, FRA (rounded), 70
        const tbody = document.getElementById("age-compare-body");
        tbody.innerHTML = "";

        const ages = [62, Math.round(fra), 70];
        ages.forEach((age) => {
          const factor = adjustmentFactor(age, fra);
          const benefit = basePIA * factor;
          const vsFRA = (factor - 1) * 100;
          const row = document.createElement("tr");
          row.className = "border-b border-gray-100 last:border-0";
          row.innerHTML = `
        <td class="py-1">${
          age === Math.round(fra) ? "FRA (" + age + ")" : age
        }</td>
        <td class="py-1 text-right">${formatCurrency(Math.round(benefit))}</td>
        <td class="py-1 text-right">${
          (vsFRA >= 0 ? "+" : "") + vsFRA.toFixed(1)
        }%</td>
      `;
          tbody.appendChild(row);
        });
      }

      document.addEventListener("DOMContentLoaded", function () {
        const claimAgeInput = document.getElementById("claim-age");
        const calcBtn = document.getElementById("ss-calc-btn");

        claimAgeInput.addEventListener("input", updateClaimAgeLabel);
        calcBtn.addEventListener("click", function (e) {
          e.preventDefault();
          calculateSocialSecurity();
        });

        // Auto-calc on load with defaults
        updateClaimAgeLabel();
        calculateSocialSecurity();
      });
    </script>

