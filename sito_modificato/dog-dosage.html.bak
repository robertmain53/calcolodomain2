    <script>
      // Medication presets (approximate reference ranges; always vet-confirmed in real life)
      const MED_PRESETS = {
        custom: {
          name: "Custom",
          minMgPerKg: null,
          maxMgPerKg: null,
          defaultFreq: "once",
          note: "Enter the mg/kg range provided by your veterinarian or a trusted veterinary reference.",
        },
        diphenhydramine: {
          name: "Diphenhydramine (Benadryl)",
          minMgPerKg: 1,
          maxMgPerKg: 2,
          defaultFreq: "q8h",
          note: "Approx. 1–2 mg/kg by mouth every 8–12 hours. Avoid combination human cold/flu products.",
        },
        famotidine: {
          name: "Famotidine (Pepcid)",
          minMgPerKg: 0.5,
          maxMgPerKg: 1,
          defaultFreq: "q12h",
          note: "Approx. 0.5–1 mg/kg by mouth every 12–24 hours.",
        },
        omeprazole: {
          name: "Omeprazole",
          minMgPerKg: 0.7,
          maxMgPerKg: 1,
          defaultFreq: "q24h",
          note: "Approx. 0.7–1 mg/kg by mouth every 24 hours.",
        },
        maropitant: {
          name: "Maropitant (Cerenia)",
          minMgPerKg: 1,
          maxMgPerKg: 1,
          defaultFreq: "q24h",
          note: "Typical dose ~1 mg/kg every 24 hours. Injectable and oral forms differ; follow your vet's protocol.",
        },
        meloxicam: {
          name: "Meloxicam (NSAID)",
          minMgPerKg: 0.05,
          maxMgPerKg: 0.1,
          defaultFreq: "q24h",
          note: "Maintenance doses often around 0.05 mg/kg every 24 hours after a loading dose. NSAIDs can be dangerous if misused.",
        },
      };

      function $(id) {
        return document.getElementById(id);
      }

      const medicationSelect = $("medication");
      const medicationNote = $("medication-note");
      const doseMinInput = $("dose-min");
      const doseMaxInput = $("dose-max");
      const frequencySelect = $("frequency");
      const weightInput = $("weight");
      const weightUnitSelect = $("weight-unit");
      const calcError = $("calc-error");
      const resultsSection = $("results-section");
      const resultWeight = $("result-weight");
      const resultDoseRange = $("result-dose-range");
      const resultDoseInterval = $("result-dose-interval");
      const resultTotalMg = $("result-total-mg");
      const resultPractical = $("result-practical");
      const tabletStrengthInput = $("tablet-strength");
      const liquidStrengthInput = $("liquid-strength");
      const strengthContainerTablet = $("strength-container-tablet");
      const strengthContainerLiquid = $("strength-container-liquid");

      // Update dose fields when medication changes
      medicationSelect.addEventListener("change", () => {
        const key = medicationSelect.value;
        const preset = MED_PRESETS[key] || MED_PRESETS.custom;

        if (preset.minMgPerKg != null) {
          doseMinInput.value = preset.minMgPerKg;
        } else {
          doseMinInput.value = "";
        }
        if (preset.maxMgPerKg != null) {
          doseMaxInput.value = preset.maxMgPerKg;
        } else {
          doseMaxInput.value = "";
        }

        if (preset.defaultFreq) {
          frequencySelect.value = preset.defaultFreq;
        }

        medicationNote.textContent = preset.note;
      });

      // Toggle formulation strength input
      document
        .querySelectorAll('input[name="formulation"]')
        .forEach((radio) => {
          radio.addEventListener("change", () => {
            if (radio.checked && radio.value === "tablet") {
              strengthContainerTablet.classList.remove("hidden");
              strengthContainerLiquid.classList.add("hidden");
            } else if (radio.checked && radio.value === "liquid") {
              strengthContainerTablet.classList.add("hidden");
              strengthContainerLiquid.classList.remove("hidden");
            }
          });
        });

      function roundTo(value, decimals) {
        const factor = Math.pow(10, decimals);
        return Math.round(value * factor) / factor;
      }

      function formatInterval(code) {
        switch (code) {
          case "q8h":
            return "every 8 hours";
          case "q12h":
            return "every 12 hours";
          case "q24h":
            return "every 24 hours";
          case "once":
            return "once";
          case "vet":
            return "as directed by your veterinarian";
          default:
            return "";
        }
      }

      $("calculate-btn").addEventListener("click", () => {
        calcError.textContent = "";
        resultsSection.classList.add("hidden");

        const weight = parseFloat(weightInput.value);
        if (!weight || weight <= 0) {
          calcError.textContent = "Enter a valid dog weight.";
          return;
        }

        const weightUnit = weightUnitSelect.value;
        const weightKg = weightUnit === "kg" ? weight : weight / 2.20462;

        const doseMin = parseFloat(doseMinInput.value);
        const doseMax = parseFloat(doseMaxInput.value);

        if (!doseMin || doseMin <= 0 || !doseMax || doseMax <= 0) {
          calcError.textContent =
            "Enter a valid minimum and maximum dose in mg/kg.";
          return;
        }
        if (doseMax < doseMin) {
          calcError.textContent =
            "Maximum mg/kg must be greater than or equal to minimum mg/kg.";
          return;
        }

        const form =
          document.querySelector('input[name="formulation"]:checked')?.value ||
          "tablet";

        let strength = null;
        if (form === "tablet") {
          strength = parseFloat(tabletStrengthInput.value);
          if (!strength || strength <= 0) {
            calcError.textContent = "Enter the tablet strength in mg.";
            return;
          }
        } else {
          strength = parseFloat(liquidStrengthInput.value);
          if (!strength || strength <= 0) {
            calcError.textContent = "Enter the liquid concentration in mg/mL.";
            return;
          }
        }

        // Calculate mg range
        const minMg = weightKg * doseMin;
        const maxMg = weightKg * doseMax;

        // Practical dose: choose mid-range, then round safely
        const targetMg = (minMg + maxMg) / 2;

        let practicalText = "";
        if (form === "tablet") {
          const exactTablets = targetMg / strength;

          // Try rounding to nearest 0.25 tablet but not exceeding max mg
          const candidates = [0.25, 0.5, 0.75, 1, 1.5, 2, 3, 4]
            .map((mult) => {
              const t = Math.round(exactTablets / mult) * mult;
              const mg = t * strength;
              return { t, mg };
            })
            .filter((c) => c.t > 0 && c.mg >= minMg && c.mg <= maxMg);

          let best = null;
          if (candidates.length > 0) {
            best = candidates.reduce((a, b) => {
              return Math.abs(b.mg - targetMg) < Math.abs(a.mg - targetMg)
                ? b
                : a;
            });
          } else {
            // Fallback: cap at max mg
            const maxTablets = maxMg / strength;
            best = { t: roundTo(maxTablets, 2), mg: maxMg };
          }

          const t = best.t;
          const mg = best.mg;
          let tabletDesc = "";

          if (Math.abs(t - Math.round(t)) < 0.01) {
            tabletDesc = `${roundTo(t, 0)} tablet${t === 1 ? "" : "s"}`;
          } else if (Math.abs(t * 2 - Math.round(t * 2)) < 0.01) {
            tabletDesc = `${roundTo(t * 2, 0)}/2 tablet`;
          } else if (Math.abs(t * 4 - Math.round(t * 4)) < 0.01) {
            tabletDesc = `${roundTo(t * 4, 0)}/4 tablet`;
          } else {
            tabletDesc = `${roundTo(t, 2)} tablets`;
          }

          practicalText = `${tabletDesc} (≈ ${roundTo(mg, 1)} mg) per dose`;
        } else {
          // Liquid
          const exactMl = targetMg / strength;

          // Round to 0.1 mL, but ensure within mg range
          let ml = roundTo(exactMl, 1);
          let mg = ml * strength;

          if (mg > maxMg) {
            ml = roundTo(maxMg / strength, 1);
            mg = ml * strength;
          } else if (mg < minMg) {
            ml = roundTo(minMg / strength, 1);
            mg = ml * strength;
          }

          practicalText = `${ml.toFixed(1)} mL (≈ ${roundTo(
            mg,
            1
          )} mg) per dose`;
        }

        // Populate results
        const displayWeight =
          weightUnit === "kg"
            ? `${roundTo(weightKg, 2)} kg (${roundTo(
                weightKg * 2.20462,
                1
              )} lb)`
            : `${roundTo(weightKg * 2.20462, 1)} lb (${roundTo(
                weightKg,
                2
              )} kg)`;

        resultWeight.textContent = displayWeight;
        resultDoseRange.textContent = `${doseMin}–${doseMax} mg/kg`;
        resultDoseInterval.textContent = `Dosing interval: ${formatInterval(
          frequencySelect.value
        )}`;

        resultTotalMg.textContent = `${roundTo(minMg, 1)}–${roundTo(
          maxMg,
          1
        )} mg per dose (reference range)`;
        resultPractical.textContent = practicalText;

        resultsSection.classList.remove("hidden");
      });

      $("reset-btn").addEventListener("click", () => {
        document.getElementById("dog-dosage-form").reset();
        calcError.textContent = "";
        resultsSection.classList.add("hidden");
        medicationSelect.value = "custom";
        medicationNote.textContent = MED_PRESETS.custom.note;
        doseMinInput.value = "";
        doseMaxInput.value = "";
        strengthContainerTablet.classList.remove("hidden");
        strengthContainerLiquid.classList.add("hidden");
      });

      // Initialize with custom
      medicationSelect.dispatchEvent(new Event("change"));
    </script>

