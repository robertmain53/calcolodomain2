                const summary = document.getElementById("qt-l-summary");
                const qtTypeSelect = document.getElementById("quart-type");
                const qtTypeHint = document.getElementById("quart-type-hint");

                const FACTORS = {
                  us_liquid: 0.946352946, // L per US liquid quart
                  us_dry: 1.1012209428, // L per US dry quart
                  imperial: 1.1365225, // L per Imperial quart
                };

                const HINTS = {
                  us_liquid: "Currently using: 1 US liquid quart ≈ 0.94635 L.",
                  us_dry: "Currently using: 1 US dry quart ≈ 1.10122 L.",
                  imperial: "Currently using: 1 Imperial quart ≈ 1.13652 L.",
                };

                let activeField = null;

                function getFactor() {
                  const type = qtTypeSelect.value;
                  return FACTORS[type] || FACTORS.us_liquid;
                }

                function roundSmart(value) {
                  if (!isFinite(value)) return "";
                  const abs = Math.abs(value);
                  if (abs === 0) return "0";
                  if (abs < 1e-4) return value.toExponential(4);
                  if (abs < 1) return value.toFixed(4);
                  if (abs < 1000) return value.toFixed(3);
                  return value.toFixed(2);
                }

                function updateSummary(qt, l) {
                  if (!isFinite(qt) || !isFinite(l)) {
                    summary.textContent =
                      "Tip: 1 US liquid quart ≈ 0.95 L, 1 Imperial quart ≈ 1.14 L.";
                    return;
                  }
                  summary.textContent = qt + " qt ≈ " + l + " L";
                }

                function handleQtInput() {
                  if (activeField === "l") return;
                  activeField = "qt";
                  const qtVal = parseFloat(qtInput.value);
                  if (isNaN(qtVal)) {
                    lInput.value = "";
                    updateSummary(NaN, NaN);
                    activeField = null;
                    return;
                  }
                  const factor = getFactor();
                  const lVal = qtVal * factor;
                  const lRounded = roundSmart(lVal);
                  lInput.value = lRounded;
                  updateSummary(roundSmart(qtVal), lRounded);
                  activeField = null;
                }

                function handleLInput() {
                  if (activeField === "qt") return;
                  activeField = "l";
                  const lVal = parseFloat(lInput.value);
                  if (isNaN(lVal)) {
                    qtInput.value = "";
                    updateSummary(NaN, NaN);
                    activeField = null;
                    return;
                  }
                  const factor = getFactor();
                  const qtVal = lVal / factor;
                  const qtRounded = roundSmart(qtVal);
                  qtInput.value = qtRounded;
                  updateSummary(qtRounded, roundSmart(lVal));
                  activeField = null;
                }

                if (qtInput) {
                  qtInput.addEventListener("input", handleQtInput);
                }
                if (lInput) {
                  lInput.addEventListener("input", handleLInput);
                }

                qtPresets.forEach((btn) => {
                  btn.addEventListener("click", () => {
                    const v = parseFloat(btn.getAttribute("data-qt"));
                    if (!isNaN(v)) {
                      qtInput.value = v;
                      handleQtInput();
                      qtInput.focus();
                    }
                  });
                });

                lPresets.forEach((btn) => {
                  btn.addEventListener("click", () => {
                    const v = parseFloat(btn.getAttribute("data-l"));
                    if (!isNaN(v)) {
                      lInput.value = v;
                      handleLInput();
                      lInput.focus();
                    }
                  });
                });

                if (swapButton) {
                  swapButton.addEventListener("click", () => {
                    const qtVal = qtInput.value;
                    const lVal = lInput.value;
                    qtInput.value = lVal;
                    lInput.value = qtVal;
                    handleQtInput();
                  });
                }

                if (clearButton) {
                  clearButton.addEventListener("click", () => {
                    qtInput.value = "";
                    lInput.value = "";
                    updateSummary(NaN, NaN);
                    qtInput.focus();
                  });
                }

                qtTypeSelect.addEventListener("change", () => {
                  const type = qtTypeSelect.value;
                  qtTypeHint.textContent = HINTS[type] || HINTS.us_liquid;
                  // Recalculate with new factor if there is a value
                  if (qtInput.value) {
                    handleQtInput();
                  } else if (lInput.value) {
                    handleLInput();
                  }
                });

                // Initial demo state
                qtInput.value = "1";
                handleQtInput();
              })();
            </script>
