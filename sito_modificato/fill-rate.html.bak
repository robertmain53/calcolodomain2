<script="">
        document.getElementById('calculateBtn').addEventListener('click', function() {
            const fulfilledOrders = parseFloat(document.getElementById('fulfilledOrders').value);
            const totalOrders = parseFloat(document.getElementById('totalOrders').value);
            const fillRateValue = document.getElementById('fillRateValue');

            if (!isNaN(fulfilledOrders) && !isNaN(totalOrders) && totalOrders > 0) {
                const fillRate = (fulfilledOrders / totalOrders) * 100;
                fillRateValue.textContent = fillRate.toFixed(2) + '%';
            } else {
                fillRateValue.textContent = 'Invalid input';
            }
        });
    </script>


