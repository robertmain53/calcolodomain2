    <script>
      // Tab switching
      const tabColligative = document.getElementById("tab-colligative");
      const tabDissociation = document.getElementById("tab-dissociation");
      const tabAssociation = document.getElementById("tab-association");

      const panelColligative = document.getElementById("panel-colligative");
      const panelDissociation = document.getElementById("panel-dissociation");
      const panelAssociation = document.getElementById("panel-association");

      function setActiveTab(activeTab) {
        [tabColligative, tabDissociation, tabAssociation].forEach((tab) => {
          tab.classList.remove("tab-active");
        });
        activeTab.classList.add("tab-active");

        panelColligative.classList.toggle(
          "hidden",
          activeTab !== tabColligative
        );
        panelDissociation.classList.toggle(
          "hidden",
          activeTab !== tabDissociation
        );
        panelAssociation.classList.toggle(
          "hidden",
          activeTab !== tabAssociation
        );
      }

      tabColligative.addEventListener("click", () =>
        setActiveTab(tabColligative)
      );
      tabDissociation.addEventListener("click", () =>
        setActiveTab(tabDissociation)
      );
      tabAssociation.addEventListener("click", () =>
        setActiveTab(tabAssociation)
      );

      // Property switching
      const propertyType = document.getElementById("property-type");
      const propertyFreezing = document.getElementById("property-freezing");
      const propertyBoiling = document.getElementById("property-boiling");
      const propertyOsmotic = document.getElementById("property-osmotic");
      const propertyVapor = document.getElementById("property-vapor");

      function updatePropertyPanel() {
        const val = propertyType.value;
        propertyFreezing.classList.add("hidden");
        propertyBoiling.classList.add("hidden");
        propertyOsmotic.classList.add("hidden");
        propertyVapor.classList.add("hidden");
        if (val === "freezing") propertyFreezing.classList.remove("hidden");
        if (val === "boiling") propertyBoiling.classList.remove("hidden");
        if (val === "osmotic") propertyOsmotic.classList.remove("hidden");
        if (val === "vapor") propertyVapor.classList.remove("hidden");
      }
      propertyType.addEventListener("change", updatePropertyPanel);

      // Colligative calculation
      const molalityInput = document.getElementById("molality");
      const deltaTfInput = document.getElementById("deltaTf");
      const KfInput = document.getElementById("Kf");
      const deltaTbInput = document.getElementById("deltaTb");
      const KbInput = document.getElementById("Kb");
      const osmoticPressureInput = document.getElementById("osmoticPressure");
      const temperatureKInput = document.getElementById("temperatureK");
      const P0Input = document.getElementById("P0");
      const PInput = document.getElementById("P");

      const btnCalcColligative = document.getElementById(
        "btn-calc-colligative"
      );
      const btnResetColligative = document.getElementById(
        "btn-reset-colligative"
      );
      const resultColligative = document.getElementById("result-colligative");
      const errorColligative = document.getElementById("error-colligative");

      btnCalcColligative.addEventListener("click", () => {
        errorColligative.textContent = "";
        let m = parseFloat(molalityInput.value);
        if (!isFinite(m) || m <= 0) {
          errorColligative.textContent = "Please enter a positive molality.";
          return;
        }
        let i = null;
        const prop = propertyType.value;

        if (prop === "freezing") {
          const dTf = parseFloat(deltaTfInput.value);
          const Kf = parseFloat(KfInput.value);
          if (!isFinite(dTf) || !isFinite(Kf) || Kf <= 0) {
            errorColligative.textContent = "Enter valid ΔTf and Kf values.";
            return;
          }
          i = dTf / (Kf * m);
        } else if (prop === "boiling") {
          const dTb = parseFloat(deltaTbInput.value);
          const Kb = parseFloat(KbInput.value);
          if (!isFinite(dTb) || !isFinite(Kb) || Kb <= 0) {
            errorColligative.textContent = "Enter valid ΔTb and Kb values.";
            return;
          }
          i = dTb / (Kb * m);
        } else if (prop === "osmotic") {
          const pi = parseFloat(osmoticPressureInput.value);
          const T = parseFloat(temperatureKInput.value);
          if (!isFinite(pi) || !isFinite(T) || T <= 0) {
            errorColligative.textContent =
              "Enter valid osmotic pressure and temperature.";
            return;
          }
          const R = 0.082057; // L·atm·K⁻1·mol⁻1
          // Approximate M ≈ m for dilute aqueous solutions
          i = pi / (m * R * T);
        } else if (prop === "vapor") {
          const P0 = parseFloat(P0Input.value);
          const P = parseFloat(PInput.value);
          if (!isFinite(P0) || !isFinite(P) || P0 <= 0 || P < 0 || P >= P0) {
            errorColligative.textContent =
              "Enter valid vapor pressures with P < P⁰.";
            return;
          }
          const deltaP = P0 - P;
          // Very rough approximation: i ≈ (ΔP / P0) / (k * m), with k ~ 1 for dilute solutions
          // We simply scale by m to give a qualitative estimate.
          i = deltaP / P0 / m;
        }

        if (!isFinite(i) || i <= 0) {
          errorColligative.textContent =
            "Calculated van ’t Hoff factor is not physical. Check your inputs.";
          return;
        }

        const iRounded = i.toFixed(3);
        let interpretation = "";
        if (iRounded < 0.9) {
          interpretation =
            " (association likely; fewer particles than formula units)";
        } else if (iRounded > 1.1) {
          interpretation =
            " (dissociation likely; more particles than formula units)";
        } else {
          interpretation = " (behaves approximately as a nonelectrolyte)";
        }

        resultColligative.textContent = `Van ’t Hoff factor i ≈ ${iRounded}${interpretation}`;
      });

      btnResetColligative.addEventListener("click", () => {
        molalityInput.value = "";
        deltaTfInput.value = "";
        KfInput.value = "";
        deltaTbInput.value = "";
        KbInput.value = "";
        osmoticPressureInput.value = "";
        temperatureKInput.value = "";
        P0Input.value = "";
        PInput.value = "";
        errorColligative.textContent = "";
        resultColligative.textContent =
          "Enter values and click “Calculate” to see the van ’t Hoff factor.";
      });

      // Dissociation calculation
      const iDissInput = document.getElementById("i-diss");
      const nIonsInput = document.getElementById("n-ions");
      const btnCalcDiss = document.getElementById("btn-calc-diss");
      const btnResetDiss = document.getElementById("btn-reset-diss");
      const resultDiss = document.getElementById("result-diss");
      const errorDiss = document.getElementById("error-diss");

      btnCalcDiss.addEventListener("click", () => {
        errorDiss.textContent = "";
        const iVal = parseFloat(iDissInput.value);
        const nVal = parseInt(nIonsInput.value, 10);

        if (!isFinite(iVal) || iVal <= 0) {
          errorDiss.textContent = "Enter a positive van ’t Hoff factor.";
          return;
        }
        if (!isFinite(nVal) || nVal < 2) {
          errorDiss.textContent =
            "Enter an integer n ≥ 2 (number of ions per formula unit).";
          return;
        }

        const alpha = (iVal - 1) / (nVal - 1);
        if (!isFinite(alpha) || alpha < 0 || alpha > 1.2) {
          errorDiss.textContent =
            "The combination of i and n is not consistent with simple dissociation (α should be between 0 and 1).";
          return;
        }

        const alphaClamped = Math.max(0, Math.min(alpha, 1));
        const particles = 1 + alphaClamped * (nVal - 1);

        resultDiss.textContent =
          `Degree of dissociation α ≈ ${(alphaClamped * 100).toFixed(1)}% ` +
          `and effective particles per formula unit ≈ ${particles.toFixed(3)}.`;
      });

      btnResetDiss.addEventListener("click", () => {
        iDissInput.value = "";
        nIonsInput.value = "";
        errorDiss.textContent = "";
        resultDiss.textContent =
          "Enter i and n, then click “Calculate” to find the degree of dissociation α and effective particles per formula unit.";
      });

      // Association calculation
      const iAssocInput = document.getElementById("i-assoc");
      const mAssocInput = document.getElementById("m-assoc");
      const btnCalcAssoc = document.getElementById("btn-calc-assoc");
      const btnResetAssoc = document.getElementById("btn-reset-assoc");
      const resultAssoc = document.getElementById("result-assoc");
      const errorAssoc = document.getElementById("error-assoc");

      btnCalcAssoc.addEventListener("click", () => {
        errorAssoc.textContent = "";
        const iVal = parseFloat(iAssocInput.value);
        const mVal = parseInt(mAssocInput.value, 10);

        if (!isFinite(iVal) || iVal <= 0) {
          errorAssoc.textContent = "Enter a positive van ’t Hoff factor.";
          return;
        }
        if (!isFinite(mVal) || mVal < 2) {
          errorAssoc.textContent =
            "Enter an integer m ≥ 2 (2 for dimers, 3 for trimers, etc.).";
          return;
        }

        const denom = 1 - 1 / mVal;
        const alpha = (1 - iVal) / denom;

        if (!isFinite(alpha) || alpha < 0 || alpha > 1.2) {
          errorAssoc.textContent =
            "The combination of i and m is not consistent with simple association (α should be between 0 and 1).";
          return;
        }

        const alphaClamped = Math.max(0, Math.min(alpha, 1));
        const particles = 1 - alphaClamped * (1 - 1 / mVal);

        resultAssoc.textContent =
          `Degree of association α ≈ ${(alphaClamped * 100).toFixed(1)}% ` +
          `and effective particles per formula unit ≈ ${particles.toFixed(3)}.`;
      });

      btnResetAssoc.addEventListener("click", () => {
        iAssocInput.value = "";
        mAssocInput.value = "";
        errorAssoc.textContent = "";
        resultAssoc.textContent =
          "Enter i and the association order m, then click “Calculate” to find the degree of association α and effective particles per formula unit.";
      });

      // Initialize property panel
      updatePropertyPanel();
    </script>

    <script>
      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
