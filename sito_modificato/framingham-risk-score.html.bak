<script="">
        document.getElementById('calculateBtn').addEventListener('click', function() {
            const age = parseInt(document.getElementById('age').value, 10);
            const gender = document.getElementById('gender').value;
            const cholesterol = parseInt(document.getElementById('cholesterol').value, 10);
            const hdl = parseInt(document.getElementById('hdl').value, 10);
            const systolic = parseInt(document.getElementById('systolic').value, 10);
            const smoker = document.getElementById('smoker').value;

            let riskScore;

            // Simple validation
            if (isNaN(age) || isNaN(cholesterol) || isNaN(hdl) || isNaN(systolic) || gender === "" || smoker === "") {
                document.getElementById('riskResult').textContent = 'Please fill in all fields correctly.';
                return;
            }

            // Simplified risk calculation based on input (not actual algorithm)
            if (gender === 'male') {
                riskScore = (age * 0.2) + (cholesterol * 0.02) - (hdl * 0.02) + (systolic * 0.01);
            } else {
                riskScore = (age * 0.15) + (cholesterol * 0.015) - (hdl * 0.025) + (systolic * 0.012);
            }

            if (smoker === 'yes') {
                riskScore += 2;
            }

            document.getElementById('riskResult').textContent = `Your estimated 10-year cardiovascular risk score is: ${riskScore.toFixed(2)}%`;
        });
    </script>


