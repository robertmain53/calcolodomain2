    <script>
      (function () {
        const unitSystemEl = document.getElementById("unit-system");
        const bcTypeEl = document.getElementById("bc-type");
        const lengthAEl = document.getElementById("length-a");
        const lengthBEl = document.getElementById("length-b");
        const thicknessEl = document.getElementById("thickness");
        const youngEl = document.getElementById("young-modulus");
        const poissonEl = document.getElementById("poisson");
        const yieldEl = document.getElementById("yield-strength");
        const loadTypeEl = document.getElementById("load-type");
        const uniformLoadEl = document.getElementById("uniform-load");
        const pointLoadEl = document.getElementById("point-load");
        const meshNxEl = document.getElementById("mesh-nx");
        const meshNyEl = document.getElementById("mesh-ny");
        const calcBtn = document.getElementById("calculate-btn");
        const resetBtn = document.getElementById("reset-btn");
        const statusEl = document.getElementById("calc-status");

        const lengthAUnit = document.getElementById("length-a-unit");
        const lengthBUnit = document.getElementById("length-b-unit");
        const thicknessUnit = document.getElementById("thickness-unit");
        const youngUnit = document.getElementById("young-modulus-unit");
        const yieldUnit = document.getElementById("yield-strength-unit");
        const uniformLoadUnit = document.getElementById("uniform-load-unit");
        const pointLoadUnit = document.getElementById("point-load-unit");

        const uniformWrapper = document.getElementById("uniform-load-wrapper");
        const pointWrapper = document.getElementById("point-load-wrapper");

        const wmaxEl = document.getElementById("result-wmax");
        const wmaxRatioEl = document.getElementById("result-wmax-ratio");
        const sigmaMaxEl = document.getElementById("result-sigma-max");
        const safetyEl = document.getElementById("result-safety");

        const canvas = document.getElementById("deflection-canvas");
        const ctx = canvas.getContext("2d");
        const hoverInfoEl = document.getElementById("hover-info");

        let lastField = null;

        function setUnits() {
          const sys = unitSystemEl.value;
          if (sys === "metric") {
            lengthAUnit.textContent = "mm";
            lengthBUnit.textContent = "mm";
            thicknessUnit.textContent = "mm";
            youngUnit.textContent = "MPa";
            yieldUnit.textContent = "MPa";
            uniformLoadUnit.textContent = "kN/m²";
            pointLoadUnit.textContent = "kN";
            if (lastField !== "switch") {
              lengthAEl.value = 1000;
              lengthBEl.value = 500;
              thicknessEl.value = 10;
              youngEl.value = 210000;
              uniformLoadEl.value = 5;
              pointLoadEl.value = 10;
            }
          } else {
            lengthAUnit.textContent = "in";
            lengthBUnit.textContent = "in";
            thicknessUnit.textContent = "in";
            youngUnit.textContent = "ksi";
            yieldUnit.textContent = "ksi";
            uniformLoadUnit.textContent = "psi";
            pointLoadUnit.textContent = "kips";
            if (lastField !== "switch") {
              lengthAEl.value = 40;
              lengthBEl.value = 20;
              thicknessEl.value = 0.5;
              youngEl.value = 30000;
              uniformLoadEl.value = 100;
              pointLoadEl.value = 2;
            }
          }
        }

        function setLoadVisibility() {
          const type = loadTypeEl.value;
          if (type === "uniform") {
            uniformWrapper.classList.remove("hidden");
            pointWrapper.classList.add("hidden");
          } else {
            uniformWrapper.classList.add("hidden");
            pointWrapper.classList.remove("hidden");
          }
        }

        function clearResults() {
          wmaxEl.textContent = "–";
          wmaxRatioEl.textContent = "Serviceability check: –";
          sigmaMaxEl.textContent = "–";
          safetyEl.textContent = "Safety factor vs. yield: –";
          ctx.clearRect(0, 0, canvas.width, canvas.height);
          hoverInfoEl.textContent = "x = –, y = –, w = –";
        }

        function validateInputs() {
          const vals = [
            { el: lengthAEl, name: "Length a" },
            { el: lengthBEl, name: "Length b" },
            { el: thicknessEl, name: "Thickness" },
            { el: youngEl, name: "Young’s modulus" },
            { el: poissonEl, name: "Poisson’s ratio" },
            { el: meshNxEl, name: "Mesh Nx" },
            { el: meshNyEl, name: "Mesh Ny" },
          ];
          for (const v of vals) {
            const val = parseFloat(v.el.value);
            if (!isFinite(val) || val <= 0) {
              alert("Please enter a valid positive value for " + v.name + ".");
              return false;
            }
          }
          const nu = parseFloat(poissonEl.value);
          if (nu <= -0.49 || nu >= 0.49) {
            alert(
              "Poisson’s ratio should be between about -0.49 and 0.49 for stability."
            );
            return false;
          }
          const nx = parseInt(meshNxEl.value, 10);
          const ny = parseInt(meshNyEl.value, 10);
          if (nx < 4 || nx > 80 || ny < 4 || ny > 80) {
            alert("Mesh divisions should be between 4 and 80.");
            return false;
          }
          if (loadTypeEl.value === "uniform") {
            const q = parseFloat(uniformLoadEl.value);
            if (!isFinite(q) || q === 0) {
              alert("Please enter a non-zero uniform pressure.");
              return false;
            }
          } else {
            const P = parseFloat(pointLoadEl.value);
            if (!isFinite(P) || P === 0) {
              alert("Please enter a non-zero point load.");
              return false;
            }
          }
          return true;
        }

        function compute() {
          if (!validateInputs()) return;
          statusEl.textContent = "Calculating...";
          clearResults();

          const sys = unitSystemEl.value;
          const a = parseFloat(lengthAEl.value);
          const b = parseFloat(lengthBEl.value);
          const t = parseFloat(thicknessEl.value);
          const E_in = parseFloat(youngEl.value);
          const nu = parseFloat(poissonEl.value);
          const nx = parseInt(meshNxEl.value, 10);
          const ny = parseInt(meshNyEl.value, 10);
          const bc = bcTypeEl.value;
          const loadType = loadTypeEl.value;

          // Convert to SI base units for internal calc
          let a_m,
            b_m,
            t_m,
            E_Pa,
            q_Pa = 0,
            P_N = 0;
          if (sys === "metric") {
            a_m = a / 1000;
            b_m = b / 1000;
            t_m = t / 1000;
            E_Pa = E_in * 1e6; // MPa to Pa
            if (loadType === "uniform") {
              q_Pa = parseFloat(uniformLoadEl.value) * 1000; // kN/m2 to N/m2
            } else {
              P_N = parseFloat(pointLoadEl.value) * 1000; // kN to N
            }
          } else {
            // inches, ksi, psi, kips
            const in_to_m = 0.0254;
            a_m = a * in_to_m;
            b_m = b * in_to_m;
            t_m = t * in_to_m;
            E_Pa = E_in * 1000 * 6894.757; // ksi -> psi -> Pa
            if (loadType === "uniform") {
              q_Pa = parseFloat(uniformLoadEl.value) * 6894.757; // psi -> Pa
            } else {
              P_N = parseFloat(pointLoadEl.value) * 1000 * 4.4482216; // kips -> lbf -> N
            }
          }

          const D = (E_Pa * Math.pow(t_m, 3)) / (12 * (1 - nu * nu));

          const aspect = a_m / b_m;
          // Coefficients for wmax ~ alpha * q a^4 / D or beta * P a^2 / D
          function alphaUniform(aspect, bc) {
            // crude interpolation based on classical tables
            const r = Math.max(1, Math.min(4, aspect));
            const s = (r - 1) / 3;
            let a_ss1 = 0.00406,
              a_ss4 = 0.0116;
            let a_cl1 = 0.00126,
              a_cl4 = 0.0036;
            if (bc === "ss") {
              return a_ss1 + (a_ss4 - a_ss1) * s;
            } else {
              return a_cl1 + (a_cl4 - a_cl1) * s;
            }
          }
          function betaPoint(aspect, bc) {
            const r = Math.max(1, Math.min(4, aspect));
            const s = (r - 1) / 3;
            let b_ss1 = 0.0116,
              b_ss4 = 0.02;
            let b_cl1 = 0.003,
              b_cl4 = 0.006;
            if (bc === "ss") {
              return b_ss1 + (b_ss4 - b_ss1) * s;
            } else {
              return b_cl1 + (b_cl4 - b_cl1) * s;
            }
          }

          let wmax_m;
          if (loadType === "uniform") {
            const alpha = alphaUniform(aspect, bc);
            wmax_m = (alpha * q_Pa * Math.pow(a_m, 4)) / D;
          } else {
            const beta = betaPoint(aspect, bc);
            wmax_m = (beta * P_N * Math.pow(a_m, 2)) / D;
          }

          // Approximate bending moment and stress using plate theory scaling
          // Use M ~ k * q * a^2 or kP * P, then sigma = 6M / t^2
          let Mmax_Nm;
          if (loadType === "uniform") {
            const kM = bc === "ss" ? 0.047 : 0.025; // rough factors
            Mmax_Nm = kM * q_Pa * Math.pow(a_m, 2);
          } else {
            const kP = bc === "ss" ? 0.2 : 0.12;
            Mmax_Nm = kP * P_N;
          }
          const sigma_max_Pa = (6 * Mmax_Nm) / (t_m * t_m);

          // Convert outputs back to user units
          let wmax_out, wmax_unit, sigma_out, sigma_unit;
          if (sys === "metric") {
            wmax_out = wmax_m * 1000; // mm
            wmax_unit = "mm";
            sigma_out = sigma_max_Pa / 1e6; // MPa
            sigma_unit = "MPa";
          } else {
            const m_to_in = 39.37007874;
            wmax_out = wmax_m * m_to_in; // in
            wmax_unit = "in";
            sigma_out = sigma_max_Pa / (1000 * 6894.757); // ksi
            sigma_unit = "ksi";
          }

          // Update summary
          wmaxEl.textContent = isFinite(wmax_out)
            ? wmax_out.toFixed(3) + " " + wmax_unit
            : "–";

          const span = sys === "metric" ? a : a; // same variable; just units
          const span_over_defl =
            isFinite(wmax_out) && wmax_out !== 0 ? span / wmax_out : null;
          if (span_over_defl) {
            wmaxRatioEl.textContent =
              "Serviceability: span / deflection ≈ " +
              span_over_defl.toFixed(0);
          } else {
            wmaxRatioEl.textContent = "Serviceability check: –";
          }

          sigmaMaxEl.textContent = isFinite(sigma_out)
            ? sigma_out.toFixed(2) + " " + sigma_unit
            : "–";

          const fy = parseFloat(yieldEl.value);
          if (isFinite(fy) && fy > 0 && isFinite(sigma_out) && sigma_out > 0) {
            const SF = fy / sigma_out;
            safetyEl.textContent =
              "Approximate safety factor vs. yield ≈ " + SF.toFixed(2);
          } else {
            safetyEl.textContent =
              "Safety factor vs. yield: enter yield strength to evaluate.";
          }

          // Build deflection field for visualization using a simple mode shape
          const nxNodes = nx;
          const nyNodes = ny;
          const dx = a_m / (nxNodes - 1);
          const dy = b_m / (nyNodes - 1);

          const wField = [];
          let wmin = Infinity,
            wmax = -Infinity;
          for (let j = 0; j < nyNodes; j++) {
            const y = j * dy;
            const row = [];
            for (let i = 0; i < nxNodes; i++) {
              const x = i * dx;
              // Mode shape approximation: sin(pi x / a) * sin(pi y / b)
              const wx = Math.sin((Math.PI * x) / a_m);
              const wy = Math.sin((Math.PI * y) / b_m);
              const w = wmax_m * wx * wy;
              row.push(w);
              if (w < wmin) wmin = w;
              if (w > wmax) wmax = w;
            }
            wField.push(row);
          }

          drawContour(wField, a_m, b_m, wmin, wmax, sys);
          statusEl.textContent = "Done.";
        }

        function colorMap(t) {
          // t in [0,1] -> blue to red via simple gradient
          const r = Math.round(255 * t);
          const g = Math.round(255 * Math.max(0, 1 - 2 * Math.abs(t - 0.5)));
          const b = Math.round(255 * (1 - t));
          return "rgb(" + r + "," + g + "," + b + ")";
        }

        function drawContour(wField, a_m, b_m, wmin, wmax, sys) {
          const nx = wField[0].length;
          const ny = wField.length;
          ctx.clearRect(0, 0, canvas.width, canvas.height);

          const margin = 10;
          const availW = canvas.width - 2 * margin;
          const availH = canvas.height - 2 * margin;
          const scale = Math.min(availW / a_m, availH / b_m);
          const plateW = a_m * scale;
          const plateH = b_m * scale;
          const offsetX = (canvas.width - plateW) / 2;
          const offsetY = (canvas.height - plateH) / 2;

          const dxPix = plateW / (nx - 1);
          const dyPix = plateH / (ny - 1);

          const range = wmax - wmin || 1;

          for (let j = 0; j < ny - 1; j++) {
            for (let i = 0; i < nx - 1; i++) {
              const w00 = wField[j][i];
              const w10 = wField[j][i + 1];
              const w01 = wField[j + 1][i];
              const w11 = wField[j + 1][i + 1];
              const wAvg = (w00 + w10 + w01 + w11) / 4;
              const t = (wAvg - wmin) / range;
              ctx.fillStyle = colorMap(t);
              const x = offsetX + i * dxPix;
              const y = offsetY + j * dyPix;
              ctx.fillRect(x, y, dxPix + 1, dyPix + 1);
            }
          }

          // border
          ctx.strokeStyle = "#111827";
          ctx.lineWidth = 1;
          ctx.strokeRect(offsetX, offsetY, plateW, plateH);

          // store for hover
          canvas._wField = wField;
          canvas._offsetX = offsetX;
          canvas._offsetY = offsetY;
          canvas._dxPix = dxPix;
          canvas._dyPix = dyPix;
          canvas._a_m = a_m;
          canvas._b_m = b_m;
          canvas._sys = sys;
        }

        canvas.addEventListener("mousemove", function (e) {
          const rect = canvas.getBoundingClientRect();
          const xPix = e.clientX - rect.left;
          const yPix = e.clientY - rect.top;
          const wField = canvas._wField;
          if (!wField) return;
          const offsetX = canvas._offsetX;
          const offsetY = canvas._offsetY;
          const dxPix = canvas._dxPix;
          const dyPix = canvas._dyPix;
          const a_m = canvas._a_m;
          const b_m = canvas._b_m;
          const sys = canvas._sys;

          if (
            xPix < offsetX ||
            xPix > offsetX + dxPix * (wField[0].length - 1) ||
            yPix < offsetY ||
            yPix > offsetY + dyPix * (wField.length - 1)
          ) {
            hoverInfoEl.textContent = "x = –, y = –, w = –";
            return;
          }

          const i = Math.round((xPix - offsetX) / dxPix);
          const j = Math.round((yPix - offsetY) / dyPix);
          const w_m = wField[j][i];
          const x_m = (i / (wField[0].length - 1)) * a_m;
          const y_m = (j / (wField.length - 1)) * b_m;

          let xOut, yOut, wOut, lenUnit, wUnit;
          if (sys === "metric") {
            xOut = x_m * 1000;
            yOut = y_m * 1000;
            wOut = w_m * 1000;
            lenUnit = "mm";
            wUnit = "mm";
          } else {
            const m_to_in = 39.37007874;
            xOut = x_m * m_to_in;
            yOut = y_m * m_to_in;
            wOut = w_m * m_to_in;
            lenUnit = "in";
            wUnit = "in";
          }

          hoverInfoEl.textContent =
            "x = " +
            xOut.toFixed(1) +
            " " +
            lenUnit +
            ", y = " +
            yOut.toFixed(1) +
            " " +
            lenUnit +
            ", w = " +
            wOut.toFixed(3) +
            " " +
            wUnit;
        });

        canvas.addEventListener("mouseleave", function () {
          hoverInfoEl.textContent = "x = –, y = –, w = –";
        });

        unitSystemEl.addEventListener("change", function () {
          lastField = "switch";
          setUnits();
          clearResults();
        });

        loadTypeEl.addEventListener("change", function () {
          setLoadVisibility();
          clearResults();
        });

        resetBtn.addEventListener("click", function () {
          lastField = null;
          unitSystemEl.value = "metric";
          bcTypeEl.value = "ss";
          loadTypeEl.value = "uniform";
          setUnits();
          setLoadVisibility();
          poissonEl.value = 0.3;
          meshNxEl.value = 16;
          meshNyEl.value = 8;
          yieldEl.value = "";
          clearResults();
          statusEl.textContent = "";
        });

        calcBtn.addEventListener("click", compute);

        // initial
        setUnits();
        setLoadVisibility();
      })();
    </script>
  
