            category: "moderate",
          },
          {
            min: 4.1,
            max: Infinity,
            intensity: "Above typical guideline dose",
            category: "above",
          },
        ],
      };

      function classifyGuideline(drug, dose) {
        const table = statinTable[drug];
        if (!table) return null;
        for (const row of table) {
          if (dose >= row.min && dose <= row.max) {
            return row;
          }
        }
        return null;
      }

      function formatCategoryBadge(category) {
        let label = "";
        let color = "";
        if (category === "high") {
          label = "High-intensity";
          color = "bg-red-100 text-red-800 border border-red-200";
        } else if (category === "moderate" || category === "moderate-high") {
          label =
            category === "moderate"
              ? "Moderate-intensity"
              : "Moderate–High (approx.)";
          color = "bg-yellow-100 text-yellow-800 border border-yellow-200";
        } else if (category === "low" || category === "low-moderate") {
          label =
            category === "low" ? "Low-intensity" : "Low–Moderate (approx.)";
          color = "bg-green-100 text-green-800 border border-green-200";
        } else if (category === "above") {
          label = "Above typical guideline dose";
          color = "bg-purple-100 text-purple-800 border border-purple-200";
        } else {
          label = "Not classified";
          color = "bg-gray-100 text-gray-700 border border-gray-200";
        }
        return `<span class="badge ${color}">${label}</span>`;
      }

      document.addEventListener("DOMContentLoaded", () => {
        const drugEl = document.getElementById("statinDrug");
        const doseEl = document.getElementById("statinDose");
        const baselineEl = document.getElementById("baselineLdl");
        const onTxEl = document.getElementById("onTreatmentLdl");
        const classifyBtn = document.getElementById("classifyBtn");
        const resetBtn = document.getElementById("resetBtn");

        const resultsCard = document.getElementById("resultsCard");
        const intensityLabel = document.getElementById("intensityLabel");
        const intensityDescription = document.getElementById(
          "intensityDescription"
        );
        const guidelineInfo = document.getElementById("guidelineInfo");
        const ldlReductionInfo = document.getElementById("ldlReductionInfo");
        const warningText = document.getElementById("warningText");

        classifyBtn.addEventListener("click", () => {
          const drug = drugEl.value;
          const dose = parseFloat(doseEl.value);

          if (!drug) {
            alert("Please select a statin.");
            return;
          }
          if (isNaN(dose) || dose <= 0) {
            alert("Please enter a valid daily dose in mg.");
            return;
          }

          const guideline = classifyGuideline(drug, dose);
          let mainCategory = guideline ? guideline.category : "unknown";

          // Build main label
          let labelText = "";
          let descText = "";
          if (!guideline) {
            labelText = "Not classified";
            descText =
              "This dose is not mapped in the standard guideline table. Check the latest cholesterol guideline or a drug reference.";
          } else {
            labelText = guideline.intensity;
            if (mainCategory === "high") {
              descText =
                "High-intensity statin therapy is typically expected to reduce LDL-C by ≥50% on average in clinical trials.";
            } else if (
              mainCategory === "moderate" ||
              mainCategory === "moderate-high"
            ) {
              descText =
                "Moderate-intensity statin therapy is typically expected to reduce LDL-C by about 30–49% on average.";
            } else if (
              mainCategory === "low" ||
              mainCategory === "low-moderate"
            ) {
              descText =
                "Low-intensity statin therapy is typically expected to reduce LDL-C by <30% on average.";
            } else if (mainCategory === "above") {
              descText =
                "This dose is above the usual guideline range. Confirm appropriateness and safety in a current drug reference.";
            } else {
              descText =
                "This regimen does not fall into a standard guideline intensity category.";
            }
          }

          intensityLabel.textContent = labelText;
          intensityDescription.textContent = descText;

          const drugName = drugEl.options[drugEl.selectedIndex].text;
          guidelineInfo.innerHTML = guideline
            ? `${formatCategoryBadge(
                mainCategory
              )}<br>${drugName} ${dose} mg daily is classified as: <strong>${
                guideline.intensity
              }</strong> in most adult guidelines.`
            : `No standard intensity classification found for ${drugName} ${dose} mg.`;

          // LDL reduction calculation
          const baseline = parseFloat(baselineEl.value);
          const onTx = parseFloat(onTxEl.value);
          let reductionInfo = "Not calculated (LDL-C values not provided).";
          let warn = "";

          if (!isNaN(baseline) && !isNaN(onTx) && baseline > 0 && onTx >= 0) {
            const reduction = ((baseline - onTx) / baseline) * 100;
            const rounded = Math.round(reduction * 10) / 10;
            let obsCategory = "";
            if (reduction >= 50)
              obsCategory = "high-intensity response (≥50% reduction)";
            else if (reduction >= 30)
              obsCategory = "moderate-intensity response (30–49% reduction)";
            else if (reduction > 0)
              obsCategory = "low-intensity response (<30% reduction)";
            else obsCategory = "no reduction or increase in LDL-C";

            reductionInfo = `Estimated observed LDL-C reduction: <strong>${
              isFinite(rounded) ? rounded + "%" : "N/A"
            }</strong> (${obsCategory}).`;

            if (
              guideline &&
              (mainCategory === "high" ||
                mainCategory === "moderate" ||
                mainCategory === "moderate-high")
            ) {
              if (reduction < 30) {
                warn =
                  "Observed LDL-C reduction is lower than expected for this intensity. Consider adherence, secondary causes, and potential need for therapy adjustment.";
              }
            }
          }

          ldlReductionInfo.innerHTML = reductionInfo;
          warningText.textContent = warn;

          resultsCard.classList.remove("hidden");
          resultsCard.scrollIntoView({ behavior: "smooth", block: "start" });
        });

        resetBtn.addEventListener("click", () => {
          drugEl.value = "";
          doseEl.value = "";
          baselineEl.value = "";
          onTxEl.value = "";
          resultsCard.classList.add("hidden");
          intensityLabel.textContent = "";
          intensityDescription.textContent = "";
          guidelineInfo.textContent = "";
          ldlReductionInfo.textContent = "";
          warningText.textContent = "";
        });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
