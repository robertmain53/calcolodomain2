    <script>
      // Direction toggle
      const dirEnItBtn = document.getElementById("dir-en-it");
      const dirItEnBtn = document.getElementById("dir-it-en");
      let direction = "en-it";

      dirEnItBtn.addEventListener("click", () => {
        direction = "en-it";
        dirEnItBtn.classList.remove(
          "border-gray-300",
          "text-gray-700",
          "bg-white"
        );
        dirEnItBtn.classList.add(
          "border-blue-500",
          "text-blue-600",
          "bg-blue-50"
        );
        dirItEnBtn.classList.remove(
          "border-blue-500",
          "text-blue-600",
          "bg-blue-50"
        );
        dirItEnBtn.classList.add(
          "border-gray-300",
          "text-gray-700",
          "bg-white"
        );
      });

      dirItEnBtn.addEventListener("click", () => {
        direction = "it-en";
        dirItEnBtn.classList.remove(
          "border-gray-300",
          "text-gray-700",
          "bg-white"
        );
        dirItEnBtn.classList.add(
          "border-blue-500",
          "text-blue-600",
          "bg-blue-50"
        );
        dirEnItBtn.classList.remove(
          "border-blue-500",
          "text-blue-600",
          "bg-blue-50"
        );
        dirEnItBtn.classList.add(
          "border-gray-300",
          "text-gray-700",
          "bg-white"
        );
      });

      // Phrase generator
      const examples = {
        "en-it": {
          invoice: [
            { en: "miscellaneous expenses", it: "miscellaneous expenses" },
            { en: "miscellaneous income", it: "proventi vari" },
            { en: "miscellaneous fees", it: "commissioni varie" },
          ],
          business: [
            { en: "miscellaneous items", it: "voci varie" },
            { en: "miscellaneous information", it: "informazioni varie" },
            { en: "miscellaneous documents", it: "documenti vari" },
          ],
          everyday: [
            { en: "miscellaneous stuff", it: "cose varie" },
            {
              en: "a box of miscellaneous things",
              it: "una scatola con cose varie",
            },
            { en: "miscellaneous notes", it: "appunti vari" },
          ],
          legal: [
            { en: "Miscellaneous provisions", it: "Disposizioni varie" },
            { en: "Miscellaneous", it: "Varie ed eventuali" },
            { en: "Miscellaneous clauses", it: "Clausole varie" },
          ],
          other: [
            { en: "miscellaneous items", it: "elementi vari" },
            { en: "miscellaneous remarks", it: "osservazioni varie" },
            { en: "miscellaneous charges", it: "oneri vari" },
          ],
        },
        "it-en": {
          invoice: [
            { it: "miscellaneous expenses", en: "miscellaneous expenses" },
            { it: "proventi vari", en: "miscellaneous income" },
            { it: "oneri vari", en: "miscellaneous charges" },
          ],
          business: [
            { it: "voci varie", en: "miscellaneous items" },
            { it: "informazioni varie", en: "miscellaneous information" },
            { it: "documenti vari", en: "miscellaneous documents" },
          ],
          everyday: [
            { it: "cose varie", en: "miscellaneous stuff" },
            { it: "oggetti vari", en: "miscellaneous items" },
            { it: "appunti vari", en: "miscellaneous notes" },
          ],
          legal: [
            { it: "Disposizioni varie", en: "Miscellaneous provisions" },
            { it: "Varie ed eventuali", en: "Miscellaneous" },
            { it: "Clausole varie", en: "Miscellaneous clauses" },
          ],
          other: [
            { it: "elementi vari", en: "miscellaneous elements" },
            { it: "osservazioni varie", en: "miscellaneous remarks" },
            { it: "miscellaneous expenses", en: "miscellaneous expenses" },
          ],
        },
      };

      function buildExamples(dir, ctx, noun) {
        const list = examples[dir][ctx] || [];
        const rows = list
          .map((pair) => {
            let source, target, labelSource, labelTarget;
            if (dir === "en-it") {
              source = pair.en;
              target = pair.it;
              labelSource = "EN";
              labelTarget = "IT";
            } else {
              source = pair.it;
              target = pair.en;
              labelSource = "IT";
              labelTarget = "EN";
            }

            // If user provided a noun, adapt the phrase where possible
            if (noun) {
              if (dir === "en-it") {
                source = "miscellaneous " + noun.toLowerCase();
                // simple heuristic for Italian
                if (ctx === "invoice") {
                  target = "miscellaneous expenses";
                } else if (ctx === "business") {
                  target = noun.toLowerCase() + " varie";
                } else {
                  target = noun.toLowerCase() + " varie";
                }
              } else {
                // it-en direction with custom noun: assume Italian noun + "varie"
                source = noun.toLowerCase() + " varie";
                target = "miscellaneous " + noun.toLowerCase();
              }
            }

            return `
        <div class="border border-gray-200 rounded-md p-2">
          <div class="flex justify-between text-xs text-gray-500 mb-1">
            <span>${labelSource}</span><span>${labelTarget}</span>
          </div>
          <div class="flex justify-between gap-3">
            <div class="flex-1 font-medium">${source}</div>
            <div class="flex-1 text-gray-800">${target}</div>
          </div>
        </div>
      `;
          })
          .join("");

        return (
          rows || '<p class="text-gray-500 text-sm">No examples available.</p>'
        );
      }

      document.getElementById("generateBtn").addEventListener("click", () => {
        const ctx = document.getElementById("context").value;
        const noun = document.getElementById("noun").value.trim();
        const html = buildExamples(direction, ctx, noun);
        const results = document.getElementById("results");
        const resultsContent = document.getElementById("resultsContent");
        resultsContent.innerHTML = html;
        results.classList.remove("hidden");
      });

      // Simple inline FAQ toggle (in addition to global script_faq, for robustness)
      document
        .querySelectorAll("#faq-container .faq-question")
        .forEach((btn) => {
          btn.addEventListener("click", () => {
            const answer = btn.parentElement.querySelector(".faq-answer");
            const icon = btn.querySelector("span:last-child");
            const isHidden = answer.classList.contains("hidden");
            document
              .querySelectorAll("#faq-container .faq-answer")
              .forEach((a) => a.classList.add("hidden"));
            document
              .querySelectorAll("#faq-container .faq-question span:last-child")
              .forEach((i) => (i.textContent = "+"));
            if (isHidden) {
              answer.classList.remove("hidden");
              icon.textContent = "−";
            }
          });
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
