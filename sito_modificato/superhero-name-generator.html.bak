            "Lumina",
          ],
          adjectives: ["Golden", "Radiant", "Holy", "Shining", "Pure", "Solar"],
          nouns: ["Guardian", "Saint", "Angel", "Paragon"],
        },
        speed: {
          cores: [
            "Velocity",
            "Dash",
            "Bolt",
            "Flash",
            "Rush",
            "Vortex",
            "Sprint",
          ],
          adjectives: ["Swift", "Rapid", "Blitz", "Turbo", "Lightning"],
          nouns: ["Runner", "Striker", "Comet"],
        },
        strength: {
          cores: [
            "Titan",
            "Colossus",
            "Atlas",
            "Bulwark",
            "Rampart",
            "Juggernaut",
          ],
          adjectives: ["Iron", "Steel", "Stone", "Unbreakable", "Mighty"],
          nouns: ["Giant", "Guardian", "Hammer"],
        },
        mind: {
          cores: ["Psi", "Oracle", "Mindstorm", "Psyche", "Mnemonic", "Echo"],
          adjectives: ["Psychic", "Mental", "Arcane", "Cerebral"],
          nouns: ["Seer", "Architect", "Whisper"],
        },
        nature: {
          cores: ["Thorn", "Briar", "Grove", "Fang", "Talon", "Vine", "Howl"],
          adjectives: ["Wild", "Emerald", "Savage", "Primal"],
          nouns: ["Druid", "Warden", "Ranger"],
        },
        time: {
          cores: ["Chronos", "Epoch", "Rift", "Paradox", "Continuum"],
          adjectives: ["Temporal", "Eternal", "Infinite", "Clockwork"],
          nouns: ["Keeper", "Warden", "Traveler"],
        },
        magic: {
          cores: ["Hex", "Rune", "Arcana", "Spellfire", "Mythos"],
          adjectives: ["Arcane", "Mystic", "Eldritch", "Enchanted"],
          nouns: ["Mage", "Warlock", "Witch", "Sorcerer", "Enchanter"],
        },
      };

      const titles = {
        hero: [
          "Captain",
          "Doctor",
          "Agent",
          "Guardian",
          "Sentinel",
          "Commander",
          "Major",
          "Star",
          "Knight",
        ],
        antihero: ["Rogue", "Shade", "Rebel", "Stray", "Nomad", "Vigil"],
        villain: [
          "Lord",
          "Baron",
          "Empress",
          "Overlord",
          "Warlord",
          "Dread",
          "Doctor",
          "Count",
        ],
      };

      const darkWords = [
        "Doom",
        "Ruin",
        "Oblivion",
        "Ravage",
        "Malice",
        "Vengeance",
        "Grim",
        "Dire",
        "Razor",
        "Venom",
      ];
      const heroicWords = [
        "Guardian",
        "Sentinel",
        "Valor",
        "Justice",
        "Hope",
        "Aegis",
        "Shield",
        "Beacon",
        "Paragon",
      ];
      const antiHeroWords = [
        "Vigil",
        "Reckoning",
        "Rift",
        "Edge",
        "Shade",
        "Crossfire",
        "Outlaw",
      ];

      const feminineEndings = ["a", "ia", "elle", "ara", "ina", "essa"];
      const masculineEndings = ["or", "ar", "us", "on", "ax", "en"];

      function pickTheme(power) {
        if (power === "random") {
          const keys = Object.keys(dataPools);
          return rand(keys);
        }
        return power;
      }

      function pickPersonality(personality) {
        if (personality === "random") {
          return rand([
            "noble",
            "brooding",
            "chaotic",
            "strategist",
            "playful",
            "ruthless",
          ]);
        }
        return personality;
      }

      function pickOrigin(origin) {
        if (origin === "random") {
          return rand([
            "accident",
            "alien",
            "legacy",
            "curse",
            "training",
            "artifact",
          ]);
        }
        return origin;
      }

      function pickTone(tone) {
        if (tone === "random") {
          return rand(["neutral", "feminine", "masculine"]);
        }
        return tone;
      }

      function adjustForTone(core, tone) {
        if (tone === "neutral") return core;
        const endings =
          tone === "feminine" ? feminineEndings : masculineEndings;
        if (chance(0.5)) {
          return core + rand(endings);
        }
        return core;
      }

      function integrateKeywords(keywords) {
        if (!keywords) return [];
        return keywords
          .split(",")
          .map((k) => k.trim())
          .filter(Boolean)
          .map((k) => capitalize(k));
      }

      function buildName(options) {
        const {
          alignment,
          theme,
          personality,
          origin,
          tone,
          realName,
          keywords,
        } = options;
        const pool = dataPools[theme] || dataPools.elemental;
        const kw = integrateKeywords(keywords);

        const coreBase = kw.length && chance(0.6) ? rand(kw) : rand(pool.cores);
        const core = adjustForTone(coreBase, tone);
        const adj = rand(pool.adjectives);
        const noun = rand(pool.nouns);

        let alignmentWords;
        if (alignment === "villain") alignmentWords = darkWords;
        else if (alignment === "antihero") alignmentWords = antiHeroWords;
        else alignmentWords = heroicWords;

        const alignWord = rand(alignmentWords);
        const title = rand(titles[alignment] || titles.hero);

        const patterns = [];

        // Pattern weights by alignment
        if (alignment === "hero") {
          patterns.push(
            "theAdjNoun",
            "coreNoun",
            "titleCore",
            "mononym",
            "nounOfElement"
          );
        } else if (alignment === "villain") {
          patterns.push(
            "titleCore",
            "theAdjNoun",
            "darkMononym",
            "nounOfElement"
          );
        } else {
          patterns.push("coreNoun", "theAdjNoun", "darkMononym", "mononym");
        }

        const pattern = rand(patterns);

        let name;
        switch (pattern) {
          case "theAdjNoun":
            name = `The ${adj} ${noun}`;
            break;
          case "coreNoun":
            name = `${core} ${alignWord}`;
            break;
          case "titleCore":
            name = `${title} ${core}`;
            break;
          case "nounOfElement":
            name = `${alignWord} of ${core}`;
            break;
          case "darkMononym":
            name = rand([core, alignWord, noun]);
            break;
          case "mononym":
          default:
            name = core;
            break;
        }

        // Occasionally add real name pattern
        if (realName && chance(0.25)) {
          const shortReal = realName.split(" ")[0];
          if (chance(0.5)) {
            name = `${name} (${shortReal})`;
          } else {
            name = `${shortReal}, the ${adj} ${noun}`;
          }
        }

        return name.replace(/\s+/g, " ").trim();
      }

      function generateNames(count) {
        const realName = document.getElementById("real-name").value.trim();
        const alignment =
          document.querySelector(
            ".align-btn.bg-blue-50, .align-btn.bg-blue-600"
          )?.dataset.align || "hero";
        const powerTheme = document.getElementById("power-theme").value;
        const personality = document.getElementById("personality").value;
        const origin = document.getElementById("origin").value;
        const tone = document.getElementById("gender-tone").value;
        const keywords = document.getElementById("custom-keywords").value;

        const theme = pickTheme(powerTheme);
        const pers = pickPersonality(personality);
        const orig = pickOrigin(origin);
        const tn = pickTone(tone);

        const options = {
          alignment,
          theme,
          personality: pers,
          origin: orig,
          tone: tn,
          realName,
          keywords,
        };

        const names = new Set();
        while (names.size < count) {
          names.add(buildName(options));
          if (names.size > count * 3) break; // safety
        }
        return Array.from(names);
      }

      function renderResults(names) {
        const container = document.getElementById("results-container");
        container.innerHTML = "";

        if (!names.length) {
          container.innerHTML =
            '<p class="text-xs text-gray-500">No names generated yet.</p>';
          return;
        }

        const list = document.createElement("ul");
        list.className = "space-y-2";

        names.forEach((name) => {
          const li = document.createElement("li");
          li.className =
            "flex items-center justify-between gap-2 bg-white rounded-md px-2 py-1 border border-gray-200";

          const left = document.createElement("div");
          left.className = "flex items-center gap-2";

          const checkbox = document.createElement("input");
          checkbox.type = "checkbox";
          checkbox.className =
            "result-checkbox h-4 w-4 text-blue-600 border-gray-300 rounded";
          checkbox.setAttribute("aria-label", `Select ${name}`);

          const span = document.createElement("span");
          span.textContent = name;
          span.className = "text-sm font-medium result-badge";

          left.appendChild(checkbox);
          left.appendChild(span);

          const favBtn = document.createElement("button");
          favBtn.type = "button";
          favBtn.className =
            "favorite-btn text-xs text-amber-500 hover:text-amber-600";
          favBtn.innerHTML = "☆";
          favBtn.setAttribute("aria-label", `Favorite ${name}`);
          favBtn.dataset.name = name;

          li.appendChild(left);
          li.appendChild(favBtn);
          list.appendChild(li);
        });

        container.appendChild(list);
        updateActionButtonsState();
      }

      function updateActionButtonsState() {
        const anyChecked = !!document.querySelector(
          "#results-container .result-checkbox:checked"
        );
        const copyBtn = document.getElementById("copy-selected-btn");
        const saveBtn = document.getElementById("save-favorites-btn");
        copyBtn.disabled = !anyChecked;
        saveBtn.disabled = !anyChecked;
      }

      function getSelectedNames() {
        const checkboxes = document.querySelectorAll(
          "#results-container .result-checkbox:checked"
        );
        const names = [];
        checkboxes.forEach((cb) => {
          const label = cb.parentElement.querySelector("span");
          if (label) names.push(label.textContent);
        });
        return names;
      }

      function copySelectedToClipboard() {
        const names = getSelectedNames();
        if (!names.length) return;
        const text = names.join("\n");
        navigator.clipboard.writeText(text).catch(() => {});
      }

      function addFavorites(names) {
        if (!names.length) return;
        const container = document.getElementById("favorites-container");
        const placeholder = container.querySelector("p.text-xs");
        if (placeholder) placeholder.remove();

        let list = container.querySelector("ul");
        if (!list) {
          list = document.createElement("ul");
          list.className = "flex flex-wrap gap-2";
          container.appendChild(list);
        }

        const existing = new Set(
          Array.from(list.querySelectorAll("span")).map((s) => s.textContent)
        );

        names.forEach((name) => {
          if (existing.has(name)) return;
          const li = document.createElement("li");
          li.className =
            "flex items-center gap-1 bg-blue-50 text-blue-800 px-2 py-1 rounded-full text-xs border border-blue-200";
          const span = document.createElement("span");
          span.textContent = name;
          const removeBtn = document.createElement("button");
          removeBtn.type = "button";
          removeBtn.className = "text-[10px] text-blue-500 hover:text-blue-700";
          removeBtn.textContent = "×";
          removeBtn.setAttribute("aria-label", `Remove ${name} from favorites`);
          removeBtn.addEventListener("click", () => {
            li.remove();
            if (!list.children.length) {
              document.getElementById("favorites-container").innerHTML =
                '<p class="text-xs text-gray-500">Click the star next to any generated name to save it here.</p>';
              document
                .getElementById("clear-favorites-btn")
                .classList.add("hidden");
            }
          });
          li.appendChild(span);
          li.appendChild(removeBtn);
          list.appendChild(li);
          existing.add(name);
        });

        document
          .getElementById("clear-favorites-btn")
          .classList.remove("hidden");
      }

      function clearFavorites() {
        const container = document.getElementById("favorites-container");
        container.innerHTML =
          '<p class="text-xs text-gray-500">Click the star next to any generated name to save it here.</p>';
        document.getElementById("clear-favorites-btn").classList.add("hidden");
      }

      function randomizeInputs() {
        const alignButtons = document.querySelectorAll(".align-btn");
        alignButtons.forEach((btn) => {
          btn.classList.remove(
            "bg-blue-50",
            "border-blue-500",
            "text-blue-600"
          );
          btn.classList.add("border-gray-300", "text-gray-700", "bg-white");
        });
        const randomAlign = rand(["hero", "antihero", "villain"]);
        const activeBtn = document.querySelector(
          `.align-btn[data-align="${randomAlign}"]`
        );
        if (activeBtn) {
          activeBtn.classList.remove(
            "border-gray-300",
            "text-gray-700",
            "bg-white"
          );
          activeBtn.classList.add(
            "border-blue-500",
            "text-blue-600",
            "bg-blue-50"
          );
        }

        const selects = ["power-theme", "personality", "origin", "gender-tone"];
        selects.forEach((id) => {
          const sel = document.getElementById(id);
          if (!sel) return;
          const opts = Array.from(sel.options);
          sel.value = rand(opts).value;
        });

        const kwSamples = [
          "raven, eclipse, neon",
          "storm, shield, nova",
          "shadow, fang, hex",
          "quantum, cipher, pulse",
        ];
        if (chance(0.6)) {
          document.getElementById("custom-keywords").value = rand(kwSamples);
        }
      }

      // Event listeners
      document.addEventListener("click", (e) => {
        if (e.target.classList.contains("align-btn")) {
          const buttons = document.querySelectorAll(".align-btn");
          buttons.forEach((btn) => {
            btn.classList.remove(
              "bg-blue-50",
              "border-blue-500",
              "text-blue-600"
            );
            btn.classList.add("border-gray-300", "text-gray-700", "bg-white");
          });
          e.target.classList.remove(
            "border-gray-300",
            "text-gray-700",
            "bg-white"
          );
          e.target.classList.add(
            "border-blue-500",
            "text-blue-600",
            "bg-blue-50"
          );
        }

        if (e.target.classList.contains("favorite-btn")) {
          const name = e.target.dataset.name;
          addFavorites([name]);
          e.target.textContent = "★";
          e.target.classList.add("text-amber-600");
        }
      });

      document.addEventListener("change", (e) => {
        if (e.target.classList.contains("result-checkbox")) {
          updateActionButtonsState();
        }
      });

      document.getElementById("generate-btn").addEventListener("click", () => {
        const count =
          parseInt(document.getElementById("count").value, 10) || 10;
        const names = generateNames(count);
        renderResults(names);
      });

      document.getElementById("randomize-btn").addEventListener("click", () => {
        randomizeInputs();
        const count =
          parseInt(document.getElementById("count").value, 10) || 10;
        const names = generateNames(count);
        renderResults(names);
      });

      document
        .getElementById("copy-selected-btn")
        .addEventListener("click", () => {
          copySelectedToClipboard();
        });

      document
        .getElementById("save-favorites-btn")
        .addEventListener("click", () => {
          const names = getSelectedNames();
          addFavorites(names);
        });

      document
        .getElementById("clear-favorites-btn")
        .addEventListener("click", () => {
          clearFavorites();
        });

      // Initial alignment styling
      (function initAlignment() {
        const heroBtn = document.querySelector('.align-btn[data-align="hero"]');
        if (heroBtn) {
          heroBtn.classList.remove(
            "border-gray-300",
            "text-gray-700",
            "bg-white"
          );
          heroBtn.classList.add(
            "border-blue-500",
            "text-blue-600",
            "bg-blue-50"
          );
        }
      })();
    </script>

