    <script>
      // Utility helpers
      function rand(arr) {
        return arr[Math.floor(Math.random() * arr.length)];
      }
      function chance(p) {
        return Math.random() < p;
      }
      function capitalize(word) {
        return word.charAt(0).toUpperCase() + word.slice(1);
      }

      // Data pools
      const dataPools = {
