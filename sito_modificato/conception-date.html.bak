    <script>
      // Utility functions
      function parseDateInput(id) {
        const val = document.getElementById(id).value;
        if (!val) return null;
        const d = new Date(val + "T12:00:00"); // avoid TZ issues
        return isNaN(d.getTime()) ? null : d;
      }

      function addDays(date, days) {
        const d = new Date(date.getTime());
        d.setDate(d.getDate() + days);
        return d;
      }

      function diffInDays(a, b) {
        const msPerDay = 24 * 60 * 60 * 1000;
        return Math.round((a - b) / msPerDay);
      }

      function formatDate(date) {
        if (!date) return "—";
        const opts = { year: "numeric", month: "short", day: "numeric" };
        return date.toLocaleDateString(undefined, opts);
      }

      function todayMidday() {
        const t = new Date();
        return new Date(t.getFullYear(), t.getMonth(), t.getDate(), 12, 0, 0);
      }

      function clamp(val, min, max) {
        return Math.min(max, Math.max(min, val));
      }

      // Method tab switching
      document.querySelectorAll(".method-tab").forEach((btn) => {
        btn.addEventListener("click", () => {
          const method = btn.getAttribute("data-method");
          document.querySelectorAll(".method-tab").forEach((b) => {
            b.classList.remove(
              "border-blue-500",
              "bg-blue-50",
              "text-blue-700",
              "font-medium"
            );
            b.classList.add("border-gray-300", "bg-white", "text-gray-700");
          });
          btn.classList.remove("border-gray-300", "bg-white", "text-gray-700");
          btn.classList.add(
            "border-blue-500",
            "bg-blue-50",
            "text-blue-700",
            "font-medium"
          );

          document
            .querySelectorAll(".method-panel")
            .forEach((p) => p.classList.add("hidden"));
          const panel = document.getElementById("method-" + method);
          if (panel) panel.classList.remove("hidden");
        });
      });

      function showError(msg) {
        const el = document.getElementById("error-msg");
        el.textContent = msg;
        el.classList.remove("hidden");
      }

      function clearError() {
        const el = document.getElementById("error-msg");
        el.textContent = "";
        el.classList.add("hidden");
      }

      function buildResultsHTML(data) {
        const {
          methodLabel,
          lmpDate,
          conceptionDate,
          fertileStart,
          fertileEnd,
          dueDate,
          gestWeeks,
          gestDays,
        } = data;

        const today = todayMidday();
        const gaStr =
          gestWeeks != null && gestDays != null
            ? `${gestWeeks} week${gestWeeks !== 1 ? "s" : ""} ${gestDays} day${
                gestDays !== 1 ? "s" : ""
              }`
            : "—";

        return `
      <p class="mb-3 text-gray-700"><strong>Method used:</strong> ${methodLabel}</p>
      <div class="grid grid-cols-1 md:grid-cols-2 gap-4 mb-3">
        <div>
          <p><span class="result-label font-semibold">Estimated LMP:</span> ${formatDate(
            lmpDate
          )}</p>
          <p><span class="result-label font-semibold">Likely conception:</span> ${formatDate(
            conceptionDate
          )}</p>
          <p><span class="result-label font-semibold">Fertile window:</span> ${formatDate(
            fertileStart
          )} – ${formatDate(fertileEnd)}</p>
        </div>
        <div>
          <p><span class="result-label font-semibold">Estimated due date:</span> ${formatDate(
            dueDate
          )}</p>
          <p><span class="result-label font-semibold">Today:</span> ${formatDate(
            today
          )}</p>
          <p><span class="result-label font-semibold">Gestational age:</span> ${gaStr}</p>
        </div>
      </div>
      <p class="text-xs text-gray-500 mt-2">
        This is an educational estimate based on typical 40-week pregnancy length and average ovulation timing.
        Always discuss pregnancy dating and any concerns with your healthcare provider.
      </p>
    `;
      }

      function buildTimelineHTML(data) {
        const { lmpDate, dueDate } = data;
        if (!lmpDate || !dueDate) {
          return '<p class="text-gray-500">Timeline is unavailable. Please check your inputs and calculate again.</p>';
        }

        const today = todayMidday();
        const daysFromLmp = diffInDays(today, lmpDate);
        const gestWeeks = clamp(Math.floor(daysFromLmp / 7), 0, 42);
        const gestDays = clamp(daysFromLmp - gestWeeks * 7, 0, 6);

        const firstTrimesterEnd = addDays(lmpDate, 13 * 7 + 6); // 13w6d
        const secondTrimesterEnd = addDays(lmpDate, 27 * 7 + 6); // 27w6d
        const thirdTrimesterStart = addDays(lmpDate, 28 * 7); // 28w0d

        return `
      <div class="grid grid-cols-1 md:grid-cols-2 gap-4 text-sm">
        <div>
          <h3 class="font-semibold mb-1">Current status</h3>
          <p><span class="result-label font-semibold">Gestational age:</span> ${gestWeeks}w ${gestDays}d</p>
          <p><span class="result-label font-semibold">Days pregnant:</span> ${Math.max(
            0,
            daysFromLmp
          )} days</p>
          <p><span class="result-label font-semibold">Days until due date:</span> ${Math.max(
            0,
            diffInDays(dueDate, today)
          )} days</p>
        </div>
        <div>
          <h3 class="font-semibold mb-1">Key milestones</h3>
          <p><span class="result-label font-semibold">End of 1st trimester:</span> ${formatDate(
            firstTrimesterEnd
          )}</p>
          <p><span class="result-label font-semibold">End of 2nd trimester:</span> ${formatDate(
            secondTrimesterEnd
          )}</p>
          <p><span class="result-label font-semibold">Start of 3rd trimester:</span> ${formatDate(
            thirdTrimesterStart
          )}</p>
        </div>
      </div>
      <p class="text-xs text-gray-500 mt-3">
        Trimester boundaries are approximate and may vary slightly between guidelines. Your clinician may use different cutoffs based on your specific situation.
      </p>
    `;
      }

      document.getElementById("calculate-btn").addEventListener("click", () => {
        clearError();
        const activeMethodBtn = Array.from(
          document.querySelectorAll(".method-tab")
        ).find((b) => b.classList.contains("border-blue-500"));
        const method = activeMethodBtn
          ? activeMethodBtn.getAttribute("data-method")
          : "due-date";

        const today = todayMidday();
        let lmpDate = null;
        let conceptionDate = null;
        let fertileStart = null;
        let fertileEnd = null;
        let dueDate = null;
        let methodLabel = "";

        try {
          if (method === "due-date") {
            methodLabel = "Due date based estimate";
            const edd = parseDateInput("due-date");
            const cycleLen =
              parseInt(document.getElementById("due-cycle-length").value, 10) ||
              28;

            if (!edd) {
              showError("Please enter a valid estimated due date.");
              return;
            }

            // LMP = EDD - 280 days (40 weeks)
            lmpDate = addDays(edd, -280);

            const ovulationOffset = clamp(cycleLen - 14, 8, 20); // typical range
            conceptionDate = addDays(lmpDate, ovulationOffset);

            fertileStart = addDays(conceptionDate, -5);
            fertileEnd = addDays(conceptionDate, 1);

            dueDate = edd;
          } else if (method === "lmp") {
            methodLabel = "Last menstrual period (LMP) based estimate";
            const lmp = parseDateInput("lmp-date");
            const cycleLen =
              parseInt(document.getElementById("lmp-cycle-length").value, 10) ||
              28;

            if (!lmp) {
              showError(
                "Please enter the first day of your last menstrual period."
              );
              return;
            }

            lmpDate = lmp;
            const ovulationOffset = clamp(cycleLen - 14, 8, 20);
            conceptionDate = addDays(lmpDate, ovulationOffset);
            fertileStart = addDays(conceptionDate, -5);
            fertileEnd = addDays(conceptionDate, 1);
            dueDate = addDays(lmpDate, 280);
          } else if (method === "ultrasound") {
            methodLabel = "Ultrasound based estimate";
            const usDate = parseDateInput("us-date");
            const usWeeks = parseInt(
              document.getElementById("us-weeks").value,
              10
            );
            const usDays =
              parseInt(document.getElementById("us-days").value, 10) || 0;

            if (
              !usDate ||
              isNaN(usWeeks) ||
              usWeeks < 4 ||
              usWeeks > 42 ||
              usDays < 0 ||
              usDays > 6
            ) {
              showError(
                "Please enter a valid ultrasound date and gestational age (weeks and days)."
              );
              return;
            }

            const gaDays = usWeeks * 7 + usDays;
            lmpDate = addDays(usDate, -gaDays);
            conceptionDate = addDays(lmpDate, 14); // assume 28-day cycle unless specified otherwise
            fertileStart = addDays(conceptionDate, -5);
            fertileEnd = addDays(conceptionDate, 1);
            dueDate = addDays(lmpDate, 280);
          } else {
            showError("Unknown method selected.");
            return;
          }

          // Basic sanity checks
          if (lmpDate > today && method !== "due-date") {
            showError(
              "The dates entered suggest an LMP in the future. Please double-check your inputs."
            );
            return;
          }

          const daysFromLmp = diffInDays(today, lmpDate);
          const gestWeeks = Math.floor(daysFromLmp / 7);
          const gestDays = daysFromLmp - gestWeeks * 7;

          const resultData = {
            methodLabel,
            lmpDate,
            conceptionDate,
            fertileStart,
            fertileEnd,
            dueDate,
            gestWeeks: isNaN(gestWeeks) ? null : gestWeeks,
            gestDays: isNaN(gestDays) ? null : gestDays,
          };

          document.getElementById("results-container").innerHTML =
            buildResultsHTML(resultData);
          document.getElementById("timeline-container").innerHTML =
            buildTimelineHTML(resultData);
        } catch (e) {
          console.error(e);
          showError(
            "An unexpected error occurred. Please check your inputs and try again."
          );
        }
      });

      document.getElementById("reset-btn").addEventListener("click", () => {
        document.getElementById("conception-form").reset();
        clearError();
        document.getElementById("results-container").innerHTML =
          '<p class="text-gray-500">Enter your information above and click “Calculate conception date” to see your results.</p>';
        document.getElementById("timeline-container").innerHTML =
          '<p class="text-gray-500">Once calculated, you’ll see your current gestational age, key trimester dates, and an estimated due date here.</p>';
      });

      // Mobile menu script (simple)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", () => {
          const menu = document.getElementById("mobile-menu");
          const expanded = menu.classList.contains("hidden") ? "true" : "false";
          menu.classList.toggle("hidden");
          document
            .getElementById("mobile-menu-toggle")
            .setAttribute("aria-expanded", expanded);
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
