    <script>
      // Utility helpers
      function parseNumber(id) {
        const el = document.getElementById(id);
        if (!el) return NaN;
        const v = parseFloat(el.value);
        return isNaN(v) ? NaN : v;
      }

      function formatPrice(value) {
        if (!isFinite(value)) return "-";
        const abs = Math.abs(value);
        let decimals = 2;
        if (abs > 1000) decimals = 2;
        else if (abs > 1) decimals = 4;
        else if (abs > 0.01) decimals = 5;
        else decimals = 6;
        return value.toFixed(decimals);
      }

      function formatRR(rr) {
        if (!isFinite(rr) || rr <= 0) return "-";
        return rr.toFixed(2) + "R";
      }

      function formatPositionSize(size) {
        if (!isFinite(size) || size <= 0) return "-";
        if (size >= 1000000) return (size / 1000000).toFixed(2) + "M";
        if (size >= 1000) return (size / 1000).toFixed(2) + "k";
        return size.toFixed(2);
      }

      function showError(msg) {
        const el = document.getElementById("errorMsg");
        el.textContent = msg || "";
      }

      function clearResults() {
        document.getElementById("resultsBody").innerHTML = "";
        document.getElementById("resultsContainer").classList.add("hidden");
        document.getElementById("riskSummary").textContent = "";
        document.getElementById("rangeSummary").textContent = "";
      }

      // Level selection buttons
      document.getElementById("selectCommon").addEventListener("click", () => {
        document.querySelectorAll(".fib-level").forEach((cb) => {
          const v = parseFloat(cb.value);
          cb.checked = [
            0, 0.236, 0.382, 0.5, 0.618, 0.786, 1, 1.272, 1.618,
          ].includes(v);
        });
      });

      document.getElementById("selectAll").addEventListener("click", () => {
        document
          .querySelectorAll(".fib-level")
          .forEach((cb) => (cb.checked = true));
      });

      document.getElementById("clearAll").addEventListener("click", () => {
        document
          .querySelectorAll(".fib-level")
          .forEach((cb) => (cb.checked = false));
      });

      // Main calculation
      document.getElementById("calculateBtn").addEventListener("click", () => {
        showError("");
        clearResults();

        const swingLow = parseNumber("swingLow");
        const swingHigh = parseNumber("swingHigh");

        if (!isFinite(swingLow) || !isFinite(swingHigh)) {
          showError("Please enter both swing high and swing low.");
          return;
        }
        if (swingHigh === swingLow) {
          showError("Swing high and low must be different values.");
          return;
        }

        const trend = document.querySelector(
          'input[name="trendDirection"]:checked'
        ).value;
        const isUptrend = trend === "up";

        // Normalize: ensure swingLow < swingHigh for range calculation
        const high = Math.max(swingHigh, swingLow);
        const low = Math.min(swingHigh, swingLow);
        const range = high - low;

        const levels = [];
        document.querySelectorAll(".fib-level:checked").forEach((cb) => {
          const ratio = parseFloat(cb.value);
          const type =
            cb.dataset.type || (ratio <= 1 ? "retracement" : "extension");
          let price, distanceFromSwing;

          if (type === "retracement") {
            if (isUptrend) {
              price = high - range * ratio;
              distanceFromSwing = high - price;
            } else {
              price = low + range * ratio;
              distanceFromSwing = price - low;
            }
          } else {
            // extension
            if (isUptrend) {
              price = high + range * (ratio - 1);
              distanceFromSwing = price - high;
            } else {
              price = low - range * (ratio - 1);
              distanceFromSwing = low - price;
            }
          }

          levels.push({
            type,
            ratio,
            price,
            distanceFromSwing,
          });
        });

        if (!levels.length) {
          showError("Select at least one Fibonacci level.");
          return;
        }

        // Optional risk inputs
        const entry = parseNumber("entryPrice");
        const stop = parseNumber("stopPrice");
        const accountSize = parseNumber("accountSize");
        const riskPercent = parseNumber("riskPercent");

        let haveRisk =
          isFinite(entry) &&
          isFinite(stop) &&
          isFinite(accountSize) &&
          isFinite(riskPercent) &&
          riskPercent > 0 &&
          accountSize > 0 &&
          entry !== stop;

        let riskPerTrade = NaN;
        let positionSize = NaN;
        let perUnitRisk = NaN;

        if (haveRisk) {
          perUnitRisk = Math.abs(entry - stop);
          if (perUnitRisk === 0) {
            haveRisk = false;
          } else {
            riskPerTrade = accountSize * (riskPercent / 100);
            positionSize = riskPerTrade / perUnitRisk;
          }
        }

        // Populate table
        const tbody = document.getElementById("resultsBody");
        levels
          .sort((a, b) => a.price - b.price)
          .forEach((lvl) => {
            let rr = NaN;
            if (haveRisk) {
              const reward = Math.abs(lvl.price - entry);
              rr = reward / perUnitRisk;
            }

            const tr = document.createElement("tr");
            tr.innerHTML = `
          <td class="px-3 py-2 whitespace-nowrap">
            <span class="inline-flex items-center gap-1">
              <span class="w-2 h-2 rounded-full ${
                lvl.type === "retracement" ? "bg-amber-500" : "bg-emerald-500"
              }"></span>
              ${lvl.type === "retracement" ? "Retracement" : "Extension"}
            </span>
          </td>
          <td class="px-3 py-2 whitespace-nowrap">${(lvl.ratio * 100).toFixed(
            1
          )}%</td>
          <td class="px-3 py-2 text-right font-mono">${formatPrice(
            lvl.price
          )}</td>
          <td class="px-3 py-2 text-right font-mono">${formatPrice(
            lvl.distanceFromSwing
          )}</td>
          <td class="px-3 py-2 text-right">${haveRisk ? formatRR(rr) : "-"}</td>
          <td class="px-3 py-2 text-right">${
            haveRisk ? formatPositionSize(positionSize) : "-"
          }</td>
        `;
            tbody.appendChild(tr);
          });

        document.getElementById(
          "rangeSummary"
        ).textContent = `Range: ${formatPrice(low)} → ${formatPrice(
          high
        )} (Δ ${formatPrice(range)})`;

        if (haveRisk) {
          document.getElementById(
            "riskSummary"
          ).textContent = `Risk per trade: ${riskPerTrade.toFixed(
            2
          )} (position size ≈ ${formatPositionSize(
            positionSize
          )} units, per-unit risk ${formatPrice(perUnitRisk)}).`;
        } else {
          document.getElementById("riskSummary").textContent =
            "Tip: Add entry, stop, account size and risk % to see position size and risk/reward for each level.";
        }

        document.getElementById("resultsContainer").classList.remove("hidden");
      });

      // Reset button
      document.getElementById("resetBtn").addEventListener("click", () => {
        [
          "swingLow",
          "swingHigh",
          "entryPrice",
          "stopPrice",
          "accountSize",
          "riskPercent",
        ].forEach((id) => {
          const el = document.getElementById(id);
          if (el) el.value = "";
        });
        document.querySelector(
          'input[name="trendDirection"][value="up"]'
        ).checked = true;
        document.getElementById("selectCommon").click();
        showError("");
        clearResults();
      });

      // Auto-calc on Enter
      [
        "swingLow",
        "swingHigh",
        "entryPrice",
        "stopPrice",
        "accountSize",
        "riskPercent",
      ].forEach((id) => {
        const el = document.getElementById(id);
        if (!el) return;
        el.addEventListener("keydown", (e) => {
          if (e.key === "Enter") {
            e.preventDefault();
            document.getElementById("calculateBtn").click();
          }
        });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
