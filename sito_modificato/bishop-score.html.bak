    <script>
      // Bishop score logic
      function getDilationPoints(range) {
        switch (range) {
          case "0-0":
            return 0;
          case "1-2":
            return 1;
          case "3-4":
            return 2;
          case "5-6":
            return 3;
          default:
            return 0;
        }
      }

      function getEffacementPoints(range) {
        switch (range) {
          case "0-30":
            return 0;
          case "40-50":
            return 1;
          case "60-70":
            return 2;
          case "80-100":
            return 3;
          default:
            return 0;
        }
      }

      function getLengthPoints(val) {
        // val is "3", "2", "1", "0" representing ranges
        switch (val) {
          case "3":
            return 0; // ≥3 cm
          case "2":
            return 1; // 2–2.9
          case "1":
            return 2; // 1–1.9
          case "0":
            return 3; // ≤0.9
          default:
            return 0;
        }
      }

      function getStationPoints(station) {
        switch (station) {
          case "-3":
            return 0;
          case "-2":
            return 1;
          case "-1":
          case "0":
            return 2;
          case "+1":
          case "+2":
            return 3;
          default:
            return 0;
        }
      }

      function getConsistencyPoints(value) {
        switch (value) {
          case "firm":
            return 0;
          case "medium":
            return 1;
          case "soft":
            return 2;
          default:
            return 0;
        }
      }

      function getPositionPoints(value) {
        switch (value) {
          case "posterior":
            return 0;
          case "mid":
            return 1;
          case "anterior":
            return 2;
          default:
            return 0;
        }
      }

      function interpretScore(score) {
        if (score >= 8) {
          return {
            label: "Favorable cervix",
            color: "bg-green-100 text-green-800",
            text:
              "A Bishop score of " +
              score +
              " suggests a favorable cervix. Induction of labor is more likely to result in successful vaginal delivery, assuming no other contraindications.",
          };
        } else if (score >= 6) {
          return {
            label: "Borderline favorable",
            color: "bg-yellow-100 text-yellow-800",
            text:
              "A Bishop score of " +
              score +
              " is intermediate. Induction may still be successful, but cervical ripening and a longer induction process may be needed.",
          };
        } else if (score >= 4) {
          return {
            label: "Unfavorable cervix",
            color: "bg-orange-100 text-orange-800",
            text:
              "A Bishop score of " +
              score +
              " indicates an unfavorable cervix. Many protocols would recommend cervical ripening before or as part of induction.",
          };
        } else {
          return {
            label: "Very unfavorable cervix",
            color: "bg-red-100 text-red-800",
            text:
              "A Bishop score of " +
              score +
              " suggests a very unfavorable cervix. The likelihood of successful induction without prior ripening is low, and labor may be prolonged.",
          };
        }
      }

      document.addEventListener("DOMContentLoaded", function () {
        const modeEffacementBtn = document.getElementById("mode-effacement");
        const modeLengthBtn = document.getElementById("mode-length");
        const effacementGroup = document.getElementById("effacement-group");
        const lengthGroup = document.getElementById("length-group");
        let useLengthMode = false;

        modeEffacementBtn.addEventListener("click", () => {
          useLengthMode = false;
          effacementGroup.classList.remove("hidden");
          lengthGroup.classList.add("hidden");
          modeEffacementBtn.classList.remove(
            "border-gray-300",
            "text-gray-700",
            "bg-white"
          );
          modeEffacementBtn.classList.add(
            "border-blue-600",
            "text-blue-600",
            "bg-blue-50"
          );
          modeLengthBtn.classList.remove(
            "border-blue-600",
            "text-blue-600",
            "bg-blue-50"
          );
          modeLengthBtn.classList.add(
            "border-gray-300",
            "text-gray-700",
            "bg-white"
          );
        });

        modeLengthBtn.addEventListener("click", () => {
          useLengthMode = true;
          effacementGroup.classList.add("hidden");
          lengthGroup.classList.remove("hidden");
          modeLengthBtn.classList.remove(
            "border-gray-300",
            "text-gray-700",
            "bg-white"
          );
          modeLengthBtn.classList.add(
            "border-blue-600",
            "text-blue-600",
            "bg-blue-50"
          );
          modeEffacementBtn.classList.remove(
            "border-blue-600",
            "text-blue-600",
            "bg-blue-50"
          );
          modeEffacementBtn.classList.add(
            "border-gray-300",
            "text-gray-700",
            "bg-white"
          );
        });

        const calculateBtn = document.getElementById("calculate-btn");
        const resetBtn = document.getElementById("reset-btn");
        const scoreSummary = document.getElementById("score-summary");
        const scoreValueEl = document.getElementById("score-value");
        const favorabilityBadge = document.getElementById("favorability-badge");
        const interpretationText = document.getElementById(
          "interpretation-text"
        );

        calculateBtn.addEventListener("click", () => {
          const dilationRange = document.getElementById("dilation").value;
          const stationVal = document.getElementById("station").value;
          const consistencyVal =
            document.querySelector('input[name="consistency"]:checked')
              ?.value || "firm";
          const positionVal =
            document.querySelector('input[name="position"]:checked')?.value ||
            "posterior";

          const dilationPts = getDilationPoints(dilationRange);
          const stationPts = getStationPoints(stationVal);
          const consistencyPts = getConsistencyPoints(consistencyVal);
          const positionPts = getPositionPoints(positionVal);

          let effOrLenPts = 0;
          if (useLengthMode) {
            const lengthVal = document.getElementById("length").value;
            effOrLenPts = getLengthPoints(lengthVal);
          } else {
            const effRange = document.getElementById("effacement").value;
            effOrLenPts = getEffacementPoints(effRange);
          }

          const totalScore =
            dilationPts +
            effOrLenPts +
            stationPts +
            consistencyPts +
            positionPts;
          const interp = interpretScore(totalScore);

          scoreValueEl.textContent = totalScore.toString();
          interpretationText.textContent = interp.text;

          // Reset badge classes then apply
          favorabilityBadge.className = "badge " + interp.color;
          favorabilityBadge.textContent = interp.label;

          scoreSummary.classList.remove("hidden");
        });

        resetBtn.addEventListener("click", () => {
          document.getElementById("dilation").selectedIndex = 0;
          document.getElementById("effacement").selectedIndex = 0;
          document.getElementById("length").selectedIndex = 0;
          document.getElementById("station").selectedIndex = 0;
          document.querySelector(
            'input[name="consistency"][value="firm"]'
          ).checked = true;
          document.querySelector(
            'input[name="position"][value="posterior"]'
          ).checked = true;
          scoreSummary.classList.add("hidden");
        });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
