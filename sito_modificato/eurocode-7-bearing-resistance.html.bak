    <script>
      // Mobile menu toggle
      document
        .getElementById("mobile-menu-toggle")
        ?.addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          if (!menu) return;
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          this.setAttribute("aria-expanded", String(isHidden));
        });

      function toRad(deg) {
        return (deg * Math.PI) / 180;
      }

      function computeBearingCapacityFactors(phiDeg) {
        const phi = toRad(phiDeg);
        if (phiDeg <= 0.1) {
          return { Nc: 5.14, Nq: 1.0, Ngamma: 0.0 }; // crude for phi≈0
        }
        const Nq =
          Math.exp(Math.PI * Math.tan(phi)) *
          Math.pow(Math.tan(Math.PI / 4 + phi / 2), 2);
        const Nc = (Nq - 1) / Math.tan(phi);
        const Ngamma = 2 * (Nq + 1) * Math.tan(phi);
        return { Nc, Nq, Ngamma };
      }

      function computeShapeFactors(B, L, Nc, Nq) {
        if (L < B) {
          const tmp = L;
          L = B;
          B = tmp;
        }
        const Bratio = B / L;
        const sq = 1 + Bratio;
        const sc = 1 + Nq * Bratio;
        const sgamma = 1 - 0.4 * Bratio;
        return { sc, sq, sgamma };
      }

      function computeInclinationFactors(Vd, Hd, Nc) {
        if (Vd <= 0) return { ic: 0, iq: 0, igamma: 0 };
        const ratio = Math.min(Hd / Vd, 0.5);
        const iq = Math.pow(1 - ratio, 2);
        const ic = iq - (1 - iq) / (Nc || 1);
        const igamma = iq * iq;
        return { ic, iq, igamma };
      }

      function formatNum(x, digits = 2) {
        if (!isFinite(x)) return "–";
        if (Math.abs(x) >= 1e4 || (Math.abs(x) < 1e-2 && x !== 0)) {
          return x.toExponential(2);
        }
        return x.toFixed(digits);
      }

      document
        .getElementById("calcBtn")
        ?.addEventListener("click", function () {
          const status = document.getElementById("statusMsg");
          const warn = document.getElementById("warningMsg");
          status.textContent = "";
          warn.classList.add("hidden");
          warn.textContent = "";

          const B = parseFloat(document.getElementById("B").value);
          const L = parseFloat(document.getElementById("L").value);
          const Df = parseFloat(document.getElementById("Df").value);
          const q0 = parseFloat(document.getElementById("q0").value) || 0;

          const gammaEff = parseFloat(
            document.getElementById("gammaEff").value
          );
          const cEff = parseFloat(document.getElementById("cEff").value);
          const phiEff = parseFloat(document.getElementById("phiEff").value);

          const Vd = parseFloat(document.getElementById("Vd").value);
          const Hd = parseFloat(document.getElementById("Hd").value);
          const Mx = parseFloat(document.getElementById("Mx").value);
          const My = parseFloat(document.getElementById("My").value);

          const gammaR = parseFloat(document.getElementById("gammaR").value);
          const useFactors =
            document.getElementById("useFactors").value === "yes";
          const allowTension =
            document.getElementById("allowTension").value === "yes";

          if ([B, L, Df, gammaEff, phiEff, Vd, gammaR].some((v) => isNaN(v))) {
            status.textContent = "Please fill in all required numeric fields.";
            status.classList.remove("text-green-700");
            status.classList.add("text-red-600");
            return;
          }

          if (B <= 0 || L <= 0 || Df < 0 || gammaEff <= 0 || gammaR <= 0) {
            status.textContent =
              "Geometry, unit weight, and γR must be positive.";
            status.classList.remove("text-green-700");
            status.classList.add("text-red-600");
            return;
          }

          if (Vd <= 0 && !allowTension) {
            status.textContent =
              "Vertical design load must be positive (compression) unless tension is explicitly allowed.";
            status.classList.remove("text-green-700");
            status.classList.add("text-red-600");
            return;
          }

          // Eccentricities and effective dimensions
          let ex = 0,
            ey = 0;
          if (Vd !== 0) {
            ex = My / Vd;
            ey = Mx / Vd;
          }
          let Beff = B - 2 * Math.abs(ex);
          let Leff = L - 2 * Math.abs(ey);

          if (Beff <= 0 || Leff <= 0) {
            status.textContent =
              "Eccentricity too large: effective foundation dimensions become non-positive.";
            status.classList.remove("text-green-700");
            status.classList.add("text-red-600");
            return;
          }

          const qEff = gammaEff * Df + q0;

          const { Nc, Nq, Ngamma } = computeBearingCapacityFactors(phiEff);
          let { sc, sq, sgamma } = computeShapeFactors(Beff, Leff, Nc, Nq);
          let { ic, iq, igamma } = computeInclinationFactors(Vd, Hd, Nc);

          if (!useFactors) {
            sc = sq = sgamma = ic = iq = igamma = 1.0;
          }

          // Characteristic resistance Rk (kN)
          const areaEff = Beff * Leff;
          const termC = cEff * Nc * sc * ic * areaEff; // kPa * m2 = kN
          const termQ = qEff * Nq * sq * iq * areaEff;
          const termG =
            0.5 * gammaEff * Beff * Ngamma * sgamma * igamma * areaEff;

          let Rk = termC + termQ + termG;
          if (!isFinite(Rk) || Rk < 0) Rk = 0;

          let Rd = Rk / gammaR;
          if (!allowTension && Vd <= 0) Rd = 0;

          const qdR = Rd / areaEff; // kPa
          const utilization = Vd > 0 && Rd > 0 ? Vd / Rd : NaN;

          // Update UI
          document.getElementById("results-section").classList.remove("hidden");
          document.getElementById("Beff").textContent = formatNum(Beff, 3);
          document.getElementById("Leff").textContent = formatNum(Leff, 3);
          document.getElementById("qEff").textContent = formatNum(qEff, 1);

          document.getElementById("Nc").textContent = formatNum(Nc, 2);
          document.getElementById("Nq").textContent = formatNum(Nq, 2);
          document.getElementById("Ngamma").textContent = formatNum(Ngamma, 2);

          document.getElementById("sc").textContent = formatNum(sc, 2);
          document.getElementById("sq").textContent = formatNum(sq, 2);
          document.getElementById("sgamma").textContent = formatNum(sgamma, 2);

          document.getElementById("ic").textContent = formatNum(ic, 2);
          document.getElementById("iq").textContent = formatNum(iq, 2);
          document.getElementById("igamma").textContent = formatNum(igamma, 2);

          document.getElementById("RkTotal").textContent = formatNum(Rk, 1);
          document.getElementById("RdTotal").textContent = formatNum(Rd, 1);
          document.getElementById("RdStress").textContent = formatNum(qdR, 1);
          document.getElementById("utilization").textContent = isNaN(
            utilization
          )
            ? "–"
            : formatNum(utilization, 2);

          const badge = document.getElementById("safetyBadge");
          badge.classList.remove(
            "bg-green-100",
            "text-green-800",
            "bg-red-100",
            "text-red-800",
            "bg-yellow-100",
            "text-yellow-800"
          );
          if (isNaN(utilization) || Rd <= 0) {
            badge.textContent = "No check";
            badge.classList.add("bg-yellow-100", "text-yellow-800");
          } else if (utilization <= 1.0) {
            badge.textContent = "Safe";
            badge.classList.add("bg-green-100", "text-green-800");
          } else {
            badge.textContent = "Not safe";
            badge.classList.add("bg-red-100", "text-red-800");
          }

          if (phiEff <= 0.1) {
            warn.textContent =
              "Warning: φ' ≈ 0°. The implemented bearing capacity factors are primarily for drained conditions; check undrained design separately.";
            warn.classList.remove("hidden");
          }

          status.textContent = "Calculation completed.";
          status.classList.remove("text-red-600");
          status.classList.add("text-green-700");
        });

      document
        .getElementById("resetBtn")
        ?.addEventListener("click", function () {
          document.getElementById("B").value = 2.0;
          document.getElementById("L").value = 3.0;
          document.getElementById("Df").value = 1.0;
          document.getElementById("q0").value = 0;
          document.getElementById("gammaEff").value = 18;
          document.getElementById("cEff").value = 0;
          document.getElementById("phiEff").value = 35;
          document.getElementById("Vd").value = 1500;
          document.getElementById("Hd").value = 50;
          document.getElementById("Mx").value = 0;
          document.getElementById("My").value = 0;
          document.getElementById("gammaR").value = 1.4;
          document.getElementById("useFactors").value = "yes";
          document.getElementById("allowTension").value = "no";
          document.getElementById("statusMsg").textContent = "";
          document.getElementById("warningMsg").classList.add("hidden");
          document.getElementById("results-section").classList.add("hidden");
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
