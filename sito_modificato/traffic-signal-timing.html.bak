    <script>
      // Initialize phase table
      function buildPhaseRows(n) {
        const tbody = document.getElementById("phaseTableBody");
        tbody.innerHTML = "";
        for (let i = 1; i <= n; i++) {
          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="px-2 py-1 border text-left">Phase ${i}</td>
        <td class="px-2 py-1 border text-right">
          <input type="number" min="0" step="1" class="w-28 border rounded px-1 py-0.5 text-right phase-v" data-phase="${i}" />
        </td>
        <td class="px-2 py-1 border text-right">
          <input type="number" min="1" step="1" class="w-28 border rounded px-1 py-0.5 text-right phase-s" data-phase="${i}" />
        </td>
        <td class="px-2 py-1 border text-right">
          <span class="phase-y" data-phase="${i}">–</span>
        </td>
      `;
          tbody.appendChild(tr);
        }
      }

      function updateYValues() {
        const vInputs = document.querySelectorAll(".phase-v");
        const sInputs = document.querySelectorAll(".phase-s");
        const ySpans = document.querySelectorAll(".phase-y");
        vInputs.forEach((vInput, idx) => {
          const sInput = sInputs[idx];
          const ySpan = ySpans[idx];
          const v = parseFloat(vInput.value);
          const s = parseFloat(sInput.value);
          if (v > 0 && s > 0) {
            const y = v / s;
            ySpan.textContent = y.toFixed(3);
          } else {
            ySpan.textContent = "–";
          }
        });
      }

      function loadExampleBalanced() {
        const numPhasesEl = document.getElementById("numPhases");
        numPhasesEl.value = "4";
        buildPhaseRows(4);
        document.getElementById("lostTime").value = 16;
        document.getElementById("targetY").value = 0.9;
        const v = [600, 600, 400, 400];
        const s = [1800, 1800, 1800, 1800];
        document
          .querySelectorAll(".phase-v")
          .forEach((el, i) => (el.value = v[i] || ""));
        document
          .querySelectorAll(".phase-s")
          .forEach((el, i) => (el.value = s[i] || ""));
        updateYValues();
      }

      function loadExampleUnbalanced() {
        const numPhasesEl = document.getElementById("numPhases");
        numPhasesEl.value = "4";
        buildPhaseRows(4);
        document.getElementById("lostTime").value = 16;
        document.getElementById("targetY").value = 0.9;
        const v = [900, 900, 300, 300]; // heavy main street
        const s = [1800, 1800, 1800, 1800];
        document
          .querySelectorAll(".phase-v")
          .forEach((el, i) => (el.value = v[i] || ""));
        document
          .querySelectorAll(".phase-s")
          .forEach((el, i) => (el.value = s[i] || ""));
        updateYValues();
      }

      function calculateTiming() {
        const n = parseInt(document.getElementById("numPhases").value, 10);
        const L = parseFloat(document.getElementById("lostTime").value);
        const Ytarget = parseFloat(document.getElementById("targetY").value);
        const minC = parseFloat(document.getElementById("minCycle").value);
        const maxC = parseFloat(document.getElementById("maxCycle").value);

        if (!(L > 0)) {
          alert("Please enter a positive total lost time L.");
          return;
        }
        if (!(Ytarget > 0 && Ytarget < 1)) {
          alert("Please enter a target degree of saturation between 0 and 1.");
          return;
        }
        if (!(minC > 0 && maxC > minC)) {
          alert(
            "Please enter reasonable min and max cycle length values (max > min)."
          );
          return;
        }

        const vInputs = document.querySelectorAll(".phase-v");
        const sInputs = document.querySelectorAll(".phase-s");

        const phases = [];
        let sumY = 0;
        for (let i = 0; i < n; i++) {
          const v = parseFloat(vInputs[i].value);
          const s = parseFloat(sInputs[i].value);
          if (!(v > 0) || !(s > 0)) {
            alert(`Please enter positive v and s for phase ${i + 1}.`);
            return;
          }
          const y = v / s;
          sumY += y;
          phases.push({ index: i + 1, v, s, y });
        }

        if (sumY >= 1) {
          alert(
            "Sum of critical flow ratios Y is ≥ 1.0. The intersection is oversaturated; consider revising flows, phases, or saturation flows."
          );
        }

        const Yeff = Math.min(sumY, Ytarget);
        const denom = 1 - Yeff;
        if (denom <= 0) {
          alert(
            "Computed denominator 1 - Y is ≤ 0. Adjust Y target or phase flows."
          );
          return;
        }

        let C0 = (1.5 * L + 5) / denom;
        let C = Math.max(minC, Math.min(maxC, C0));

        const G = C - L;
        const phasesOut = [];
        phases.forEach((p) => {
          const gi = (p.y / sumY) * G;
          const giRatio = gi / C;
          const Xi = (p.v / p.s) * (C / gi);
          phasesOut.push({
            ...p,
            g: gi,
            gRatio: giRatio,
            X: Xi,
          });
        });

        // Update Y values in input table
        updateYValues();

        // Display results
        document.getElementById("resultCycle").textContent = `${C.toFixed(
          1
        )} s (raw Webster: ${C0.toFixed(1)} s)`;
        document.getElementById("resultSumY").textContent = sumY.toFixed(3);
        const greenRatio = G / C;
        document.getElementById("resultGreenRatio").textContent =
          greenRatio.toFixed(3);

        const tbody = document.getElementById("resultPhaseBody");
        tbody.innerHTML = "";
        phasesOut.forEach((p) => {
          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="px-2 py-1 border text-left">Phase ${p.index}</td>
        <td class="px-2 py-1 border text-right">${p.y.toFixed(3)}</td>
        <td class="px-2 py-1 border text-right">${p.g.toFixed(1)}</td>
        <td class="px-2 py-1 border text-right">${p.gRatio.toFixed(3)}</td>
        <td class="px-2 py-1 border text-right ${
          p.X > 1 ? "text-red-600 font-semibold" : ""
        }">${p.X.toFixed(2)}</td>
      `;
          tbody.appendChild(tr);
        });

        const notesEl = document.getElementById("resultNotes");
        let note = "";
        if (C !== C0) {
          note += `Cycle length was clipped to [${minC.toFixed(
            0
          )}, ${maxC.toFixed(0)}] s. `;
        }
        if (phasesOut.some((p) => p.X > 1)) {
          note +=
            "One or more phases have degree of saturation X > 1.0 (highlighted in red). Consider increasing cycle length, adjusting splits, or revising phase design.";
        } else if (!note) {
          note =
            "All phases have X ≤ 1.0 for the specified flows and saturation flows.";
        }
        notesEl.textContent = note;
      }

      function resetCalculator() {
        const numPhasesEl = document.getElementById("numPhases");
        numPhasesEl.value = "4";
        document.getElementById("lostTime").value = 16;
        document.getElementById("targetY").value = 0.9;
        document.getElementById("minCycle").value = 40;
        document.getElementById("maxCycle").value = 180;
        buildPhaseRows(4);
        document.getElementById("resultCycle").textContent = "–";
        document.getElementById("resultSumY").textContent = "–";
        document.getElementById("resultGreenRatio").textContent = "–";
        document.getElementById("resultPhaseBody").innerHTML = "";
        document.getElementById("resultNotes").textContent = "";
      }

      document.addEventListener("DOMContentLoaded", () => {
        buildPhaseRows(
          parseInt(document.getElementById("numPhases").value, 10)
        );
        document.getElementById("numPhases").addEventListener("change", (e) => {
          buildPhaseRows(parseInt(e.target.value, 10));
        });
        document.addEventListener("input", (e) => {
          if (
            e.target.classList.contains("phase-v") ||
            e.target.classList.contains("phase-s")
          ) {
            updateYValues();
          }
        });
        document
          .getElementById("btnExampleBalanced")
          .addEventListener("click", loadExampleBalanced);
        document
          .getElementById("btnExampleUnbalanced")
          .addEventListener("click", loadExampleUnbalanced);
        document
          .getElementById("btnCalculate")
          .addEventListener("click", calculateTiming);
        document
          .getElementById("btnReset")
          .addEventListener("click", resetCalculator);
      });

      // Mobile menu script (from template)
      document.addEventListener("DOMContentLoaded", function () {
        const toggle = document.getElementById("mobile-menu-toggle");
        const menu = document.getElementById("mobile-menu");
        if (toggle && menu) {
          toggle.addEventListener("click", () => {
            const isHidden = menu.classList.contains("hidden");
            menu.classList.toggle("hidden", !isHidden);
            toggle.setAttribute("aria-expanded", isHidden ? "true" : "false");
          });
        }
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
