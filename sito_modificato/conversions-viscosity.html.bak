    <!-- Viscosity converter logic -->
    <script>
      (function () {
        "use strict";

        // Tab handling
        var tabButtons = [
          document.getElementById("tab-dynamic-btn"),
          document.getElementById("tab-kinematic-btn"),
          document.getElementById("tab-dynkin-btn"),
        ];
        var tabPanels = {
          "tab-dynamic": document.getElementById("tab-dynamic"),
          "tab-kinematic": document.getElementById("tab-kinematic"),
          "tab-dynkin": document.getElementById("tab-dynkin"),
        };

        tabButtons.forEach(function (btn) {
          btn.addEventListener("click", function () {
            var targetId = btn.getAttribute("data-target");

            // Update button styles
            tabButtons.forEach(function (b) {
              if (b === btn) {
                b.classList.remove("tab-btn-inactive");
                b.classList.add("tab-btn-active");
              } else {
                b.classList.remove("tab-btn-active");
                b.classList.add("tab-btn-inactive");
              }
            });

            // Show / hide panels
            Object.keys(tabPanels).forEach(function (id) {
              if (id === targetId) {
                tabPanels[id].classList.remove("hidden");
              } else {
                tabPanels[id].classList.add("hidden");
              }
            });
          });
        });

        // --- Dynamic viscosity converter ---
        var dynValueEl = document.getElementById("dyn-value");
        var dynFromEl = document.getElementById("dyn-from");
        var dynToEl = document.getElementById("dyn-to");
        var dynResultEl = document.getElementById("dyn-result");
        var dynBtn = document.getElementById("dyn-convert-btn");

        // Conversion factors to Pa·s
        var dynToPa = {
          pa_s: 1, // Pa·s
          mpa_s: 1e-3, // 1 mPa·s = 0.001 Pa·s
          cp: 1e-3, // 1 cP = 1 mPa·s
          p: 0.1, // 1 P = 0.1 Pa·s
          kg_m_s: 1, // 1 kg/(m·s) = 1 Pa·s
          lb_ft_s: 1.48816, // tribology table: 1 lb/(ft·s) ≈ 1.48816 Pa·s
        };

        function convertDynamic() {
          var value = parseFloat(dynValueEl.value);
          if (isNaN(value)) {
            dynResultEl.textContent = "Enter a numeric value.";
            return;
          }
          var from = dynFromEl.value;
          var to = dynToEl.value;

          var toPaFactorFrom = dynToPa[from];
          var toPaFactorTo = dynToPa[to];

          if (!toPaFactorFrom || !toPaFactorTo) {
            dynResultEl.textContent = "Unsupported unit.";
            return;
          }

          var valuePa = value * toPaFactorFrom;
          var result = valuePa / toPaFactorTo;

          dynResultEl.textContent = result.toPrecision(6) + " " + dynLabel(to);
        }

        function dynLabel(unit) {
          switch (unit) {
            case "pa_s":
              return "Pa·s";
            case "mpa_s":
              return "mPa·s";
            case "cp":
              return "cP";
            case "p":
              return "P";
            case "kg_m_s":
              return "kg/(m·s)";
            case "lb_ft_s":
              return "lb/(ft·s)";
            default:
              return unit;
          }
        }

        dynBtn.addEventListener("click", convertDynamic);

        // --- Kinematic viscosity converter ---
        var kinValueEl = document.getElementById("kin-value");
        var kinFromEl = document.getElementById("kin-from");
        var kinToEl = document.getElementById("kin-to");
        var kinResultEl = document.getElementById("kin-result");
        var kinBtn = document.getElementById("kin-convert-btn");

        // Conversion factors to m²/s
        var kinToM2 = {
          m2_s: 1, // m²/s
          mm2_s: 1e-6, // 1 mm²/s = 1×10⁻⁶ m²/s
          cst: 1e-6, // 1 cSt = 1 mm²/s
          st: 1e-4, // 1 St = 1×10⁻⁴ m²/s
          ft2_s: 0.092903, // 1 ft²/s ≈ 0.092903 m²/s
        };

        function kinLabel(unit) {
          switch (unit) {
            case "m2_s":
              return "m²/s";
            case "mm2_s":
              return "mm²/s";
            case "cst":
              return "cSt";
            case "st":
              return "St";
            case "ft2_s":
              return "ft²/s";
            default:
              return unit;
          }
        }

        function convertKinematic() {
          var value = parseFloat(kinValueEl.value);
          if (isNaN(value)) {
            kinResultEl.textContent = "Enter a numeric value.";
            return;
          }
          var from = kinFromEl.value;
          var to = kinToEl.value;

          var toM2From = kinToM2[from];
          var toM2To = kinToM2[to];

          if (!toM2From || !toM2To) {
            kinResultEl.textContent = "Unsupported unit.";
            return;
          }

          var valueM2 = value * toM2From;
          var result = valueM2 / toM2To;

          kinResultEl.textContent = result.toPrecision(6) + " " + kinLabel(to);
        }

        kinBtn.addEventListener("click", convertKinematic);

        // --- Dynamic ↔ kinematic using density ---
        var dkModeEl = document.getElementById("dk-mode");
        var dkViscEl = document.getElementById("dk-visc");
        var dkViscUnitEl = document.getElementById("dk-visc-unit");
        var dkDensityEl = document.getElementById("dk-density");
        var dkResultEl = document.getElementById("dk-result");
        var dkBtn = document.getElementById("dk-convert-btn");

        function dkConvert() {
          var mode = dkModeEl.value;
          var viscValue = parseFloat(dkViscEl.value);
          var density = parseFloat(dkDensityEl.value);

          if (isNaN(viscValue)) {
            dkResultEl.textContent = "Enter a numeric viscosity value.";
            return;
          }
          if (isNaN(density) || density <= 0) {
            dkResultEl.textContent = "Enter a positive density value (kg/m³).";
            return;
          }

          var unit = dkViscUnitEl.value;

          // First convert input to either Pa·s (for dynamic) or m²/s (for kinematic)
          var muPaS = null;
          var nuM2S = null;

          if (unit === "dyn_pa_s") {
            muPaS = viscValue;
          } else if (unit === "dyn_mpa_s") {
            muPaS = viscValue * 1e-3; // mPa·s -> Pa·s
          } else if (unit === "dyn_cp") {
            muPaS = viscValue * 1e-3; // cP -> Pa·s
          } else if (unit === "kin_m2_s") {
            nuM2S = viscValue;
          } else if (unit === "kin_cst" || unit === "kin_mm2_s") {
            nuM2S = viscValue * 1e-6; // cSt or mm²/s -> m²/s
          } else {
            dkResultEl.textContent = "Unsupported viscosity unit.";
            return;
          }

          var resultText = "";

          if (mode === "dyn-to-kin") {
            // Ensure we have dynamic viscosity in Pa·s
            if (nuM2S !== null && muPaS === null) {
              // If user actually provided kinematic, convert to dynamic first
              muPaS = nuM2S * density;
            }
            if (muPaS === null) {
              dkResultEl.textContent =
                "For dynamic → kinematic, provide a dynamic viscosity unit (Pa·s, mPa·s or cP).";
              return;
            }
            var nu = muPaS / density; // m²/s
            var nuCst = nu / 1e-6;
            resultText =
              nu.toExponential(6) + " m²/s ≈ " + nuCst.toPrecision(6) + " cSt";
          } else {
            // Kinematic → dynamic
            if (muPaS !== null && nuM2S === null) {
              // If user actually provided dynamic, convert to kinematic first
              nuM2S = muPaS / density;
            }
            if (nuM2S === null) {
              dkResultEl.textContent =
                "For kinematic → dynamic, provide a kinematic viscosity unit (m²/s, cSt or mm²/s).";
              return;
            }
            var mu = nuM2S * density; // Pa·s
            var muCP = mu / 1e-3;
            resultText =
              mu.toExponential(6) + " Pa·s ≈ " + muCP.toPrecision(6) + " cP";
          }

          dkResultEl.textContent = resultText;
        }

        dkBtn.addEventListener("click", dkConvert);
      })();
    </script>
  
