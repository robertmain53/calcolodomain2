    <script>
      // Tab switching
      document.querySelectorAll(".lift-tab").forEach((btn) => {
        btn.addEventListener("click", () => {
          document.querySelectorAll(".lift-tab").forEach((b) => {
            b.classList.remove(
              "border-blue-600",
              "text-blue-600",
              "font-medium"
            );
            b.classList.add("border-transparent", "text-gray-600");
          });
          btn.classList.add("border-blue-600", "text-blue-600", "font-medium");
          btn.classList.remove("border-transparent", "text-gray-600");

          const target = btn.getAttribute("data-target");
          document
            .querySelectorAll(".lift-section")
            .forEach((sec) => sec.classList.add("hidden"));
          document.querySelector(target).classList.remove("hidden");
        });
      });

      function toKg(value, unit) {
        if (!value || isNaN(value)) return 0;
        const v = parseFloat(value);
        switch (unit) {
          case "kg":
            return v;
          case "lb":
            return v * 0.45359237;
          case "t":
            return v * 1000;
          case "us_ton":
            return v * 907.18474;
          default:
            return v;
        }
      }

      function fromKgToDisplay(kg) {
        if (!kg) return "0 kg";
        if (kg >= 1000) return (kg / 1000).toFixed(2) + " t";
        return kg.toFixed(1) + " kg";
      }

      function toAreaM2(value, unit) {
        if (!value || isNaN(value)) return 0;
        const v = parseFloat(value);
        switch (unit) {
          case "m2":
            return v;
          case "ft2":
            return v * 0.09290304;
          default:
            return v;
        }
      }

      function fromKPaToDisplay(kPa, targetUnit) {
        if (!kPa) return "0";
        if (targetUnit === "psi") {
          return (kPa * 0.145037738).toFixed(2) + " psi";
        }
        return kPa.toFixed(1) + " kPa";
      }

      document.getElementById("btn-calc").addEventListener("click", () => {
        const loadKg = toKg(
          document.getElementById("load-weight").value,
          document.getElementById("load-weight-unit").value
        );
        const rigKg = toKg(
          document.getElementById("rigging-weight").value,
          document.getElementById("rigging-weight-unit").value
        );
        const hookKg = toKg(
          document.getElementById("hook-weight").value,
          document.getElementById("hook-weight-unit").value
        );
        const dynFactor =
          parseFloat(document.getElementById("dynamic-factor").value) || 1.0;

        const totalStaticKg = loadKg + rigKg + hookKg;
        const totalKg = totalStaticKg * dynFactor;

        const chartKg = toKg(
          document.getElementById("chart-capacity").value,
          document.getElementById("chart-capacity-unit").value
        );
        const capLimit =
          parseFloat(document.getElementById("capacity-limit").value) || 75;

        let utilization = 0;
        let withinLimit = "–";
        let withinClass = "text-gray-800";
        if (chartKg > 0 && totalKg > 0) {
          utilization = (totalKg / chartKg) * 100;
          const limitVal = capLimit;
          const within = utilization <= limitVal;
          withinLimit = within
            ? "YES (within planning limit)"
            : "NO (exceeds planning limit)";
          withinClass = within ? "text-green-700" : "text-red-700";
        }

        document.getElementById("res-total-load").textContent =
          fromKgToDisplay(totalKg);
        document.getElementById("res-chart-capacity").textContent = chartKg
          ? fromKgToDisplay(chartKg)
          : "–";
        document.getElementById("res-capacity-limit").textContent =
          capLimit.toFixed(0) + " % of chart";

        document.getElementById("res-utilization").textContent = utilization
          ? utilization.toFixed(1) + " % of chart"
          : "–";
        const capEl = document.getElementById("res-capacity-ok");
        capEl.textContent = withinLimit;
        capEl.className = "font-semibold " + withinClass;

        // Rigging forces
        const legs =
          parseInt(document.getElementById("rigging-legs").value, 10) || 1;
        const angle =
          parseFloat(document.getElementById("rigging-angle").value) || 60;
        const angleRadHalf = (angle * Math.PI) / 180 / 2;
        let loadPerLegKg = 0;
        let tensionKg = 0;
        if (totalKg > 0 && legs > 0) {
          loadPerLegKg = totalKg / legs;
          const cosHalf = Math.cos(angleRadHalf) || 1;
          tensionKg = loadPerLegKg / cosHalf;
        }
        document.getElementById("res-load-per-leg").textContent = loadPerLegKg
          ? fromKgToDisplay(loadPerLegKg)
          : "–";
        document.getElementById("res-sling-tension").textContent = tensionKg
          ? fromKgToDisplay(tensionKg)
          : "–";
        document.getElementById("res-angle").textContent =
          angle.toFixed(0) + "° included";

        let rigNote = "";
        if (angle >= 120)
          rigNote = "Very high sling tension – avoid if possible.";
        else if (angle >= 90)
          rigNote = "High sling tension – verify WLL carefully.";
        else if (angle >= 60)
          rigNote = "Moderate sling tension – still check WLL.";
        else rigNote = "Favorable sling angle – lower tension per leg.";
        document.getElementById("res-rigging-note").textContent = rigNote;

        // Ground bearing
        const supportAreaM2 = toAreaM2(
          document.getElementById("support-area").value,
          document.getElementById("support-area-unit").value
        );
        const soilCapVal = parseFloat(
          document.getElementById("soil-capacity").value
        );
        const soilCapUnit = document.getElementById("soil-capacity-unit").value;
        let soilCapKPa = 0;
        if (!isNaN(soilCapVal)) {
          soilCapKPa =
            soilCapUnit === "psi" ? soilCapVal / 0.145037738 : soilCapVal;
        }

        let qGroundKPa = 0;
        if (supportAreaM2 > 0 && totalKg > 0) {
          const totalKN = (totalKg * 9.80665) / 1000; // kN
          qGroundKPa = totalKN / supportAreaM2; // kPa
        }

        document.getElementById("res-total-reaction").textContent = totalKg
          ? fromKgToDisplay(totalKg) + " (approx.)"
          : "–";
        document.getElementById("res-support-area").textContent = supportAreaM2
          ? supportAreaM2.toFixed(2) + " m²"
          : "–";
        document.getElementById("res-ground-pressure").textContent = qGroundKPa
          ? fromKPaToDisplay(qGroundKPa, soilCapUnit)
          : "–";
        document.getElementById("res-soil-capacity").textContent = soilCapKPa
          ? fromKPaToDisplay(soilCapKPa, soilCapUnit)
          : "–";

        const groundEl = document.getElementById("res-ground-ok");
        if (qGroundKPa && soilCapKPa) {
          const ok = qGroundKPa <= soilCapKPa;
          groundEl.textContent = ok
            ? "YES (estimated pressure ≤ allowable)"
            : "NO (estimated pressure &gt; allowable)";
          groundEl.className =
            "font-semibold " + (ok ? "text-green-700" : "text-red-700");
        } else {
          groundEl.textContent = "–";
          groundEl.className = "font-semibold text-gray-800";
        }

        // Build summary text
        const craneMake = document.getElementById("crane-make").value || "";
        const craneConfig = document.getElementById("crane-config").value || "";
        const radiusVal = document.getElementById("crane-radius").value;
        const radiusUnit = document.getElementById("crane-radius-unit").value;
        const boomLen = document.getElementById("boom-length").value;
        const boomUnit = document.getElementById("boom-length-unit").value;
        const boomAng = document.getElementById("boom-angle").value;
        const swingAng = document.getElementById("swing-angle").value;
        const supportType = document.getElementById("support-type").value;

        const summaryLines = [];
        summaryLines.push("CRANE LIFT PLAN SUMMARY");
        summaryLines.push("========================");
        summaryLines.push("");
        summaryLines.push("Crane & configuration");
        summaryLines.push("----------------------");
        summaryLines.push("Crane: " + (craneMake || "N/A"));
        summaryLines.push("Configuration: " + (craneConfig || "N/A"));
        summaryLines.push(
          "Boom length: " + (boomLen ? boomLen + " " + boomUnit : "N/A")
        );
        summaryLines.push(
          "Boom angle: " + (boomAng ? boomAng + " deg" : "N/A")
        );
        summaryLines.push(
          "Operating radius: " +
            (radiusVal ? radiusVal + " " + radiusUnit : "N/A")
        );
        summaryLines.push(
          "Swing angle: " + (swingAng ? swingAng + " deg" : "N/A")
        );
        summaryLines.push("");
        summaryLines.push("Capacity check");
        summaryLines.push("--------------");
        summaryLines.push(
          "Chart capacity at radius: " +
            (chartKg ? fromKgToDisplay(chartKg) : "N/A")
        );
        summaryLines.push(
          "Planning limit: " + capLimit.toFixed(0) + " % of chart"
        );
        summaryLines.push(
          "Total load (incl. rigging, hook, dynamic factor " +
            dynFactor.toFixed(2) +
            "): " +
            (totalKg ? fromKgToDisplay(totalKg) : "N/A")
        );
        summaryLines.push(
          "Utilization vs. chart: " +
            (utilization ? utilization.toFixed(1) + " %" : "N/A")
        );
        summaryLines.push("Within planning limit: " + withinLimit);
        summaryLines.push("");
        summaryLines.push("Rigging");
        summaryLines.push("-------");
        summaryLines.push("Number of sling legs: " + legs);
        summaryLines.push(
          "Included angle between legs: " + angle.toFixed(0) + " deg"
        );
        summaryLines.push(
          "Load per leg (ideal): " +
            (loadPerLegKg ? fromKgToDisplay(loadPerLegKg) : "N/A")
        );
        summaryLines.push(
          "Estimated sling tension per leg: " +
            (tensionKg ? fromKgToDisplay(tensionKg) : "N/A")
        );
        summaryLines.push("Rigging note: " + rigNote);
        summaryLines.push("");
        summaryLines.push("Ground & supports");
        summaryLines.push("-----------------");
        summaryLines.push("Support type: " + supportType);
        summaryLines.push(
          "Support area: " +
            (supportAreaM2 ? supportAreaM2.toFixed(2) + " m²" : "N/A")
        );
        summaryLines.push(
          "Estimated ground bearing pressure: " +
            (qGroundKPa ? fromKPaToDisplay(qGroundKPa, soilCapUnit) : "N/A")
        );
        summaryLines.push(
          "Allowable bearing capacity: " +
            (soilCapKPa ? fromKPaToDisplay(soilCapKPa, soilCapUnit) : "N/A")
        );
        summaryLines.push(
          "Within allowable: " + (groundEl.textContent || "N/A")
        );
        summaryLines.push("");
        summaryLines.push("Notes & approvals");
        summaryLines.push("-----------------");
        summaryLines.push(
          "- Verify all weights (load, rigging, hook) with drawings, certificates, or scale readings."
        );
        summaryLines.push(
          "- Confirm crane configuration and load chart selection with the operator and manufacturer documentation."
        );
        summaryLines.push(
          "- Have ground conditions and mat design reviewed by a qualified engineer for critical lifts."
        );
        summaryLines.push(
          "- Complete your company's formal lift plan form and obtain required signatures before lifting."
        );

        document.getElementById("summary-text").value = summaryLines.join("\n");
      });

      document.getElementById("btn-reset").addEventListener("click", () => {
        document.querySelectorAll("input").forEach((inp) => {
          if (
            [
              "capacity-limit",
              "dynamic-factor",
              "rigging-legs",
              "rigging-angle",
              "swing-angle",
            ].includes(inp.id)
          )
            return;
          if (inp.type === "number" || inp.type === "text") inp.value = "";
        });
        document.getElementById("dynamic-factor").value = 1.1;
        document.getElementById("capacity-limit").value = 75;
        document.getElementById("rigging-legs").value = 2;
        document.getElementById("rigging-angle").value = 60;
        document.getElementById("swing-angle").value = 90;
        document.getElementById("summary-text").value = "";
        document
          .querySelectorAll('#results-container span[id^="res-"]')
          .forEach((span) => {
            span.textContent = "–";
            span.className =
              span.id === "res-capacity-ok" || span.id === "res-ground-ok"
                ? "font-semibold text-gray-800"
                : "";
          });
      });

      document
        .getElementById("btn-copy-summary")
        .addEventListener("click", async () => {
          const txt = document.getElementById("summary-text").value;
          const status = document.getElementById("copy-status");
          if (!txt.trim()) {
            status.textContent = "Nothing to copy yet – calculate first.";
            return;
          }
          try {
            await navigator.clipboard.writeText(txt);
            status.textContent = "Summary copied to clipboard.";
            setTimeout(() => {
              status.textContent = "";
            }, 3000);
          } catch (e) {
            status.textContent =
              "Copy failed – you can select and copy manually.";
            setTimeout(() => {
              status.textContent = "";
            }, 4000);
          }
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
