    <script>
      // Georgia tax model parameters (simplified, illustrative)
      const GA_FLAT_RATE = 0.0549; // 5.49% illustrative flat rate

      const GA_STANDARD_DEDUCTION = {
        single: 5400,
        mfs: 5400,
        mfj: 7100,
        hoh: 7100,
      };

      const GA_PERSONAL_EXEMPTION_BASE = {
        single: 2700,
        mfs: 3700,
        mfj: 7400,
        hoh: 2700,
      };

      const GA_DEPENDENT_EXEMPTION = 3000;

      function formatMoney(value) {
        if (!isFinite(value)) return "$0";
        return value.toLocaleString("en-US", {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 0,
        });
      }

      function formatPercent(value) {
        if (!isFinite(value)) return "0%";
        return (value * 100).toFixed(1) + "%";
      }

      function getSelectedDeductionType() {
        const radios = document.querySelectorAll(
          'input[name="deduction-type"]'
        );
        for (const r of radios) {
          if (r.checked) return r.value;
        }
        return "standard";
      }

      function calculateGeorgiaTax() {
        const status = document.getElementById("filing-status").value;
        const income = parseFloat(document.getElementById("income").value) || 0;
        const dependents = Math.max(
          0,
          parseInt(document.getElementById("dependents").value) || 0
        );
        const pretax = Math.max(
          0,
          parseFloat(document.getElementById("pretax-deductions").value) || 0
        );
        const credits = Math.max(
          0,
          parseFloat(document.getElementById("ga-credits").value) || 0
        );
        const itemized = Math.max(
          0,
          parseFloat(document.getElementById("itemized-deductions").value) || 0
        );
        const deductionType = getSelectedDeductionType();

        const adjustedIncome = Math.max(0, income - pretax);

        const stdDeduction = GA_STANDARD_DEDUCTION[status] || 0;
        const deductionUsed =
          deductionType === "itemized" ? itemized : stdDeduction;

        const baseAfterDeduction = Math.max(0, adjustedIncome - deductionUsed);

        const personalExemptionBase = GA_PERSONAL_EXEMPTION_BASE[status] || 0;
        const personalExemptions =
          personalExemptionBase + dependents * GA_DEPENDENT_EXEMPTION;

        const taxableIncome = Math.max(
          0,
          baseAfterDeduction - personalExemptions
        );

        const taxBeforeCredits = taxableIncome * GA_FLAT_RATE;
        const stateTax = Math.max(0, taxBeforeCredits - credits);

        const effectiveRate = income > 0 ? stateTax / income : 0;
        const marginalRate = taxableIncome > 0 ? GA_FLAT_RATE : 0;
        const takeHome = income - stateTax;

        document.getElementById("out-taxable-income").textContent =
          formatMoney(taxableIncome);
        document.getElementById("out-state-tax").textContent =
          formatMoney(stateTax);
        document.getElementById("out-effective-rate").textContent =
          formatPercent(effectiveRate);
        document.getElementById("out-marginal-rate").textContent =
          formatPercent(marginalRate);
        document.getElementById("out-takehome").textContent =
          formatMoney(takeHome);
      }

      document.addEventListener("DOMContentLoaded", function () {
        const itemizedInput = document.getElementById("itemized-deductions");
        const radios = document.querySelectorAll(
          'input[name="deduction-type"]'
        );
        radios.forEach((r) => {
          r.addEventListener("change", () => {
            if (getSelectedDeductionType() === "itemized") {
              itemizedInput.disabled = false;
              itemizedInput.classList.remove("bg-gray-100");
            } else {
              itemizedInput.disabled = true;
              itemizedInput.classList.add("bg-gray-100");
            }
            calculateGeorgiaTax();
          });
        });

        document
          .getElementById("calc-btn")
          .addEventListener("click", calculateGeorgiaTax);

        [
          "filing-status",
          "income",
          "dependents",
          "pretax-deductions",
          "ga-credits",
          "itemized-deductions",
        ].forEach((id) => {
          const el = document.getElementById(id);
          if (el) {
            el.addEventListener("input", () => {
              // Debounce a bit via requestAnimationFrame
              window.requestAnimationFrame(calculateGeorgiaTax);
            });
          }
        });

        document
          .getElementById("toggle-advanced")
          .addEventListener("click", function () {
            const adv = document.getElementById("advanced-section");
            const isHidden = adv.classList.contains("hidden");
            adv.classList.toggle("hidden");
            this.textContent = isHidden
              ? "Hide advanced options"
              : "Show advanced options";
          });

        // Initial calculation
        calculateGeorgiaTax();
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
