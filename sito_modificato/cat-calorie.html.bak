    <script>
      // Cat calorie calculator logic
      function toKg(value, unit) {
        if (!value || value <= 0) return null;
        return unit === "lb" ? value * 0.45359237 : value;
      }

      function round1(x) {
        return Math.round(x * 10) / 10;
      }

      function round2(x) {
        return Math.round(x * 100) / 100;
      }

      function calcRER(kg) {
        return 70 * Math.pow(kg, 0.75);
      }

      function getMultiplier(lifeStage) {
        switch (lifeStage) {
          case "adult_neutered_indoor":
            return 1.3;
          case "adult_intact_active":
            return 1.5;
          case "adult_obese_prone":
            return 1.0;
          case "senior":
            return 1.2;
          case "kitten_0_4":
            return 2.5;
          case "kitten_4_12":
            return 2.0;
          case "weight_gain":
            return 1.2;
          default:
            return 1.3;
        }
      }

      function updateFoodHelper() {
        const type = document.getElementById("food-type").value;
        const helper = document.getElementById("food-kcal-helper");
        const unitSel = document.getElementById("food-unit");
        if (type === "dry") {
          helper.textContent = "kcal per cup (dry kibble).";
          unitSel.value = "cup";
        } else if (type === "wet") {
          helper.textContent = "kcal per can (or pouch).";
          unitSel.value = "can";
        } else {
          helper.textContent = "kcal per selected unit (cup, can, or 100 g).";
        }
      }

      document
        .getElementById("food-type")
        .addEventListener("change", updateFoodHelper);

      document
        .getElementById("calculate-btn")
        .addEventListener("click", function () {
          const errorEl = document.getElementById("calc-error");
          errorEl.textContent = "";

          const weightVal = parseFloat(
            document.getElementById("cat-weight").value
          );
          const weightUnit = document.getElementById("cat-weight-unit").value;
          const idealVal = parseFloat(
            document.getElementById("cat-ideal-weight").value
          );
          const idealUnit = document.getElementById(
            "cat-ideal-weight-unit"
          ).value;
          const lifeStage = document.getElementById("cat-life-stage").value;

          const kg = toKg(weightVal, weightUnit);
          const idealKg = toKg(idealVal, idealUnit);

          if (!kg) {
            errorEl.textContent = "Please enter a valid current weight.";
            document.getElementById("results-section").classList.add("hidden");
            return;
          }

          // RER based on current weight
          const rer = calcRER(kg);
          const mult = getMultiplier(lifeStage);
          const maintKcal = rer * mult;

          // Ideal-weight based RER (if provided), else use current
          const baseKgForIdeal = idealKg || kg;
          const rerIdeal = calcRER(baseKgForIdeal);
          const idealMaintKcal = rerIdeal * mult;

          // Weight-loss and weight-gain plans (based on ideal or current if no ideal)
          const weightLossKcal = rerIdeal * 0.8; // conservative
          const weightGainKcal = rerIdeal * 1.2; // modest gain

          // Update results
          document.getElementById("rer-kcal").textContent = Math.round(rer);
          document.getElementById("maintenance-kcal").textContent =
            Math.round(maintKcal);
          document.getElementById("ideal-maintenance-kcal").textContent =
            Math.round(idealMaintKcal);
          document.getElementById("weight-loss-kcal").textContent =
            Math.round(weightLossKcal);
          document.getElementById("weight-gain-kcal").textContent =
            Math.round(weightGainKcal);

          // Food portions
          const foodKcal = parseFloat(
            document.getElementById("food-kcal").value
          );
          const foodUnit = document.getElementById("food-unit").value;
          const foodResults = document.getElementById("food-results");

          if (foodKcal && foodKcal > 0) {
            const maintPortion = maintKcal / foodKcal;
            const idealPortion = idealMaintKcal / foodKcal;
            const lossPortion = weightLossKcal / foodKcal;

            document.getElementById("maint-portion").textContent =
              round2(maintPortion);
            document.getElementById("ideal-portion").textContent =
              round2(idealPortion);
            document.getElementById("loss-portion").textContent =
              round2(lossPortion);

            let unitLabel;
            if (foodUnit === "cup") unitLabel = "cups per day";
            else if (foodUnit === "can") unitLabel = "cans per day";
            else unitLabel = "× 100 g portions per day";

            document.getElementById("maint-portion-unit").textContent =
              unitLabel;
            document.getElementById("ideal-portion-unit").textContent =
              unitLabel;
            document.getElementById("loss-portion-unit").textContent =
              unitLabel;

            const densityLabel =
              foodUnit === "100g"
                ? `${foodKcal} kcal per 100 g`
                : `${foodKcal} kcal per ${foodUnit}`;
            document.getElementById("food-density-label").textContent =
              densityLabel;

            foodResults.classList.remove("hidden");
          } else {
            foodResults.classList.add("hidden");
          }

          document.getElementById("results-section").classList.remove("hidden");
        });

      document
        .getElementById("reset-btn")
        .addEventListener("click", function () {
          document.getElementById("cat-weight").value = "";
          document.getElementById("cat-ideal-weight").value = "";
          document.getElementById("cat-life-stage").value =
            "adult_neutered_indoor";
          document.getElementById("cat-body-condition").value = "";
          document.getElementById("food-type").value = "dry";
          document.getElementById("food-kcal").value = "";
          document.getElementById("food-unit").value = "cup";
