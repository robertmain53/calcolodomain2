    <script>
      // Simple xmur3 hash and mulberry32 PRNG for seeded randomness
      function xmur3(str) {
        let h = 1779033703 ^ str.length;
        for (let i = 0; i < str.length; i++) {
          h = Math.imul(h ^ str.charCodeAt(i), 3432918353);
          h = (h << 13) | (h >>> 19);
        }
        return function () {
          h = Math.imul(h ^ (h >>> 16), 2246822507);
          h = Math.imul(h ^ (h >>> 13), 3266489909);
          h ^= h >>> 16;
          return h >>> 0;
        };
      }

      function mulberry32(a) {
        return function () {
          let t = (a += 0x6d2b79f5);
          t = Math.imul(t ^ (t >>> 15), t | 1);
          t ^= t + Math.imul(t ^ (t >>> 7), t | 61);
          return ((t ^ (t >>> 14)) >>> 0) / 4294967296;
        };
      }

      function createRng(seedStr) {
        if (!seedStr) {
          // derive a seed from time + random + a little noise
          seedStr = (Date.now() + Math.random()).toString();
        }
        const seedFn = xmur3(seedStr);
        return mulberry32(seedFn());
      }

      function getNumberRng() {
        const seedInput = document.getElementById("seed").value.trim();
        return createRng(seedInput);
      }

      function fisherYatesShuffle(array, rng) {
        for (let i = array.length - 1; i > 0; i--) {
          const j = Math.floor(rng() * (i + 1));
          [array[i], array[j]] = [array[j], array[i]];
        }
        return array;
      }

      // Tabs
      document.querySelectorAll(".rng-tab").forEach((btn) => {
        btn.addEventListener("click", () => {
          const target = btn.getAttribute("data-tab");
          document.querySelectorAll(".rng-tab").forEach((b) => {
            b.classList.remove("border-blue-600", "text-blue-600");
            b.classList.add("border-transparent", "text-gray-600");
          });
          btn.classList.add("border-blue-600", "text-blue-600");
          btn.classList.remove("border-transparent", "text-gray-600");

          document
            .querySelectorAll(".rng-panel")
            .forEach((panel) => panel.classList.add("hidden"));
          const panel = document.getElementById("tab-" + target);
          if (panel) panel.classList.remove("hidden");
        });
      });

      // Number type toggle
      document
        .querySelectorAll('input[name="number-type"]')
        .forEach((radio) => {
          radio.addEventListener("change", () => {
            const wrapper = document.getElementById("decimal-places-wrapper");
            if (radio.value === "decimal" && radio.checked) {
              wrapper.classList.remove("hidden");
            } else if (radio.value === "integer" && radio.checked) {
              wrapper.classList.add("hidden");
            }
          });
        });

      // Generate numbers
      document
        .getElementById("generate-numbers")
        .addEventListener("click", () => {
          const min = Number(document.getElementById("min-value").value);
          const max = Number(document.getElementById("max-value").value);
          const qty = Number(document.getElementById("quantity").value);
          const unique = document.getElementById("unique-numbers").checked;
          const sort = document.getElementById("sort-numbers").checked;
          const type = document.querySelector(
            'input[name="number-type"]:checked'
          ).value;
          const decimals =
            Number(document.getElementById("decimal-places").value) || 2;

          const out = document.getElementById("numbers-output");
          const err = document.getElementById("numbers-error");
          err.classList.add("hidden");
          err.textContent = "";
          out.textContent = "";

          if (!Number.isFinite(min) || !Number.isFinite(max)) {
            err.textContent =
              "Please enter valid numeric minimum and maximum values.";
            err.classList.remove("hidden");
            return;
          }
          if (max < min) {
            err.textContent =
              "Maximum value must be greater than or equal to minimum value.";
            err.classList.remove("hidden");
            return;
          }
          if (!Number.isInteger(qty) || qty < 1 || qty > 10000) {
            err.textContent =
              "Quantity must be an integer between 1 and 10,000.";
            err.classList.remove("hidden");
            return;
          }

          const rng = getNumberRng();
          let results = [];

          if (type === "integer") {
            const rangeSize = Math.floor(max) - Math.ceil(min) + 1;
            const minInt = Math.ceil(min);
            const maxInt = Math.floor(max);

            if (rangeSize <= 0) {
              err.textContent =
                "For integers, the range must contain at least one integer.";
              err.classList.remove("hidden");
              return;
            }
            if (unique && qty > rangeSize) {
              err.textContent = `You requested ${qty} unique numbers, but the integer range only contains ${rangeSize} distinct values.`;
              err.classList.remove("hidden");
              return;
            }

            if (unique) {
              const pool = [];
              for (let v = minInt; v <= maxInt; v++) pool.push(v);
              fisherYatesShuffle(pool, rng);
              results = pool.slice(0, qty);
            } else {
              for (let i = 0; i < qty; i++) {
                const u = rng();
                const n = minInt + Math.floor(u * rangeSize);
                results.push(n);
              }
            }
          } else {
            // decimals
            if (unique && qty > 10000) {
              err.textContent =
                "Unique decimals are limited to 10,000 values for performance reasons.";
              err.classList.remove("hidden");
              return;
            }
            const factor = Math.pow(10, Math.min(Math.max(decimals, 1), 10));
            const scaledMin = Math.round(min * factor);
            const scaledMax = Math.round(max * factor);
            if (scaledMax < scaledMin) {
              err.textContent =
                "After rounding to the requested decimal places, the range is invalid. Try widening the range or reducing decimal places.";
              err.classList.remove("hidden");
              return;
            }
            const rangeSize = scaledMax - scaledMin + 1;
            if (unique && qty > rangeSize) {
              err.textContent = `You requested ${qty} unique decimals, but the rounded range only contains ${rangeSize} distinct values.`;
              err.classList.remove("hidden");
              return;
            }

            if (unique) {
              const pool = [];
              for (let v = scaledMin; v <= scaledMax; v++) pool.push(v);
              fisherYatesShuffle(pool, rng);
              results = pool
                .slice(0, qty)
                .map((v) => (v / factor).toFixed(decimals));
            } else {
              for (let i = 0; i < qty; i++) {
                const u = rng();
                const val = min + u * (max - min);
                results.push(val.toFixed(decimals));
              }
            }
          }

          if (sort) {
            results.sort((a, b) => Number(a) - Number(b));
          }

          out.textContent = results.join(", ");
        });

      // Copy helpers
      function copyFromElement(id) {
        const el = document.getElementById(id);
        if (!el) return;
        const text = el.textContent || "";
        if (!text.trim()) return;
        navigator.clipboard?.writeText(text).catch(() => {
          // fallback: do nothing silently
        });
      }

      document
        .getElementById("copy-numbers")
        .addEventListener("click", () => copyFromElement("numbers-output"));

      // List generator
      document.getElementById("generate-list").addEventListener("click", () => {
        const raw = document.getElementById("list-input").value;
        const qty = Number(document.getElementById("list-quantity").value);
        const unique = document.getElementById("list-unique").checked;
        const shuffleAll = document.getElementById("list-shuffle-all").checked;
        const out = document.getElementById("list-output");
        const err = document.getElementById("list-error");
        err.classList.add("hidden");
        err.textContent = "";
        out.textContent = "";

        let items = raw
          .split(/\r?\n/)
          .map((s) => s.trim())
          .filter(Boolean);
        if (items.length === 0) {
          err.textContent = "Please enter at least one non‑empty item.";
          err.classList.remove("hidden");
          return;
        }
        if (items.length > 10000) {
          err.textContent = "The list is limited to 10,000 items.";
          err.classList.remove("hidden");
          return;
        }
        if (!Number.isInteger(qty) || qty < 1) {
          err.textContent =
            "Quantity must be an integer greater than or equal to 1.";
          err.classList.remove("hidden");
          return;
        }
        if (unique && qty > items.length) {
          err.textContent = `You requested ${qty} unique items, but the list only contains ${items.length} entries.`;
          err.classList.remove("hidden");
          return;
        }

        const rng = createRng("list-" + Date.now() + Math.random());

        if (shuffleAll) {
          fisherYatesShuffle(items, rng);
          out.textContent = items.join("\n");
          return;
        }

        // sample
        fisherYatesShuffle(items, rng);
        const picked = unique ? items.slice(0, qty) : [];
        if (!unique) {
          for (let i = 0; i < qty; i++) {
            const idx = Math.floor(rng() * items.length);
            picked.push(items[idx]);
          }
        }
        out.textContent = picked.join("\n");
      });

      document
        .getElementById("copy-list")
        .addEventListener("click", () => copyFromElement("list-output"));

      // Dice
      document.getElementById("generate-dice").addEventListener("click", () => {
        const sides = Number(document.getElementById("dice-sides").value);
        const count = Number(document.getElementById("dice-count").value);
        const rolls = Number(document.getElementById("dice-rolls").value);
        const out = document.getElementById("dice-output");
        const err = document.getElementById("dice-error");
        err.classList.add("hidden");
        err.textContent = "";
        out.textContent = "";

        if (!Number.isInteger(sides) || sides < 2) {
          err.textContent = "Sides per die must be an integer of at least 2.";
          err.classList.remove("hidden");
          return;
        }
        if (!Number.isInteger(count) || count < 1 || count > 100) {
          err.textContent =
            "Number of dice must be an integer between 1 and 100.";
          err.classList.remove("hidden");
          return;
        }
        if (!Number.isInteger(rolls) || rolls < 1 || rolls > 1000) {
          err.textContent = "Rolls must be an integer between 1 and 1,000.";
          err.classList.remove("hidden");
          return;
        }

        const rng = createRng("dice-" + Date.now() + Math.random());
        const lines = [];
        for (let r = 0; r < rolls; r++) {
          const vals = [];
          let sum = 0;
          for (let d = 0; d < count; d++) {
            const v = 1 + Math.floor(rng() * sides);
            vals.push(v);
            sum += v;
          }
          lines.push(`Roll ${r + 1}: [${vals.join(", ")}]  (sum = ${sum})`);
        }
        out.textContent = lines.join("\n");
      });

      document
        .getElementById("copy-dice")
        .addEventListener("click", () => copyFromElement("dice-output"));

      // Coin
      document.getElementById("generate-coin").addEventListener("click", () => {
        const flips = Number(document.getElementById("coin-flips").value);
        const out = document.getElementById("coin-output");
        const err = document.getElementById("coin-error");
        err.classList.add("hidden");
        err.textContent = "";
        out.textContent = "";

        if (!Number.isInteger(flips) || flips < 1 || flips > 10000) {
          err.textContent =
            "Number of flips must be an integer between 1 and 10,000.";
          err.classList.remove("hidden");
          return;
        }

        const rng = createRng("coin-" + Date.now() + Math.random());
        const results = [];
        let heads = 0;
        let tails = 0;
        for (let i = 0; i < flips; i++) {
          if (rng() < 0.5) {
            results.push("Heads");
            heads++;
          } else {
            results.push("Tails");
            tails++;
          }
        }
        const summary = `Heads: ${heads}, Tails: ${tails}`;
        out.textContent =
          flips === 1 ? results[0] : results.join(", ") + "\n\n" + summary;
      });

      document
        .getElementById("copy-coin")
        .addEventListener("click", () => copyFromElement("coin-output"));

      // FAQ toggle (simple, in addition to global script_faq if any)
      document.querySelectorAll("#faq .faq-question").forEach((btn) => {
        btn.addEventListener("click", () => {
          const answer = btn.nextElementSibling;
          if (!answer) return;
          const isHidden = answer.classList.contains("hidden");
          document
            .querySelectorAll("#faq .faq-answer")
            .forEach((a) => a.classList.add("hidden"));
          document
            .querySelectorAll("#faq .faq-question span:last-child")
            .forEach((icon) => (icon.textContent = "+"));
          if (isHidden) {
            answer.classList.remove("hidden");
            btn.querySelector("span:last-child").textContent = "−";
          }
        });
      });
    </script>

