    <script>
      // Primer Tm calculator logic
      (function () {
        const seqInput = document.getElementById("sequence");
        const seqError = document.getElementById("seq-error");
        const oligoConcInput = document.getElementById("oligo-conc");
        const oligoConcUnit = document.getElementById("oligo-conc-unit");
        const naConcInput = document.getElementById("na-conc");
        const mgConcInput = document.getElementById("mg-conc");
        const dntpConcInput = document.getElementById("dntp-conc");
        const modelSelect = document.getElementById("tm-model");

        const lenOut = document.getElementById("len-out");
        const gcOut = document.getElementById("gc-out");
        const taOut = document.getElementById("ta-out");
        const tmNnOut = document.getElementById("tm-nn-out");
        const tmGcOut = document.getElementById("tm-gc-out");
        const tmWallaceOut = document.getElementById("tm-wallace-out");

        const calcBtn = document.getElementById("calc-btn");
        const clearBtn = document.getElementById("clear-btn");

        function cleanSequence(raw) {
          return raw.toUpperCase().replace(/[^ACGT]/g, "");
        }

        function validateSequence(raw) {
          const cleaned = raw.toUpperCase().replace(/\s/g, "");
          if (!cleaned)
            return {
              ok: false,
              msg: "Please enter a primer sequence using A, C, G, and T.",
            };
          if (/[^ACGT]/i.test(cleaned)) {
            return {
              ok: false,
              msg: "Sequence contains invalid characters. Use only A, C, G, and T (no degenerate bases).",
            };
          }
          if (cleaned.length < 8) {
            return {
              ok: false,
              msg: "Primer is very short. Use at least 8 bases for a meaningful Tm estimate.",
            };
          }
          return { ok: true, msg: "", seq: cleaned };
        }

        function countBases(seq) {
          let A = 0,
            C = 0,
            G = 0,
            T = 0;
          for (let ch of seq) {
            if (ch === "A") A++;
            else if (ch === "C") C++;
            else if (ch === "G") G++;
            else if (ch === "T") T++;
          }
          const len = seq.length;
          const gc = len ? ((G + C) / len) * 100 : 0;
          return { A, C, G, T, len, gc };
        }

        function wallaceTm(counts) {
          const { A, C, G, T } = counts;
          return 2 * (A + T) + 4 * (G + C);
        }

        function gcTm(counts) {
          const { len, gc } = counts;
          if (len === 0) return NaN;
          return 81.5 + 0.41 * gc - 675 / len;
        }

        // Nearest-neighbor parameters (SantaLucia 1998, DNA/DNA, 1M Na+)
        const NN_PARAMS = {
          AA: { dH: -7.9, dS: -22.2 },
          TT: { dH: -7.9, dS: -22.2 },
          AT: { dH: -7.2, dS: -20.4 },
          TA: { dH: -7.2, dS: -21.3 },
          CA: { dH: -8.5, dS: -22.7 },
          TG: { dH: -8.5, dS: -22.7 },
          GT: { dH: -8.4, dS: -22.4 },
          AC: { dH: -8.4, dS: -22.4 },
          CT: { dH: -7.8, dS: -21.0 },
          AG: { dH: -7.8, dS: -21.0 },
          GA: { dH: -8.2, dS: -22.2 },
          TC: { dH: -8.2, dS: -22.2 },
          CG: { dH: -10.6, dS: -27.2 },
          GC: { dH: -9.8, dS: -24.4 },
          GG: { dH: -8.0, dS: -19.9 },
          CC: { dH: -8.0, dS: -19.9 },
        };

        function nearestNeighborTm(
          seq,
          primerConcM,
          naConcM,
          mgConcM,
          dntpConcM
        ) {
          const R = 1.987; // cal/(K·mol)
          const len = seq.length;
          if (len < 2) return NaN;

          let dH = 0; // kcal/mol
          let dS = 0; // cal/(mol·K)

          // Initiation terms (SantaLucia 1998)
          const first = seq[0];
          const last = seq[len - 1];
          // initiation enthalpy (kcal/mol)
          dH += 0.2;
          // initiation entropy (cal/mol·K)
          dS += -5.7;

          // Terminal AT penalty
          if (first === "A" || first === "T") {
            dH += 2.3;
            dS += 4.1;
          }
          if (last === "A" || last === "T") {
            dH += 2.3;
            dS += 4.1;
          }

          // Sum nearest-neighbor steps
          for (let i = 0; i < len - 1; i++) {
            const step = seq[i] + seq[i + 1];
            const p = NN_PARAMS[step];
            if (!p) continue;
            dH += p.dH;
            dS += p.dS;
          }

          // Convert dH to cal/mol
          const dH_cal = dH * 1000;

          // Effective strand concentration (non-self-complementary)
          const Ce = primerConcM / 4;

          // Effective monovalent concentration including Mg2+ (very simplified)
          let NaEq = naConcM;
          if (mgConcM > 0) {
            const freeMg = Math.max(mgConcM - dntpConcM, 0);
            NaEq += 3.3 * Math.sqrt(freeMg);
          }
          if (NaEq <= 0) NaEq = 0.0000001;

          // Tm in Kelvin
          const TmK =
            dH_cal / (dS + R * Math.log(Ce)) + 16.6 * Math.log10(NaEq);
          const TmC = TmK - 273.15;
          return TmC;
        }

        function formatTm(value) {
          if (isNaN(value)) return "–";
          return value.toFixed(1) + " °C";
        }

        function calculate() {
          seqError.classList.add("hidden");
          seqError.textContent = "";

          const rawSeq = seqInput.value.trim();
          const v = validateSequence(rawSeq);
          if (!v.ok) {
            seqError.textContent = v.msg;
            seqError.classList.remove("hidden");
            lenOut.textContent = "–";
            gcOut.textContent = "–";
            taOut.textContent = "–";
            tmNnOut.textContent = "–";
            tmGcOut.textContent = "–";
            tmWallaceOut.textContent = "–";
            return;
          }
          const seq = v.seq;
          const counts = countBases(seq);

          lenOut.textContent = counts.len + " nt";
          gcOut.textContent = counts.gc.toFixed(1) + " %";

          // Concentrations
          let oligoConc = parseFloat(oligoConcInput.value);
          if (isNaN(oligoConc) || oligoConc <= 0) oligoConc = 0.5;
          let oligoConcM = oligoConc * 1e-6;
          if (oligoConcUnit.value === "nM") {
            oligoConcM = oligoConc * 1e-9;
          }

          let naConc = parseFloat(naConcInput.value);
          if (isNaN(naConc) || naConc <= 0) naConc = 50;
          const naConcM = naConc * 1e-3;

          let mgConc = parseFloat(mgConcInput.value);
          if (isNaN(mgConc) || mgConc < 0) mgConc = 0;
          const mgConcM = mgConc * 1e-3;

          let dntpConc = parseFloat(dntpConcInput.value);
          if (isNaN(dntpConc) || dntpConc < 0) dntpConc = 0;
          const dntpConcM = dntpConc * 1e-3;

          const model = modelSelect.value;

          const tmWallace = wallaceTm(counts);
          const tmGcVal = gcTm(counts);
          const tmNnVal = nearestNeighborTm(
            seq,
            oligoConcM,
            naConcM,
            mgConcM,
            dntpConcM
          );

          // Fill outputs depending on model
          if (model === "wallace") {
            tmWallaceOut.textContent = formatTm(tmWallace);
            tmGcOut.textContent = "–";
            tmNnOut.textContent = "–";
            taOut.textContent = "–";
          } else if (model === "gc") {
            tmWallaceOut.textContent = "–";
            tmGcOut.textContent = formatTm(tmGcVal);
            tmNnOut.textContent = "–";
            taOut.textContent = "–";
          } else if (model === "nn") {
            tmWallaceOut.textContent = "–";
            tmGcOut.textContent = "–";
            tmNnOut.textContent = formatTm(tmNnVal);
            if (!isNaN(tmNnVal)) {
              taOut.textContent =
                (tmNnVal - 3).toFixed(1) + " °C (suggested start)";
            } else {
              taOut.textContent = "–";
            }
          } else {
            // all
            tmWallaceOut.textContent = formatTm(tmWallace);
            tmGcOut.textContent = formatTm(tmGcVal);
            tmNnOut.textContent = formatTm(tmNnVal);
            if (!isNaN(tmNnVal)) {
              taOut.textContent =
                (tmNnVal - 3).toFixed(1) + " °C (suggested start)";
            } else {
              taOut.textContent = "–";
            }
          }
        }

        function clearAll() {
          seqInput.value = "";
          seqError.classList.add("hidden");
          seqError.textContent = "";
          lenOut.textContent = "–";
          gcOut.textContent = "–";
          taOut.textContent = "–";
          tmNnOut.textContent = "–";
          tmGcOut.textContent = "–";
          tmWallaceOut.textContent = "–";
        }

        calcBtn.addEventListener("click", calculate);
        clearBtn.addEventListener("click", clearAll);
      })();
    </script>

