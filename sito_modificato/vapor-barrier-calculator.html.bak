    <script>
      // Utility functions
      function toNumber(id) {
        const v = parseFloat(document.getElementById(id).value);
        return isNaN(v) ? 0 : v;
      }

      function setHidden(el, hidden) {
        if (hidden) el.classList.add("hidden");
        else el.classList.remove("hidden");
      }

      // Unit handling
      const unitSystemEl = document.getElementById("unit-system");
      const overlapUnitEl = document.getElementById("overlap-unit");
      const upturnUnitEls = [
        document.getElementById("upturn-unit"),
        document.getElementById("upturn-unit-l"),
        document.getElementById("upturn-unit-c"),
      ];
      const perimeterUnitEls = [
        document.getElementById("perimeter-unit"),
        document.getElementById("perimeter-unit-l"),
        document.getElementById("perimeter-unit-c"),
      ];
      const rectLengthUnitLabel = document.getElementById(
        "rect-length-unit-label"
      );
      const rectWidthUnitLabel = document.getElementById(
        "rect-width-unit-label"
      );
      const rectLengthInWrapper = document.getElementById(
        "rect-length-in-wrapper"
      );
      const rectWidthInWrapper = document.getElementById(
        "rect-width-in-wrapper"
      );
      const customAreaUnitEl = document.getElementById("custom-area-unit");
      const rollWidthUnitEl = document.getElementById("roll-width-unit");
      const rollLengthUnitEl = document.getElementById("roll-length-unit");
      const unitPriceHelpEl = document.getElementById("unit-price-help");

      function updateUnits() {
        const isImperial = unitSystemEl.value === "imperial";
        overlapUnitEl.textContent = isImperial ? "in" : "cm";
        upturnUnitEls.forEach(
          (el) => (el.textContent = isImperial ? "ft" : "m")
        );
        perimeterUnitEls.forEach(
          (el) => (el.textContent = isImperial ? "ft" : "m")
        );
        rectLengthUnitLabel.textContent = isImperial ? "ft" : "m";
        rectWidthUnitLabel.textContent = isImperial ? "ft" : "m";
        customAreaUnitEl.textContent = isImperial ? "ft²" : "m²";
        rollWidthUnitEl.textContent = isImperial ? "ft" : "m";
        rollLengthUnitEl.textContent = isImperial ? "ft" : "m";
        unitPriceHelpEl.textContent = isImperial
          ? "Per ft². Used if roll price is unknown."
          : "Per m². Used if roll price is unknown.";
        setHidden(rectLengthInWrapper, !isImperial);
        setHidden(rectWidthInWrapper, !isImperial);
      }

      unitSystemEl.addEventListener("change", updateUnits);
      updateUnits();

      // Tabs / layouts
      const layoutSections = {
        rectangular: document.getElementById("layout-rectangular"),
        lshape: document.getElementById("layout-lshape"),
        custom: document.getElementById("layout-custom"),
      };

      function activateTab(layout) {
        ["tab-rect", "tab-lshape", "tab-custom"].forEach((id) => {
          const btn = document.getElementById(id);
          btn.classList.remove("tab-active");
          btn.classList.remove("bg-blue-50");
          btn.classList.add("bg-white");
        });
        if (layout === "rectangular")
          document.getElementById("tab-rect").classList.add("tab-active");
        if (layout === "lshape")
          document.getElementById("tab-lshape").classList.add("tab-active");
        if (layout === "custom")
          document.getElementById("tab-custom").classList.add("tab-active");

        Object.keys(layoutSections).forEach((key) =>
          setHidden(layoutSections[key], key !== layout)
        );
      }

      document
        .getElementById("tab-rect")
        .addEventListener("click", () => activateTab("rectangular"));
      document
        .getElementById("tab-lshape")
        .addEventListener("click", () => activateTab("lshape"));
      document
        .getElementById("tab-custom")
        .addEventListener("click", () => activateTab("custom"));

      // Main calculation
      const errorMsgEl = document.getElementById("error-msg");
      const resultsSection = document.getElementById("results-section");

      function calculateVaporBarrier() {
        errorMsgEl.textContent = "";
        const isImperial = unitSystemEl.value === "imperial";

        // Determine active layout
        let layout = "rectangular";
        if (!layoutSections.rectangular.classList.contains("hidden"))
          layout = "rectangular";
        if (!layoutSections.lshape.classList.contains("hidden"))
          layout = "lshape";
        if (!layoutSections.custom.classList.contains("hidden"))
          layout = "custom";

        let floorArea = 0;
        let perimeter = 0;
        let upturnHeight = 0;

        if (layout === "rectangular") {
          let L = toNumber("rect-length-ft");
          let W = toNumber("rect-width-ft");
          if (isImperial) {
            const Lin = toNumber("rect-length-in");
            const Win = toNumber("rect-width-in");
            L += Lin / 12;
            W += Win / 12;
          }
          if (L <= 0 || W <= 0) {
            errorMsgEl.textContent =
              "Please enter positive length and width for the rectangular area.";
            setHidden(resultsSection, true);
            return;
          }
          floorArea = L * W;
          upturnHeight = toNumber("upturn-height");
          perimeter = toNumber("perimeter");
          if (perimeter <= 0) {
            perimeter = 2 * (L + W);
          }
        } else if (layout === "lshape") {
          const La = toNumber("la");
          const Wa = toNumber("wa");
          const Lb = toNumber("lb");
          const Wb = toNumber("wb");
          if (La <= 0 || Wa <= 0 || Lb <= 0 || Wb <= 0) {
            errorMsgEl.textContent =
              "Please enter positive dimensions for both rectangles in the L-shaped area.";
            setHidden(resultsSection, true);
            return;
          }
          floorArea = La * Wa + Lb * Wb;
          upturnHeight = toNumber("upturn-height-l");
          perimeter = toNumber("perimeter-l");
          if (perimeter <= 0) {
            // Rough approximation: sum of perimeters minus one shared side
            perimeter = (2 * (La + Wa + Lb + Wb)) / 2;
          }
        } else if (layout === "custom") {
          floorArea = toNumber("custom-area");
          if (floorArea <= 0) {
            errorMsgEl.textContent =
              "Please enter a positive total floor area.";
            setHidden(resultsSection, true);
            return;
          }
          upturnHeight = toNumber("upturn-height-c");
          perimeter = toNumber("perimeter-c");
          if (upturnHeight > 0 && perimeter <= 0) {
            errorMsgEl.textContent =
              "Please enter a perimeter length when using a wall upturn with custom area.";
            setHidden(resultsSection, true);
            return;
          }
        }

        // Convert units to a consistent base (ft or m)
        let floorAreaBase = floorArea;
        let perimeterBase = perimeter;
        let upturnHeightBase = upturnHeight;

        // Upturn area
        let upturnArea = 0;
        if (upturnHeightBase > 0 && perimeterBase > 0) {
          upturnArea = perimeterBase * upturnHeightBase;
        }

        const subtotalArea = floorAreaBase + upturnArea;

        // Overlap factor (approximate)
        const overlap = toNumber("overlap"); // in or cm
        let overlapFactor = 0;
        if (overlap > 0) {
          // Assume seams every 10 ft (3 m) across width
          const typicalSpacing = isImperial ? 10 : 3;
          const overlapLength = isImperial ? overlap / 12 : overlap / 100; // ft or m
          overlapFactor = overlapLength / typicalSpacing;
        }

        const wastePct = toNumber("waste") / 100;
        const totalFactor = 1 + overlapFactor + wastePct;
        const totalArea = subtotalArea * totalFactor;

        // Roll info
        let rollWidth = toNumber("roll-width");
        let rollLength = toNumber("roll-length");
        if (rollWidth <= 0 || rollLength <= 0) {
          errorMsgEl.textContent =
            "Please enter positive roll width and length.";
          setHidden(resultsSection, true);
          return;
        }
        const rollArea = rollWidth * rollLength;
        const rollsRequired = Math.ceil(totalArea / rollArea);

        // Cost
        const rollPrice = toNumber("roll-price");
        const unitPrice = toNumber("unit-price");
        let cost = 0;
        if (rollPrice > 0) {
          cost = rollsRequired * rollPrice;
        } else if (unitPrice > 0) {
          cost = totalArea * unitPrice;
        }

        // Seam tape estimate (very approximate)
        const includeTape = document.getElementById("include-tape").checked;
        let tapeRolls = 0;
        if (includeTape) {
          // Approximate seam length as floorArea / typical roll width
          const typicalRollWidth = rollWidth;
          const seamLength = floorAreaBase / typicalRollWidth;
          tapeRolls = Math.ceil(seamLength / 400); // 1 roll per 400 ft or m equivalent
        }

        // Display results
        const unitAreaLabel = isImperial ? "ft²" : "m²";

        document.getElementById("res-floor-area").textContent =
          floorAreaBase.toFixed(1) + " " + unitAreaLabel;
        document.getElementById("res-upturn-area").textContent =
          upturnArea.toFixed(1) + " " + unitAreaLabel;
        document.getElementById("res-subtotal-area").textContent =
          subtotalArea.toFixed(1) + " " + unitAreaLabel;
        document.getElementById("res-factor").textContent =
          totalFactor.toFixed(2) + "×";
        document.getElementById("res-total-area").textContent =
          totalArea.toFixed(1) + " " + unitAreaLabel;
        document.getElementById("res-roll-area").textContent =
          rollArea.toFixed(1) + " " + unitAreaLabel;
        document.getElementById("res-rolls").textContent = isFinite(
          rollsRequired
        )
          ? rollsRequired
          : "–";
        document.getElementById("res-cost").textContent =
          cost > 0 ? "$" + cost.toFixed(2) : "–";
        document.getElementById("res-tape").textContent = includeTape
          ? tapeRolls + " roll(s) (approx.)"
          : "Not included";

        setHidden(resultsSection, false);
      }

      document
        .getElementById("calculate-btn")
        .addEventListener("click", calculateVaporBarrier);
      document.getElementById("reset-btn").addEventListener("click", () => {
        errorMsgEl.textContent = "";
        setHidden(resultsSection, true);
        setTimeout(updateUnits, 0);
      });

      // Keyboard shortcut: Enter triggers calculation
      document.getElementById("vapor-form").addEventListener("keydown", (e) => {
        if (e.key === "Enter") {
          e.preventDefault();
          calculateVaporBarrier();
        }
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
