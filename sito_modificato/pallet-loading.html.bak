    <script>
      // Utility: unit conversions
      function toInches(value, unit) {
        if (!isFinite(value)) return NaN;
        return unit === "cm" ? value / 2.54 : value;
      }
      function fromInches(value, unit) {
        if (!isFinite(value)) return NaN;
        return unit === "cm" ? value * 2.54 : value;
      }
      function toPounds(value, unit) {
        if (!isFinite(value)) return NaN;
        return unit === "kg" ? value * 2.2046226218 : value;
      }
      function fromPounds(value, unit) {
        if (!isFinite(value)) return NaN;
        return unit === "kg" ? value / 2.2046226218 : value;
      }

      function readNumber(id) {
        const el = document.getElementById(id);
        if (!el) return NaN;
        const v = parseFloat(el.value);
        return isNaN(v) ? NaN : v;
      }

      function calcPattern(
        palletL,
        palletW,
        caseL,
        caseW,
        caseH,
        palletH,
        maxTotalH,
        allowLayers,
        availableCases,
        caseWeightLb,
        maxPalletWeightLb,
        name,
        orientation
      ) {
        // orientation: 'LxW' or 'WxL'
        const len = orientation === "LxW" ? caseL : caseW;
        const wid = orientation === "LxW" ? caseW : caseL;

        if (len <= 0 || wid <= 0) return null;

        const countL = Math.floor(palletL / len);
        const countW = Math.floor(palletW / wid);
        const perLayer = countL * countW;

        if (perLayer <= 0) return null;

        const maxLayersByHeight = Math.floor((maxTotalH - palletH) / caseH);
        const maxLayers = Math.max(0, maxLayersByHeight);

        if (maxLayers <= 0) return null;

        let layers = maxLayers;
        let totalCases = perLayer * layers;

        if (
          allowLayers &&
          availableCases &&
          availableCases > 0 &&
          totalCases > availableCases
        ) {
          layers = Math.floor(availableCases / perLayer);
          totalCases = perLayer * layers;
          if (layers <= 0) return null;
        }

        // Weight check
        let totalWeightLb = totalCases * caseWeightLb;
        if (
          maxPalletWeightLb &&
          maxPalletWeightLb > 0 &&
          totalWeightLb > maxPalletWeightLb
        ) {
          // Reduce layers until within weight
          while (layers > 0 && totalWeightLb > maxPalletWeightLb) {
            layers--;
            totalCases = perLayer * layers;
            totalWeightLb = totalCases * caseWeightLb;
          }
          if (layers <= 0) return null;
        }

        const totalHeight = palletH + layers * caseH;
        const footprintUtil =
          ((perLayer * len * wid) / (palletL * palletW)) * 100;

        return {
          name,
          orientation,
          perLayer,
          layers,
          totalCases,
          totalHeight,
          totalWeightLb,
          footprintUtil,
          cellL: len,
          cellW: wid,
        };
      }

      function buildPatternTable(patterns) {
        const tbody = document.getElementById("pattern-table-body");
        tbody.innerHTML = "";
        if (!patterns.length) {
          tbody.innerHTML =
            '<tr><td colspan="5" class="px-2 py-2 text-center text-gray-500">No feasible pattern found. Check dimensions and limits.</td></tr>';
          return;
        }
        patterns.forEach((p) => {
          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="px-2 py-1 border-b text-gray-800">${p.name}</td>
        <td class="px-2 py-1 border-b text-right">${p.perLayer}</td>
        <td class="px-2 py-1 border-b text-right">${p.layers}</td>
        <td class="px-2 py-1 border-b text-right">${p.totalCases}</td>
        <td class="px-2 py-1 border-b text-right">${p.footprintUtil.toFixed(
          1
        )}%</td>
      `;
          tbody.appendChild(tr);
        });
      }

      function drawPatternVisual(pattern, palletL, palletW) {
        const container = document.getElementById("pattern-visual");
        container.innerHTML = "";
        container.classList.remove(
          "items-center",
          "justify-center",
          "text-gray-400"
        );

        if (!pattern) {
          container.classList.add(
            "flex",
            "items-center",
            "justify-center",
            "text-gray-400"
          );
          container.textContent = "No pattern yet.";
          return;
        }

        const perLayer = pattern.perLayer;
        if (perLayer <= 0) {
          container.classList.add(
            "flex",
            "items-center",
            "justify-center",
            "text-gray-400"
          );
          container.textContent =
            "No cases fit on the pallet with these dimensions.";
          return;
        }

        const len = pattern.cellL;
        const wid = pattern.cellW;
        const countL = Math.floor(palletL / len);
        const countW = Math.floor(palletW / wid);

        const grid = document.createElement("div");
        grid.style.display = "grid";
        grid.style.width = "100%";
        grid.style.height = "100%";
        grid.style.gridTemplateColumns = `repeat(${countL}, 1fr)`;
        grid.style.gridTemplateRows = `repeat(${countW}, 1fr)`;
        grid.style.gap = "2px";
        grid.style.padding = "4px";
        grid.style.boxSizing = "border-box";

        for (let j = 0; j < countW; j++) {
          for (let i = 0; i < countL; i++) {
            const cell = document.createElement("div");
            cell.className = "pattern-visual-cell";
            grid.appendChild(cell);
          }
        }

        container.appendChild(grid);
      }

      document
        .getElementById("calculate-btn")
        .addEventListener("click", function () {
          const errorEl = document.getElementById("input-error");
          errorEl.classList.add("hidden");
          errorEl.textContent = "";

          const palletL = toInches(
            readNumber("pallet-length"),
            document.getElementById("pallet-length-unit").value
          );
          const palletW = toInches(
            readNumber("pallet-width"),
            document.getElementById("pallet-width-unit").value
          );
          const palletH = toInches(
            readNumber("pallet-height"),
            document.getElementById("pallet-height-unit").value
          );
          const maxTotalH = toInches(
            readNumber("max-total-height"),
            document.getElementById("max-total-height-unit").value
          );

          const caseL = toInches(
            readNumber("case-length"),
            document.getElementById("case-length-unit").value
          );
          const caseW = toInches(
            readNumber("case-width"),
            document.getElementById("case-width-unit").value
          );
          const caseH = toInches(
            readNumber("case-height"),
            document.getElementById("case-height-unit").value
          );

          const caseWeightLb = toPounds(
            readNumber("case-weight"),
            document.getElementById("case-weight-unit").value
          );
          const maxPalletWeightLb = toPounds(
            readNumber("max-pallet-weight"),
            document.getElementById("max-pallet-weight-unit").value
          );

          const availableCasesVal = readNumber("available-cases");
          const availableCases =
            isNaN(availableCasesVal) || availableCasesVal <= 0
              ? null
              : Math.floor(availableCasesVal);

          const allowRotation =
            document.getElementById("allow-rotation").checked;
          const allowMixedLayers =
            document.getElementById("allow-mixed-layers").checked;

          if (
            [
              palletL,
              palletW,
              palletH,
              maxTotalH,
              caseL,
              caseW,
              caseH,
              caseWeightLb,
            ].some((v) => !isFinite(v) || v <= 0)
          ) {
            errorEl.textContent =
              "Please enter valid positive numbers for all required fields.";
            errorEl.classList.remove("hidden");
            return;
          }
          if (maxTotalH <= palletH) {
            errorEl.textContent =
              "Maximum total height must be greater than pallet height.";
            errorEl.classList.remove("hidden");
            return;
          }

          const patterns = [];

          const p1 = calcPattern(
            palletL,
            palletW,
            caseL,
            caseW,
            caseH,
            palletH,
            maxTotalH,
            true,
            availableCases,
            caseWeightLb,
            maxPalletWeightLb,
            "Column stack (lengthwise)",
            "LxW"
          );
          if (p1) patterns.push(p1);

          if (allowRotation) {
            const p2 = calcPattern(
              palletL,
              palletW,
              caseL,
              caseW,
              caseH,
              palletH,
              maxTotalH,
              true,
              availableCases,
              caseWeightLb,
              maxPalletWeightLb,
              "Column stack (widthwise)",
              "WxL"
            );
            if (p2) patterns.push(p2);
          }

          // Mixed pattern: alternate orientation by layer if allowed
          let mixedPattern = null;
          if (allowRotation && allowMixedLayers) {
            const base1 = calcPattern(
              palletL,
              palletW,
              caseL,
              caseW,
              caseH,
              palletH,
              maxTotalH,
              false,
              null,
              caseWeightLb,
              maxPalletWeightLb,
              "LxW",
              "LxW"
            );
            const base2 = calcPattern(
              palletL,
              palletW,
              caseL,
              caseW,
              caseH,
              palletH,
              maxTotalH,
              false,
              null,
              caseWeightLb,
              maxPalletWeightLb,
              "WxL",
              "WxL"
            );
            if (base1 && base2) {
              const perLayer1 = base1.perLayer;
              const perLayer2 = base2.perLayer;
              const maxLayersByHeight = Math.floor(
                (maxTotalH - palletH) / caseH
              );
              let layers = maxLayersByHeight;

              if (layers > 0) {
                let totalCases = 0;
                for (let i = 0; i < layers; i++) {
                  totalCases += i % 2 === 0 ? perLayer1 : perLayer2;
                }
                if (
                  availableCases &&
                  availableCases > 0 &&
                  totalCases > availableCases
                ) {
                  // reduce layers to fit available cases
                  layers = 0;
                  let running = 0;
                  while (running < availableCases) {
                    const add = layers % 2 === 0 ? perLayer1 : perLayer2;
                    if (running + add > availableCases) break;
                    running += add;
                    layers++;
                  }
                  totalCases = running;
                }
                let totalWeightLb = totalCases * caseWeightLb;
                if (
                  maxPalletWeightLb &&
                  maxPalletWeightLb > 0 &&
                  totalWeightLb > maxPalletWeightLb
                ) {
                  // reduce layers until within weight
                  while (layers > 0 && totalWeightLb > maxPalletWeightLb) {
                    layers--;
                    let running = 0;
                    for (let i = 0; i < layers; i++) {
                      running += i % 2 === 0 ? perLayer1 : perLayer2;
                    }
                    totalCases = running;
                    totalWeightLb = totalCases * caseWeightLb;
                  }
                }
                if (layers > 0 && totalCases > 0) {
                  const totalHeight = palletH + layers * caseH;
                  const avgPerLayer = totalCases / layers;
                  const avgUtil =
                    ((avgPerLayer * caseL * caseW) / (palletL * palletW)) * 100;
                  mixedPattern = {
                    name: "Mixed pattern (alternating orientation)",
                    orientation: "Mixed",
                    perLayer: Math.round(avgPerLayer),
                    layers,
                    totalCases,
                    totalHeight,
                    totalWeightLb,
                    footprintUtil: avgUtil,
                    cellL: base1.cellL,
                    cellW: base1.cellW,
                  };
                  patterns.push(mixedPattern);
                }
              }
            }
          }

          // Choose best pattern by totalCases, then utilization
          const feasible = patterns.filter((p) => p && p.totalCases > 0);
          buildPatternTable(feasible);

          const summaryIds = {
            casesPerLayer: document.getElementById("cases-per-layer"),
            numLayers: document.getElementById("num-layers"),
            totalCases: document.getElementById("total-cases"),
            totalHeight: document.getElementById("total-height"),
            totalWeight: document.getElementById("total-weight"),
            footprintUtil: document.getElementById("footprint-utilization"),
          };
          const patternLabel = document.getElementById("pattern-label");
          const weightWarning = document.getElementById("weight-warning");
          const heightWarning = document.getElementById("height-warning");
          const casesLimitNote = document.getElementById("cases-limit-note");

          weightWarning.classList.add("hidden");
          heightWarning.classList.add("hidden");
          casesLimitNote.classList.add("hidden");

          if (!feasible.length) {
            summaryIds.casesPerLayer.textContent = "–";
            summaryIds.numLayers.textContent = "–";
            summaryIds.totalCases.textContent = "–";
            summaryIds.totalHeight.textContent = "–";
            summaryIds.totalWeight.textContent = "–";
            summaryIds.footprintUtil.textContent = "–";
            patternLabel.classList.add("hidden");
            drawPatternVisual(null, palletL, palletW);
            return;
          }

          feasible.sort((a, b) => {
            if (b.totalCases !== a.totalCases)
              return b.totalCases - a.totalCases;
            return b.footprintUtil - a.footprintUtil;
          });
          const best = feasible[0];

          summaryIds.casesPerLayer.textContent = best.perLayer.toString();
          summaryIds.numLayers.textContent = best.layers.toString();
          summaryIds.totalCases.textContent = best.totalCases.toString();

          const outHeightUnit = document.getElementById(
            "max-total-height-unit"
          ).value;
          const outWeightUnit = document.getElementById(
            "max-pallet-weight-unit"
          ).value;

          const bestHeightOut = fromInches(best.totalHeight, outHeightUnit);
          const bestWeightOut = fromPounds(best.totalWeightLb, outWeightUnit);

          summaryIds.totalHeight.textContent = `${bestHeightOut.toFixed(
            1
          )} ${outHeightUnit}`;
          summaryIds.totalWeight.textContent = `${bestWeightOut.toFixed(
            1
          )} ${outWeightUnit}`;
          summaryIds.footprintUtil.textContent = `${best.footprintUtil.toFixed(
            1
          )} %`;

          patternLabel.textContent = best.name;
          patternLabel.classList.remove("hidden");

          const maxHeightOut = fromInches(maxTotalH, outHeightUnit);
          if (best.totalHeight > maxTotalH + 1e-6) {
            heightWarning.textContent = `Warning: calculated height exceeds the specified maximum of ${maxHeightOut.toFixed(
              1
            )} ${outHeightUnit}.`;
            heightWarning.classList.remove("hidden");
          }

          if (
            maxPalletWeightLb &&
            maxPalletWeightLb > 0 &&
            best.totalWeightLb > maxPalletWeightLb + 1e-6
          ) {
            const maxWeightOut = fromPounds(maxPalletWeightLb, outWeightUnit);
            weightWarning.textContent = `Warning: calculated weight exceeds the specified maximum of ${maxWeightOut.toFixed(
              1
            )} ${outWeightUnit}.`;
            weightWarning.classList.remove("hidden");
          }

          if (availableCases && best.totalCases === availableCases) {
            casesLimitNote.textContent =
              "Note: total cases are limited by the available quantity you entered.";
            casesLimitNote.classList.remove("hidden");
          }

          drawPatternVisual(best, palletL, palletW);
        });

      // Mobile menu toggle
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          this.setAttribute("aria-expanded", isHidden ? "true" : "false");
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
