    <script>
      // Quick planner logic
      function clamp(val, min, max) {
        return Math.min(Math.max(val, min), max);
      }

      document.getElementById("qp-calc").addEventListener("click", function () {
        const age = clamp(
          parseFloat(document.getElementById("qp-age").value) || 30,
          10,
          100
        );
        const sex = document.getElementById("qp-sex").value;
        const height = clamp(
          parseFloat(document.getElementById("qp-height").value) || 170,
          120,
          230
        );
        const weight = clamp(
          parseFloat(document.getElementById("qp-weight").value) || 70,
          35,
          250
        );
        const activity =
          parseFloat(document.getElementById("qp-activity").value) || 1.55;
        const goal = document.getElementById("qp-goal").value;

        // Mifflin–St Jeor BMR
        let bmr;
        if (sex === "male") {
          bmr = 10 * weight + 6.25 * height - 5 * age + 5;
        } else {
          bmr = 10 * weight + 6.25 * height - 5 * age - 161;
        }
        let tdee = bmr * activity;

        // Adjust for goal
        let calories = tdee;
        if (goal === "loss") {
          calories = tdee - 400; // moderate deficit
        } else if (goal === "gain") {
          calories = tdee + 250; // lean gain
        }
        calories = Math.round(calories);

        // Protein: 1.8 g/kg baseline
        const protein = Math.round(weight * 1.8);

        // Water: 35 ml/kg
        const waterLiters = (weight * 0.035).toFixed(1);

        document.getElementById("qp-calories").textContent =
          calories.toLocaleString();
        document.getElementById("qp-protein").textContent =
          protein.toLocaleString();
        document.getElementById("qp-water").textContent = waterLiters;

        document.getElementById("qp-results").classList.remove("hidden");
      });

      document
        .querySelectorAll("#faq-container .faq-question")
        .forEach(function (btn) {
          btn.addEventListener("click", function () {
            const answer = btn.parentElement.querySelector(".faq-answer");
            const isHidden = answer.classList.contains("hidden");
            document
              .querySelectorAll("#faq-container .faq-answer")
              .forEach(function (a) {
                a.classList.add("hidden");
              });
            document
              .querySelectorAll("#faq-container .faq-question span:last-child")
              .forEach(function (icon) {
                icon.textContent = "+";
              });
            if (isHidden) {
              answer.classList.remove("hidden");
              btn.querySelector("span:last-child").textContent = "−";
            }
          });
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
