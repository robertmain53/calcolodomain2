            <script>
              document.addEventListener('DOMContentLoaded', function() {
                const inputA = document.getElementById('coeff-a');
                const inputB = document.getElementById('coeff-b');
                const inputC = document.getElementById('coeff-c');
                const calculateBtn = document.getElementById('calculate-btn');
                const resultsContainer = document.getElementById('results-container');
                const errorMessage = document.getElementById('error-message');
                const resultDelta = document.getElementById('result-delta');
                const resultNature = document.getElementById('result-nature');
                const stepByStep = document.getElementById('step-by-step');
                const rootX1 = document.getElementById('root-x1');
                const rootX2 = document.getElementById('root-x2');
                
                // --- Helper: Parse fractions and decimals ---
                function parseValue(val) {
                  val = String(val).trim();
                  if (val.includes('/')) {
                    const parts = val.split('/');
                    if (parts.length === 2) {
                      const num = parseFloat(parts[0]);
                      const den = parseFloat(parts[1]);
                      if (den !== 0 && !isNaN(num) && !isNaN(den)) {
                        return num / den;
                      }
                    }
                  }
                  const num = parseFloat(val);
                  return isNaN(num) ? NaN : num;
                }
                
                function formatNumber(num) {
                    if (Math.abs(num) < 1e-10) return '0';
                    return parseFloat(num.toFixed(10)).toString();
                }
                
                function formatComplex(real, imaginary) {
                    if (Math.abs(imaginary) < 1e-10) return formatNumber(real);
                    
                    const sign = (imaginary > 0) ? '+' : '-';
                    const absImaginary = formatNumber(Math.abs(imaginary));
                    const formattedReal = formatNumber(real);
                    
                    if (formattedReal === '0') return `${sign} ${absImaginary}i`;
                    
                    return `${formattedReal} ${sign} ${absImaginary}i`;
                }
                
                function showError(message) {
                    errorMessage.textContent = `Error: ${message}`;
                    resultsContainer.classList.add('hidden');
                }

                // --- Calculation Logic ---
                calculateBtn.addEventListener('click', () => {
                  const a = parseValue(inputA.value);
                  const b = parseValue(inputB.value);
                  const c = parseValue(inputC.value);
                  
                  try {
                    if (isNaN(a) || isNaN(b) || isNaN(c)) throw new Error('Please enter valid numerical coefficients (a, b, c).');
                    if (Math.abs(a) < 1e-10) throw new Error("'a' cannot be zero for a quadratic equation. This is a linear equation.");
                    
                    // 1. Calculate Discriminant (Delta)
                    const delta = b * b - 4 * a * c;
                    const fDelta = formatNumber(delta);
                    const fA = formatNumber(a);
                    const fB = formatNumber(b);
                    const fC = formatNumber(c);
                    
                    // 2. Determine Nature of Roots and set Step-by-Step
                    let natureText = '';
                    let rootsHTML = '';
                    let steps = `
                      <p>1. <strong>Identify Coefficients:</strong> $a = ${fA}$, $b = ${fB}$, $c = ${fC}$</p>
                      <p>2. <strong>Discriminant Formula:</strong> $\\Delta = b^2 - 4ac$</p>
                      <p>3. <strong>Substitute Values:</strong> $\\Delta = (${fB})^2 - 4(${fA})(${fC})$</p>
                      <p>4. <strong>Calculate:</strong> $\\Delta = ${formatNumber(b * b)} - ${formatNumber(4 * a * c)}$</p>
                      <p>5. <strong>Result:</strong> $\\Delta = ${fDelta}$</p>
                      <h4 class="font-semibold text-lg mt-4">Root Classification:</h4>
                    `;

                    // 3. Calculate Roots
                    if (delta > 1e-10) { // Positive Delta
                      natureText = 'Two Distinct Real Roots';
                      const sqrtDelta = Math.sqrt(delta);
                      const x1 = (-b + sqrtDelta) / (2 * a);
                      const x2 = (-b - sqrtDelta) / (2 * a);
                      
                      rootsHTML = `
                        <p class="root-result">$$x_1 = \\frac{${formatNumber(-b)} + \\sqrt{${fDelta}}}{${formatNumber(2 * a)}} = ${formatNumber(x1)}$$</p>
                        <p class="root-result" id="root-x2">$$x_2 = \\frac{${formatNumber(-b)} - \\sqrt{${fDelta}}}{${formatNumber(2 * a)}} = ${formatNumber(x2)}$$</p>
                      `;
                      steps += `<p>Since $\\Delta > 0$, the equation has <strong>${natureText}</strong>.</p>`;
                      
                    } else if (Math.abs(delta) < 1e-10) { // Zero Delta
                      natureText = 'One Repeated Real Root';
                      const x = -b / (2 * a);
                      
                      rootsHTML = `
                        <p class="root-result">$$x = \\frac{-b}{2a} = ${formatNumber(x)}$$</p>
                      `;
                      steps += `<p>Since $\\Delta = 0$, the equation has <strong>${natureText}</strong> (The vertex touches the x-axis).</p>`;
                      
                    } else { // Negative Delta
                      natureText = 'Two Distinct Non-Real (Complex) Roots';
                      const sqrtAbsDelta = Math.sqrt(Math.abs(delta));
                      const realPart = -b / (2 * a);
                      const imaginaryPart = sqrtAbsDelta / (2 * a);
                      
                      rootsHTML = `
                        <p class="root-result" id="root-x1">$$x_{1, 2} = \\frac{-b \\pm i\\sqrt{|\\Delta|}}{2a} = ${formatComplex(realPart, imaginaryPart)} \\text{ and } ${formatComplex(realPart, -imaginaryPart)}$$</p>
                      `;
                      steps += `<p>Since $\\Delta < 0$, the equation has <strong>${natureText}</strong>.</p>
                               <p>The solutions involve the imaginary unit $i = \\sqrt{-1}$.</p>`;
                    }
                    
                    // 4. Update DOM
                    resultDelta.textContent = fDelta;
                    resultNature.textContent = natureText;
                    stepByStep.innerHTML = steps;
                    document.getElementById('result-roots').innerHTML = rootsHTML;
                    resultsContainer.classList.remove('hidden');

                    // 5. Re-render MathJax for the new content
                    if (window.MathJax) {
                        window.MathJax.typesetPromise([stepByStep, document.getElementById('result-roots')]);
                    }

                  } catch (e) {
                    showError(e.message);
                  }
                });
                
                // Initial calculation (for example values)
                calculateBtn.click(); 
              });
            </script>

