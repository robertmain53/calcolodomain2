    <script>
      // Simple helper: clamp
      function clamp(val, min, max) {
        return Math.min(Math.max(val, min), max);
      }

      function getSuggestedVt(region, riskCategory) {
        // Very rough conceptual suggestions, NOT from ASCE maps
        let base;
        switch (region) {
          case "low":
            base = 120;
            break;
          case "moderate":
            base = 170;
            break;
          case "high":
            base = 220;
            break;
          default:
            base = 170;
        }
        let factor = 1.0;
        if (riskCategory === "III") factor = 1.05;
        if (riskCategory === "IV") factor = 1.1;
        return Math.round(base * factor);
      }

      function getImportanceFactor(riskCategory) {
        // Simplified tornado importance factor It
        if (riskCategory === "III") return 1.1;
        if (riskCategory === "IV") return 1.2;
        return 1.0; // Risk Category II
      }

      function getKz(exposure, z) {
        // Very rough approximation of exposure factor vs height
        const h = clamp(z, 10, 200);
        let alpha;
        switch (exposure) {
          case "B":
            alpha = 7.0;
            break;
          case "C":
            alpha = 9.5;
            break;
          case "D":
            alpha = 11.5;
            break;
          default:
            alpha = 9.5;
        }
        const Kz = Math.pow(h / 33.0, 2.0 / alpha);
        return Kz;
      }

      function formatNumber(val, decimals) {
        if (!isFinite(val)) return "–";
        return val.toFixed(decimals);
      }

      function calculateTornadoLoads() {
        const riskCategory = document.getElementById("risk-category").value;
        const region = document.getElementById("tornado-region").value;
        const VtInput = parseFloat(document.getElementById("Vt").value);
        const height = parseFloat(document.getElementById("height").value);
        const exposure = document.getElementById("exposure").value;
        const GCp = parseFloat(document.getElementById("GCp").value);
        const At = parseFloat(document.getElementById("tributary-area").value);
        const comboFactor = parseFloat(
          document.getElementById("load-combo-factor").value
        );
        const GCpi = parseFloat(
          document.getElementById("internal-pressure").value
        );

        if (
          !VtInput ||
          !height ||
          !At ||
          !comboFactor ||
          isNaN(GCp) ||
          isNaN(GCpi)
        ) {
          alert("Please enter valid numeric values for all required fields.");
          return;
        }

        const suggestedVt = getSuggestedVt(region, riskCategory);
        const Vt = clamp(VtInput, 50, 350);
        const It = getImportanceFactor(riskCategory);
        const Kz = getKz(exposure, height);

        // Simplified velocity pressure (psf)
        const qz = 0.00256 * Kz * It * Vt * Vt;

        const pe = qz * GCp;
        const pnet = qz * (GCp + GCpi);
        const pu = pnet * comboFactor;
        const resultant = (pu * At) / 1000.0; // kips

        // Update UI
        document.getElementById("suggested-Vt").textContent = formatNumber(
          suggestedVt,
          0
        );
        document.getElementById("It").textContent = formatNumber(It, 2);
        document.getElementById("Kz").textContent = formatNumber(Kz, 3);
        document.getElementById("qz").textContent = formatNumber(qz, 1);
        document.getElementById("pe").textContent = formatNumber(pe, 1);
        document.getElementById("pnet").textContent = formatNumber(pnet, 1);
        document.getElementById("pu").textContent = formatNumber(pu, 1);
        document.getElementById("resultant-load").textContent = formatNumber(
          resultant,
          3
        );
      }

      document
        .getElementById("calculate-btn")
        .addEventListener("click", calculateTornadoLoads);

      document
        .getElementById("reset-btn")
        .addEventListener("click", function () {
          document.getElementById("risk-category").value = "II";
          document.getElementById("tornado-region").value = "moderate";
          document.getElementById("Vt").value = 200;
          document.getElementById("height").value = 30;
          document.getElementById("exposure").value = "C";
          document.getElementById("surface-type").value = "roof";
          document.getElementById("GCp").value = -0.9;
          document.getElementById("tributary-area").value = 100;
          document.getElementById("load-combo-factor").value = 1.0;
          document.getElementById("internal-pressure").value = 0.0;

          [
            "suggested-Vt",
            "It",
            "Kz",
            "qz",
            "pe",
            "pnet",
            "pu",
            "resultant-load",
          ].forEach((id) => {
            document.getElementById(id).textContent = "–";
          });
        });

      // Auto-update suggested Vt when region or risk category changes (without overwriting user edits)
      ["risk-category", "tornado-region"].forEach((id) => {
        document.getElementById(id).addEventListener("change", function () {
          const riskCategory = document.getElementById("risk-category").value;
          const region = document.getElementById("tornado-region").value;
          const suggestedVt = getSuggestedVt(region, riskCategory);
          document.getElementById("suggested-Vt").textContent = formatNumber(
            suggestedVt,
            0
          );
        });
      });

      // Initialize suggested Vt display
      window.addEventListener("DOMContentLoaded", function () {
        const riskCategory = document.getElementById("risk-category").value;
        const region = document.getElementById("tornado-region").value;
        const suggestedVt = getSuggestedVt(region, riskCategory);
        document.getElementById("suggested-Vt").textContent = formatNumber(
          suggestedVt,
          0
        );
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
