    <script>
      // Utility functions
      function roundTo(value, decimals) {
        const factor = Math.pow(10, decimals);
        return Math.round((value + Number.EPSILON) * factor) / factor;
      }

      function createSection(index) {
        const div = document.createElement("div");
        div.className =
          "border border-gray-200 rounded-md p-3 relative bg-gray-50";
        div.dataset.sectionIndex = index;

        div.innerHTML = `
      <div class="flex items-center justify-between mb-2">
        <h3 class="text-sm font-semibold text-gray-700">Section #<span class="section-number">${
          index + 1
        }</span></h3>
        <button type="button" class="text-xs text-red-600 hover:underline remove-section-btn">Remove</button>
      </div>
      <div class="input-grid">
        <div>
          <label class="block text-xs font-medium text-gray-700 mb-1">Length</label>
          <div class="flex items-center gap-2">
            <input type="number" step="0.01" min="0" class="slab-length w-full border-gray-300 rounded-md shadow-sm text-sm px-2 py-1" value="10">
            <span class="length-unit text-xs text-gray-600">ft</span>
          </div>
        </div>
        <div>
          <label class="block text-xs font-medium text-gray-700 mb-1">Width</label>
          <div class="flex items-center gap-2">
            <input type="number" step="0.01" min="0" class="slab-width w-full border-gray-300 rounded-md shadow-sm text-sm px-2 py-1" value="10">
            <span class="width-unit text-xs text-gray-600">ft</span>
          </div>
        </div>
        <div>
          <label class="block text-xs font-medium text-gray-700 mb-1">Thickness</label>
          <div class="flex items-center gap-2">
            <input type="number" step="0.25" min="0" class="slab-thickness w-full border-gray-300 rounded-md shadow-sm text-sm px-2 py-1" value="4">
            <span class="thickness-unit text-xs text-gray-600">in</span>
          </div>
        </div>
      </div>
      <p class="mt-2 text-xs text-gray-500 section-summary">Area: 100.0 ft²</p>
    `;
        return div;
      }

      function updateSectionUnits(unitSystem) {
        document
          .querySelectorAll("#sectionsContainer > div")
          .forEach((section) => {
            section.querySelector(".length-unit").textContent =
              unitSystem === "imperial" ? "ft" : "m";
            section.querySelector(".width-unit").textContent =
              unitSystem === "imperial" ? "ft" : "m";
            section.querySelector(".thickness-unit").textContent =
              unitSystem === "imperial" ? "in" : "cm";
            const summary = section.querySelector(".section-summary");
            if (summary) {
              summary.textContent =
                unitSystem === "imperial" ? "Area: 0.0 ft²" : "Area: 0.0 m²";
            }
          });
      }

      function updateDensityUnit(unitSystem) {
        const densityLabel = document.getElementById("densityUnit");
        const densityInput = document.getElementById("density");
        if (unitSystem === "imperial") {
          densityLabel.textContent = "lb/ft³";
          if (Math.abs(densityInput.value - 2400) < 10) {
            densityInput.value = 150;
          }
        } else {
          densityLabel.textContent = "kg/m³";
          if (Math.abs(densityInput.value - 150) < 10) {
            densityInput.value = 2400;
          }
        }
      }

      function updateRebarUnits(unitSystem) {
        document.getElementById("rebarSpacingUnit").textContent =
          unitSystem === "imperial" ? "in" : "cm";
        document.getElementById("rebarLengthUnit").textContent =
          unitSystem === "imperial" ? "ft" : "m";
        if (unitSystem === "metric") {
          const stock = document.getElementById("rebarLengthStock");
          if (Math.abs(stock.value - 20) < 0.1) stock.value = 6;
        } else {
          const stock = document.getElementById("rebarLengthStock");
          if (Math.abs(stock.value - 6) < 0.1) stock.value = 20;
        }
      }

      function recalc() {
        const unitSystem = document.querySelector(
          'input[name="unitSystem"]:checked'
        ).value;
        const wastePercent =
          parseFloat(document.getElementById("wastePercent").value) || 0;
        const bagSize =
          parseFloat(document.getElementById("bagSize").value) || 0;
        const costPerYard =
          parseFloat(document.getElementById("costPerYard").value) || 0;
        const costPerBag =
          parseFloat(document.getElementById("costPerBag").value) || 0;
        const density =
          parseFloat(document.getElementById("density").value) ||
          (unitSystem === "imperial" ? 150 : 2400);
        const enableRebar = document.getElementById("enableRebar").checked;
        const rebarSpacing =
          parseFloat(document.getElementById("rebarSpacing").value) || 0;
        const rebarLengthStock =
          parseFloat(document.getElementById("rebarLengthStock").value) || 0;
        const rebarWaste =
          parseFloat(document.getElementById("rebarWaste").value) || 0;

        let totalVolFt3 = 0;
        let totalVolM3 = 0;
        let totalAreaFt2 = 0;
        let totalAreaM2 = 0;

        const sections = document.querySelectorAll("#sectionsContainer > div");
        sections.forEach((section) => {
          const len =
            parseFloat(section.querySelector(".slab-length").value) || 0;
          const wid =
            parseFloat(section.querySelector(".slab-width").value) || 0;
          const thk =
            parseFloat(section.querySelector(".slab-thickness").value) || 0;

          let areaFt2 = 0,
            volFt3 = 0,
            areaM2 = 0,
            volM3 = 0;

          if (unitSystem === "imperial") {
            const thicknessFt = thk / 12.0;
            areaFt2 = len * wid;
            volFt3 = areaFt2 * thicknessFt;
            areaM2 = areaFt2 * 0.092903;
            volM3 = volFt3 * 0.0283168;
          } else {
            const lenM = len;
            const widM = wid;
            const thkM = thk / 100.0;
            areaM2 = lenM * widM;
            volM3 = areaM2 * thkM;
            areaFt2 = areaM2 * 10.7639;
            volFt3 = volM3 * 35.3147;
          }

          totalVolFt3 += volFt3;
          totalVolM3 += volM3;
          totalAreaFt2 += areaFt2;
          totalAreaM2 += areaM2;

          const summary = section.querySelector(".section-summary");
          if (summary) {
            if (unitSystem === "imperial") {
              summary.textContent = `Area: ${roundTo(areaFt2, 1)} ft²`;
            } else {
              summary.textContent = `Area: ${roundTo(areaM2, 2)} m²`;
            }
          }
        });

        const totalVolYd3 = totalVolFt3 / 27.0;
        const totalVolYd3WithWaste = totalVolYd3 * (1 + wastePercent / 100.0);
        const totalVolFt3WithWaste = totalVolFt3 * (1 + wastePercent / 100.0);
        const totalVolM3WithWaste = totalVolM3 * (1 + wastePercent / 100.0);

        document.getElementById("volNoWaste").textContent = `${roundTo(
          totalVolYd3,
          3
        )} yd³`;
        document.getElementById("volWithWaste").textContent = `${roundTo(
          totalVolYd3WithWaste,
          3
        )} yd³`;
        document.getElementById("volFt3Label").textContent = `${roundTo(
          totalVolFt3WithWaste,
          2
        )} ft³`;
        document.getElementById("volM3Label").textContent = `${roundTo(
          totalVolM3WithWaste,
          3
        )} m³`;

        let bagCount = 0;
        if (bagSize > 0) {
          if (unitSystem === "imperial") {
            bagCount = Math.ceil(totalVolFt3WithWaste / bagSize);
          } else {
            bagCount = Math.ceil(totalVolM3WithWaste / bagSize);
          }
        }
        document.getElementById("bagCount").textContent = isFinite(bagCount)
          ? bagCount
          : 0;

        let totalWeightLb, totalWeightKg;
        if (unitSystem === "imperial") {
          totalWeightLb = totalVolFt3WithWaste * density;
          totalWeightKg = totalWeightLb * 0.453592;
        } else {
          totalWeightKg = totalVolM3WithWaste * density;
          totalWeightLb = totalWeightKg * 2.20462;
        }
        document.getElementById("totalWeight").textContent = `${roundTo(
          totalWeightLb,
          0
        )} lb`;
        document.getElementById("totalWeightMetric").textContent = `${roundTo(
          totalWeightKg,
          0
        )} kg`;

        const costReadyMix =
          costPerYard > 0 ? totalVolYd3WithWaste * costPerYard : 0;
        const costBags =
          costPerBag > 0 && bagCount > 0 ? bagCount * costPerBag : 0;
        document.getElementById("costReadyMix").textContent = `$${roundTo(
          costReadyMix,
          2
        ).toFixed(2)}`;
        document.getElementById("costBags").textContent = `$${roundTo(
          costBags,
          2
        ).toFixed(2)}`;

        let totalRebarLenFt = 0;
        if (enableRebar && rebarSpacing > 0) {
          sections.forEach((section) => {
            const len =
              parseFloat(section.querySelector(".slab-length").value) || 0;
            const wid =
              parseFloat(section.querySelector(".slab-width").value) || 0;

            let lenFt, widFt, spacingFt;
            if (unitSystem === "imperial") {
              lenFt = len;
              widFt = wid;
              spacingFt = rebarSpacing / 12.0;
            } else {
              lenFt = len * 3.28084;
              widFt = wid * 3.28084;
              spacingFt = (rebarSpacing / 100.0) * 3.28084;
            }
            if (spacingFt <= 0) return;

            const barsAlongLength = Math.max(
              2,
              Math.floor(lenFt / spacingFt) + 1
            );
            const barsAlongWidth = Math.max(
              2,
              Math.floor(widFt / spacingFt) + 1
            );

            const lenDirTotal = barsAlongWidth * lenFt;
            const widDirTotal = barsAlongLength * widFt;

            totalRebarLenFt += lenDirTotal + widDirTotal;
          });

          totalRebarLenFt *= 1 + rebarWaste / 100.0;
        }

        const totalRebarLenM = totalRebarLenFt * 0.3048;
        const barsCount =
          enableRebar && rebarLengthStock > 0
            ? Math.ceil(
                unitSystem === "imperial"
                  ? totalRebarLenFt / rebarLengthStock
                  : totalRebarLenM / rebarLengthStock
              )
            : 0;

        document.getElementById("rebarLength").textContent = `${roundTo(
          totalRebarLenFt,
          1
        )} ft`;
        document.getElementById("rebarLengthMetric").textContent = `${roundTo(
          totalRebarLenM,
          1
        )} m`;
        document.getElementById("rebarBars").textContent = isFinite(barsCount)
          ? barsCount
          : 0;
      }

      document.addEventListener("DOMContentLoaded", () => {
        const sectionsContainer = document.getElementById("sectionsContainer");
        let sectionCount = 0;

        function addSection() {
          const section = createSection(sectionCount++);
          sectionsContainer.appendChild(section);
          updateSectionUnits(
            document.querySelector('input[name="unitSystem"]:checked').value
          );
          recalc();
        }

        addSection();

        document
          .getElementById("addSectionBtn")
          .addEventListener("click", () => {
            addSection();
          });

        sectionsContainer.addEventListener("click", (e) => {
          if (e.target.classList.contains("remove-section-btn")) {
            const section = e.target.closest("div[data-section-index]");
            if (section && sectionsContainer.children.length > 1) {
              section.remove();
              Array.from(sectionsContainer.children).forEach((sec, idx) => {
                const numSpan = sec.querySelector(".section-number");
                if (numSpan) numSpan.textContent = idx + 1;
              });
              recalc();
            }
          }
        });

        sectionsContainer.addEventListener("input", (e) => {
          if (
            e.target.classList.contains("slab-length") ||
            e.target.classList.contains("slab-width") ||
            e.target.classList.contains("slab-thickness")
          ) {
            recalc();
          }
        });

        document
          .querySelectorAll('input[name="unitSystem"]')
          .forEach((radio) => {
            radio.addEventListener("change", () => {
              const unitSystem = radio.value;
              updateSectionUnits(unitSystem);
              updateDensityUnit(unitSystem);
              updateRebarUnits(unitSystem);
              recalc();
            });
          });

        [
          "wastePercent",
          "bagSize",
          "costPerYard",
          "costPerBag",
          "density",
          "rebarSpacing",
          "rebarLengthStock",
          "rebarWaste",
        ].forEach((id) => {
          const el = document.getElementById(id);
          if (el) el.addEventListener("input", recalc);
        });

        document
          .getElementById("enableRebar")
          .addEventListener("change", (e) => {
            const cfg = document.getElementById("rebarConfig");
            if (e.target.checked) {
              cfg.classList.remove("opacity-50", "pointer-events-none");
            } else {
              cfg.classList.add("opacity-50", "pointer-events-none");
            }
            recalc();
          });

        document.getElementById("resetBtn").addEventListener("click", () => {
          sectionsContainer.innerHTML = "";
          sectionCount = 0;
          addSection();

          document.querySelector(
            'input[name="unitSystem"][value="imperial"]'
          ).checked = true;
          document.getElementById("wastePercent").value = 10;
          document.getElementById("bagSize").value = "0.6";
          document.getElementById("costPerYard").value = "";
          document.getElementById("costPerBag").value = "";
          document.getElementById("density").value = 150;
          document.getElementById("enableRebar").checked = false;
          document.getElementById("rebarSpacing").value = 18;
          document.getElementById("rebarLengthStock").value = 20;
          document.getElementById("rebarWaste").value = 5;
          document
            .getElementById("rebarConfig")
            .classList.add("opacity-50", "pointer-events-none");
          updateSectionUnits("imperial");
          updateDensityUnit("imperial");
          updateRebarUnits("imperial");
          recalc();
        });

        updateSectionUnits("imperial");
        updateDensityUnit("imperial");
        updateRebarUnits("imperial");
        recalc();
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
