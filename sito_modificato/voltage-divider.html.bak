    <script>
      // Utility: format number with sensible precision
      function fmt(val, decimals = 4) {
        if (!isFinite(val)) return "–";
        if (Math.abs(val) >= 1000 || Math.abs(val) < 0.001) {
          return val.toExponential(3);
        }
        return val
          .toFixed(decimals)
          .replace(/\.0+$/, "")
          .replace(/(\.\d*?[1-9])0+$/, "$1");
      }

      function showError(msg) {
        const el = document.getElementById("vd-error");
        el.textContent = msg;
        el.classList.remove("hidden");
      }

      function clearError() {
        const el = document.getElementById("vd-error");
        el.textContent = "";
        el.classList.add("hidden");
      }

      function parseOhms(id) {
        const v = document.getElementById(id).value.trim();
        if (v === "") return null;
        const num = Number(v);
        return isFinite(num) ? num : NaN;
      }

      function activeMode() {
        const btn = document.querySelector(
          ".vd-tab.border-b-2.border-blue-600"
        );
        return btn ? btn.getAttribute("data-mode") : "basic";
      }

      function setMode(mode) {
        document.querySelectorAll(".vd-tab").forEach((btn) => {
          if (btn.getAttribute("data-mode") === mode) {
            btn.classList.add("border-blue-600", "text-blue-600");
            btn.classList.remove("border-transparent", "text-gray-600");
          } else {
            btn.classList.remove("border-blue-600", "text-blue-600");
            btn.classList.add("border-transparent", "text-gray-600");
          }
        });
        document
          .getElementById("mode-basic")
          .classList.toggle("hidden", mode !== "basic");
        document
          .getElementById("mode-three")
          .classList.toggle("hidden", mode !== "three");
        document
          .getElementById("mode-inverse")
          .classList.toggle("hidden", mode !== "inverse");
      }

      document.querySelectorAll(".vd-tab").forEach((btn) => {
        btn.addEventListener("click", () => {
          setMode(btn.getAttribute("data-mode"));
          clearError();
          document.getElementById("results").classList.add("hidden");
        });
      });

      document.getElementById("btn-reset").addEventListener("click", () => {
        document.getElementById("vin").value = 12;
        document.getElementById("rload").value = "";
        document.getElementById("r1").value = 10000;
        document.getElementById("r2").value = 10000;
        document.getElementById("r1_3").value = 10000;
        document.getElementById("r2_3").value = 10000;
        document.getElementById("r3_3").value = 10000;
        document.getElementById("vout_target").value = 5;
        document.getElementById("rtotal").value = 20000;
        clearError();
        document.getElementById("results").classList.add("hidden");
      });

      document.getElementById("btn-calc").addEventListener("click", () => {
        clearError();
        const mode = activeMode();
        const vin = Number(document.getElementById("vin").value);
        if (!isFinite(vin)) {
          showError("Please enter a valid input voltage Vin.");
          return;
        }
        const rload = parseOhms("rload"); // may be null

        // Reset result visibility
        document.getElementById("res-vout1-row").classList.add("hidden");
        document.getElementById("res-vout2-row").classList.add("hidden");
        document.getElementById("res-vout-load-row").classList.add("hidden");
        document.getElementById("res-iload-row").classList.add("hidden");
        document.getElementById("res-p3-row").classList.add("hidden");

        let vout = NaN,
          voutLoad = NaN,
          idiv = NaN,
          iload = 0;
        let p1 = 0,
          p2 = 0,
          p3 = 0;

        if (mode === "basic") {
          const r1 = parseOhms("r1");
          const r2 = parseOhms("r2");
          if (!isFinite(r1) || r1 <= 0 || !isFinite(r2) || r2 <= 0) {
            showError("R1 and R2 must be positive numbers.");
            return;
          }
          const rtot = r1 + r2;
          idiv = vin / rtot;
          vout = vin * (r2 / rtot);
          p1 = idiv * idiv * r1;
          p2 = idiv * idiv * r2;

          if (rload && isFinite(rload) && rload > 0) {
            const req = 1 / (1 / r2 + 1 / rload);
            voutLoad = vin * (req / (r1 + req));
            iload = voutLoad / rload;
            document
              .getElementById("res-vout-load-row")
              .classList.remove("hidden");
            document.getElementById("res-iload-row").classList.remove("hidden");
          }
        } else if (mode === "three") {
          const r1 = parseOhms("r1_3");
          const r2 = parseOhms("r2_3");
          const r3 = parseOhms("r3_3");
          if (
            !isFinite(r1) ||
            r1 <= 0 ||
            !isFinite(r2) ||
            r2 <= 0 ||
            !isFinite(r3) ||
            r3 <= 0
          ) {
            showError("R1, R2 and R3 must be positive numbers.");
            return;
          }
          const rtot = r1 + r2 + r3;
          idiv = vin / rtot;
          const vout1 = vin * ((r2 + r3) / rtot);
          const vout2 = vin * (r3 / rtot);
          vout = vout2; // treat bottom tap as main Vout
          p1 = idiv * idiv * r1;
          p2 = idiv * idiv * r2;
          p3 = idiv * idiv * r3;

          document.getElementById("res-vout1-row").classList.remove("hidden");
          document.getElementById("res-vout2-row").classList.remove("hidden");
          document.getElementById("res-p3-row").classList.remove("hidden");
          document.getElementById("res-vout1").textContent = fmt(vout1, 4);
          document.getElementById("res-vout2").textContent = fmt(vout2, 4);

          if (rload && isFinite(rload) && rload > 0) {
            const req = 1 / (1 / r3 + 1 / rload);
            const rtotLoad = r1 + r2 + req;
            const idivLoad = vin / rtotLoad;
            voutLoad = vin * (req / rtotLoad);
            iload = voutLoad / rload;
            document
              .getElementById("res-vout-load-row")
              .classList.remove("hidden");
            document.getElementById("res-iload-row").classList.remove("hidden");
            // For simplicity, power values shown are still for the unloaded case.
          }
        } else if (mode === "inverse") {
          const voutTarget = Number(
            document.getElementById("vout_target").value
          );
          const rtotal = parseOhms("rtotal");
          if (!isFinite(voutTarget) || voutTarget <= 0) {
            showError("Please enter a positive desired Vout.");
            return;
          }
          if (!isFinite(rtotal) || rtotal <= 0) {
            showError("Total resistance must be a positive number.");
            return;
          }
          if (voutTarget >= vin) {
            showError(
              "Desired Vout must be less than Vin for a passive divider."
            );
            return;
          }
          const ratio = voutTarget / vin;
          const r2 = rtotal * ratio;
          const r1 = rtotal - r2;
          idiv = vin / rtotal;
          vout = voutTarget;
          p1 = idiv * idiv * r1;
          p2 = idiv * idiv * r2;

          // Display designed R1/R2 back into the basic fields for convenience
          document.getElementById("r1").value = fmt(r1, 2);
          document.getElementById("r2").value = fmt(r2, 2);
          setMode("basic");
        }

        // Populate shared results
        document.getElementById("res-vout").textContent = fmt(vout, 4);
        if (!isNaN(voutLoad)) {
          document.getElementById("res-vout-load").textContent = fmt(
            voutLoad,
            4
          );
        }
        document.getElementById("res-idiv").textContent = fmt(idiv * 1000, 3);
        if (iload) {
          document.getElementById("res-iload").textContent = fmt(
            iload * 1000,
            3
          );
        }
        document.getElementById("res-p1").textContent = fmt(p1 * 1000, 3);
        document.getElementById("res-p2").textContent = fmt(p2 * 1000, 3);
        if (p3) {
          document.getElementById("res-p3").textContent = fmt(p3 * 1000, 3);
        }

        document.getElementById("results").classList.remove("hidden");
      });

      // Default mode
