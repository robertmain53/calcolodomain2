<script>
        function calculateBiorhythms() {
            const birthDate = new Date(document.getElementById('birthDate').value);
            const today = new Date();
            const days = Math.floor((today - birthDate) / (1000 * 60 * 60 * 24));
            
            const physical = Math.sin((2 * Math.PI * days) / 23).toFixed(2);
            const emotional = Math.sin((2 * Math.PI * days) / 28).toFixed(2);
            const intellectual = Math.sin((2 * Math.PI * days) / 33).toFixed(2);

            document.getElementById('physicalCycle').textContent = physical;
            document.getElementById('emotionalCycle').textContent = emotional;
            document.getElementById('intellectualCycle').textContent = intellectual;
        }
    </script>


