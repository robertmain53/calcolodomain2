                  <div class="flex gap-2">
                    <input
                      id="ratio-short"
                      type="number"
                      step="0.01"
                      class="w-full border rounded-md px-3 py-2 text-sm"
                      placeholder="e.g. 150"
                    />
                    <span
                      class="input-unit flex items-center text-sm text-gray-500"
                      id="ratio-short-unit-label"
                      >mm</span
                    >
                  </div>
                </div>

                <div class="space-y-3">
                  <label class="block text-sm font-medium text-gray-700"
                    >Long side (height)</label
                  >
                  <div class="flex gap-2">
                    <input
                      id="ratio-long"
                      type="number"
                      step="0.01"
                      class="w-full border rounded-md px-3 py-2 text-sm"
                      placeholder="e.g. 150"
                    />
                    <span
                      class="input-unit flex items-center text-sm text-gray-500"
                      id="ratio-long-unit-label"
                      >mm</span
                    >
                  </div>
                </div>
              </div>

              <div class="grid md:grid-cols-3 gap-4 mt-2">
