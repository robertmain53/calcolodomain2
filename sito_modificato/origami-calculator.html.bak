    <script>
      // Tab switching
      document.querySelectorAll(".tab-btn").forEach((btn) => {
        btn.addEventListener("click", () => {
          const target = btn.getAttribute("data-tab");
          document
            .querySelectorAll(".tab-btn")
            .forEach((b) => b.classList.remove("tab-active"));
          document
            .querySelectorAll(".tab-panel")
            .forEach((p) => p.classList.add("hidden"));
          btn.classList.add("tab-active");
          document.getElementById(target).classList.remove("hidden");
        });
      });

      // Helper: unit label sync
      const ratioUnitSelect = document.getElementById("ratio-unit");
      const ratioShortUnitLabel = document.getElementById(
        "ratio-short-unit-label"
      );
      const ratioLongUnitLabel = document.getElementById(
        "ratio-long-unit-label"
      );
      const stripUnitSelect = document.getElementById("strip-unit");
      const stripMarginUnitLabel = document.getElementById(
        "strip-margin-unit-label"
      );
      const layoutUnitSelect = document.getElementById("layout-unit");
      const layoutGapUnitLabel = document.getElementById(
        "layout-gap-unit-label"
      );

      function syncUnitLabels() {
        const u1 = ratioUnitSelect.value;
        ratioShortUnitLabel.textContent = u1;
        ratioLongUnitLabel.textContent = u1;

        const u2 = stripUnitSelect.value;
        stripMarginUnitLabel.textContent = u2;

        const u3 = layoutUnitSelect.value;
        layoutGapUnitLabel.textContent = u3;
      }
      ratioUnitSelect.addEventListener("change", syncUnitLabels);
      stripUnitSelect.addEventListener("change", syncUnitLabels);
      layoutUnitSelect.addEventListener("change", syncUnitLabels);
      syncUnitLabels();

      // Presets for ratio tab (values in mm)
      const ratioPresets = {
        A4: { short: 210, long: 297, unit: "mm" },
        A3: { short: 297, long: 420, unit: "mm" },
        A5: { short: 148, long: 210, unit: "mm" },
        Letter: { short: 8.5, long: 11, unit: "in" },
        Square150: { short: 150, long: 150, unit: "mm" },
        Square200: { short: 200, long: 200, unit: "mm" },
      };

      document
        .getElementById("ratio-preset")
        .addEventListener("change", (e) => {
          const v = e.target.value;
          const preset = ratioPresets[v];
          if (!preset) return;
          document.getElementById("ratio-unit").value = preset.unit;
          document.getElementById("ratio-short").value = preset.short;
          document.getElementById("ratio-long").value = preset.long;
          syncUnitLabels();
          // auto-fill ratio from preset
          const r = preset.short / preset.long;
          document.getElementById("ratio-w").value = 1;
          document.getElementById("ratio-h").value = (1 / r).toFixed(4);
        });

      // Ratio calculation
      document.getElementById("ratio-calc").addEventListener("click", () => {
        const shortVal = parseFloat(
          document.getElementById("ratio-short").value
        );
        const longVal = parseFloat(document.getElementById("ratio-long").value);
        let rw = parseFloat(document.getElementById("ratio-w").value);
        let rh = parseFloat(document.getElementById("ratio-h").value);
        const target = document.getElementById("ratio-target").value;
        const unit = document.getElementById("ratio-unit").value;

        if (!shortVal && !longVal) {
          alert("Enter at least one side (short or long).");
          return;
        }

        // If ratio not fully specified, derive from given dimensions
        if (!rw || !rh) {
          if (shortVal && longVal) {
            rw = 1;
            rh = longVal / shortVal;
            document.getElementById("ratio-w").value = rw.toFixed(4);
            document.getElementById("ratio-h").value = rh.toFixed(4);
          } else {
            alert("Provide either both sides or a ratio.");
            return;
          }
        }

        let shortSide = shortVal;
        let longSide = longVal;

        const ratio = rw / rh;

        if (target === "short") {
          if (!shortSide) {
            alert("Enter the short side length.");
            return;
          }
          longSide = shortSide / ratio;
          document.getElementById("ratio-long").value = longSide.toFixed(2);
        } else {
          if (!longSide) {
            alert("Enter the long side length.");
            return;
          }
          shortSide = longSide * ratio;
          document.getElementById("ratio-short").value = shortSide.toFixed(2);
        }

        const ratioText = `Width : Height ≈ 1 : ${(1 / ratio).toFixed(
          4
        )} (or ${ratio.toFixed(4)} : 1)`;
        const sizeText = `Short side ≈ ${shortSide.toFixed(
          2
        )} ${unit}, Long side ≈ ${longSide.toFixed(2)} ${unit}`;

        const out = document.getElementById("ratio-result");
        document.getElementById(
          "ratio-result-text"
        ).innerHTML = `${sizeText}<br>${ratioText}`;
        out.classList.remove("hidden");
      });

      // Strips & grids
      document.getElementById("strip-calc").addEventListener("click", () => {
        const W = parseFloat(document.getElementById("strip-width").value);
        const n = parseInt(document.getElementById("strip-count").value, 10);
        const m = parseFloat(
          document.getElementById("strip-margin").value || "0"
        );
        const unit = document.getElementById("strip-unit").value;
        const roundMode = document.getElementById("strip-round").value;

        if (!W || !n || n <= 0) {
          alert("Enter paper width and a positive number of strips.");
          return;
        }
        if (2 * m >= W) {
          alert("Margins are too large for the given width.");
          return;
        }

        const usable = W - 2 * m;
        let wStrip = usable / n;

        // Rounding in mm or inches
        let step = 0;
        if (roundMode === "0.5") step = unit === "in" ? 1 / 64 : 0.5;
        if (roundMode === "1") step = unit === "in" ? 1 / 32 : 1;
        if (step > 0) {
          wStrip = Math.round(wStrip / step) * step;
        }

        const used = wStrip * n;
        const leftover = W - 2 * m - used;

        const textLines = [];
        textLines.push(`Strip width ≈ ${wStrip.toFixed(3)} ${unit}`);
        textLines.push(
          `Total used width (excluding margins): ${used.toFixed(3)} ${unit}`
        );
        textLines.push(`Leftover inside width: ${leftover.toFixed(3)} ${unit}`);
        textLines.push(`Outer margins: ${m.toFixed(3)} ${unit} on each side`);

        const out = document.getElementById("strip-result");
        document.getElementById("strip-result-text").innerHTML =
          textLines.join("<br>");
        out.classList.remove("hidden");
      });

      // Sheet layout
      document.getElementById("layout-calc").addEventListener("click", () => {
        const Bw = parseFloat(document.getElementById("layout-big-w").value);
        const Bh = parseFloat(document.getElementById("layout-big-h").value);
        const Sw = parseFloat(document.getElementById("layout-small-w").value);
        const Sh = parseFloat(document.getElementById("layout-small-h").value);
        const gap = parseFloat(
          document.getElementById("layout-gap").value || "0"
        );
        const allowRotate =
          document.getElementById("layout-rotate").value === "yes";
        const unit = document.getElementById("layout-unit").value;

        if (!Bw || !Bh || !Sw || !Sh) {
          alert("Enter both large sheet and small piece dimensions.");
          return;
        }

        function layoutCount(Bw, Bh, Sw, Sh, g) {
          const across = Math.max(0, Math.floor((Bw + g) / (Sw + g)));
          const down = Math.max(0, Math.floor((Bh + g) / (Sh + g)));
          return { across, down, total: across * down };
        }

        const base = layoutCount(Bw, Bh, Sw, Sh, gap);
        let best = { ...base, rotated: false };

        if (allowRotate) {
          const rot = layoutCount(Bw, Bh, Sh, Sw, gap);
          if (rot.total > best.total) {
            best = { ...rot, rotated: true };
          }
        }

        const usedW = best.across * Sw + (best.across - 1) * gap;
        const usedH = best.down * Sh + (best.down - 1) * gap;
        const leftoverW = Bw - usedW;
        const leftoverH = Bh - usedH;

        const lines = [];
        lines.push(`Pieces across × down: ${best.across} × ${best.down}`);
        lines.push(`Total pieces: ${best.total}`);
        lines.push(
          `Orientation: ${
            best.rotated
              ? "rotated (small width = " + Sh + " " + unit + ")"
              : "not rotated"
          }`
        );
        lines.push(
          `Used area: ${usedW.toFixed(2)} × ${usedH.toFixed(2)} ${unit}`
        );
        lines.push(
          `Leftover margins: width ≈ ${leftoverW.toFixed(
            2
          )} ${unit}, height ≈ ${leftoverH.toFixed(2)} ${unit}`
        );

        const out = document.getElementById("layout-result");
        document.getElementById("layout-result-text").innerHTML =
          lines.join("<br>");
        out.classList.remove("hidden");
      });

      // Scale model
      document.getElementById("scale-calc").addEventListener("click", () => {
        const P0 = parseFloat(
          document.getElementById("scale-paper-original").value
        );
        const M0 = parseFloat(
          document.getElementById("scale-model-original").value
        );
        const Mt = parseFloat(
          document.getElementById("scale-model-target").value
        );
        const inUnit = document.getElementById("scale-unit").value;
        const outUnitSel = document.getElementById("scale-output-unit").value;

        if (!P0 || !M0 || !Mt) {
          alert(
            "Enter original paper size, original model size, and desired model size."
          );
          return;
        }

        const scale = Mt / M0;
        let Pt = P0 * scale;

        // Unit conversion if needed
        let outUnit = inUnit;
        if (outUnitSel !== "same") {
          outUnit = outUnitSel;
          // convert P0 to mm, then to outUnit
          function toMm(v, u) {
            if (u === "mm") return v;
            if (u === "cm") return v * 10;
            if (u === "in") return v * 25.4;
            return v;
          }
          function fromMm(v, u) {
            if (u === "mm") return v;
            if (u === "cm") return v / 10;
            if (u === "in") return v / 25.4;
            return v;
          }
          const P0mm = toMm(P0, inUnit);
          Pt = fromMm(P0mm * scale, outUnit);
        }

        const lines = [];
        lines.push(`Scale factor ≈ ${scale.toFixed(4)}×`);
        lines.push(`Required paper (short side) ≈ ${Pt.toFixed(2)} ${outUnit}`);
        lines.push(`(Assuming the model scales linearly with paper size.)`);

        const out = document.getElementById("scale-result");
        document.getElementById("scale-result-text").innerHTML =
          lines.join("<br>");
        out.classList.remove("hidden");
      });

      // Mobile menu script (from template)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const expanded = this.getAttribute("aria-expanded") === "true";
          this.setAttribute("aria-expanded", String(!expanded));
          menu.classList.toggle("hidden");
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
