    <script>
      (function () {
        const unitSystemEl = document.getElementById("unitSystem");
        const designModeEl = document.getElementById("designMode");
        const runBtn = document.getElementById("runCalc");
        const resetBtn = document.getElementById("resetCalc");
        const resultsContainer = document.getElementById("resultsContainer");

        const ids = {
          b: "b",
          h: "h",
          d: "d",
          fc: "fc",
          fy: "fy",
          As: "As",
          MuCheck: "MuCheck",
          MuDesign: "MuDesign",
          phiOverride: "phiOverride",
          rhoMaxFactor: "rhoMaxFactor",
        };

        function $(id) {
          return document.getElementById(id);
        }

        function setUnits() {
          const us = unitSystemEl.value === "us";
          $("bUnit").textContent = us ? "in" : "mm";
          $("hUnit").textContent = us ? "in" : "mm";
          $("dUnit").textContent = us ? "in" : "mm";
          $("fcUnit").textContent = us ? "ksi" : "MPa";
          $("fyUnit").textContent = us ? "ksi" : "MPa";
          $("AsUnit").textContent = us ? "in²" : "mm²";
          $("MuCheckUnit").textContent = us ? "kip·ft" : "kN·m";
          $("MuDesignUnit").textContent = us ? "kip·ft" : "kN·m";

          // Provide reasonable defaults when switching
          if (us) {
            if (!$("b").dataset.setByUser) $("b").value = 12;
            if (!$("h").dataset.setByUser) $("h").value = 20;
            if (!$("d").dataset.setByUser) $("d").value = 18;
            if (!$("fc").dataset.setByUser) $("fc").value = 4;
            if (!$("fy").dataset.setByUser) $("fy").value = 60;
            if (!$("As").dataset.setByUser) $("As").value = 3;
            if (!$("MuDesign").dataset.setByUser) $("MuDesign").value = 150;
          } else {
            if (!$("b").dataset.setByUser) $("b").value = 300;
            if (!$("h").dataset.setByUser) $("h").value = 500;
            if (!$("d").dataset.setByUser) $("d").value = 450;
            if (!$("fc").dataset.setByUser) $("fc").value = 30;
            if (!$("fy").dataset.setByUser) $("fy").value = 420;
            if (!$("As").dataset.setByUser) $("As").value = 1500;
            if (!$("MuDesign").dataset.setByUser) $("MuDesign").value = 200;
          }
        }

        // Track manual edits so we don't overwrite on unit change
        ["b", "h", "d", "fc", "fy", "As", "MuDesign"].forEach((id) => {
          $(id).addEventListener("input", () => {
            $(id).dataset.setByUser = "1";
          });
        });

        function setMode() {
          const mode = designModeEl.value;
          const checkDiv = document.getElementById("checkInputs");
          const designDiv = document.getElementById("designInputs");
          if (mode === "check") {
            checkDiv.classList.remove("hidden");
            designDiv.classList.add("hidden");
          } else {
            checkDiv.classList.add("hidden");
            designDiv.classList.remove("hidden");
          }
        }

        function beta1(fc_psi) {
          // fc_psi in psi
          if (fc_psi <= 4000) return 0.85;
          if (fc_psi >= 8000) return 0.65;
          return 0.85 - 0.05 * ((fc_psi - 4000) / 1000);
        }

        function computePhi(eps_t) {
          // Simple approximation based on ACI tension/compression limits
          if (!isFinite(eps_t) || eps_t <= 0) return 0.65;
          if (eps_t >= 0.005) return 0.9;
          if (eps_t <= 0.002) return 0.65;
          // Linear interpolation between 0.002 and 0.005
          const phiMin = 0.65,
            phiMax = 0.9;
          return (
            phiMin + (phiMax - phiMin) * ((eps_t - 0.002) / (0.005 - 0.002))
          );
        }

        function formatNumber(val, digits) {
          if (!isFinite(val)) return "–";
          return Number(val.toFixed(digits)).toString();
        }

        function calculate() {
          const us = unitSystemEl.value === "us";

          function val(id) {
            const v = parseFloat($(id).value);
            return isNaN(v) ? null : v;
          }

          const b = val("b");
          const h = val("h");
          const d = val("d");
          const fc = val("fc");
          const fy = val("fy");
          const As = val("As");
          const MuCheck = val("MuCheck");
          const MuDesign = val("MuDesign");
          const phiOverride = val("phiOverride");
          const rhoMaxFactor = val("rhoMaxFactor") ?? 0.75;

          const errors = [];
          if (!b || b <= 0) errors.push("Beam width b must be positive.");
          if (!h || h <= 0) errors.push("Overall depth h must be positive.");
          if (!d || d <= 0 || d >= h)
            errors.push("Effective depth d must be positive and less than h.");
          if (!fc || fc <= 0)
            errors.push("Concrete strength f'c must be positive.");
          if (!fy || fy <= 0)
            errors.push("Steel yield strength fy must be positive.");
          if (designModeEl.value === "check") {
            if (!As || As <= 0) errors.push("Steel area As must be positive.");
          } else {
            if (!MuDesign || MuDesign <= 0)
              errors.push("Required factored moment Mu must be positive.");
          }

          if (errors.length) {
            resultsContainer.innerHTML =
              '<div class="p-4 rounded-md bg-red-50 border border-red-200 text-sm text-red-700">' +
              '<ul class="list-disc ml-5">' +
              errors.map((e) => "<li>" + e + "</li>").join("") +
              "</ul></div>";
            return;
          }

          // Convert to consistent internal units: use US (in, ksi, kips) internally
          let b_in, d_in, fc_ksi, fy_ksi, As_in2, Mu_kipft;
          if (us) {
            b_in = b;
            d_in = d;
            fc_ksi = fc;
            fy_ksi = fy;
            As_in2 = As;
            Mu_kipft = designModeEl.value === "check" ? MuCheck : MuDesign;
          } else {
            b_in = b / 25.4;
            d_in = d / 25.4;
            fc_ksi = fc / 6.894757; // MPa to ksi
            fy_ksi = fy / 6.894757;
            As_in2 = As / (25.4 * 25.4);
            Mu_kipft =
              (designModeEl.value === "check" ? MuCheck : MuDesign) * 0.737562; // kN·m to kip·ft
          }

          const beta1Val = beta1(fc_ksi * 1000); // fc in psi
          const a = (As_in2 * fy_ksi) / (0.85 * fc_ksi * b_in);
          const c = a / beta1Val;
          const eps_t = (0.003 * (d_in - c)) / c;

          let phi =
            phiOverride && phiOverride > 0 ? phiOverride : computePhi(eps_t);

          const Mn_kipin = As_in2 * fy_ksi * (d_in - a / 2); // kip·in
          const Mn_kipft = Mn_kipin / 12;
          const phiMn_kipft = phi * Mn_kipft;

          // Reinforcement ratio
          const rho = As_in2 / (b_in * d_in);

          // Balanced ratio (approximate)
          const Es_ksi = 29000;
          const eps_y = fy_ksi / Es_ksi;
          const eps_cu = 0.003;
          const rho_b =
            0.85 * beta1Val * (fc_ksi / fy_ksi) * (eps_cu / (eps_cu + eps_y));
          const rho_max = rhoMaxFactor * rho_b;

          // Convert results back to selected units
          let Mn_disp,
            phiMn_disp,
            c_disp,
            a_disp,
            As_req_disp = null,
            Mu_disp = null;
          if (us) {
            Mn_disp = Mn_kipft;
            phiMn_disp = phiMn_kipft;
            c_disp = c;
            a_disp = a;
            Mu_disp = Mu_kipft;
          } else {
            Mn_disp = Mn_kipft / 0.737562; // kN·m
            phiMn_disp = phiMn_kipft / 0.737562;
            c_disp = c * 25.4; // mm
            a_disp = a * 25.4; // mm
            Mu_disp = Mu_kipft / 0.737562;
          }

          let capacityMsg = "";
          if (designModeEl.value === "check" && MuCheck) {
            const ok = phiMn_kipft >= Mu_kipft;
            const ratio = phiMn_kipft / Mu_kipft;
            capacityMsg = ok
              ? `OK: φMₙ ≥ Mᵤ (capacity ratio = ${formatNumber(
                  ratio,
                  2
                )} ≥ 1.0).`
              : `NG: φMₙ &lt; Mᵤ (capacity ratio = ${formatNumber(
                  ratio,
                  2
                )} &lt; 1.0). Increase Aₛ, b, or d.`;
          }

          let designMsg = "";
          if (designModeEl.value === "design") {
            // Simple design: assume tension-controlled φ = 0.9, solve for As iteratively
            const phi_design = 0.9;
            const Mu_req_kipft = Mu_kipft;
            // Use iterative approach on As
            let As_try =
              (Mu_req_kipft * 12) / (phi_design * fy_ksi * (0.9 * d_in)); // rough initial guess
            for (let i = 0; i < 20; i++) {
              const a_try = (As_try * fy_ksi) / (0.85 * fc_ksi * b_in);
              const Mn_try_kipin = As_try * fy_ksi * (d_in - a_try / 2);
              const phiMn_try_kipft = phi_design * (Mn_try_kipin / 12);
              const error = Mu_req_kipft - phiMn_try_kipft;
              if (Math.abs(error) < 1e-3) break;
              // Update As_try (simple proportional correction)
              As_try *= 1 + error / Mu_req_kipft;
              if (As_try <= 0) {
                As_try = 0.1;
              }
            }
            As_req_disp = us ? As_try : As_try * 25.4 * 25.4;

            const rho_req = As_try / (b_in * d_in);
            const overMax = rho_req > rho_max;
            designMsg = overMax
              ? "Warning: Required reinforcement exceeds the specified maximum ratio ρₘₐₓ. Consider increasing b or d, or using a doubly reinforced section."
              : "Required reinforcement is within the specified maximum ratio ρₘₐₓ.";
          }

          const behavior =
            eps_t >= 0.005
              ? "Tension-controlled"
              : eps_t <= 0.002
              ? "Compression-controlled"
              : "Transition";

          const unitLen = us ? "in" : "mm";
          const unitArea = us ? "in²" : "mm²";
          const unitMoment = us ? "kip·ft" : "kN·m";

          let html = "";

          html += '<div class="grid md:grid-cols-2 gap-4">';
          html +=
            '<div class="p-4 rounded-md bg-blue-50 border border-blue-200 text-sm">';
          html += '<h3 class="font-semibold mb-2">Flexural strength</h3>';
          html +=
            '<p><span class="font-medium">β₁:</span> ' +
            formatNumber(beta1Val, 3) +
            "</p>";
          html +=
            '<p><span class="font-medium">a:</span> ' +
            formatNumber(a_disp, 2) +
            " " +
            unitLen +
            "</p>";
          html +=
            '<p><span class="font-medium">c:</span> ' +
            formatNumber(c_disp, 2) +
            " " +
            unitLen +
            "</p>";
          html +=
            '<p><span class="font-medium">εₜ:</span> ' +
            formatNumber(eps_t, 5) +
            "</p>";
          html +=
            '<p><span class="font-medium">Section behavior:</span> ' +
            behavior +
            "</p>";
          html +=
            '<p><span class="font-medium">φ (used):</span> ' +
            formatNumber(phi, 3) +
            "</p>";
          html +=
            '<p><span class="font-medium">Mₙ:</span> ' +
            formatNumber(Mn_disp, 2) +
            " " +
            unitMoment +
            "</p>";
          html +=
            '<p><span class="font-medium">φMₙ:</span> ' +
            formatNumber(phiMn_disp, 2) +
            " " +
            unitMoment +
            "</p>";
          if (designModeEl.value === "check" && MuCheck) {
            html +=
              '<p class="mt-2"><span class="font-medium">Mᵤ (input):</span> ' +
              formatNumber(Mu_disp, 2) +
              " " +
              unitMoment +
              "</p>";
          }
          html += "</div>";

          html +=
            '<div class="p-4 rounded-md bg-green-50 border border-green-200 text-sm">';
          html += '<h3 class="font-semibold mb-2">Reinforcement ratios</h3>';
          html +=
            '<p><span class="font-medium">ρ:</span> ' +
            formatNumber(rho, 4) +
            "</p>";
          html +=
            '<p><span class="font-medium">ρ_b (balanced):</span> ' +
            formatNumber(rho_b, 4) +
            "</p>";
          html +=
            '<p><span class="font-medium">ρₘₐₓ = ' +
            formatNumber(rhoMaxFactor, 2) +
            "·ρ_b:</span> " +
            formatNumber(rho_max, 4) +
            "</p>";
          if (designModeEl.value === "design" && As_req_disp != null) {
            html += '<hr class="my-2">';
            html +=
              '<p><span class="font-medium">Required Aₛ (φ = 0.90 assumed):</span> ' +
              formatNumber(As_req_disp, 2) +
              " " +
              unitArea +
              "</p>";
            html += '<p class="mt-1 text-gray-700">' + designMsg + "</p>";
          } else {
            html +=
              '<p class="mt-2 text-gray-700">Provided Aₛ: ' +
              formatNumber(us ? As_in2 : As_in2 * 25.4 * 25.4, 2) +
              " " +
              unitArea +
              "</p>";
          }
          html += "</div>";
          html += "</div>";

          if (designModeEl.value === "check" && capacityMsg) {
            html +=
              '<div class="mt-4 p-4 rounded-md ' +
              (capacityMsg.startsWith("OK")
                ? "bg-emerald-50 border border-emerald-200 text-emerald-800"
                : "bg-red-50 border border-red-200 text-red-800") +
              ' text-sm">' +
              capacityMsg +
              "</div>";
          }

          html +=
            '<p class="mt-4 text-xs text-gray-500">Note: This tool is for singly reinforced rectangular beams only and does not check shear, deflection, or detailing requirements.</p>';

          resultsContainer.innerHTML = html;
        }

        function reset() {
          unitSystemEl.value = "si";
          designModeEl.value = "check";
          [
            "b",
            "h",
            "d",
            "fc",
            "fy",
            "As",
            "MuCheck",
            "MuDesign",
            "phiOverride",
            "rhoMaxFactor",
          ].forEach((id) => {
            const el = $(id);
            el.value = "";
            delete el.dataset.setByUser;
          });
          $("b").value = 300;
          $("h").value = 500;
          $("d").value = 450;
          $("fc").value = 30;
          $("fy").value = 420;
          $("As").value = 1500;
          $("rhoMaxFactor").value = 0.75;
          $("MuCheck").value = "";
          $("MuDesign").value = 200;
          $("phiOverride").value = "";
          setUnits();
          setMode();
          resultsContainer.innerHTML =
            '<div class="p-4 rounded-md bg-gray-50 border border-dashed border-gray-300 text-sm text-gray-600">Enter beam data and click “Calculate” to see flexural capacity, neutral axis depth, and required steel area.</div>';
        }

        unitSystemEl.addEventListener("change", () => {
          setUnits();
        });
        designModeEl.addEventListener("change", () => {
          setMode();
        });
        runBtn.addEventListener("click", calculate);
        resetBtn.addEventListener("click", reset);

        // Initial setup
        setUnits();
        setMode();
      })();
    </script>
  
