    <script>
      // Utility: format number with 2-3 decimals and unit
      function fmt(value, decimals = 2) {
        if (!isFinite(value)) return "-";
        return value.toFixed(decimals);
      }

      const modeSizeBtn = document.getElementById("mode-size");
      const modeCheckBtn = document.getElementById("mode-check");
      let currentMode = "size"; // 'size' or 'check'

      function setMode(mode) {
        currentMode = mode;
        if (mode === "size") {
          modeSizeBtn.classList.remove("bg-gray-100", "text-gray-700");
          modeSizeBtn.classList.add("bg-blue-600", "text-white");
          modeCheckBtn.classList.remove("bg-blue-600", "text-white");
          modeCheckBtn.classList.add("bg-gray-100", "text-gray-700");
        } else {
          modeCheckBtn.classList.remove("bg-gray-100", "text-gray-700");
          modeCheckBtn.classList.add("bg-blue-600", "text-white");
          modeSizeBtn.classList.remove("bg-blue-600", "text-white");
          modeSizeBtn.classList.add("bg-gray-100", "text-gray-700");
        }
      }

      modeSizeBtn.addEventListener("click", () => setMode("size"));
      modeCheckBtn.addEventListener("click", () => setMode("check"));

      // Unit system handling
      const unitRadios = document.querySelectorAll('input[name="unitSystem"]');
      const loadUnitSpan = document.getElementById("loadUnit");
      const qAllowUnitSpan = document.getElementById("qAllowUnit");
      const footingBUnitSpan = document.getElementById("footingBUnit");
      const footingLUnitSpan = document.getElementById("footingLUnit");
      const depthUnitSpan = document.getElementById("depthUnit");

      function updateUnits() {
        const system = document.querySelector(
          'input[name="unitSystem"]:checked'
        ).value;
        if (system === "metric") {
          loadUnitSpan.textContent = "kN";
          qAllowUnitSpan.textContent = "kPa";
          footingBUnitSpan.textContent = "m";
          footingLUnitSpan.textContent = "m";
          depthUnitSpan.textContent = "m";
        } else {
          loadUnitSpan.textContent = "kips";
          qAllowUnitSpan.textContent = "psf";
          footingBUnitSpan.textContent = "ft";
          footingLUnitSpan.textContent = "ft";
          depthUnitSpan.textContent = "ft";
        }
      }

      unitRadios.forEach((r) => r.addEventListener("change", updateUnits));
      updateUnits();

      const calculateBtn = document.getElementById("calculateBtn");
      const resetBtn = document.getElementById("resetBtn");
      const resultsDiv = document.getElementById("results");
      const resultsContent = document.getElementById("resultsContent");

      calculateBtn.addEventListener("click", () => {
        const system = document.querySelector(
          'input[name="unitSystem"]:checked'
        ).value;

        const load = parseFloat(document.getElementById("load").value);
        const selfWeightFactor =
          parseFloat(document.getElementById("selfWeightFactor").value) || 1.0;
        const qAllow = parseFloat(document.getElementById("qAllow").value);
        const B = parseFloat(document.getElementById("footingB").value);
        const L = parseFloat(document.getElementById("footingL").value);
        const aspectRatio =
          parseFloat(document.getElementById("aspectRatio").value) || 1.0;
        const fsBearing =
          parseFloat(document.getElementById("fsBearing").value) || 1.0;

        const unitLoad = system === "metric" ? "kN" : "kips";
        const unitLength = system === "metric" ? "m" : "ft";
        const unitPressure = system === "metric" ? "kPa" : "psf";

        let errors = [];
        if (!isFinite(load) || load <= 0)
          errors.push("Enter a positive service load.");
        if (!isFinite(qAllow) || qAllow <= 0)
          errors.push("Enter a positive allowable soil bearing capacity.");
        if (selfWeightFactor <= 0)
          errors.push("Self-weight factor must be greater than 0.");
        if (fsBearing <= 0)
          errors.push("Factor of safety on bearing must be greater than 0.");

        if (currentMode === "check") {
          if (!isFinite(B) || B <= 0 || !isFinite(L) || L <= 0) {
            errors.push(
              "In bearing check mode, enter both footing dimensions B and L."
            );
          }
        }

        if (errors.length) {
          resultsDiv.classList.remove("hidden");
          resultsContent.innerHTML =
            '<p class="text-red-600 font-medium">' +
            errors.join("<br>") +
            "</p>";
          return;
        }

        const Peff = load * selfWeightFactor;
        const qDesign = qAllow / fsBearing;

        let html = "";
        html += `<p><strong>Unit system:</strong> ${
          system === "metric"
            ? "Metric (kN, m, kPa)"
            : "Imperial (kips, ft, psf)"
        }</p>`;
        html += `<p><strong>Effective service load (including self-weight):</strong> ${fmt(
          Peff
        )} ${unitLoad}</p>`;
        html += `<p><strong>Design bearing capacity q<sub>design</sub>:</strong> ${fmt(
          qDesign
        )} ${unitPressure}</p>`;

        if (currentMode === "size") {
          // Sizing mode
          const Areq = Peff / qDesign; // area
          let Buse = B;
          let Luse = L;

          if (!isFinite(Buse) || Buse <= 0 || !isFinite(Luse) || Luse <= 0) {
            // derive from aspect ratio
            const r = aspectRatio > 0 ? aspectRatio : 1.0;
            Buse = Math.sqrt(Areq / r);
            Luse = r * Buse;
          }

          const qAct = Peff / (Buse * Luse);
          const utilization = qAct / qDesign;

          html += `<p><strong>Required footing area A<sub>req</sub>:</strong> ${fmt(
            Areq,
            3
          )} ${unitLength}²</p>`;
          html += `<p><strong>Suggested footing dimensions:</strong> B = ${fmt(
            Buse,
            3
          )} ${unitLength}, L = ${fmt(Luse, 3)} ${unitLength} (L/B ≈ ${fmt(
            Luse / Buse,
            2
          )})</p>`;
          html += `<p><strong>Resulting bearing pressure q<sub>act</sub>:</strong> ${fmt(
            qAct,
            2
          )} ${unitPressure}</p>`;

          const status = utilization <= 1 ? "PASS" : "FAIL";
          const color = utilization <= 1 ? "green" : "red";
          html += `<p><span class="badge bg-${color}-100 text-${color}-800 border border-${color}-300 font-semibold">Bearing check: ${status}</span> (utilization = ${fmt(
            utilization * 100,
            1
          )}%)</p>`;
          html += `<p class="text-xs text-gray-500 mt-2">Note: dimensions are approximate and should be rounded to practical construction sizes and then rechecked.</p>`;
        } else {
          // Check mode
          const A = B * L;
          const qAct = Peff / A;
          const utilization = qAct / qDesign;
          const status = utilization <= 1 ? "PASS" : "FAIL";
          const color = utilization <= 1 ? "green" : "red";

          html += `<p><strong>Footing area A:</strong> ${fmt(
            A,
            3
          )} ${unitLength}² (B = ${fmt(B, 3)} ${unitLength}, L = ${fmt(
            L,
            3
          )} ${unitLength})</p>`;
          html += `<p><strong>Actual bearing pressure q<sub>act</sub>:</strong> ${fmt(
            qAct,
            2
          )} ${unitPressure}</p>`;
          html += `<p><span class="badge bg-${color}-100 text-${color}-800 border border-${color}-300 font-semibold">Bearing check: ${status}</span> (utilization = ${fmt(
            utilization * 100,
            1
          )}%)</p>`;
          if (utilization > 1) {
            html += `<p class="text-xs text-red-600 mt-2">q<sub>act</sub> exceeds q<sub>design</sub>. Increase footing size, reduce load, or use a different foundation system.</p>`;
          } else {
            html += `<p class="text-xs text-gray-500 mt-2">q<sub>act</sub> is within allowable limits. Still verify settlement and structural design separately.</p>`;
          }
        }

        resultsDiv.classList.remove("hidden");
        resultsContent.innerHTML = html;
      });

      resetBtn.addEventListener("click", () => {
        document.getElementById("load").value = "";
        document.getElementById("selfWeightFactor").value = "1.1";
        document.getElementById("qAllow").value = "";
        document.getElementById("footingB").value = "";
        document.getElementById("footingL").value = "";
        document.getElementById("aspectRatio").value = "1.0";
        document.getElementById("depth").value = "";
        document.getElementById("fsBearing").value = "1.0";
        resultsDiv.classList.add("hidden");
        resultsContent.innerHTML = "";
        setMode("size");
        updateUnits();
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
