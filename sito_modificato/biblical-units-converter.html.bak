          <script>
            (function(){
              const errBox = document.getElementById('error-box');
              const errMsg = document.getElementById('error-msg');
              function showError(msg){ errMsg.textContent = msg; errBox.classList.remove('hidden'); }
              function clearError(){ errBox.classList.add('hidden'); }

              // Tabs
              const tabLength = document.getElementById('tab-length');
              const tabVolume = document.getElementById('tab-volume');
              const tabWeight = document.getElementById('tab-weight');
              const lengthBox = document.getElementById('length-box');
              const volumeBox = document.getElementById('volume-box');
              const weightBox = document.getElementById('weight-box');

              function activate(tab){
                tabLength.classList.replace('tab-active','tab-inactive');
                tabVolume.classList.replace('tab-active','tab-inactive');
                tabWeight.classList.replace('tab-active','tab-inactive');
                lengthBox.classList.add('hidden');
                volumeBox.classList.add('hidden');
                weightBox.classList.add('hidden');

                tab.classList.replace('tab-inactive','tab-active');
              }

              tabLength.addEventListener('click', () => {
                activate(tabLength);
                lengthBox.classList.remove('hidden');
              });
              tabVolume.addEventListener('click', () => {
                activate(tabVolume);
                volumeBox.classList.remove('hidden');
              });
              tabWeight.addEventListener('click', () => {
                activate(tabWeight);
                weightBox.classList.remove('hidden');
              });

              // data
              const lengthUnits = {
                "cubit (≈0.45 m)": 0.45,
                "span (≈0.23 m)": 0.23,
                "handbreadth / palm (≈0.08 m)": 0.08,
                "finger / digit (≈0.02 m)": 0.02,
                "meter (m)": 1,
                "centimeter (cm)": 0.01,
                "inch (in)": 0.0254,
                "foot (ft)": 0.3048
              };
              const volumeUnits = {
                "homer / cor (220 L)": 220,
                "ephah / bath (22 L)": 22,
                "seah (7.3 L)": 7.3,
                "omer (2 L)": 2,
                "hin (4 L)": 4,
                "log (0.3 L)": 0.3,
                "liter (L)": 1,
                "milliliter (mL)": 0.001,
                "US gallon (gal)": 3.785411784
              };
              const weightUnits = {
                "talent (34.272 kg)": 34272,
                "mina (571.2 g)": 571.2,
                "shekel (11.424 g)": 11.424,
                "beka (5.712 g)": 5.712,
                "gerah (0.5712 g)": 0.5712,
                "gram (g)": 1,
                "kilogram (kg)": 1000,
                "pound (lb)": 453.59237,
                "ounce (oz)": 28.349523125
              };

              // populate selects
              const lengthFrom = document.getElementById('length-from');
              const lengthTo = document.getElementById('length-to');
              const volumeFrom = document.getElementById('volume-from');
              const volumeTo = document.getElementById('volume-to');
              const weightFrom = document.getElementById('weight-from');
              const weightTo = document.getElementById('weight-to');

              function fillSelect(sel, obj){
                sel.innerHTML = "";
                Object.keys(obj).forEach(name => {
                  const opt = document.createElement('option');
                  opt.value = name;
                  opt.textContent = name;
                  sel.appendChild(opt);
                });
              }
              fillSelect(lengthFrom, lengthUnits);
              fillSelect(lengthTo, lengthUnits);
              fillSelect(volumeFrom, volumeUnits);
              fillSelect(volumeTo, volumeUnits);
              fillSelect(weightFrom, weightUnits);
              fillSelect(weightTo, weightUnits);

              lengthFrom.value = "cubit (≈0.45 m)";
              lengthTo.value = "meter (m)";
              volumeFrom.value = "ephah / bath (22 L)";
              volumeTo.value = "liter (L)";
              weightFrom.value = "talent (34.272 kg)";
              weightTo.value = "gram (g)";

              // LENGTH conversion
              const lengthValue = document.getElementById('length-value');
              const lengthBtn = document.getElementById('length-convert');
              const lengthResult = document.getElementById('length-result');
              const lengthLast = document.getElementById('length-last');
              const lengthSwap = document.getElementById('length-swap');
              const lengthCopy = document.getElementById('length-copy');

              function convertLength(){
                clearError();
                const v = parseFloat(lengthValue.value);
                if (isNaN(v)) { showError("Enter a numeric length."); return; }
                const from = lengthFrom.value;
                const to = lengthTo.value;
                const base = v * lengthUnits[from];
                const out = base / lengthUnits[to];
                lengthResult.textContent = out.toFixed(6);
                lengthLast.textContent = `Converted ${v} ${from} → ${out.toFixed(6)} ${to}`;
              }
              lengthBtn.addEventListener('click', convertLength);
              lengthSwap.addEventListener('click', () => {
                const tmp = lengthFrom.value;
                lengthFrom.value = lengthTo.value;
                lengthTo.value = tmp;
                convertLength();
              });
              lengthCopy.addEventListener('click', async () => {
                try { await navigator.clipboard.writeText(lengthResult.textContent); } catch(e){}
              });

              // VOLUME conversion
              const volumeValue = document.getElementById('volume-value');
              const volumeBtn = document.getElementById('volume-convert');
              const volumeResult = document.getElementById('volume-result');
              const volumeLast = document.getElementById('volume-last');
              const volumeSwap = document.getElementById('volume-swap');
              const volumeCopy = document.getElementById('volume-copy');

              function convertVolume(){
                clearError();
                const v = parseFloat(volumeValue.value);
                if (isNaN(v)) { showError("Enter a numeric volume."); return; }
                const from = volumeFrom.value;
                const to = volumeTo.value;
                const base = v * volumeUnits[from];
                const out = base / volumeUnits[to];
                volumeResult.textContent = out.toFixed(6);
                volumeLast.textContent = `Converted ${v} ${from} → ${out.toFixed(6)} ${to}`;
              }
              volumeBtn.addEventListener('click', convertVolume);
              volumeSwap.addEventListener('click', () => {
                const tmp = volumeFrom.value;
                volumeFrom.value = volumeTo.value;
                volumeTo.value = tmp;
                convertVolume();
              });
              volumeCopy.addEventListener('click', async () => {
                try { await navigator.clipboard.writeText(volumeResult.textContent); } catch(e){}
              });

              // WEIGHT conversion
              const weightValue = document.getElementById('weight-value');
              const weightBtn = document.getElementById('weight-convert');
              const weightResult = document.getElementById('weight-result');
              const weightLast = document.getElementById('weight-last');
              const weightSwapBtn = document.getElementById('weight-swap');
              const weightCopy = document.getElementById('weight-copy');

              function convertWeight(){
                clearError();
                const v = parseFloat(weightValue.value);
                if (isNaN(v)) { showError("Enter a numeric weight."); return; }
                const from = weightFrom.value;
                const to = weightTo.value;
                const base = v * weightUnits[from];
                const out = base / weightUnits[to];
                weightResult.textContent = out.toFixed(6);
                weightLast.textContent = `Converted ${v} ${from} → ${out.toFixed(6)} ${to}`;
              }
              weightBtn.addEventListener('click', convertWeight);
              weightSwapBtn.addEventListener('click', () => {
                const tmp = weightFrom.value;
                weightFrom.value = weightTo.value;
                weightTo.value = tmp;
                convertWeight();
              });
              weightCopy.addEventListener('click', async () => {
                try { await navigator.clipboard.writeText(weightResult.textContent); } catch(e){}
              });

              // initial conversions
              convertLength();
              convertVolume();
              convertWeight();
            })();
          </script>
