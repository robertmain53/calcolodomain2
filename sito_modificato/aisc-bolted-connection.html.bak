    <script>
      // Core calculation logic for AISC bolted connection (simplified)
      function getFub(boltType) {
        return boltType === "A490" ? 150 : 120; // ksi
      }

      function formatKips(x) {
        if (!isFinite(x)) return "-";
        return x.toFixed(2);
      }

      function formatRatio(x) {
        if (!isFinite(x)) return "-";
        return x.toFixed(2);
      }

      function calcBoltedConnection() {
        const designMethod = document.getElementById("designMethod").value;
        const connectionType = document.getElementById("connectionType").value;
        const boltType = document.getElementById("boltType").value;
        const d = parseFloat(document.getElementById("boltDiameter").value);
        const t = parseFloat(document.getElementById("plateThickness").value);
        const e = parseFloat(document.getElementById("edgeDistance").value);
        const s = parseFloat(document.getElementById("spacing").value);
        const n = parseInt(document.getElementById("numBolts").value, 10);
        const Fy = parseFloat(document.getElementById("Fy").value);
        const loadType = document.getElementById("loadType").value;
        const V = parseFloat(document.getElementById("appliedShear").value);

        const statusMsg = document.getElementById("statusMsg");
        statusMsg.textContent = "";

        if ([d, t, e, s, n, Fy, V].some((v) => !isFinite(v) || v <= 0)) {
          statusMsg.textContent =
            "Please enter positive numeric values for all inputs.";
          return;
        }

        const Fub = getFub(boltType); // ksi
        const Fu = 1.1 * Fy; // approximate plate tensile strength

        // Bolt area (in^2)
        const Ab = (Math.PI * Math.pow(d, 2)) / 4;

        // Nominal shear per bolt (kips)
        let shearCoeff = connectionType === "slip" ? 0.3 : 0.48;
        let Rn_v = (shearCoeff * Fub * Ab) / 1.0; // ksi * in^2 = kips

        // Nominal bearing per bolt (edge and interior), kips
        const holeDiam = d + 1.0 / 16.0; // standard hole approx
        const Lc_edge = Math.max(e - holeDiam / 2, 0);
        const Lc_int = Math.max(s - holeDiam, 0);
        const Rn_b_edge = (1.2 * Lc_edge * t * Fu) / 1000.0; // ksi*in^2 -> kips
        const Rn_b_int = (1.2 * Lc_int * t * Fu) / 1000.0;
        const Rn_b = Math.min(Rn_b_edge, Rn_b_int);

        // φ and Ω
        const phi_shear = 0.75;
        const phi_bearing = 0.75;
        const Omega_shear = 2.0;
        const Omega_bearing = 2.0;

        let phiRn_v, phiRn_b;
        if (designMethod === "LRFD") {
          phiRn_v = phi_shear * Rn_v;
          phiRn_b = phi_bearing * Rn_b;
        } else {
          phiRn_v = Rn_v / Omega_shear;
          phiRn_b = Rn_b / Omega_bearing;
        }

        const totalShear = phiRn_v * n;
        const totalBearing = phiRn_b * n;
        const V_design = V; // user already chose factored or service

        const utilShear = V_design / totalShear;
        const utilBearing = V_design / totalBearing;

        const governingPerBolt = Math.min(phiRn_v, phiRn_b);
        const nReq = Math.ceil(V_design / governingPerBolt);

        // Update UI
        document.getElementById("resultsSection").classList.remove("hidden");
        document.getElementById("boltArea").textContent = Ab.toFixed(3);
        document.getElementById("RnShear").textContent = formatKips(Rn_v);
        document.getElementById("phiRnShear").textContent = formatKips(phiRn_v);
        document.getElementById("totalShearStrength").textContent =
          formatKips(totalShear);
        document.getElementById("shearUtil").textContent =
          formatRatio(utilShear);

        document.getElementById("edgeDisplay").textContent = e.toFixed(2);
        document.getElementById("spacingDisplay").textContent = s.toFixed(2);
        document.getElementById("RnBearing").textContent = formatKips(Rn_b);
        document.getElementById("phiRnBearing").textContent =
          formatKips(phiRn_b);
        document.getElementById("totalBearingStrength").textContent =
          formatKips(totalBearing);
        document.getElementById("bearingUtil").textContent =
          formatRatio(utilBearing);

        const shearStatus = document.getElementById("shearStatus");
        const bearingStatus = document.getElementById("bearingStatus");

        function setStatus(el, util) {
          el.className = "badge";
          if (!isFinite(util)) {
            el.textContent = "";
            return;
          }
          if (util <= 0.8) {
            el.textContent = "OK";
            el.classList.add(
              "bg-green-100",
              "text-green-700",
              "border",
              "border-green-300"
            );
          } else if (util <= 1.0) {
            el.textContent = "Near limit";
            el.classList.add(
              "bg-yellow-100",
              "text-yellow-700",
              "border",
              "border-yellow-300"
            );
          } else {
            el.textContent = "Overstressed";
            el.classList.add(
              "bg-red-100",
              "text-red-700",
              "border",
              "border-red-300"
            );
          }
        }

        setStatus(shearStatus, utilShear);
        setStatus(bearingStatus, utilBearing);

        let governingText;
        if (governingPerBolt === phiRn_v) {
          governingText = "Bolt shear controls the connection strength.";
        } else if (governingPerBolt === phiRn_b) {
          governingText =
            "Plate bearing at bolt holes controls the connection strength.";
        } else {
          governingText =
            "Bolt shear and plate bearing have similar strength; check detailed design.";
        }

        document.getElementById("governingLS").textContent =
          governingText +
          " Governing design strength per bolt: " +
          formatKips(governingPerBolt) +
          " kips.";
        document.getElementById("requiredBolts").textContent = isFinite(nReq)
          ? nReq
          : "-";

        statusMsg.textContent = "Calculation updated.";
      }

      document
        .getElementById("calcBtn")
        .addEventListener("click", calcBoltedConnection);
      document.getElementById("resetBtn").addEventListener("click", () => {
        document.getElementById("designMethod").value = "LRFD";
        document.getElementById("connectionType").value = "bearing";
        document.getElementById("boltType").value = "A325";
        document.getElementById("boltDiameter").value = "0.75";
        document.getElementById("plateThickness").value = "0.5";
        document.getElementById("edgeDistance").value = "1.5";
        document.getElementById("spacing").value = "3.0";
        document.getElementById("numBolts").value = "4";
        document.getElementById("Fy").value = "50";
        document.getElementById("loadType").value = "factored";
        document.getElementById("appliedShear").value = "60";
        document.getElementById("statusMsg").textContent = "";
        document.getElementById("resultsSection").classList.add("hidden");
      });

      // Auto-run once on load with defaults
      window.addEventListener("DOMContentLoaded", () => {
        calcBoltedConnection();
      });

      // Mobile menu script (simple)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", () => {
          const menu = document.getElementById("mobile-menu");
          const expanded = menu.classList.toggle("hidden");
          document
            .getElementById("mobile-menu-toggle")
            .setAttribute("aria-expanded", !expanded);
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
