    <script>
      // Born–Haber calculator logic
      (function () {
        const inputs = {
          Hf: document.getElementById("Hf"),
          Hsub: document.getElementById("Hsub"),
          IE: document.getElementById("IE"),
          D: document.getElementById("D"),
          EA: document.getElementById("EA"),
          U: document.getElementById("U"),
        };

        const unknownSelect = document.getElementById("unknown");
        const solveBtn = document.getElementById("solveBtn");
        const clearBtn = document.getElementById("clearBtn");
        const exampleBtn = document.getElementById("exampleNaClBtn");
        const resultCard = document.getElementById("resultCard");
        const resultText = document.getElementById("resultText");
        const resultEq = document.getElementById("resultEquation");
        const errorCard = document.getElementById("errorCard");
        const errorText = document.getElementById("errorText");
        const compoundInput = document.getElementById("compound");

        const barAtoms = document.getElementById("barAtoms");
        const barIons = document.getElementById("barIons");
        const barLattice = document.getElementById("barLattice");

        function parseVal(el) {
          const v = parseFloat(el.value);
          return isNaN(v) ? null : v;
        }

        function resetHighlight() {
          Object.values(inputs).forEach((el) => {
            el.classList.remove("ring-2", "ring-red-400");
          });
        }

        function showError(msg) {
          errorText.textContent = msg;
          errorCard.classList.remove("hidden");
          resultCard.classList.add("hidden");
        }

        function showResult(label, value, eq) {
          errorCard.classList.add("hidden");
          resultCard.classList.remove("hidden");
          const compound = compoundInput.value.trim();
          const compoundLabel = compound ? ` for ${compound}` : "";
          resultText.textContent = `${label}${compoundLabel}: ${value.toFixed(
            1
          )} kJ/mol`;
          resultEq.textContent = eq;
        }

        function updateBars(Hsub, IE, D, EA, U) {
          // qualitative bar heights based on magnitudes
          const endoAtoms = Math.abs(Hsub || 0) + Math.abs((D || 0) / 2);
          const endoIons = endoAtoms + Math.abs(IE || 0);
          const latticeMag = Math.abs(U || 0);

          const maxVal = Math.max(endoAtoms, endoIons, latticeMag, 1);
          const scale = 90 / maxVal; // px

          barAtoms.style.height = endoAtoms * scale + 10 + "px";
          barIons.style.height = endoIons * scale + 10 + "px";
          barLattice.style.height = latticeMag * scale + 10 + "px";
        }

        function solve() {
          resetHighlight();
          const unknown = unknownSelect.value;

          const Hf = parseVal(inputs.Hf);
          const Hsub = parseVal(inputs.Hsub);
          const IE = parseVal(inputs.IE);
          const D = parseVal(inputs.D);
          const EA = parseVal(inputs.EA);
          const U = parseVal(inputs.U);

          // Check enough known values (4 of 5 on RHS if solving for U, etc.)
          const values = { Hf, Hsub, IE, D, EA, U };
          const requiredKeys = ["Hf", "Hsub", "IE", "D", "EA", "U"].filter(
            (k) => k !== unknown
          );
          const missing = requiredKeys.filter((k) => values[k] === null);

          if (missing.length > 0) {
            missing.forEach((k) => {
              if (inputs[k]) inputs[k].classList.add("ring-2", "ring-red-400");
            });
            showError(
              "Please provide values for: " + missing.join(", ") + " (kJ/mol)."
            );
            return;
          }

          let result, eqText;

          // ΔHf = Hsub + IE + 0.5D + EA + U
          switch (unknown) {
            case "U":
              result = Hf - Hsub - IE - D / 2 - EA;
              eqText = `U = ΔHf° − ΔHsub − IE − ½D − EA = ${Hf} − ${Hsub} − ${IE} − ${
                D / 2
              } − ${EA}`;
              break;
            case "Hf":
              result = Hsub + IE + D / 2 + EA + U;
              eqText = `ΔHf° = ΔHsub + IE + ½D + EA + U = ${Hsub} + ${IE} + ${
                D / 2
              } + ${EA} + ${U}`;
              break;
            case "Hsub":
              result = Hf - IE - D / 2 - EA - U;
              eqText = `ΔHsub = ΔHf° − IE − ½D − EA − U = ${Hf} − ${IE} − ${
                D / 2
              } − ${EA} − ${U}`;
              break;
            case "IE":
              result = Hf - Hsub - D / 2 - EA - U;
              eqText = `IE = ΔHf° − ΔHsub − ½D − EA − U = ${Hf} − ${Hsub} − ${
                D / 2
              } − ${EA} − ${U}`;
              break;
            case "D":
              result = 2 * (Hf - Hsub - IE - EA - U);
              eqText = `D = 2(ΔHf° − ΔHsub − IE − EA − U) = 2(${Hf} − ${Hsub} − ${IE} − ${EA} − ${U})`;
              break;
            case "EA":
              result = Hf - Hsub - IE - D / 2 - U;
              eqText = `EA = ΔHf° − ΔHsub − IE − ½D − U = ${Hf} − ${Hsub} − ${IE} − ${
                D / 2
              } − ${U}`;
              break;
          }

          if (!isFinite(result)) {
            showError(
              "The provided values lead to an invalid result. Please check your inputs."
            );
            return;
          }

          // Write result back into the corresponding input
          if (inputs[unknown]) {
            inputs[unknown].value = result.toFixed(2);
          }

          const labelMap = {
            U: "Lattice energy U",
            Hf: "Enthalpy of formation ΔHf°",
            Hsub: "Sublimation enthalpy ΔHsub",
            IE: "Ionization energy IE",
            D: "Bond dissociation enthalpy D(X2)",
            EA: "Electron affinity EA",
          };

          showResult(labelMap[unknown] || "Result", result, eqText);

          // Update qualitative energy bars
          updateBars(
            unknown === "Hsub" ? result : Hsub,
            unknown === "IE" ? result : IE,
            unknown === "D" ? result : D,
            unknown === "EA" ? result : EA,
            unknown === "U" ? result : U
          );
        }

        function clearAll() {
          Object.values(inputs).forEach((el) => {
            el.value = "";
          });
          compoundInput.value = "";
          resultCard.classList.add("hidden");
          errorCard.classList.add("hidden");
          resetHighlight();
          barAtoms.style.height = "20px";
          barIons.style.height = "20px";
          barLattice.style.height = "20px";
        }

        function loadNaClExample() {
          clearAll();
          compoundInput.value = "NaCl(s)";
          unknownSelect.value = "U";
          inputs.Hf.value = -411;
          inputs.Hsub.value = 108;
          inputs.IE.value = 496;
          inputs.D.value = 242;
          inputs.EA.value = -349;
          solve();
        }

        solveBtn.addEventListener("click", solve);
        clearBtn.addEventListener("click", clearAll);
        exampleBtn.addEventListener("click", loadNaClExample);

        // Re-solve when unknown selection changes and all required values are present
        unknownSelect.addEventListener("change", () => {
          resultCard.classList.add("hidden");
          errorCard.classList.add("hidden");
          resetHighlight();
        });
      })();
    </script>

