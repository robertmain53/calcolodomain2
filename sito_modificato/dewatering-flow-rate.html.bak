    <script>
      // Utility functions
      function showError(message) {
        const box = document.getElementById("errorBox");
        box.textContent = message;
        box.classList.remove("hidden");
      }

      function clearError() {
        const box = document.getElementById("errorBox");
        box.textContent = "";
        box.classList.add("hidden");
      }

      function formatFlowLps(value) {
        if (!isFinite(value)) return "–";
        if (value < 0.001) return (value * 1000).toFixed(2) + " mL/s";
        return value.toFixed(3) + " L/s";
      }

      function formatFlowM3h(value) {
        if (!isFinite(value)) return "–";
        return value.toFixed(2) + " m³/h";
      }

      function formatFlowGpm(value) {
        if (!isFinite(value)) return "–";
        return value.toFixed(1) + " gpm";
      }

      function formatComponentLps(value) {
        if (!isFinite(value)) return "–";
        return value.toFixed(3) + " L/s";
      }

      function resetResults() {
        document.getElementById("qGroundwater").textContent = "–";
        document.getElementById("qRain").textContent = "–";
        document.getElementById("qBase").textContent = "–";
        document.getElementById("qDesignLps").textContent = "–";
        document.getElementById("qDesignM3h").textContent = "–";
        document.getElementById("qDesignGpm").textContent = "–";
      }

      function getUnitSystem() {
        const radios = document.querySelectorAll(".unit-system-radio");
        for (const r of radios) {
          if (r.checked) return r.value;
        }
        return "metric";
      }

      function updateUnitLabels() {
        const system = getUnitSystem();
        document.getElementById("lengthUnit").textContent =
          system === "metric" ? "m" : "ft";
        document.getElementById("widthUnit").textContent =
          system === "metric" ? "m" : "ft";
        document.getElementById("depthUnit").textContent =
          system === "metric" ? "m" : "ft";
        document.getElementById("gwHeadUnit").textContent =
          system === "metric" ? "m" : "ft";
        document.getElementById("kUnit").textContent =
          system === "metric" ? "m/s" : "ft/day";
        document.getElementById("rainUnit").textContent =
          system === "metric" ? "mm/hr" : "in/hr";
      }

      document.querySelectorAll(".unit-system-radio").forEach((radio) => {
        radio.addEventListener("change", () => {
          updateUnitLabels();
          resetResults();
          clearError();
        });
      });

      document.getElementById("resetBtn").addEventListener("click", () => {
        const system = getUnitSystem();
        document.getElementById("length").value = 20;
        document.getElementById("width").value = 10;
        document.getElementById("depth").value = 3;
        document.getElementById("gwHead").value = 8;
        document.getElementById("k").value = system === "metric" ? 0.0001 : 30;
        document.getElementById("hydraulicGradient").value = 0.02;
        document.getElementById("rainIntensity").value =
          system === "metric" ? 10 : 0.4;
        document.getElementById("runoffCoeff").value = 0.7;
        document.getElementById("safetyFactor").value = 1.5;
        resetResults();
        clearError();
      });

      document.getElementById("calculateBtn").addEventListener("click", () => {
        clearError();
        resetResults();

        const system = getUnitSystem();

        const length = parseFloat(document.getElementById("length").value);
        const width = parseFloat(document.getElementById("width").value);
        const depth = parseFloat(document.getElementById("depth").value);
        const gwHead = parseFloat(document.getElementById("gwHead").value);
        const kInput = parseFloat(document.getElementById("k").value);
        const i = parseFloat(
          document.getElementById("hydraulicGradient").value
        );
        const rainInput = parseFloat(
          document.getElementById("rainIntensity").value
        );
        const C = parseFloat(document.getElementById("runoffCoeff").value);
        const SF = parseFloat(document.getElementById("safetyFactor").value);

        // Basic validation
        if (
          [length, width, depth, gwHead, kInput, i, SF].some(
            (v) => isNaN(v) || v <= 0
          )
        ) {
          showError(
            "Please enter positive numeric values for all required fields (geometry, soil, and safety factor)."
          );
          return;
        }
        if (rainInput < 0 || isNaN(rainInput)) {
          showError("Rainfall intensity cannot be negative.");
          return;
        }
        if (C < 0 || C > 1 || isNaN(C)) {
          showError("Runoff coefficient must be between 0 and 1.");
          return;
        }
        if (i <= 0 || i > 1) {
          showError(
            "Hydraulic gradient should be between 0 and 1 for typical groundwater conditions."
          );
          return;
        }
        if (SF < 1 || SF > 5) {
          showError("Safety factor should typically be between 1 and 5.");
          return;
        }

        // Convert all to SI (meters, seconds)
        let L = length;
        let W = width;
        let D = depth;
        let H = gwHead;
        let k = kInput;
        let rain = rainInput;

        if (system === "imperial") {
          const ftToM = 0.3048;
          L = length * ftToM;
          W = width * ftToM;
          D = depth * ftToM;
          H = gwHead * ftToM;
          // k in ft/day -> m/s
          k = (kInput * ftToM) / 86400;
          // rain in in/hr -> m/s
          rain = (rainInput * 0.0254) / 3600;
        } else {
          // metric: k in m/s already, rain in mm/hr -> m/s
          rain = rainInput / 1000 / 3600;
        }

        // Geometry
        const perimeter = 2 * (L + W); // m
        const area = L * W; // m²
        const H_eff = Math.max(H, D); // conservative: use larger of aquifer thickness or excavation depth

        // Groundwater inflow (Darcy-based approximation)
        const A_wetted = perimeter * H_eff; // m²
        const Q_gw_m3s = k * i * A_wetted; // m³/s

        // Rainfall inflow
        const Q_rain_m3s = rain > 0 ? C * rain * area : 0;

        const Q_base_m3s = Q_gw_m3s + Q_rain_m3s;
        const Q_design_m3s = Q_base_m3s * SF;

        if (!isFinite(Q_design_m3s) || Q_design_m3s <= 0) {
          showError(
            "Calculated flow rate is not valid. Please review your inputs."
          );
          return;
        }

        // Convert to L/s, m³/h, gpm
        const Q_design_Lps = Q_design_m3s * 1000;
        const Q_design_m3h = Q_design_m3s * 3600;
        const Q_design_gpm = Q_design_m3s * 15850.323; // 1 m³/s = 15850.323 gpm

        const Q_gw_Lps = Q_gw_m3s * 1000;
        const Q_rain_Lps = Q_rain_m3s * 1000;
        const Q_base_Lps = Q_base_m3s * 1000;

        document.getElementById("qGroundwater").textContent =
          formatComponentLps(Q_gw_Lps);
        document.getElementById("qRain").textContent =
          formatComponentLps(Q_rain_Lps);
        document.getElementById("qBase").textContent =
          formatComponentLps(Q_base_Lps);

        document.getElementById("qDesignLps").textContent =
          formatFlowLps(Q_design_Lps);
        document.getElementById("qDesignM3h").textContent =
          formatFlowM3h(Q_design_m3h);
        document.getElementById("qDesignGpm").textContent =
          formatFlowGpm(Q_design_gpm);
      });

      // Initialize unit labels on load
      updateUnitLabels();
    </script>

