    <script>
      // --- Core tax logic (simplified, educational only) ---

      // Resident tax brackets for 2025–26 (assumed)
      const residentBrackets = [
        { upTo: 18200, baseTax: 0, rate: 0 },
        { upTo: 45000, baseTax: 0, rate: 0.19, threshold: 18200 },
        { upTo: 135000, baseTax: 5092, rate: 0.325, threshold: 45000 },
        { upTo: 190000, baseTax: 31592, rate: 0.37, threshold: 135000 },
        { upTo: Infinity, baseTax: 51667, rate: 0.45, threshold: 190000 },
      ];

      function calcBaseTaxResident(income) {
        if (income <= 18200) return 0;
        if (income <= 45000) return (income - 18200) * 0.19;
        if (income <= 135000) return 5092 + (income - 45000) * 0.325;
        if (income <= 190000) return 31592 + (income - 135000) * 0.37;
        return 51667 + (income - 190000) * 0.45;
      }

      // Simplified Medicare levy: 0 below threshold, 2% above
      function calcMedicareLevy(income) {
        const lowIncomeThreshold = 26000; // simplified single threshold
        if (income <= lowIncomeThreshold) return 0;
        return income * 0.02;
      }

      // Simplified MLS: 1% if above threshold and no private cover
      function calcMLS(income, hasPrivateCover, familyStatus) {
        if (hasPrivateCover) return 0;
        const singleThreshold = 97000;
        const familyThreshold = 194000;
        const threshold =
          familyStatus === "family" ? familyThreshold : singleThreshold;
        if (income <= threshold) return 0;
        const rate = 0.01; // simplified flat rate
        return income * rate;
      }

      // Very rough LITO approximation
      function calcLITO(income) {
        // Approximate: max $700 up to 37k, then phases out to 0 at 66k
        if (income <= 0) return 0;
        if (income <= 37000) return 700;
        if (income <= 66000) {
          const reduction = (income - 37000) * 0.05;
          return Math.max(0, 700 - reduction);
        }
        return 0;
      }

      // Very rough HELP repayment approximation
      function calcHELP(income, includeHelp) {
        if (!includeHelp) return 0;
        if (income < 50000) return 0;
        if (income < 70000) return income * 0.02;
        if (income < 90000) return income * 0.04;
        if (income < 120000) return income * 0.06;
        return income * 0.08;
      }

      function formatMoney(value) {
        if (!isFinite(value)) return "$0";
        return (
          "$" + value.toLocaleString("en-AU", { maximumFractionDigits: 0 })
        );
      }

      function formatPercent(value) {
        if (!isFinite(value)) return "0%";
        return value.toFixed(1) + "%";
      }

      function getMarginalRate(income) {
        // base marginal rate from brackets
        let baseRate = 0;
        for (const b of residentBrackets) {
          if (income <= b.upTo) {
            baseRate = b.rate;
            break;
          }
        }
        // add 2% Medicare if above threshold
        const medicareExtra = income > 26000 ? 0.02 : 0;
        return (baseRate + medicareExtra) * 100;
      }

      function recalc() {
        const incomeInput = document.getElementById("income");
        let income = parseFloat(incomeInput.value);
        if (!isFinite(income) || income < 0) income = 0;

        const residentStatus = document.getElementById("resident-status").value;
        const phic =
          document.querySelector('input[name="phic"]:checked')?.value || "yes";
        const dependants = document.getElementById("dependants").value;
        const helpOpt = document.getElementById("help").value;

        // Only resident supported
        let baseTax =
          residentStatus === "resident" ? calcBaseTaxResident(income) : 0;
        const medicare =
          residentStatus === "resident" ? calcMedicareLevy(income) : 0;
        const mls =
          residentStatus === "resident"
            ? calcMLS(income, phic === "yes", dependants)
            : 0;
        const lito = residentStatus === "resident" ? calcLITO(income) : 0;
        const help = calcHELP(income, helpOpt === "yes");

        const totalTax = Math.max(0, baseTax + medicare + mls - lito + help);
        const avgRate = income > 0 ? (totalTax / income) * 100 : 0;
        const marginalRate =
          residentStatus === "resident" ? getMarginalRate(income) : 0;

        // Update UI
        document.getElementById("out-income").textContent = formatMoney(income);
        document.getElementById("out-base-tax").textContent =
          formatMoney(baseTax);
        document.getElementById("out-medicare").textContent =
          formatMoney(medicare);
        document.getElementById("out-mls").textContent = formatMoney(mls);
        document.getElementById("out-lito").textContent =
          "−" + formatMoney(lito).replace("$", "$");
        document.getElementById("out-help").textContent = formatMoney(help);
        document.getElementById("out-total-tax").textContent =
          formatMoney(totalTax);
        document.getElementById("out-avg-rate").textContent =
          formatPercent(avgRate);
        document.getElementById("out-marginal-rate").textContent =
          formatPercent(marginalRate);
      }

      document
        .getElementById("calc-btn")
        .addEventListener("click", function (e) {
          e.preventDefault();
          recalc();
        });

      // Recalculate on input changes for responsiveness
      ["income", "resident-status", "dependants", "help"].forEach((id) => {
        const el = document.getElementById(id);
        if (el) el.addEventListener("input", recalc);
      });
      document
        .querySelectorAll('input[name="phic"]')
        .forEach((r) => r.addEventListener("change", recalc));

      // Initial calculation
      recalc();

      // Mobile menu script (simple)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          this.setAttribute("aria-expanded", isHidden ? "true" : "false");
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
