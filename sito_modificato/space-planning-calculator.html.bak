    <script>
      // Utility
      function toNumber(val) {
        const n = parseFloat(val);
        return isNaN(n) ? 0 : n;
      }
      function formatNumber(n) {
        return n.toLocaleString(undefined, { maximumFractionDigits: 0 });
      }
      function formatNumber1(n) {
        return n.toLocaleString(undefined, { maximumFractionDigits: 1 });
      }

      const modeButtons = document.querySelectorAll(".mode-btn");
      const densityButtons = document.querySelectorAll(".density-btn");
      const densityHelp = document.getElementById("density-help");

      const sections = {
        office: document.getElementById("office-inputs"),
        meeting: document.getElementById("meeting-inputs"),
        medical: document.getElementById("medical-inputs"),
        custom: document.getElementById("custom-inputs"),
      };

      let currentMode = "office";
      let currentDensity = "standard";

      function setMode(mode) {
        currentMode = mode;
        modeButtons.forEach((btn) => {
          if (btn.dataset.mode === mode) {
            btn.classList.remove(
              "border-gray-300",
              "bg-white",
              "text-gray-700"
            );
            btn.classList.add(
              "border-blue-500",
              "bg-blue-50",
              "text-blue-700",
              "font-medium"
            );
          } else {
            btn.classList.add("border-gray-300", "bg-white", "text-gray-700");
            btn.classList.remove(
              "border-blue-500",
              "bg-blue-50",
              "text-blue-700",
              "font-medium"
            );
          }
        });
        Object.keys(sections).forEach((key) => {
          sections[key].classList.toggle("hidden", key !== mode);
        });
        document
          .getElementById("density-wrapper")
          .classList.toggle("hidden", mode === "custom");

        // Update density help text based on mode
        if (mode === "meeting") {
          densityHelp.textContent =
            "Density presets are less important for events; layout type drives sq ft per person.";
        } else if (mode === "medical") {
          densityHelp.textContent =
            "Medical offices typically feel more spacious; consider higher circulation factors.";
        } else {
          densityHelp.textContent =
            "Standard office: ~175 sq ft per person including circulation.";
        }
        calculate();
      }

      function setDensity(density) {
        currentDensity = density;
        densityButtons.forEach((btn) => {
          if (btn.dataset.density === density) {
            btn.classList.remove(
              "border-gray-300",
              "bg-white",
              "text-gray-700"
            );
            btn.classList.add(
              "border-blue-500",
              "bg-blue-50",
              "text-blue-700",
              "font-medium"
            );
          } else {
            btn.classList.add("border-gray-300", "bg-white", "text-gray-700");
            btn.classList.remove(
              "border-blue-500",
              "bg-blue-50",
              "text-blue-700",
              "font-medium"
            );
          }
        });
        calculate();
      }

      modeButtons.forEach((btn) => {
        btn.addEventListener("click", () => setMode(btn.dataset.mode));
      });
      densityButtons.forEach((btn) => {
        btn.addEventListener("click", () => setDensity(btn.dataset.density));
      });

      // Meeting layout help
      const meetingLayoutSelect = document.getElementById("meeting-layout");
      const meetingLayoutHelp = document.getElementById("meeting-layout-help");
      function updateMeetingHelp() {
        const v = meetingLayoutSelect.value;
        let text = "";
        if (v === "theater") text = "Theater: ~18–20 sq ft per person.";
        else if (v === "classroom")
          text = "Classroom: ~30–40 sq ft per person.";
        else if (v === "banquet")
          text = "Banquet: ~12–15 sq ft per person including circulation.";
        else if (v === "boardroom")
          text = "Boardroom / U-shape: ~30–35 sq ft per person.";
        else if (v === "reception")
          text = "Standing reception: ~8–10 sq ft per person.";
        meetingLayoutHelp.textContent = text;
      }
      meetingLayoutSelect.addEventListener("change", () => {
        updateMeetingHelp();
        calculate();
      });
      updateMeetingHelp();

      // Custom rows
      const customRowsBody = document.getElementById("custom-rows");
      document
        .getElementById("add-custom-row")
        .addEventListener("click", () => {
          const tr = document.createElement("tr");
          tr.innerHTML = `
      <td class="px-2 py-1">
        <input type="text" value="New space" class="w-full border border-gray-200 rounded px-2 py-1" />
      </td>
      <td class="px-2 py-1 text-right">
        <input type="number" min="0" value="0" class="custom-qty w-20 border border-gray-200 rounded px-2 py-1 text-right" />
      </td>
      <td class="px-2 py-1 text-right">
        <input type="number" min="0" value="0" class="custom-sf w-24 border border-gray-200 rounded px-2 py-1 text-right" />
      </td>
      <td class="px-2 py-1 text-right text-gray-700 custom-subtotal">0</td>
    `;
          customRowsBody.appendChild(tr);
          attachCustomRowListeners(tr);
        });

      function attachCustomRowListeners(tr) {
        tr.querySelectorAll(".custom-qty, .custom-sf").forEach((input) => {
          input.addEventListener("input", () => {
            const qty = toNumber(tr.querySelector(".custom-qty").value);
            const sf = toNumber(tr.querySelector(".custom-sf").value);
            tr.querySelector(".custom-subtotal").textContent = formatNumber(
              qty * sf
            );
            calculate();
          });
        });
      }
      customRowsBody.querySelectorAll("tr").forEach(attachCustomRowListeners);

      // Core calculation
      function calculateOffice() {
        const densityMultiplier =
          currentDensity === "efficient"
            ? 0.9
            : currentDensity === "spacious"
            ? 1.15
            : 1.0;

        const numWorkstations = toNumber(
          document.getElementById("num-workstations").value
        );
        const numPrivateOffices = toNumber(
          document.getElementById("num-private-offices").value
        );
        const numSmall = toNumber(
          document.getElementById("num-small-meeting").value
        );
        const numMedium = toNumber(
          document.getElementById("num-medium-meeting").value
        );
        const numLarge = toNumber(
          document.getElementById("num-large-meeting").value
        );

        const includeReception =
          document.getElementById("include-reception").checked;
        const includeBreakroom =
          document.getElementById("include-breakroom").checked;
        const includeSupport =
          document.getElementById("include-support").checked;

        // Base assumptions (sq ft per unit) before density multiplier
        const sfPer = {
          workstation: 110,
          privateOffice: 150,
          smallRoom: 100,
          mediumRoom: 180,
          largeRoom: 320,
          reception: 200,
          breakroom: 180,
          support: 150,
        };

        const breakdown = [];

        const wsArea = numWorkstations * sfPer.workstation * densityMultiplier;
        if (wsArea > 0)
          breakdown.push({
            label: `Open workstations (${numWorkstations})`,
            area: wsArea,
          });

        const poArea =
          numPrivateOffices * sfPer.privateOffice * densityMultiplier;
        if (poArea > 0)
          breakdown.push({
            label: `Private offices (${numPrivateOffices})`,
            area: poArea,
          });

        const smArea = numSmall * sfPer.smallRoom;
        if (smArea > 0)
          breakdown.push({
            label: `Small meeting rooms (${numSmall})`,
            area: smArea,
          });

        const mdArea = numMedium * sfPer.mediumRoom;
        if (mdArea > 0)
          breakdown.push({
            label: `Medium meeting rooms (${numMedium})`,
            area: mdArea,
          });

        const lgArea = numLarge * sfPer.largeRoom;
        if (lgArea > 0)
          breakdown.push({
            label: `Large meeting rooms (${numLarge})`,
            area: lgArea,
          });

        if (includeReception)
          breakdown.push({ label: "Reception / lobby", area: sfPer.reception });
        if (includeBreakroom)
          breakdown.push({
            label: "Break room / kitchen",
            area: sfPer.breakroom,
          });
        if (includeSupport)
          breakdown.push({ label: "Storage / copy / IT", area: sfPer.support });

        const usable = breakdown.reduce((sum, item) => sum + item.area, 0);
        const headcount = numWorkstations + numPrivateOffices;
        return { usable, headcount, breakdown };
      }

      function calculateMeeting() {
        const headcount = toNumber(
          document.getElementById("meeting-headcount").value
        );
        const layout = document.getElementById("meeting-layout").value;
        const includeStage = document.getElementById("meeting-stage").checked;
        const includeFoyer = document.getElementById("meeting-foyer").checked;

        let sfPerPerson;
        if (layout === "theater") sfPerPerson = 19;
        else if (layout === "classroom") sfPerPerson = 35;
        else if (layout === "banquet") sfPerPerson = 14;
        else if (layout === "boardroom") sfPerPerson = 32;
        else if (layout === "reception") sfPerPerson = 9;
        else sfPerPerson = 20;

        const mainArea = headcount * sfPerPerson;
        const breakdown = [];
        if (mainArea > 0)
          breakdown.push({
            label: `Main room (${layout} seating)`,
            area: mainArea,
          });

        if (includeStage)
          breakdown.push({ label: "Stage / AV area", area: 200 });
        if (includeFoyer)
          breakdown.push({
            label: "Foyer / registration",
            area: Math.max(150, headcount * 2),
          });

        const usable = breakdown.reduce((sum, item) => sum + item.area, 0);
        return { usable, headcount, breakdown };
      }

      function calculateMedical() {
        const numExam = toNumber(
          document.getElementById("num-exam-rooms").value
        );
        const numProviders = toNumber(
          document.getElementById("num-providers").value
        );
        const numSupport = toNumber(
          document.getElementById("num-support-staff").value
        );

        const includeWaiting =
          document.getElementById("include-waiting").checked;
        const includeLab = document.getElementById("include-lab").checked;
        const includeAdmin = document.getElementById("include-admin").checked;

        const sfPer = {
          exam: 120,
          providerOffice: 130,
          supportWork: 75,
          waitingSeat: 16,
          lab: 160,
          admin: 140,
        };

        const breakdown = [];

        const examArea = numExam * sfPer.exam;
        if (examArea > 0)
          breakdown.push({ label: `Exam rooms (${numExam})`, area: examArea });

        const providerArea = numProviders * sfPer.providerOffice;
        if (providerArea > 0)
          breakdown.push({
            label: `Provider offices (${numProviders})`,
            area: providerArea,
          });

        const supportArea = numSupport * sfPer.supportWork;
        if (supportArea > 0)
          breakdown.push({
            label: `Support staff work area (${numSupport})`,
            area: supportArea,
          });

        if (includeWaiting) {
          const waitingSeats = Math.max(
            4,
            Math.round((numExam + numProviders + numSupport) * 0.4)
          );
          breakdown.push({
            label: `Waiting room (~${waitingSeats} seats)`,
            area: waitingSeats * sfPer.waitingSeat,
          });
        }
        if (includeLab)
          breakdown.push({ label: "Lab / procedure room", area: sfPer.lab });
        if (includeAdmin)
          breakdown.push({ label: "Admin / billing", area: sfPer.admin });

        const usable = breakdown.reduce((sum, item) => sum + item.area, 0);
        const headcount = numProviders + numSupport;
        return { usable, headcount, breakdown };
      }

      function calculateCustom() {
        const rows = customRowsBody.querySelectorAll("tr");
        const breakdown = [];
        let usable = 0;
        rows.forEach((tr) => {
          const labelInput = tr.querySelector('input[type="text"]');
          const qty = toNumber(tr.querySelector(".custom-qty").value);
          const sf = toNumber(tr.querySelector(".custom-sf").value);
          const area = qty * sf;
          if (area > 0) {
            breakdown.push({
              label: `${labelInput.value || "Space"} (${qty} × ${sf} sq ft)`,
              area,
            });
            usable += area;
          }
          tr.querySelector(".custom-subtotal").textContent = formatNumber(area);
        });
        // Headcount is unknown in custom mode; leave as 0
        return { usable, headcount: 0, breakdown };
      }

      function calculate() {
        let result;
        if (currentMode === "office") result = calculateOffice();
        else if (currentMode === "meeting") result = calculateMeeting();
        else if (currentMode === "medical") result = calculateMedical();
        else result = calculateCustom();

        const circFactor = toNumber(
          document.getElementById("circulation-factor").value
        );
        const usable = result.usable;
        const total = usable * (1 + circFactor / 100);

        const usableM2 = usable * 0.092903;
        const totalM2 = total * 0.092903;

        document.getElementById("usable-sf").textContent = `${formatNumber(
          usable
        )} sq ft`;
        document.getElementById("total-sf").textContent = `${formatNumber(
          total
        )} sq ft`;
        document.getElementById("usable-sf-m2").textContent = `${formatNumber1(
          usableM2
        )} m²`;
        document.getElementById("total-sf-m2").textContent = `${formatNumber1(
          totalM2
        )} m²`;

        document.getElementById("total-headcount").textContent =
          result.headcount ? formatNumber(result.headcount) : "—";
        const sfPerPersonEl = document.getElementById("sf-per-person");
        if (result.headcount && total > 0) {
          const sfPerPerson = total / result.headcount;
          sfPerPersonEl.textContent = `${formatNumber1(
            sfPerPerson
          )} sq ft per person`;
        } else {
          sfPerPersonEl.textContent = "—";
        }

        const breakdownList = document.getElementById("breakdown-list");
        breakdownList.innerHTML = "";
        result.breakdown.forEach((item) => {
          const div = document.createElement("div");
          div.className = "flex justify-between";
          div.innerHTML = `<span>${
            item.label
          }</span><span class="font-medium">${formatNumber(
            item.area
          )} sq ft</span>`;
          breakdownList.appendChild(div);
        });
        if (result.breakdown.length === 0) {
          const div = document.createElement("div");
          div.className = "text-sm text-gray-500";
          div.textContent =
            "Enter values above to see a detailed area breakdown.";
          breakdownList.appendChild(div);
        }
      }

      // Attach input listeners for live updates
      [
        "num-workstations",
        "num-private-offices",
        "num-small-meeting",
        "num-medium-meeting",
        "num-large-meeting",
        "include-reception",
        "include-breakroom",
        "include-support",
        "meeting-headcount",
        "meeting-layout",
        "meeting-stage",
        "meeting-foyer",
        "num-exam-rooms",
        "num-providers",
        "num-support-staff",
        "include-waiting",
        "include-lab",
        "include-admin",
        "circulation-factor",
      ].forEach((id) => {
        const el = document.getElementById(id);
        if (!el) return;
        el.addEventListener("input", calculate);
        el.addEventListener("change", calculate);
      });

      document
        .getElementById("calculate-btn")
        .addEventListener("click", calculate);

      document.getElementById("reset-btn").addEventListener("click", () => {
        // Simple reset to defaults
        setMode("office");
        setDensity("standard");
        document.getElementById("num-workstations").value = 20;
        document.getElementById("num-private-offices").value = 4;
        document.getElementById("num-small-meeting").value = 2;
        document.getElementById("num-medium-meeting").value = 2;
        document.getElementById("num-large-meeting").value = 1;
        document.getElementById("include-reception").checked = true;
        document.getElementById("include-breakroom").checked = true;
        document.getElementById("include-support").checked = true;
        document.getElementById("meeting-headcount").value = 50;
        document.getElementById("meeting-layout").value = "theater";
        document.getElementById("meeting-stage").checked = false;
        document.getElementById("meeting-foyer").checked = true;
        document.getElementById("num-exam-rooms").value = 6;
        document.getElementById("num-providers").value = 3;
        document.getElementById("num-support-staff").value = 6;
        document.getElementById("include-waiting").checked = true;
        document.getElementById("include-lab").checked = false;
        document.getElementById("include-admin").checked = true;
        document.getElementById("circulation-factor").value = 30;
        updateMeetingHelp();
        calculate();
      });

      // Initial calculation
      calculate();
    </script>

