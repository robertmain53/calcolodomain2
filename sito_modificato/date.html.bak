    <script>
      // Utility functions
      function pad(num) {
        return num.toString().padStart(2, "0");
      }

      function formatDateISO(date) {
        return (
          date.getFullYear() +
          "-" +
          pad(date.getMonth() + 1) +
          "-" +
          pad(date.getDate())
        );
      }

      function formatDateReadable(date) {
        const opts = { year: "numeric", month: "long", day: "numeric" };
        return date.toLocaleDateString(undefined, opts);
      }

      function getWeekday(date) {
        return date.toLocaleDateString(undefined, { weekday: "long" });
      }

      function addMonthsKeepingDay(date, months) {
        const d = new Date(date.getTime());
        const targetMonth = d.getMonth() + months;
        const targetYear = d.getFullYear() + Math.floor(targetMonth / 12);
        const normalizedMonth = ((targetMonth % 12) + 12) % 12;
        const day = d.getDate();

        const result = new Date(targetYear, normalizedMonth, 1);
        const lastDay = new Date(targetYear, normalizedMonth + 1, 0).getDate();
        result.setDate(Math.min(day, lastDay));
        return result;
      }

      // Tab switching
      const tabAddSubtract = document.getElementById("tab-add-subtract");
      const tabDiff = document.getElementById("tab-diff");
      const panelAddSubtract = document.getElementById("panel-add-subtract");
      const panelDiff = document.getElementById("panel-diff");

      tabAddSubtract.addEventListener("click", () => {
        tabAddSubtract.classList.add("border-blue-600", "text-blue-600");
        tabAddSubtract.classList.remove("text-gray-600");
        tabDiff.classList.remove("border-blue-600", "text-blue-600");
        tabDiff.classList.add("text-gray-600");
        panelAddSubtract.classList.remove("hidden");
        panelDiff.classList.add("hidden");
      });

      tabDiff.addEventListener("click", () => {
        tabDiff.classList.add("border-blue-600", "text-blue-600");
        tabDiff.classList.remove("text-gray-600");
        tabAddSubtract.classList.remove("border-blue-600", "text-blue-600");
        tabAddSubtract.classList.add("text-gray-600");
        panelDiff.classList.remove("hidden");
        panelAddSubtract.classList.add("hidden");
      });

      // Add/Subtract logic
      const btnAdd = document.getElementById("btn-add");
      const btnSubtract = document.getElementById("btn-subtract");
      let operation = "add";

      btnAdd.addEventListener("click", () => {
        operation = "add";
        btnAdd.classList.add("toggle-btn-active", "bg-blue-600", "text-white");
        btnSubtract.classList.remove(
          "toggle-btn-active",
          "bg-blue-600",
          "text-white"
        );
        btnSubtract.classList.add("bg-white", "text-gray-700");
      });

      btnSubtract.addEventListener("click", () => {
        operation = "subtract";
        btnSubtract.classList.add(
          "toggle-btn-active",
          "bg-blue-600",
          "text-white"
        );
        btnAdd.classList.remove(
          "toggle-btn-active",
          "bg-blue-600",
          "text-white"
        );
        btnAdd.classList.add("bg-white", "text-gray-700");
      });

      const startDateInput = document.getElementById("start-date");
      const yearsInput = document.getElementById("years");
      const monthsInput = document.getElementById("months");
      const weeksInput = document.getElementById("weeks");
      const daysInput = document.getElementById("days");
      const btnCalcAddSubtract = document.getElementById(
        "btn-calc-add-subtract"
      );
      const btnResetAddSubtract = document.getElementById(
        "btn-reset-add-subtract"
      );
      const resultAddSubtract = document.getElementById("result-add-subtract");
      const resultDateSpan = document.getElementById("result-date");
      const resultWeekdaySpan = document.getElementById("result-weekday");
      const resultTotalDaysSpan = document.getElementById("result-total-days");

      // Initialize start date to today
      (function initToday() {
        const today = new Date();
        startDateInput.value = formatDateISO(today);
        document.getElementById("diff-start").value = formatDateISO(today);
        const tomorrow = new Date(today.getTime() + 24 * 60 * 60 * 1000);
        document.getElementById("diff-end").value = formatDateISO(tomorrow);
      })();

      btnCalcAddSubtract.addEventListener("click", () => {
        if (!startDateInput.value) {
          alert("Please select a start date.");
          return;
        }

        const baseDate = new Date(startDateInput.value + "T00:00:00");
        if (isNaN(baseDate.getTime())) {
          alert("Invalid start date.");
          return;
        }

        const years = parseInt(yearsInput.value || "0", 10);
        const months = parseInt(monthsInput.value || "0", 10);
        const weeks = parseInt(weeksInput.value || "0", 10);
        const days = parseInt(daysInput.value || "0", 10);

        const sign = operation === "add" ? 1 : -1;
        const totalMonths = sign * (years * 12 + months);
        const totalDays = sign * (days + weeks * 7);

        let resultDate = addMonthsKeepingDay(baseDate, totalMonths);
        resultDate = new Date(
          resultDate.getTime() + totalDays * 24 * 60 * 60 * 1000
        );

        resultDateSpan.textContent = formatDateReadable(resultDate);
        resultWeekdaySpan.textContent = getWeekday(resultDate);
        resultTotalDaysSpan.textContent = (
          sign *
          (days + weeks * 7)
        ).toString();

        resultAddSubtract.classList.remove("hidden");
      });

      btnResetAddSubtract.addEventListener("click", () => {
        const today = new Date();
        startDateInput.value = formatDateISO(today);
        yearsInput.value = "0";
        monthsInput.value = "0";
        weeksInput.value = "0";
        daysInput.value = "0";
        resultAddSubtract.classList.add("hidden");
      });

      // Difference logic
      const diffStartInput = document.getElementById("diff-start");
      const diffEndInput = document.getElementById("diff-end");
      const btnCalcDiff = document.getElementById("btn-calc-diff");
      const btnResetDiff = document.getElementById("btn-reset-diff");
      const resultDiff = document.getElementById("result-diff");
      const diffTotalDaysSpan = document.getElementById("diff-total-days");
      const diffBreakdownSpan = document.getElementById("diff-breakdown");

      btnCalcDiff.addEventListener("click", () => {
        if (!diffStartInput.value || !diffEndInput.value) {
          alert("Please select both start and end dates.");
          return;
        }

        const start = new Date(diffStartInput.value + "T00:00:00");
        const end = new Date(diffEndInput.value + "T00:00:00");

        if (isNaN(start.getTime()) || isNaN(end.getTime())) {
          alert("One or both dates are invalid.");
          return;
        }

        const msPerDay = 24 * 60 * 60 * 1000;
        const diffMs = end.getTime() - start.getTime();
        const totalDays = Math.round(diffMs / msPerDay);

        diffTotalDaysSpan.textContent = Math.abs(totalDays).toString();

        const absDays = Math.abs(totalDays);
        const years = Math.floor(absDays / 365);
        const remainingAfterYears = absDays - years * 365;
        const months = Math.floor(remainingAfterYears / 30);
        const remDays = remainingAfterYears - months * 30;

        let parts = [];
        if (years) parts.push(years + " year" + (years !== 1 ? "s" : ""));
        if (months) parts.push(months + " month" + (months !== 1 ? "s" : ""));
        if (remDays || (!years && !months))
          parts.push(remDays + " day" + (remDays !== 1 ? "s" : ""));

        diffBreakdownSpan.textContent = parts.join(", ") + " (approximate)";
        resultDiff.classList.remove("hidden");
      });

      btnResetDiff.addEventListener("click", () => {
        const today = new Date();
        diffStartInput.value = formatDateISO(today);
        const tomorrow = new Date(today.getTime() + 24 * 60 * 60 * 1000);
        diffEndInput.value = formatDateISO(tomorrow);
        resultDiff.classList.add("hidden");
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
