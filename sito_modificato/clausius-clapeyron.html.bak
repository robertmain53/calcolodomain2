    <script>
      // Clausius-Clapeyron calculator logic
      (function () {
        const modeTabs = document.querySelectorAll(".cc-mode-tab");
        const modeState = { current: "p2" }; // 'p2', 't2', 'dhvap'

        const RInput = document.getElementById("cc-R");
        const RUnits = document.getElementById("cc-R-units");
        const T1Input = document.getElementById("cc-T1");
        const T2Input = document.getElementById("cc-T2");
        const T2Wrapper = document.getElementById("cc-T2-wrapper");
        const P1Input = document.getElementById("cc-P1");
        const P2Input = document.getElementById("cc-P2");
        const P2Wrapper = document.getElementById("cc-P2-wrapper");
        const P2Help = document.getElementById("cc-P2-help");
        const PUnits = document.getElementById("cc-P-units");
        const dHvapWrapper = document.getElementById("cc-dHvap-wrapper");
        const dHvapInput = document.getElementById("cc-dHvap");
        const dHvapUnits = document.getElementById("cc-dHvap-units");
        const dHvapHelp = document.getElementById("cc-dHvap-help");

        const calcBtn = document.getElementById("cc-calc-btn");
        const resetBtn = document.getElementById("cc-reset-btn");
        const errorEl = document.getElementById("cc-error");
        const resultsBox = document.getElementById("cc-results");
        const resultMain = document.getElementById("cc-result-main");
        const resultSecondary = document.getElementById("cc-result-secondary");
        const stepsEl = document.getElementById("cc-steps");

        function setMode(mode) {
          modeState.current = mode;
          modeTabs.forEach((btn) => {
            if (btn.dataset.mode === mode) {
              btn.classList.add("bg-blue-600", "text-white");
              btn.classList.remove("text-gray-700", "hover:bg-gray-100");
            } else {
              btn.classList.remove("bg-blue-600", "text-white");
              btn.classList.add("text-gray-700", "hover:bg-gray-100");
            }
          });

          // Adjust visible/required fields and helper text
          if (mode === "p2") {
            T2Wrapper.classList.remove("hidden");
            P2Wrapper.classList.remove("hidden");
            dHvapWrapper.classList.remove("hidden");
            P2Help.textContent = "Leave blank to solve for P₂.";
            dHvapHelp.textContent = "Provide ΔH_vap to compute P₂ at T₂.";
          } else if (mode === "t2") {
            T2Wrapper.classList.remove("hidden");
            P2Wrapper.classList.remove("hidden");
            dHvapWrapper.classList.remove("hidden");
            P2Help.textContent = "Provide P₂ to solve for T₂.";
            dHvapHelp.textContent =
              "Provide ΔH_vap to compute T₂ for given P₂.";
          } else if (mode === "dhvap") {
            T2Wrapper.classList.remove("hidden");
            P2Wrapper.classList.remove("hidden");
            dHvapWrapper.classList.remove("hidden");
            P2Help.textContent = "Provide P₂ to solve for ΔH_vap.";
            dHvapHelp.textContent =
              "Result will be ΔH_vap from the two measurements.";
          }

          clearError();
          hideResults();
        }

        function clearError() {
          errorEl.textContent = "";
        }

        function showError(msg) {
          errorEl.textContent = msg;
          hideResults();
        }

        function hideResults() {
          resultsBox.classList.add("hidden");
        }

        function showResults() {
          resultsBox.classList.remove("hidden");
        }

        function parsePositive(value, name) {
          const v = parseFloat(value);
          if (!isFinite(v) || v <= 0) {
            throw new Error(name + " must be a positive number.");
          }
          return v;
        }

        function calculate() {
          clearError();
          try {
            const R = parsePositive(RInput.value, "R");
            const T1 = parsePositive(T1Input.value, "T₁");
            const T2 = parsePositive(T2Input.value, "T₂");
            const P1 = parsePositive(P1Input.value, "P₁");
            const Punit = PUnits.value;
            const mode = modeState.current;

            let dHvap = parseFloat(dHvapInput.value);
            const useKJ = dHvapUnits.value === "kJmol";
            const RUnitIsJ = RUnits.value === "J";

            // Convert ΔHvap to match R units
            if (mode !== "dhvap") {
              if (!isFinite(dHvap) || dHvap <= 0) {
                throw new Error("ΔH_vap must be a positive number.");
              }
              if (useKJ && RUnitIsJ) {
                dHvap *= 1000; // kJ/mol -> J/mol
              } else if (!useKJ && !RUnitIsJ) {
                dHvap / 1000; // J/mol -> kJ/mol
              }
            }

            let P2 = parseFloat(P2Input.value);
            let lnP2P1,
              invT2minusInvT1,
              resultText = "",
              secondaryText = "",
              stepsText = "";

            if (mode === "p2") {
              // Solve for P2
              invT2minusInvT1 = 1 / T2 - 1 / T1;
              const exponent = -(dHvap / R) * invT2minusInvT1;
              const ratio = Math.exp(exponent);
              P2 = P1 * ratio;

              resultText = `P₂ ≈ ${P2.toPrecision(4)} ${Punit}`;
              secondaryText = `Pressure ratio P₂/P₁ ≈ ${ratio.toPrecision(4)}.`;

              stepsText =
                `1. Compute 1/T₂ - 1/T₁ = ${(1 / T2).toExponential(4)} - ${(
                  1 / T1
                ).toExponential(4)} = ${invT2minusInvT1.toExponential(
                  4
                )} K⁻¹.<br>` +
                `2. Compute ΔH_vap/R = ${(dHvap / R).toExponential(4)}.<br>` +
                `3. Exponent = -(ΔH_vap/R)·(1/T₂ - 1/T₁) = ${exponent.toFixed(
                  4
                )}.<br>` +
                `4. P₂ = P₁ · exp(exponent) = ${P1} · exp(${exponent.toFixed(
                  4
                )}) ≈ ${P2.toPrecision(4)} ${Punit}.`;
            } else if (mode === "t2") {
              // Need P2
              P2 = parsePositive(P2Input.value, "P₂");
              lnP2P1 = Math.log(P2 / P1);
              const invT2 = 1 / T1 - (R / dHvap) * lnP2P1;
              if (invT2 <= 0) {
                throw new Error(
                  "Computed 1/T₂ is non-positive. Check your inputs; they may be inconsistent."
                );
              }
              const T2calc = 1 / invT2;

              resultText = `T₂ ≈ ${T2calc.toFixed(2)} K (${(
                T2calc - 273.15
              ).toFixed(2)} °C)`;
              secondaryText = `Using ln(P₂/P₁) = ${lnP2P1.toFixed(
                4
              )} and ΔH_vap/R = ${(dHvap / R).toExponential(4)}.`;

              stepsText =
                `1. Compute ln(P₂/P₁) = ln(${P2}/${P1}) = ${lnP2P1.toFixed(
                  4
                )}.<br>` +
                `2. Compute (R/ΔH_vap) = ${(R / dHvap).toExponential(4)}.<br>` +
                `3. 1/T₂ = 1/T₁ - (R/ΔH_vap)·ln(P₂/P₁) = ${(
                  1 / T1
                ).toExponential(4)} - ${((R / dHvap) * lnP2P1).toExponential(
                  4
                )} = ${invT2.toExponential(4)} K⁻¹.<br>` +
                `4. T₂ = 1 / (1/T₂) ≈ ${T2calc.toFixed(2)} K.`;
            } else if (mode === "dhvap") {
              // Solve for ΔHvap
              P2 = parsePositive(P2Input.value, "P₂");
              lnP2P1 = Math.log(P2 / P1);
              invT2minusInvT1 = 1 / T2 - 1 / T1;
              if (invT2minusInvT1 === 0) {
                throw new Error(
                  "T₁ and T₂ must be different to solve for ΔH_vap."
                );
              }
              let dHvapCalc = (-R * lnP2P1) / invT2minusInvT1; // in units of R
              let displayVal = dHvapCalc;
              let displayUnits = RUnitIsJ ? "J/mol" : "kJ/mol";

              // Convert to user-selected ΔHvap units for display
              if (dHvapUnits.value === "kJmol" && RUnitIsJ) {
                displayVal = dHvapCalc / 1000;
                displayUnits = "kJ/mol";
              } else if (dHvapUnits.value === "Jmol" && !RUnitIsJ) {
                displayVal = dHvapCalc * 1000;
                displayUnits = "J/mol";
              }

              resultText = `ΔH_vap ≈ ${displayVal.toPrecision(
                4
              )} ${displayUnits}`;
              secondaryText = `From ln(P₂/P₁) = ${lnP2P1.toFixed(
                4
              )} and (1/T₂ - 1/T₁) = ${invT2minusInvT1.toExponential(4)} K⁻¹.`;

              stepsText =
                `1. Compute ln(P₂/P₁) = ln(${P2}/${P1}) = ${lnP2P1.toFixed(
                  4
                )}.<br>` +
                `2. Compute 1/T₂ - 1/T₁ = ${(1 / T2).toExponential(4)} - ${(
                  1 / T1
                ).toExponential(4)} = ${invT2minusInvT1.toExponential(
                  4
                )} K⁻¹.<br>` +
                `3. ΔH_vap = -R · ln(P₂/P₁) / (1/T₂ - 1/T₁) ≈ ${dHvapCalc.toExponential(
                  4
                )} in the units of R.<br>` +
                `4. Converted to your selected units: ≈ ${displayVal.toPrecision(
                  4
                )} ${displayUnits}.`;
            }

            resultMain.textContent = resultText;
            resultSecondary.textContent = secondaryText;
            stepsEl.innerHTML = stepsText;
            showResults();
          } catch (err) {
            showError(err.message || "Invalid input.");
          }
        }

        function resetForm() {
          RInput.value = 8.314;
          RUnits.value = "J";
          T1Input.value = 373.15;
          T2Input.value = 353.15;
          P1Input.value = 1;
          PUnits.value = "atm";
          P2Input.value = "";
          dHvapInput.value = 40.7;
          dHvapUnits.value = "kJmol";
          clearError();
          hideResults();
        }

        modeTabs.forEach((btn) => {
          btn.addEventListener("click", () => setMode(btn.dataset.mode));
        });
        calcBtn.addEventListener("click", calculate);
        resetBtn.addEventListener("click", resetForm);

        // Initialize
        setMode("p2");
      })();
    </script>

