    <script>
      // Core fugacity calculations
      function calculateFugacity() {
        const P = parseFloat(document.getElementById("pressure").value);
        const T = parseFloat(document.getElementById("temperature").value);
        const Z = parseFloat(document.getElementById("z-factor").value);
        const ZrefVal = document.getElementById("z-ref").value;
        const PrefVal = document.getElementById("pref").value;
        const phirefVal = document.getElementById("phi-ref").value;

        const mode = document.querySelector('input[name="mode"]:checked').value;
        const errorEl = document.getElementById("calc-error");
        const resultsCard = document.getElementById("results-card");
        const phiOut = document.getElementById("phi-out");
        const fOut = document.getElementById("f-out");
        const modeNote = document.getElementById("mode-note");

        errorEl.textContent = "";
        resultsCard.classList.add("hidden");

        if (isNaN(P) || P <= 0) {
          errorEl.textContent = "Please enter a valid positive pressure.";
          return;
        }
        if (isNaN(Z) || Z <= 0) {
          errorEl.textContent =
            "Please enter a valid positive compressibility factor Z.";
          return;
        }

        let phi,
          note = "";

        if (mode === "approx") {
          // ln φ ≈ Z − 1
          const lnphi = Z - 1.0;
          phi = Math.exp(lnphi);
          note =
            "Quick approximation using ln φ ≈ Z − 1 at the specified pressure. Best for moderate pressures where Z does not vary strongly with P.";
        } else {
          // integral mode: need Zref and Pref
          const Zref = parseFloat(ZrefVal);
          const Pref = parseFloat(PrefVal);
          let phiref = parseFloat(phirefVal);

          if (isNaN(Zref) || Zref <= 0 || isNaN(Pref) || Pref <= 0) {
            errorEl.textContent =
              "For the two-point integral mode, please provide positive values for Z_ref and P_ref.";
            return;
          }
          if (isNaN(phiref) || phiref <= 0) {
            phiref = 1.0; // assume ideal at reference
          }

          // trapezoidal approximation in ln P
          const lnP = Math.log(P);
          const lnPref = Math.log(Pref);
          const avgZminus1 = (Z - 1.0 + (Zref - 1.0)) / 2.0;
          const lnphi = Math.log(phiref) + avgZminus1 * (lnP - lnPref);
          phi = Math.exp(lnphi);

          note =
            "Two-point integral approximation using Z and Z_ref between P_ref and P. Assumes Z − 1 varies roughly linearly with ln P.";
        }

        const f = phi * P;

        phiOut.textContent = phi.toFixed(5);
        fOut.textContent = f.toFixed(4) + " bar";
        modeNote.textContent =
          note +
          (isNaN(T) ? "" : " Temperature entered: " + T.toFixed(2) + " K.");
        resultsCard.classList.remove("hidden");
      }

      document
        .getElementById("calc-btn")
        .addEventListener("click", calculateFugacity);

      document.getElementById("reset-btn").addEventListener("click", () => {
        document.getElementById("pressure").value = 10;
        document.getElementById("temperature").value = "";
        document.getElementById("z-factor").value = 0.9;
        document.getElementById("z-ref").value = "";
        document.getElementById("pref").value = "";
        document.getElementById("phi-ref").value = "";
        document.getElementById("calc-error").textContent = "";
        document.getElementById("results-card").classList.add("hidden");
        document.querySelector(
          'input[name="mode"][value="approx"]'
        ).checked = true;
      });

      // Simple FAQ toggle (in addition to global script_faq if present)
      document
        .querySelectorAll("#faq-container .faq-question")
        .forEach((btn) => {
          btn.addEventListener("click", () => {
            const answer = btn.parentElement.querySelector(".faq-answer");
            const icon = btn.querySelector("span:last-child");
            const isHidden = answer.classList.contains("hidden");
            document
              .querySelectorAll("#faq-container .faq-answer")
              .forEach((a) => a.classList.add("hidden"));
            document
              .querySelectorAll("#faq-container .faq-question span:last-child")
              .forEach((i) => (i.textContent = "+"));
            if (isHidden) {
              answer.classList.remove("hidden");
              icon.textContent = "−";
            }
          });
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
