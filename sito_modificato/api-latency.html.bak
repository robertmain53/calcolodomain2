    <script>
      // Core calculator logic using Little's Law
      function getNumber(id) {
        const v = parseFloat(document.getElementById(id).value);
        return isNaN(v) ? 0 : v;
      }

      function setText(id, value, decimals = 2) {
        const el = document.getElementById(id);
        if (!el) return;
        const num = typeof value === "number" ? value : parseFloat(value);
        if (isNaN(num)) {
          el.textContent = "–";
        } else {
          const d = Math.abs(num) >= 1000 ? 0 : decimals;
          el.textContent = num.toFixed(d);
        }
      }

      function latencyToSeconds(latency, unit) {
        return unit === "s" ? latency : latency / 1000;
      }

      function secondsToLatencyMs(seconds) {
        return seconds * 1000;
      }

      function updateDerived() {
        const latency = getNumber("latency");
        const latencyUnit = document.getElementById("latencyUnit").value;
        const throughput = getNumber("throughput");
        const concurrency = getNumber("concurrency");
        const instances = Math.max(1, Math.round(getNumber("instances") || 1));

        const latencySec = latencyToSeconds(latency, latencyUnit);

        // Update summary outputs
        setText("outLatencyMs", secondsToLatencyMs(latencySec));
        setText("outThroughput", throughput);
        setText("outConcurrency", concurrency);

        const rpsPerInstance = throughput / instances;
        const concPerInstance = concurrency / instances;
        setText("outRpsPerInstance", rpsPerInstance);
        setText("outConcurrencyPerInstance", concPerInstance);

        // Percentile estimate
        const percentile = document.getElementById("percentile").value;
        let multiplier = 1;
        if (percentile === "p90") multiplier = 1.5;
        else if (percentile === "p95") multiplier = 2;
        else if (percentile === "p99") multiplier = 3;
        const percLatencyMs = secondsToLatencyMs(latencySec) * multiplier;
        document.getElementById("outPercentileLabel").textContent = percentile;
        setText("outPercentileLatency", percLatencyMs, 1);

        // Health badge & UX hint
        const badge = document.getElementById("healthBadge");
        const uxHint = document.getElementById("uxHint");
        const ms = secondsToLatencyMs(latencySec);

        if (ms <= 150) {
          badge.textContent = "Excellent";
          badge.className = "badge bg-emerald-100 text-emerald-700";
          uxHint.textContent =
            "Sub-150 ms latency feels instant for most interactions. You have strong UX headroom.";
        } else if (ms <= 300) {
          badge.textContent = "Healthy";
          badge.className = "badge bg-green-100 text-green-700";
          uxHint.textContent =
            "Around 150–300 ms latency is generally perceived as snappy for web and mobile APIs.";
        } else if (ms <= 700) {
          badge.textContent = "Borderline";
          badge.className = "badge bg-yellow-100 text-yellow-700";
          uxHint.textContent =
            "300–700 ms is noticeable but often acceptable. Consider optimizing hot paths and caching.";
        } else {
          badge.textContent = "Slow";
          badge.className = "badge bg-red-100 text-red-700";
          uxHint.textContent =
            "Above ~700 ms users will clearly feel lag. Investigate network distance, database queries, and downstream services.";
        }
      }

      function solveLatency() {
        const throughput = getNumber("throughput");
        const concurrency = getNumber("concurrency");
        if (throughput <= 0) return;
        const latencySec = concurrency / throughput;
        const latencyMs = secondsToLatencyMs(latencySec);
        document.getElementById("latency").value = latencyMs.toFixed(2);
        document.getElementById("latencyUnit").value = "ms";
        updateDerived();
      }

      function solveThroughput() {
        const latency = getNumber("latency");
        const latencyUnit = document.getElementById("latencyUnit").value;
        const concurrency = getNumber("concurrency");
        const latencySec = latencyToSeconds(latency, latencyUnit);
        if (latencySec <= 0) return;
        const throughput = concurrency / latencySec;
        document.getElementById("throughput").value = throughput.toFixed(2);
        updateDerived();
      }

      function solveConcurrency() {
        const latency = getNumber("latency");
        const latencyUnit = document.getElementById("latencyUnit").value;
        const throughput = getNumber("throughput");
        const latencySec = latencyToSeconds(latency, latencyUnit);
        const concurrency = throughput * latencySec;
        document.getElementById("concurrency").value = concurrency.toFixed(2);
        updateDerived();
      }

      document.addEventListener("DOMContentLoaded", function () {
        ["latency", "throughput", "concurrency", "instances"].forEach((id) => {
          const el = document.getElementById(id);
          if (el) el.addEventListener("input", updateDerived);
        });
        ["latencyUnit", "throughputUnit", "percentile"].forEach((id) => {
          const el = document.getElementById(id);
          if (el) el.addEventListener("change", updateDerived);
        });

        document
          .getElementById("solveLatency")
          .addEventListener("click", function (e) {
            e.preventDefault();
            solveLatency();
          });
        document
          .getElementById("solveThroughput")
          .addEventListener("click", function (e) {
            e.preventDefault();
            solveThroughput();
          });
        document
          .getElementById("solveConcurrency")
          .addEventListener("click", function (e) {
            e.preventDefault();
            solveConcurrency();
          });

        updateDerived();
      });

      // Mobile menu toggle
      document.addEventListener("DOMContentLoaded", function () {
        const toggle = document.getElementById("mobile-menu-toggle");
        const menu = document.getElementById("mobile-menu");
        if (toggle && menu) {
          toggle.addEventListener("click", () => {
            const isHidden = menu.classList.contains("hidden");
            menu.classList.toggle("hidden", !isHidden);
            toggle.setAttribute("aria-expanded", String(isHidden));
          });
        }
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
