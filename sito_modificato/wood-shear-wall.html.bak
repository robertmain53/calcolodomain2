            var load = parseFloat(document.getElementById('load').value);
            if (isNaN(wallLength) || wallLength <= 0) {
                document.getElementById('wallLengthError').textContent = 'Please enter a valid wall length.';
            } else {
                document.getElementById('wallLengthError').textContent = '';
            }
            if (isNaN(load) || load <= 0) {
                document.getElementById('loadError').textContent = 'Please enter a valid load.';
            } else {
                document.getElementById('loadError').textContent = '';
            }
            if (!isNaN(wallLength) && wallLength > 0 && !isNaN(load) && load > 0) {
                var shearStrength = load / wallLength;
                document.getElementById('shearStrengthValue').textContent = shearStrength.toFixed(2) + ' lbs/ft';
            }
        });
    </script>


