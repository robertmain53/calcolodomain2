    <script>
      // Simple z-value lookup for common confidence levels
      function zForConfidence(conf) {
        if (conf >= 0.995) return 2.807; // ~99.5%
        if (conf >= 0.99) return 2.576;
        if (conf >= 0.975) return 1.96; // 95%
        if (conf >= 0.95) return 1.96;
        if (conf >= 0.9) return 1.645;
        return 1.96;
      }

      function formatNumber(x, digits = 2) {
        if (!isFinite(x)) return "—";
        if (x === 0) return "0";
        if (Math.abs(x) >= 1e5 || Math.abs(x) < 1e-3) {
          return x.toExponential(digits);
        }
        return x.toLocaleString(undefined, { maximumFractionDigits: digits });
      }

      function clearErrors() {
        document.getElementById("calc-error").textContent = "";
        ["input-M", "input-C", "input-R"].forEach((id) => {
          document.getElementById(id).classList.remove("input-error");
        });
      }

      document
        .getElementById("btn-calc")
        .addEventListener("click", function () {
          clearErrors();
          const M = Number(document.getElementById("input-M").value);
          const C = Number(document.getElementById("input-C").value);
          const R = Number(document.getElementById("input-R").value);
          const conf = Number(document.getElementById("input-alpha").value);

          let hasError = false;
          if (!Number.isFinite(M) || M <= 0) {
            document.getElementById("input-M").classList.add("input-error");
            hasError = true;
          }
          if (!Number.isFinite(C) || C <= 0) {
            document.getElementById("input-C").classList.add("input-error");
            hasError = true;
          }
          if (!Number.isFinite(R) || R < 0) {
            document.getElementById("input-R").classList.add("input-error");
            hasError = true;
          }
          if (hasError) {
            document.getElementById("calc-error").textContent =
              "Please enter valid positive values for M, C, and R.";
            document
              .getElementById("results-container")
              .classList.add("hidden");
            return;
          }
          if (R === 0) {
            document.getElementById("input-R").classList.add("input-error");
            document.getElementById("calc-error").textContent =
              "R = 0 (no recaptures). The Lincoln-Petersen estimator cannot be computed. Increase sample sizes.";
            document
              .getElementById("results-container")
              .classList.add("hidden");
            return;
          }
          if (R > M || R > C) {
            document.getElementById("input-R").classList.add("input-error");
            document.getElementById("calc-error").textContent =
              "R cannot be greater than M or C. Check your counts.";
            document
              .getElementById("results-container")
              .classList.add("hidden");
            return;
          }

          // Chapman estimator
          const Nhat = ((M + 1) * (C + 1)) / (R + 1) - 1;

          // Variance of Chapman estimator
          const numerator = (M + 1) * (C + 1) * (M - R) * (C - R);
          const denominator = (R + 1) * (R + 1) * (R + 2);
          let variance = numerator / denominator;
          if (!Number.isFinite(variance) || variance < 0) variance = NaN;
          const se = Math.sqrt(Math.max(variance, 0));

          const z = zForConfidence(conf);
          let ciLow = Nhat - z * se;
          let ciHigh = Nhat + z * se;
          if (!Number.isFinite(ciLow)) ciLow = NaN;
          if (!Number.isFinite(ciHigh)) ciHigh = NaN;
          if (ciLow < 0) ciLow = 0;

          // Diagnostics
          const recaptureRate = R / C;
          const markedFraction = M / Nhat;
          const cv = se / Nhat;

          document.getElementById("res-Nhat").textContent = formatNumber(
            Nhat,
            0
          );
          document.getElementById("res-ci-low").textContent = formatNumber(
            ciLow,
            0
          );
          document.getElementById("res-ci-high").textContent = formatNumber(
            ciHigh,
            0
          );
          document.getElementById("res-ci-level").textContent =
            Math.round(conf * 100) + "%";

          document.getElementById("res-recapture-rate").textContent =
            formatNumber(recaptureRate, 3);
          document.getElementById("res-marked-fraction").textContent =
            formatNumber(markedFraction, 3);
          document.getElementById("res-se").textContent = formatNumber(se, 2);
          document.getElementById("res-cv").textContent = formatNumber(cv, 3);

          // Intermediate formulas
          const NhatFormula = `((${M} + 1)(${C} + 1) / (${R} + 1)) − 1 = ${formatNumber(
            Nhat,
            3
          )}`;
          document.getElementById("res-Nhat-formula").textContent = NhatFormula;
          document.getElementById("res-var").textContent = isNaN(variance)
            ? "—"
            : formatNumber(variance, 3);
          document.getElementById("res-z").textContent = formatNumber(z, 3);

          // Warning logic
          const warningEl = document.getElementById("res-warning");
          let warnings = [];
          if (recaptureRate < 0.05) {
            warnings.push(
              "Very low recapture rate (R / C). The estimate may be highly uncertain; consider increasing sample sizes."
            );
          }
          if (cv > 0.3) {
            warnings.push(
              "High coefficient of variation (CV > 0.3). The population estimate is imprecise."
            );
          }
          if (markedFraction < 0.05) {
            warnings.push(
              "Only a small fraction of the estimated population was marked (M / N̂ < 0.05). Consider marking more individuals."
            );
          }
          if (warnings.length > 0) {
            warningEl.textContent = warnings.join(" ");
            warningEl.classList.remove("hidden");
          } else {
            warningEl.textContent = "";
            warningEl.classList.add("hidden");
          }

          document
            .getElementById("results-container")
            .classList.remove("hidden");
        });

      document
        .getElementById("btn-clear")
        .addEventListener("click", function () {
          ["input-M", "input-C", "input-R"].forEach((id) => {
            document.getElementById(id).value = "";
            document.getElementById(id).classList.remove("input-error");
          });
          document.getElementById("calc-error").textContent = "";
          document.getElementById("results-container").classList.add("hidden");
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
