    <script>
      // Helper: update hints based on input type & period
      function updateHints() {
        const returnType = document.getElementById("returnType").value;
        const period = document.getElementById("period").value;
        const suffix =
          period === "annual"
            ? "per year"
            : period === "monthly"
            ? "per month"
            : "per quarter";
        const format = returnType === "percent" ? "Percent" : "Decimal";
        document.getElementById("portfolioReturnHint").textContent =
          format + " " + suffix;
        document.getElementById("riskFreeRateHint").textContent =
          format + " " + suffix;
      }

      function parseReturn(value, type) {
        if (isNaN(value)) return NaN;
        return type === "percent" ? value / 100 : value;
      }

      function formatReturn(value, type) {
        if (!isFinite(value)) return "–";
        if (type === "percent") {
          return (value * 100).toFixed(2) + "%";
        }
        return value.toFixed(4);
      }

      function interpretTreynor(t) {
        if (!isFinite(t))
          return "Treynor ratio is undefined for beta close to zero.";
        if (t > 0.2)
          return "Very strong risk-adjusted performance relative to systematic risk (for the chosen period).";
        if (t > 0.1) return "Good risk-adjusted performance per unit of beta.";
        if (t > 0.0)
          return "Positive risk-adjusted performance; portfolio beat the risk-free rate given its beta.";
        if (t === 0)
          return "Portfolio performed roughly in line with the risk-free rate after adjusting for beta.";
        return "Negative risk-adjusted performance; the portfolio underperformed the risk-free rate given its beta.";
      }

      document.addEventListener("DOMContentLoaded", function () {
        updateHints();

        const returnTypeEl = document.getElementById("returnType");
        const periodEl = document.getElementById("period");
        const calcBtn = document.getElementById("calculateBtn");
        const addBtn = document.getElementById("addPortfolioBtn");
        const resetBtn = document.getElementById("resetBtn");

        let lastResult = null;
        let portfolioCounter = 1;

        returnTypeEl.addEventListener("change", updateHints);
        periodEl.addEventListener("change", updateHints);

        calcBtn.addEventListener("click", function () {
          const rawRp = parseFloat(
            document.getElementById("portfolioReturn").value
          );
          const rawRf = parseFloat(
            document.getElementById("riskFreeRate").value
          );
          const beta = parseFloat(document.getElementById("beta").value);
          const type = returnTypeEl.value;
          const period = periodEl.value;

          if (isNaN(rawRp) || isNaN(rawRf) || isNaN(beta)) {
            alert("Please enter portfolio return, risk-free rate, and beta.");
            return;
          }
          if (beta === 0) {
            alert(
              "Beta cannot be zero for the Treynor ratio. Please use a non-zero beta."
            );
            return;
          }

          const Rp = parseReturn(rawRp, type);
          const Rf = parseReturn(rawRf, type);
          const excess = Rp - Rf;
          const treynor = excess / beta;

          lastResult = {
            Rp,
            Rf,
            beta,
            treynor,
            rawRp,
            rawRf,
            type,
            period,
          };

          const resultCard = document.getElementById("resultCard");
          document.getElementById("treynorValue").textContent =
            treynor.toFixed(4);
          document.getElementById("treynorUnits").textContent =
            type === "percent"
              ? "(excess % return per unit of beta)"
              : "(excess decimal return per unit of beta)";
          document.getElementById("excessReturn").textContent = formatReturn(
            excess,
            type
          );
          document.getElementById("interpretationText").textContent =
            interpretTreynor(treynor);
          document.getElementById("summaryPeriod").textContent =
            period === "annual"
              ? "annual"
              : period === "monthly"
              ? "monthly"
              : "quarterly";
          document.getElementById("summaryReturnType").textContent =
            type === "percent" ? "percent" : "decimal";

          resultCard.classList.remove("hidden");
        });

        addBtn.addEventListener("click", function () {
          if (!lastResult) {
            alert("Calculate a Treynor ratio first.");
            return;
          }
          const tbody = document.getElementById("comparisonBody");
          const placeholder = document.getElementById("comparisonPlaceholder");
          if (placeholder) placeholder.remove();

          const tr = document.createElement("tr");
          tr.className = "border-t";

          const nameTd = document.createElement("td");
          const nameInput = document.createElement("input");
          nameInput.type = "text";
          nameInput.value = "Portfolio " + portfolioCounter++;
          nameInput.className =
            "w-full border border-gray-200 rounded px-2 py-1 text-xs";
          nameTd.className = "px-3 py-2";
          nameTd.appendChild(nameInput);

          const rTd = document.createElement("td");
          rTd.className = "px-3 py-2 text-right text-xs text-gray-700";
          rTd.textContent = formatReturn(lastResult.Rp, lastResult.type);

          const rfTd = document.createElement("td");
          rfTd.className = "px-3 py-2 text-right text-xs text-gray-700";
          rfTd.textContent = formatReturn(lastResult.Rf, lastResult.type);

          const bTd = document.createElement("td");
          bTd.className = "px-3 py-2 text-right text-xs text-gray-700";
          bTd.textContent = lastResult.beta.toFixed(3);

          const tTd = document.createElement("td");
          tTd.className = "px-3 py-2 text-right text-xs font-semibold";
          tTd.textContent = lastResult.treynor.toFixed(4);

          const actionTd = document.createElement("td");
          actionTd.className = "px-3 py-2 text-center";
          const delBtn = document.createElement("button");
          delBtn.type = "button";
          delBtn.textContent = "Remove";
          delBtn.className = "text-xs text-red-600 hover:underline";
          delBtn.addEventListener("click", function () {
            tr.remove();
            if (!tbody.querySelector("tr")) {
              const ph = document.createElement("tr");
              ph.id = "comparisonPlaceholder";
              ph.innerHTML =
                '<td colspan="6" class="px-3 py-3 text-center text-gray-400 text-xs">No portfolios added yet. Calculate a Treynor ratio and click “Add to comparison table”.</td>';
              tbody.appendChild(ph);
            }
          });
          actionTd.appendChild(delBtn);

          tr.appendChild(nameTd);
          tr.appendChild(rTd);
          tr.appendChild(rfTd);
          tr.appendChild(bTd);
          tr.appendChild(tTd);
          tr.appendChild(actionTd);

          tbody.appendChild(tr);
        });

        resetBtn.addEventListener("click", function () {
          document.getElementById("portfolioReturn").value = "";
          document.getElementById("riskFreeRate").value = "";
          document.getElementById("beta").value = "";
          document.getElementById("resultCard").classList.add("hidden");
          lastResult = null;
        });
      });
    </script>

