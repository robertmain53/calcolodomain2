    <script>
      const tabAngle = document.getElementById('tab-angle');
      const tabSagitta = document.getElementById('tab-sagitta');
      const tabDist = document.getElementById('tab-dist');
      const tabArc = document.getElementById('tab-arc');

      const panelAngle = document.getElementById('panel-angle');
      const panelSagitta = document.getElementById('panel-sagitta');
      const panelDist = document.getElementById('panel-dist');
      const panelArc = document.getElementById('panel-arc');

      function setActiveTab(tab) {
        [tabAngle, tabSagitta, tabDist, tabArc].forEach(t => t.classList.remove('tab-active'));
        tab.classList.add('tab-active');
      }
      function showPanel(panel) {
        [panelAngle, panelSagitta, panelDist, panelArc].forEach(p => p.classList.add('hidden'));
        panel.classList.remove('hidden');
      }

      tabAngle.addEventListener('click', () => { setActiveTab(tabAngle); showPanel(panelAngle); });
      tabSagitta.addEventListener('click', () => { setActiveTab(tabSagitta); showPanel(panelSagitta); });
      tabDist.addEventListener('click', () => { setActiveTab(tabDist); showPanel(panelDist); });
      tabArc.addEventListener('click', () => { setActiveTab(tabArc); showPanel(panelArc); });

      const btnCalc = document.getElementById('btn-calc');
      const btnReset = document.getElementById('btn-reset');

      const resultBox = document.getElementById('result-box');
      const resChord = document.getElementById('res-chord');
      const resAngleDeg = document.getElementById('res-angle-deg');
      const resAngleRad = document.getElementById('res-angle-rad');
      const resArc = document.getElementById('res-arc');
      const resSeg = document.getElementById('res-seg');
      const resTri = document.getElementById('res-tri');

      function round4(x) {
        return Math.round((x + Number.EPSILON) * 10000) / 10000;
      }

      function calcFromAngle() {
        const r = parseFloat(document.getElementById('ra-radius').value);
        const deg = parseFloat(document.getElementById('ra-angle').value);
        if (isNaN(r) || isNaN(deg) || r <= 0 || deg <= 0) return { error: 'Enter positive radius and angle.' };
        const rad = deg * Math.PI / 180;
        const chord = 2 * r * Math.sin(rad / 2);
        const arc = r * rad;
        const triArea = 0.5 * r * r * Math.sin(rad);
        const segArea = 0.5 * r * r * (rad - Math.sin(rad));
        return { chord, deg, rad, arc, segArea, triArea };
      }

      function calcFromSagitta() {
        const r = parseFloat(document.getElementById('rs-radius').value);
        const s = parseFloat(document.getElementById('rs-sagitta').value);
        if (isNaN(r) || isNaN(s) || r <= 0 || s <= 0) return { error: 'Enter positive radius and sagitta.' };
        if (s >= 2*r) return { error: 'Sagitta too large for given radius.' };
        const chord = 2 * Math.sqrt(2*r*s - s*s);
        // find angle via chord
        const rad = 2 * Math.asin(chord / (2*r));
        const deg = rad * 180 / Math.PI;
        const arc = r * rad;
        const triArea = 0.5 * r * r * Math.sin(rad);
        const segArea = 0.5 * r * r * (rad - Math.sin(rad));
        return { chord, deg, rad, arc, segArea, triArea };
      }

      function calcFromDist() {
        const r = parseFloat(document.getElementById('rd-radius').value);
        const d = parseFloat(document.getElementById('rd-dist').value);
        if (isNaN(r) || isNaN(d) || r <= 0 || d < 0) return { error: 'Enter positive radius and non-negative distance.' };
        if (d > r) return { error: 'Distance to chord cannot exceed radius.' };
        const half = Math.sqrt(r*r - d*d);
        const chord = 2 * half;
        const rad = 2 * Math.asin(chord / (2*r));
        const deg = rad * 180 / Math.PI;
        const arc = r * rad;
        const triArea = 0.5 * r * r * Math.sin(rad);
        const segArea = 0.5 * r * r * (rad - Math.sin(rad));
        return { chord, deg, rad, arc, segArea, triArea };
      }

      function calcFromArc() {
        const r = parseFloat(document.getElementById('rl-radius').value);
        const L = parseFloat(document.getElementById('rl-arc').value);
        if (isNaN(r) || isNaN(L) || r <= 0 || L <= 0) return { error: 'Enter positive radius and arc length.' };
        const rad = L / r;
        if (rad > 2*Math.PI) return { error: 'Arc length corresponds to angle > 360°.' };
        const deg = rad * 180 / Math.PI;
        const chord = 2 * r * Math.sin(rad / 2);
        const triArea = 0.5 * r * r * Math.sin(rad);
        const segArea = 0.5 * r * r * (rad - Math.sin(rad));
        return { chord, deg, rad, arc: L, segArea, triArea };
      }

      btnCalc.addEventListener('click', function() {
        let res;
        if (!panelAngle.classList.contains('hidden')) {
          res = calcFromAngle();
        } else if (!panelSagitta.classList.contains('hidden')) {
          res = calcFromSagitta();
        } else if (!panelDist.classList.contains('hidden')) {
          res = calcFromDist();
        } else {
          res = calcFromArc();
        }

        resultBox.classList.remove('hidden');

        if (res.error) {
          resChord.textContent = res.error;
          resAngleDeg.textContent = '—';
          resAngleRad.textContent = '—';
          resArc.textContent = '—';
          resSeg.textContent = '—';
          resTri.textContent = '—';
          return;
        }

        resChord.textContent = round4(res.chord);
        resAngleDeg.textContent = round4(res.deg);
        resAngleRad.textContent = round4(res.rad);
        resArc.textContent = round4(res.arc);
        resSeg.textContent = round4(res.segArea);
        resTri.textContent = round4(res.triArea);
      });

      btnReset.addEventListener('click', function() {
        document.getElementById('ra-radius').value = 10;
        document.getElementById('ra-angle').value = 60;
        document.getElementById('rs-radius').value = 10;
        document.getElementById('rs-sagitta').value = 2;
        document.getElementById('rd-radius').value = 10;
        document.getElementById('rd-dist').value = 6;
        document.getElementById('rl-radius').value = 10;
        document.getElementById('rl-arc').value = 6;
        resultBox.classList.add('hidden');
      });
    </script>

