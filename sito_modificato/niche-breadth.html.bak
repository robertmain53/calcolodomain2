    <script>
      // Mobile menu toggle
      document
        .getElementById("mobile-menu-toggle")
        ?.addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          if (!menu) return;
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          this.setAttribute("aria-expanded", String(isHidden));
        });

      function parseResourceNames(raw) {
        if (!raw) return [];
        return raw
          .split(/[\n,;\t]/)
          .map((s) => s.trim())
          .filter((s) => s.length > 0);
      }

      function parseSpeciesData(raw, nCategories) {
        const lines = raw
          .split(/\r?\n/)
          .map((l) => l.trim())
          .filter((l) => l.length > 0);
        const species = [];
        const errors = [];

        lines.forEach((line, idx) => {
          const parts = line.split(/[\t,]+/).map((s) => s.trim());
          if (parts.length < 2) {
            errors.push(`Line ${idx + 1}: not enough columns.`);
            return;
          }
          const name = parts[0] || `Species_${idx + 1}`;
          const values = parts
            .slice(1)
            .map((v) => parseFloat(v.replace(",", ".")));
          if (values.some((v) => isNaN(v))) {
            errors.push(
              `Line ${idx + 1} (${name}): contains non-numeric values.`
            );
            return;
          }
          if (nCategories && values.length !== nCategories) {
            errors.push(
              `Line ${
                idx + 1
              } (${name}): expected ${nCategories} values, found ${
                values.length
              }.`
            );
            return;
          }
          species.push({ name, values });
        });

        return { species, errors };
      }

      function normalize(values) {
        const sum = values.reduce((acc, v) => acc + (v > 0 ? v : 0), 0);
        if (sum <= 0) return null;
        return values.map((v) => (v > 0 ? v / sum : 0));
      }

      function computeLevins(p) {
        const denom = p.reduce((acc, pi) => acc + pi * pi, 0);
        if (denom === 0) return { B: 0, Bsta: 0 };
        const B = 1 / denom;
        return { B };
      }

      function computeStandardizedB(B, n) {
        if (n <= 1) return 0;
        return (B - 1) / (n - 1);
      }

      function computeShannon(p) {
        let H = 0;
        p.forEach((pi) => {
          if (pi > 0) H += -pi * Math.log(pi);
        });
        const Neff = Math.exp(H);
        return { H, Neff };
      }

      function classifySpecies(Bsta, generalistThr, specialistThr) {
        if (isNaN(Bsta)) return "Not classified";
        if (Bsta >= generalistThr) return "Generalist";
        if (Bsta <= specialistThr) return "Specialist";
        return "Intermediate";
      }

      function formatNumber(x, digits = 3) {
        if (!isFinite(x)) return "–";
        return x.toFixed(digits);
      }

      document
        .getElementById("add-example-btn")
        ?.addEventListener("click", () => {
          const resEl = document.getElementById("resource-names");
          const dataEl = document.getElementById("species-data");
          if (!resEl || !dataEl) return;

          resEl.value = "Habitat A, Habitat B, Habitat C, Habitat D";
          dataEl.value = `Warbler_A, 25, 5, 0, 0
Warbler_B, 10, 12, 8, 10
Warbler_C, 0, 0, 18, 2`;
        });

      document.getElementById("reset-btn")?.addEventListener("click", () => {
        const resEl = document.getElementById("resource-names");
        const dataEl = document.getElementById("species-data");
        const errorBox = document.getElementById("error-box");
        const resultsSection = document.getElementById("results-section");
        if (resEl) resEl.value = "";
        if (dataEl) dataEl.value = "";
        if (errorBox) {
          errorBox.classList.add("hidden");
          errorBox.textContent = "";
        }
        if (resultsSection) resultsSection.classList.add("hidden");
      });

      document
        .getElementById("calculate-btn")
        ?.addEventListener("click", () => {
          const resEl = document.getElementById("resource-names");
          const dataEl = document.getElementById("species-data");
          const errorBox = document.getElementById("error-box");
          const resultsSection = document.getElementById("results-section");
          const tbody = document.getElementById("results-table-body");
          const summaryBox = document.getElementById("summary-box");
          const normWrapper = document.getElementById(
            "normalized-table-wrapper"
          );

          if (
            !resEl ||
            !dataEl ||
            !errorBox ||
            !resultsSection ||
            !tbody ||
            !summaryBox ||
            !normWrapper
          )
            return;

          errorBox.classList.add("hidden");
          errorBox.textContent = "";
          resultsSection.classList.add("hidden");
          tbody.innerHTML = "";
          summaryBox.textContent = "";
          normWrapper.innerHTML = "";

          const resources = parseResourceNames(resEl.value);
          if (resources.length < 2) {
            errorBox.textContent =
              "Please provide at least two resource categories.";
            errorBox.classList.remove("hidden");
            return;
          }

          const { species, errors } = parseSpeciesData(
            dataEl.value,
            resources.length
          );
          if (species.length === 0) {
            errorBox.textContent = errors.length
              ? errors.join(" ")
              : "Please enter at least one species line.";
            errorBox.classList.remove("hidden");
            return;
          }

          const inputMode =
            document.querySelector('input[name="inputMode"]:checked')?.value ||
            "counts";
          const computeStd =
            document.getElementById("compute-standardized")?.checked ?? true;
          const generalistThr = parseFloat(
            document.getElementById("generalist-threshold")?.value || "0.6"
          );
          const specialistThr = parseFloat(
            document.getElementById("specialist-threshold")?.value || "0.3"
          );

          const skipped = [];
          const results = [];

          species.forEach((sp) => {
            let p;
            if (inputMode === "counts") {
              p = normalize(sp.values);
            } else {
              p = normalize(sp.values); // renormalize proportions to be safe
            }
            if (!p) {
              skipped.push(sp.name);
              return;
            }

            const { B } = computeLevins(p);
            const Bsta = computeStd
              ? computeStandardizedB(B, resources.length)
              : NaN;
            const { H, Neff } = computeShannon(p);

            const classification = computeStd
              ? classifySpecies(Bsta, generalistThr, specialistThr)
              : "–";

            results.push({
              name: sp.name,
              p,
              B,
              Bsta,
              H,
              Neff,
              classification,
            });
          });

          if (results.length === 0) {
            errorBox.textContent =
              "No valid species after filtering. Check that each species has positive counts or proportions.";
            errorBox.classList.remove("hidden");
            return;
          }

          results.forEach((r) => {
            const tr = document.createElement("tr");
            tr.innerHTML = `
        <td class="px-3 py-2 border-b text-gray-800">${r.name}</td>
        <td class="px-3 py-2 border-b text-right tabular-nums">${formatNumber(
          r.B,
          3
        )}</td>
        <td class="px-3 py-2 border-b text-right tabular-nums">${
          computeStd ? formatNumber(r.Bsta, 3) : "–"
        }</td>
        <td class="px-3 py-2 border-b text-right tabular-nums">${formatNumber(
          r.H,
          3
        )}</td>
        <td class="px-3 py-2 border-b text-right tabular-nums">${formatNumber(
          r.Neff,
          2
        )}</td>
        <td class="px-3 py-2 border-b text-left text-gray-700">${
          r.classification
        }</td>
      `;
            tbody.appendChild(tr);
          });

          // Summary
          const Bstas = results.map((r) => r.Bsta).filter((v) => isFinite(v));
          const Bs = results.map((r) => r.B).filter((v) => isFinite(v));
          const Hs = results.map((r) => r.H).filter((v) => isFinite(v));
          const Neffs = results.map((r) => r.Neff).filter((v) => isFinite(v));

          function mean(arr) {
            if (!arr.length) return NaN;
            return arr.reduce((a, b) => a + b, 0) / arr.length;
          }

          const meanB = mean(Bs);
          const meanBsta = mean(Bstas);
          const meanH = mean(Hs);
          const meanNeff = mean(Neffs);

          summaryBox.innerHTML = `
      <p class="mb-1"><strong>Number of species:</strong> ${results.length}</p>
      <p class="mb-1"><strong>Mean Levins’ B:</strong> ${formatNumber(
        meanB,
        3
      )}</p>
      ${
        computeStd
          ? `<p class="mb-1"><strong>Mean standardized B<sub>sta</sub>:</strong> ${formatNumber(
              meanBsta,
              3
            )}</p>`
          : ""
      }
      <p class="mb-1"><strong>Mean Shannon H′:</strong> ${formatNumber(
        meanH,
        3
      )} (mean effective states: ${formatNumber(meanNeff, 2)})</p>
      ${
        skipped.length
          ? `<p class="mt-1 text-xs text-gray-500">Skipped species (zero or invalid totals): ${skipped.join(
              ", "
            )}</p>`
          : ""
      }
    `;

          // Normalized table
          const table = document.createElement("table");
          table.className =
            "min-w-full text-xs border border-gray-200 rounded-md overflow-hidden";
          const thead = document.createElement("thead");
          thead.className = "bg-gray-100";
          const headRow = document.createElement("tr");
          headRow.innerHTML = `
      <th class="px-2 py-1 text-left font-semibold text-gray-700 border-b">Species</th>
      ${resources
        .map(
          (r) =>
            `<th class="px-2 py-1 text-right font-semibold text-gray-700 border-b">${r}</th>`
        )
        .join("")}
    `;
          thead.appendChild(headRow);
          table.appendChild(thead);

          const tb = document.createElement("tbody");
          tb.className = "divide-y divide-gray-100";
          results.forEach((r) => {
            const row = document.createElement("tr");
            row.innerHTML = `
        <td class="px-2 py-1 border-b text-gray-800">${r.name}</td>
        ${r.p
          .map(
            (pi) =>
              `<td class="px-2 py-1 border-b text-right tabular-nums">${formatNumber(
                pi,
                3
              )}</td>`
          )
          .join("")}
      `;
            tb.appendChild(row);
          });
          table.appendChild(tb);
          normWrapper.appendChild(table);

          resultsSection.classList.remove("hidden");
        });
    </script>

