      // Simple client-side filter for civil groups/items
      (function () {
        const input = document.getElementById("civil-filter");
        if (!input) return;
        const groups = Array.from(document.querySelectorAll(".civil-group"));
        const items = Array.from(document.querySelectorAll(".civil-item"));

        function normalize(str) {
          return (str || "").toLowerCase();
        }

        input.addEventListener("input", function () {
          const q = normalize(this.value.trim());
          if (!q) {
            groups.forEach((g) => g.classList.remove("hidden"));
            items.forEach((i) => i.classList.remove("hidden"));
            return;
          }

          groups.forEach((group) => {
            const groupTags = normalize(group.getAttribute("data-group"));
            let groupMatch = groupTags.includes(q);
            let anyItemMatch = false;

            const groupItems = Array.from(
              group.querySelectorAll(".civil-item")
            );
            groupItems.forEach((item) => {
              const text = normalize(item.textContent);
              const tags = normalize(item.getAttribute("data-tags"));
              const match = text.includes(q) || tags.includes(q);
              if (match) {
                item.classList.remove("hidden");
                anyItemMatch = true;
              } else {
                item.classList.add("hidden");
              }
            });

            if (groupMatch || anyItemMatch) {
              group.classList.remove("hidden");
            } else {
              group.classList.add("hidden");
            }
          });
        });
      })();
    </script>

