    <script>
      function parseNumber(id) {
        const v = parseFloat(
          document.getElementById(id).value.replace(/,/g, "")
        );
        return isNaN(v) ? 0 : Math.max(0, v);
      }

      function formatINR(n) {
        if (!isFinite(n)) return "₹0";
        const sign = n < 0 ? "-" : "";
        n = Math.round(Math.abs(n));
        const s = n.toString();
        if (s.length <= 3) return sign + "₹" + s;
        const last3 = s.slice(-3);
        const rest = s.slice(0, -3);
        const restWithCommas = rest.replace(/\B(?=(\d{2})+(?!\d))/g, ",");
        return sign + "₹" + restWithCommas + "," + last3;
      }

      function slabTaxOld(taxable, ageCat) {
        // Simplified old regime slabs (illustrative)
        let slabs;
        if (ageCat === "super") {
          slabs = [
            { upTo: 500000, rate: 0 },
            { upTo: 1000000, rate: 0.2 },
            { upTo: Infinity, rate: 0.3 },
          ];
        } else if (ageCat === "senior") {
          slabs = [
            { upTo: 300000, rate: 0 },
            { upTo: 500000, rate: 0.05 },
            { upTo: 1000000, rate: 0.2 },
            { upTo: Infinity, rate: 0.3 },
          ];
        } else {
          slabs = [
            { upTo: 250000, rate: 0 },
            { upTo: 500000, rate: 0.05 },
            { upTo: 1000000, rate: 0.2 },
            { upTo: Infinity, rate: 0.3 },
          ];
        }
        let tax = 0;
        let prev = 0;
        for (const s of slabs) {
          if (taxable <= prev) break;
          const amt = Math.min(taxable, s.upTo) - prev;
          tax += amt * s.rate;
          prev = s.upTo;
        }
        return tax;
      }

      function slabTaxNew(taxable) {
        // New regime slabs (illustrative for FY 2025-26)
        const slabs = [
          { upTo: 300000, rate: 0 },
          { upTo: 700000, rate: 0.05 },
          { upTo: 1000000, rate: 0.1 },
          { upTo: 1200000, rate: 0.15 },
          { upTo: 1500000, rate: 0.2 },
          { upTo: Infinity, rate: 0.3 },
        ];
        let tax = 0;
        let prev = 0;
        for (const s of slabs) {
          if (taxable <= prev) break;
          const amt = Math.min(taxable, s.upTo) - prev;
          tax += amt * s.rate;
          prev = s.upTo;
        }
        return tax;
      }

      function surchargeRate(totalIncome) {
        if (totalIncome <= 5000000) return 0;
        if (totalIncome <= 10000000) return 0.1;
        if (totalIncome <= 20000000) return 0.15;
        if (totalIncome <= 50000000) return 0.25;
        return 0.37;
      }

      function calculate() {
        const ageCat = document.getElementById("age").value;
        const surchargeMode = document.getElementById("surcharge-toggle").value;

        const salary = parseNumber("income-salary");
        const other = parseNumber("income-other");
        const gross = salary + other;

        if (gross <= 0) {
          const err = document.getElementById("error-box");
          err.textContent = "Please enter a positive income amount.";
          err.classList.remove("hidden");
          document.getElementById("results").classList.add("hidden");
          return;
        } else {
          document.getElementById("error-box").classList.add("hidden");
        }

        const stdOld = parseNumber("std-ded-old");
        const stdNew = parseNumber("std-ded-new");

        const ded80c = parseNumber("ded-80c");
        const ded80d = parseNumber("ded-80d");
        const dedHousing = parseNumber("ded-housing-interest");
        const dedOther = parseNumber("ded-other");

        const totalDeductionsOld =
          stdOld + ded80c + ded80d + dedHousing + dedOther;
        const taxableOld = Math.max(0, gross - totalDeductionsOld);
        const taxableNew = Math.max(0, gross - stdNew);

        const taxCoreOld = slabTaxOld(taxableOld, ageCat);
        const taxCoreNew = slabTaxNew(taxableNew);

        let surOld = 0,
          surNew = 0;
        if (surchargeMode === "auto") {
          const rOld = surchargeRate(gross);
          const rNew = surchargeRate(gross);
          surOld = taxCoreOld * rOld;
          surNew = taxCoreNew * rNew;
        }

        const cessOld = 0.04 * (taxCoreOld + surOld);
        const cessNew = 0.04 * (taxCoreNew + surNew);

        const totalOld = taxCoreOld + surOld + cessOld;
        const totalNew = taxCoreNew + surNew + cessNew;

        // Update summary
        document.getElementById("tax-old").textContent = formatINR(totalOld);
        document.getElementById("tax-new").textContent = formatINR(totalNew);

        const effOld = gross > 0 ? (totalOld / gross) * 100 : 0;
        const effNew = gross > 0 ? (totalNew / gross) * 100 : 0;
        document.getElementById("eff-rate-old").textContent =
          effOld.toFixed(2) + "%";
        document.getElementById("eff-rate-new").textContent =
          effNew.toFixed(2) + "%";

        // Table
        document.getElementById("row-gross-old").textContent =
          formatINR(gross).slice(1);
        document.getElementById("row-gross-new").textContent =
          formatINR(gross).slice(1);
        document.getElementById("row-std-old").textContent =
          formatINR(stdOld).slice(1);
        document.getElementById("row-std-new").textContent =
          formatINR(stdNew).slice(1);
        document.getElementById("row-ded-old").textContent = formatINR(
          ded80c + ded80d + dedHousing + dedOther
        ).slice(1);
        document.getElementById("row-taxable-old").textContent =
          formatINR(taxableOld).slice(1);
        document.getElementById("row-taxable-new").textContent =
          formatINR(taxableNew).slice(1);
        document.getElementById("row-taxcore-old").textContent =
          formatINR(taxCoreOld).slice(1);
        document.getElementById("row-taxcore-new").textContent =
          formatINR(taxCoreNew).slice(1);
        document.getElementById("row-surcharge-old").textContent =
          formatINR(surOld).slice(1);
        document.getElementById("row-surcharge-new").textContent =
          formatINR(surNew).slice(1);
        document.getElementById("row-cess-old").textContent =
          formatINR(cessOld).slice(1);
        document.getElementById("row-cess-new").textContent =
          formatINR(cessNew).slice(1);
        document.getElementById("row-total-old").textContent =
          formatINR(totalOld).slice(1);
        document.getElementById("row-total-new").textContent =
          formatINR(totalNew).slice(1);

        // Recommendation badges
        const badgeOld = document.getElementById("badge-old");
        const badgeNew = document.getElementById("badge-new");
        badgeOld.textContent = "–";
        badgeNew.textContent = "–";
        badgeOld.className = "badge bg-gray-200 text-gray-800";
        badgeNew.className = "badge bg-gray-200 text-gray-800";

        const rec = document.getElementById("recommendation");
        if (Math.abs(totalOld - totalNew) < 1) {
          rec.textContent =
            "Both regimes give almost the same tax in this scenario. You can choose based on simplicity and future planning.";
        } else if (totalOld < totalNew) {
          badgeOld.textContent = "Lower tax";
          badgeOld.className = "badge bg-green-100 text-green-800";
          badgeNew.textContent = "Higher tax";
          badgeNew.className = "badge bg-red-100 text-red-800";
          rec.textContent =
            "Based on your inputs, the old regime gives lower estimated tax than the new regime. If you can actually claim these deductions, the old regime may be preferable.";
        } else {
          badgeNew.textContent = "Lower tax";
          badgeNew.className = "badge bg-green-100 text-green-800";
          badgeOld.textContent = "Higher tax";
          badgeOld.className = "badge bg-red-100 text-red-800";
          rec.textContent =
            "Based on your inputs, the new regime gives lower estimated tax than the old regime. If you do not plan to claim many deductions, the new regime may be preferable.";
        }

        document.getElementById("results").classList.remove("hidden");
      }

      document.getElementById("calc-btn").addEventListener("click", calculate);

      document.getElementById("reset-btn").addEventListener("click", () => {
        [
          "income-salary",
          "income-other",
          "ded-80c",
          "ded-80d",
          "ded-housing-interest",
          "ded-other",
        ].forEach((id) => {
          document.getElementById(id).value = "";
        });
        document.getElementById("std-ded-old").value = 75000;
        document.getElementById("std-ded-new").value = 75000;
        document.getElementById("age").value = "below60";
        document.getElementById("resident-status").value = "resident";
        document.getElementById("surcharge-toggle").value = "auto";
        document.getElementById("results").classList.add("hidden");
        document.getElementById("error-box").classList.add("hidden");
      });
    </script>

