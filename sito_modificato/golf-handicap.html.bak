    <script>
      // Utility: create rows
      function createStrokeRow(i) {
        return `
      <tr class="border-t">
        <td class="px-2 py-1 text-gray-500">${i + 1}</td>
        <td class="px-2 py-1">
          <input type="date" class="w-full border rounded-md px-2 py-1 text-xs" />
        </td>
        <td class="px-2 py-1">
          <input type="number" class="w-20 border rounded-md px-2 py-1 text-xs input-small stroke-gross" min="40" max="200" step="1" />
        </td>
        <td class="px-2 py-1">
          <input type="number" class="w-20 border rounded-md px-2 py-1 text-xs input-small stroke-rating" step="0.1" />
        </td>
        <td class="px-2 py-1">
          <input type="number" class="w-20 border rounded-md px-2 py-1 text-xs input-small stroke-slope" min="55" max="155" step="1" />
        </td>
        <td class="px-2 py-1">
          <input type="number" class="w-16 border rounded-md px-2 py-1 text-xs input-small stroke-par" step="1" />
        </td>
        <td class="px-2 py-1">
          <input type="number" class="w-16 border rounded-md px-2 py-1 text-xs input-small stroke-pcc" step="0.1" />
        </td>
        <td class="px-2 py-1 text-right text-gray-700 stroke-diff">–</td>
      </tr>
    `;
      }

      function createStablefordRow(i) {
        return `
      <tr class="border-t">
        <td class="px-2 py-1 text-gray-500">${i + 1}</td>
        <td class="px-2 py-1">
          <input type="date" class="w-full border rounded-md px-2 py-1 text-xs" />
        </td>
        <td class="px-2 py-1">
          <input type="number" class="w-20 border rounded-md px-2 py-1 text-xs input-small stab-points" min="0" max="54" step="1" />
        </td>
        <td class="px-2 py-1">
          <input type="number" class="w-20 border rounded-md px-2 py-1 text-xs input-small stab-rating" step="0.1" />
        </td>
        <td class="px-2 py-1">
          <input type="number" class="w-20 border rounded-md px-2 py-1 text-xs input-small stab-slope" min="55" max="155" step="1" />
        </td>
        <td class="px-2 py-1">
          <input type="number" class="w-16 border rounded-md px-2 py-1 text-xs input-small stab-par" step="1" />
        </td>
        <td class="px-2 py-1">
          <input type="number" class="w-16 border rounded-md px-2 py-1 text-xs input-small stab-pcc" step="0.1" />
        </td>
        <td class="px-2 py-1 text-right text-gray-700 stab-diff">–</td>
      </tr>
    `;
      }

      function initRows() {
        const strokeBody = document.getElementById("stroke-rows");
        const stabBody = document.getElementById("stableford-rows");
        let strokeHtml = "";
        let stabHtml = "";
        for (let i = 0; i < 20; i++) {
          strokeHtml += createStrokeRow(i);
          stabHtml += createStablefordRow(i);
        }
        strokeBody.innerHTML = strokeHtml;
        stabBody.innerHTML = stabHtml;
      }

      function parseNum(v) {
        const n = parseFloat(v);
        return isNaN(n) ? null : n;
      }

      function calcDifferentialsFromStroke() {
        const rows = document.querySelectorAll("#stroke-rows tr");
        const diffs = [];
        rows.forEach((row) => {
          const gross = parseNum(row.querySelector(".stroke-gross").value);
          const rating = parseNum(row.querySelector(".stroke-rating").value);
          const slope = parseNum(row.querySelector(".stroke-slope").value);
          const par = parseNum(row.querySelector(".stroke-par").value);
          const pcc = parseNum(row.querySelector(".stroke-pcc").value) || 0;
          const cell = row.querySelector(".stroke-diff");

          if (
            gross != null &&
            rating != null &&
            slope != null &&
            par != null &&
            slope > 0
          ) {
            const diff = ((gross - rating - pcc) * 113) / slope;
            diffs.push(diff);
            cell.textContent = diff.toFixed(1);
          } else {
            cell.textContent = "–";
          }
        });
        return diffs;
      }

      function calcDifferentialsFromStableford() {
        const rows = document.querySelectorAll("#stableford-rows tr");
        const diffs = [];
        rows.forEach((row) => {
          const pts = parseNum(row.querySelector(".stab-points").value);
          const rating = parseNum(row.querySelector(".stab-rating").value);
          const slope = parseNum(row.querySelector(".stab-slope").value);
          const par = parseNum(row.querySelector(".stab-par").value);
          const pcc = parseNum(row.querySelector(".stab-pcc").value) || 0;
          const cell = row.querySelector(".stab-diff");

          if (
            pts != null &&
            rating != null &&
            slope != null &&
            par != null &&
            slope > 0
          ) {
            // Approximate Course Handicap from index is unknown; assume CH ≈ (slope/113)*index.
            // Without index, WHS uses a more complex method; here we approximate AGS using:
            // AGS ≈ Par + (36 - Stableford points)
            const adjGross = par + (36 - pts);
            const diff = ((adjGross - rating - pcc) * 113) / slope;
            diffs.push(diff);
            cell.textContent = diff.toFixed(1);
          } else {
            cell.textContent = "–";
          }
        });
        return diffs;
      }

      function computeHandicapIndex() {
        const activeTab = document
          .getElementById("panel-stableford")
          .classList.contains("hidden")
          ? "stroke"
          : "stableford";
        let diffs = [];
        if (activeTab === "stroke") {
          diffs = calcDifferentialsFromStroke();
        } else {
          diffs = calcDifferentialsFromStableford();
        }
        const hiEl = document.getElementById("handicap-index");
        const detailEl = document.getElementById("handicap-index-detail");

        if (!diffs.length) {
          hiEl.textContent = "–";
          detailEl.textContent =
            "Enter at least one valid round to compute a handicap.";
          return null;
        }

        diffs.sort((a, b) => a - b);
        const used =
          diffs.length >= 8 ? diffs.slice(0, 8) : diffs.slice(0, diffs.length);
        const avg = used.reduce((s, v) => s + v, 0) / used.length;
        const index = avg * 0.96;
        const rounded = Math.round(index * 10) / 10;

        hiEl.textContent = rounded.toFixed(1);
        detailEl.textContent = `Based on ${used.length} best Score Differentials out of ${diffs.length} round(s).`;
        return rounded;
      }

      function computeCourseAndPlaying() {
        const indexVal = parseNum(
          document.getElementById("handicap-index").textContent
        );
        const chEl = document.getElementById("course-handicap");
        const phEl = document.getElementById("playing-handicap");
        const phDetail = document.getElementById("playing-handicap-detail");

        if (indexVal == null) {
          chEl.textContent = "–";
          phEl.textContent = "–";
          phDetail.textContent = "Calculate your Handicap Index first.";
          return;
        }

        const slope = parseNum(document.getElementById("course-slope").value);
        const rating = parseNum(document.getElementById("course-rating").value);
        const par = parseNum(document.getElementById("course-par").value);
        const allowance = parseNum(document.getElementById("allowance").value);

        if (slope == null || rating == null || par == null || slope <= 0) {
          chEl.textContent = "–";
          phEl.textContent = "–";
          phDetail.textContent = "Enter slope, rating and par for the course.";
          return;
        }

        const courseHcpRaw = indexVal * (slope / 113) + (rating - par);
        const courseHcp = Math.round(courseHcpRaw);
        chEl.textContent = courseHcp.toString();

        if (allowance == null) {
          phEl.textContent = "–";
          phDetail.textContent =
            "Enter a handicap allowance (%) to get Playing Handicap.";
          return;
        }

        const playingRaw = courseHcp * (allowance / 100);
        const playing = Math.round(playingRaw);
        phEl.textContent = playing.toString();
        phDetail.textContent = `Using ${allowance}% allowance from Course Handicap ${courseHcp}.`;
      }

      function clearAll() {
        document
          .querySelectorAll("#stroke-rows input, #stableford-rows input")
          .forEach((i) => (i.value = ""));
        document
          .querySelectorAll(".stroke-diff, .stab-diff")
          .forEach((c) => (c.textContent = "–"));
        document.getElementById("handicap-index").textContent = "–";
        document.getElementById("handicap-index-detail").textContent = "";
        document.getElementById("course-handicap").textContent = "–";
        document.getElementById("playing-handicap").textContent = "–";
        document.getElementById("playing-handicap-detail").textContent = "";
        document.getElementById("course-slope").value = "";
        document.getElementById("course-rating").value = "";
        document.getElementById("course-par").value = "";
        document.getElementById("allowance").value = "";
      }

      function initTabs() {
        const tabStroke = document.getElementById("tab-stroke");
        const tabStable = document.getElementById("tab-stableford");
        const panelStroke = document.getElementById("panel-stroke");
        const panelStable = document.getElementById("panel-stableford");

        tabStroke.addEventListener("click", () => {
          tabStroke.classList.add("bg-blue-600", "text-white");
          tabStroke.classList.remove("bg-white", "text-blue-600");
          tabStable.classList.remove("bg-blue-600", "text-white");
          tabStable.classList.add("bg-white", "text-blue-600");
          panelStroke.classList.remove("hidden");
          panelStable.classList.add("hidden");
        });

        tabStable.addEventListener("click", () => {
          tabStable.classList.add("bg-blue-600", "text-white");
          tabStable.classList.remove("bg-white", "text-blue-600");
          tabStroke.classList.remove("bg-blue-600", "text-white");
          tabStroke.classList.add("bg-white", "text-blue-600");
          panelStable.classList.remove("hidden");
          panelStroke.classList.add("hidden");
        });
      }

      document.addEventListener("DOMContentLoaded", () => {
        initRows();
        initTabs();
        document
          .getElementById("btn-calc-handicap")
          .addEventListener("click", () => {
            const hi = computeHandicapIndex();
            if (hi != null) {
              computeCourseAndPlaying();
            }
          });
        document
          .getElementById("btn-calc-course")
          .addEventListener("click", computeCourseAndPlaying);
        document
          .getElementById("btn-clear")
          .addEventListener("click", clearAll);
      });

