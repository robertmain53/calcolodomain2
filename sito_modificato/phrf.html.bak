    <script>
      // Utility: parse h:m:s to seconds
      function hmsToSeconds(h, m, s) {
        h = Number(h) || 0;
        m = Number(m) || 0;
        s = Number(s) || 0;
        return h * 3600 + m * 60 + s;
      }

      // Utility: seconds to h:mm:ss
      function secondsToHMS(totalSeconds) {
        if (!isFinite(totalSeconds)) return "";
        const sign = totalSeconds < 0 ? "-" : "";
        totalSeconds = Math.abs(Math.round(totalSeconds));
        const h = Math.floor(totalSeconds / 3600);
        const m = Math.floor((totalSeconds % 3600) / 60);
        const s = totalSeconds % 60;
        const pad = (n) => n.toString().padStart(2, "0");
        return sign + h + ":" + pad(m) + ":" + pad(s);
      }

      // Boat row management
      let boatIdCounter = 0;

      function createBoatRow(defaults = {}) {
        const tbody = document.getElementById("boats-tbody");
        const tr = document.createElement("tr");
        const id = boatIdCounter++;
        tr.dataset.boatId = id;

        tr.innerHTML = `
      <td class="border-t px-2 py-1">
        <input type="text" class="boat-name w-full border border-gray-300 rounded-md px-2 py-1 text-sm" placeholder="Boat name" value="${
          defaults.name || ""
        }">
      </td>
      <td class="border-t px-2 py-1">
        <input type="text" class="boat-sail w-full border border-gray-300 rounded-md px-2 py-1 text-sm" placeholder="Sail #" value="${
          defaults.sail || ""
        }">
      </td>
      <td class="border-t px-2 py-1">
        <input type="number" step="1" class="boat-rating w-24 border border-gray-300 rounded-md px-2 py-1 text-sm" placeholder="e.g. 150" value="${
          defaults.rating ?? ""
        }">
      </td>
      <td class="border-t px-2 py-1">
        <div class="grid grid-cols-3 gap-1">
          <input type="number" min="0" class="boat-h border border-gray-300 rounded-md px-1 py-1 text-xs" placeholder="h" value="${
            defaults.h ?? ""
          }">
          <input type="number" min="0" max="59" class="boat-m border border-gray-300 rounded-md px-1 py-1 text-xs" placeholder="m" value="${
            defaults.m ?? ""
          }">
          <input type="number" min="0" max="59" class="boat-s border border-gray-300 rounded-md px-1 py-1 text-xs" placeholder="s" value="${
            defaults.s ?? ""
          }">
        </div>
      </td>
      <td class="border-t px-2 py-1">
        <input type="number" step="0.0001" class="boat-tcf w-20 border border-gray-300 rounded-md px-2 py-1 text-xs" placeholder="auto" value="${
          defaults.tcf ?? ""
        }">
      </td>
      <td class="border-t px-2 py-1">
        <button type="button" class="remove-boat text-xs text-red-600 hover:underline">Remove</button>
      </td>
    `;

        tbody.appendChild(tr);
      }

      function initDefaultBoats() {
        if (document.getElementById("boats-tbody").children.length === 0) {
          createBoatRow({
            name: "Boat A",
            sail: "USA 1",
            rating: 90,
            h: 1,
            m: 5,
            s: 0,
          });
          createBoatRow({
            name: "Boat B",
            sail: "USA 2",
            rating: 150,
            h: 1,
            m: 10,
            s: 0,
          });
          createBoatRow();
        }
      }

      // Toggle TOT settings visibility
      function updateScoringUI() {
        const method = document.getElementById("scoring-method").value;
        const totSettings = document.getElementById("tot-settings");
        if (method === "tot") {
          totSettings.classList.remove("hidden");
        } else {
          totSettings.classList.add("hidden");
        }
      }

      // Compute TCF for a given rating
      function computeTCFForRating(R) {
        const formula = document.getElementById("tot-formula").value;
        const A = parseFloat(document.getElementById("tot-a").value);
        const B = parseFloat(document.getElementById("tot-b").value);
        const K = parseFloat(document.getElementById("tot-k").value);

        if (formula === "a-bR") {
          if (!isFinite(A) || !isFinite(B)) return null;
          const denom = B + R;
          if (denom === 0) return null;
          return A / denom;
        } else if (formula === "1-kr") {
          if (!isFinite(K)) return null;
          const denom = 1 + K * R;
          if (denom === 0) return null;
          return 1 / denom;
        }
        return null; // custom, handled per-boat
      }

      function calculatePHRF() {
        const errorEl = document.getElementById("error-msg");
        errorEl.textContent = "";
        const method = document.getElementById("scoring-method").value;
        const distance = parseFloat(
          document.getElementById("course-distance").value
        );
        if (!isFinite(distance) || distance <= 0) {
          errorEl.textContent =
            "Enter a valid course distance greater than zero.";
          return;
        }

        const boats = [];
        const rows = document.querySelectorAll("#boats-tbody tr");
        if (rows.length === 0) {
          errorEl.textContent = "Add at least one boat.";
          return;
        }

        for (const row of rows) {
          const name = row.querySelector(".boat-name").value.trim() || "Boat";
          const sail = row.querySelector(".boat-sail").value.trim();
          const ratingVal = row.querySelector(".boat-rating").value;
          const rating = ratingVal === "" ? NaN : parseFloat(ratingVal);
          const h = row.querySelector(".boat-h").value;
          const m = row.querySelector(".boat-m").value;
          const s = row.querySelector(".boat-s").value;
          const tcfInput = row.querySelector(".boat-tcf").value;

          if (!isFinite(rating)) {
            errorEl.textContent = "Each boat must have a valid PHRF rating.";
            return;
          }
          const elapsed = hmsToSeconds(h, m, s);
          if (!elapsed || elapsed <= 0) {
            errorEl.textContent =
              "Each boat must have a valid elapsed time (h:m:s).";
            return;
          }

          let tcf = null;
          if (method === "tot") {
            if (tcfInput !== "") {
              tcf = parseFloat(tcfInput);
              if (!isFinite(tcf) || tcf <= 0) {
                errorEl.textContent = "TCF values must be positive numbers.";
                return;
              }
            } else {
              tcf = computeTCFForRating(rating);
              if (!isFinite(tcf) || tcf <= 0) {
                errorEl.textContent =
                  "Unable to compute TCF from the selected formula and parameters.";
                return;
              }
            }
          }

          boats.push({ name, sail, rating, elapsed, tcf });
        }

        // Compute corrected times
        const D = distance;
        for (const boat of boats) {
          if (method === "tod") {
            const allowance = boat.rating * D;
            boat.corrected = boat.elapsed - allowance;
          } else {
            boat.corrected = boat.elapsed * boat.tcf;
          }
        }

        // Sort by corrected time ascending
        boats.sort((a, b) => a.corrected - b.corrected);

        // Render results
        const resultsTbody = document.getElementById("results-tbody");
        resultsTbody.innerHTML = "";
        const bestCorrected = boats[0].corrected;

        boats.forEach((boat, index) => {
          const tr = document.createElement("tr");
          tr.className = index === 0 ? "bg-green-50" : "";
          const delta = boat.corrected - bestCorrected;
          tr.innerHTML = `
        <td class="border-t px-2 py-1">${index + 1}</td>
        <td class="border-t px-2 py-1">${boat.name}</td>
        <td class="border-t px-2 py-1">${boat.sail || "&mdash;"}</td>
        <td class="border-t px-2 py-1">${boat.rating}</td>
        <td class="border-t px-2 py-1">${secondsToHMS(boat.elapsed)}</td>
        <td class="border-t px-2 py-1 font-semibold">${secondsToHMS(
          boat.corrected
        )}</td>
        <td class="border-t px-2 py-1">${
          index === 0 ? "—" : secondsToHMS(delta)
        }</td>
      `;
          resultsTbody.appendChild(tr);
        });

        const note = document.getElementById("results-note");
        if (method === "tod") {
          note.textContent =
            "Scoring method: Time-on-Distance (Corrected = Elapsed − Rating × Distance). Lower corrected time wins.";
        } else {
          note.textContent =
            "Scoring method: Time-on-Time (Corrected = Elapsed × TCF). Lower corrected time wins.";
        }

        document.getElementById("results-section").classList.remove("hidden");
      }

      function resetPHRF() {
        document.getElementById("course-distance").value = 5;
        document.getElementById("distance-unit").value = "nm";
        document.getElementById("scoring-method").value = "tod";
        updateScoringUI();
        document.getElementById("boats-tbody").innerHTML = "";
        initDefaultBoats();
        document.getElementById("results-section").classList.add("hidden");
        document.getElementById("error-msg").textContent = "";
      }

      document.addEventListener("DOMContentLoaded", function () {
        initDefaultBoats();
        updateScoringUI();

        document
          .getElementById("add-boat-btn")
          .addEventListener("click", () => createBoatRow());
        document
          .getElementById("scoring-method")
          .addEventListener("change", updateScoringUI);
        document
          .getElementById("calculate-btn")
          .addEventListener("click", calculatePHRF);
        document
          .getElementById("reset-btn")
          .addEventListener("click", resetPHRF);

        document
          .getElementById("boats-tbody")
          .addEventListener("click", (e) => {
            if (e.target.classList.contains("remove-boat")) {
              const row = e.target.closest("tr");
              row.parentNode.removeChild(row);
            }
          });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
