      // High-precision stopwatch using performance.now()
      (function () {
        const displayHours = document.getElementById("hours");
        const displayMinutes = document.getElementById("minutes");
        const displaySeconds = document.getElementById("seconds");
        const displayMs = document.getElementById("milliseconds");

        const startStopBtn = document.getElementById("start-stop-btn");
        const lapBtn = document.getElementById("lap-btn");
        const resetBtn = document.getElementById("reset-btn");
        const togglePrecisionBtn = document.getElementById(
          "toggle-precision-btn"
        );
        const lapsTbody = document.getElementById("laps-tbody");
        const exportLapsBtn = document.getElementById("export-laps-btn");
        const clearLapsBtn = document.getElementById("clear-laps-btn");
        const lapSummary = document.getElementById("lap-summary");

        let running = false;
        let startTime = 0; // performance.now() when last started
        let accumulated = 0; // ms accumulated while previously running
        let rafId = null;
        let laps = [];
        let showMs = true;

        function formatTime(ms, withMs = true) {
          ms = Math.max(0, ms);
          const totalSeconds = ms / 1000;
          const hours = Math.floor(totalSeconds / 3600);
          const minutes = Math.floor((totalSeconds % 3600) / 60);
          const seconds = Math.floor(totalSeconds % 60);
          const milliseconds = Math.floor(ms % 1000);

          const hh = String(hours).padStart(2, "0");
          const mm = String(minutes).padStart(2, "0");
          const ss = String(seconds).padStart(2, "0");
          const mmm = String(milliseconds).padStart(3, "0");

          return withMs ? `${hh}:${mm}:${ss}.${mmm}` : `${hh}:${mm}:${ss}`;
        }

        function formatSeconds(ms) {
          return (ms / 1000).toFixed(3);
        }

        function updateDisplay(ms) {
          ms = Math.max(0, ms);
          const totalSeconds = ms / 1000;
          const hours = Math.floor(totalSeconds / 3600);
          const minutes = Math.floor((totalSeconds % 3600) / 60);
          const seconds = Math.floor(totalSeconds % 60);
          const milliseconds = Math.floor(ms % 1000);

          displayHours.textContent = String(hours).padStart(2, "0");
          displayMinutes.textContent = String(minutes).padStart(2, "0");
          displaySeconds.textContent = String(seconds).padStart(2, "0");
          displayMs.textContent = String(milliseconds).padStart(3, "0");
          displayMs.style.display = showMs ? "inline-block" : "none";
          const sep = displaySeconds.nextSibling;
          if (sep && sep.nodeType === Node.TEXT_NODE) {
            // keep the dot even if ms hidden
          }
        }

        function getElapsedMs() {
          if (!running) return accumulated;
          return accumulated + (performance.now() - startTime);
        }

        function tick() {
          updateDisplay(getElapsedMs());
          if (running) {
            rafId = requestAnimationFrame(tick);
          }
        }

        function start() {
          if (running) return;
          running = true;
          startTime = performance.now();
          startStopBtn.textContent = "Stop";
          startStopBtn.classList.remove("bg-green-600", "hover:bg-green-700");
          startStopBtn.classList.add("bg-red-600", "hover:bg-red-700");
          lapBtn.disabled = false;
          resetBtn.disabled = false;
          rafId = requestAnimationFrame(tick);
        }

        function stop() {
          if (!running) return;
          running = false;
          accumulated = getElapsedMs();
          startStopBtn.textContent = "Start";
          startStopBtn.classList.remove("bg-red-600", "hover:bg-red-700");
          startStopBtn.classList.add("bg-green-600", "hover:bg-green-700");
          if (rafId) cancelAnimationFrame(rafId);
          updateDisplay(accumulated);
        }

        function reset() {
          running = false;
          accumulated = 0;
          startTime = 0;
          if (rafId) cancelAnimationFrame(rafId);
          updateDisplay(0);
          startStopBtn.textContent = "Start";
          startStopBtn.classList.remove("bg-red-600", "hover:bg-red-700");
          startStopBtn.classList.add("bg-green-600", "hover:bg-green-700");
          lapBtn.disabled = true;
          resetBtn.disabled = true;
          laps = [];
          renderLaps();
        }

        function addLap() {
          const current = getElapsedMs();
          const lastTotal = laps.length ? laps[laps.length - 1].totalMs : 0;
          const lapMs = current - lastTotal;
          laps.push({ index: laps.length + 1, lapMs, totalMs: current });
          renderLaps();
        }

        function renderLaps() {
          lapsTbody.innerHTML = "";
          if (!laps.length) {
            const row = document.createElement("tr");
            row.className = "text-gray-400";
            row.innerHTML = `<td colspan="5" class="px-2 py-3 text-center">No laps yet. Press <span class="font-semibold">Lap</span> while running to record.</td>`;
            lapsTbody.appendChild(row);
            exportLapsBtn.disabled = true;
            clearLapsBtn.disabled = true;
            lapSummary.classList.add("hidden");
            lapSummary.textContent = "";
            return;
          }

          // Determine fastest and slowest laps
          let fastest = laps[0].lapMs;
          let slowest = laps[0].lapMs;
          laps.forEach((l) => {
            if (l.lapMs < fastest) fastest = l.lapMs;
            if (l.lapMs > slowest) slowest = l.lapMs;
          });

          laps.forEach((lap) => {
            const tr = document.createElement("tr");
            tr.className = "lap-row";
            if (lap.lapMs === fastest) tr.classList.add("lap-row-fastest");
            if (lap.lapMs === slowest) tr.classList.add("lap-row-slowest");

            tr.innerHTML = `
          <td class="px-2 py-1 text-gray-700">${lap.index}</td>
          <td class="px-2 py-1 font-mono text-gray-900">${formatTime(
            lap.lapMs,
            true
          )}</td>
          <td class="px-2 py-1 font-mono text-gray-900">${formatTime(
            lap.totalMs,
            true
          )}</td>
          <td class="px-2 py-1 font-mono text-gray-700">${formatSeconds(
            lap.lapMs
          )}</td>
          <td class="px-2 py-1 font-mono text-gray-700">${formatSeconds(
            lap.totalMs
          )}</td>
        `;
            lapsTbody.appendChild(tr);
          });

          exportLapsBtn.disabled = false;
          clearLapsBtn.disabled = false;

          const totalTime = laps[laps.length - 1].totalMs;
          const avgLap = totalTime / laps.length;
          lapSummary.textContent = `Laps: ${
            laps.length
          } · Total time: ${formatTime(
            totalTime,
            true
          )} · Average lap: ${formatTime(avgLap, true)}`;
          lapSummary.classList.remove("hidden");
        }

        function exportLapsToClipboard() {
          if (!laps.length) return;
          let text =
            "Lap\tLap time (h:m:s.ms)\tTotal time (h:m:s.ms)\tLap (s)\tTotal (s)\n";
          laps.forEach((lap) => {
            text += `${lap.index}\t${formatTime(lap.lapMs, true)}\t${formatTime(
              lap.totalMs,
              true
            )}\t${formatSeconds(lap.lapMs)}\t${formatSeconds(lap.totalMs)}\n`;
          });
          navigator.clipboard
            .writeText(text)
            .then(() => {
              exportLapsBtn.textContent = "Copied!";
              setTimeout(() => {
                exportLapsBtn.textContent = "Copy table";
              }, 1500);
            })
            .catch(() => {
              alert("Unable to copy to clipboard. Please copy manually.");
            });
        }

        function clearLaps() {
          laps = [];
          renderLaps();
        }

        function togglePrecision() {
          showMs = !showMs;
          displayMs.style.display = showMs ? "inline-block" : "none";
          togglePrecisionBtn.textContent = showMs
            ? "Hide ms precision"
            : "Show ms precision";
        }

        // Event listeners
        startStopBtn.addEventListener("click", () => {
          running ? stop() : start();
        });

        lapBtn.addEventListener("click", () => {
          if (!running) return;
          addLap();
        });

        resetBtn.addEventListener("click", reset);
        exportLapsBtn.addEventListener("click", exportLapsToClipboard);
        clearLapsBtn.addEventListener("click", clearLaps);
        togglePrecisionBtn.addEventListener("click", togglePrecision);
        togglePrecisionBtn.textContent = "Hide ms precision";

        // Keyboard shortcuts
        document.addEventListener("keydown", (e) => {
          if (e.target && ["INPUT", "TEXTAREA"].includes(e.target.tagName))
            return;
          if (e.code === "Space") {
            e.preventDefault();
            running ? stop() : start();
          } else if (e.key === "l" || e.key === "L") {
            if (running) addLap();
          } else if (e.key === "r" || e.key === "R") {
            reset();
          }
        });

        // Initialize display
        updateDisplay(0);
      })();

      // Time conversion helper
      (function () {
        const hEl = document.getElementById("conv-hours");
        const mEl = document.getElementById("conv-minutes");
        const sEl = document.getElementById("conv-seconds");
        const msEl = document.getElementById("conv-ms");
        const totalSecondsEl = document.getElementById("conv-total-seconds");
        const decimalHoursEl = document.getElementById("conv-decimal-hours");
        const resultEl = document.getElementById("conv-result");

        function hmsToSeconds() {
          const h = parseFloat(hEl.value) || 0;
          const m = parseFloat(mEl.value) || 0;
          const s = parseFloat(sEl.value) || 0;
          const ms = parseFloat(msEl.value) || 0;
          const totalSeconds = h * 3600 + m * 60 + s + ms / 1000;
          const decimalHours = totalSeconds / 3600;
          totalSecondsEl.value = totalSeconds.toFixed(3);
          decimalHoursEl.value = decimalHours.toFixed(6);
          resultEl.textContent = `Total seconds: ${totalSeconds.toFixed(
            3
          )}, Decimal hours: ${decimalHours.toFixed(6)}`;
        }

        function secondsOrHoursToHms() {
          let totalSeconds = parseFloat(totalSecondsEl.value);
          const decimalHours = parseFloat(decimalHoursEl.value);

          if (isNaN(totalSeconds) && isNaN(decimalHours)) {
            resultEl.textContent =
              "Enter total seconds or decimal hours to convert.";
            return;
          }

          if (isNaN(totalSeconds)) {
            totalSeconds = decimalHours * 3600;
          }

          const hours = Math.floor(totalSeconds / 3600);
          const minutes = Math.floor((totalSeconds % 3600) / 60);
          const seconds = Math.floor(totalSeconds % 60);
          const ms = Math.round(
            (totalSeconds - Math.floor(totalSeconds)) * 1000
          );

          hEl.value = hours;
          mEl.value = minutes;
          sEl.value = seconds;
          msEl.value = ms;

          const dh = totalSeconds / 3600;
          decimalHoursEl.value = dh.toFixed(6);
          totalSecondsEl.value = totalSeconds.toFixed(3);

          resultEl.textContent = `Converted to ${hours} h ${minutes} min ${seconds} s ${ms} ms`;
        }

        document
          .getElementById("convert-from-hms-btn")
          .addEventListener("click", hmsToSeconds);
        document
          .getElementById("convert-from-seconds-btn")
          .addEventListener("click", secondsOrHoursToHms);
      })();

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
