    <script>
      // Standard normal PDF
      function normPDF(x) {
        return Math.exp(-0.5 * x * x) / Math.sqrt(2 * Math.PI);
      }

      // Standard normal CDF (Abramowitz-Stegun approximation)
      function normCDF(x) {
        var sign = x < 0 ? -1 : 1;
        x = Math.abs(x) / Math.sqrt(2);
        var t = 1 / (1 + 0.3275911 * x);
        var a1 = 0.254829592,
          a2 = -0.284496736,
          a3 = 1.421413741,
          a4 = -1.453152027,
          a5 = 1.061405429;
        var erf =
          1 -
          ((((a5 * t + a4) * t + a3) * t + a2) * t + a1) * t * Math.exp(-x * x);
        return 0.5 * (1 + sign * erf);
      }

      function formatNumber(x, decimals) {
        if (!isFinite(x)) return "–";
        return x.toFixed(decimals);
      }

      function calculateCurrencyOption() {
        const errorEl = document.getElementById("errorMsg");
        errorEl.classList.add("hidden");
        errorEl.textContent = "";

        const isCall = document.getElementById("callOption").checked;

        const S = parseFloat(document.getElementById("spot").value);
        const K = parseFloat(document.getElementById("strike").value);
        let T = parseFloat(document.getElementById("timeToMaturity").value);
        const timeUnit = document.getElementById("timeUnit").value;
        const rdPct = parseFloat(document.getElementById("domesticRate").value);
        const rfPct = parseFloat(document.getElementById("foreignRate").value);
        const volPct = parseFloat(document.getElementById("volatility").value);
        const notional = parseFloat(document.getElementById("notional").value);

        if (!(S > 0) || !(K > 0) || !(T > 0) || !(volPct > 0)) {
          errorEl.textContent =
            "Please enter positive values for spot, strike, time to maturity, and volatility.";
          errorEl.classList.remove("hidden");
          return;
        }

        // Convert time to years
        if (timeUnit === "months") {
          T = T / 12.0;
        } else if (timeUnit === "days") {
          T = T / 365.0;
        }

        const rd = rdPct / 100.0;
        const rf = rfPct / 100.0;
        const sigma = volPct / 100.0;

        const sqrtT = Math.sqrt(T);
        const d1 =
          (Math.log(S / K) + (rd - rf + 0.5 * sigma * sigma) * T) /
          (sigma * sqrtT);
        const d2 = d1 - sigma * sqrtT;

        const Nd1 = normCDF(isCall ? d1 : -d1);
        const Nd2 = normCDF(isCall ? d2 : -d2);
        const Nd1Sign = normCDF(d1);
        const Nd2Sign = normCDF(d2);

        const discForeign = Math.exp(-rf * T);
        const discDomestic = Math.exp(-rd * T);

        let price;
        if (isCall) {
          price = S * discForeign * Nd1Sign - K * discDomestic * Nd2Sign;
        } else {
          price =
            K * discDomestic * normCDF(-d2) - S * discForeign * normCDF(-d1);
        }

        // Greeks
        const pdf_d1 = normPDF(d1);
        const delta = isCall
          ? discForeign * Nd1Sign
          : -discForeign * normCDF(-d1);
        const gamma = (discForeign * pdf_d1) / (S * sigma * sqrtT);
        const vega = (S * discForeign * pdf_d1 * sqrtT) / 100.0; // per 1% change
        const thetaAnnual =
          -(S * discForeign * pdf_d1 * sigma) / (2 * sqrtT) -
          (isCall
            ? rd * K * discDomestic * Nd2Sign - rf * S * discForeign * Nd1Sign
            : -rd * K * discDomestic * normCDF(-d2) +
              rf * S * discForeign * normCDF(-d1));
        const thetaDaily = thetaAnnual / 365.0;
        const rhoDomestic =
          (isCall
            ? K * T * discDomestic * Nd2Sign
            : -K * T * discDomestic * normCDF(-d2)) / 100.0; // per 1% change
        const rhoForeign =
          (isCall
            ? -S * T * discForeign * Nd1Sign
            : S * T * discForeign * normCDF(-d1)) / 100.0; // per 1% change

        document.getElementById("premiumResult").textContent = formatNumber(
          price,
          6
        );

        if (notional > 0) {
          const totalPremium = price * notional;
          document.getElementById("totalPremiumResult").textContent =
            formatNumber(totalPremium, 2);
        } else {
          document.getElementById("totalPremiumResult").textContent = "–";
        }

        document.getElementById("deltaResult").textContent = formatNumber(
          delta,
          6
        );
        document.getElementById("gammaResult").textContent = formatNumber(
          gamma,
          6
        );
        document.getElementById("vegaResult").textContent = formatNumber(
          vega,
          6
        );
        document.getElementById("thetaResult").textContent = formatNumber(
          thetaDaily,
          6
        );
        document.getElementById("rhoDomesticResult").textContent = formatNumber(
          rhoDomestic,
          6
        );
        document.getElementById("rhoForeignResult").textContent = formatNumber(
          rhoForeign,
          6
        );

        document.getElementById("calcDetails").textContent =
          "Model: Garman–Kohlhagen. d1 = " +
          formatNumber(d1, 4) +
          ", d2 = " +
          formatNumber(d2, 4) +
          ", T (years) = " +
          formatNumber(T, 4) +
          ".";
      }

      document
        .getElementById("calculateBtn")
        .addEventListener("click", calculateCurrencyOption);

      document
        .getElementById("resetBtn")
        .addEventListener("click", function () {
          document.getElementById("callOption").checked = true;
          document.getElementById("putOption").checked = false;
          document.getElementById("spot").value = 1.1;
          document.getElementById("strike").value = 1.1;
          document.getElementById("timeToMaturity").value = 0.5;
          document.getElementById("timeUnit").value = "years";
          document.getElementById("domesticRate").value = 3.0;
          document.getElementById("foreignRate").value = 1.0;
          document.getElementById("volatility").value = 10.0;
          document.getElementById("notional").value = "";
          document.getElementById("premiumResult").textContent = "–";
          document.getElementById("totalPremiumResult").textContent = "–";
          document.getElementById("deltaResult").textContent = "–";
          document.getElementById("gammaResult").textContent = "–";
          document.getElementById("vegaResult").textContent = "–";
          document.getElementById("thetaResult").textContent = "–";
          document.getElementById("rhoDomesticResult").textContent = "–";
          document.getElementById("rhoForeignResult").textContent = "–";
          document.getElementById("calcDetails").textContent = "";
          const errorEl = document.getElementById("errorMsg");
          errorEl.classList.add("hidden");
          errorEl.textContent = "";
        });

      // Auto-calc on load with defaults
      window.addEventListener("load", calculateCurrencyOption);
    </script>

