    <script>
      // IRS-like sample rates for demonstration; always verify against official IRS notices.
      const IRS_RATES = {
        2025: 0.69,
        2024: 0.67,
        2023: 0.655,
        2022: 0.625, // using higher mid-year rate for simplicity
      };

      const taxYearEl = document.getElementById("taxYear");
      const ratePerMileEl = document.getElementById("ratePerMile");
      const startOdoEl = document.getElementById("startOdo");
      const endOdoEl = document.getElementById("endOdo");
      const tripMilesEl = document.getElementById("tripMiles");
      const tripDateEl = document.getElementById("tripDate");
      const tripPurposeEl = document.getElementById("tripPurpose");
      const tripNotesEl = document.getElementById("tripNotes");
      const addTripBtn = document.getElementById("addTripBtn");
      const clearTripsBtn = document.getElementById("clearTripsBtn");
      const exportCsvBtn = document.getElementById("exportCsvBtn");
      const tripTableBody = document.getElementById("tripTableBody");
      const totalBusinessMilesEl =
        document.getElementById("totalBusinessMiles");
      const totalNonBusinessMilesEl = document.getElementById(
        "totalNonBusinessMiles"
      );
      const totalReimbursementEl =
        document.getElementById("totalReimbursement");

      let trips = [];

      function updateRateFromYear() {
        const year = taxYearEl.value;
        if (IRS_RATES[year] !== undefined) {
          ratePerMileEl.value = IRS_RATES[year].toFixed(3);
          ratePerMileEl.readOnly = false;
        } else {
          // Custom year
          ratePerMileEl.readOnly = false;
        }
        recalcTotals();
      }

      function calcTripMiles() {
        const start = parseFloat(startOdoEl.value);
        const end = parseFloat(endOdoEl.value);
        if (!isNaN(start) && !isNaN(end) && end >= start) {
          const miles = end - start;
          tripMilesEl.value = miles.toFixed(1);
        } else {
          tripMilesEl.value = "";
        }
      }

      function renderTable() {
        tripTableBody.innerHTML = "";
        if (trips.length === 0) {
          const row = document.createElement("tr");
          row.className = "text-gray-500";
          row.innerHTML =
            '<td colspan="8" class="px-3 py-4 text-center text-xs">No trips added yet. Enter details above and click “Add trip to log”.</td>';
          tripTableBody.appendChild(row);
          return;
        }

        trips.forEach((trip, index) => {
          const row = document.createElement("tr");
          row.className = index % 2 === 0 ? "bg-white" : "bg-gray-50";
          row.innerHTML = `
        <td class="px-2 py-1 border-t border-gray-100">${trip.date || ""}</td>
        <td class="px-2 py-1 border-t border-gray-100">${
          trip.purpose || ""
        }</td>
        <td class="px-2 py-1 border-t border-gray-100 text-right">${
          trip.startOdo !== null ? trip.startOdo.toFixed(1) : ""
        }</td>
        <td class="px-2 py-1 border-t border-gray-100 text-right">${
          trip.endOdo !== null ? trip.endOdo.toFixed(1) : ""
        }</td>
        <td class="px-2 py-1 border-t border-gray-100 text-right">${trip.miles.toFixed(
          1
        )}</td>
        <td class="px-2 py-1 border-t border-gray-100">
          <span class="inline-flex items-center px-2 py-0.5 rounded-full text-[11px] ${
            trip.type === "business"
              ? "bg-green-100 text-green-800"
              : trip.type === "commuting"
              ? "bg-yellow-100 text-yellow-800"
              : "bg-gray-100 text-gray-800"
          }">
            ${trip.type.charAt(0).toUpperCase() + trip.type.slice(1)}
          </span>
        </td>
        <td class="px-2 py-1 border-t border-gray-100">${trip.notes || ""}</td>
        <td class="px-2 py-1 border-t border-gray-100 text-center">
          <button type="button" data-index="${index}" class="text-red-600 hover:text-red-800 text-xs remove-trip-btn">✕</button>
        </td>
      `;
          tripTableBody.appendChild(row);
        });

        // Attach remove handlers
        tripTableBody.querySelectorAll(".remove-trip-btn").forEach((btn) => {
          btn.addEventListener("click", (e) => {
            const idx = parseInt(
              e.currentTarget.getAttribute("data-index"),
              10
            );
            if (!isNaN(idx)) {
              trips.splice(idx, 1);
              recalcTotals();
              renderTable();
            }
          });
        });
      }

      function recalcTotals() {
        let businessMiles = 0;
        let nonBusinessMiles = 0;
        trips.forEach((trip) => {
          if (trip.type === "business") {
            businessMiles += trip.miles;
          } else {
            nonBusinessMiles += trip.miles;
          }
        });

        const rate = parseFloat(ratePerMileEl.value) || 0;
        const reimbursement = businessMiles * rate;

        totalBusinessMilesEl.textContent = businessMiles.toFixed(1);
        totalNonBusinessMilesEl.textContent = nonBusinessMiles.toFixed(1);
        totalReimbursementEl.textContent = reimbursement.toFixed(2);
      }

      function addTrip() {
        const miles = parseFloat(tripMilesEl.value);
        const start =
          startOdoEl.value === "" ? null : parseFloat(startOdoEl.value);
        const end = endOdoEl.value === "" ? null : parseFloat(endOdoEl.value);
        const type =
          document.querySelector('input[name="tripType"]:checked')?.value ||
          "business";

        if (isNaN(miles) || miles <= 0) {
          alert(
            "Please enter valid start and end odometer readings so miles can be calculated."
          );
          return;
        }

        const trip = {
          date: tripDateEl.value,
          purpose: tripPurposeEl.value.trim(),
          startOdo: start,
          endOdo: end,
          miles: miles,
          type: type,
          notes: tripNotesEl.value.trim(),
        };

        trips.push(trip);
        recalcTotals();
        renderTable();
        clearTripInputs();
      }

      function clearTripInputs() {
        // Keep date and purpose to speed up repeated entries if desired
        startOdoEl.value = "";
        endOdoEl.value = "";
        tripMilesEl.value = "";
        tripNotesEl.value = "";
      }

      function clearAllTrips() {
        if (!confirm("Clear all trips from the log?")) return;
        trips = [];
        recalcTotals();
        renderTable();
      }

      function exportCsv() {
        if (trips.length === 0) {
          alert("No trips to export.");
          return;
        }
        const header = [
          "Date",
          "Purpose",
          "StartOdometer",
          "EndOdometer",
          "Miles",
          "Type",
          "Notes",
        ];
        const rows = trips.map((t) => [
          t.date || "",
          t.purpose || "",
          t.startOdo !== null ? t.startOdo.toFixed(1) : "",
          t.endOdo !== null ? t.endOdo.toFixed(1) : "",
          t.miles.toFixed(1),
          t.type,
          t.notes || "",
        ]);

        const csvContent = [header, ...rows]
          .map((r) =>
            r
              .map((field) => {
                const f = String(field).replace(/"/g, '""');
                return `"${f}"`;
              })
              .join(",")
          )
          .join("\r\n");

        const blob = new Blob([csvContent], {
          type: "text/csv;charset=utf-8;",
        });
        const url = URL.createObjectURL(blob);
        const a = document.createElement("a");
        const year = taxYearEl.value === "custom" ? "" : taxYearEl.value;
        a.href = url;
        a.download = `business-mileage-log${year ? "-" + year : ""}.csv`;
        document.body.appendChild(a);
        a.click();
        document.body.removeChild(a);
        URL.revokeObjectURL(url);
      }

      // Event listeners
      taxYearEl.addEventListener("change", updateRateFromYear);
      startOdoEl.addEventListener("input", calcTripMiles);
      endOdoEl.addEventListener("input", calcTripMiles);
      ratePerMileEl.addEventListener("input", recalcTotals);
      addTripBtn.addEventListener("click", addTrip);
      clearTripsBtn.addEventListener("click", clearAllTrips);
      exportCsvBtn.addEventListener("click", exportCsv);

      // Initialize
      updateRateFromYear();
      renderTable();

      // Simple FAQ toggle (in addition to global script_faq if present)
      document
        .querySelectorAll("#faq-container .faq-question")
        .forEach((btn) => {
          btn.addEventListener("click", () => {
            const answer = btn.parentElement.querySelector(".faq-answer");
            const icon = btn.querySelector("span:last-child");
            const isHidden = answer.classList.contains("hidden");
            document
              .querySelectorAll("#faq-container .faq-answer")
              .forEach((a) => a.classList.add("hidden"));
            document
              .querySelectorAll("#faq-container .faq-question span:last-child")
              .forEach((i) => (i.textContent = "+"));
            if (isHidden) {
              answer.classList.remove("hidden");
              icon.textContent = "−";
            }
          });
        });

      // Mobile menu toggle
      const mobileToggle = document.getElementById("mobile-menu-toggle");
      const mobileMenu = document.getElementById("mobile-menu");
      if (mobileToggle && mobileMenu) {
        mobileToggle.addEventListener("click", () => {
          const isHidden = mobileMenu.classList.contains("hidden");
          mobileMenu.classList.toggle("hidden", !isHidden);
          mobileToggle.setAttribute(
            "aria-expanded",
            isHidden ? "true" : "false"
          );
        });
      }

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
