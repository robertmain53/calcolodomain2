    <script>
      // Species presets
      const speciesPresets = {
        SPF_No2: { Fb: 875, Fc: 1150, E: 1400000 },
        DF_No2: { Fb: 900, Fc: 1350, E: 1600000 },
        GL24: { Fb: 2400, Fc: 1600, E: 1800000 },
      };

      document
        .getElementById("species")
        .addEventListener("change", function () {
          const val = this.value;
          if (speciesPresets[val]) {
            document.getElementById("Fb").value = speciesPresets[val].Fb;
            document.getElementById("Fc").value = speciesPresets[val].Fc;
            document.getElementById("E").value = speciesPresets[val].E;
          }
        });

      function setStatusBadge(el, ratio) {
        el.textContent = ratio === null ? "–" : ratio <= 1 ? "OK" : "NG";
        el.classList.remove("result-badge-ok", "result-badge-ng");
        el.classList.add(
          ratio !== null && ratio <= 1 ? "result-badge-ok" : "result-badge-ng"
        );
      }

      document
        .getElementById("calc-btn")
        .addEventListener("click", function () {
          const warn = document.getElementById("calc-warning");
          warn.classList.add("hidden");
          warn.textContent = "";

          const b = parseFloat(document.getElementById("b").value);
          const d = parseFloat(document.getElementById("d").value);
          const Lft = parseFloat(document.getElementById("length").value);
          const ky = parseFloat(document.getElementById("k-major").value);
          const kx = parseFloat(document.getElementById("k-minor").value);
          const Fb = parseFloat(document.getElementById("Fb").value);
          const Fc = parseFloat(document.getElementById("Fc").value);
          const E = parseFloat(document.getElementById("E").value);
          const Pk = parseFloat(document.getElementById("P").value);
          const My_kipin = parseFloat(document.getElementById("My").value);
          const Mx_kipin = parseFloat(document.getElementById("Mx").value);
          let Cp = parseFloat(document.getElementById("Cp").value);
          const Cl = parseFloat(document.getElementById("Cb").value);

          if (
            [b, d, Lft, ky, kx, Fb, Fc, E].some((v) => !isFinite(v) || v <= 0)
          ) {
            warn.textContent =
              "Please enter positive numeric values for geometry and material.";
            warn.classList.remove("hidden");
            return;
          }

          const L = Lft * 12; // in
          const A = b * d;
          const Iy = (b * Math.pow(d, 3)) / 12;
          const Ix = (d * Math.pow(b, 3)) / 12;
          const ry = Math.sqrt(Iy / A);
          const rx = Math.sqrt(Ix / A);
          const slenderY = (ky * L) / ry;
          const slenderX = (kx * L) / rx;

          // Auto-estimate Cp if left as 1.0
          if (!isFinite(Cp) || Cp <= 0) Cp = 1.0;
          if (Math.abs(Cp - 1.0) < 1e-3) {
            const slender = Math.max(slenderY, slenderX);
            // crude Euler-based reduction: FcE = (pi^2 * E) / ( (kL/r)^2 * 12^2 ) in psi
            const FcE = (Math.PI * Math.PI * E) / Math.pow(slender, 2);
            const FcAdj = Math.min(Fc, FcE);
            Cp = FcAdj / Fc;
            if (Cp > 1) Cp = 1;
            if (Cp < 0.1) Cp = 0.1;
            document.getElementById("Cp").value = Cp.toFixed(3);
          }

          const FcAdj = Fc * Cp;
          const FbYAdj = Fb * Cl;
          const FbXAdj = Fb * Cl;

          const P = Pk * 1000; // kips -> lbs
          const My = My_kipin * 1000; // kip-in -> lb-in
          const Mx = Mx_kipin * 1000;

          const sigc = P / A;
          const cy = d / 2;
          const cx = b / 2;
          const sigby = Iy === 0 ? 0 : (My * cy) / Iy;
          const sigbx = Ix === 0 ? 0 : (Mx * cx) / Ix;

          const axialRatio = FcAdj > 0 ? sigc / FcAdj : null;
          const bendRatioY = FbYAdj > 0 ? Math.abs(sigby) / FbYAdj : 0;
          const bendRatioX = FbXAdj > 0 ? Math.abs(sigbx) / FbXAdj : 0;
          const bendingRatio = Math.max(bendRatioY, bendRatioX);

          let comboRatio = null;
          if (FcAdj > 0 && FbYAdj > 0 && FbXAdj > 0) {
            comboRatio =
              sigc / FcAdj +
              Math.abs(sigby) / FbYAdj +
              Math.abs(sigbx) / FbXAdj;
          }

          // Update summary
          document.getElementById("axial-ratio").textContent =
            axialRatio !== null ? axialRatio.toFixed(2) : "–";
          document.getElementById("bending-ratio").textContent = isFinite(
            bendingRatio
          )
            ? bendingRatio.toFixed(2)
            : "–";
          document.getElementById("combo-ratio").textContent =
            comboRatio !== null ? comboRatio.toFixed(2) : "–";

          setStatusBadge(document.getElementById("axial-status"), axialRatio);
          setStatusBadge(
            document.getElementById("bending-status"),
            isFinite(bendingRatio) ? bendingRatio : null
          );
          setStatusBadge(document.getElementById("combo-status"), comboRatio);

          // Detailed results
          document.getElementById("res-A").textContent = A.toFixed(2);
          document.getElementById("res-Iy").textContent = Iy.toFixed(2);
          document.getElementById("res-Ix").textContent = Ix.toFixed(2);
          document.getElementById("res-ry").textContent = ry.toFixed(2);
          document.getElementById("res-rx").textContent = rx.toFixed(2);
          document.getElementById("res-slender-y").textContent =
            slenderY.toFixed(1);
          document.getElementById("res-slender-x").textContent =
            slenderX.toFixed(1);
          document.getElementById("res-sigc").textContent = isFinite(sigc)
            ? sigc.toFixed(0)
            : "–";
          document.getElementById("res-sigby").textContent = isFinite(sigby)
            ? sigby.toFixed(0)
            : "–";
          document.getElementById("res-sigbx").textContent = isFinite(sigbx)
            ? sigbx.toFixed(0)
            : "–";
          document.getElementById("res-FcAdj").textContent = FcAdj.toFixed(0);
          document.getElementById("res-FbYAdj").textContent = FbYAdj.toFixed(0);
          document.getElementById("res-FbXAdj").textContent = FbXAdj.toFixed(0);

          // Warnings
          const msgs = [];
          if (slenderY > 50 || slenderX > 50) {
            msgs.push(
              "Slenderness kL/r exceeds 50 on at least one axis; check NDS slenderness limits."
            );
          }
          if (comboRatio !== null && comboRatio > 1.0) {
            msgs.push(
              "Combined interaction ratio exceeds 1.0. Consider increasing section size, shortening unbraced length, or reducing loads."
            );
          }
          if (msgs.length) {
            warn.textContent = msgs.join(" ");
            warn.classList.remove("hidden");
          }
        });

      document
        .querySelectorAll("#faq-container .faq-question")
        .forEach((btn) => {
          btn.addEventListener("click", () => {
            const ans = btn.parentElement.querySelector(".faq-answer");
            const icon = btn.querySelector("span:last-child");
            const isHidden = ans.classList.contains("hidden");
            document
              .querySelectorAll("#faq-container .faq-answer")
              .forEach((a) => a.classList.add("hidden"));
            document
              .querySelectorAll("#faq-container .faq-question span:last-child")
              .forEach((i) => (i.textContent = "+"));
            if (isHidden) {
              ans.classList.remove("hidden");
              icon.textContent = "−";
            }
          });
        });

      // Mobile menu
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const expanded = this.getAttribute("aria-expanded") === "true";
          this.setAttribute("aria-expanded", String(!expanded));
          menu.classList.toggle("hidden");
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
