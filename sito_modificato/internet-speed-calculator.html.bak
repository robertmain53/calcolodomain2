    <script>
      // Tab switching
      const tabNeeds = document.getElementById("tab-needs");
      const tabDownload = document.getElementById("tab-download");
      const panelNeeds = document.getElementById("panel-needs");
      const panelDownload = document.getElementById("panel-download");

      function activateTab(tab) {
        if (tab === "needs") {
          tabNeeds.classList.add("border-blue-600", "text-blue-600");
          tabNeeds.classList.remove("border-transparent", "text-gray-600");
          tabDownload.classList.remove("border-blue-600", "text-blue-600");
          tabDownload.classList.add("border-transparent", "text-gray-600");
          panelNeeds.classList.remove("hidden");
          panelDownload.classList.add("hidden");
        } else {
          tabDownload.classList.add("border-blue-600", "text-blue-600");
          tabDownload.classList.remove("border-transparent", "text-gray-600");
          tabNeeds.classList.remove("border-blue-600", "text-blue-600");
          tabNeeds.classList.add("border-transparent", "text-gray-600");
          panelDownload.classList.remove("hidden");
          panelNeeds.classList.add("hidden");
        }
      }

      tabNeeds.addEventListener("click", () => activateTab("needs"));
      tabDownload.addEventListener("click", () => activateTab("download"));

      // Slider outputs
      function bindSlider(id, outputId) {
        const el = document.getElementById(id);
        const out = document.getElementById(outputId);
        if (!el || !out) return;
        const update = () => {
          out.textContent = el.value;
        };
        el.addEventListener("input", update);
        update();
      }

      bindSlider("streams-hd", "streams-hd-output");
      bindSlider("streams-uhd", "streams-uhd-output");
      bindSlider("video-calls", "video-calls-output");
      bindSlider("gamers", "gamers-output");

      // Bandwidth needs calculation
      function calculateNeeds() {
        const people =
          parseFloat(document.getElementById("household-size").value) || 0;
        const devicesPerPerson =
          parseFloat(document.getElementById("devices-per-person").value) || 0;
        const hd = parseFloat(document.getElementById("streams-hd").value) || 0;
        const uhd =
          parseFloat(document.getElementById("streams-uhd").value) || 0;
        const calls =
          parseFloat(document.getElementById("video-calls").value) || 0;
        const gamers = parseFloat(document.getElementById("gamers").value) || 0;
        const heavyDownloads =
          parseInt(document.getElementById("heavy-downloads").value, 10) || 0;
        const cloudBackup =
          parseInt(document.getElementById("cloud-backup").value, 10) || 0;

        const totalDevices = people * devicesPerPerson;

        // Baseline per active user/devices
        const baseDownload = Math.min(Math.max(people * 3, 5), 50); // 3 Mbps per person, min 5, max 50
        const deviceOverhead = Math.min(totalDevices * 0.5, 20); // 0.5 Mbps per device

        // Activity-based
        const hdDownload = hd * 6; // 6 Mbps per HD stream
        const uhdDownload = uhd * 25; // 25 Mbps per 4K stream
        const callDownload = calls * 4; // 4 Mbps per HD call
        const gamerDownload = gamers * 4; // 4 Mbps per gamer

        let heavyDownloadExtra = 0;
        if (heavyDownloads === 1) heavyDownloadExtra = 10;
        else if (heavyDownloads === 2) heavyDownloadExtra = 25;
        else if (heavyDownloads === 3) heavyDownloadExtra = 50;

        let cloudUploadExtra = 0;
        if (cloudBackup === 1) cloudUploadExtra = 5;
        else if (cloudBackup === 2) cloudUploadExtra = 20;

        const totalDownload =
          baseDownload +
          deviceOverhead +
          hdDownload +
          uhdDownload +
          callDownload +
          gamerDownload +
          heavyDownloadExtra;
        const totalUpload = calls * 3 + gamers * 2 + cloudUploadExtra + 2; // 2 Mbps baseline

        // Add 30% headroom
        const recDownload = Math.ceil(totalDownload * 1.3);
        const recUpload = Math.ceil(totalUpload * 1.3);

        // Plan tier suggestion
        let tierLabel = "";
        let tierRange = "";
        if (recDownload <= 50) {
          tierLabel = "Basic plan";
          tierRange = "Around 25–50 Mbps download";
        } else if (recDownload <= 150) {
          tierLabel = "Standard plan";
          tierRange = "Around 50–150 Mbps download";
        } else if (recDownload <= 300) {
          tierLabel = "Fast plan";
          tierRange = "Around 150–300 Mbps download";
        } else if (recDownload <= 600) {
          tierLabel = "Very fast plan";
          tierRange = "Around 300–600 Mbps download";
        } else {
          tierLabel = "Gigabit‑class plan";
          tierRange = "600 Mbps–1 Gbps+ download";
        }

        document.getElementById("recommended-download").textContent =
          recDownload.toString();
        document.getElementById("recommended-upload").textContent =
          recUpload.toString();
        document.getElementById("plan-tier").textContent = tierLabel;
        document.getElementById("plan-tier-range").textContent = tierRange;

        const summary = [];
        summary.push(
          `For a household of about ${
            people || 0
          } people and roughly ${Math.round(totalDevices)} devices,`
        );
        if (hd + uhd > 0) {
          summary.push(`with up to ${hd} HD and ${uhd} 4K streams,`);
        }
        if (calls > 0) {
          summary.push(`${calls} concurrent video calls,`);
        }
        if (gamers > 0) {
          summary.push(`${gamers} active gamers,`);
        }
        summary.push(
          `we recommend a plan around ${recDownload} Mbps down and ${recUpload} Mbps up to stay smooth at peak times.`
        );

        document.getElementById("needs-summary").textContent =
          summary.join(" ");

        const results = document.getElementById("needs-results");
        results.classList.remove("hidden");
      }

      document
        .getElementById("calculate-needs")
        .addEventListener("click", calculateNeeds);

      document.getElementById("reset-needs").addEventListener("click", () => {
        document.getElementById("household-size").value = 3;
        document.getElementById("devices-per-person").value = 3;
        document.getElementById("streams-hd").value = 2;
        document.getElementById("streams-uhd").value = 1;
        document.getElementById("video-calls").value = 2;
        document.getElementById("gamers").value = 1;
        document.getElementById("heavy-downloads").value = 1;
        document.getElementById("cloud-backup").value = 1;
        document.getElementById("wifi-quality").value = 0.7;
        bindSlider("streams-hd", "streams-hd-output");
        bindSlider("streams-uhd", "streams-uhd-output");
        bindSlider("video-calls", "video-calls-output");
        bindSlider("gamers", "gamers-output");
        document.getElementById("needs-results").classList.add("hidden");
      });

      // Download time calculation
      function calculateDownloadTime() {
        const sizeVal = parseFloat(document.getElementById("file-size").value);
        const sizeUnit = document.getElementById("file-unit").value;
        const speedVal = parseFloat(
          document.getElementById("speed-mbps").value
        );
        const direction = document.getElementById("speed-direction").value;

        if (!sizeVal || sizeVal <= 0 || !speedVal || speedVal <= 0) {
          alert("Please enter positive values for file size and speed.");
          return;
        }

        // Convert file size to megabytes (MB)
        let sizeMB = sizeVal;
        if (sizeUnit === "GB") sizeMB = sizeVal * 1024;
        else if (sizeUnit === "TB") sizeMB = sizeVal * 1024 * 1024;

        // Convert MB to megabits
        const sizeMb = sizeMB * 8;

        // Time in seconds
        const timeSec = sizeMb / speedVal;
        const timeMin = timeSec / 60;
        const timeHours = timeSec / 3600;

        function formatTime(h) {
          if (h < 1) {
            const m = h * 60;
            if (m < 1) return `${Math.round(h * 3600)} seconds`;
            return `${m.toFixed(1)} minutes`;
          }
          return `${h.toFixed(2)} hours`;
        }

        document.getElementById("transfer-time-main").textContent =
          formatTime(timeHours);
        document.getElementById("transfer-time-min").textContent =
          timeMin.toFixed(1);
        document.getElementById("transfer-time-sec").textContent =
          timeSec.toFixed(0);

        const dirLabel = direction === "download" ? "download" : "upload";
        document.getElementById(
          "download-summary"
        ).textContent = `Transferring ${sizeVal} ${sizeUnit} at ${speedVal} Mbps ${dirLabel} speed will take about ${formatTime(
          timeHours
        )} in ideal conditions.`;

        document.getElementById("download-results").classList.remove("hidden");
      }

      document
        .getElementById("calculate-download")
        .addEventListener("click", calculateDownloadTime);

      document.getElementById("swap-speed").addEventListener("click", () => {
        const sel = document.getElementById("speed-direction");
        sel.value = sel.value === "download" ? "upload" : "download";
      });

      // Optional: auto-calc initial needs
      calculateNeeds();

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
