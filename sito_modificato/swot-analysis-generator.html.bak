    <script>
      // Simple in-browser SWOT state
      const swotState = {
        strengths: [],
        weaknesses: [],
        opportunities: [],
        threats: [],
        format: "markdown",
      };

      const STORAGE_KEY = "calcdomain_swot_analysis_v1";

      function $(id) {
        return document.getElementById(id);
      }

      function renderQuadrant(quadrant) {
        const listEl = document.querySelector(
          '[data-quadrant-list="' + quadrant + '"]'
        );
        listEl.innerHTML = "";
        swotState[quadrant].forEach((item, index) => {
          const div = document.createElement("div");
          div.className =
            "swot-tag flex items-start justify-between gap-2 px-2.5 py-1.5 rounded-md bg-white shadow-sm border border-gray-200 text-xs md:text-sm";
          div.draggable = true;
          div.dataset.quadrant = quadrant;
          div.dataset.index = index.toString();
          div.innerHTML = `
        <span class="flex-1">${item}</span>
        <div class="flex items-center gap-1 text-gray-400">
          <button type="button" class="swot-move-up hover:text-gray-700" title="Move up" aria-label="Move up">▲</button>
          <button type="button" class="swot-move-down hover:text-gray-700" title="Move down" aria-label="Move down">▼</button>
          <button type="button" class="swot-delete hover:text-rose-600" title="Remove" aria-label="Remove">✕</button>
        </div>
      `;
          listEl.appendChild(div);
        });
        const countEl = $("count-" + quadrant);
        if (countEl) {
          const n = swotState[quadrant].length;
          countEl.textContent = n + (n === 1 ? " item" : " items");
        }
      }

      function renderAll() {
        ["strengths", "weaknesses", "opportunities", "threats"].forEach(
          renderQuadrant
        );
        if ($("swot-save-toggle")?.checked) {
          try {
            localStorage.setItem(
              STORAGE_KEY,
              JSON.stringify({
                ...swotState,
                title: $("swot-title").value || "",
                context: $("swot-context").value || "",
              })
            );
          } catch (e) {}
        }
      }

      function addItem(quadrant, text) {
        const trimmed = text.trim();
        if (!trimmed) return;
        swotState[quadrant].push(trimmed);
        renderAll();
      }

      function clearAll() {
        swotState.strengths = [];
        swotState.weaknesses = [];
        swotState.opportunities = [];
        swotState.threats = [];
        renderAll();
        const exportContainer = $("swot-export-container");
        if (exportContainer) exportContainer.classList.add("hidden");
      }

      function generateExport(format) {
        const title = $("swot-title").value.trim();
        const context = $("swot-context").value.trim();
        let lines = [];

        if (format === "markdown") {
          if (title) lines.push("# " + title);
          if (context) {
            if (title) lines.push("");
            lines.push(context);
          }
          if (title || context) lines.push("");
          ["strengths", "weaknesses", "opportunities", "threats"].forEach(
            (q) => {
              const label = q.charAt(0).toUpperCase() + q.slice(1);
              lines.push("## " + label);
              if (swotState[q].length === 0) {
                lines.push("- (none noted)");
              } else {
                swotState[q].forEach((item) => lines.push("- " + item));
              }
              lines.push("");
            }
          );
        } else {
          if (title) lines.push(title.toUpperCase());
          if (context) {
            lines.push("");
            lines.push(context);
          }
          if (title || context) lines.push("");
          ["strengths", "weaknesses", "opportunities", "threats"].forEach(
            (q) => {
              const label = q.charAt(0).toUpperCase() + q.slice(1);
              lines.push(label + ":");
              if (swotState[q].length === 0) {
                lines.push("  - (none noted)");
              } else {
                swotState[q].forEach((item) => lines.push("  - " + item));
              }
              lines.push("");
            }
          );
        }
        return lines.join("\n").trim() + "\n";
      }

      function updateExportArea() {
        const exportContainer = $("swot-export-container");
        const exportText = $("swot-export-text");
        if (!exportContainer || !exportText) return;
        exportContainer.classList.remove("hidden");
        exportText.value = generateExport(swotState.format);
      }

      function loadFromStorage() {
        try {
          const raw = localStorage.getItem(STORAGE_KEY);
          if (!raw) return;
          const data = JSON.parse(raw);
          ["strengths", "weaknesses", "opportunities", "threats"].forEach(
            (q) => {
              if (Array.isArray(data[q])) swotState[q] = data[q];
            }
          );
          if (data.title && $("swot-title")) $("swot-title").value = data.title;
          if (data.context && $("swot-context"))
            $("swot-context").value = data.context;
          renderAll();
        } catch (e) {}
      }

      document.addEventListener("DOMContentLoaded", () => {
        renderAll();
        loadFromStorage();

        const input = $("swot-item-text");
        const select = $("swot-category");
        const addBtn = $("swot-add-btn");
        const clearBtn = $("swot-clear-btn");
        const exportBtn = $("swot-export-btn");
        const saveToggle = $("swot-save-toggle");

        if (addBtn && input && select) {
          addBtn.addEventListener("click", () => {
            addItem(select.value, input.value);
            input.value = "";
            input.focus();
          });
          input.addEventListener("keydown", (e) => {
            if (e.key === "Enter") {
              e.preventDefault();
              addItem(select.value, input.value);
              input.value = "";
            }
          });
        }

        if (clearBtn) {
          clearBtn.addEventListener("click", () => {
            if (confirm("Clear all SWOT items? This cannot be undone.")) {
              clearAll();
              if (saveToggle?.checked) {
                try {
                  localStorage.removeItem(STORAGE_KEY);
                } catch (e) {}
              }
            }
          });
        }

        if (exportBtn) {
          exportBtn.addEventListener("click", () => {
            updateExportArea();
            $("swot-export-text")?.focus();
            $("swot-export-text")?.select();
          });
        }

        if (saveToggle) {
          saveToggle.addEventListener("change", () => {
            if (!saveToggle.checked) {
              try {
                localStorage.removeItem(STORAGE_KEY);
              } catch (e) {}
            } else {
              renderAll();
            }
          });
        }

        // Export format buttons
        document.querySelectorAll(".swot-format-btn").forEach((btn) => {
          btn.addEventListener("click", () => {
            const format = btn.getAttribute("data-export-format") || "markdown";
            swotState.format = format;
            document.querySelectorAll(".swot-format-btn").forEach((b) => {
              b.classList.remove("bg-gray-100", "text-gray-900", "font-medium");
              b.classList.add("bg-white", "text-gray-700");
            });
            btn.classList.add("bg-gray-100", "text-gray-900", "font-medium");
            btn.classList.remove("bg-white", "text-gray-700");
            updateExportArea();
          });
        });

        // Delegated events for move/delete
        document.body.addEventListener("click", (e) => {
          const target = e.target;
          if (!(target instanceof HTMLElement)) return;
          if (
            target.classList.contains("swot-delete") ||
            target.closest(".swot-delete")
          ) {
            const tag = target.closest(".swot-tag");
            if (!tag) return;
            const q = tag.dataset.quadrant;
            const idx = parseInt(tag.dataset.index || "0", 10);
            if (!q || !Array.isArray(swotState[q])) return;
            swotState[q].splice(idx, 1);
            renderAll();
          } else if (
            target.classList.contains("swot-move-up") ||
            target.closest(".swot-move-up")
          ) {
            const tag = target.closest(".swot-tag");
            if (!tag) return;
            const q = tag.dataset.quadrant;
            let idx = parseInt(tag.dataset.index || "0", 10);
            if (!q || !Array.isArray(swotState[q]) || idx <= 0) return;
            const arr = swotState[q];
            [arr[idx - 1], arr[idx]] = [arr[idx], arr[idx - 1]];
            renderAll();
          } else if (
            target.classList.contains("swot-move-down") ||
            target.closest(".swot-move-down")
          ) {
            const tag = target.closest(".swot-tag");
            if (!tag) return;
            const q = tag.dataset.quadrant;
            let idx = parseInt(tag.dataset.index || "0", 10);
            if (!q || !Array.isArray(swotState[q])) return;
            const arr = swotState[q];
            if (idx >= arr.length - 1) return;
            [arr[idx + 1], arr[idx]] = [arr[idx], arr[idx + 1]];
            renderAll();
          }
        });

        // Drag & drop between quadrants
        let dragData = null;
        document.body.addEventListener("dragstart", (e) => {
          const target = e.target;
          if (!(target instanceof HTMLElement)) return;
          if (target.classList.contains("swot-tag")) {
            dragData = {
              quadrant: target.dataset.quadrant,
              index: parseInt(target.dataset.index || "0", 10),
            };
            e.dataTransfer?.setData("text/plain", "swot");
            e.dataTransfer?.setDragImage(target, 10, 10);
          }
        });

        document.querySelectorAll(".swot-drop-target").forEach((area) => {
          area.addEventListener("dragover", (e) => {
            if (!dragData) return;
            e.preventDefault();
            area.classList.add("ring-2", "ring-blue-400");
          });
          area.addEventListener("dragleave", () => {
            area.classList.remove("ring-2", "ring-blue-400");
          });
          area.addEventListener("drop", (e) => {
            if (!dragData) return;
            e.preventDefault();
            area.classList.remove("ring-2", "ring-blue-400");
            const fromQ = dragData.quadrant;
            const fromIdx = dragData.index;
            const toQ = area.getAttribute("data-quadrant-list");
            if (
              !fromQ ||
              !toQ ||
              !Array.isArray(swotState[fromQ]) ||
              !Array.isArray(swotState[toQ])
            )
              return;
            const [item] = swotState[fromQ].splice(fromIdx, 1);
            swotState[toQ].push(item);
            dragData = null;
            renderAll();
          });
        });

        // Simple suggestions for inspiration
        const suggestBtn = $("swot-suggest-btn");
        if (suggestBtn) {
          suggestBtn.classList.remove("hidden");
          suggestBtn.addEventListener("click", () => {
            if (
              swotState.strengths.length +
                swotState.weaknesses.length +
                swotState.opportunities.length +
                swotState.threats.length >
              0
            ) {
              if (!confirm("Add example items on top of your current SWOT?"))
                return;
            }
            const examples = {
              strengths: [
                "Strong brand recognition in our target niche",
                "Loyal customer base with high repeat purchase rate",
                "Lean cost structure and healthy gross margins",
              ],
              weaknesses: [
                "Limited marketing budget compared to key competitors",
                "Dependence on a small number of large clients",
                "Outdated internal reporting and analytics tools",
              ],
              opportunities: [
                "Growing demand in adjacent market segments",
                "Potential partnerships with complementary platforms",
                "Regulatory changes favoring digital solutions",
              ],
              threats: [
                "New well-funded competitors entering the market",
                "Economic downturn reducing customer budgets",
                "Rapid technology shifts increasing switching risk",
              ],
            };
            ["strengths", "weaknesses", "opportunities", "threats"].forEach(
              (q) => {
                examples[q].forEach((item) => swotState[q].push(item));
              }
            );
            renderAll();
          });
        }
      });
    </script>

