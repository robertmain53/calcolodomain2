    <script>
      // State
      let currentUnit = "metric"; // 'metric' or 'imperial'

      function updateMultiplierLabels() {
        const lower =
          parseFloat(document.getElementById("lower-multiplier").value) || 0;
        const upper =
          parseFloat(document.getElementById("upper-multiplier").value) || 0;
        const lowerLabel = document.getElementById("lower-multiplier-label");
        const upperLabel = document.getElementById("upper-multiplier-label");

        if (currentUnit === "metric") {
          lowerLabel.textContent = `≈ ${lower.toFixed(2)} g/kg`;
          upperLabel.textContent = `≈ ${upper.toFixed(2)} g/kg`;
        } else {
          lowerLabel.textContent = `≈ ${lower.toFixed(2)} g/lb`;
          upperLabel.textContent = `≈ ${upper.toFixed(2)} g/lb`;
        }
      }

      function setDefaultsForGoal() {
        const goal = document.getElementById("goal").value;
        const activity = document.getElementById("activity").value;
        const age = parseInt(document.getElementById("age").value, 10) || 30;

        let low, high;

        if (goal === "health") {
          low = 1.0;
          high = 1.2;
        } else if (goal === "maintenance") {
          low = 1.2;
          high = 1.6;
        } else if (goal === "muscle-gain") {
          low = 1.6;
          high = 2.2;
        } else if (goal === "fat-loss") {
          low = 1.8;
          high = 2.4;
        } else if (goal === "older-adult") {
          low = 1.2;
          high = 1.8;
          if (age >= 65) {
            low = 1.3;
          }
        } else {
          low = 1.2;
          high = 1.6;
        }

        // Slightly adjust by activity (within reasonable bounds)
        if (activity === "sedentary") {
          low -= 0.1;
          high -= 0.1;
        } else if (activity === "high" || activity === "athlete") {
          low += 0.1;
          high += 0.1;
        }

        low = Math.max(0.8, Math.round(low * 10) / 10);
        high = Math.min(2.6, Math.round(high * 10) / 10);

        document.getElementById("lower-multiplier").value = low.toFixed(1);
        document.getElementById("upper-multiplier").value = high.toFixed(1);
        updateMultiplierLabels();
      }

      function getBodyMassKg() {
        if (currentUnit === "metric") {
          return parseFloat(document.getElementById("weight-kg").value) || 0;
        } else {
          const lb =
            parseFloat(document.getElementById("weight-lb").value) || 0;
          return lb / 2.2046;
        }
      }

      function getLeanMassKg(totalKg) {
        const bf = parseFloat(document.getElementById("body-fat").value);
        if (isNaN(bf) || bf <= 0 || bf >= 60) return null;
        const fraction = bf / 100;
        return totalKg * (1 - fraction);
      }

      function formatGrams(g) {
        if (!isFinite(g)) return "–";
        if (g < 10) return g.toFixed(1);
        return Math.round(g).toString();
      }

      function calculateProtein() {
        const errorEl = document.getElementById("form-error");
        errorEl.classList.add("hidden");

        const age = parseInt(document.getElementById("age").value, 10);
        if (!age || age < 14 || age > 90) {
          errorEl.textContent = "Please enter a valid age between 14 and 90.";
          errorEl.classList.remove("hidden");
          return;
        }

        let weightKg = getBodyMassKg();
        if (!weightKg || weightKg < 35 || weightKg > 250) {
          errorEl.textContent = "Please enter a realistic body weight.";
          errorEl.classList.remove("hidden");
          return;
        }

        const basis =
          document.querySelector('input[name="basis"]:checked')?.value ||
          "total";
        const leanMass = getLeanMassKg(weightKg);
        let massForCalcKg = weightKg;
        let usingLean = false;

        if (basis === "lean" && leanMass && leanMass > 0) {
          massForCalcKg = leanMass;
          usingLean = true;
        }

        let lowerMult = parseFloat(
          document.getElementById("lower-multiplier").value
        );
        let upperMult = parseFloat(
          document.getElementById("upper-multiplier").value
        );
        if (
          !lowerMult ||
          !upperMult ||
          lowerMult <= 0 ||
          upperMult <= 0 ||
          upperMult < lowerMult
        ) {
          errorEl.textContent =
            "Please check the advanced protein range values.";
          errorEl.classList.remove("hidden");
          return;
        }

        // If user is in imperial mode, treat multipliers as g/lb instead of g/kg
        let proteinLow, proteinHigh;
        if (currentUnit === "metric") {
          proteinLow = massForCalcKg * lowerMult;
          proteinHigh = massForCalcKg * upperMult;
        } else {
          const massLb = massForCalcKg * 2.2046;
          proteinLow = massLb * lowerMult;
          proteinHigh = massLb * upperMult;
        }

        const proteinTarget = (proteinLow + proteinHigh) / 2;

        const meals = parseInt(document.getElementById("meals").value, 10) || 3;
        const snacks =
          parseInt(document.getElementById("snacks").value, 10) || 0;
        const feedings = Math.max(1, meals + snacks);

        const perFeeding = proteinTarget / feedings;

        const results = document.getElementById("results-container");

        const goal = document.getElementById("goal").value;
        const activity = document.getElementById("activity").value;

        const massLabel = usingLean ? "lean mass" : "body weight";
        const unitLabel = currentUnit === "metric" ? "g/kg" : "g/lb";

        let goalText;
        if (goal === "muscle-gain") goalText = "muscle gain and strength";
        else if (goal === "fat-loss")
          goalText = "fat loss while preserving muscle";
        else if (goal === "maintenance")
          goalText = "weight maintenance and performance";
        else if (goal === "older-adult")
          goalText = "healthy aging and muscle retention";
        else goalText = "general health";

        let activityText;
        if (activity === "sedentary") activityText = "sedentary";
        else if (activity === "light") activityText = "lightly active";
        else if (activity === "moderate") activityText = "moderately active";
        else if (activity === "high") activityText = "very active";
        else activityText = "athlete / heavy training";

        results.innerHTML = `
      <div class="border border-blue-100 bg-blue-50 rounded-md p-4">
        <h3 class="text-lg font-semibold mb-2">Daily protein summary</h3>
        <p class="text-sm text-gray-700 mb-3">
          For a <strong>${activityText}</strong> person focused on <strong>${goalText}</strong>,
          your recommended protein intake based on <strong>${massLabel}</strong> is:
        </p>
        <div class="grid grid-cols-1 md:grid-cols-3 gap-4 text-sm">
          <div class="bg-white rounded-md p-3 shadow-sm">
            <p class="text-gray-500 text-xs uppercase tracking-wide mb-1">Daily range</p>
            <p class="text-lg font-semibold">${formatGrams(
              proteinLow
            )}–${formatGrams(proteinHigh)} g/day</p>
          </div>
          <div class="bg-white rounded-md p-3 shadow-sm">
            <p class="text-gray-500 text-xs uppercase tracking-wide mb-1">Daily target</p>
            <p class="text-lg font-semibold">${formatGrams(
              proteinTarget
            )} g/day</p>
          </div>
          <div class="bg-white rounded-md p-3 shadow-sm">
            <p class="text-gray-500 text-xs uppercase tracking-wide mb-1">Relative intake</p>
            <p class="text-sm">
              ${lowerMult.toFixed(2)}–${upperMult.toFixed(2)} ${unitLabel}
            </p>
          </div>
        </div>
      </div>

      <div class="border border-gray-200 rounded-md p-4 mt-4">
        <h3 class="text-base font-semibold mb-2">Per‑meal and per‑snack suggestions</h3>
        <p class="text-sm text-gray-700 mb-3">
          With <strong>${meals}</strong> main meals and <strong>${snacks}</strong> protein snacks (${feedings} total feedings),
          aim for roughly:
        </p>
        <ul class="list-disc ml-5 text-sm text-gray-700 space-y-1">
          <li><strong>${formatGrams(
            perFeeding
          )} g</strong> of protein per eating occasion (meal or snack).</li>
          <li>That’s about <strong>${formatGrams(
            perFeeding * meals
          )} g</strong> from meals and <strong>${formatGrams(
          perFeeding * snacks
        )} g</strong> from snacks.</li>
        </ul>
        <p class="text-xs text-gray-500 mt-2">
          Research suggests that 20–40 g of high‑quality protein per meal maximally stimulates muscle protein synthesis for most adults.
          Your target is adjusted for your body size and goals.
        </p>
      </div>
    `;
      }

      document.addEventListener("DOMContentLoaded", function () {
        // Unit toggle
        document.querySelectorAll(".unit-toggle").forEach((btn) => {
          btn.addEventListener("click", () => {
            const selected = btn.getAttribute("data-unit");
            if (selected === currentUnit) return;
            currentUnit = selected;

            document.querySelectorAll(".unit-toggle").forEach((b) => {
              if (b.getAttribute("data-unit") === currentUnit) {
                b.classList.add("bg-blue-50", "text-blue-700");
                b.classList.remove("text-gray-600");
              } else {
                b.classList.remove("bg-blue-50", "text-blue-700");
                b.classList.add("text-gray-600");
              }
            });

            // Toggle fields
            document
              .getElementById("height-metric-group")
              .classList.toggle("hidden", currentUnit !== "metric");
            document
              .getElementById("height-imperial-group")
              .classList.toggle("hidden", currentUnit !== "imperial");
            document
              .getElementById("weight-metric-group")
              .classList.toggle("hidden", currentUnit !== "metric");
            document
              .getElementById("weight-imperial-group")
              .classList.toggle("hidden", currentUnit !== "imperial");

            // Convert existing values when switching
            if (currentUnit === "imperial") {
              const kg = parseFloat(document.getElementById("weight-kg").value);
              if (kg) {
                document.getElementById("weight-lb").value = Math.round(
                  kg * 2.2046
                );
              }
              const cm = parseFloat(document.getElementById("height-cm").value);
              if (cm) {
                const totalIn = cm / 2.54;
                const ft = Math.floor(totalIn / 12);
                const inch = Math.round(totalIn - ft * 12);
                document.getElementById("height-ft").value = ft;
                document.getElementById("height-in").value = inch;
              }
            } else {
              const lb = parseFloat(document.getElementById("weight-lb").value);
              if (lb) {
                document.getElementById("weight-kg").value =
                  Math.round((lb / 2.2046) * 10) / 10;
              }
              const ft = parseFloat(document.getElementById("height-ft").value);
              const inch = parseFloat(
                document.getElementById("height-in").value
              );
              if (ft || inch) {
                const totalIn = ft * 12 + inch;
                document.getElementById("height-cm").value = Math.round(
                  totalIn * 2.54
                );
              }
            }

            updateMultiplierLabels();
          });
        });

        // Sex toggle styling
        document.querySelectorAll(".sex-toggle").forEach((btn) => {
          btn.addEventListener("click", () => {
            document.querySelectorAll(".sex-toggle").forEach((b) => {
              if (b === btn) {
                b.classList.add("bg-blue-50", "text-blue-700");
                b.classList.remove("text-gray-600");
              } else {
                b.classList.remove("bg-blue-50", "text-blue-700");
                b.classList.add("text-gray-600");
              }
            });
          });
        });

        // Goal / activity change updates defaults
        document
          .getElementById("goal")
          .addEventListener("change", setDefaultsForGoal);
        document
          .getElementById("activity")
          .addEventListener("change", setDefaultsForGoal);
        document
          .getElementById("age")
          .addEventListener("change", setDefaultsForGoal);

        // Multiplier label updates
        document
          .getElementById("lower-multiplier")
          .addEventListener("input", updateMultiplierLabels);
        document
          .getElementById("upper-multiplier")
          .addEventListener("input", updateMultiplierLabels);

        // Calculate button
        document
          .getElementById("calculate-btn")
          .addEventListener("click", calculateProtein);

        // Reset
        document
          .getElementById("protein-form")
          .addEventListener("reset", () => {
            setTimeout(() => {
              currentUnit = "metric";
              document.querySelector('[data-unit="metric"]').click();
              document.querySelector('[data-sex="male"]').click();
              document.querySelector(
                'input[name="basis"][value="total"]'
              ).checked = true;
              setDefaultsForGoal();
              document.getElementById("results-container").innerHTML = `
          <p class="text-sm text-gray-600">Fill in your details and click “Calculate” to see your personalized daily protein range, a single target, and per‑meal suggestions.</p>
        `;
              document.getElementById("form-error").classList.add("hidden");
            }, 0);
          });

        // Initial defaults
        setDefaultsForGoal();
        updateMultiplierLabels();
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
