    <script>
      // Tab switching
      const tabs = {
        flexure: {
          btn: document.getElementById("tab-flexure"),
          panel: document.getElementById("panel-flexure"),
        },
        axial: {
          btn: document.getElementById("tab-axial"),
          panel: document.getElementById("panel-axial"),
        },
        shear: {
          btn: document.getElementById("tab-shear"),
          panel: document.getElementById("panel-shear"),
        },
      };

      function activateTab(name) {
        Object.keys(tabs).forEach((key) => {
          const { btn, panel } = tabs[key];
          if (key === name) {
            btn.classList.add("border-blue-600", "text-blue-600");
            btn.classList.remove("border-transparent", "text-gray-500");
            panel.classList.remove("hidden");
          } else {
            btn.classList.remove("border-blue-600", "text-blue-600");
            btn.classList.add("border-transparent", "text-gray-500");
            panel.classList.add("hidden");
          }
        });
      }

      tabs.flexure.btn.addEventListener("click", () => activateTab("flexure"));
      tabs.axial.btn.addEventListener("click", () => activateTab("axial"));
      tabs.shear.btn.addEventListener("click", () => activateTab("shear"));

      // Flexure calculations
      document
        .getElementById("btn-flexure-calc")
        .addEventListener("click", () => {
          const f_m = parseFloat(document.getElementById("f_m").value);
          const fy = parseFloat(document.getElementById("fy").value);
          const b = parseFloat(document.getElementById("b").value);
          const d = parseFloat(document.getElementById("d").value);
          const As = parseFloat(document.getElementById("As").value);
          const phi = parseFloat(document.getElementById("phi_flexure").value);

          if ([f_m, fy, b, d, As, phi].some((v) => !isFinite(v) || v <= 0))
            return;

          const a = (As * fy) / (0.85 * f_m * b);
          const Mn_inlb = As * fy * (d - a / 2);
          const Mn_kipft = Mn_inlb / 12 / 1000;
          const phiMn_kipft = phi * Mn_kipft;

          const rho = As / (b * d);
          const fs = fy; // assume yielding for this simplified check

          document.getElementById("res_a").textContent = a.toFixed(2);
          document.getElementById("res_Mn").textContent = Mn_kipft.toFixed(2);
          document.getElementById("res_phiMn").textContent =
            phiMn_kipft.toFixed(2);
          document.getElementById("res_fs").textContent = fs.toFixed(0);
          document.getElementById("res_rho").textContent = rho.toExponential(2);

          document.getElementById("flexure-results").classList.remove("hidden");
        });

      document
        .getElementById("btn-flexure-reset")
        .addEventListener("click", () => {
          document.getElementById("flexure-form").reset();
          document.getElementById("f_m").value = 1500;
          document.getElementById("fy").value = 60000;
          document.getElementById("b").value = 7.625;
          document.getElementById("d").value = 9.5;
          document.getElementById("As").value = 0.6;
          document.getElementById("phi_flexure").value = 0.9;
          document.getElementById("flexure-results").classList.add("hidden");
        });

      // Axial + flexure interaction
      document
        .getElementById("btn-axial-calc")
        .addEventListener("click", () => {
          const Pu = parseFloat(document.getElementById("Pu").value);
          const Mu = parseFloat(document.getElementById("Mu").value);
          const Pn0 = parseFloat(document.getElementById("Pn0").value);
          const phiMn0 = parseFloat(document.getElementById("phiMn_int").value);

          if ([Pu, Mu, Pn0, phiMn0].some((v) => !isFinite(v) || v <= 0)) return;

          const axialRatio = Pu / Pn0;
          const momentRatio = Mu / phiMn0;
          const interaction = axialRatio + momentRatio;

          document.getElementById("res_axial_ratio").textContent =
            axialRatio.toFixed(2);
          document.getElementById("res_moment_ratio").textContent =
            momentRatio.toFixed(2);
          document.getElementById("res_interaction").textContent =
            interaction.toFixed(2);

          const statusEl = document.getElementById("res_interaction_status");
          if (interaction <= 1.0) {
            statusEl.textContent =
              "OK: Simplified interaction check is satisfied (≤ 1.0).";
            statusEl.classList.remove("text-red-600");
            statusEl.classList.add("text-green-700");
          } else {
            statusEl.textContent =
              "Not OK: Interaction value exceeds 1.0. Increase capacity or reduce loads.";
            statusEl.classList.remove("text-green-700");
            statusEl.classList.add("text-red-600");
          }

          document.getElementById("axial-results").classList.remove("hidden");
        });

      document
        .getElementById("btn-axial-reset")
        .addEventListener("click", () => {
          document.getElementById("axial-form").reset();
          document.getElementById("Pu").value = 50;
          document.getElementById("Mu").value = 40;
          document.getElementById("Pn0").value = 200;
          document.getElementById("phiMn_int").value = 80;
          document.getElementById("axial-results").classList.add("hidden");
        });

      // Shear calculations
      document
        .getElementById("btn-shear-calc")
        .addEventListener("click", () => {
          const Vu = parseFloat(document.getElementById("V_u").value);
          const phi = parseFloat(document.getElementById("phi_shear").value);
          const bv = parseFloat(document.getElementById("b_v").value);
          const dv = parseFloat(document.getElementById("d_v").value);
          const v_mn = parseFloat(document.getElementById("v_mn").value);
          const Avf = parseFloat(document.getElementById("Avf").value);
          const s = parseFloat(document.getElementById("s_v").value);
          const fy_v = parseFloat(document.getElementById("fy_v").value);

          if (
            [Vu, phi, bv, dv, v_mn, s, fy_v].some(
              (v) => !isFinite(v) || v <= 0
            ) ||
            !isFinite(Avf) ||
            Avf < 0
          )
            return;

          const Vm = (v_mn * bv * dv) / 1000.0;
          const Vs = (Avf * fy_v * dv) / (s * 1000.0);
          const Vn = Vm + Vs;
          const phiVn = phi * Vn;
          const ratio = Vu / phiVn;

          document.getElementById("res_Vm").textContent = Vm.toFixed(2);
          document.getElementById("res_Vs").textContent = Vs.toFixed(2);
          document.getElementById("res_Vn").textContent = Vn.toFixed(2);
          document.getElementById("res_phiVn").textContent = phiVn.toFixed(2);
          document.getElementById("res_V_ratio").textContent = ratio.toFixed(2);

          const statusEl = document.getElementById("res_V_status");
          if (ratio <= 1.0) {
            statusEl.textContent =
              "OK: Shear demand is within design capacity.";
            statusEl.classList.remove("text-red-600");
            statusEl.classList.add("text-green-700");
          } else {
            statusEl.textContent =
              "Not OK: Shear demand exceeds design capacity. Increase section or reinforcement.";
            statusEl.classList.remove("text-green-700");
            statusEl.classList.add("text-red-600");
          }

          document.getElementById("shear-results").classList.remove("hidden");
        });

      document
        .getElementById("btn-shear-reset")
        .addEventListener("click", () => {
          document.getElementById("shear-form").reset();
          document.getElementById("V_u").value = 20;
          document.getElementById("phi_shear").value = 0.75;
          document.getElementById("b_v").value = 7.625;
          document.getElementById("d_v").value = 9.0;
          document.getElementById("v_mn").value = 35;
          document.getElementById("Avf").value = 0.0;
          document.getElementById("s_v").value = 16;
          document.getElementById("fy_v").value = 60000;
          document.getElementById("shear-results").classList.add("hidden");
        });

      // Mobile menu script (simple)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", () => {
          const menu = document.getElementById("mobile-menu");
          const expanded = menu.classList.contains("hidden") ? "true" : "false";
          menu.classList.toggle("hidden");
          document
            .getElementById("mobile-menu-toggle")
            .setAttribute("aria-expanded", expanded);
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
