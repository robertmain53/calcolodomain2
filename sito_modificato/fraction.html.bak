    <script>
      function gcd(a, b) {
        a = Math.abs(a); b = Math.abs(b);
        while (b) {
          const t = b;
          b = a % b;
          a = t;
        }
        return a || 1;
      }

      function lcm(a, b) {
        return Math.abs(a * b) / gcd(a, b);
      }

      function parseFraction(str) {
        // returns {num, den} in integers
        if (!str) return {num: 0, den: 1};
        str = String(str).trim();
        // mixed number: "2 1/3"
        if (str.includes(' ')) {
          const parts = str.split(' ');
          const whole = parseFloat(parts[0]);
          const fracPart = parseFraction(parts.slice(1).join(' '));
          const num = whole >= 0 ? whole * fracPart.den + fracPart.num : whole * fracPart.den - fracPart.num;
          return {num: num, den: fracPart.den};
        }
        // pure fraction: "3/4"
        if (str.includes('/')) {
          const [n, d] = str.split('/');
          return {num: parseFloat(n), den: parseFloat(d)};
        }
        // decimal
        if (str.includes('.')) {
          const dec = str;
          const parts = dec.split('.');
          const digits = parts[1].length;
          const den = Math.pow(10, digits);
          const num = Math.round(parseFloat(dec) * den);
          return {num, den};
        }
        // integer
        return {num: parseFloat(str), den: 1};
      }

      function simplify(frac) {
        if (frac.den < 0) {
          frac.num *= -1;
          frac.den *= -1;
        }
        const g = gcd(frac.num, frac.den);
        return {num: frac.num / g, den: frac.den / g};
      }

      function toMixed(frac) {
        const sign = frac.num < 0 ? -1 : 1;
        const n = Math.abs(frac.num);
        const d = Math.abs(frac.den);
        const whole = Math.floor(n / d);
        const rem = n % d;
        return {sign, whole, num: rem, den: d};
      }

      function fracToDecimal(frac) {
        return frac.num / frac.den;
      }

      function calculateFraction() {
        const f1 = parseFraction(document.getElementById('frac1').value);
        const op = document.getElementById('operation').value;
        const f2 = parseFraction(document.getElementById('frac2').value);

        const resFracEl = document.getElementById('res-frac');
        const resMixedEl = document.getElementById('res-mixed');
        const resDecEl = document.getElementById('res-dec');
        const resPctEl = document.getElementById('res-pct');
        const resStepsEl = document.getElementById('res-steps');

        let result = {num: 0, den: 1};
        let steps = '';

        if (op === 'simplify') {
          const simp = simplify(f1);
          result = simp;
          steps = `Original: ${f1.num}/${f1.den}\nGCD(${f1.num}, ${f1.den}) = ${gcd(f1.num,f1.den)}\nSimplified: ${simp.num}/${simp.den}`;
        } else if (op === 'add' || op === 'sub') {
          const sign = op === 'add' ? 1 : -1;
          const commonDen = lcm(f1.den, f2.den);
          const n1 = f1.num * (commonDen / f1.den);
          const n2 = f2.num * (commonDen / f2.den) * sign;
          const sum = n1 + n2;
          result = simplify({num: sum, den: commonDen});
          steps = `Find common denominator: lcm(${f1.den}, ${f2.den}) = ${commonDen}\n` +
                  `Convert: ${f1.num}/${f1.den} → ${n1}/${commonDen}, ${f2.num}/${f2.den} → ${Math.abs(n2)}/${commonDen} (with sign)\n` +
                  `Add: ${n1} ${sign === 1 ? '+' : '-'} ${Math.abs(n2)} = ${sum}\n` +
                  `Result before simplify: ${sum}/${commonDen}\n` +
                  `Simplified: ${result.num}/${result.den}`;
        } else if (op === 'mul') {
          const num = f1.num * f2.num;
          const den = f1.den * f2.den;
          result = simplify({num, den});
          steps = `Multiply numerators: ${f1.num} × ${f2.num} = ${num}\n` +
                  `Multiply denominators: ${f1.den} × ${f2.den} = ${den}\n` +
                  `Simplify: ${result.num}/${result.den}`;
        } else if (op === 'div') {
          const num = f1.num * f2.den;
          const den = f1.den * f2.num;
          result = simplify({num, den});
          steps = `Division = multiply by reciprocal: ${f1.num}/${f1.den} × ${f2.den}/${f2.num}\n` +
                  `= ${num}/${den}\nSimplified: ${result.num}/${result.den}`;
        } else if (op === 'compare') {
          const left = f1.num * f2.den;
          const right = f2.num * f1.den;
          let cmp = '';
          if (left > right) cmp = 'Fraction 1 > Fraction 2';
          else if (left < right) cmp = 'Fraction 1 < Fraction 2';
          else cmp = 'Fraction 1 = Fraction 2';
          result = simplify(f1); // show first fraction simplified
          steps = `Compare by cross multiplication:\n${f1.num} × ${f2.den} = ${left}\n${f2.num} × ${f1.den} = ${right}\n→ ${cmp}`;
        }

        // Output
        resFracEl.textContent = `${result.num}/${result.den}`;
        const mixed = toMixed(result);
        if (mixed.num === 0) {
          // no fractional part
          resMixedEl.textContent = (mixed.sign * mixed.whole).toString();
        } else {
          const sgn = mixed.sign < 0 ? '-' : '';
          resMixedEl.textContent = `${sgn}${mixed.whole > 0 ? mixed.whole + ' ' : ''}${mixed.num}/${mixed.den}`;
        }
        const dec = fracToDecimal(result);
        resDecEl.textContent = isFinite(dec) ? dec.toFixed(6).replace(/\.?0+$/,'') : '—';
        resPctEl.textContent = isFinite(dec) ? (dec * 100).toFixed(2) + '%' : '—';
        resStepsEl.textContent = steps;
      }

      document.getElementById('calc-btn').addEventListener('click', calculateFraction);
      // initial
      calculateFraction();
    </script>

