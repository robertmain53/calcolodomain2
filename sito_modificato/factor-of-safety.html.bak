    <script>
      (function () {
        const modeButtons = document.querySelectorAll(".mode-btn");
        const capacityLabel = document.getElementById("capacity-label");
        const appliedLabel = document.getElementById("applied-label");
        const appliedGroup = document.getElementById("applied-group");
        const fosInputGroup = document.getElementById("fos-input-group");
        const statusBadge = document.getElementById("status-badge");
        const resultCard = document.getElementById("results-card");
        const resultMain = document.getElementById("result-main");
        const resultSecondary = document.getElementById("result-secondary");
        const resultInterpretation = document.getElementById(
          "result-interpretation"
        );

        let currentMode = "fos-load";

        function setMode(mode) {
          currentMode = mode;
          modeButtons.forEach((btn) => {
            if (btn.dataset.mode === mode) {
              btn.classList.remove(
                "border-gray-300",
                "text-gray-700",
                "bg-white"
              );
              btn.classList.add(
                "border-blue-500",
                "text-blue-600",
                "bg-blue-50",
                "font-medium"
              );
            } else {
              btn.classList.remove(
                "border-blue-500",
                "text-blue-600",
                "bg-blue-50",
                "font-medium"
              );
              btn.classList.add("border-gray-300", "text-gray-700", "bg-white");
            }
          });

          if (mode === "fos-load") {
            capacityLabel.textContent = "Capacity (ultimate or allowable load)";
            appliedLabel.textContent = "Applied load (design load)";
            appliedGroup.classList.remove("hidden");
            fosInputGroup.classList.add("hidden");
          } else if (mode === "fos-stress") {
            capacityLabel.textContent = "Allowable or limiting stress";
            appliedLabel.textContent = "Working stress";
            appliedGroup.classList.remove("hidden");
            fosInputGroup.classList.add("hidden");
          } else if (mode === "allowable-load") {
            capacityLabel.textContent = "Capacity (ultimate or allowable load)";
            appliedGroup.classList.add("hidden");
            fosInputGroup.classList.remove("hidden");
          } else if (mode === "allowable-stress") {
            capacityLabel.textContent =
              "Limiting stress (e.g., yield, ultimate)";
            appliedGroup.classList.add("hidden");
            fosInputGroup.classList.remove("hidden");
          }

          statusBadge.classList.add("hidden");
          resultCard.classList.add("hidden");
        }

        modeButtons.forEach((btn) => {
          btn.addEventListener("click", () => setMode(btn.dataset.mode));
        });

        function classifyFoS(fos) {
          if (!isFinite(fos) || fos <= 0)
            return {
              label: "Invalid",
              color: "bg-red-100 text-red-700 border-red-300",
            };
          if (fos < 1)
            return {
              label: "Unsafe (FoS < 1)",
              color: "bg-red-100 text-red-700 border-red-300",
            };
          if (fos < 1.3)
            return {
              label: "Very low margin",
              color: "bg-orange-100 text-orange-700 border-orange-300",
            };
          if (fos <= 3)
            return {
              label: "Typical engineering range",
              color: "bg-green-100 text-green-700 border-green-300",
            };
          return {
            label: "High margin",
            color: "bg-blue-100 text-blue-700 border-blue-300",
          };
        }

        document
          .getElementById("calculate-btn")
          .addEventListener("click", () => {
            const capacity = parseFloat(
              document.getElementById("capacity").value
            );
            const applied = parseFloat(
              document.getElementById("applied").value
            );
            const fosInput = parseFloat(
              document.getElementById("fos-input").value
            );
            const capUnit =
              document.getElementById("capacity-unit").value || "";
            const appUnit = document.getElementById("applied-unit").value || "";

            statusBadge.classList.add("hidden");
            resultCard.classList.add("hidden");

            function showError(msg) {
              statusBadge.textContent = msg;
              statusBadge.className =
                "badge bg-red-100 text-red-700 border border-red-300";
              statusBadge.classList.remove("hidden");
            }

            if (currentMode === "fos-load" || currentMode === "fos-stress") {
              if (!(capacity > 0) || !(applied > 0)) {
                showError(
                  "Enter positive values for both capacity and applied value."
                );
                return;
              }
              const fos = capacity / applied;
              const cls = classifyFoS(fos);

              statusBadge.textContent = cls.label;
              statusBadge.className = "badge " + cls.color + " border";
              statusBadge.classList.remove("hidden");

              resultMain.textContent = `Factor of safety = ${fos.toFixed(3)}`;
              resultSecondary.textContent = `Capacity: ${capacity} ${capUnit.trim()}  |  Demand: ${applied} ${appUnit.trim()}`;
              let interp = "";
              if (fos < 1) {
                interp =
                  "The demand exceeds the capacity. This indicates an unsafe design under the given assumptions.";
              } else if (fos < 1.3) {
                interp =
                  "Very small safety margin. Check whether this is acceptable for your code and application.";
              } else if (fos <= 3) {
                interp =
                  "This is within a typical range for many engineering applications, but you must still verify against the relevant design code.";
              } else {
                interp =
                  "High factor of safety. This may be intentional (e.g., high uncertainty or critical system) but can also indicate overdesign.";
              }
              resultInterpretation.textContent = interp;
              resultCard.classList.remove("hidden");
            } else if (currentMode === "allowable-load") {
              if (!(capacity > 0) || !(fosInput > 0)) {
                showError("Enter positive values for capacity and target FoS.");
                return;
              }
              const allowableLoad = capacity / fosInput;
              const cls = classifyFoS(fosInput);

              statusBadge.textContent = `Target FoS = ${fosInput.toFixed(3)} (${
                cls.label
              })`;
              statusBadge.className = "badge " + cls.color + " border";
              statusBadge.classList.remove("hidden");

              resultMain.textContent = `Allowable load = ${allowableLoad.toFixed(
                3
              )} ${capUnit.trim()}`;
              resultSecondary.textContent = `Capacity: ${capacity} ${capUnit.trim()}  |  Target FoS: ${fosInput.toFixed(
                3
              )}`;
              resultInterpretation.textContent =
                "If the applied load does not exceed this allowable value, the target factor of safety will be satisfied (under the assumptions used).";
              resultCard.classList.remove("hidden");
            } else if (currentMode === "allowable-stress") {
              if (!(capacity > 0) || !(fosInput > 0)) {
                showError(
                  "Enter positive values for limiting stress and target FoS."
                );
                return;
              }
              const allowableStress = capacity / fosInput;
              const cls = classifyFoS(fosInput);

              statusBadge.textContent = `Target FoS = ${fosInput.toFixed(3)} (${
                cls.label
              })`;
              statusBadge.className = "badge " + cls.color + " border";
              statusBadge.classList.remove("hidden");

              resultMain.textContent = `Allowable stress = ${allowableStress.toFixed(
                3
              )} ${capUnit.trim()}`;
              resultSecondary.textContent = `Limiting stress: ${capacity} ${capUnit.trim()}  |  Target FoS: ${fosInput.toFixed(
                3
              )}`;
              resultInterpretation.textContent =
                "Design your member so that the working stress does not exceed this allowable stress to maintain the target factor of safety.";
              resultCard.classList.remove("hidden");
            }
          });

        document.getElementById("reset-btn").addEventListener("click", () => {
          document.getElementById("capacity").value = "";
          document.getElementById("applied").value = "";
          document.getElementById("fos-input").value = "";
          statusBadge.classList.add("hidden");
          resultCard.classList.add("hidden");
        });

        // Initialize default mode
        setMode("fos-load");
      })();
    </script>
  
