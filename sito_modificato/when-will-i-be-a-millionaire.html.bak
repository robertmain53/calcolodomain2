    <script>
      (function () {
        const currencySelect = document.getElementById("currency");
        const symbolEls = [
          document.getElementById("currency-symbol"),
          document.getElementById("currency-symbol-monthly"),
          document.getElementById("currency-symbol-target"),
        ];
        const annualReturnInput = document.getElementById("annual-return");
        const annualReturnDisplay = document.getElementById(
          "annual-return-display"
        );
        const inflationInput = document.getElementById("inflation");
        const inflationDisplay = document.getElementById("inflation-display");
        const calculateBtn = document.getElementById("calculate-btn");
        const resetBtn = document.getElementById("reset-btn");

        const resultsCard = document.getElementById("results-card");
        const noSolutionCard = document.getElementById("no-solution-card");
        const statusText = document.getElementById("status-text");
        const millionaireDateEl = document.getElementById("millionaire-date");
        const millionaireAgeEl = document.getElementById("millionaire-age");
        const timeToTargetEl = document.getElementById("time-to-target");
        const timeToTargetDetailEl = document.getElementById(
          "time-to-target-detail"
        );
        const totalContributionsEl = document.getElementById(
          "total-contributions"
        );
        const totalGrowthEl = document.getElementById("total-growth");
        const realTargetValueEl = document.getElementById("real-target-value");
        const realTargetNoteEl = document.getElementById("real-target-note");
        const yearlyTableBody = document.getElementById("yearly-table-body");

        const currentBalanceInput = document.getElementById("current-balance");
        const monthlyContributionInput = document.getElementById(
          "monthly-contribution"
        );
        const targetAmountInput = document.getElementById("target-amount");
        const currentAgeInput = document.getElementById("current-age");
        const contributionFrequencySelect = document.getElementById(
          "contribution-frequency"
        );

        function getCurrencySymbol(code) {
          switch (code) {
            case "EUR":
              return "€";
            case "GBP":
              return "£";
            case "CAD":
            case "AUD":
            case "USD":
              return "$";
            case "INR":
              return "₹";
            default:
              return "$";
          }
        }

        function formatMoney(value, currencyCode) {
          if (!isFinite(value)) return "–";
          try {
            return new Intl.NumberFormat(undefined, {
              style: "currency",
              currency: currencyCode,
              maximumFractionDigits: 0,
            }).format(value);
          } catch (e) {
            // Fallback if currency code not supported
            const symbol = getCurrencySymbol(currencyCode);
            return (
              symbol + value.toFixed(0).replace(/\B(?=(\d{3})+(?!\d))/g, ",")
            );
          }
        }

        function formatYearsMonths(totalMonths) {
          const years = Math.floor(totalMonths / 12);
          const months = totalMonths % 12;
          const parts = [];
          if (years > 0) parts.push(years + " year" + (years !== 1 ? "s" : ""));
          if (months > 0)
            parts.push(months + " month" + (months !== 1 ? "s" : ""));
          return parts.length ? parts.join(" ") : "0 months";
        }

        function updateRangeDisplays() {
          annualReturnDisplay.textContent = parseFloat(
            annualReturnInput.value
          ).toFixed(1);
          inflationDisplay.textContent = parseFloat(
            inflationInput.value
          ).toFixed(1);
        }

        function updateCurrencySymbols() {
          const symbol = getCurrencySymbol(currencySelect.value);
          symbolEls.forEach((el) => {
            if (el) el.textContent = symbol;
          });
        }

        function clearResults() {
          resultsCard.classList.add("hidden");
          noSolutionCard.classList.add("hidden");
          yearlyTableBody.innerHTML = "";
        }

        function calculateMillionaireDate() {
          clearResults();

          const currencyCode = currencySelect.value || "USD";
          const P0 = parseFloat(currentBalanceInput.value) || 0;
          let contribution = parseFloat(monthlyContributionInput.value) || 0;
          const target = parseFloat(targetAmountInput.value) || 0;
          const annualReturn = parseFloat(annualReturnInput.value) / 100;
          const inflation = parseFloat(inflationInput.value) / 100;
          const currentAge = parseFloat(currentAgeInput.value);
          const freq = contributionFrequencySelect.value;

          if (target <= 0) {
            alert("Please enter a positive target amount.");
            return;
          }
          if (annualReturn < 0 || annualReturn > 1) {
            alert("Please enter a realistic annual return (0%–100%).");
            return;
          }

          let periodsPerYear;
          if (freq === "monthly") {
            periodsPerYear = 12;
          } else if (freq === "biweekly") {
            periodsPerYear = 26;
          } else {
            periodsPerYear = 1;
          }

          // Normalize contribution to per-period
          let contributionPerPeriod;
          if (freq === "monthly") {
            contributionPerPeriod = contribution;
          } else if (freq === "biweekly") {
            // User enters monthly; convert approx to bi-weekly
            contributionPerPeriod = (contribution * 12) / 26;
          } else {
            // yearly
            contributionPerPeriod = contribution * 12;
          }

          const i = annualReturn / periodsPerYear;
          const maxYears = 100;
          const maxPeriods = maxYears * periodsPerYear;

          let balance = P0;
          let totalContributions = 0;
          let totalGrowth = 0;
          let reached = false;
          let reachedPeriod = 0;

          const now = new Date();
          const startYear = now.getFullYear();
          let currentYear = startYear;
          let periodsThisYear = 0;
          let contributionsThisYear = 0;
          let growthThisYear = 0;
          let lastYearBalance = P0;

          yearlyTableBody.innerHTML = "";

          for (let n = 1; n <= maxPeriods; n++) {
            // Apply growth
            const beforeGrowth = balance;
            balance *= 1 + i;
            const growth = balance - beforeGrowth;

            // Contribution at end of period
            balance += contributionPerPeriod;
            totalContributions += contributionPerPeriod;
            totalGrowth += growth;

            periodsThisYear++;
            contributionsThisYear += contributionPerPeriod;
            growthThisYear += growth;

            const isYearEnd =
              periodsThisYear === periodsPerYear || n === maxPeriods;

            if (isYearEnd) {
              const yearIndex = Math.round(n / periodsPerYear);
              const ageThisYear = isFinite(currentAge)
                ? currentAge + yearIndex
                : null;

              const row = document.createElement("tr");
              row.innerHTML = `
          <td class="px-2 py-1 text-left text-gray-700">${currentYear}</td>
          <td class="px-2 py-1 text-right text-gray-700">${
            ageThisYear != null ? ageThisYear.toFixed(0) : "–"
          }</td>
          <td class="px-2 py-1 text-right text-gray-700">${formatMoney(
            balance,
            currencyCode
          )}</td>
          <td class="px-2 py-1 text-right text-gray-700">${formatMoney(
            contributionsThisYear,
            currencyCode
          )}</td>
          <td class="px-2 py-1 text-right text-gray-700">${formatMoney(
            growthThisYear,
            currencyCode
          )}</td>
        `;
              yearlyTableBody.appendChild(row);

              currentYear++;
              periodsThisYear = 0;
              contributionsThisYear = 0;
              growthThisYear = 0;
              lastYearBalance = balance;
            }

            if (balance >= target) {
              reached = true;
              reachedPeriod = n;
              break;
            }
          }

          if (!reached) {
            noSolutionCard.classList.remove("hidden");
            return;
          }

          const yearsToTarget = reachedPeriod / periodsPerYear;
          const monthsToTarget = Math.round(yearsToTarget * 12);

          const targetDate = new Date(now);
          targetDate.setMonth(targetDate.getMonth() + monthsToTarget);

          const options = { year: "numeric", month: "long" };
          const dateStr = targetDate.toLocaleDateString(undefined, options);

          millionaireDateEl.textContent = dateStr;
          timeToTargetEl.textContent = formatYearsMonths(monthsToTarget);
          timeToTargetDetailEl.textContent = `Approximately ${yearsToTarget.toFixed(
            1
          )} years from now.`;

          if (isFinite(currentAge)) {
            const ageAtTarget = currentAge + yearsToTarget;
            millionaireAgeEl.textContent = `You’ll be about age ${ageAtTarget.toFixed(
              0
            )}.`;
          } else {
            millionaireAgeEl.textContent =
              "Enter your age above to see how old you’ll be.";
          }

          totalContributionsEl.textContent = formatMoney(
            totalContributions,
            currencyCode
          );
          totalGrowthEl.textContent = formatMoney(
            balance - P0 - totalContributions,
            currencyCode
          );

          // Real value of target
          const realValue =
            target / Math.pow(1 + inflation, yearsToTarget || 0);
          realTargetValueEl.textContent = formatMoney(realValue, currencyCode);
          realTargetNoteEl.textContent = `What ${formatMoney(
            target,
            currencyCode
          )} will feel like in today’s money at ${(inflation * 100).toFixed(
            1
          )}% inflation.`;

          statusText.textContent = `Based on your inputs, you’re on track to reach ${formatMoney(
            target,
            currencyCode
          )} in about ${formatYearsMonths(monthsToTarget)}.`;

          resultsCard.classList.remove("hidden");
        }

        function resetForm() {
          currencySelect.value = "USD";
          currentBalanceInput.value = 20000;
          monthlyContributionInput.value = 1000;
          targetAmountInput.value = 1000000;
          annualReturnInput.value = 7;
          inflationInput.value = 2.5;
          currentAgeInput.value = "";
          contributionFrequencySelect.value = "monthly";
          updateCurrencySymbols();
          updateRangeDisplays();
          clearResults();
        }

        annualReturnInput.addEventListener("input", updateRangeDisplays);
        inflationInput.addEventListener("input", updateRangeDisplays);
        currencySelect.addEventListener("change", updateCurrencySymbols);
        calculateBtn.addEventListener("click", calculateMillionaireDate);
        resetBtn.addEventListener("click", resetForm);

        // Initialize
        updateCurrencySymbols();
        updateRangeDisplays();
      })();
    </script>
  
