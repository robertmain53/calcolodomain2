    <script>
      // Utility functions
      function roundTo(value, decimals) {
        const factor = Math.pow(10, decimals);
        return Math.round((value + Number.EPSILON) * factor) / factor;
      }

      function showError(msg) {
        document.getElementById("error-msg").textContent = msg || "";
      }

      function setMode(mode) {
        document.querySelectorAll(".mode-btn").forEach((btn) => {
          if (btn.dataset.mode === mode) {
            btn.classList.remove(
              "border-gray-300",
              "bg-white",
              "text-gray-700"
            );
            btn.classList.add(
              "border-blue-500",
              "bg-blue-50",
              "text-blue-700",
              "font-semibold"
            );
          } else {
            btn.classList.remove(
              "border-blue-500",
              "bg-blue-50",
              "text-blue-700",
              "font-semibold"
            );
            btn.classList.add("border-gray-300", "bg-white", "text-gray-700");
          }
        });

        const slcOptions = document.getElementById("slc-options");
        const sheetOptions = document.getElementById("sheet-options");
        const thkBlock = document.getElementById("res-thickness-block");

        if (mode === "sheet") {
          slcOptions.classList.add("hidden");
          sheetOptions.classList.remove("hidden");
          thkBlock.classList.add("hidden");
        } else {
          slcOptions.classList.remove("hidden");
          sheetOptions.classList.add("hidden");
          thkBlock.classList.remove("hidden");
        }

        document.getElementById("results-content").classList.add("hidden");
        showError("");
      }

      function setLengthUnit(unit) {
        const dimsImperial = document.getElementById("dims-imperial");
        const dimsMetric = document.getElementById("dims-metric");
        document.querySelectorAll(".len-unit-btn").forEach((btn) => {
          if (btn.dataset.lenUnit === unit) {
            btn.classList.add("bg-blue-600", "text-white");
            btn.classList.remove("bg-white", "text-gray-700");
          } else {
            btn.classList.remove("bg-blue-600", "text-white");
            btn.classList.add("bg-white", "text-gray-700");
          }
        });
        if (unit === "imperial") {
          dimsImperial.classList.remove("hidden");
          dimsMetric.classList.add("hidden");
        } else {
          dimsImperial.classList.add("hidden");
          dimsMetric.classList.remove("hidden");
        }
        document.getElementById("results-content").classList.add("hidden");
        showError("");
      }

      function setThicknessUnit(unit) {
        document.querySelectorAll(".thk-unit-btn").forEach((btn) => {
          if (btn.dataset.thkUnit === unit) {
            btn.classList.add("bg-blue-600", "text-white");
            btn.classList.remove("bg-white", "text-gray-700");
          } else {
            btn.classList.remove("bg-blue-600", "text-white");
            btn.classList.add("bg-white", "text-gray-700");
          }
        });
        document.getElementById("results-content").classList.add("hidden");
        showError("");
      }

      function getSelectedMode() {
        const active = Array.from(document.querySelectorAll(".mode-btn")).find(
          (btn) => btn.classList.contains("border-blue-500")
        );
        return active ? active.dataset.mode : "selflevel";
      }

      function getSelectedLenUnit() {
        const active = Array.from(
          document.querySelectorAll(".len-unit-btn")
        ).find((btn) => btn.classList.contains("bg-blue-600"));
        return active ? active.dataset.lenUnit : "imperial";
      }

      function getSelectedThkUnit() {
        const active = Array.from(
          document.querySelectorAll(".thk-unit-btn")
        ).find((btn) => btn.classList.contains("bg-blue-600"));
        return active ? active.dataset.thkUnit : "inch";
      }

      function parseNumber(id) {
        const el = document.getElementById(id);
        if (!el) return NaN;
        const v = parseFloat(el.value);
        return isNaN(v) ? NaN : v;
      }

      function resetCalculator() {
        document
          .querySelectorAll('input[type="number"], input[type="text"]')
          .forEach((input) => {
            if (input.id === "waste-pct") {
              input.value = 10;
            } else if (input.id === "currency-symbol") {
              input.value = "$";
            } else {
              input.value = "";
            }
          });
        document.getElementById("bag-unit").value = "ft2";
        document.getElementById("roll-unit").value = "ft";
        setMode("selflevel");
        setLengthUnit("imperial");
        setThicknessUnit("inch");
        document.getElementById("results-content").classList.add("hidden");
        document.getElementById("res-notes").textContent = "";
        showError("");
      }

      function calculate() {
        showError("");
        const mode = getSelectedMode();
        const lenUnit = getSelectedLenUnit();
        const thkUnit = getSelectedThkUnit();

        // 1. Area
        let areaFt2 = 0;
        let areaM2 = 0;

        if (lenUnit === "imperial") {
          const lf = parseNumber("length-ft") || 0;
          const li = parseNumber("length-in") || 0;
          const wf = parseNumber("width-ft") || 0;
          const wi = parseNumber("width-in") || 0;
          const lengthFt = lf + li / 12;
          const widthFt = wf + wi / 12;
          if (lengthFt <= 0 || widthFt <= 0) {
            showError("Please enter a positive length and width.");
            return;
          }
          areaFt2 = lengthFt * widthFt;
          areaM2 = areaFt2 * 0.092903;
        } else {
          const lm = parseNumber("length-m") || 0;
          const lcm = parseNumber("length-cm") || 0;
          const wm = parseNumber("width-m") || 0;
          const wcm = parseNumber("width-cm") || 0;
          const lengthM = lm + lcm / 100;
          const widthM = wm + wcm / 100;
          if (lengthM <= 0 || widthM <= 0) {
            showError("Please enter a positive length and width.");
            return;
          }
          areaM2 = lengthM * widthM;
          areaFt2 = areaM2 / 0.092903;
        }

        const wastePct = parseNumber("waste-pct");
        if (isNaN(wastePct) || wastePct < 0) {
          showError("Please enter a valid waste percentage (0 or higher).");
          return;
        }
        const factor = 1 + wastePct / 100;
        const effAreaFt2 = areaFt2 * factor;
        const effAreaM2 = areaM2 * factor;

        let unitsRequired = 0;
        let coveragePerUnitFt2 = 0;
        let coveragePerUnitM2 = 0;
        let notes = "";

        if (mode === "sheet") {
          // Sheet / roll mode
          const rollLen = parseNumber("roll-length");
          const rollWid = parseNumber("roll-width");
          const rollUnit = document.getElementById("roll-unit").value;

          if (!rollLen || !rollWid || rollLen <= 0 || rollWid <= 0) {
            showError("Please enter a positive roll length and width.");
            return;
          }

          let rollAreaFt2;
          if (rollUnit === "ft") {
            rollAreaFt2 = rollLen * rollWid;
          } else {
            const rollAreaM2 = rollLen * rollWid;
            rollAreaFt2 = rollAreaM2 / 0.092903;
          }

          if (rollAreaFt2 <= 0) {
            showError("Roll area must be greater than zero.");
            return;
          }

          unitsRequired = effAreaFt2 / rollAreaFt2;
          coveragePerUnitFt2 = rollAreaFt2;
          coveragePerUnitM2 = rollAreaFt2 * 0.092903;
          notes =
            "Calculated as rolls/sheets of underlayment based on roll dimensions and total area with waste.";
        } else {
          // Self-leveling or patch mode
          const minThk = parseNumber("min-thk");
          const maxThk = parseNumber("max-thk");
          const avgThk = parseNumber("avg-thk");
          const bagCoverage = parseNumber("bag-coverage");
          const bagThkRef = parseNumber("bag-thk-ref");
          const bagUnit = document.getElementById("bag-unit").value;

          if (!avgThk || avgThk <= 0) {
            showError("Please enter a positive average thickness.");
            return;
          }
          if (
            !bagCoverage ||
            bagCoverage <= 0 ||
            !bagThkRef ||
            bagThkRef <= 0
          ) {
            showError(
              "Please enter valid product coverage and reference thickness."
            );
            return;
          }

          // Convert thickness to inches for display, but coverage scaling uses same units as entered
          let avgThkIn, minThkIn, maxThkIn;
          if (thkUnit === "inch") {
            avgThkIn = avgThk;
            minThkIn = isNaN(minThk) ? NaN : minThk;
            maxThkIn = isNaN(maxThk) ? NaN : maxThk;
          } else {
            avgThkIn = avgThk / 25.4;
            minThkIn = isNaN(minThk) ? NaN : minThk / 25.4;
            maxThkIn = isNaN(maxThk) ? NaN : maxThk / 25.4;
          }

          // Coverage scaling
          const coverageAtAvg = bagCoverage * (bagThkRef / avgThk);
          if (coverageAtAvg <= 0) {
            showError(
              "Calculated coverage at average thickness is invalid. Check your inputs."
            );
            return;
          }

          if (bagUnit === "ft2") {
            coveragePerUnitFt2 = coverageAtAvg;
            coveragePerUnitM2 = coverageAtAvg * 0.092903;
            unitsRequired = effAreaFt2 / coverageAtAvg;
          } else {
            coveragePerUnitM2 = coverageAtAvg;
            coveragePerUnitFt2 = coverageAtAvg / 0.092903;
            unitsRequired = effAreaM2 / coverageAtAvg;
          }

          notes =
            "Coverage scaled from manufacturer data using reference thickness and your average pour thickness.";
        }

        if (unitsRequired <= 0 || !isFinite(unitsRequired)) {
          showError(
            "Unable to compute a valid quantity. Please review your inputs."
          );
          return;
        }

        const unitsRounded = Math.ceil(unitsRequired);

        const unitCost = parseNumber("unit-cost");
        const currency = (
          document.getElementById("currency-symbol").value || "$"
        ).trim();
        let totalCostText = "Not provided";
        if (!isNaN(unitCost) && unitCost > 0) {
          const totalCost = unitsRounded * unitCost;
          totalCostText = `${currency}${totalCost.toFixed(
            2
          )} (at ${currency}${unitCost.toFixed(2)} per unit)`;
        }

        // Populate results
        document.getElementById("res-area").textContent = `${roundTo(
          areaFt2,
          2
        )} ft²  |  ${roundTo(areaM2, 2)} m²`;
        document.getElementById("res-area-waste").textContent = `${roundTo(
          effAreaFt2,
          2
        )} ft²  |  ${roundTo(effAreaM2, 2)} m² (includes ${
          wastePct || 0
        }% waste)`;

        if (mode !== "sheet") {
          const thkUnitLabel = thkUnit === "inch" ? "in" : "mm";
          const avgDisplay = `${roundTo(avgThk, 3)} ${thkUnitLabel}`;
          const minDisplay = isNaN(parseNumber("min-thk"))
            ? "—"
            : `${roundTo(parseNumber("min-thk"), 3)} ${thkUnitLabel}`;
          const maxDisplay = isNaN(parseNumber("max-thk"))
            ? "—"
            : `${roundTo(parseNumber("max-thk"), 3)} ${thkUnitLabel}`;
          document.getElementById("res-avg-thk").textContent = avgDisplay;
          document.getElementById("res-min-thk").textContent = minDisplay;
          document.getElementById("res-max-thk").textContent = maxDisplay;
        }

        const unitLabel = mode === "sheet" ? "rolls / sheets" : "bags";
        document.getElementById(
          "res-units"
        ).textContent = `${unitsRounded} ${unitLabel} (raw calc: ${roundTo(
          unitsRequired,
          2
        )})`;
        document.getElementById("res-coverage").textContent = `${roundTo(
          coveragePerUnitFt2,
          2
        )} ft² per unit  |  ${roundTo(coveragePerUnitM2, 2)} m² per unit`;
        document.getElementById("res-cost").textContent = totalCostText;
        document.getElementById("res-notes").textContent = notes;

        document.getElementById("results-content").classList.remove("hidden");
      }

      document.addEventListener("DOMContentLoaded", () => {
        // Mode buttons
        document.querySelectorAll(".mode-btn").forEach((btn) => {
          btn.addEventListener("click", () => setMode(btn.dataset.mode));
        });

        // Length unit buttons
        document.querySelectorAll(".len-unit-btn").forEach((btn) => {
          btn.addEventListener("click", () =>
            setLengthUnit(btn.dataset.lenUnit)
          );
        });

        // Thickness unit buttons
        document.querySelectorAll(".thk-unit-btn").forEach((btn) => {
          btn.addEventListener("click", () =>
            setThicknessUnit(btn.dataset.thkUnit)
          );
        });

        document
          .getElementById("calc-btn")
          .addEventListener("click", calculate);
        document
          .getElementById("reset-btn")
          .addEventListener("click", resetCalculator);

        // Recalculate on Enter key in any input
        document.querySelectorAll("input, select").forEach((el) => {
          el.addEventListener("keydown", (e) => {
            if (e.key === "Enter") {
              e.preventDefault();
              calculate();
            }
          });
        });

        // Initialize defaults
        setMode("selflevel");
        setLengthUnit("imperial");
        setThicknessUnit("inch");
      });
    </script>

