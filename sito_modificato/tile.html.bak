<script>
  // Utility: format number with fixed decimals and unit
  function fmt(value, decimals, unit) {
    if (isNaN(value) || !isFinite(value)) return '–';
    return value.toFixed(decimals) + (unit ? ' ' + unit : '');
  }

  const state = {
    metric: true
  };

  const els = {
    unitToggle: document.getElementById('unitToggle'),
    resetBtn: document.getElementById('resetBtn'),
    calculateBtn: document.getElementById('calculateBtn'),
    errorMsg: document.getElementById('errorMsg'),
    resultsSection: document.getElementById('resultsSection'),
    wasteSlider: document.getElementById('wasteFactor'),
    wasteLabel: document.getElementById('wasteLabel')
  };

  const inputs = {
    areaLength: document.getElementById('areaLength'),
    areaWidth: document.getElementById('areaWidth'),
    areaTotal: document.getElementById('areaTotal'),
    tileLength: document.getElementById('tileLength'),
    tileWidth: document.getElementById('tileWidth'),
    tileThickness: document.getElementById('tileThickness'),
    jointWidth: document.getElementById('jointWidth'),
    groutDepth: document.getElementById('groutDepth'),
    layoutType: document.getElementById('layoutType'),
    mortarBed: document.getElementById('mortarBed'),
    mortarCoverage: document.getElementById('mortarCoverage'),
    mortarBagWeight: document.getElementById('mortarBagWeight'),
    mortarBagCost: document.getElementById('mortarBagCost'),
    groutCoverage: document.getElementById('groutCoverage'),
    groutBagWeight: document.getElementById('groutBagWeight'),
    groutBagCost: document.getElementById('groutBagCost')
  };

  const units = {
    areaLengthUnit: document.getElementById('areaLengthUnit'),
    areaWidthUnit: document.getElementById('areaWidthUnit'),
    areaTotalUnit: document.getElementById('areaTotalUnit'),
    tileLengthUnit: document.getElementById('tileLengthUnit'),
    tileWidthUnit: document.getElementById('tileWidthUnit'),
    tileThicknessUnit: document.getElementById('tileThicknessUnit'),
    jointWidthUnit: document.getElementById('jointWidthUnit'),
    groutDepthUnit: document.getElementById('groutDepthUnit'),
    mortarBedUnit: document.getElementById('mortarBedUnit'),
    mortarCoverageUnit: document.getElementById('mortarCoverageUnit'),
    mortarBagWeightUnit: document.getElementById('mortarBagWeightUnit'),
    groutCoverageUnit: document.getElementById('groutCoverageUnit'),
    groutBagWeightUnit: document.getElementById('groutBagWeightUnit')
  };

  const results = {
    resAreaWithWaste: document.getElementById('resAreaWithWaste'),
    resMortarCoverage: document.getElementById('resMortarCoverage'),
    resMortarBags: document.getElementById('resMortarBags'),
    resMortarWeight: document.getElementById('resMortarWeight'),
    resMortarCost: document.getElementById('resMortarCost'),
    resGroutFactor: document.getElementById('resGroutFactor'),
    resGroutCoverage: document.getElementById('resGroutCoverage'),
    resGroutBags: document.getElementById('resGroutBags'),
    resGroutWeight: document.getElementById('resGroutWeight'),
    resGroutCost: document.getElementById('resGroutCost')
  };

  function updateWasteLabel() {
    els.wasteLabel.textContent = els.wasteSlider.value + '%';
  }

  function setUnits() {
    if (state.metric) {
      units.areaLengthUnit.textContent = 'm';
      units.areaWidthUnit.textContent = 'm';
      units.areaTotalUnit.textContent = 'm²';
      units.tileLengthUnit.textContent = 'mm';
      units.tileWidthUnit.textContent = 'mm';
      units.tileThicknessUnit.textContent = 'mm';
      units.jointWidthUnit.textContent = 'mm';
      units.groutDepthUnit.textContent = 'mm';
      units.mortarBedUnit.textContent = 'mm';
      units.mortarCoverageUnit.textContent = 'm²/bag';
      units.mortarBagWeightUnit.textContent = 'kg';
      units.groutCoverageUnit.textContent = 'm²/bag';
      units.groutBagWeightUnit.textContent = 'kg';
    } else {
      units.areaLengthUnit.textContent = 'ft';
      units.areaWidthUnit.textContent = 'ft';
      units.areaTotalUnit.textContent = 'ft²';
      units.tileLengthUnit.textContent = 'in';
      units.tileWidthUnit.textContent = 'in';
      units.tileThicknessUnit.textContent = 'in';
      units.jointWidthUnit.textContent = 'in';
      units.groutDepthUnit.textContent = 'in';
      units.mortarBedUnit.textContent = 'in';
      units.mortarCoverageUnit.textContent = 'ft²/bag';
      units.mortarBagWeightUnit.textContent = 'lb';
      units.groutCoverageUnit.textContent = 'ft²/bag';
      units.groutBagWeightUnit.textContent = 'lb';
    }
  }

  function resetForm() {
    document.getElementById('tileCalculator').reset();
    // Restore defaults
    inputs.tileLength.value = 600;
    inputs.tileWidth.value = 300;
    inputs.tileThickness.value = 10;
    inputs.jointWidth.value = 3;
    inputs.mortarBed.value = 4;
    inputs.mortarCoverage.value = 5;
    inputs.mortarBagWeight.value = 25;
    inputs.groutCoverage.value = 10;
    inputs.groutBagWeight.value = 10;
    els.wasteSlider.value = 10;
    updateWasteLabel();
    els.errorMsg.classList.add('hidden');
    els.resultsSection.classList.add('hidden');
  }

  function calculate() {
    els.errorMsg.classList.add('hidden');

    const L = parseFloat(inputs.areaLength.value);
    const W = parseFloat(inputs.areaWidth.value);
    const Auser = parseFloat(inputs.areaTotal.value);

    let area = !isNaN(Auser) && Auser > 0 ? Auser : ( (L > 0 && W > 0) ? L * W : NaN );

    if (isNaN(area) || area <= 0) {
      els.errorMsg.textContent = 'Please enter a valid area (length & width or total area).';
      els.errorMsg.classList.remove('hidden');
      els.resultsSection.classList.add('hidden');
      return;
    }

    const wastePct = parseFloat(els.wasteSlider.value) || 0;
    const areaWithWaste = area * (1 + wastePct / 100);

    const layout = inputs.layoutType.value;
    let layoutFactor = 1.0;
    if (layout === 'brick') layoutFactor = 1.05;
    if (layout === 'diagonal') layoutFactor = 1.10;

    const mortarCoverage = parseFloat(inputs.mortarCoverage.value);
    const groutCoverage = parseFloat(inputs.groutCoverage.value);

    if (isNaN(mortarCoverage) || mortarCoverage <= 0 || isNaN(groutCoverage) || groutCoverage <= 0) {
      els.errorMsg.textContent = 'Please enter valid coverage values for mortar and grout.';
      els.errorMsg.classList.remove('hidden');
      els.resultsSection.classList.add('hidden');
      return;
    }

    // Mortar calculations
    const mortarBagsRaw = areaWithWaste / mortarCoverage;
    const mortarBags = Math.ceil(mortarBagsRaw);
    const mortarBagWeight = parseFloat(inputs.mortarBagWeight.value) || 0;
    const mortarTotalWeight = mortarBags * mortarBagWeight;
    const mortarBagCost = parseFloat(inputs.mortarBagCost.value) || 0;
    const mortarCost = mortarBags * mortarBagCost;

    // Grout calculations
    const effectiveGroutCoverage = groutCoverage / layoutFactor;
    const groutBagsRaw = areaWithWaste / effectiveGroutCoverage;
    const groutBags = Math.ceil(groutBagsRaw);
    const groutBagWeight = parseFloat(inputs.groutBagWeight.value) || 0;
    const groutTotalWeight = groutBags * groutBagWeight;
    const groutBagCost = parseFloat(inputs.groutBagCost.value) || 0;
    const groutCost = groutBags * groutBagCost;

    // Units for display
    const areaUnit = state.metric ? 'm²' : 'ft²';
    const weightUnit = state.metric ? 'kg' : 'lb';

    results.resAreaWithWaste.textContent = fmt(areaWithWaste, 2, areaUnit);
    results.resMortarCoverage.textContent = fmt(mortarCoverage, 2, areaUnit + '/bag');
    results.resMortarBags.textContent = isNaN(mortarBags) ? '–' : mortarBags + ' bag' + (mortarBags !== 1 ? 's' : '');
    results.resMortarWeight.textContent = mortarTotalWeight > 0 ? fmt(mortarTotalWeight, 1, weightUnit) : '–';
    results.resMortarCost.textContent = mortarCost > 0 ? '$' + mortarCost.toFixed(2) : '–';

    results.resGroutFactor.textContent = layoutFactor.toFixed(2) + '× vs straight layout';
    results.resGroutCoverage.textContent = fmt(effectiveGroutCoverage, 2, areaUnit + '/bag');
    results.resGroutBags.textContent = isNaN(groutBags) ? '–' : groutBags + ' bag' + (groutBags !== 1 ? 's' : '');
    results.resGroutWeight.textContent = groutTotalWeight > 0 ? fmt(groutTotalWeight, 1, weightUnit) : '–';
    results.resGroutCost.textContent = groutCost > 0 ? '$' + groutCost.toFixed(2) : '–';

    els.resultsSection.classList.remove('hidden');
  }

  // Event listeners
  els.unitToggle.addEventListener('change', () => {
    state.metric = !els.unitToggle.checked; // unchecked = metric
    setUnits();
  });

  els.resetBtn.addEventListener('click', (e) => {
    e.preventDefault();
    resetForm();
  });

  els.calculateBtn.addEventListener('click', (e) => {
    e.preventDefault();
    calculate();
  });

  els.wasteSlider.addEventListener('input', updateWasteLabel);

  // Init
  state.metric = true;
  els.unitToggle.checked = false;
  setUnits();
  updateWasteLabel();
</script>

