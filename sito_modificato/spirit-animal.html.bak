      function selectQuestionSet(mode) {
        const total = questions.length; // 16
        if (mode === "short") {
          // pick 8 roughly evenly spaced
          const indices = [0, 2, 4, 6, 8, 10, 12, 14];
          return indices.map((i) => questions[i]);
        } else if (mode === "deep") {
          // use all questions plus duplicates of some key ones (here just all)
          return [...questions];
        }
        return [...questions]; // standard
      }

      function renderQuestion() {
        const q = activeQuestions[currentIndex];
        const total = activeQuestions.length;
        document.getElementById("question-text").textContent = q.text;

        const answersContainer = document.getElementById("answers-container");
        answersContainer.innerHTML = "";
        q.answers.forEach((ans, idx) => {
          const btn = document.createElement("button");
          btn.type = "button";
          btn.className =
            "answer-option w-full text-left border border-gray-200 rounded-md px-3 py-2 text-sm hover:border-blue-400 hover:bg-blue-50";
          btn.dataset.index = idx;
          btn.innerHTML = ans.text;
          if (answersChosen[currentIndex] === idx) {
            btn.classList.add("answer-selected");
          }
          btn.addEventListener("click", () => {
            answersChosen[currentIndex] = idx;
            document
              .querySelectorAll(".answer-option")
              .forEach((el) => el.classList.remove("answer-selected"));
            btn.classList.add("answer-selected");
            updateNavButtons();
          });
          answersContainer.appendChild(btn);
        });

        // Progress
        const progressLabel = document.getElementById("quiz-progress-label");
        const progressPercent = document.getElementById(
          "quiz-progress-percent"
        );
        const progressBar = document.getElementById("quiz-progress-bar");
        const answeredCount = answersChosen.filter(
          (a) => a !== undefined
        ).length;
        const pct = Math.round((answeredCount / total) * 100);
        progressLabel.textContent = `Question ${currentIndex + 1} of ${total}`;
        progressPercent.textContent = `${pct}%`;
        progressBar.style.width = `${pct}%`;

        updateNavButtons();
      }

      function updateNavButtons() {
        const prevBtn = document.getElementById("prev-question-btn");
        const nextBtn = document.getElementById("next-question-btn");
        prevBtn.disabled = currentIndex === 0;
        const isLast = currentIndex === activeQuestions.length - 1;
        const hasAnswer = answersChosen[currentIndex] !== undefined;
        nextBtn.disabled = !hasAnswer;
        nextBtn.textContent = isLast ? "See my spirit animal" : "Next";
      }

      function computeResult() {
        initScores();
        activeQuestions.forEach((q, qIdx) => {
          const ansIdx = answersChosen[qIdx];
          if (ansIdx === undefined) return;
          const ans = q.answers[ansIdx];
          Object.entries(ans.weights).forEach(([animalKey, value]) => {
            if (scores[animalKey] !== undefined) {
              scores[animalKey] += value;
            }
          });
        });

        const entries = Object.entries(scores).sort((a, b) => b[1] - a[1]);
        const top = entries[0];
        if (!top || top[1] === 0) return null;

        const maxScore = top[1];
        const totalScore = entries.reduce((sum, [, v]) => sum + v, 0) || 1;
        const primaryKey = top[0];

        // Secondary: next two with at least 60% of primary score
        const secondary = entries
          .slice(1)
          .filter(([, v]) => v >= 0.6 * maxScore && v > 0)
          .slice(0, 2);

        return {
          primaryKey,
          primaryScore: maxScore,
          totalScore,
          secondary,
        };
      }

      function renderResult() {
        const result = computeResult();
        const resultContainer = document.getElementById("result-container");
        const primaryCard = document.getElementById("primary-animal-card");
        const secondaryWrapper = document.getElementById(
          "secondary-animals-wrapper"
        );
        const secondaryContainer = document.getElementById("secondary-animals");

        if (!result) {
          resultContainer.classList.remove("hidden");
          primaryCard.innerHTML =
            "<p class='text-sm text-gray-700'>We couldn't calculate a clear result. Please answer more questions and try again.</p>";
          secondaryWrapper.classList.add("hidden");
          return;
        }

        const primaryData = animals[result.primaryKey];
        document.getElementById("primary-animal-name").textContent =
          primaryData.name;
        const pct = Math.round((result.primaryScore / result.totalScore) * 100);
        document.getElementById(
          "primary-animal-score"
        ).textContent = `Match strength: ${pct}%`;

        document.getElementById("primary-animal-summary").textContent =
          primaryData.summary;

        const traitsList = document.getElementById("primary-animal-traits");
        traitsList.innerHTML = "";
        primaryData.traits.forEach((t) => {
          const li = document.createElement("li");
          li.textContent = t;
          traitsList.appendChild(li);
        });

        const guidanceList = document.getElementById("primary-animal-guidance");
        guidanceList.innerHTML = "";
        primaryData.guidance.forEach((g) => {
          const li = document.createElement("li");
          li.textContent = g;
          guidanceList.appendChild(li);
        });

        // Secondary animals
        secondaryContainer.innerHTML = "";
        if (result.secondary.length > 0) {
          result.secondary.forEach(([key, score]) => {
            const data = animals[key];
            const secPct = Math.round((score / result.totalScore) * 100);
            const card = document.createElement("div");
            card.className = "border border-gray-200 rounded-md p-3 text-sm";
            card.innerHTML = `
          <div class="flex items-center justify-between mb-1">
            <span class="font-semibold text-gray-900">${data.name}</span>
            <span class="text-xs text-gray-500">${secPct}%</span>
          </div>
          <p class="text-gray-700 mb-1">${data.summary}</p>
          <ul class="list-disc list-inside text-gray-700">
            ${data.traits
              .slice(0, 2)
              .map((t) => `<li>${t}</li>`)
              .join("")}
          </ul>
        `;
            secondaryContainer.appendChild(card);
          });
          secondaryWrapper.classList.remove("hidden");
        } else {
          secondaryWrapper.classList.add("hidden");
        }

        resultContainer.scrollIntoView({ behavior: "smooth", block: "start" });
        resultContainer.classList.remove("hidden");
      }

      document
        .getElementById("start-quiz-btn")
        .addEventListener("click", () => {
          const mode = document.getElementById("quiz-length").value;
          activeQuestions = selectQuestionSet(mode);
          currentIndex = 0;
          answersChosen = new Array(activeQuestions.length);
          initScores();
          document.getElementById("quiz-container").classList.remove("hidden");
          document
            .getElementById("quiz-progress-wrapper")
            .classList.remove("hidden");
          document.getElementById("result-container").classList.add("hidden");
          renderQuestion();
        });

      document
        .getElementById("prev-question-btn")
        .addEventListener("click", () => {
          if (currentIndex > 0) {
            currentIndex--;
            renderQuestion();
          }
        });

      document
        .getElementById("next-question-btn")
        .addEventListener("click", () => {
          const isLast = currentIndex === activeQuestions.length - 1;
          if (isLast) {
            renderResult();
          } else {
            currentIndex++;
            renderQuestion();
          }
        });

      document
        .getElementById("retake-quiz-btn")
        .addEventListener("click", () => {
          document.getElementById("start-quiz-btn").click();
        });

      // Mobile menu script (from template)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const expanded = this.getAttribute("aria-expanded") === "true";
          this.setAttribute("aria-expanded", String(!expanded));
          menu.classList.toggle("hidden");
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
