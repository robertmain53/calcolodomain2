    <script>
      // FIRE calculator logic
      function formatCurrency(value) {
        if (!isFinite(value)) return "-";
        const abs = Math.abs(value);
        const options = { maximumFractionDigits: 0 };
        if (abs >= 1e9) {
          return "$" + (value / 1e9).toFixed(2) + "B";
        } else if (abs >= 1e6) {
          return "$" + (value / 1e6).toFixed(2) + "M";
        } else if (abs >= 1e3) {
          return "$" + (value / 1e3).toFixed(1) + "k";
        }
        return "$" + value.toLocaleString(undefined, options);
      }

      function formatYears(y) {
        if (!isFinite(y)) return "-";
        return y.toFixed(1) + " years";
      }

      function clamp(num, min, max) {
        return Math.min(Math.max(num, min), max);
      }

      function calculateFire() {
        const currentAge =
          parseFloat(document.getElementById("current-age").value) || 0;
        const spending =
          parseFloat(document.getElementById("retirement-spending").value) || 0;
        const currentPortfolio =
          parseFloat(document.getElementById("current-portfolio").value) || 0;
        const monthlyContribution =
          parseFloat(document.getElementById("monthly-contribution").value) ||
          0;
        const expectedReturn =
          parseFloat(document.getElementById("expected-return").value) || 0;
        const withdrawalRate =
          parseFloat(document.getElementById("withdrawal-rate").value) || 0;
        const maxYears =
          parseFloat(document.getElementById("max-years").value) || 60;

        const r = expectedReturn / 100; // real annual return
        const wr = withdrawalRate / 100;

        // FIRE number
        let fireNumber = 0;
        if (wr > 0) {
          fireNumber = spending / wr;
        }

        // Savings rate vs target lifestyle
        const annualSavings = monthlyContribution * 12;
        let savingsRate = 0;
        if (spending > 0) {
          savingsRate = annualSavings / spending;
        }

        // Progress toward FIRE
        let progress = 0;
        if (fireNumber > 0) {
          progress = currentPortfolio / fireNumber;
        }

        // Time to FIRE (simulation)
        let years = 0;
        let portfolio = currentPortfolio;
        let reached = false;
        const yearlyContribution = monthlyContribution * 12;

        if (fireNumber > 0 && yearlyContribution > 0 && r >= 0) {
          for (let t = 0; t <= maxYears; t++) {
            if (portfolio >= fireNumber) {
              years = t;
              reached = true;
              break;
            }
            // grow and contribute
            portfolio = portfolio * (1 + r) + yearlyContribution;
          }
          if (!reached && portfolio >= fireNumber) {
            years = maxYears;
            reached = true;
          }
        } else if (
          fireNumber > 0 &&
          yearlyContribution === 0 &&
          r > 0 &&
          currentPortfolio > 0
        ) {
          // No new contributions, just growth
          years = Math.log(fireNumber / currentPortfolio) / Math.log(1 + r);
          reached = years >= 0 && years <= maxYears;
          portfolio = fireNumber;
        }

        // If already at or above FIRE
        if (currentPortfolio >= fireNumber && fireNumber > 0) {
          years = 0;
          reached = true;
          portfolio = currentPortfolio;
        }

        // Update DOM
        document.getElementById("fire-number").textContent =
          fireNumber > 0 ? formatCurrency(Math.round(fireNumber)) : "-";
        document.getElementById("savings-rate").textContent = isFinite(
          savingsRate
        )
          ? (savingsRate * 100).toFixed(1) + "%"
          : "-";
        document.getElementById("progress-percent").textContent = isFinite(
          progress
        )
          ? (progress * 100).toFixed(1) + "%"
          : "-";

        if (reached && isFinite(years) && years >= 0) {
          document.getElementById("years-to-fire").textContent =
            formatYears(years);
          const ageAtFire = currentAge + years;
          document.getElementById("age-at-fire").textContent = isFinite(
            ageAtFire
          )
            ? ageAtFire.toFixed(1) + " years"
            : "-";
          if (years === 0) {
            document.getElementById("fire-status").textContent =
              "You are already at or above your FIRE number based on these assumptions.";
          } else {
            document.getElementById("fire-status").textContent =
              "On this path, you could reach financial independence in about " +
              years.toFixed(1) +
              " years.";
          }
        } else if (fireNumber === 0 || !isFinite(fireNumber)) {
          document.getElementById("years-to-fire").textContent = "-";
          document.getElementById("age-at-fire").textContent = "-";
          document.getElementById("fire-status").textContent =
            "Enter a positive withdrawal rate and retirement spending to compute your FIRE number.";
        } else {
          document.getElementById("years-to-fire").textContent =
            "> " + maxYears + " years";
          document.getElementById("age-at-fire").textContent =
            (currentAge + maxYears).toFixed(1) + "+ years";
          document.getElementById("fire-status").textContent =
            "With these assumptions, you may not reach your FIRE number within the simulated period. Consider increasing savings, reducing target spending, or adjusting your assumptions.";
        }

        // Simple projection for chart (real dollars)
        const chartCurrent = document.getElementById("chart-current");
        const chart5 = document.getElementById("chart-5yr");
        const chart10 = document.getElementById("chart-10yr");
        const chartFire = document.getElementById("chart-fire");

        function futureValue(yearsAhead) {
          if (yearsAhead <= 0) return currentPortfolio;
          if (r === 0) {
            return currentPortfolio + yearlyContribution * yearsAhead;
          }
          return (
            currentPortfolio * Math.pow(1 + r, yearsAhead) +
            yearlyContribution * ((Math.pow(1 + r, yearsAhead) - 1) / r)
          );
        }

        const vNow = currentPortfolio;
        const v5 = futureValue(5);
        const v10 = futureValue(10);
        const vFire =
          reached && isFinite(years) && years > 0
            ? futureValue(years)
            : fireNumber || futureValue(maxYears);

        const maxVal = Math.max(vNow, v5, v10, vFire, fireNumber || 0) || 1;
        function heightFor(v) {
          const ratio = clamp(v / maxVal, 0, 1);
          return 20 + ratio * 80 + "%"; // min 20% height
        }

        chartCurrent.style.height = heightFor(vNow);
        chart5.style.height = heightFor(v5);
        chart10.style.height = heightFor(v10);
        chartFire.style.height = heightFor(vFire);
      }

      document.addEventListener("DOMContentLoaded", function () {
        const expectedReturnInput = document.getElementById("expected-return");
        const expectedReturnValue = document.getElementById(
          "expected-return-value"
        );
        const withdrawalRateInput = document.getElementById("withdrawal-rate");
        const withdrawalRateValue = document.getElementById(
          "withdrawal-rate-value"
        );
        const inflationRateInput = document.getElementById("inflation-rate");
        const inflationRateValue = document.getElementById(
          "inflation-rate-value"
        );
        const calcBtn = document.getElementById("calculate-fire");

        function syncSlider(input, label, suffix) {
          input.addEventListener("input", function () {
            label.textContent = parseFloat(input.value).toFixed(1) + suffix;
            calculateFire();
          });
        }

        syncSlider(expectedReturnInput, expectedReturnValue, "%");
        syncSlider(withdrawalRateInput, withdrawalRateValue, "%");
        syncSlider(inflationRateInput, inflationRateValue, "%");

        calcBtn.addEventListener("click", function (e) {
          e.preventDefault();
          calculateFire();
        });

        // Recalculate on basic input changes for responsiveness
        [
          "current-age",
          "retirement-spending",
          "current-portfolio",
          "monthly-contribution",
          "max-years",
        ].forEach((id) => {
          const el = document.getElementById(id);
          el.addEventListener("input", calculateFire);
        });

        calculateFire();
      });
    </script>

