  <script>
    (function() {
      const baseSize = document.getElementById('baseSize');
      const emInput = document.getElementById('emInput');
      const pxInput = document.getElementById('pxInput');
      const resultPx = document.getElementById('resultPx');
      const resultEm = document.getElementById('resultEm');
      const resultRem = document.getElementById('resultRem');
      const tableBody = document.getElementById('tableBody');
      const pills = document.querySelectorAll('[data-base]');
      let updating = false;

      function toNumber(el) {
        const v = parseFloat(el.value);
        return isNaN(v) ? null : v;
      }

      function format(val, dec) {
        if (val == null || !isFinite(val)) return '—';
        return Number(val.toFixed(dec)).toString();
      }

      function recalcFromEm() {
        if (updating) return;
        updating = true;
        const base = toNumber(baseSize);
        const em = toNumber(emInput);
        if (base && em != null) {
          const px = em * base;
          pxInput.value = format(px, 4);
          resultPx.textContent = format(px, 4) + ' px';
        } else {
          resultPx.textContent = '—';
        }
        // also show em from px if px has value
        const px2 = toNumber(pxInput);
        if (base && px2 != null) {
          const em2 = px2 / base;
          resultEm.textContent = format(em2, 4) + ' em';
        } else {
          resultEm.textContent = '—';
        }
        updating = false;
      }

      function recalcFromPx() {
        if (updating) return;
        updating = true;
        const base = toNumber(baseSize);
        const px = toNumber(pxInput);
        if (base && px != null) {
          const em = px / base;
          emInput.value = format(em, 4);
          resultEm.textContent = format(em, 4) + ' em';
        } else {
          resultEm.textContent = '—';
        }
        // also recompute px from em in case there's an em value
        const em2 = toNumber(emInput);
        if (base && em2 != null) {
          const px2 = em2 * base;
          resultPx.textContent = format(px2, 4) + ' px';
        } else {
          resultPx.textContent = '—';
        }
        updating = false;
      }

      function rebuildTable() {
        const base = toNumber(baseSize) || 16;
        const emVals = [0.25, 0.5, 0.75, 1, 1.25, 1.5, 2];
        tableBody.innerHTML = '';
        emVals.forEach(e => {
          const tr = document.createElement('tr');
          const px = e * base;
          tr.innerHTML = `<td>${e}</td><td>${px}</td>`;
          tableBody.appendChild(tr);
        });
        resultRem.textContent = 'If root is ' + base + 'px, 1rem = ' + base + 'px.';
      }

      baseSize.addEventListener('input', function() {
        rebuildTable();
        recalcFromEm();
      });
      emInput.addEventListener('input', recalcFromEm);
      pxInput.addEventListener('input', recalcFromPx);

      pills.forEach(btn => {
        btn.addEventListener('click', () => {
          baseSize.value = btn.dataset.base;
          rebuildTable();
          recalcFromEm();
        });
      });

      // init
      rebuildTable();
      emInput.value = 1;
      recalcFromEm();
    })();
  </script>

