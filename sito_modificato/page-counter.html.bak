    <script>
      // Utility functions
      function countWords(text) {
        if (!text) return 0;
        const cleaned = text
          .replace(/[\u2018\u2019]/g, "'")
          .replace(/[\u201C\u201D]/g, '"')
          .trim();
        if (!cleaned) return 0;
        const words = cleaned.split(/\s+/).filter((w) => w.length > 0);
        return words.length;
      }

      function countChars(text, excludeSpaces = false) {
        if (!text) return 0;
        if (excludeSpaces) {
          return text.replace(/\s+/g, "").length;
        }
        return text.length;
      }

      function estimateWordsFromChars(chars, avgCharsPerWord) {
        if (!chars || !avgCharsPerWord) return 0;
        return chars / avgCharsPerWord;
      }

      function formatMinutes(minutes) {
        if (!isFinite(minutes) || minutes <= 0) return "0 min";
        const totalSeconds = Math.round(minutes * 60);
        const mins = Math.floor(totalSeconds / 60);
        const secs = totalSeconds % 60;
        if (mins === 0) return secs + " sec";
        if (mins < 60) {
          return secs === 0 ? mins + " min" : mins + " min " + secs + " sec";
        }
        const hours = Math.floor(mins / 60);
        const remMins = mins % 60;
        if (remMins === 0) return hours + " hr";
        return hours + " hr " + remMins + " min";
      }

      function updateFromTextArea() {
        const text = document.getElementById("pc-text-input").value || "";
        const words = countWords(text);
        const chars = countChars(text, false);
        const charsNoSpace = countChars(text, true);

        document.getElementById("pc-live-counts").classList.remove("hidden");
        document.getElementById("pc-live-words").textContent = words;
        document.getElementById("pc-live-chars").textContent = chars;
        document.getElementById("pc-live-chars-nospace").textContent =
          charsNoSpace;

        // Sync to counts tab
        document.getElementById("pc-words-input").value = words || "";
        document.getElementById("pc-chars-input").value = chars || "";
        document.getElementById("pc-chars-nospace-input").value =
          charsNoSpace || "";

        recalcPages();
      }

      function getTotalWords() {
        const activeTab = document.querySelector(".pc-tab-btn.border-blue-600");
        const mode = activeTab ? activeTab.getAttribute("data-tab") : "text";

        if (mode === "text" || mode === "url") {
          const wordsVal = parseInt(
            document.getElementById("pc-words-input").value,
            10
          );
          if (!isNaN(wordsVal) && wordsVal > 0) return wordsVal;
          const text = document.getElementById("pc-text-input").value || "";
          return countWords(text);
        }

        // counts mode
        let words = parseInt(
          document.getElementById("pc-words-input").value,
          10
        );
        if (!isNaN(words) && words > 0) return words;

        const chars = parseInt(
          document.getElementById("pc-chars-input").value,
          10
        );
        const charsNoSpace = parseInt(
          document.getElementById("pc-chars-nospace-input").value,
          10
        );

        if (!isNaN(chars) && chars > 0) {
          // assume ~6 chars per word including spaces
          return Math.round(estimateWordsFromChars(chars, 6));
        }
        if (!isNaN(charsNoSpace) && charsNoSpace > 0) {
          // assume ~5 chars per word without spaces
          return Math.round(estimateWordsFromChars(charsNoSpace, 5));
        }
        return 0;
      }

      function getWordsPerPage() {
        const fontPreset = document.getElementById("pc-font-size").value;
        const customWppInput = document.getElementById("pc-custom-wpp");
        let wpp;

        if (fontPreset === "custom") {
          wpp = parseInt(customWppInput.value, 10);
        } else {
          wpp = parseInt(fontPreset, 10);
        }

        if (!isNaN(wpp) && wpp > 0) return wpp;
        return 300; // default fallback
      }

      function recalcPages() {
        const totalWords = getTotalWords();
        const wpp = getWordsPerPage();
        const roundUp = document.getElementById("pc-round-pages").checked;
        let pages = totalWords / wpp;
        if (!isFinite(pages) || pages < 0) pages = 0;
        const rawPages = pages;
        if (roundUp && pages > 0) {
          pages = Math.ceil(pages * 100) / 100; // keep 2 decimals
        } else {
          pages = Math.round(pages * 100) / 100;
        }

        document.getElementById("pc-pages").textContent = pages
          .toFixed(pages >= 10 ? 1 : 2)
          .replace(/\.0+$/, "");
        document.getElementById("pc-pages-detail").textContent =
          totalWords + " words · " + wpp + " words/page";

        // Reading & speaking time
        const readingSpeed =
          parseInt(document.getElementById("pc-reading-speed").value, 10) ||
          220;
        const speakingSpeed =
          parseInt(document.getElementById("pc-speaking-speed").value, 10) ||
          130;

        const readingMinutes = totalWords > 0 ? totalWords / readingSpeed : 0;
        const speakingMinutes = totalWords > 0 ? totalWords / speakingSpeed : 0;

        document.getElementById("pc-reading-time").textContent =
          formatMinutes(readingMinutes);
        document.getElementById("pc-speaking-time").textContent =
          formatMinutes(speakingMinutes);

        document.getElementById("pc-reading-detail").textContent =
          totalWords + " words at " + readingSpeed + " wpm";
        document.getElementById("pc-speaking-detail").textContent =
          totalWords + " words at " + speakingSpeed + " wpm";
      }

      function switchTab(tabName) {
        document.querySelectorAll(".pc-tab-btn").forEach((btn) => {
          if (btn.getAttribute("data-tab") === tabName) {
            btn.classList.add("border-blue-600", "text-blue-600");
            btn.classList.remove("border-transparent", "text-gray-600");
          } else {
            btn.classList.remove("border-blue-600", "text-blue-600");
            btn.classList.add("border-transparent", "text-gray-600");
          }
        });

        document.querySelectorAll(".pc-tab-panel").forEach((panel) => {
          if (panel.id === "pc-tab-" + tabName) {
            panel.classList.remove("hidden");
          } else {
            panel.classList.add("hidden");
          }
        });

        recalcPages();
      }

      async function fetchUrlContent(url) {
        const statusEl = document.getElementById("pc-url-status");
        statusEl.textContent = "Fetching content...";
        statusEl.classList.remove("text-red-600");
        statusEl.classList.add("text-gray-500");

        try {
          const response = await fetch(url, { mode: "cors" });
          if (!response.ok) {
            throw new Error("HTTP " + response.status);
          }
          const html = await response.text();
          // Basic text extraction: strip tags and scripts/styles
          const tmp = document.createElement("div");
          tmp.innerHTML = html;
          // Remove script and style
          tmp
            .querySelectorAll("script, style, noscript")
            .forEach((el) => el.remove());
          const text = tmp.textContent || tmp.innerText || "";
          const cleaned = text.replace(/\s+/g, " ").trim();

          if (!cleaned) {
            throw new Error("No readable text found");
          }

          document.getElementById("pc-text-input").value = cleaned;
          updateFromTextArea();

          statusEl.textContent =
            "Content fetched successfully. Text copied into the Text tab.";
          statusEl.classList.remove("text-gray-500");
          statusEl.classList.add("text-green-600");

          // Switch to text tab for clarity
          switchTab("text");
        } catch (err) {
          statusEl.textContent =
            "Could not fetch content. Some sites block cross-origin requests. Please copy-paste the text instead.";
          statusEl.classList.remove("text-gray-500");
          statusEl.classList.add("text-red-600");
          console.error(err);
        }
      }

      function copySummary() {
        const pages = document.getElementById("pc-pages").textContent;
        const pagesDetail =
          document.getElementById("pc-pages-detail").textContent;
        const reading = document.getElementById("pc-reading-time").textContent;
        const readingDetail =
          document.getElementById("pc-reading-detail").textContent;
        const speaking =
          document.getElementById("pc-speaking-time").textContent;
        const speakingDetail =
          document.getElementById("pc-speaking-detail").textContent;

        const summary = [
          "Page count summary:",
          "- Estimated pages: " + pages + " (" + pagesDetail + ")",
          "- Reading time: " + reading + " (" + readingDetail + ")",
          "- Speaking time: " + speaking + " (" + speakingDetail + ")",
        ].join("\n");

        navigator.clipboard
          .writeText(summary)
          .then(() => {
            const status = document.getElementById("pc-copy-status");
            status.classList.remove("hidden");
            setTimeout(() => status.classList.add("hidden"), 2000);
          })
          .catch(() => {
            alert("Could not copy to clipboard. Please copy manually.");
          });
      }

      document.addEventListener("DOMContentLoaded", function () {
        // Tabs
        document.querySelectorAll(".pc-tab-btn").forEach((btn) => {
          btn.addEventListener("click", () => {
            const tab = btn.getAttribute("data-tab");
            switchTab(tab);
          });
        });

        // Text input live counting
        document
          .getElementById("pc-text-input")
          .addEventListener("input", updateFromTextArea);

        document
          .getElementById("pc-clear-text")
          .addEventListener("click", () => {
            document.getElementById("pc-text-input").value = "";
            updateFromTextArea();
          });

        // Counts inputs
        ["pc-words-input", "pc-chars-input", "pc-chars-nospace-input"].forEach(
          (id) => {
            const el = document.getElementById(id);
            el.addEventListener("input", () => {
              // If user edits counts directly, we don't overwrite text area
              recalcPages();
            });
          }
        );

        // Layout / presets
        document
          .getElementById("pc-font-size")
          .addEventListener("change", (e) => {
            const val = e.target.value;
            const customWrapper = document.getElementById(
              "pc-custom-wpp-wrapper"
            );
            if (val === "custom") {
              customWrapper.classList.remove("hidden");
            } else {
              customWrapper.classList.add("hidden");
            }
            recalcPages();
          });

        document
          .getElementById("pc-custom-wpp")
          .addEventListener("input", recalcPages);
        document
          .getElementById("pc-reading-speed")
          .addEventListener("change", recalcPages);
        document
          .getElementById("pc-speaking-speed")
          .addEventListener("change", recalcPages);
        document
          .getElementById("pc-round-pages")
          .addEventListener("change", recalcPages);

        // URL fetch
        document
          .getElementById("pc-fetch-url")
          .addEventListener("click", () => {
            const urlInput = document
              .getElementById("pc-url-input")
              .value.trim();
            if (!urlInput) {
              document.getElementById("pc-url-status").textContent =
                "Please enter a URL first.";
              document
                .getElementById("pc-url-status")
                .classList.add("text-red-600");
              return;
            }
            let url = urlInput;
            if (!/^https?:\/\//i.test(url)) {
              url = "https://" + url;
            }
            fetchUrlContent(url);
          });

        // Copy summary
        document
          .getElementById("pc-copy-summary")
          .addEventListener("click", copySummary);

