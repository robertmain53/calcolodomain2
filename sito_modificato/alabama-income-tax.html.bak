    <script>
      // Alabama income tax calculator logic (simplified, planning-only)
      (function () {
        const incomeEl = document.getElementById("al-income");
        const statusEl = document.getElementById("al-filing-status");
        const depsEl = document.getElementById("al-dependents");
        const withholdEl = document.getElementById("al-withholding");
        const calcBtn = document.getElementById("al-calc-btn");
        const resetBtn = document.getElementById("al-reset-btn");

        const taxOutEl = document.getElementById("al-tax-output");
        const effRateEl = document.getElementById("al-effective-rate");
        const margRateEl = document.getElementById("al-marginal-rate");
        const taxableOutEl = document.getElementById("al-taxable-output");
        const refundOutEl = document.getElementById("al-refund-output");
        const bracketRowsEl = document.getElementById("al-bracket-rows");

        function getExemptions(status, dependents) {
          // Very rough approximations for planning
          let base;
          switch (status) {
            case "single":
            case "mfs":
              base = 1500;
              break;
            case "mfj":
              base = 3000;
              break;
            case "hoh":
              base = 2500;
              break;
            default:
              base = 1500;
          }
          const depExemption = 1000 * Math.max(0, dependents || 0);
          return base + depExemption;
        }

        function getBrackets(status) {
          // Simplified Alabama-style brackets (illustrative only)
          if (status === "mfj" || status === "hoh") {
            return [
              { upTo: 1000, rate: 0.02 },
              { upTo: 6000, rate: 0.04 },
              { upTo: Infinity, rate: 0.05 },
            ];
          } else {
            // single / mfs
            return [
              { upTo: 500, rate: 0.02 },
              { upTo: 3000, rate: 0.04 },
              { upTo: Infinity, rate: 0.05 },
            ];
          }
        }

        function formatMoney(v) {
          if (!isFinite(v)) return "$0";
          return "$" + v.toFixed(0).replace(/\B(?=(\d{3})+(?!\d))/g, ",");
        }

        function formatPercent(v) {
          if (!isFinite(v)) return "0%";
          return (v * 100).toFixed(1) + "%";
        }

        function calculate() {
          let income = parseFloat(incomeEl.value.replace(/,/g, "")) || 0;
          let deps = parseInt(depsEl.value, 10);
          if (!isFinite(deps) || deps < 0) deps = 0;
          let withheld = parseFloat(withholdEl.value.replace(/,/g, "")) || 0;
          const status = statusEl.value;

          income = Math.max(0, income);
          withheld = Math.max(0, withheld);

          const exemptions = getExemptions(status, deps);
          const taxable = Math.max(0, income - exemptions);
          const brackets = getBrackets(status);

          let remaining = taxable;
          let lastLimit = 0;
          let totalTax = 0;
          let marginalRate = 0;
          const rows = [];

          for (const b of brackets) {
            if (remaining <= 0) break;
            const bandIncome = Math.min(remaining, b.upTo - lastLimit);
            if (bandIncome > 0) {
              const tax = bandIncome * b.rate;
              totalTax += tax;
              marginalRate = b.rate;
              rows.push({
                range: `${formatMoney(lastLimit)} – ${
                  b.upTo === Infinity ? "∞" : formatMoney(b.upTo)
                }`,
                income: bandIncome,
                rate: b.rate,
                tax: tax,
              });
              remaining -= bandIncome;
              lastLimit = b.upTo;
            }
          }

          const effectiveRate = taxable > 0 ? totalTax / taxable : 0;
          const refund = withheld - totalTax; // positive = refund, negative = amount due

          // Update UI
          taxOutEl.textContent = formatMoney(totalTax);
          effRateEl.textContent = formatPercent(effectiveRate);
          margRateEl.textContent = formatPercent(marginalRate);
          taxableOutEl.textContent = formatMoney(taxable);

          let refundLabel;
          if (Math.abs(refund) < 1) {
            refundLabel = "$0";
          } else if (refund > 0) {
            refundLabel = formatMoney(refund) + " refund (approx.)";
          } else {
            refundLabel = formatMoney(-refund) + " due (approx.)";
          }
          refundOutEl.textContent = refundLabel;

          if (rows.length === 0) {
            bracketRowsEl.innerHTML =
              '<tr><td class="px-3 py-2 text-gray-500" colspan="4">No taxable income after estimated exemptions.</td></tr>';
          } else {
            bracketRowsEl.innerHTML = rows
              .map(
                (r) => `
          <tr class="border-t border-gray-100">
            <td class="px-3 py-2 text-gray-700">${r.range}</td>
            <td class="px-3 py-2 text-right text-gray-700">${formatMoney(
              r.income
            )}</td>
            <td class="px-3 py-2 text-right text-gray-700">${formatPercent(
              r.rate
            )}</td>
            <td class="px-3 py-2 text-right text-gray-700">${formatMoney(
              r.tax
            )}</td>
          </tr>
        `
              )
              .join("");
          }
        }

        calcBtn.addEventListener("click", function (e) {
          e.preventDefault();
          calculate();
        });

        resetBtn.addEventListener("click", function (e) {
          e.preventDefault();
          incomeEl.value = 60000;
          statusEl.value = "single";
          depsEl.value = 0;
          withholdEl.value = 0;
          calculate();
        });

        // Auto-calc on load
        calculate();
      })();
    </script>

