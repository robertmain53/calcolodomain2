    <script>
      // Unit handling
      const unitLabels = {
        metric: {
          H: "(m)",
          B: "(m)",
          gamma: "(kN/m³)",
          q: "(kPa)",
          qAllow: "(kPa)",
          W: "kN/m",
          Pa: "kN/m",
          e: "m",
          xR: "m",
          qBase: "kPa",
        },
        us: {
          H: "(ft)",
          B: "(ft)",
          gamma: "(pcf)",
          q: "(psf)",
          qAllow: "(psf)",
          W: "lb/ft",
          Pa: "lb/ft",
          e: "ft",
          xR: "ft",
          qBase: "psf",
        },
      };

      function updateUnitLabels() {
        const sys = document.getElementById("unitSystem").value;
        const L = unitLabels[sys];
        document.getElementById("lblHUnit").textContent = L.H;
        document.getElementById("lblBUnit").textContent = L.B;
        document.getElementById("lblGammaSoilUnit").textContent = L.gamma;
        document.getElementById("lblGammaGabionUnit").textContent = L.gamma;
        document.getElementById("lblQUnit").textContent = L.q;
        document.getElementById("lblQallowUnit").textContent = L.qAllow;
        document.getElementById("lblWUnit").textContent = L.W;
        document.getElementById("lblPaUnit").textContent = L.Pa;
        document.getElementById("lblEUnit").textContent = L.e;
        document.getElementById("lblXRUnit").textContent = L.xR;
        document.getElementById("lblQmaxUnit").textContent = L.qBase;
        document.getElementById("lblQminUnit").textContent = L.qBase;
      }

      document
        .getElementById("unitSystem")
        .addEventListener("change", updateUnitLabels);
      updateUnitLabels();

      function toRad(deg) {
        return (deg * Math.PI) / 180;
      }

      function setBadge(el, value, required) {
        el.className = "ml-2 px-2 py-0.5 rounded-full text-xs";
        if (!isFinite(value)) {
          el.textContent = "";
          return;
        }
        if (value >= required) {
          el.textContent = "OK";
          el.classList.add("result-badge-ok");
        } else if (value >= 0.9 * required) {
          el.textContent = "Low";
          el.classList.add("result-badge-warn");
        } else {
          el.textContent = "Fail";
          el.classList.add("result-badge-fail");
        }
      }

      function calculateGabionWall() {
        const H = parseFloat(document.getElementById("H").value);
        const B = parseFloat(document.getElementById("B").value);
        const batterFront = parseFloat(
          document.getElementById("batterFront").value
        );
        const batterBack = parseFloat(
          document.getElementById("batterBack").value
        );
        const gammaSoil = parseFloat(
          document.getElementById("gammaSoil").value
        );
        const gammaGabion = parseFloat(
          document.getElementById("gammaGabion").value
        );
        const phi = parseFloat(document.getElementById("phi").value);
        const mu = parseFloat(document.getElementById("mu").value);
        const q = parseFloat(document.getElementById("q").value);
        const qAllow = parseFloat(document.getElementById("qAllow").value);
        const FSreqSliding = parseFloat(
          document.getElementById("FSreqSliding").value
        );
        const FSreqOverturn = parseFloat(
          document.getElementById("FSreqOverturn").value
        );
        const FSreqBearing = parseFloat(
          document.getElementById("FSreqBearing").value
        );

        if (
          [H, B, gammaSoil, gammaGabion, phi, mu, qAllow].some(
            (v) => !isFinite(v) || v <= 0
          )
        ) {
          alert(
            "Please enter valid positive values for height, base width, unit weights, friction angle, friction coefficient and allowable bearing."
          );
          return;
        }

        const topWidth = B - (batterFront + batterBack);
        if (topWidth <= 0) {
          alert(
            "Top width becomes zero or negative. Reduce batter or increase base width."
          );
          return;
        }

        // Active earth pressure coefficient (Rankine)
        const Ka = Math.tan(toRad(45 - phi / 2)) ** 2;

        // Earth pressure from soil and surcharge
        const Pa_soil = 0.5 * gammaSoil * Ka * H * H;
        const Pa_surcharge = q * Ka * H;
        const Pa = Pa_soil + Pa_surcharge;

        // Wall area and weight (per meter length)
        const A = 0.5 * (B + topWidth) * H;
        const W = gammaGabion * A;

        // Centroid of trapezoid from toe (xW)
        const a = B;
        const c = topWidth;
        const xW = ((H / 3) * (2 * a + c)) / (a + c); // from toe along base

        // Moments about toe
        const Mo = Pa * (H / 3); // overturning
        const Mr = W * xW; // resisting

        // Resultant location and eccentricity
        const xR = (Mr - Mo) / W;
        const e = B / 2 - xR;

        // Base pressures
        let qmax, qmin;
        if (Math.abs(e) <= B / 6) {
          const q0 = W / B;
          qmax = q0 * (1 + (6 * e) / B);
          qmin = q0 * (1 - (6 * e) / B);
        } else {
          // Toe-only bearing (triangular)
          const z = B / 2 - Math.abs(e);
          qmax = (2 * W) / (3 * z);
          qmin = 0;
        }

        // Factors of safety
        const Rsliding = mu * W;
        const FSsliding = Rsliding / Pa;
        const FSoverturn = Mr / Mo;
        const FSbearing = qAllow / qmax;

        // Populate results
        const sys = document.getElementById("unitSystem").value;
        const num = (v, d = 2) => (isFinite(v) ? v.toFixed(d) : "–");

        document.getElementById("resW").textContent = num(W);
        document.getElementById("resPa").textContent = num(Pa);
        document.getElementById("resE").textContent = num(e);
        document.getElementById("resXR").textContent = num(xR);
        document.getElementById("resQmax").textContent = num(qmax);
        document.getElementById("resQmin").textContent = num(qmin);
        document.getElementById("resFSsliding").textContent = num(FSsliding, 2);
        document.getElementById("resFSoverturn").textContent = num(
          FSoverturn,
          2
        );
        document.getElementById("resFSbearing").textContent = num(FSbearing, 2);

        setBadge(
          document.getElementById("badgeFSsliding"),
          FSsliding,
          FSreqSliding
        );
        setBadge(
          document.getElementById("badgeFSoverturn"),
          FSoverturn,
          FSreqOverturn
        );
        setBadge(
          document.getElementById("badgeFSbearing"),
          FSbearing,
          FSreqBearing
        );

        // Simple conceptual layout suggestion
        const basketStd = sys === "metric" ? 1.0 : 3.0; // 1m or 3ft nominal height
        const nTiers = Math.ceil(H / basketStd);
        const textUnits = sys === "metric" ? "m" : "ft";
        const layout =
          `Conceptually, you could use about ${nTiers} vertical tiers of gabion baskets of height ≈ ${basketStd} ${textUnits}. ` +
          `A base width of ${num(B)} ${textUnits} suggests roughly ${Math.round(
            B / basketStd
          )} basket depths from toe to heel (depending on supplier sizes). ` +
          `Always confirm final basket dimensions and detailing with the manufacturer and project engineer.`;
        document.getElementById("resLayout").textContent = layout;

        document.getElementById("resultsSection").classList.remove("hidden");
      }

      document
        .getElementById("btnCalculate")
        .addEventListener("click", calculateGabionWall);

      document.getElementById("btnReset").addEventListener("click", () => {
        document.getElementById("H").value = 3;
        document.getElementById("B").value = 2.0;
        document.getElementById("batterFront").value = 0.1;
        document.getElementById("batterBack").value = 0.3;
        document.getElementById("gammaSoil").value = 18;
        document.getElementById("gammaGabion").value = 20;
        document.getElementById("phi").value = 30;
        document.getElementById("mu").value = 0.5;
        document.getElementById("q").value = 0;
        document.getElementById("qAllow").value = 200;
        document.getElementById("FSreqSliding").value = 1.5;
        document.getElementById("FSreqOverturn").value = 2.0;
        document.getElementById("FSreqBearing").value = 3.0;
        document.getElementById("resultsSection").classList.add("hidden");
      });

      document.getElementById("btnExample").addEventListener("click", () => {
        document.getElementById("unitSystem").value = "metric";
        updateUnitLabels();
        document.getElementById("H").value = 3.0;
        document.getElementById("B").value = 2.4;
        document.getElementById("batterFront").value = 0.1;
        document.getElementById("batterBack").value = 0.5;
        document.getElementById("gammaSoil").value = 18;
        document.getElementById("gammaGabion").value = 20;
        document.getElementById("phi").value = 32;
        document.getElementById("mu").value = 0.55;
        document.getElementById("q").value = 5;
        document.getElementById("qAllow").value = 250;
        document.getElementById("FSreqSliding").value = 1.5;
        document.getElementById("FSreqOverturn").value = 2.0;
        document.getElementById("FSreqBearing").value = 3.0;
        calculateGabionWall();
      });

      // Mobile menu script (simple)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", () => {
          const menu = document.getElementById("mobile-menu");
          const expanded = menu.classList.contains("hidden");
          menu.classList.toggle("hidden");
          document
            .getElementById("mobile-menu-toggle")
            .setAttribute("aria-expanded", expanded ? "true" : "false");
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
