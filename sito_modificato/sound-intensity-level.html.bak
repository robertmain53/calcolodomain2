    <script>
      // Tab switching
      const modeTabs = document.querySelectorAll(".mode-tab");
      const modes = document.querySelectorAll(".calculator-mode");

      modeTabs.forEach((tab) => {
        tab.addEventListener("click", () => {
          const target = tab.getAttribute("data-mode");
          modeTabs.forEach((t) => {
            t.classList.remove("border-blue-600", "text-blue-600");
            t.classList.add("border-transparent", "text-gray-600");
          });
          tab.classList.add("border-blue-600", "text-blue-600");
          tab.classList.remove("text-gray-600");

          modes.forEach((section) => {
            section.classList.toggle("hidden", section.id !== "mode-" + target);
          });
        });
      });

      // Helper: format scientific notation nicely
      function formatSci(value) {
        if (!isFinite(value) || value === 0) return "0";
        const abs = Math.abs(value);
        if (abs >= 1e-3 && abs < 1e3) {
          return value.toPrecision(4);
        }
        const exp = Math.floor(Math.log10(abs));
        const mant = value / Math.pow(10, exp);
        return mant.toPrecision(3) + "e" + (exp >= 0 ? "+" + exp : exp);
      }

      // Single intensity -> dB
      const intensityInput = document.getElementById("intensity-input");
      const refIntensityInput = document.getElementById(
        "reference-intensity-input"
      );
      const singleBtn = document.getElementById("calc-single-btn");
      const resetSingleBtn = document.getElementById("reset-single-btn");
      const singleResult = document.getElementById("single-result");
      const singleDbOutput = document.getElementById("single-db-output");
      const singleInterp = document.getElementById("single-interpretation");

      singleBtn.addEventListener("click", () => {
        const I = parseFloat(intensityInput.value);
        const I0 = parseFloat(refIntensityInput.value);
        if (!(I > 0) || !(I0 > 0)) {
          alert(
            "Please enter positive values for intensity and reference intensity."
          );
          return;
        }
        const L = 10 * Math.log10(I / I0);
        singleDbOutput.textContent = L.toFixed(2);

        let desc = "";
        if (L < 0)
          desc = "Extremely quiet, below the standard threshold of hearing.";
        else if (L < 30)
          desc =
            "Very quiet sound, comparable to a whisper or rustling leaves.";
        else if (L < 60)
          desc =
            "Moderate sound level, typical of a quiet office or conversation at a distance.";
        else if (L < 85)
          desc =
            "Loud but generally safe for limited exposure (e.g., busy street, vacuum cleaner).";
        else if (L < 110)
          desc =
            "Very loud; prolonged exposure may damage hearing (e.g., heavy traffic, loud music).";
        else
          desc =
            "Extremely loud; short exposure can cause hearing damage (e.g., rock concert, jet engine).";

        singleInterp.textContent = desc;
        singleResult.classList.remove("hidden");
      });

      resetSingleBtn.addEventListener("click", () => {
        intensityInput.value = "";
        refIntensityInput.value = "1e-12";
        singleResult.classList.add("hidden");
      });

      // Compare two intensities
      const intensity1Input = document.getElementById("intensity1-input");
      const intensity2Input = document.getElementById("intensity2-input");
      const ratioBtn = document.getElementById("calc-ratio-btn");
      const resetRatioBtn = document.getElementById("reset-ratio-btn");
      const ratioResult = document.getElementById("ratio-result");
      const ratioDbOutput = document.getElementById("ratio-db-output");
      const ratioInterp = document.getElementById("ratio-interpretation");

      ratioBtn.addEventListener("click", () => {
        const I1 = parseFloat(intensity1Input.value);
        const I2 = parseFloat(intensity2Input.value);
        if (!(I1 > 0) || !(I2 > 0)) {
          alert("Please enter positive values for both intensities.");
          return;
        }
        const deltaL = 10 * Math.log10(I2 / I1);
        ratioDbOutput.textContent = deltaL.toFixed(2);

        let relation;
        if (Math.abs(deltaL) < 0.5) {
          relation = "Both sounds have essentially the same intensity.";
        } else if (deltaL > 0) {
          const factor = Math.pow(10, deltaL / 10);
          relation = `Sound 2 is about ${factor.toFixed(
            1
          )}× more intense than Sound 1.`;
        } else {
          const factor = Math.pow(10, -deltaL / 10);
          relation = `Sound 2 is about ${factor.toFixed(
            1
          )}× less intense than Sound 1.`;
        }
        ratioInterp.textContent = relation;
        ratioResult.classList.remove("hidden");
      });

      resetRatioBtn.addEventListener("click", () => {
        intensity1Input.value = "";
        intensity2Input.value = "";
        ratioResult.classList.add("hidden");
      });

      // Distance effect
      const distance1Input = document.getElementById("distance1-input");
      const distance2Input = document.getElementById("distance2-input");
      const distanceBtn = document.getElementById("calc-distance-btn");
      const resetDistanceBtn = document.getElementById("reset-distance-btn");
      const distanceResult = document.getElementById("distance-result");
      const distanceIntensityRatio = document.getElementById(
        "distance-intensity-ratio"
      );
      const distanceDbChange = document.getElementById("distance-db-change");
      const distanceInterp = document.getElementById("distance-interpretation");

      distanceBtn.addEventListener("click", () => {
        const r1 = parseFloat(distance1Input.value);
        const r2 = parseFloat(distance2Input.value);
        if (!(r1 > 0) || !(r2 > 0)) {
          alert("Please enter positive distances.");
          return;
        }
        const ratio = (r1 * r1) / (r2 * r2); // I2 / I1
        const deltaL = 10 * Math.log10(ratio);
        distanceIntensityRatio.textContent = ratio.toFixed(3);
        distanceDbChange.textContent =
          (deltaL >= 0 ? "+" : "") + deltaL.toFixed(2) + " dB";

        let msg = "";
        if (r2 > r1) {
          msg = `Moving from ${r1} m to ${r2} m reduces intensity to ${ratio.toFixed(
            3
          )} of the original, a change of ${deltaL.toFixed(1)} dB (quieter).`;
        } else if (r2 < r1) {
          msg = `Moving from ${r1} m to ${r2} m increases intensity to ${(
            1 / ratio
          ).toFixed(3)} times the original, a change of ${deltaL.toFixed(
            1
          )} dB (louder).`;
        } else {
          msg = "Distances are equal, so intensity and level do not change.";
        }
        distanceInterp.textContent = msg;
        distanceResult.classList.remove("hidden");
      });

      resetDistanceBtn.addEventListener("click", () => {
        distance1Input.value = "1";
        distance2Input.value = "2";
        distanceResult.classList.add("hidden");
      });

      // Slider: dB -> intensity
      const dbSlider = document.getElementById("db-slider");
      const dbSliderValue = document.getElementById("db-slider-value");
      const dbSliderIntensity = document.getElementById("db-slider-intensity");

      function updateSlider() {
        const L = parseFloat(dbSlider.value);
        dbSliderValue.textContent = L.toFixed(0);
        const I0 = 1e-12;
        const I = I0 * Math.pow(10, L / 10);
        dbSliderIntensity.textContent = formatSci(I);
      }

      dbSlider.addEventListener("input", updateSlider);
      updateSlider();
    </script>

