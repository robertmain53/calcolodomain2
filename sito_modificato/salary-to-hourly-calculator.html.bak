    <script>
      (function () {
        const payPeriodButtons = document.querySelectorAll(".pay-period-btn");
        const salaryAmountInput = document.getElementById("salary-amount");
        const salaryPeriodLabel = document.getElementById(
          "salary-period-label"
        );
        const currencySelect = document.getElementById("currency-select");

        const hoursPerWeekInput = document.getElementById("hours-per-week");
        const weeksPerYearInput = document.getElementById("weeks-per-year");
        const unpaidBreaksInput = document.getElementById("unpaid-breaks");

        const includeOvertimeCheckbox =
          document.getElementById("include-overtime");
        const overtimeSection = document.getElementById("overtime-section");
        const overtimeHoursInput = document.getElementById("overtime-hours");
        const overtimeMultiplierSelect = document.getElementById(
          "overtime-multiplier"
        );

        const calculateBtn = document.getElementById("calculate-btn");
        const resetBtn = document.getElementById("reset-btn");

        const resultHourly = document.getElementById("result-hourly");
        const resultEffectiveHourly = document.getElementById(
          "result-effective-hourly"
        );
        const resultWeekly = document.getElementById("result-weekly");
        const resultBiweekly = document.getElementById("result-biweekly");
        const resultMonthly = document.getElementById("result-monthly");
        const resultAnnual = document.getElementById("result-annual");
        const resultsNote = document.getElementById("results-note");

        let currentPeriod = "annual";

        function updatePeriodLabel() {
          const map = {
            annual: "Per year (gross, before tax)",
            monthly: "Per month (gross, before tax)",
            biweekly: "Per 2 weeks (gross, before tax)",
            weekly: "Per week (gross, before tax)",
          };
          salaryPeriodLabel.textContent = map[currentPeriod] || map.annual;
        }

        payPeriodButtons.forEach((btn) => {
          btn.addEventListener("click", () => {
            currentPeriod = btn.dataset.period;
            payPeriodButtons.forEach((b) => {
              b.classList.remove("toggle-btn-active");
              b.classList.add("toggle-btn-inactive");
            });
            btn.classList.add("toggle-btn-active");
            btn.classList.remove("toggle-btn-inactive");
            updatePeriodLabel();
          });
        });

        includeOvertimeCheckbox.addEventListener("change", () => {
          if (includeOvertimeCheckbox.checked) {
            overtimeSection.classList.remove(
              "opacity-40",
              "pointer-events-none"
            );
          } else {
            overtimeSection.classList.add("opacity-40", "pointer-events-none");
          }
        });

        function formatMoney(value) {
          if (!isFinite(value)) return "–";
          const symbol = currencySelect.value || "$";
          return (
            symbol + value.toFixed(2).replace(/\B(?=(\d{3})+(?!\d))/g, ",")
          );
        }

        function clearResults() {
          [
            resultHourly,
            resultEffectiveHourly,
            resultWeekly,
            resultBiweekly,
            resultMonthly,
            resultAnnual,
          ].forEach((el) => {
            el.textContent = "–";
          });
          resultsNote.textContent =
            "Enter your salary and schedule, then click “Calculate hourly rate” to see results.";
        }

        function calculate() {
          const salary = parseFloat(salaryAmountInput.value);
          const hoursPerWeek = parseFloat(hoursPerWeekInput.value);
          const weeksPerYear = parseFloat(weeksPerYearInput.value);
          const unpaidBreaks = parseFloat(unpaidBreaksInput.value) || 0;

          if (
            !salary ||
            salary <= 0 ||
            !hoursPerWeek ||
            hoursPerWeek <= 0 ||
            !weeksPerYear ||
            weeksPerYear <= 0
          ) {
            clearResults();
            resultsNote.textContent =
              "Please enter a positive salary, hours per week, and paid weeks per year.";
            return;
          }

          // Convert salary to annual
          let annualSalary;
          switch (currentPeriod) {
            case "monthly":
              annualSalary = salary * 12;
              break;
            case "biweekly":
              annualSalary = salary * 26;
              break;
            case "weekly":
              annualSalary = salary * 52;
              break;
            case "annual":
            default:
              annualSalary = salary;
          }

          // Paid hours per week (subtract unpaid breaks for 5 days/week)
          const paidHoursPerWeek = Math.max(0, hoursPerWeek - unpaidBreaks * 5);
          const hoursPerYear = paidHoursPerWeek * weeksPerYear;

          if (hoursPerYear <= 0) {
            clearResults();
            resultsNote.textContent =
              "Working hours per year must be greater than zero. Check your hours and breaks.";
            return;
          }

          const baseHourly = annualSalary / hoursPerYear;

          // Effective hourly with overtime
          let effectiveHourly = baseHourly;
          if (includeOvertimeCheckbox.checked) {
            const overtimeHours = parseFloat(overtimeHoursInput.value) || 0;
            const overtimeMultiplier =
              parseFloat(overtimeMultiplierSelect.value) || 1.5;
            const totalHoursPerWeek = paidHoursPerWeek + overtimeHours;
            const totalHoursPerYear = totalHoursPerWeek * weeksPerYear;
            if (totalHoursPerYear > 0) {
              effectiveHourly = annualSalary / totalHoursPerYear;
            }
          }

          // Period equivalents based on base hourly
          const weeklyPay = baseHourly * paidHoursPerWeek;
          const biweeklyPay = weeklyPay * 2;
          const monthlyPay = annualSalary / 12;

          resultHourly.textContent = formatMoney(baseHourly);
          resultEffectiveHourly.textContent = formatMoney(effectiveHourly);
          resultWeekly.textContent = formatMoney(weeklyPay);
          resultBiweekly.textContent = formatMoney(biweeklyPay);
          resultMonthly.textContent = formatMoney(monthlyPay);
          resultAnnual.textContent = formatMoney(annualSalary);

          resultsNote.textContent =
            "These amounts are based on gross pay and your specified working schedule. Taxes and deductions are not included.";
        }

        function reset() {
          currentPeriod = "annual";
          payPeriodButtons.forEach((b) => {
            if (b.dataset.period === "annual") {
              b.classList.add("toggle-btn-active");
              b.classList.remove("toggle-btn-inactive");
            } else {
              b.classList.remove("toggle-btn-active");
              b.classList.add("toggle-btn-inactive");
            }
          });
          salaryAmountInput.value = "";
          currencySelect.value = "$";
          hoursPerWeekInput.value = 40;
          weeksPerYearInput.value = 52;
          unpaidBreaksInput.value = 0;
          includeOvertimeCheckbox.checked = false;
          overtimeHoursInput.value = 5;
          overtimeMultiplierSelect.value = "1.5";
          overtimeSection.classList.add("opacity-40", "pointer-events-none");
          updatePeriodLabel();
          clearResults();
        }

        calculateBtn.addEventListener("click", calculate);
        resetBtn.addEventListener("click", reset);

        // Initialize
        updatePeriodLabel();
        clearResults();
      })();
    </script>

