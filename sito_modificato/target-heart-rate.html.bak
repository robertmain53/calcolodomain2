    <script>
      // Target Heart Rate Calculator Logic
      (function () {
        const ageInput = document.getElementById("age");
        const restingInput = document.getElementById("resting-hr");
        const sexInput = document.getElementById("sex");
        const fitnessInput = document.getElementById("fitness-level");
        const calcBtn = document.getElementById("calculate-btn");
        const resetBtn = document.getElementById("reset-btn");
        const errorMsg = document.getElementById("error-msg");

        const resultsSection = document.getElementById("results-section");
        const maxHrMainEl = document.getElementById("max-hr-main");
        const maxHrAltEl = document.getElementById("max-hr-alt");
        const restingDisplayEl = document.getElementById("resting-hr-display");
        const hrrDisplayEl = document.getElementById("hrr-display");
        const zonesTableBody = document.getElementById("zones-table-body");

        function clamp(value, min, max) {
          return Math.min(Math.max(value, min), max);
        }

        function getZonesForFitness(level) {
          // Base % ranges (as fractions of max HR)
          let zones = [
            {
              id: 1,
              name: "Zone 1",
              label: "Very light / warm-up",
              min: 0.5,
              max: 0.6,
            },
            {
              id: 2,
              name: "Zone 2",
              label: "Light / fat-burning",
              min: 0.6,
              max: 0.7,
            },
            {
              id: 3,
              name: "Zone 3",
              label: "Moderate / cardio fitness",
              min: 0.7,
              max: 0.8,
            },
            {
              id: 4,
              name: "Zone 4",
              label: "Hard / vigorous",
              min: 0.8,
              max: 0.9,
            },
            {
              id: 5,
              name: "Zone 5",
              label: "Very hard / near max",
              min: 0.9,
              max: 1.0,
            },
          ];

          // Slightly bias recommended ranges based on fitness level
          if (level === "beginner") {
            zones = zones.map((z) => ({
              ...z,
              min: clamp(z.min - 0.02, 0.4, 1.0),
              max: clamp(z.max - 0.02, 0.45, 1.0),
            }));
          } else if (level === "advanced") {
            zones = zones.map((z) => ({
              ...z,
              min: clamp(z.min + 0.02, 0.4, 1.0),
              max: clamp(z.max + 0.02, 0.45, 1.0),
            }));
          }
          return zones;
        }

        function formatBpmRange(min, max) {
          if (isNaN(min) || isNaN(max)) return "–";
          return `${Math.round(min)}–${Math.round(max)}`;
        }

        function calculate() {
          errorMsg.textContent = "";
          const age = parseInt(ageInput.value, 10);
          if (isNaN(age) || age < 5 || age > 100) {
            errorMsg.textContent =
              "Please enter a valid age between 5 and 100.";
            resultsSection.classList.add("hidden");
            return;
          }

          const resting = parseInt(restingInput.value, 10);
          const hasResting = !isNaN(resting) && resting >= 30 && resting <= 120;

          // Max HR formulas
          const maxHrMain = 220 - age;
          const maxHrAlt = Math.round(208 - 0.7 * age); // Tanaka; sex not strongly used but shown for comparison

          // HRR
          const hrr = hasResting ? maxHrMain - resting : NaN;

          // Update summary
          maxHrMainEl.textContent = maxHrMain;
          maxHrAltEl.textContent = maxHrAlt;
          restingDisplayEl.textContent = hasResting ? resting : "Not provided";
          hrrDisplayEl.textContent = hasResting ? hrr : "Not applicable";

          // Zones
          const fitnessLevel = fitnessInput.value;
          const zones = getZonesForFitness(fitnessLevel);

          const typicalActivities = {
            1: "Gentle walking, easy cycling, mobility work",
            2: "Brisk walking, light jogging, easy cycling",
            3: "Steady jogging, swimming, dance cardio",
            4: "Tempo runs, hill repeats, fast cycling",
            5: "Sprints, HIIT intervals, race efforts",
          };

          zonesTableBody.innerHTML = "";
          zones.forEach((z) => {
            const simpleMin = maxHrMain * z.min;
            const simpleMax = maxHrMain * z.max;

            let karvMin = NaN,
              karvMax = NaN;
            if (hasResting) {
              karvMin = hrr * z.min + resting;
              karvMax = hrr * z.max + resting;
            }

            const tr = document.createElement("tr");
            tr.innerHTML = `
          <td class="px-3 py-2 font-medium text-gray-800">${z.name}</td>
          <td class="px-3 py-2 text-gray-700">${z.label}</td>
          <td class="px-3 py-2 text-gray-700">${Math.round(
            z.min * 100
          )}–${Math.round(z.max * 100)}%</td>
          <td class="px-3 py-2 text-gray-700">${formatBpmRange(
            simpleMin,
            simpleMax
          )}</td>
          <td class="px-3 py-2 text-gray-700">${
            hasResting ? formatBpmRange(karvMin, karvMax) : "–"
          }</td>
          <td class="px-3 py-2 text-gray-600">${
            typicalActivities[z.id] || ""
          }</td>
        `;
            zonesTableBody.appendChild(tr);
          });

          resultsSection.classList.remove("hidden");
          resultsSection.scrollIntoView({ behavior: "smooth", block: "start" });
        }

        function resetForm() {
          document.getElementById("thr-form").reset();
          errorMsg.textContent = "";
          resultsSection.classList.add("hidden");
          zonesTableBody.innerHTML = "";
          maxHrMainEl.textContent = "–";
          maxHrAltEl.textContent = "–";
          restingDisplayEl.textContent = "–";
          hrrDisplayEl.textContent = "–";
        }

        calcBtn.addEventListener("click", calculate);
        resetBtn.addEventListener("click", resetForm);

        // Allow Enter key to trigger calculation
        document
          .getElementById("thr-form")
          .addEventListener("submit", function (e) {
            e.preventDefault();
            calculate();
          });
      })();
    </script>

