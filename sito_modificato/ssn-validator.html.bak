    <script>
      // Mobile menu toggle
      document
        .getElementById("mobile-menu-toggle")
        ?.addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          if (!menu) return;
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          this.setAttribute("aria-expanded", String(isHidden));
        });

      // SSN Validator Logic
      (function () {
        const input = document.getElementById("ssn-input");
        const validateBtn = document.getElementById("validate-btn");
        const clearBtn = document.getElementById("clear-btn");

        const resultCard = document.getElementById("result-card");
        const overallBadge = document.getElementById("overall-status-badge");
        const normalizedSpan = document.getElementById("normalized-ssn");
        const formatStatus = document.getElementById("format-status");
        const areaStatus = document.getElementById("area-status");
        const groupStatus = document.getElementById("group-status");
        const serialStatus = document.getElementById("serial-status");
        const patternWarnings = document.getElementById("pattern-warnings");
        const errorMessages = document.getElementById("error-messages");

        function resetResults() {
          resultCard.classList.add("hidden");
          patternWarnings.classList.add("hidden");
          errorMessages.classList.add("hidden");
          patternWarnings.textContent = "";
          errorMessages.textContent = "";
        }

        function classifyBadge(el, type, text) {
          el.className = "text-xs font-semibold px-2.5 py-1 rounded-full";
          if (type === "valid") {
            el.classList.add("badge-valid");
          } else if (type === "invalid") {
            el.classList.add("badge-invalid");
          } else {
            el.classList.add("badge-warning");
          }
          el.textContent = text;
        }

        function validateSSN(raw) {
          const errors = [];
          const warnings = [];

          // Normalize: remove spaces and dashes
          const cleaned = (raw || "").replace(/[\s-]/g, "");
          if (!cleaned) {
            errors.push("Please enter an SSN to validate.");
            return { valid: false, cleaned: "", errors, warnings, parts: null };
          }

          if (!/^\d+$/.test(cleaned)) {
            errors.push("SSN must contain digits only (0–9).");
          }

          if (cleaned.length !== 9) {
            errors.push("SSN must be exactly 9 digits long.");
          }

          if (errors.length) {
            return { valid: false, cleaned, errors, warnings, parts: null };
          }

          const area = cleaned.slice(0, 3);
          const group = cleaned.slice(3, 5);
          const serial = cleaned.slice(5);

          // Area checks
          let areaMsg = "";
          let areaValid = true;
          const areaNum = parseInt(area, 10);

          if (area === "000") {
            areaValid = false;
            areaMsg = "Area number 000 is never assigned.";
          } else if (area === "666") {
            areaValid = false;
            areaMsg = "Area number 666 is not used for regular SSNs.";
          } else if (areaNum >= 900) {
            areaValid = false;
            areaMsg = "Area numbers 900–999 are not used for regular SSNs.";
          } else {
            areaMsg = "Area number is in an allowed range.";
          }

          // Group checks
          let groupMsg = "";
          let groupValid = true;
          if (group === "00") {
            groupValid = false;
            groupMsg = "Group number 00 is never assigned.";
          } else {
            groupMsg = "Group number is non-zero.";
          }

          // Serial checks
          let serialMsg = "";
          let serialValid = true;
          if (serial === "0000") {
            serialValid = false;
            serialMsg = "Serial number 0000 is never assigned.";
          } else {
            serialMsg = "Serial number is non-zero.";
          }

          // Pattern-based warnings
          const allSame = /^(\d)\1{8}$/.test(cleaned);
          if (allSame) {
            warnings.push(
              "All digits are the same (e.g., 111-11-1111). This is almost always a fake or test SSN."
            );
          }

          const knownFakes = [
            "123456789",
            "078051120",
            "219099999",
            "123121234",
            "012345678",
          ];
          if (knownFakes.includes(cleaned)) {
            warnings.push(
              "This SSN matches a well-known example or test number and is very unlikely to be legitimate."
            );
          }

          if (cleaned === "012345678") {
            warnings.push(
              "Digits are in a simple ascending sequence (012345678), which is commonly used as a placeholder."
            );
          }

          const overallValid = areaValid && groupValid && serialValid;

          return {
            valid: overallValid,
            cleaned,
            errors,
            warnings,
            parts: {
              area,
              group,
              serial,
              areaValid,
              areaMsg,
              groupValid,
              groupMsg,
              serialValid,
              serialMsg,
            },
          };
        }

        function onValidate() {
          resetResults();
          const raw = input.value;
          const result = validateSSN(raw);

          if (result.errors.length) {
            errorMessages.textContent = result.errors.join(" ");
            errorMessages.classList.remove("hidden");
            resultCard.classList.remove("hidden");
            classifyBadge(overallBadge, "invalid", "Invalid SSN");
            normalizedSpan.textContent = result.cleaned
              ? `Normalized: ${result.cleaned}`
              : "";
            formatStatus.textContent = "Failed basic format checks.";
            areaStatus.textContent = "Not evaluated due to format errors.";
            groupStatus.textContent = "Not evaluated due to format errors.";
            serialStatus.textContent = "Not evaluated due to format errors.";
            return;
          }

          const {
            area,
            group,
            serial,
            areaValid,
            areaMsg,
            groupValid,
            groupMsg,
            serialValid,
            serialMsg,
          } = result.parts;

          // Overall badge
          if (result.valid) {
            classifyBadge(overallBadge, "valid", "Structurally valid");
          } else {
            classifyBadge(overallBadge, "invalid", "Structurally invalid");
          }

          normalizedSpan.textContent = `Normalized: ${area}-${group}-${serial}`;

          // Format status
          formatStatus.textContent =
            "9 digits, numeric only. Dashes (AAA-GG-SSSS) are optional.";

          // Area status
          areaStatus.textContent = `${area} – ${areaMsg}`;

          // Group status
          groupStatus.textContent = `${group} – ${groupMsg}`;

          // Serial status
          serialStatus.textContent = `${serial} – ${serialMsg}`;

          // Warnings
          if (result.warnings.length) {
            patternWarnings.innerHTML =
              "<strong>Warning:</strong> " + result.warnings.join(" ");
            patternWarnings.classList.remove("hidden");
            if (result.valid) {
              classifyBadge(
                overallBadge,
                "warning",
                "Structurally valid (high‑risk pattern)"
              );
            }
          }

          resultCard.classList.remove("hidden");
        }

        function onClear() {
          input.value = "";
          resetResults();
          input.focus();
        }

        validateBtn?.addEventListener("click", onValidate);
        clearBtn?.addEventListener("click", onClear);
        input?.addEventListener("keydown", function (e) {
          if (e.key === "Enter") {
            e.preventDefault();
            onValidate();
          }
        });
      })();
    </script>

