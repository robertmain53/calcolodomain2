    <script>
      // Risk mapping table (approximate % per year, untreated AF)
      const riskMap = {
        0: 0.2,
        1: 0.6,
        2: 2.2,
        3: 3.2,
        4: 4.8,
        5: 7.2,
        6: 9.7,
        7: 11.2,
        8: 10.8,
        9: 12.2,
      };

      function getRiskCategory(score, sex) {
        // sex: 'male' or 'female'
        if (
          (sex === "male" && score === 0) ||
          (sex === "female" && score === 1)
        ) {
          return {
            label: "Low risk (no anticoagulation usually recommended)",
            color: "bg-emerald-100 text-emerald-800",
          };
        }
        if (
          (sex === "male" && score === 1) ||
          (sex === "female" && score === 2)
        ) {
          return {
            label: "Intermediate risk (consider anticoagulation)",
            color: "bg-amber-100 text-amber-800",
          };
        }
        if (
          (sex === "male" && score >= 2) ||
          (sex === "female" && score >= 3)
        ) {
          return {
            label: "High risk (anticoagulation generally recommended)",
            color: "bg-red-100 text-red-800",
          };
        }
        return {
          label: "Risk category not defined",
          color: "bg-gray-200 text-gray-800",
        };
      }

      function buildRiskTable() {
        const tbody = document.getElementById("risk-table-body");
        if (!tbody) return;
        tbody.innerHTML = "";
        Object.keys(riskMap)
          .sort((a, b) => Number(a) - Number(b))
          .forEach((key) => {
            const score = Number(key);
            const risk = riskMap[score];
            let level = "";
            if (score <= 1) level = "Very low to low";
            else if (score <= 3) level = "Moderate";
            else if (score <= 5) level = "High";
            else level = "Very high";

            const tr = document.createElement("tr");
            tr.className = score % 2 === 0 ? "bg-white" : "bg-gray-50";
            tr.innerHTML = `
        <td class="px-3 py-2 text-gray-800 font-medium">${score}</td>
        <td class="px-3 py-2 text-gray-800">${risk.toFixed(1)}% per year</td>
        <td class="px-3 py-2 text-gray-700">${level}</td>
      `;
            tbody.appendChild(tr);
          });
      }

      function calculateScore() {
        const ageInput = document.getElementById("age");
        const sexInputs = document.querySelectorAll('input[name="sex"]');
        const chf = document.getElementById("chf").checked;
        const htn = document.getElementById("htn").checked;
        const dm = document.getElementById("dm").checked;
        const stroke = document.getElementById("stroke").checked;
        const vascular = document.getElementById("vascular").checked;
        const validation = document.getElementById("validation-message");

        let sex = "male";
        sexInputs.forEach((r) => {
          if (r.checked) sex = r.value;
        });

        const ageVal = ageInput.value ? Number(ageInput.value) : NaN;
        if (isNaN(ageVal) || ageVal < 18 || ageVal > 120) {
          validation.textContent =
            "Please enter a valid age between 18 and 120 years.";
          return null;
        }
        validation.textContent = "";

        let score = 0;
        // CHF
        if (chf) score += 1;
        // Hypertension
        if (htn) score += 1;
        // Age
        if (ageVal >= 75) score += 2;
        else if (ageVal >= 65) score += 1;
        // Diabetes
        if (dm) score += 1;
        // Stroke/TIA/TE
        if (stroke) score += 2;
        // Vascular disease
        if (vascular) score += 1;
        // Sex category
        if (sex === "female") score += 1;

        return { score, sex, age: ageVal };
      }

      function updateResults() {
        const result = calculateScore();
        if (!result) return;

        const { score, sex } = result;
        const scoreDisplay = document.getElementById("score-display");
        const riskDisplay = document.getElementById("risk-display");
        const riskCategoryEl = document.getElementById("risk-category");
        const interpretation = document.getElementById("interpretation-text");
        const sexNote = document.getElementById("sex-note");

        scoreDisplay.textContent = score.toString();

        const risk = riskMap.hasOwnProperty(score) ? riskMap[score] : null;
        if (risk !== null) {
          riskDisplay.textContent = risk.toFixed(1) + "% per year";
        } else {
          riskDisplay.textContent = "N/A";
        }

        const cat = getRiskCategory(score, sex);
        riskCategoryEl.textContent = cat.label;
        riskCategoryEl.className =
          "text-xs font-medium mt-1 px-2 py-0.5 inline-block rounded-full " +
          cat.color;

        let interp = `This patient's CHA₂DS₂-VASc score is ${score}. `;
        if (risk !== null) {
          interp += `This corresponds to an approximate annual ischemic stroke risk of about ${risk.toFixed(
            1
          )}% in untreated atrial fibrillation cohorts. `;
        }
        if (
          (sex === "female" && score === 1) ||
          (sex === "male" && score === 0)
        ) {
          interp +=
            "This is generally considered low risk; oral anticoagulation is usually not recommended in the absence of other indications.";
        } else if (
          (sex === "female" && score === 2) ||
          (sex === "male" && score === 1)
        ) {
          interp +=
            "This is an intermediate-risk category; anticoagulation may be considered based on bleeding risk, comorbidities, and patient preferences.";
        } else if (
          (sex === "female" && score >= 3) ||
          (sex === "male" && score >= 2)
        ) {
          interp +=
            "This is a high-risk category; oral anticoagulation is generally recommended unless contraindicated.";
        } else {
          interp +=
            "Use this score together with bleeding risk assessment and current guidelines to guide anticoagulation decisions.";
        }
        interpretation.textContent = interp;

        if (sex === "female") {
          sexNote.textContent =
            "Note: Female sex alone (CHA₂DS₂-VASc = 1 with no other risk factors) is often considered a risk modifier rather than an indication for anticoagulation.";
          sexNote.classList.remove("hidden");
        } else {
          sexNote.textContent = "";
          sexNote.classList.add("hidden");
        }
      }

      function resetForm() {
        document.getElementById("cha2ds2-form").reset();
        document.getElementById("score-display").textContent = "–";
        document.getElementById("risk-display").textContent = "–";
        const riskCategoryEl = document.getElementById("risk-category");
        riskCategoryEl.textContent = "No calculation yet";
        riskCategoryEl.className =
          "text-xs font-medium mt-1 px-2 py-0.5 inline-block rounded-full bg-gray-200 text-gray-800";
        document.getElementById("interpretation-text").textContent =
          "Enter patient data above and click “Calculate” to see the CHA₂DS₂-VASc score and estimated stroke risk.";
        document.getElementById("validation-message").textContent = "";
        const sexNote = document.getElementById("sex-note");
        sexNote.textContent = "";
        sexNote.classList.add("hidden");
        // Ensure default sex is male
        const sexInputs = document.querySelectorAll('input[name="sex"]');
        sexInputs.forEach((r) => {
          if (r.value === "male") r.checked = true;
        });
      }

      document.addEventListener("DOMContentLoaded", () => {
        buildRiskTable();
        document
          .getElementById("calculate-btn")
          .addEventListener("click", updateResults);
        document
          .getElementById("reset-btn")
          .addEventListener("click", resetForm);

        // Recalculate on change for a smoother UX
        ["age", "chf", "htn", "dm", "stroke", "vascular"].forEach((id) => {
          const el = document.getElementById(id);
          if (el)
            el.addEventListener("change", () => {
              if (
                document.getElementById("score-display").textContent !== "–"
              ) {
                updateResults();
              }
            });
        });
        document.querySelectorAll('input[name="sex"]').forEach((radio) => {
          radio.addEventListener("change", () => {
            if (document.getElementById("score-display").textContent !== "–") {
              updateResults();
            }
          });
        });
      });
    </script>

