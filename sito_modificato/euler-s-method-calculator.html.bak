        function buildFunction(expr, variableNames) {
          const trimmed = (expr || '').trim();
          if (!trimmed) return null;

          let safeExpr = trimmed.replace(/\^/g, '**');

          const globals = [
            'sin','cos','tan','asin','acos','atan','sinh','cosh','tanh',
            'exp','log','sqrt','abs','pow','floor','ceil','round','max','min'
          ];

          const args = variableNames.concat(globals.map(name => name));
          const argList = args.join(',');

          const body = 'return (' + safeExpr + ');';

