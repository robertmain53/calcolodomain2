    <script>
      // Calculator logic
      (function () {
        const currencySelect = document.getElementById("currency-select");
        const currencyEls = [
          document.getElementById("currency-sales"),
          document.getElementById("currency-assets-begin"),
          document.getElementById("currency-assets-end"),
          document.getElementById("currency-assets-single"),
        ];

        const salesInput = document.getElementById("sales-input");
        const assetsBeginInput = document.getElementById("assets-begin-input");
        const assetsEndInput = document.getElementById("assets-end-input");
        const assetsSingleInput = document.getElementById(
          "assets-single-input"
        );

        const avgAssetsOutput = document.getElementById("avg-assets-output");
        const ratioOutput = document.getElementById("ratio-output");
        const ratioInterpretation = document.getElementById(
          "ratio-interpretation"
        );
        const insightList = document.getElementById("insight-list");

        const calcBtn = document.getElementById("calc-btn");
        const resetBtn = document.getElementById("reset-btn");

        const assetsAverageWrapper = document.getElementById(
          "assets-average-wrapper"
        );
        const assetsSingleWrapper = document.getElementById(
          "assets-single-wrapper"
        );
        const assetModeRadios = document.querySelectorAll(".asset-mode-radio");

        function formatCurrency(value, symbol) {
          if (isNaN(value)) return "–";
          return (
            symbol +
            value.toLocaleString(undefined, { maximumFractionDigits: 0 })
          );
        }

        function formatRatio(value) {
          if (isNaN(value)) return "–";
          return value.toFixed(2) + "×";
        }

        function updateCurrencySymbol() {
          const symbol = currencySelect.value;
          currencyEls.forEach((el) => {
            if (el) el.textContent = symbol;
          });
        }

        currencySelect.addEventListener("change", updateCurrencySymbol);
        updateCurrencySymbol();

        assetModeRadios.forEach((radio) => {
          radio.addEventListener("change", () => {
            if (radio.checked && radio.value === "average") {
              assetsAverageWrapper.classList.remove("hidden");
              assetsSingleWrapper.classList.add("hidden");
            } else if (radio.checked && radio.value === "single") {
              assetsAverageWrapper.classList.add("hidden");
              assetsSingleWrapper.classList.remove("hidden");
            }
          });
        });

        function interpretRatio(ratio) {
          if (!isFinite(ratio) || ratio <= 0) {
            return {
              summary: "",
              bullets: [
                "Check that both sales and assets are positive numbers.",
              ],
            };
          }

          let summary = "";
          const bullets = [];

          if (ratio < 0.5) {
            summary =
              "Very low asset turnover – assets may be under‑utilized or the business is highly capital‑intensive.";
            bullets.push("Review capacity utilization and idle assets.");
            bullets.push("Compare against peers in the same industry.");
          } else if (ratio < 1.0) {
            summary =
              "Moderate asset turnover – typical for many asset‑heavy businesses.";
            bullets.push("Track the trend over several years.");
            bullets.push(
              "Look at fixed asset turnover and inventory turnover for more detail."
            );
          } else if (ratio < 2.0) {
            summary =
              "Healthy asset turnover – assets are being used efficiently in many industries.";
            bullets.push(
              "Compare to direct competitors to confirm the advantage."
            );
            bullets.push(
              "Ensure efficiency is sustainable (no under‑investment in maintenance or capex)."
            );
          } else {
            summary =
              "Very high asset turnover – common in asset‑light business models.";
            bullets.push(
              "Check that capacity and infrastructure are sufficient to support growth."
            );
            bullets.push(
              "Monitor service quality and customer satisfaction; over‑stretching assets can hurt reliability."
            );
          }

          return { summary, bullets };
        }

        calcBtn.addEventListener("click", () => {
          const sales = parseFloat(salesInput.value);
          const mode =
            Array.from(assetModeRadios).find((r) => r.checked)?.value ||
            "average";

          let avgAssets;

          if (mode === "average") {
            const begin = parseFloat(assetsBeginInput.value);
            const end = parseFloat(assetsEndInput.value);
            if (!isFinite(begin) || !isFinite(end) || begin < 0 || end < 0) {
              alert(
                "Please enter valid non‑negative numbers for beginning and ending total assets."
              );
              return;
            }
            avgAssets = (begin + end) / 2;
          } else {
            const single = parseFloat(assetsSingleInput.value);
            if (!isFinite(single) || single < 0) {
              alert(
                "Please enter a valid non‑negative number for total assets."
              );
              return;
            }
            avgAssets = single;
          }

          if (!isFinite(sales) || sales < 0) {
            alert(
              "Please enter a valid non‑negative number for net sales / revenue."
            );
            return;
          }
          if (avgAssets === 0) {
            alert("Average total assets cannot be zero.");
            return;
          }

          const ratio = sales / avgAssets;
          const symbol = currencySelect.value;

          avgAssetsOutput.textContent = formatCurrency(avgAssets, symbol);
          ratioOutput.textContent = formatRatio(ratio);

          const { summary, bullets } = interpretRatio(ratio);
          ratioInterpretation.textContent = summary;

          insightList.innerHTML = "";
          bullets.forEach((text) => {
            const li = document.createElement("li");
            li.textContent = text;
            insightList.appendChild(li);
          });
        });

        resetBtn.addEventListener("click", () => {
          salesInput.value = "";
          assetsBeginInput.value = "";
          assetsEndInput.value = "";
          assetsSingleInput.value = "";
          avgAssetsOutput.textContent = "–";
          ratioOutput.textContent = "–";
          ratioInterpretation.textContent = "";
          insightList.innerHTML =
            "<li>Enter values and click “Calculate” to see insights.</li>";
        });
      })();
    </script>

