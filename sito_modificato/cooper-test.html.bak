<script>
  // Mode toggle
  const modes = {
    distanceToVo2: document.getElementById('mode-distance-to-vo2'),
    vo2ToDistance: document.getElementById('mode-vo2-to-distance'),
    compare: document.getElementById('mode-compare')
  };
  const panels = {
    distanceToVo2: document.getElementById('panel-distance-to-vo2'),
    vo2ToDistance: document.getElementById('panel-vo2-to-distance'),
    compare: document.getElementById('panel-compare')
  };
  const resultsBlocks = {
    distanceToVo2: document.getElementById('results-distance-to-vo2'),
    vo2ToDistance: document.getElementById('results-vo2-to-distance'),
    compare: document.getElementById('results-compare')
  };
  const resultsSection = document.getElementById('results-section');
  const errorMessage = document.getElementById('error-message');
  let currentMode = 'distanceToVo2';

  function setMode(mode) {
    currentMode = mode;
    // Toggle buttons
    Object.entries(modes).forEach(([key, btn]) => {
      if (key === mode) {
        btn.classList.add('toggle-btn-active');
      } else {
        btn.classList.remove('toggle-btn-active');
      }
    });
    // Toggle panels
    Object.entries(panels).forEach(([key, panel]) => {
      panel.classList.toggle('hidden', key !== mode);
    });
    // Hide results & errors
    resultsSection.classList.add('hidden');
    Object.values(resultsBlocks).forEach(b => b.classList.add('hidden'));
    errorMessage.classList.add('hidden');
  }

  modes.distanceToVo2.addEventListener('click', () => setMode('distanceToVo2'));
  modes.vo2ToDistance.addEventListener('click', () => setMode('vo2ToDistance'));
  modes.compare.addEventListener('click', () => setMode('compare'));

  // Helpers
  function toMeters(value, unit) {
    const v = Number(value);
    if (!isFinite(v)) return NaN;
    switch (unit) {
      case 'm': return v;
      case 'km': return v * 1000;
      case 'mi': return v * 1609.344;
      default: return NaN;
    }
  }

  function fromMeters(m) {
    return {
      m: m,
      km: m / 1000,
      mi: m / 1609.344
    };
  }

  function toKg(value, unit) {
    const v = Number(value);
    if (!isFinite(v)) return NaN;
    return unit === 'lb' ? v * 0.45359237 : v;
  }

  function formatNumber(num, decimals = 1) {
    return Number(num).toFixed(decimals);
  }

  // Simple age/sex rating based on distance (approximate)
  function cooperRating(distanceMeters, age, sex) {
    if (!isFinite(distanceMeters)) return { label: 'Unknown', note: 'Enter age and sex for a more precise rating.' };

    const a = Number(age);
    const s = sex === 'male' || sex === 'female' ? sex : 'unknown';

    // Base thresholds for men 20–29
    let thresholds = {
      excellent: 2800,
      good: 2400,
      average: 2200,
      below: 1600
    };

    // Adjust for sex
    if (s === 'female') {
      thresholds = {
        excellent: 2700,
        good: 2200,
        average: 1800,
        below: 1500
      };
    }

    // Adjust for age (roughly -50 m per decade above 30, +50 m per decade below 20)
    if (isFinite(a)) {
      const decadeOffset = (a - 25) / 10; // 25 as reference
      const delta = -50 * decadeOffset;
      Object.keys(thresholds).forEach(k => {
        thresholds[k] += delta;
      });
    }

    let label;
    if (distanceMeters >= thresholds.excellent) label = 'Excellent';
    else if (distanceMeters >= thresholds.good) label = 'Good';
    else if (distanceMeters >= thresholds.average) label = 'Average';
    else if (distanceMeters >= thresholds.below) label = 'Below average';
    else label = 'Poor';

    return {
      label,
      note: 'Approximate rating based on Cooper norms; individual variation applies.'
    };
  }

  // Pace formatting (min/km and min/mile)
  function paceFromDistance(distanceMeters, timeSeconds) {
    if (!isFinite(distanceMeters) || distanceMeters <= 0) return null;
    const speedMps = distanceMeters / timeSeconds;
    const pacePerKmSec = 1000 / speedMps;
    const pacePerMiSec = 1609.344 / speedMps;

    function formatPace(sec) {
      const m = Math.floor(sec / 60);
      const s = Math.round(sec % 60);
      return m + ':' + String(s).padStart(2, '0') + ' min';
    }

    return {
      perKm: formatPace(pacePerKmSec) + '/km',
      perMi: formatPace(pacePerMiSec) + '/mi'
    };
  }

  // Main calculation
  document.getElementById('calculate-btn').addEventListener('click', () => {
    errorMessage.classList.add('hidden');
    resultsSection.classList.add('hidden');
    Object.values(resultsBlocks).forEach(b => b.classList.add('hidden'));

    const sex = document.getElementById('sex').value;
    const age = document.getElementById('age').value ? Number(document.getElementById('age').value) : NaN;

    if (currentMode === 'distanceToVo2') {
      const distanceVal = document.getElementById('distance-value').value;
      const distanceUnit = document.getElementById('distance-unit').value;
      const weightVal = document.getElementById('body-weight').value;
      const weightUnit = document.getElementById('weight-unit').value;

      const dMeters = toMeters(distanceVal, distanceUnit);
      if (!isFinite(dMeters) || dMeters <= 200 || dMeters > 6000) {
        errorMessage.textContent = 'Please enter a realistic distance covered in 12 minutes (e.g. 800–4000 m).';
        errorMessage.classList.remove('hidden');
        return;
      }

      const vo2 = (dMeters - 504.9) / 44.73;
      if (!isFinite(vo2) || vo2 <= 0) {
        errorMessage.textContent = 'Unable to compute a valid VO₂ max from the provided distance.';
        errorMessage.classList.remove('hidden');
        return;
      }

      const rating = cooperRating(dMeters, age, sex);
      const pace = paceFromDistance(dMeters, 12 * 60);

      document.getElementById('vo2-result').textContent = formatNumber(vo2, 1);
      document.getElementById('rating-result').textContent = rating.label;
      document.getElementById('rating-detail').textContent = rating.note;
      document.getElementById('pace-result').textContent = pace ? `${pace.perKm} · ${pace.perMi}` : '–';

      const absWrapper = document.getElementById('absolute-vo2-wrapper');
      const absSpan = document.getElementById('absolute-vo2');
      if (weightVal) {
        const kg = toKg(weightVal, weightUnit);
        if (isFinite(kg) && kg > 0) {
          const absVo2 = vo2 * kg / 1000;
          absSpan.textContent = formatNumber(absVo2, 2);
          absWrapper.classList.remove('hidden');
        } else {
          absWrapper.classList.add('hidden');
        }
      } else {
        absWrapper.classList.add('hidden');
      }

      resultsBlocks.distanceToVo2.classList.remove('hidden');
      resultsSection.classList.remove('hidden');
    } else if (currentMode === 'vo2ToDistance') {
      const vo2TargetVal = document.getElementById('vo2-target').value;
      const weightVal = document.getElementById('body-weight-2').value;
      const weightUnit = document.getElementById('weight-unit-2').value;

      const vo2 = Number(vo2TargetVal);
      if (!isFinite(vo2) || vo2 < 20 || vo2 > 80) {
        errorMessage.textContent = 'Please enter a realistic VO₂ max value between about 20 and 80 ml·kg⁻¹·min⁻¹.';
        errorMessage.classList.remove('hidden');
        return;
      }

      const dMeters = vo2 * 44.73 + 504.9;
      const dAll = fromMeters(dMeters);
      document.getElementById('distance-required').textContent =
        `${Math.round(dAll.m)} m  ·  ${dAll.km.toFixed(2)} km  ·  ${dAll.mi.toFixed(2)} mi`;

      const pace = paceFromDistance(dMeters, 12 * 60);
      document.getElementById('target-pace').textContent = pace ? `${pace.perKm} · ${pace.perMi}` : '–';

      const absWrapper2 = document.getElementById('absolute-vo2-wrapper-2');
      const absSpan2 = document.getElementById('absolute-vo2-2');
      if (weightVal) {
        const kg = toKg(weightVal, weightUnit);
        if (isFinite(kg) && kg > 0) {
          const absVo2 = vo2 * kg / 1000;
          absSpan2.textContent = formatNumber(absVo2, 2);
          absWrapper2.classList.remove('hidden');
        } else {
          absWrapper2.classList.add('hidden');
        }
      } else {
        absWrapper2.classList.add('hidden');
      }

      resultsBlocks.vo2ToDistance.classList.remove('hidden');
      resultsSection.classList.remove('hidden');
    } else if (currentMode === 'compare') {
      const d1Val = document.getElementById('distance1').value;
      const d1Unit = document.getElementById('distance1-unit').value;
      const d2Val = document.getElementById('distance2').value;
      const d2Unit = document.getElementById('distance2-unit').value;

      const d1 = toMeters(d1Val, d1Unit);
      const d2 = toMeters(d2Val, d2Unit);

      if (!isFinite(d1) || !isFinite(d2) || d1 <= 200 || d2 <= 200 || d1 > 6000 || d2 > 6000) {
        errorMessage.textContent = 'Please enter realistic distances for both tests (e.g. 800–4000 m).';
        errorMessage.classList.remove('hidden');
        return;
      }

      const vo2_1 = (d1 - 504.9) / 44.73;
      const vo2_2 = (d2 - 504.9) / 44.73;

      document.getElementById('vo2-test1').textContent = formatNumber(vo2_1, 1) + ' ml·kg⁻¹·min⁻¹';
      document.getElementById('vo2-test2').textContent = formatNumber(vo2_2, 1) + ' ml·kg⁻¹·min⁻¹';

      const deltaVo2 = vo2_2 - vo2_1;
      const deltaDist = d2 - d1;
      const pctVo2 = (deltaVo2 / vo2_1) * 100;

      let summary = `Between Test 1 and Test 2 you improved by ${Math.round(deltaDist)} m `;
      summary += `and ${formatNumber(deltaVo2, 1)} ml·kg⁻¹·min⁻¹ in estimated VO₂ max`;
      if (isFinite(pctVo2)) summary += ` (${formatNumber(pctVo2, 1)}% change).`;
      else summary += '.';

      document.getElementById('improvement').textContent =
        `${Math.round(deltaDist)} m · ${formatNumber(deltaVo2, 1)} ml·kg⁻¹·min⁻¹`;
      document.getElementById('compare-summary').textContent = summary;

      resultsBlocks.compare.classList.remove('hidden');
      resultsSection.classList.remove('hidden');
    }
  });
</script>

