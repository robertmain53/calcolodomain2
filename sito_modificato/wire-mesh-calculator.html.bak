    <script>
      (function () {
        const modeMeshBtn = document.getElementById("mode-mesh");
        const modeOpeningBtn = document.getElementById("mode-opening");
        const meshInputs = document.getElementById("mesh-inputs");
        const openingInputs = document.getElementById("opening-inputs");
        const meshTypeEl = document.getElementById("mesh-type");
        const unitSystemEl = document.getElementById("unit-system");
        const wireDiameterUnit = document.getElementById("wire-diameter-unit");
        const openingXUnit = document.getElementById("opening-x-unit");
        const openingYUnit = document.getElementById("opening-y-unit");
        const widthUnit = document.getElementById("width-unit");
        const lengthUnit = document.getElementById("length-unit");
        const priceUnit = document.getElementById("price-unit");
        const materialDensityEl = document.getElementById("material-density");
        const customDensityRow = document.getElementById("custom-density-row");
        const calculateBtn = document.getElementById("calculate-btn");
        const resetBtn = document.getElementById("reset-btn");

        function setMode(mode) {
          if (mode === "mesh") {
            meshInputs.classList.remove("hidden");
            openingInputs.classList.add("hidden");
            modeMeshBtn.classList.add("tab-active");
            modeOpeningBtn.classList.remove("tab-active");
          } else {
            meshInputs.classList.add("hidden");
            openingInputs.classList.remove("hidden");
            modeMeshBtn.classList.remove("tab-active");
            modeOpeningBtn.classList.add("tab-active");
          }
        }

        function updateUnits() {
          const system = unitSystemEl.value;
          if (system === "metric") {
            wireDiameterUnit.textContent = "mm";
            openingXUnit.textContent = "mm";
            openingYUnit.textContent = "mm";
            widthUnit.textContent = "m";
            lengthUnit.textContent = "m";
            priceUnit.textContent = "per kg";
          } else {
            wireDiameterUnit.textContent = "in";
            openingXUnit.textContent = "in";
            openingYUnit.textContent = "in";
            widthUnit.textContent = "ft";
            lengthUnit.textContent = "ft";
            priceUnit.textContent = "per lb";
          }
        }

        function updateMeshType() {
          const type = meshTypeEl.value;
          const meshYWrapper = document.getElementById("mesh-y-wrapper");
          const openingYWrapper = document.getElementById("opening-y-wrapper");
          if (type === "square") {
            meshYWrapper.classList.add("opacity-60");
            openingYWrapper.classList.add("opacity-60");
          } else {
            meshYWrapper.classList.remove("opacity-60");
            openingYWrapper.classList.remove("opacity-60");
          }
        }

        function updateDensityVisibility() {
          if (materialDensityEl.value === "custom") {
            customDensityRow.classList.remove("hidden");
          } else {
            customDensityRow.classList.add("hidden");
          }
        }

        function parseNumber(id) {
          const v = document.getElementById(id).value;
          const n = parseFloat(v);
          return isNaN(n) ? null : n;
        }

        function formatNumber(value, decimals) {
          if (!isFinite(value)) return "–";
          return value.toFixed(decimals);
        }

        function calculate() {
          const mode = meshInputs.classList.contains("hidden")
            ? "opening"
            : "mesh";
          const system = unitSystemEl.value;
          const type = meshTypeEl.value;

          let meshX = null,
            meshY = null;
          let pitchX_m = null,
            pitchY_m = null;
          let openingX_m = null,
            openingY_m = null;
          let d_m = null;

          const wireDiameter = parseNumber("wire-diameter");
          if (wireDiameter === null || wireDiameter <= 0) {
            alert("Please enter a valid wire diameter.");
            return;
          }

          if (system === "metric") {
            d_m = wireDiameter / 1000.0;
          } else {
            d_m = wireDiameter * 0.0254;
          }

          if (mode === "mesh") {
            meshX = parseNumber("mesh-x");
            meshY = parseNumber("mesh-y");
            if (!meshX || meshX <= 0) {
              alert("Please enter a valid mesh count in X direction.");
              return;
            }
            if (type === "square" || !meshY || meshY <= 0) {
              meshY = meshX;
            }
            const pitchX_in = 1.0 / meshX;
            const pitchY_in = 1.0 / meshY;
            pitchX_m = pitchX_in * 0.0254;
            pitchY_m = pitchY_in * 0.0254;

            openingX_m = pitchX_m - d_m;
            openingY_m = pitchY_m - d_m;
          } else {
            const openingX = parseNumber("opening-x");
            let openingY = parseNumber("opening-y");
            if (openingX === null || openingX <= 0) {
              alert("Please enter a valid clear opening in X direction.");
              return;
            }
            if (type === "square" || openingY === null || openingY <= 0) {
              openingY = openingX;
            }
            if (system === "metric") {
              openingX_m = openingX / 1000.0;
              openingY_m = openingY / 1000.0;
              d_m = d_m;
            } else {
              openingX_m = openingX * 0.0254;
              openingY_m = openingY * 0.0254;
            }
            pitchX_m = openingX_m + d_m;
            pitchY_m = openingY_m + d_m;

            const pitchX_in = pitchX_m / 0.0254;
            const pitchY_in = pitchY_m / 0.0254;
            meshX = 1.0 / pitchX_in;
            meshY = 1.0 / pitchY_in;
          }

          if (openingX_m <= 0 || openingY_m <= 0) {
            alert(
              "Wire diameter is too large for the given mesh/opening – no clear opening remains."
            );
            return;
          }

          const openFracX = openingX_m / pitchX_m;
          const openFracY = openingY_m / pitchY_m;
          const openAreaFrac = openFracX * openFracY;
          const openAreaPct = openAreaFrac * 100.0;

          const dOverPx = d_m / pitchX_m;
          const dOverPy = d_m / pitchY_m;
          const metalFracX = dOverPx;
          const metalFracY = dOverPy;
          const metalFrac = metalFracX + metalFracY - metalFracX * metalFracY;

          let density = parseFloat(materialDensityEl.value);
          if (materialDensityEl.value === "custom") {
            const cd = parseNumber("custom-density");
            if (!cd || cd <= 0) {
              alert("Please enter a valid custom density.");
              return;
            }
            density = cd;
          }

          const weightPerArea = metalFrac * density; // kg/m²

          const width = parseNumber("width");
          const length = parseNumber("length");
          const quantity = parseNumber("quantity") || 1;
          if (!width || !length || width <= 0 || length <= 0) {
            alert("Please enter valid panel width and length.");
            return;
          }

          let width_m, length_m;
          if (system === "metric") {
            width_m = width;
            length_m = length;
          } else {
            width_m = width * 0.3048;
            length_m = length * 0.3048;
          }

          const area_m2 = width_m * length_m;
          const weightPanel_kg = weightPerArea * area_m2;
          const totalWeight_kg = weightPanel_kg * quantity;

          const pricePerWeight = parseNumber("price-per-kg");
          let costPanel = null;
          let costTotal = null;
          if (pricePerWeight && pricePerWeight > 0) {
            if (system === "imperial") {
              const pricePerKg = pricePerWeight * 2.2046226218;
              costPanel = weightPanel_kg * pricePerKg;
              costTotal = totalWeight_kg * pricePerKg;
            } else {
              costPanel = weightPanel_kg * pricePerWeight;
              costTotal = totalWeight_kg * pricePerWeight;
            }
          }

          const resMeshX = document.getElementById("res-mesh-x");
          const resMeshY = document.getElementById("res-mesh-y");
          const resPitchX = document.getElementById("res-pitch-x");
          const resPitchY = document.getElementById("res-pitch-y");
          const resOpeningX = document.getElementById("res-opening-x");
          const resOpeningY = document.getElementById("res-opening-y");
          const resOpenArea = document.getElementById("res-open-area");
          const resArea = document.getElementById("res-area");
          const resWeightPerArea = document.getElementById(
            "res-weight-per-area"
          );
          const resWeightPanel = document.getElementById("res-weight-panel");
          const resWeightTotal = document.getElementById("res-weight-total");
          const resCostPanel = document.getElementById("res-cost-panel");
          const resCostTotal = document.getElementById("res-cost-total");

          resMeshX.textContent = formatNumber(meshX, 2) + " openings/in";
          resMeshY.textContent = formatNumber(meshY, 2) + " openings/in";

          const pitchX_mm = pitchX_m * 1000.0;
          const pitchY_mm = pitchY_m * 1000.0;
          const openingX_mm = openingX_m * 1000.0;
          const openingY_mm = openingY_m * 1000.0;

          resPitchX.textContent = formatNumber(pitchX_mm, 2) + " mm";
          resPitchY.textContent = formatNumber(pitchY_mm, 2) + " mm";
          resOpeningX.textContent = formatNumber(openingX_mm, 2) + " mm";
          resOpeningY.textContent = formatNumber(openingY_mm, 2) + " mm";
          resOpenArea.textContent = formatNumber(openAreaPct, 2) + " %";

          if (system === "metric") {
            resArea.textContent = formatNumber(area_m2, 3) + " m²";
            resWeightPerArea.textContent =
              formatNumber(weightPerArea, 2) + " kg/m²";
            resWeightPanel.textContent =
              formatNumber(weightPanel_kg, 2) + " kg";
            resWeightTotal.textContent =
              formatNumber(totalWeight_kg, 2) + " kg";
          } else {
            const area_ft2 = area_m2 * 10.7639104167;
            const weightPerArea_lb_ft2 =
              (weightPerArea * 0.2046226218) / 10.7639104167;
            const weightPanel_lb = weightPanel_kg * 2.2046226218;
            const totalWeight_lb = totalWeight_kg * 2.2046226218;
            resArea.textContent = formatNumber(area_ft2, 2) + " ft²";
            resWeightPerArea.textContent =
              formatNumber(weightPerArea_lb_ft2, 3) + " lb/ft²";
            resWeightPanel.textContent =
              formatNumber(weightPanel_lb, 2) + " lb";
            resWeightTotal.textContent =
              formatNumber(totalWeight_lb, 2) + " lb";
          }

          if (costPanel !== null && costTotal !== null) {
            resCostPanel.textContent = formatNumber(costPanel, 2);
            resCostTotal.textContent = formatNumber(costTotal, 2);
          } else {
            resCostPanel.textContent = "–";
            resCostTotal.textContent = "–";
          }
        }

        function resetForm() {
          document.getElementById("wire-mesh-form").reset();
          setMode("mesh");
          updateUnits();
          updateMeshType();
          updateDensityVisibility();
          const ids = [
            "res-mesh-x",
            "res-mesh-y",
            "res-pitch-x",
            "res-pitch-y",
            "res-opening-x",
            "res-opening-y",
            "res-open-area",
            "res-area",
            "res-weight-per-area",
            "res-weight-panel",
            "res-weight-total",
            "res-cost-panel",
            "res-cost-total",
          ];
          ids.forEach((id) => {
            const el = document.getElementById(id);
            if (el) el.textContent = "–";
          });
        }

        modeMeshBtn.addEventListener("click", () => setMode("mesh"));
        modeOpeningBtn.addEventListener("click", () => setMode("opening"));
        unitSystemEl.addEventListener("change", updateUnits);
        meshTypeEl.addEventListener("change", updateMeshType);
        materialDensityEl.addEventListener("change", updateDensityVisibility);
        calculateBtn.addEventListener("click", calculate);
        resetBtn.addEventListener("click", resetForm);

        updateUnits();
        updateMeshType();
      })();
    </script>

