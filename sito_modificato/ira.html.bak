    <script>
      // Utility: format currency
      function fmtCurrency(value) {
        if (!isFinite(value)) return "$0";
        return value.toLocaleString("en-US", {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 0,
        });
      }
      function fmtPercent(value) {
        if (!isFinite(value)) return "0%";
        return value.toFixed(1).replace(/\.0$/, "") + "%";
      }

      // Sync slider labels
      const sliders = [
        { id: "expected-return", label: "expected-return-value", suffix: "%" },
        {
          id: "current-tax-rate",
          label: "current-tax-rate-value",
          suffix: "%",
        },
        {
          id: "retirement-tax-rate",
          label: "retirement-tax-rate-value",
          suffix: "%",
        },
      ];
      sliders.forEach((s) => {
        const input = document.getElementById(s.id);
        const label = document.getElementById(s.label);
        if (!input || !label) return;
        const update = () => {
          label.textContent = input.value + s.suffix;
        };
        input.addEventListener("input", update);
        update();
      });

      // Contribution type toggle
      let contribMode = "equal"; // 'equal' or 'afterTax'
      const contribEqualBtn = document.getElementById("contrib-equal");
      const contribAfterTaxBtn = document.getElementById("contrib-after-tax");
      function setContribMode(mode) {
        contribMode = mode;
        if (mode === "equal") {
          contribEqualBtn.classList.add(
            "bg-blue-50",
            "text-blue-700",
            "border-blue-400"
          );
          contribAfterTaxBtn.classList.remove(
            "bg-blue-50",
            "text-blue-700",
            "border-blue-400"
          );
        } else {
          contribAfterTaxBtn.classList.add(
            "bg-blue-50",
            "text-blue-700",
            "border-blue-400"
          );
          contribEqualBtn.classList.remove(
            "bg-blue-50",
            "text-blue-700",
            "border-blue-400"
          );
        }
      }
      contribEqualBtn.addEventListener("click", () => setContribMode("equal"));
      contribAfterTaxBtn.addEventListener("click", () =>
        setContribMode("afterTax")
      );

      // Presets
      const presetButtons = document.querySelectorAll("[data-preset]");
      presetButtons.forEach((btn) => {
        btn.addEventListener("click", () => {
          const p = btn.getAttribute("data-preset");
          const currentAge = document.getElementById("current-age");
          const retirementAge = document.getElementById("retirement-age");
          const currentBalance = document.getElementById("current-balance");
          const annualContribution = document.getElementById(
            "annual-contribution"
          );
          const expectedReturn = document.getElementById("expected-return");
          const currentTax = document.getElementById("current-tax-rate");
          const retireTax = document.getElementById("retirement-tax-rate");

          if (p === "starter") {
            currentAge.value = 25;
            retirementAge.value = 67;
            currentBalance.value = 0;
            annualContribution.value = 6000;
            expectedReturn.value = 7;
            expectedReturn.dispatchEvent(new Event("input"));
            currentTax.value = 22;
            currentTax.dispatchEvent(new Event("input"));
            retireTax.value = 24;
            retireTax.dispatchEvent(new Event("input"));
          } else if (p === "mid") {
            currentAge.value = 40;
            retirementAge.value = 67;
            currentBalance.value = 80000;
            annualContribution.value = 7500;
            expectedReturn.value = 6.5;
            expectedReturn.dispatchEvent(new Event("input"));
            currentTax.value = 24;
            currentTax.dispatchEvent(new Event("input"));
            retireTax.value = 22;
            retireTax.dispatchEvent(new Event("input"));
          } else if (p === "late") {
            currentAge.value = 55;
            retirementAge.value = 67;
            currentBalance.value = 150000;
            annualContribution.value = 8000;
            expectedReturn.value = 5.5;
            expectedReturn.dispatchEvent(new Event("input"));
            currentTax.value = 24;
            currentTax.dispatchEvent(new Event("input"));
            retireTax.value = 22;
            retireTax.dispatchEvent(new Event("input"));
          }
        });
      });

      function calculateIRA() {
        const errorEl = document.getElementById("error-message");
        errorEl.textContent = "";

        const age = parseFloat(document.getElementById("current-age").value);
        const retireAge = parseFloat(
          document.getElementById("retirement-age").value
        );
        const balance0 = parseFloat(
          document.getElementById("current-balance").value
        );
        const contribInput = parseFloat(
          document.getElementById("annual-contribution").value
        );
        const r =
          parseFloat(document.getElementById("expected-return").value) / 100;
        const taxNow =
          parseFloat(document.getElementById("current-tax-rate").value) / 100;
        const taxRetire =
          parseFloat(document.getElementById("retirement-tax-rate").value) /
          100;

        if (isNaN(age) || isNaN(retireAge) || retireAge <= age) {
          errorEl.textContent =
            "Retirement age must be greater than current age.";
          return;
        }
        if (r < 0) {
          errorEl.textContent = "Expected return must be zero or positive.";
          return;
        }
        if (contribInput < 0 || balance0 < 0) {
          errorEl.textContent =
            "Balances and contributions must be zero or positive.";
          return;
        }

        const years = Math.max(0, Math.round(retireAge - age));

        // Contributions for each account
        let rothContrib = contribInput;
        let tradContrib = contribInput;
        if (contribMode === "afterTax" && taxNow < 1) {
          // Same after-tax cost: increase Traditional contribution
          tradContrib = contribInput / (1 - taxNow || 1);
        }

        // Future value helper
        function futureValue(balance, contrib, rate, n) {
          const fvBalance = balance * Math.pow(1 + rate, n);
          const fvContrib =
            rate === 0
              ? contrib * n
              : contrib * ((Math.pow(1 + rate, n) - 1) / rate);
          return { fvBalance, fvContrib, total: fvBalance + fvContrib };
        }

        const tradFV = futureValue(balance0, tradContrib, r, years);
        const rothFV = futureValue(balance0, rothContrib, r, years);

        const tradTaxDue = tradFV.total * taxRetire;
        const tradAfterTax = tradFV.total - tradTaxDue;
        const rothTaxDue = 0; // assuming qualified withdrawals
        const rothAfterTax = rothFV.total;

        const tradTaxSavingsNow = tradContrib * taxNow;
        const rothTaxCostNow = rothContrib * taxNow;

        // Update results
        document.getElementById("trad-contrib").textContent = fmtCurrency(
          tradContrib * years
        );
        document.getElementById("trad-balance").textContent = fmtCurrency(
          tradFV.total
        );
        document.getElementById("trad-tax").textContent =
          fmtCurrency(tradTaxDue);
        document.getElementById("trad-after-tax").textContent =
          fmtCurrency(tradAfterTax);
        document.getElementById("trad-tax-savings").textContent = fmtCurrency(
          tradTaxSavingsNow * years
        );

        document.getElementById("roth-contrib").textContent = fmtCurrency(
          rothContrib * years
        );
        document.getElementById("roth-balance").textContent = fmtCurrency(
          rothFV.total
        );
        document.getElementById("roth-tax").textContent =
          fmtCurrency(rothTaxDue);
        document.getElementById("roth-after-tax").textContent =
          fmtCurrency(rothAfterTax);
        document.getElementById("roth-tax-cost").textContent = fmtCurrency(
          rothTaxCostNow * years
        );

        // Comparison table
        document.getElementById("years-invested-td").textContent =
          years.toString();
        document.getElementById("trad-contrib-td").textContent = fmtCurrency(
          tradContrib * years
        );
        document.getElementById("roth-contrib-td").textContent = fmtCurrency(
          rothContrib * years
        );
        document.getElementById("trad-after-tax-td").textContent =
          fmtCurrency(tradAfterTax);
        document.getElementById("roth-after-tax-td").textContent =
          fmtCurrency(rothAfterTax);

        const tradEffectiveTax =
          tradFV.total > 0 ? (tradTaxDue / tradFV.total) * 100 : 0;
        const rothEffectiveTax = 0;
        document.getElementById("trad-effective-tax-td").textContent =
          fmtPercent(tradEffectiveTax);
        document.getElementById("roth-effective-tax-td").textContent =
          fmtPercent(rothEffectiveTax);

        // Winner text
        const winnerText = document.getElementById("winner-text");
        if (Math.abs(tradAfterTax - rothAfterTax) < 1) {
          winnerText.textContent =
            "Under these assumptions, Traditional and Roth IRAs produce nearly the same after-tax value at retirement.";
        } else if (tradAfterTax > rothAfterTax) {
          winnerText.textContent =
            "Under these assumptions, the Traditional IRA leaves you with more after-tax money at retirement than the Roth IRA.";
        } else {
          winnerText.textContent =
            "Under these assumptions, the Roth IRA leaves you with more after-tax money at retirement than the Traditional IRA.";
        }
      }

      document
        .getElementById("calculate-btn")
        .addEventListener("click", calculateIRA);

      document.getElementById("reset-btn").addEventListener("click", () => {
        document.getElementById("current-age").value = 35;
        document.getElementById("retirement-age").value = 67;
        document.getElementById("current-balance").value = 0;
        document.getElementById("annual-contribution").value = 6500;
        document.getElementById("expected-return").value = 7;
        document
          .getElementById("expected-return")
          .dispatchEvent(new Event("input"));
        document.getElementById("current-tax-rate").value = 24;
        document
          .getElementById("current-tax-rate")
          .dispatchEvent(new Event("input"));
        document.getElementById("retirement-tax-rate").value = 22;
        document
          .getElementById("retirement-tax-rate")
          .dispatchEvent(new Event("input"));
        setContribMode("equal");
        document.getElementById("error-message").textContent = "";
        document.getElementById("winner-text").textContent =
          "Enter your information and click “Calculate” to compare.";
        // Clear key outputs
        const idsToClear = [
          "trad-contrib",
