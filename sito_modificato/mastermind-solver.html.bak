    <script>
      // Mastermind Solver Logic
      (function () {
        const colorDefs = [
          { id: 1, name: "1", class: "bg-red-500" },
          { id: 2, name: "2", class: "bg-green-500" },
          { id: 3, name: "3", class: "bg-blue-500" },
          { id: 4, name: "4", class: "bg-yellow-400" },
          { id: 5, name: "5", class: "bg-purple-500" },
          { id: 6, name: "6", class: "bg-pink-500" },
          { id: 7, name: "7", class: "bg-orange-500" },
          { id: 8, name: "8", class: "bg-teal-500" },
        ];

        let codeLength = 4;
        let numColors = 6;
        let allowDuplicates = true;
        let candidates = [];
        let history = [];
        let currentGuess = [];
        let selectedColor = 1;

        const codeLengthEl = document.getElementById("code-length");
        const numColorsEl = document.getElementById("num-colors");
        const allowDuplicatesEl = document.getElementById("allow-duplicates");
        const autoSuggestEl = document.getElementById("auto-suggest");
        const newGameBtn = document.getElementById("new-game-btn");
        const colorPaletteEl = document.getElementById("color-palette");
        const currentGuessPegsEl =
          document.getElementById("current-guess-pegs");
        const blackPegsEl = document.getElementById("black-pegs");
        const whitePegsEl = document.getElementById("white-pegs");
        const applyFeedbackBtn = document.getElementById("apply-feedback-btn");
        const suggestGuessBtn = document.getElementById("suggest-guess-btn");
        const statusMessageEl = document.getElementById("status-message");
        const candidatesCountEl = document.getElementById("candidates-count");
        const guessesCountEl = document.getElementById("guesses-count");
        const lastEntropyEl = document.getElementById("last-entropy");
        const bestCaseEl = document.getElementById("best-case");
        const worstCaseEl = document.getElementById("worst-case");
        const historyContainerEl = document.getElementById("history-container");
        const candidatesListEl = document.getElementById("candidates-list");

        function renderPalette() {
          colorPaletteEl.innerHTML = "";
          for (let i = 0; i < numColors; i++) {
            const c = colorDefs[i];
            const btn = document.createElement("button");
            btn.type = "button";
            btn.className = `color-swatch ${c.class} ${
              c.id === selectedColor ? "selected" : ""
            }`;
            btn.textContent = c.name;
            btn.dataset.colorId = c.id;
            btn.addEventListener("click", () => {
              selectedColor = c.id;
              renderPalette();
            });
            colorPaletteEl.appendChild(btn);
          }
        }

        function generateAllCodes() {
          const colors = [];
          for (let i = 1; i <= numColors; i++) colors.push(i);

          const result = [];
          function backtrack(pos, current) {
            if (pos === codeLength) {
              result.push(current.slice());
              return;
            }
            for (let c of colors) {
              if (!allowDuplicates && current.includes(c)) continue;
              current.push(c);
              backtrack(pos + 1, current);
              current.pop();
            }
          }
          backtrack(0, []);
          return result;
        }

        function feedback(secret, guess) {
          let black = 0;
          const secretCounts = {};
          const guessCounts = {};
          for (let i = 0; i < secret.length; i++) {
            if (secret[i] === guess[i]) {
              black++;
            } else {
              secretCounts[secret[i]] = (secretCounts[secret[i]] || 0) + 1;
              guessCounts[guess[i]] = (guessCounts[guess[i]] || 0) + 1;
            }
          }
          let colorMatches = 0;
          for (let c in guessCounts) {
            if (secretCounts[c]) {
              colorMatches += Math.min(secretCounts[c], guessCounts[c]);
            }
          }
          const white = colorMatches;
          return { black, white };
        }

        function codesEqual(a, b) {
          if (a.length !== b.length) return false;
          for (let i = 0; i < a.length; i++) if (a[i] !== b[i]) return false;
          return true;
        }

        function filterCandidates(guess, fb) {
          candidates = candidates.filter((code) => {
            const f = feedback(code, guess);
            return f.black === fb.black && f.white === fb.white;
          });
        }

        function entropyForGuess(guess) {
          const map = new Map();
          for (let code of candidates) {
            const f = feedback(code, guess);
            const key = f.black + "-" + f.white;
            map.set(key, (map.get(key) || 0) + 1);
          }
          let H = 0;
          let best = Infinity;
          let worst = 0;
          const total = candidates.length || 1;
          for (let count of map.values()) {
            const p = count / total;
            H -= p * Math.log2(p);
            if (count < best) best = count;
            if (count > worst) worst = count;
          }
          return { H, best, worst };
        }

        function suggestNextGuess() {
          if (candidates.length === 0) {
            statusMessageEl.textContent =
              "No candidates left. Check that previous feedback was entered correctly, or start a new game.";
            return null;
          }
          if (candidates.length === 1) {
            currentGuess = candidates[0].slice();
            renderCurrentGuess();
            const { H, best, worst } = entropyForGuess(currentGuess);
            updateStatsAfterSuggestion(H, best, worst);
            statusMessageEl.textContent =
              "Only one candidate left – this must be the secret code.";
            return currentGuess;
          }

          let bestGuess = null;
          let bestScore = -Infinity;
          let bestStats = { H: 0, best: 0, worst: 0 };

          // Limit search space for performance if many candidates
          const pool =
            candidates.length > 400 ? candidates.slice(0, 400) : candidates;

          for (let g of pool) {
            const { H, best, worst } = entropyForGuess(g);
            // Prefer higher entropy; tie-breaker: smaller worst-case
            const score = H - 0.0001 * worst;
            if (score > bestScore) {
              bestScore = score;
              bestGuess = g;
              bestStats = { H, best, worst };
            }
          }

          currentGuess = bestGuess.slice();
          renderCurrentGuess();
          updateStatsAfterSuggestion(
            bestStats.H,
            bestStats.best,
            bestStats.worst
          );
          statusMessageEl.textContent =
            "Suggested next guess based on maximum information gain.";
          return currentGuess;
        }

        function updateStatsAfterSuggestion(H, best, worst) {
          lastEntropyEl.textContent = H.toFixed(3) + " bits";
          bestCaseEl.textContent = best.toString();
          worstCaseEl.textContent = worst.toString();
        }

        function renderCurrentGuess() {
          currentGuessPegsEl.innerHTML = "";
          for (let i = 0; i < codeLength; i++) {
            const val = currentGuess[i] || null;
            const peg = document.createElement("button");
            peg.type = "button";
            peg.className =
              "peg " + (val ? colorDefs[val - 1].class : "peg-empty");
            peg.dataset.index = i;
            peg.textContent = val ? colorDefs[val - 1].name : "?";
            peg.addEventListener("click", () => {
              currentGuess[i] = selectedColor;
              renderCurrentGuess();
            });
            currentGuessPegsEl.appendChild(peg);
          }
        }

        function renderHistory() {
          historyContainerEl.innerHTML = "";
          if (!history.length) {
            const p = document.createElement("p");
            p.className = "text-xs text-gray-500";
            p.textContent =
              "No guesses yet. Configure the game and click “Suggest Next Guess” or enter your own guess.";
            historyContainerEl.appendChild(p);
            return;
          }
          history.forEach((entry, idx) => {
            const row = document.createElement("div");
            row.className =
              "flex items-center justify-between border-b border-gray-100 pb-1 mb-1";
            const left = document.createElement("div");
            left.className = "flex items-center gap-2";
            const label = document.createElement("span");
            label.className = "text-xs text-gray-500";
            label.textContent = "#" + (idx + 1);
            left.appendChild(label);
            entry.guess.forEach((v) => {
              const peg = document.createElement("span");
              peg.className = "peg " + colorDefs[v - 1].class;
              peg.textContent = colorDefs[v - 1].name;
              left.appendChild(peg);
            });
            const right = document.createElement("div");
            right.className = "text-xs text-gray-600";
            right.textContent = `B:${entry.black} W:${entry.white}`;
            row.appendChild(left);
            row.appendChild(right);
            historyContainerEl.appendChild(row);
          });
        }

        function renderCandidates() {
          candidatesCountEl.textContent = candidates.length.toString();
          candidatesListEl.innerHTML = "";
          if (!candidates.length) {
            const p = document.createElement("p");
            p.className = "text-gray-500";
            p.textContent =
              "No candidates left. Check your feedback or start a new game.";
            candidatesListEl.appendChild(p);
            return;
          }
          const maxShow = 200;
          const toShow = candidates.slice(0, maxShow);
          const span = document.createElement("span");
          span.textContent = toShow.map((code) => code.join("")).join(" ");
          candidatesListEl.appendChild(span);
          if (candidates.length > maxShow) {
            const p = document.createElement("p");
            p.className = "mt-1 text-gray-500";
            p.textContent = `… and ${candidates.length - maxShow} more.`;
            candidatesListEl.appendChild(p);
          }
        }

        function resetGame() {
          codeLength = parseInt(codeLengthEl.value, 10);
          numColors = parseInt(numColorsEl.value, 10);
          allowDuplicates = allowDuplicatesEl.checked;
          candidates = generateAllCodes();
          history = [];
          currentGuess = new Array(codeLength).fill(null);
          selectedColor = 1;
          blackPegsEl.value = 0;
          whitePegsEl.value = 0;
          guessesCountEl.textContent = "0";
          lastEntropyEl.textContent = "—";
          bestCaseEl.textContent = "—";
          worstCaseEl.textContent = "—";
          statusMessageEl.textContent =
            "New game started. Enter a guess or let the solver suggest one.";
          renderPalette();
          renderCurrentGuess();
          renderHistory();
          renderCandidates();
        }

        newGameBtn.addEventListener("click", resetGame);
        codeLengthEl.addEventListener("change", resetGame);
        numColorsEl.addEventListener("change", resetGame);
        allowDuplicatesEl.addEventListener("change", resetGame);

        applyFeedbackBtn.addEventListener("click", () => {
          if (currentGuess.some((v) => !v)) {
            statusMessageEl.textContent =
              "Please fill all pegs in the current guess before applying feedback.";
            return;
          }
          const black = parseInt(blackPegsEl.value, 10) || 0;
          const white = parseInt(whitePegsEl.value, 10) || 0;
          if (black < 0 || white < 0 || black + white > codeLength) {
            statusMessageEl.textContent =
              "Feedback values are not valid for this code length.";
            return;
          }
          history.push({ guess: currentGuess.slice(), black, white });
          guessesCountEl.textContent = history.length.toString();
          filterCandidates(currentGuess, { black, white });
          renderHistory();
          renderCandidates();

          if (candidates.length === 0) {
            statusMessageEl.textContent =
              "No candidates remain. Feedback is inconsistent with previous rounds. Consider undoing or starting a new game.";
            return;
          }
          if (candidates.length === 1 && black !== codeLength) {
            statusMessageEl.textContent =
              "Only one candidate remains. This is almost certainly the secret code.";
          } else if (black === codeLength) {
            statusMessageEl.textContent =
              "Congratulations! The code is solved.";
          } else {
            statusMessageEl.textContent =
              "Feedback applied. Candidates filtered. You can now request the next suggestion.";
          }

          if (autoSuggestEl.checked && black !== codeLength) {
            suggestNextGuess();
          } else {
            currentGuess = new Array(codeLength).fill(null);
            renderCurrentGuess();
          }
        });

        suggestGuessBtn.addEventListener("click", () => {
          suggestNextGuess();
        });

        // Initialize
        resetGame();
      })();
    </script>

