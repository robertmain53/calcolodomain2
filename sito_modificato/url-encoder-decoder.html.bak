    <script>
      (function () {
        const inputEl = document.getElementById("input-text");
        const outputEl = document.getElementById("output-text");
        const statusModeEl = document.getElementById("status-mode");

        const modeEncodeBtn = document.getElementById("mode-encode");
        const modeDecodeBtn = document.getElementById("mode-decode");
        const modeAutoBtn = document.getElementById("mode-auto");

        const optSpacePlus = document.getElementById("opt-space-plus");
        const optMultiLine = document.getElementById("opt-multi-line");

        const btnEncode = document.getElementById("btn-encode");
        const btnDecode = document.getElementById("btn-decode");
        const btnSwap = document.getElementById("btn-swap");
        const btnClear = document.getElementById("btn-clear");
        const btnCopyOutput = document.getElementById("btn-copy-output");

        const exampleButtons = document.querySelectorAll(".example-btn");

        let mode = "encode"; // 'encode' | 'decode' | 'auto'

        function setMode(newMode) {
          mode = newMode;
          statusModeEl.textContent =
            newMode.charAt(0).toUpperCase() + newMode.slice(1);
          [modeEncodeBtn, modeDecodeBtn, modeAutoBtn].forEach((btn) => {
            btn.classList.remove(
              "border-blue-600",
              "text-blue-600",
              "bg-blue-50"
            );
            btn.classList.add("border-gray-300", "text-gray-700", "bg-white");
          });
          if (newMode === "encode") {
            modeEncodeBtn.classList.add(
              "border-blue-600",
              "text-blue-600",
              "bg-blue-50"
            );
          } else if (newMode === "decode") {
            modeDecodeBtn.classList.add(
              "border-blue-600",
              "text-blue-600",
              "bg-blue-50"
            );
          } else {
            modeAutoBtn.classList.add(
              "border-blue-600",
              "text-blue-600",
              "bg-blue-50"
            );
          }
        }

        modeEncodeBtn.addEventListener("click", () => setMode("encode"));
        modeDecodeBtn.addEventListener("click", () => setMode("decode"));
        modeAutoBtn.addEventListener("click", () => setMode("auto"));

        function encodeValue(str) {
          if (!optMultiLine.checked) {
            return encodeSingle(str);
          }
          return str.split(/\r?\n/).map(encodeSingle).join("\n");
        }

        function encodeSingle(str) {
          let encoded = encodeURIComponent(str);
          if (optSpacePlus.checked) {
            encoded = encoded.replace(/%20/g, "+");
          }
          return encoded;
        }

        function decodeValue(str) {
          if (!optMultiLine.checked) {
            return decodeSingle(str);
          }
          return str.split(/\r?\n/).map(decodeSingle).join("\n");
        }

        function decodeSingle(str) {
          let s = str;
          if (optSpacePlus.checked) {
            s = s.replace(/\+/g, "%20");
          }
          try {
            return decodeURIComponent(s);
          } catch (e) {
            // Fallback: try to decode best-effort by only decoding valid sequences
            try {
              return decodeURI(s);
            } catch (e2) {
              return s; // return original if completely invalid
            }
          }
        }

        function autoDetectAndConvert(str) {
          // Simple heuristic: if it contains %XX or + and decodes cleanly, treat as encoded
          const hasPercent = /%[0-9A-Fa-f]{2}/.test(str);
          const hasPlus = /\+/.test(str);
          if (hasPercent || (hasPlus && optSpacePlus.checked)) {
            const decoded = decodeValue(str);
            return { modeUsed: "decode", result: decoded };
          }
          const encoded = encodeValue(str);
          return { modeUsed: "encode", result: encoded };
        }

        function doEncode() {
          const input = inputEl.value || "";
          outputEl.value = encodeValue(input);
        }

        function doDecode() {
          const input = inputEl.value || "";
          outputEl.value = decodeValue(input);
        }

        btnEncode.addEventListener("click", () => {
          setMode("encode");
          doEncode();
        });

        btnDecode.addEventListener("click", () => {
          setMode("decode");
          doDecode();
        });

        btnSwap.addEventListener("click", () => {
          const tmp = inputEl.value;
          inputEl.value = outputEl.value;
          outputEl.value = tmp;
        });

        btnClear.addEventListener("click", () => {
          inputEl.value = "";
          outputEl.value = "";
        });

        btnCopyOutput.addEventListener("click", async () => {
          try {
            await navigator.clipboard.writeText(outputEl.value || "");
            btnCopyOutput.textContent = "Copied";
            setTimeout(() => {
              btnCopyOutput.textContent = "Copy";
            }, 1200);
          } catch (e) {
            btnCopyOutput.textContent = "Error";
            setTimeout(() => {
              btnCopyOutput.textContent = "Copy";
            }, 1200);
          }
        });

        exampleButtons.forEach((btn) => {
          btn.addEventListener("click", () => {
            const example = btn.getAttribute("data-example") || "";
            inputEl.value = example;
            if (mode === "encode") {
              doEncode();
            } else if (mode === "decode") {
              doDecode();
            } else {
              const { modeUsed, result } = autoDetectAndConvert(example);
              setMode(modeUsed);
              outputEl.value = result;
            }
          });
        });

        // Live auto mode: update output as user types when in auto mode
        inputEl.addEventListener("input", () => {
          if (mode !== "auto") return;
          const text = inputEl.value || "";
          if (!text) {
            outputEl.value = "";
            return;
          }
          const { modeUsed, result } = autoDetectAndConvert(text);
          statusModeEl.textContent = "Auto (" + modeUsed + ")";
          outputEl.value = result;
        });

        // Initialize defaults
        setMode("encode");
      })();
    </script>

