    <script>
      // Unit handling and core calculations
      const unitSystemEl = document.getElementById("unitSystem");
      const spanEl = document.getElementById("span");
      const bearingEl = document.getElementById("bearing");
      const widthEl = document.getElementById("width");
      const depthEl = document.getElementById("depth");
      const wUniformEl = document.getElementById("wUniform");
      const pPointEl = document.getElementById("pPoint");
      const fAllowBendEl = document.getElementById("fAllowBend");
      const fAllowShearEl = document.getElementById("fAllowShear");
      const EEl = document.getElementById("E");
      const deflLimitEl = document.getElementById("deflectionLimit");
      const sectionOverrideEl = document.getElementById("sectionOverride");
      const sectionOverrideFields = document.getElementById(
        "sectionOverrideFields"
      );
      const SxEl = document.getElementById("Sx");
      const IxEl = document.getElementById("Ix");
      const calcErrorEl = document.getElementById("calcError");

      const spanUnitLabel = document.getElementById("spanUnitLabel");
      const bearingUnitLabel = document.getElementById("bearingUnitLabel");
      const widthUnitLabel = document.getElementById("widthUnitLabel");
      const depthUnitLabel = document.getElementById("depthUnitLabel");
      const wUnitLabel = document.getElementById("wUnitLabel");
      const pUnitLabel = document.getElementById("pUnitLabel");
      const fBendUnitLabel = document.getElementById("fBendUnitLabel");
      const fShearUnitLabel = document.getElementById("fShearUnitLabel");
      const eUnitLabel = document.getElementById("eUnitLabel");
      const sUnitLabel = document.getElementById("sUnitLabel");
      const iUnitLabel = document.getElementById("iUnitLabel");

      const resMmax = document.getElementById("resMmax");
      const resVmax = document.getElementById("resVmax");
      const resDefl = document.getElementById("resDefl");
      const resBendDetail = document.getElementById("resBendDetail");
      const resShearDetail = document.getElementById("resShearDetail");
      const resDeflDetail = document.getElementById("resDeflDetail");
      const statusBend = document.getElementById("statusBend");
      const statusShear = document.getElementById("statusShear");
      const statusDefl = document.getElementById("statusDefl");
      const overallStatus = document.getElementById("overallStatus");

      function setUnitLabels() {
        if (unitSystemEl.value === "metric") {
          spanUnitLabel.textContent = " (m)";
          bearingUnitLabel.textContent = " (mm)";
          widthUnitLabel.textContent = " (mm)";
          depthUnitLabel.textContent = " (mm)";
          wUnitLabel.textContent = " (kN/m)";
          pUnitLabel.textContent = " (kN)";
          fBendUnitLabel.textContent = " (MPa)";
          fShearUnitLabel.textContent = " (MPa)";
          eUnitLabel.textContent = " (GPa)";
          sUnitLabel.textContent = " (mm³)";
          iUnitLabel.textContent = " (mm⁴)";
        } else {
          spanUnitLabel.textContent = " (ft)";
          bearingUnitLabel.textContent = " (in)";
          widthUnitLabel.textContent = " (in)";
          depthUnitLabel.textContent = " (in)";
          wUnitLabel.textContent = " (plf)";
          pUnitLabel.textContent = " (lb)";
          fBendUnitLabel.textContent = " (ksi)";
          fShearUnitLabel.textContent = " (ksi)";
          eUnitLabel.textContent = " (ksi)";
          sUnitLabel.textContent = " (in³)";
          iUnitLabel.textContent = " (in⁴)";
        }
      }

      function setStatusBadge(el, pass) {
        if (pass === null) {
          el.textContent = "–";
          el.className = "result-badge bg-gray-100 text-gray-700";
          return;
        }
        if (pass) {
          el.textContent = "PASS";
          el.className = "result-badge bg-green-100 text-green-700";
        } else {
          el.textContent = "FAIL";
          el.className = "result-badge bg-red-100 text-red-700";
        }
      }

      function calculateLintel() {
        calcErrorEl.classList.add("hidden");
        calcErrorEl.textContent = "";

        const system = unitSystemEl.value;

        let L = parseFloat(spanEl.value);
        let b = parseFloat(widthEl.value);
        let d = parseFloat(depthEl.value);
        let w = parseFloat(wUniformEl.value);
        let P = parseFloat(pPointEl.value);
        let fB = parseFloat(fAllowBendEl.value);
        let fV = parseFloat(fAllowShearEl.value);
        let E = parseFloat(EEl.value);
        let Ldiv = parseFloat(deflLimitEl.value);

        if ([L, b, d, w, fB, fV, E, Ldiv].some((v) => isNaN(v) || v <= 0)) {
          calcErrorEl.textContent =
            "Please enter positive numeric values for span, section, loads and material properties.";
          calcErrorEl.classList.remove("hidden");
          return;
        }
        if (isNaN(P) || P < 0) P = 0;

        // Convert to consistent base units
        let L_m, b_m, d_m, w_Npm, P_N, fB_Pa, fV_Pa, E_Pa;
        if (system === "metric") {
          L_m = L;
          b_m = b / 1000;
          d_m = d / 1000;
          w_Npm = w * 1000; // kN/m -> N/m
          P_N = P * 1000; // kN -> N
          fB_Pa = fB * 1e6; // MPa -> Pa
          fV_Pa = fV * 1e6;
          E_Pa = E * 1e9; // GPa -> Pa
        } else {
          // US: L in ft, b,d in in, w in plf, P in lb, f in ksi, E in ksi
          L_m = L * 0.3048;
          b_m = b * 0.0254;
          d_m = d * 0.0254;
          w_Npm = (w * 4.4482216152605) / 0.3048; // lb/ft -> N/m
          P_N = P * 4.4482216152605; // lb -> N
          fB_Pa = fB * 6894757.293; // ksi -> Pa
          fV_Pa = fV * 6894757.293;
          E_Pa = E * 6894757.293; // ksi -> Pa
        }

        // Section properties
        let I_m4, S_m3;
        if (sectionOverrideEl.checked) {
          let Suser = parseFloat(SxEl.value);
          let Iuser = parseFloat(IxEl.value);
          if (isNaN(Suser) || isNaN(Iuser) || Suser <= 0 || Iuser <= 0) {
            calcErrorEl.textContent =
              "When overriding section properties, please enter positive numeric values for S and I.";
            calcErrorEl.classList.remove("hidden");
            return;
          }
          if (system === "metric") {
            S_m3 = Suser * 1e-9; // mm³ -> m³
            I_m4 = Iuser * 1e-12; // mm⁴ -> m⁴
          } else {
            S_m3 = Suser * Math.pow(0.0254, 3); // in³ -> m³
            I_m4 = Iuser * Math.pow(0.0254, 4); // in⁴ -> m⁴
          }
        } else {
          I_m4 = (b_m * Math.pow(d_m, 3)) / 12;
          S_m3 = (b_m * Math.pow(d_m, 2)) / 6;
        }

        // Internal forces
        const M_w = (w_Npm * Math.pow(L_m, 2)) / 8;
        const V_w = (w_Npm * L_m) / 2;
        const delta_w = (5 * w_Npm * Math.pow(L_m, 4)) / (384 * E_Pa * I_m4);

        const M_P = (P_N * L_m) / 4;
        const V_P = P_N / 2;
        const delta_P = (P_N * Math.pow(L_m, 3)) / (48 * E_Pa * I_m4);

        const Mmax = M_w + M_P;
        const Vmax = V_w + V_P;
        const deltaMax = delta_w + delta_P;

        // Stresses
        const sigmaB = Mmax / S_m3;
        const tauV = Vmax / (b_m * d_m);

        // Allowable deflection
        const deltaAllow = L_m / Ldiv;

        // Convert results back to user units
        let M_disp, V_disp, defl_disp, sigmaB_disp, tauV_disp, deltaAllow_disp;
        let M_unit, V_unit, defl_unit, stress_unit, deflAllow_unit;

        if (system === "metric") {
          M_disp = Mmax / 1000; // N·m -> kN·m
          V_disp = Vmax / 1000; // N -> kN
          defl_disp = deltaMax * 1000; // m -> mm
          deltaAllow_disp = deltaAllow * 1000; // m -> mm
          sigmaB_disp = sigmaB / 1e6; // Pa -> MPa
          tauV_disp = tauV / 1e6;
          M_unit = "kN·m";
          V_unit = "kN";
          defl_unit = "mm";
          deflAllow_unit = "mm";
          stress_unit = "MPa";
        } else {
          M_disp = Mmax / 1.3558179483314; // N·m -> ft·lb approx
          V_disp = Vmax / 4.4482216152605; // N -> lb
          defl_disp = deltaMax / 0.0254; // m -> in
          deltaAllow_disp = deltaAllow / 0.0254; // m -> in
          sigmaB_disp = sigmaB / 6894757.293; // Pa -> ksi
          tauV_disp = tauV / 6894757.293;
          M_unit = "ft·lb";
          V_unit = "lb";
          defl_unit = "in";
          deflAllow_unit = "in";
          stress_unit = system === "imperial" ? "ksi" : "MPa";
        }

        // Update summary
        resMmax.textContent = M_disp.toFixed(2) + " " + M_unit;
        resVmax.textContent = V_disp.toFixed(2) + " " + V_unit;
        resDefl.textContent = defl_disp.toFixed(3) + " " + defl_unit;

        // Checks
        const bendPass = sigmaB <= fB_Pa;
        const shearPass = tauV <= fV_Pa;
        const deflPass = deltaMax <= deltaAllow;

        resBendDetail.textContent =
          sigmaB_disp.toFixed(3) +
          " " +
          stress_unit +
          " ≤ " +
          (system === "metric"
            ? fAllowBendEl.value + " MPa"
            : fAllowBendEl.value + " ksi");
        resShearDetail.textContent =
          tauV_disp.toFixed(3) +
          " " +
          stress_unit +
          " ≤ " +
          (system === "metric"
            ? fAllowShearEl.value + " MPa"
            : fAllowShearEl.value + " ksi");
        resDeflDetail.textContent =
          defl_disp.toFixed(3) +
          " " +
          defl_unit +
          " ≤ " +
          deltaAllow_disp.toFixed(3) +
          " " +
          deflAllow_unit +
          " (L/" +
          Ldiv.toFixed(0) +
          ")";

        setStatusBadge(statusBend, bendPass);
        setStatusBadge(statusShear, shearPass);
        setStatusBadge(statusDefl, deflPass);

        const overallPass = bendPass && shearPass && deflPass;
        setStatusBadge(overallStatus, overallPass);
        overallStatus.textContent = overallPass
          ? "OK for given inputs"
          : "Check required";
      }

      unitSystemEl.addEventListener("change", () => {
        setUnitLabels();
        // Do not auto-convert numeric values; user controls them.
      });

      sectionOverrideEl.addEventListener("change", () => {
        const enabled = sectionOverrideEl.checked;
        sectionOverrideFields.classList.toggle("opacity-50", !enabled);
        SxEl.disabled = !enabled;
        IxEl.disabled = !enabled;
      });

      document
        .getElementById("calculateBtn")
        .addEventListener("click", calculateLintel);

      document.getElementById("resetBtn").addEventListener("click", () => {
        unitSystemEl.value = "metric";
        setUnitLabels();
        spanEl.value = 1.8;
        bearingEl.value = 150;
        widthEl.value = 190;
        depthEl.value = 200;
        wUniformEl.value = 8;
        pPointEl.value = 0;
        fAllowBendEl.value = 2.5;
        fAllowShearEl.value = 0.3;
        EEl.value = 10;
        deflLimitEl.value = 600;
        sectionOverrideEl.checked = false;
        sectionOverrideFields.classList.add("opacity-50");
        SxEl.disabled = true;
        IxEl.disabled = true;
        SxEl.value = "";
        IxEl.value = "";
        calcErrorEl.classList.add("hidden");
        calcErrorEl.textContent = "";
        resMmax.textContent = "–";
        resVmax.textContent = "–";
        resDefl.textContent = "–";
        resBendDetail.textContent = "–";
        resShearDetail.textContent = "–";
        resDeflDetail.textContent = "–";
        setStatusBadge(statusBend, null);
        setStatusBadge(statusShear, null);
        setStatusBadge(statusDefl, null);
        overallStatus.textContent = "Awaiting input";
        overallStatus.className = "result-badge bg-gray-100 text-gray-700";
      });

      // Initialize labels
      setUnitLabels();
    </script>

