    <script>
      // --- Utility functions for color math ---

      function clamp(v, min, max) {
        return Math.min(max, Math.max(min, v));
      }

      function srgbToLinear(c) {
        c = c / 255;
        return c <= 0.04045 ? c / 12.92 : Math.pow((c + 0.055) / 1.055, 2.4);
      }

      function linearToSrgb(c) {
        return c <= 0.0031308
          ? 12.92 * c
          : 1.055 * Math.pow(c, 1 / 2.4) - 0.055;
      }

      function rgbToHex(r, g, b) {
        return (
          "#" +
          [r, g, b]
            .map((v) => {
              const h = clamp(Math.round(v), 0, 255)
                .toString(16)
                .padStart(2, "0");
              return h;
            })
            .join("")
        );
      }

      function hexToRgb(hex) {
        hex = hex.trim();
        if (hex[0] === "#") hex = hex.slice(1);
        if (hex.length === 3) {
          hex = hex
            .split("")
            .map((ch) => ch + ch)
            .join("");
        }
        if (hex.length !== 6) return null;
        const num = parseInt(hex, 16);
        if (Number.isNaN(num)) return null;
        return {
          r: (num >> 16) & 255,
          g: (num >> 8) & 255,
          b: num & 255,
        };
      }

      function rgbToHsl(r, g, b) {
        r / 255;
        g / 255;
        b / 255;
        const max = Math.max(r, g, b),
          min = Math.min(r, g, b);
        let h,
          s,
          l = (max + min) / 2;
        if (max === min) {
          h = s = 0;
        } else {
          const d = max - min;
          s = l > 0.5 ? d / (2 - max - min) : d / (max + min);
          switch (max) {
            case r:
              h = (g - b) / d + (g < b ? 6 : 0);
              break;
            case g:
              h = (b - r) / d + 2;
              break;
            default:
              h = (r - g) / d + 4;
          }
          h *= 60;
        }
        return { h, s: s * 100, l: l * 100 };
      }

      function hslToRgb(h, s, l) {
        s / 100;
        l / 100;
        const c = (1 - Math.abs(2 * l - 1)) * s;
        const hp = (h % 360) / 60;
        const x = c * (1 - Math.abs((hp % 2) - 1));
        let r1 = 0,
          g1 = 0,
          b1 = 0;
        if (0 <= hp && hp < 1) {
          r1 = c;
          g1 = x;
        } else if (1 <= hp && hp < 2) {
          r1 = x;
          g1 = c;
        } else if (2 <= hp && hp < 3) {
          g1 = c;
          b1 = x;
        } else if (3 <= hp && hp < 4) {
          g1 = x;
          b1 = c;
        } else if (4 <= hp && hp < 5) {
          r1 = x;
          b1 = c;
        } else if (5 <= hp && hp < 6) {
          r1 = c;
          b1 = x;
        }
        const m = l - c / 2;
        return {
          r: Math.round((r1 + m) * 255),
          g: Math.round((g1 + m) * 255),
          b: Math.round((b1 + m) * 255),
        };
      }

      function rgbToHsv(r, g, b) {
        r / 255;
        g / 255;
        b / 255;
        const max = Math.max(r, g, b),
          min = Math.min(r, g, b);
        const d = max - min;
        let h;
        if (d === 0) h = 0;
        else if (max === r) h = 60 * (((g - b) / d) % 6);
        else if (max === g) h = 60 * ((b - r) / d + 2);
        else h = 60 * ((r - g) / d + 4);
        if (h < 0) h += 360;
        const s = max === 0 ? 0 : d / max;
        const v = max;
        return { h, s: s * 100, v: v * 100 };
      }

      function hsvToRgb(h, s, v) {
        s / 100;
        v / 100;
        const c = v * s;
        const hp = (h % 360) / 60;
        const x = c * (1 - Math.abs((hp % 2) - 1));
        let r1 = 0,
          g1 = 0,
          b1 = 0;
        if (0 <= hp && hp < 1) {
          r1 = c;
          g1 = x;
        } else if (1 <= hp && hp < 2) {
          r1 = x;
          g1 = c;
        } else if (2 <= hp && hp < 3) {
          g1 = c;
          b1 = x;
        } else if (3 <= hp && hp < 4) {
          g1 = x;
          b1 = c;
        } else if (4 <= hp && hp < 5) {
          r1 = x;
          b1 = c;
        } else if (5 <= hp && hp < 6) {
          r1 = c;
          b1 = x;
        }
        const m = v - c;
        return {
          r: Math.round((r1 + m) * 255),
          g: Math.round((g1 + m) * 255),
          b: Math.round((b1 + m) * 255),
        };
      }

      function rgbToCmyk(r, g, b) {
        const r1 = r / 255,
          g1 = g / 255,
          b1 = b / 255;
        const k = 1 - Math.max(r1, g1, b1);
        if (k === 1) return { c: 0, m: 0, y: 0, k: 100 };
        const c = (1 - r1 - k) / (1 - k);
        const m = (1 - g1 - k) / (1 - k);
        const y = (1 - b1 - k) / (1 - k);
        return { c: c * 100, m: m * 100, y: y * 100, k: k * 100 };
      }

      function cmykToRgb(c, m, y, k) {
        c / 100;
        m / 100;
        y / 100;
        k / 100;
        const r = 255 * (1 - c) * (1 - k);
        const g = 255 * (1 - m) * (1 - k);
        const b = 255 * (1 - y) * (1 - k);
        return { r: Math.round(r), g: Math.round(g), b: Math.round(b) };
      }

      function rgbToXyz(r, g, b) {
        const rl = srgbToLinear(r);
        const gl = srgbToLinear(g);
        const bl = srgbToLinear(b);
        const X = 0.4124564 * rl + 0.3575761 * gl + 0.1804375 * bl;
        const Y = 0.2126729 * rl + 0.7151522 * gl + 0.072175 * bl;
        const Z = 0.0193339 * rl + 0.119192 * gl + 0.9503041 * bl;
        return { X, Y, Z };
      }

      function xyzToLab(X, Y, Z) {
        // D65 reference white
        const Xn = 0.95047,
          Yn = 1.0,
          Zn = 1.08883;
        function f(t) {
          const delta = 6 / 29;
          if (t > Math.pow(delta, 3)) return Math.cbrt(t);
          return t / (3 * delta * delta) + 4 / 29;
        }
        const fx = f(X / Xn);
        const fy = f(Y / Yn);
        const fz = f(Z / Zn);
        const L = 116 * fy - 16;
        const a = 500 * (fx - fy);
        const b = 200 * (fy - fz);
        return { L, a, b };
      }

      // OKLAB / OKLCH implementation based on Björn Ottosson's reference
      function rgbToOklab(r, g, b) {
        const rl = srgbToLinear(r);
        const gl = srgbToLinear(g);
        const bl = srgbToLinear(b);

        const l = 0.4122214708 * rl + 0.5363325363 * gl + 0.0514459929 * bl;
        const m = 0.2119034982 * rl + 0.6806995451 * gl + 0.1073969566 * bl;
        const s = 0.0883024619 * rl + 0.2817188376 * gl + 0.6299787005 * bl;

        const l_ = Math.cbrt(l);
        const m_ = Math.cbrt(m);
        const s_ = Math.cbrt(s);

        const L = 0.2104542553 * l_ + 0.793617785 * m_ - 0.0040720468 * s_;
        const a = 1.9779984951 * l_ - 2.428592205 * m_ + 0.4505937099 * s_;
        const b2 = 0.0259040371 * l_ + 0.7827717662 * m_ - 0.808675766 * s_;
        return { L, a, b: b2 };
      }

      function oklabToOklch(L, a, b) {
        const C = Math.sqrt(a * a + b * b);
        let h = (Math.atan2(b, a) * 180) / Math.PI;
        if (h < 0) h += 360;
        return { L, C, h };
      }

      // Relative luminance for WCAG
      function relativeLuminance(r, g, b) {
        const rl = srgbToLinear(r);
        const gl = srgbToLinear(g);
        const bl = srgbToLinear(b);
        return 0.2126 * rl + 0.7152 * gl + 0.0722 * bl;
      }

      function contrastRatio(rgb1, rgb2) {
        const L1 = relativeLuminance(rgb1.r, rgb1.g, rgb1.b);
        const L2 = relativeLuminance(rgb2.r, rgb2.g, rgb2.b);
        const lighter = Math.max(L1, L2);
        const darker = Math.min(L1, L2);
        return (lighter + 0.05) / (darker + 0.05);
      }

      // Parse generic CSS-like color string using browser
      function parseCssColor(str) {
        const ctx = document.createElement("canvas").getContext("2d");
        ctx.fillStyle = "#000";
        ctx.fillStyle = str;
        const computed = ctx.fillStyle;
        const m = /^#([0-9a-f]{6})$/i.exec(computed);
        if (!m) return null;
        return hexToRgb(m[0]);
      }

      document.addEventListener("DOMContentLoaded", () => {
        const colorPicker = document.getElementById("color-picker");
        const colorInput = document.getElementById("color-input");
        const swatch = document.getElementById("color-swatch");

        const rgbR = document.getElementById("rgb-r");
        const rgbG = document.getElementById("rgb-g");
        const rgbB = document.getElementById("rgb-b");
        const rgbStringEl = document.getElementById("rgb-string");
        const rgbPercentEl = document.getElementById("rgb-percent");

        const hexInput = document.getElementById("hex-input");
        const hexStringEl = document.getElementById("hex-string");
        const hexShortEl = document.getElementById("hex-short");

        const hslH = document.getElementById("hsl-h");
        const hslS = document.getElementById("hsl-s");
        const hslL = document.getElementById("hsl-l");
        const hslStringEl = document.getElementById("hsl-string");

        const hsvH = document.getElementById("hsv-h");
        const hsvS = document.getElementById("hsv-s");
        const hsvV = document.getElementById("hsv-v");
        const hsvStringEl = document.getElementById("hsv-string");

        const cmykC = document.getElementById("cmyk-c");
        const cmykM = document.getElementById("cmyk-m");
        const cmykY = document.getElementById("cmyk-y");
        const cmykK = document.getElementById("cmyk-k");
        const cmykStringEl = document.getElementById("cmyk-string");

        const xyzStringEl = document.getElementById("xyz-string");
        const labStringEl = document.getElementById("lab-string");
        const oklabStringEl = document.getElementById("oklab-string");
        const oklchStringEl = document.getElementById("oklch-string");

        const contrastTextColor = document.getElementById(
          "contrast-text-color"
        );
        const contrastTextInput = document.getElementById(
          "contrast-text-input"
        );
        const contrastPreview = document.getElementById("contrast-preview");
        const contrastRatioEl = document.getElementById("contrast-ratio");
        const wcagAaNormal = document.getElementById("wcag-aa-normal");
        const wcagAaaNormal = document.getElementById("wcag-aaa-normal");
        const wcagAaLarge = document.getElementById("wcag-aa-large");
        const wcagAaaLarge = document.getElementById("wcag-aaa-large");

        let current = { r: 255, g: 102, b: 0 };
        let isUpdating = false;

        function updateAllFromRgb() {
          if (isUpdating) return;
          isUpdating = true;
          const { r, g, b } = current;

          // Swatch & picker
          const hex = rgbToHex(r, g, b);
          swatch.style.backgroundColor = hex;
          colorPicker.value = hex;
          colorInput.value = hex;

          // RGB fields
          rgbR.value = r;
          rgbG.value = g;
          rgbB.value = b;
          rgbStringEl.textContent = `rgb(${r}, ${g}, ${b})`;
          rgbPercentEl.textContent = `(${((r / 255) * 100).toFixed(1)}%, ${(
            (g / 255) *
            100
          ).toFixed(1)}%, ${((b / 255) * 100).toFixed(1)}%)`;

          // HEX fields
          hexInput.value = hex;
          hexStringEl.textContent = hex;
          // Short hex if possible
          const rs =
            r >> 4 === (r & 0xf) &&
            g >> 4 === (g & 0xf) &&
            b >> 4 === (b & 0xf);
          hexShortEl.textContent = rs
            ? "#" +
              ((r >> 4).toString(16) +
                (g >> 4).toString(16) +
                (b >> 4).toString(16))
            : "";

          // HSL
          const hsl = rgbToHsl(r, g, b);
          hslH.value = hsl.h.toFixed(1);
          hslS.value = hsl.s.toFixed(1);
          hslL.value = hsl.l.toFixed(1);
          hslStringEl.textContent = `hsl(${hsl.h.toFixed(1)}, ${hsl.s.toFixed(
            1
          )}%, ${hsl.l.toFixed(1)}%)`;

          // HSV
          const hsv = rgbToHsv(r, g, b);
          hsvH.value = hsv.h.toFixed(1);
          hsvS.value = hsv.s.toFixed(1);
          hsvV.value = hsv.v.toFixed(1);
          hsvStringEl.textContent = `hsv(${hsv.h.toFixed(1)}, ${hsv.s.toFixed(
            1
          )}%, ${hsv.v.toFixed(1)}%)`;

          // CMYK
          const cmyk = rgbToCmyk(r, g, b);
          cmykC.value = cmyk.c.toFixed(1);
          cmykM.value = cmyk.m.toFixed(1);
          cmykY.value = cmyk.y.toFixed(1);
          cmykK.value = cmyk.k.toFixed(1);
          cmykStringEl.textContent = `cmyk(${cmyk.c.toFixed(
            1
          )}%, ${cmyk.m.toFixed(1)}%, ${cmyk.y.toFixed(1)}%, ${cmyk.k.toFixed(
            1
          )}%)`;

          // XYZ, Lab
          const xyz = rgbToXyz(r, g, b);
          xyzStringEl.textContent = `X=${xyz.X.toFixed(4)}, Y=${xyz.Y.toFixed(
            4
          )}, Z=${xyz.Z.toFixed(4)}`;
          const lab = xyzToLab(xyz.X, xyz.Y, xyz.Z);
          labStringEl.textContent = `L=${lab.L.toFixed(1)}, a=${lab.a.toFixed(
            1
          )}, b=${lab.b.toFixed(1)}`;

          // OKLAB / OKLCH
          const oklab = rgbToOklab(r, g, b);
          oklabStringEl.textContent = `L=${oklab.L.toFixed(
            4
          )}, a=${oklab.a.toFixed(4)}, b=${oklab.b.toFixed(4)}`;
          const oklch = oklabToOklch(oklab.L, oklab.a, oklab.b);
          oklchStringEl.textContent = `L=${oklch.L.toFixed(
            4
          )}, C=${oklch.C.toFixed(4)}, h=${oklch.h.toFixed(1)}°`;

          // Update contrast preview
          updateContrast();

          isUpdating = false;
        }

        function setRgb(r, g, b) {
          current = {
            r: clamp(Math.round(r), 0, 255),
            g: clamp(Math.round(g), 0, 255),
            b: clamp(Math.round(b), 0, 255),
          };
          updateAllFromRgb();
        }

        // Contrast checker
        function parseColorToRgb(str) {
          const rgb = parseCssColor(str);
          return rgb;
        }

        function updateContrast() {
          const bg = current;
          const textRgb = parseColorToRgb(
            contrastTextInput.value || contrastTextColor.value
          );
          if (!textRgb) {
            contrastRatioEl.textContent = "–";
            wcagAaNormal.textContent = "–";
            wcagAaaNormal.textContent = "–";
            wcagAaLarge.textContent = "–";
            wcagAaaLarge.textContent = "–";
            contrastPreview.style.backgroundColor = rgbToHex(bg.r, bg.g, bg.b);
            contrastPreview.style.color = "#000000";
            return;
          }
          const ratio = contrastRatio(bg, textRgb);
          contrastRatioEl.textContent = ratio.toFixed(2) + ":1";
          contrastPreview.style.backgroundColor = rgbToHex(bg.r, bg.g, bg.b);
          contrastPreview.style.color = rgbToHex(
            textRgb.r,
            textRgb.g,
            textRgb.b
          );

          function pass(threshold) {
            return ratio >= threshold ? "Pass" : "Fail";
          }
          wcagAaNormal.textContent = pass(4.5);
          wcagAaaNormal.textContent = pass(7);
          wcagAaLarge.textContent = pass(3);
          wcagAaaLarge.textContent = pass(4.5);
        }

        // Event wiring

        colorPicker.addEventListener("input", () => {
          const rgb = hexToRgb(colorPicker.value);
          if (rgb) setRgb(rgb.r, rgb.g, rgb.b);
        });

        colorInput.addEventListener("change", () => {
          const val = colorInput.value.trim();
          let rgb = hexToRgb(val);
          if (!rgb) rgb = parseCssColor(val);
          if (rgb) setRgb(rgb.r, rgb.g, rgb.b);
        });

        [rgbR, rgbG, rgbB].forEach((el) => {
          el.addEventListener("input", () => {
            const r = parseFloat(rgbR.value);
            const g = parseFloat(rgbG.value);
            const b = parseFloat(rgbB.value);
            if (!Number.isNaN(r) && !Number.isNaN(g) && !Number.isNaN(b)) {
              setRgb(r, g, b);
            }
          });
        });

        hexInput.addEventListener("input", () => {
          const rgb = hexToRgb(hexInput.value);
          if (rgb) setRgb(rgb.r, rgb.g, rgb.b);
        });

        [hslH, hslS, hslL].forEach((el) => {
          el.addEventListener("input", () => {
            const h = parseFloat(hslH.value);
            const s = parseFloat(hslS.value);
            const l = parseFloat(hslL.value);
            if (!Number.isNaN(h) && !Number.isNaN(s) && !Number.isNaN(l)) {
              const rgb = hslToRgb(h, s, l);
              setRgb(rgb.r, rgb.g, rgb.b);
            }
          });
        });

        [hsvH, hsvS, hsvV].forEach((el) => {
          el.addEventListener("input", () => {
            const h = parseFloat(hsvH.value);
            const s = parseFloat(hsvS.value);
            const v = parseFloat(hsvV.value);
            if (!Number.isNaN(h) && !Number.isNaN(s) && !Number.isNaN(v)) {
              const rgb = hsvToRgb(h, s, v);
              setRgb(rgb.r, rgb.g, rgb.b);
            }
          });
        });

        [cmykC, cmykM, cmykY, cmykK].forEach((el) => {
          el.addEventListener("input", () => {
            const c = parseFloat(cmykC.value);
            const m = parseFloat(cmykM.value);
            const y = parseFloat(cmykY.value);
            const k = parseFloat(cmykK.value);
            if (![c, m, y, k].some(Number.isNaN)) {
              const rgb = cmykToRgb(c, m, y, k);
              setRgb(rgb.r, rgb.g, rgb.b);
            }
          });
        });

        contrastTextColor.addEventListener("input", () => {
          contrastTextInput.value = contrastTextColor.value;
          updateContrast();
        });

        contrastTextInput.addEventListener("change", () => {
          const rgb = parseColorToRgb(contrastTextInput.value.trim());
          if (rgb) {
            contrastTextColor.value = rgbToHex(rgb.r, rgb.g, rgb.b);
          }
          updateContrast();
        });

        // Copy buttons
        document.querySelectorAll(".copy-btn").forEach((btn) => {
          btn.addEventListener("click", () => {
            const targetId = btn.getAttribute("data-copy-target");
            const el = document.getElementById(targetId);
            if (!el) return;
            const text = el.textContent.trim();
            navigator.clipboard
              .writeText(text)
              .then(() => {
                const badge = btn.querySelector(".copied");
                if (!badge) return;
                badge.classList.add("show");
                setTimeout(() => badge.classList.remove("show"), 900);
              })
              .catch(() => {});
          });
        });

        // Initialize defaults
        contrastTextColor.value = "#000000";
        contrastTextInput.value = "#000000";
        setRgb(current.r, current.g, current.b);
      });
    </script>

    <script>
