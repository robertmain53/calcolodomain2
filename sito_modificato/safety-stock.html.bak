    <script>
      // Simple Z-score lookup from service level
      function zFromServiceLevel(p) {
        // p in [0,1]; use approximation of inverse normal CDF (Acklam)
        if (p <= 0 || p >= 1 || isNaN(p)) return NaN;
        var a1 = -39.69683028665376,
          a2 = 220.9460984245205,
          a3 = -275.9285104469687;
        var a4 = 138.357751867269,
          a5 = -30.66479806614716,
          a6 = 2.506628277459239;
        var b1 = -54.47609879822406,
          b2 = 161.5858368580409,
          b3 = -155.6989798598866;
        var b4 = 66.80131188771972,
          b5 = -13.28068155288572;
        var c1 = -0.007784894002430293,
          c2 = -0.3223964580411365,
          c3 = -2.400758277161838;
        var c4 = -2.549732539343734,
          c5 = 4.374664141464968,
          c6 = 2.938163982698783;
        var d1 = 0.007784695709041462,
          d2 = 0.3224671290700398,
          d3 = 2.445134137142996;
        var d4 = 3.754408661907416;
        var plow = 0.02425,
          phigh = 1 - plow,
          q,
          r;
        var x;
        if (p < plow) {
          q = Math.sqrt(-2 * Math.log(p));
          x =
            (((((c1 * q + c2) * q + c3) * q + c4) * q + c5) * q + c6) /
            ((((d1 * q + d2) * q + d3) * q + d4) * q + 1);
        } else if (phigh < p) {
          q = Math.sqrt(-2 * Math.log(1 - p));
          x =
            -(((((c1 * q + c2) * q + c3) * q + c4) * q + c5) * q + c6) /
            ((((d1 * q + d2) * q + d3) * q + d4) * q + 1);
        } else {
          q = p - 0.5;
          r = q * q;
          x =
            ((((((a1 * r + a2) * r + a3) * r + a4) * r + a5) * r + a6) * q) /
            (((((b1 * r + b2) * r + b3) * r + b4) * r + b5) * r + 1);
        }
        return x;
      }

      const modeTabs = document.querySelectorAll(".mode-tab");
      let currentMode = "basic";

      function updateModeUI() {
        modeTabs.forEach((tab) => {
          if (tab.dataset.mode === currentMode) {
            tab.classList.add("tab-active");
            tab.classList.remove("text-gray-600");
          } else {
            tab.classList.remove("tab-active");
            tab.classList.add("text-gray-600");
          }
        });

        const demandGroup = document.getElementById("demand-variability-group");
        const leadtimeGroup = document.getElementById(
          "leadtime-variability-group"
        );
        const variabilitySection = document.getElementById(
          "variability-section"
        );

        if (currentMode === "basic") {
          variabilitySection.classList.add("hidden");
        } else {
          variabilitySection.classList.remove("hidden");
          demandGroup.classList.toggle("hidden", currentMode === "leadtime");
          leadtimeGroup.classList.toggle("hidden", currentMode === "demand");
        }
      }

      modeTabs.forEach((tab) => {
        tab.addEventListener("click", () => {
          currentMode = tab.dataset.mode;
          updateModeUI();
        });
      });

      updateModeUI();

      function parseVal(id) {
        const v = parseFloat(document.getElementById(id).value);
        return isNaN(v) ? NaN : v;
      }

      function formatNumber(n, decimals = 2) {
        if (!isFinite(n)) return "–";
        return n.toLocaleString(undefined, {
          maximumFractionDigits: decimals,
          minimumFractionDigits: 0,
        });
      }

      document.getElementById("calc-btn").addEventListener("click", () => {
        const errEl = document.getElementById("error-msg");
        errEl.textContent = "";

        const d = parseVal("avg-demand");
        const L = parseVal("avg-leadtime");
        const servicePct = parseVal("service-level");
        let z = parseVal("z-value");

        if (!isFinite(d) || d <= 0 || !isFinite(L) || L <= 0) {
          errEl.textContent =
            "Please enter positive values for average demand and lead time.";
          return;
        }

        if (!isFinite(z)) {
          if (!isFinite(servicePct) || servicePct <= 0 || servicePct >= 100) {
            errEl.textContent =
              "Please enter a valid service level (0–100%) or a Z-score.";
            return;
          }
          z = zFromServiceLevel(servicePct / 100);
        }

        if (!isFinite(z)) {
          errEl.textContent =
            "Could not determine a valid Z-score from the service level.";
          return;
        }

        let ss = 0;
        const dlt = d * L;

        const sdD = parseVal("sd-demand");
        const sdL = parseVal("sd-leadtime");

        if (currentMode === "basic") {
          // For basic mode, approximate max demand and max lead time as mean + 2*std if provided
          let maxD = d;
          let maxL = L;
          if (isFinite(sdD) && sdD > 0) maxD = d + 2 * sdD;
          if (isFinite(sdL) && sdL > 0) maxL = L + 2 * sdL;
          ss = maxD * maxL - d * L;
        } else if (currentMode === "demand") {
          if (!isFinite(sdD) || sdD <= 0) {
            errEl.textContent =
              "Please enter a positive standard deviation of demand.";
            return;
          }
          ss = z * sdD * Math.sqrt(L);
        } else if (currentMode === "leadtime") {
          if (!isFinite(sdL) || sdL <= 0) {
            errEl.textContent =
              "Please enter a positive standard deviation of lead time.";
            return;
          }
          ss = z * d * sdL;
        } else if (currentMode === "combined") {
          if (!isFinite(sdD) || sdD <= 0 || !isFinite(sdL) || sdL <= 0) {
            errEl.textContent =
              "Please enter positive standard deviations for both demand and lead time.";
            return;
          }
          const variance = L * sdD * sdD + d * d * sdL * sdL;
          ss = z * Math.sqrt(variance);
        }

        if (!isFinite(ss) || ss < 0) {
          errEl.textContent = "Calculation error. Please check your inputs.";
          return;
        }

        const rop = dlt + ss;

        document.getElementById("ss-result").textContent = formatNumber(ss);
        document.getElementById("rop-result").textContent = formatNumber(rop);
        document.getElementById("dlt-result").textContent = formatNumber(dlt);
        document.getElementById("z-used").textContent = z.toFixed(2);

        const modeLabelMap = {
          basic: "Basic (rule of thumb)",
          demand: "Demand variability",
          leadtime: "Lead time variability",
          combined: "Combined demand & lead time variability",
        };
        document.getElementById("ss-mode-label").textContent =
          "Mode: " + (modeLabelMap[currentMode] || currentMode);

        // Value change vs current policy
        const currentSS = parseVal("current-ss");
        const unitCost = parseVal("unit-cost");
        let valueText = "–";
        if (isFinite(currentSS) && isFinite(unitCost) && unitCost > 0) {
          const deltaUnits = ss - currentSS;
          const deltaValue = deltaUnits * unitCost;
          const sign = deltaValue > 0 ? "+" : "";
          valueText =
            sign +
            "$" +
            formatNumber(deltaValue, 2) +
            " (" +
            (deltaUnits > 0 ? "+" : "") +
            formatNumber(deltaUnits, 2) +
            " units)";
        }
        document.getElementById("value-change").textContent = valueText;

        // Formula description
        const fd = document.getElementById("formula-desc");
        if (currentMode === "basic") {
          fd.innerHTML =
            "Basic rule-of-thumb safety stock is approximated as:<br>" +
            "<code>(Max daily demand × Max lead time) − (Average daily demand × Average lead time)</code>." +
            " If you provide standard deviations, the calculator approximates max values as mean + 2σ.";
        } else if (currentMode === "demand") {
          fd.innerHTML =
            "Demand variability formula:<br>" +
            "<code>Safety stock = Z × σ_d × √L</code>, where σ_d is the standard deviation of demand per day and L is average lead time.";
        } else if (currentMode === "leadtime") {
          fd.innerHTML =
            "Lead time variability formula:<br>" +
            "<code>Safety stock = Z × d̄ × σ_L</code>, where d̄ is average demand per day and σ_L is the standard deviation of lead time.";
        } else {
          fd.innerHTML =
            "Combined variability formula:<br>" +
            "<code>Safety stock = Z × √(L × σ_d² + d̄² × σ_L²)</code>, capturing uncertainty in both demand and lead time.";
        }
      });

      document.getElementById("reset-btn").addEventListener("click", () => {
        [
          "avg-demand",
          "avg-leadtime",
          "service-level",
          "z-value",
          "sd-demand",
          "sd-leadtime",
          "current-ss",
          "unit-cost",
        ].forEach((id) => {
          const el = document.getElementById(id);
          if (el) el.value = "";
        });
        document.getElementById("error-msg").textContent = "";
        document.getElementById("ss-result").textContent = "–";
        document.getElementById("rop-result").textContent = "–";
        document.getElementById("dlt-result").textContent = "–";
        document.getElementById("z-used").textContent = "–";
        document.getElementById("value-change").textContent = "–";
        document.getElementById("formula-desc").textContent =
          "Select a mode and calculate to see the formula explanation.";
      });

      // Mobile menu script (simple)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const expanded = this.getAttribute("aria-expanded") === "true";
          this.setAttribute("aria-expanded", String(!expanded));
          menu.classList.toggle("hidden");
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
