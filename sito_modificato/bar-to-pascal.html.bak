    <script>
      (function () {
        const barInput = document.getElementById("barInput");
        const paInput = document.getElementById("paInput");
        const kpaOutput = document.getElementById("kpaOutput");
        const mpaOutput = document.getElementById("mpaOutput");
        const sciOutput = document.getElementById("sciOutput");
        const liveExplanation = document.getElementById("liveExplanation");
        const activeDirection = document.getElementById("activeDirection");
        const swapBtn = document.getElementById("swapBtn");
        const clearBtn = document.getElementById("clearBtn");
        const tableBody = document.getElementById("conversionTableBody");

        const FACTOR = 100000; // 1 bar = 100,000 Pa
        let direction = "bar-to-pa"; // or 'pa-to-bar'

        function formatNumber(value, decimals = 4) {
          if (!isFinite(value)) return "–";
          if (
            Math.abs(value) >= 1e6 ||
            (Math.abs(value) > 0 && Math.abs(value) < 1e-2)
          ) {
            return value.toExponential(4);
          }
          return Number(value.toFixed(decimals)).toString();
        }

        function updateFromBar() {
          const barVal = parseFloat(barInput.value);
          if (isNaN(barVal)) {
            paInput.value = "";
            kpaOutput.textContent = "kPa: –";
            mpaOutput.textContent = "MPa: –";
            sciOutput.textContent = "Pa: –";
            liveExplanation.textContent =
              "Enter a value in bar to see the equivalent pressure in pascal.";
            return;
          }
          const paVal = barVal * FACTOR;
          paInput.value = paVal;
          const kpa = paVal / 1000;
          const mpa = paVal / 1e6;
          kpaOutput.textContent = "kPa: " + formatNumber(kpa, 3);
          mpaOutput.textContent = "MPa: " + formatNumber(mpa, 4);
          sciOutput.textContent = "Pa: " + paVal.toExponential(4);
          liveExplanation.textContent =
            barVal + " bar = " + paVal.toLocaleString("en-US") + " Pa";
        }

        function updateFromPa() {
          const paVal = parseFloat(paInput.value);
          if (isNaN(paVal)) {
            barInput.value = "";
            kpaOutput.textContent = "kPa: –";
            mpaOutput.textContent = "MPa: –";
            sciOutput.textContent = "Pa: –";
            liveExplanation.textContent =
              "Enter a value in pascal to see the equivalent pressure in bar.";
            return;
          }
          const barVal = paVal / FACTOR;
          barInput.value = barVal;
          const kpa = paVal / 1000;
          const mpa = paVal / 1e6;
          kpaOutput.textContent = "kPa: " + formatNumber(kpa, 3);
          mpaOutput.textContent = "MPa: " + formatNumber(mpa, 4);
          sciOutput.textContent = "Pa: " + paVal.toExponential(4);
          liveExplanation.textContent =
            paVal.toLocaleString("en-US") +
            " Pa = " +
            formatNumber(barVal, 6) +
            " bar";
        }

        barInput.addEventListener("input", function () {
          if (direction === "bar-to-pa") {
            updateFromBar();
          }
        });

        paInput.addEventListener("input", function () {
          if (direction === "pa-to-bar") {
            updateFromPa();
          }
        });

        swapBtn.addEventListener("click", function () {
          if (direction === "bar-to-pa") {
            direction = "pa-to-bar";
            activeDirection.textContent = "Current: Pa → bar";
            liveExplanation.textContent =
              "Enter a value in pascal to see the equivalent pressure in bar.";
          } else {
            direction = "bar-to-pa";
            activeDirection.textContent = "Current: bar → Pa";
            liveExplanation.textContent =
              "Enter a value in bar to see the equivalent pressure in pascal.";
          }
          barInput.value = "";
          paInput.value = "";
          kpaOutput.textContent = "kPa: –";
          mpaOutput.textContent = "MPa: –";
          sciOutput.textContent = "Pa: –";
          barInput.focus();
        });

        clearBtn.addEventListener("click", function () {
          barInput.value = "";
          paInput.value = "";
          kpaOutput.textContent = "kPa: –";
          mpaOutput.textContent = "MPa: –";
          sciOutput.textContent = "Pa: –";
          liveExplanation.textContent =
            direction === "bar-to-pa"
              ? "Enter a value in bar to see the equivalent pressure in pascal."
              : "Enter a value in pascal to see the equivalent pressure in bar.";
          barInput.focus();
        });

        function buildTable() {
          const values = [0.1, 0.5, 1, 2, 5, 10, 20, 50, 100];
          const fragment = document.createDocumentFragment();
          values.forEach((bar) => {
            const pa = bar * FACTOR;
            const kpa = pa / 1000;
            const mpa = pa / 1e6;
            const tr = document.createElement("tr");
            tr.innerHTML = `
          <td class="px-3 py-2 whitespace-nowrap">${bar}</td>
          <td class="px-3 py-2 whitespace-nowrap">${pa.toLocaleString(
            "en-US"
          )}</td>
          <td class="px-3 py-2 whitespace-nowrap">${formatNumber(kpa, 3)}</td>
          <td class="px-3 py-2 whitespace-nowrap">${formatNumber(mpa, 4)}</td>
        `;
            fragment.appendChild(tr);
          });
          tableBody.appendChild(fragment);
        }

        buildTable();
      })();
    </script>

    <script>
