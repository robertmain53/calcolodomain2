    <script>
      // Tab handling
      document.querySelectorAll(".tab-btn").forEach((btn) => {
        btn.addEventListener("click", () => {
          const target = btn.getAttribute("data-tab");
          document.querySelectorAll(".tab-btn").forEach((b) => {
            b.classList.remove("border-blue-600", "text-blue-600");
            b.classList.add("border-transparent", "text-gray-600");
          });
          btn.classList.add("border-blue-600", "text-blue-600");
          btn.classList.remove("border-transparent", "text-gray-600");

          document
            .querySelectorAll(".tab-panel")
            .forEach((p) => p.classList.add("hidden"));
          document.getElementById("tab-" + target).classList.remove("hidden");
        });
      });

      // Utility: parse holidays
      function parseHolidayDates(text) {
        if (!text) return new Set();
        const parts = text
          .split(/[\n,;]+/)
          .map((s) => s.trim())
          .filter(Boolean);
        const set = new Set();
        for (const p of parts) {
          const d = new Date(p);
          if (!isNaN(d)) {
            set.add(d.toISOString().slice(0, 10));
          }
        }
        return set;
      }

      function clampDate(year, month, day) {
        // month: 0-11
        const d = new Date(year, month + 1, 0); // last day of target month
        const maxDay = d.getDate();
        return new Date(year, month, Math.min(day, maxDay));
      }

      function diffYMD(start, end) {
        // assumes start <= end
        let y = end.getFullYear() - start.getFullYear();
        let m = end.getMonth() - start.getMonth();
        let d = end.getDate() - start.getDate();

        if (d < 0) {
          m -= 1;
          const prevMonth = new Date(end.getFullYear(), end.getMonth(), 0);
          d += prevMonth.getDate();
        }
        if (m < 0) {
          y -= 1;
          m += 12;
        }
        return { years: y, months: m, days: d };
      }

      function formatYMD(diff) {
        const parts = [];
        if (diff.years)
          parts.push(diff.years + " year" + (diff.years !== 1 ? "s" : ""));
        if (diff.months)
          parts.push(diff.months + " month" + (diff.months !== 1 ? "s" : ""));
        if (diff.days || parts.length === 0)
          parts.push(diff.days + " day" + (diff.days !== 1 ? "s" : ""));
        return parts.join(", ");
      }

      function formatWeeksDays(totalDays) {
        const weeks = Math.floor(totalDays / 7);
        const days = totalDays % 7;
        const parts = [];
        if (weeks) parts.push(weeks + " week" + (weeks !== 1 ? "s" : ""));
        if (days) parts.push(days + " day" + (days !== 1 ? "s" : ""));
        if (!parts.length) return "0 days";
        return parts.join(", ");
      }

      function weekdayName(date) {
        return date.toLocaleDateString(undefined, { weekday: "long" });
      }

      // Days Between Dates calculation
      document
        .getElementById("btn-calc-between")
        .addEventListener("click", () => {
          const startInput = document.getElementById("start-date").value;
          const endInput = document.getElementById("end-date").value;
          const errorEl = document.getElementById("between-error");
          const resultEl = document.getElementById("between-results");

          errorEl.classList.add("hidden");
          resultEl.classList.add("hidden");

          if (!startInput || !endInput) {
            errorEl.textContent =
              "Please select both a start date and an end date.";
            errorEl.classList.remove("hidden");
            return;
          }

          let start = new Date(startInput);
          let end = new Date(endInput);
          if (isNaN(start) || isNaN(end)) {
            errorEl.textContent =
              "One or both dates are invalid. Please use the date picker.";
            errorEl.classList.remove("hidden");
            return;
          }

          // Ensure start <= end
          if (start > end) {
            const tmp = start;
            start = end;
            end = tmp;
          }

          const rangeType = document.querySelector(
            'input[name="range-type"]:checked'
          ).value;
          const inclusive = rangeType === "inclusive";

          // Total days (exclusive base)
          const msPerDay = 24 * 60 * 60 * 1000;
          const diffMs = end.setHours(0, 0, 0, 0) - start.setHours(0, 0, 0, 0);
          let totalDaysExclusive = Math.round(diffMs / msPerDay);
          if (totalDaysExclusive < 0) totalDaysExclusive = 0;

          const totalDays = inclusive
            ? totalDaysExclusive + 1
            : totalDaysExclusive;

          // Workdays / weekends
          let workdays = 0;
          let weekendDays = 0;
          const countWorkdays =
            document.getElementById("count-workdays").checked;
          const countWeekends =
            document.getElementById("count-weekends").checked;
          const holidaysSet = parseHolidayDates(
            document.getElementById("holidays").value
          );
          let holidayCount = 0;

          if (countWorkdays || countWeekends || holidaysSet.size > 0) {
            const iterStart = new Date(start);
            const iterEnd = new Date(end);
            if (!inclusive) {
              // exclusive: start+1 to end-1
              iterStart.setDate(iterStart.getDate() + 1);
            }
            const last = new Date(iterEnd);
            if (!inclusive) {
              last.setDate(last.getDate() - 1);
            }

            for (
              let d = new Date(iterStart);
              d <= last;
              d.setDate(d.getDate() + 1)
            ) {
              const day = d.getDay(); // 0=Sun,6=Sat
              const iso = d.toISOString().slice(0, 10);
              const isHoliday = holidaysSet.has(iso);
              const isWeekend = day === 0 || day === 6;
              if (isWeekend) {
                if (countWeekends) weekendDays++;
              } else {
                if (countWorkdays) {
                  workdays++;
                  if (isHoliday) {
                    holidayCount++;
                    workdays--; // subtract from workdays
                  }
                }
              }
            }
          }

          // YMD breakdown (always inclusive logic for human-friendly span)
          const spanEnd = new Date(end);
          const spanStart = new Date(start);
          const ymd = diffYMD(spanStart, spanEnd);

          // Populate UI
          document.getElementById("total-days").textContent =
            totalDays + " day" + (totalDays !== 1 ? "s" : "");
          document.getElementById("total-span").textContent =
            "Equivalent to " + formatWeeksDays(totalDays);

          document.getElementById("workdays").textContent = countWorkdays
            ? workdays
            : "—";
          document.getElementById("weekend-days").textContent = countWeekends
            ? weekendDays
            : "—";
          document.getElementById("holiday-count").textContent =
            holidaysSet.size ? holidayCount : "0";

          document.getElementById("year-month-day-breakdown").textContent =
            "Calendar span: " + formatYMD(ymd) + " between the selected dates.";
          document.getElementById("weeks-days-breakdown").textContent =
            "Total days: " +
            totalDays +
            " (" +
            formatWeeksDays(totalDays) +
            ").";

          const startStr = startInput;
          const endStr = endInput;
          document.getElementById("start-end-summary").textContent =
            "From " +
            startStr +
            " (" +
            weekdayName(start) +
            ") to " +
            endStr +
            " (" +
            weekdayName(end) +
            ").";

          resultEl.classList.remove("hidden");
        });

      // Swap dates
      document.getElementById("btn-swap").addEventListener("click", () => {
        const s = document.getElementById("start-date");
        const e = document.getElementById("end-date");
        const tmp = s.value;
        s.value = e.value;
        e.value = tmp;
      });

      // Today → +30 days
      document
        .getElementById("btn-today-range")
        .addEventListener("click", () => {
          const today = new Date();
          const end = new Date();
          end.setDate(end.getDate() + 30);
          document.getElementById("start-date").value = today
            .toISOString()
            .slice(0, 10);
          document.getElementById("end-date").value = end
            .toISOString()
            .slice(0, 10);
        });

      // Add/Subtract Dates
      document
        .getElementById("btn-calc-addsub")
        .addEventListener("click", () => {
          const startInput = document.getElementById("as-start-date").value;
          const errorEl = document.getElementById("addsub-error");
          const resultEl = document.getElementById("addsub-results");

          errorEl.classList.add("hidden");
          resultEl.classList.add("hidden");

          if (!startInput) {
            errorEl.textContent = "Please select a start date.";
            errorEl.classList.remove("hidden");
            return;
          }

          let start = new Date(startInput);
          if (isNaN(start)) {
            errorEl.textContent =
              "Invalid start date. Please use the date picker.";
            errorEl.classList.remove("hidden");
            return;
          }

          const op = document.querySelector(
            'input[name="as-op"]:checked'
          ).value;
          const sign = op === "add" ? 1 : -1;

          const years = parseInt(
            document.getElementById("as-years").value || "0",
            10
          );
          const months = parseInt(
            document.getElementById("as-months").value || "0",
            10
          );
          const weeks = parseInt(
            document.getElementById("as-weeks").value || "0",
            10
          );
          const days = parseInt(
            document.getElementById("as-days").value || "0",
            10
          );

          if (years < 0 || months < 0 || weeks < 0 || days < 0) {
            errorEl.textContent = "All offsets must be zero or positive.";
            errorEl.classList.remove("hidden");
            return;
          }

          const totalMonths = years * 12 + months;
          const totalDays = weeks * 7 + days;

          let y = start.getFullYear();
          let m = start.getMonth();
          const d = start.getDate();

          let newMonthIndex = m + sign * totalMonths;
          let newYear = y + Math.floor(newMonthIndex / 12);
          newMonthIndex = ((newMonthIndex % 12) + 12) % 12;

          let result = clampDate(newYear, newMonthIndex, d);
          result.setDate(result.getDate() + sign * totalDays);

          const resultIso = result.toISOString().slice(0, 10);
          document.getElementById("as-result-date").textContent = resultIso;
          document.getElementById("as-result-weekday").textContent =
            "Day of week: " + weekdayName(result);

          const parts = [];
          if (years) parts.push(years + " year" + (years !== 1 ? "s" : ""));
          if (months) parts.push(months + " month" + (months !== 1 ? "s" : ""));
          if (weeks) parts.push(weeks + " week" + (weeks !== 1 ? "s" : ""));
          if (days) parts.push(days + " day" + (days !== 1 ? "s" : ""));
          const offsetStr = parts.length ? parts.join(", ") : "0 days";

          document.getElementById("as-result-summary").textContent =
            (op === "add" ? "Added " : "Subtracted ") +
            offsetStr +
            " to/from " +
            startInput +
            " to get " +
            resultIso +
            ".";

          resultEl.classList.remove("hidden");
        });

      document.getElementById("btn-as-clear").addEventListener("click", () => {
        ["as-years", "as-months", "as-weeks", "as-days"].forEach((id) => {
          document.getElementById(id).value = "0";
        });
        document.getElementById("addsub-results").classList.add("hidden");
        document.getElementById("addsub-error").classList.add("hidden");
      });

      document.getElementById("btn-as-today").addEventListener("click", () => {
        const today = new Date();
        document.getElementById("as-start-date").value = today
          .toISOString()
          .slice(0, 10);
      });

      // FAQ toggle (simple, in addition to global script_faq if present)
      document.querySelectorAll("#faq .faq-question").forEach((btn) => {
        btn.addEventListener("click", () => {
          const answer = btn.parentElement.querySelector(".faq-answer");
          const symbol = btn.querySelector("span:last-child");
          const isHidden = answer.classList.contains("hidden");
          document
            .querySelectorAll("#faq .faq-answer")
            .forEach((a) => a.classList.add("hidden"));
          document
            .querySelectorAll("#faq .faq-question span:last-child")
            .forEach((s) => (s.textContent = "+"));
          if (isHidden) {
            answer.classList.remove("hidden");
            symbol.textContent = "−";
          }
        });
      });

      // Initialize defaults
      (function initDefaults() {
        const today = new Date();
        const in7 = new Date();
        in7.setDate(in7.getDate() + 7);
        document.getElementById("start-date").value = today
          .toISOString()
          .slice(0, 10);
        document.getElementById("end-date").value = in7
          .toISOString()
          .slice(0, 10);
        document.getElementById("as-start-date").value = today
          .toISOString()
          .slice(0, 10);
      })();

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
