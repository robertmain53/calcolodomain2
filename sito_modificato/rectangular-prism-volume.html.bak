    <script>
      // Unit conversion factors to meters
      const lengthToMeters = {
        mm: 0.001,
        cm: 0.01,
        m: 1,
        in: 0.0254,
        ft: 0.3048,
        yd: 0.9144,
      };

      // Volume units to cubic meters
      const volumeToM3 = {
        mm3: 1e-9,
        cm3: 1e-6,
        m3: 1,
        in3: Math.pow(0.0254, 3),
        ft3: Math.pow(0.3048, 3),
        yd3: Math.pow(0.9144, 3),
      };

      function parseInput(id) {
        const el = document.getElementById(id);
        if (!el) return null;
        const v = el.value.trim();
        if (v === "") return null;
        const num = Number(v);
        if (!isFinite(num)) return NaN;
        return num;
      }

      function formatNumber(val, decimals = 4) {
        if (!isFinite(val)) return "–";
        if (Math.abs(val) >= 1e4 || (Math.abs(val) < 1e-3 && val !== 0)) {
          return val.toExponential(4);
        }
        return Number(val.toFixed(decimals)).toString();
      }

      function calculateRectangularPrism() {
        const errEl = document.getElementById("calc-error");
        errEl.textContent = "";

        const lVal = parseInput("length-value");
        const wVal = parseInput("width-value");
        const hVal = parseInput("height-value");
        const Vknown = parseInput("volume-known");

        const lUnit = document.getElementById("length-unit").value;
        const wUnit = document.getElementById("width-unit").value;
        const hUnit = document.getElementById("height-unit").value;
        const Vunit = document.getElementById("volume-unit").value;
        const outUnit = document.getElementById("output-unit").value;

        // Count how many of l, w, h are missing
        const sides = { l: lVal, w: wVal, h: hVal };
        const missingSides = Object.values(sides).filter(
          (v) => v === null
        ).length;

        if (Vknown === null) {
          // Forward mode: need all three sides
          if (missingSides > 0) {
            errEl.textContent =
              "Enter length, width, and height, or provide a volume to solve for a missing side.";
            return;
          }
        } else {
          // Reverse mode: must have exactly one missing side
          if (missingSides !== 1) {
            errEl.textContent =
              "To solve from volume, leave exactly one side blank and fill in the other two.";
            return;
          }
        }

        // Convert sides to meters
        let l_m = lVal !== null ? lVal * lengthToMeters[lUnit] : null;
        let w_m = wVal !== null ? wVal * lengthToMeters[wUnit] : null;
        let h_m = hVal !== null ? hVal * lengthToMeters[hUnit] : null;

        // Convert known volume to m³
        let V_m3 = Vknown !== null ? Vknown * volumeToM3[Vunit] : null;

        // Solve for missing side if needed
        if (V_m3 !== null) {
          const knownProduct =
            (l_m !== null ? l_m : 1) *
            (w_m !== null ? w_m : 1) *
            (h_m !== null ? h_m : 1);

          if (knownProduct === 0) {
            errEl.textContent = "Side lengths must be greater than zero.";
            return;
          }

          if (l_m === null) {
            l_m = V_m3 / (w_m * h_m);
            if (!isFinite(l_m) || l_m <= 0) {
              errEl.textContent =
                "The given volume and sides are inconsistent.";
              return;
            }
            document.getElementById("length-value").value = formatNumber(
              l_m / lengthToMeters[lUnit],
              4
            );
          } else if (w_m === null) {
            w_m = V_m3 / (l_m * h_m);
            if (!isFinite(w_m) || w_m <= 0) {
              errEl.textContent =
                "The given volume and sides are inconsistent.";
              return;
            }
            document.getElementById("width-value").value = formatNumber(
              w_m / lengthToMeters[wUnit],
              4
            );
          } else if (h_m === null) {
            h_m = V_m3 / (l_m * w_m);
            if (!isFinite(h_m) || h_m <= 0) {
              errEl.textContent =
                "The given volume and sides are inconsistent.";
              return;
            }
            document.getElementById("height-value").value = formatNumber(
              h_m / lengthToMeters[hUnit],
              4
            );
          }
        }

        // Validate positive sides
        if (l_m <= 0 || w_m <= 0 || h_m <= 0) {
          errEl.textContent = "All side lengths must be positive numbers.";
          return;
        }

        // Compute volume in m³
        const Vcalc_m3 = l_m * w_m * h_m;

        // Convert to requested output unit
        const V_out = Vcalc_m3 / volumeToM3[outUnit];

        // Also compute an alternate unit (liters if cm³ or m³)
        let altText = "";
        if (outUnit === "cm3" || outUnit === "m3") {
          const liters = Vcalc_m3 * 1000; // 1 m³ = 1000 L
          altText = `≈ ${formatNumber(liters, 4)} L`;
        } else if (outUnit === "ft3") {
          const gallons = Vcalc_m3 / 0.003785411784; // 1 m³ = 264.172 gal ≈ /0.003785...
          altText = `≈ ${formatNumber(gallons, 4)} US gallons`;
        }

        // Surface area (in m²) and diagonal (in m)
        const S_m2 = 2 * (l_m * w_m + l_m * h_m + w_m * h_m);
        const d_m = Math.sqrt(l_m * l_m + w_m * w_m + h_m * h_m);

        // Choose a reasonable area unit based on side units
        let areaUnit = "m²";
        let diagUnit = "m";
        if (lUnit === "cm" && wUnit === "cm" && hUnit === "cm") {
          areaUnit = "cm²";
          diagUnit = "cm";
        } else if (lUnit === "mm" && wUnit === "mm" && hUnit === "mm") {
          areaUnit = "mm²";
          diagUnit = "mm";
        } else if (lUnit === "in" && wUnit === "in" && hUnit === "in") {
          areaUnit = "in²";
          diagUnit = "in";
        } else if (lUnit === "ft" && wUnit === "ft" && hUnit === "ft") {
          areaUnit = "ft²";
          diagUnit = "ft";
        }

        let S_out = S_m2;
        let d_out = d_m;
        if (areaUnit === "cm²") S_out = S_m2 * 1e4;
        if (areaUnit === "mm²") S_out = S_m2 * 1e6;
        if (areaUnit === "in²") S_out = S_m2 / Math.pow(0.0254, 2);
        if (areaUnit === "ft²") S_out = S_m2 / Math.pow(0.3048, 2);

        if (diagUnit === "cm") d_out = d_m * 100;
        if (diagUnit === "mm") d_out = d_m * 1000;
        if (diagUnit === "in") d_out = d_m / 0.0254;
        if (diagUnit === "ft") d_out = d_m / 0.3048;

        // Update DOM
        document.getElementById("results-container").classList.remove("hidden");
        document.getElementById("result-volume").textContent = `${formatNumber(
          V_out,
          6
        )} ${outUnit.replace("2", "²").replace("3", "³")}`;
        document.getElementById("result-volume-alt").textContent = altText;

        document.getElementById(
          "result-surface"
        ).textContent = `Surface area: ${formatNumber(S_out, 4)} ${areaUnit}`;
        document.getElementById(
          "result-diagonal"
        ).textContent = `Space diagonal: ${formatNumber(d_out, 4)} ${diagUnit}`;

        // Build step-by-step explanation
        const stepsContainer = document.getElementById("steps-container");
        const stepsContent = document.getElementById("steps-content");
        const lDisplay = `${formatNumber(
          lVal !== null ? lVal : l_m / lengthToMeters[lUnit],
          4
        )} ${lUnit}`;
        const wDisplay = `${formatNumber(
          wVal !== null ? wVal : w_m / lengthToMeters[wUnit],
          4
        )} ${wUnit}`;
        const hDisplay = `${formatNumber(
          hVal !== null ? hVal : h_m / lengthToMeters[hUnit],
          4
        )} ${hUnit}`;

        let html = "";
        html += `<p><strong>1.</strong> Use the volume formula: V = ℓ × w × h.</p>`;
        html += `<p><strong>2.</strong> Convert all sides to a common unit (internally we use meters):</p>`;
        html += `<p class="ml-4">ℓ = ${formatNumber(
          l_m,
          6
        )} m, w = ${formatNumber(w_m, 6)} m, h = ${formatNumber(
          h_m,
          6
        )} m.</p>`;
        html += `<p><strong>3.</strong> Multiply the three sides:</p>`;
        html += `<p class="ml-4">V = ${formatNumber(l_m, 6)} × ${formatNumber(
          w_m,
          6
        )} × ${formatNumber(h_m, 6)} = ${formatNumber(Vcalc_m3, 6)} m³.</p>`;
        html += `<p><strong>4.</strong> Convert to your chosen output unit (${outUnit
          .replace("2", "²")
          .replace("3", "³")}):</p>`;
        html += `<p class="ml-4">V ≈ ${formatNumber(V_out, 6)} ${outUnit
          .replace("2", "²")
          .replace("3", "³")}.</p>`;
        html += `<p class="mt-2 text-gray-700">Original dimensions: ℓ = ${lDisplay}, w = ${wDisplay}, h = ${hDisplay}.</p>`;

        stepsContent.innerHTML = html;
        stepsContainer.classList.remove("hidden");
      }

      document
        .getElementById("calc-btn")
        .addEventListener("click", calculateRectangularPrism);

      document.getElementById("reset-btn").addEventListener("click", () => {
        ["length-value", "width-value", "height-value", "volume-known"].forEach(
          (id) => {
            const el = document.getElementById(id);
            if (el) el.value = "";
          }
        );
        document.getElementById("calc-error").textContent = "";
        document.getElementById("results-container").classList.add("hidden");
        document.getElementById("steps-container").classList.add("hidden");
      });

      // Allow Enter key to trigger calculation
      ["length-value", "width-value", "height-value", "volume-known"].forEach(
        (id) => {
          const el = document.getElementById(id);
          if (el) {
            el.addEventListener("keydown", (e) => {
              if (e.key === "Enter") {
                e.preventDefault();
                calculateRectangularPrism();
              }
            });
          }
        }
      );

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
