    <script>
      function formatNumber(value, decimals = 2) {
        if (!isFinite(value)) return "–";
        return Number(value).toLocaleString(undefined, {
          minimumFractionDigits: decimals,
          maximumFractionDigits: decimals,
        });
      }

      function formatCurrency(value, decimals = 2) {
        if (!isFinite(value)) return "–";
        return (
          "$" +
          Number(value).toLocaleString(undefined, {
            minimumFractionDigits: decimals,
            maximumFractionDigits: decimals,
          })
        );
      }

      document.addEventListener("DOMContentLoaded", function () {
        const annualDemandEl = document.getElementById("annualDemand");
        const orderingCostEl = document.getElementById("orderingCost");
        const holdingCostEl = document.getElementById("holdingCost");
        const unitCostEl = document.getElementById("unitCost");
        const workingDaysEl = document.getElementById("workingDays");
        const leadTimeEl = document.getElementById("leadTime");

        const eoqOutput = document.getElementById("eoqOutput");
        const ordersPerYearOutput = document.getElementById(
          "ordersPerYearOutput"
        );
        const daysBetweenOrdersOutput = document.getElementById(
          "daysBetweenOrdersOutput"
        );
        const orderingCostOutput =
          document.getElementById("orderingCostOutput");
        const holdingCostOutput = document.getElementById("holdingCostOutput");
        const totalRelevantCostOutput = document.getElementById(
          "totalRelevantCostOutput"
        );
        const totalCostWithPurchaseOutput = document.getElementById(
          "totalCostWithPurchaseOutput"
        );
        const reorderPointOutput =
          document.getElementById("reorderPointOutput");
        const errorMessage = document.getElementById("errorMessage");

        const calculateBtn = document.getElementById("calculateBtn");
        const resetBtn = document.getElementById("resetBtn");

        function clearOutputs() {
          eoqOutput.textContent = "–";
          ordersPerYearOutput.textContent = "–";
          daysBetweenOrdersOutput.textContent = "–";
          orderingCostOutput.textContent = "–";
          holdingCostOutput.textContent = "–";
          totalRelevantCostOutput.textContent = "–";
          totalCostWithPurchaseOutput.textContent = "–";
          reorderPointOutput.textContent = "–";
        }

        function showError(msg) {
          errorMessage.textContent = msg;
          errorMessage.classList.remove("hidden");
        }

        function clearError() {
          errorMessage.textContent = "";
          errorMessage.classList.add("hidden");
        }

        function calculateEOQ() {
          clearError();

          const D = parseFloat(annualDemandEl.value);
          const S = parseFloat(orderingCostEl.value);
          const H = parseFloat(holdingCostEl.value);
          const C = parseFloat(unitCostEl.value);
          const W = parseFloat(workingDaysEl.value);
          const L = parseFloat(leadTimeEl.value);

          if (!isFinite(D) || D <= 0) {
            showError("Please enter a positive annual demand (D).");
            clearOutputs();
            return;
          }
          if (!isFinite(S) || S <= 0) {
            showError("Please enter a positive ordering cost per order (S).");
            clearOutputs();
            return;
          }
          if (!isFinite(H) || H <= 0) {
            showError(
              "Please enter a positive annual holding cost per unit (H)."
            );
            clearOutputs();
            return;
          }
          if (!isFinite(W) || W <= 0) {
            showError(
              "Please enter a positive number of working days per year."
            );
            clearOutputs();
            return;
          }

          // EOQ
          const Q = Math.sqrt((2 * D * S) / H);

          if (!isFinite(Q) || Q <= 0) {
            showError(
              "Unable to compute EOQ with the given inputs. Please check your values."
            );
            clearOutputs();
            return;
          }

          const ordersPerYear = D / Q;
          const daysBetweenOrders = W / ordersPerYear;

          const annualOrderingCost = S * ordersPerYear;
          const annualHoldingCost = H * (Q / 2);
          const totalRelevantCost = annualOrderingCost + annualHoldingCost;

          let totalCostWithPurchase = NaN;
          if (isFinite(C) && C > 0) {
            totalCostWithPurchase = totalRelevantCost + C * D;
          }

          let reorderPoint = NaN;
          if (isFinite(L) && L >= 0) {
            const dailyDemand = D / W;
            reorderPoint = dailyDemand * L;
          }

          // Update outputs
          eoqOutput.textContent = formatNumber(Q, 2) + " units";
          ordersPerYearOutput.textContent = formatNumber(ordersPerYear, 2);
          daysBetweenOrdersOutput.textContent =
            formatNumber(daysBetweenOrders, 2) + " days";
          orderingCostOutput.textContent = formatCurrency(
            annualOrderingCost,
            2
          );
          holdingCostOutput.textContent = formatCurrency(annualHoldingCost, 2);
          totalRelevantCostOutput.textContent = formatCurrency(
            totalRelevantCost,
            2
          );
          totalCostWithPurchaseOutput.textContent = isFinite(
            totalCostWithPurchase
          )
            ? formatCurrency(totalCostWithPurchase, 2)
            : "–";
          reorderPointOutput.textContent = isFinite(reorderPoint)
            ? formatNumber(reorderPoint, 2) + " units"
            : "–";
        }

        calculateBtn.addEventListener("click", calculateEOQ);

        [
          annualDemandEl,
          orderingCostEl,
          holdingCostEl,
          unitCostEl,
          workingDaysEl,
          leadTimeEl,
        ].forEach((el) => {
          el.addEventListener("keydown", function (e) {
            if (e.key === "Enter") {
              e.preventDefault();
              calculateEOQ();
            }
          });
        });

        resetBtn.addEventListener("click", function () {
          annualDemandEl.value = "";
          orderingCostEl.value = "";
          holdingCostEl.value = "";
          unitCostEl.value = "";
          workingDaysEl.value = "250";
          leadTimeEl.value = "";
          clearOutputs();
          clearError();
        });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
