    <script>
      (function () {
        const adjacencyInput = document.getElementById("adjacency-input");
        const graphTypeSelect = document.getElementById("graph-type");
        const exampleSelect = document.getElementById("example-select");
        const parseGraphBtn = document.getElementById("parse-graph-btn");
        const runBtn = document.getElementById("run-bfs-btn");
        const stepBtn = document.getElementById("step-bfs-btn");
        const autoPlayBtn = document.getElementById("auto-play-btn");
        const resetBtn = document.getElementById("reset-bfs-btn");
        const speedRange = document.getElementById("speed-range");
        const startNodeInput = document.getElementById("start-node");
        const targetNodeInput = document.getElementById("target-node");
        const svg = document.getElementById("graph-svg");
        const queueView = document.getElementById("queue-view");
        const queueSizeLabel = document.getElementById("queue-size-label");
        const levelsView = document.getElementById("levels-view");
        const parentsView = document.getElementById("parents-view");
        const pathOutput = document.getElementById("path-output");

        let graph = {};
        let nodes = [];
        let edges = [];
        let positions = {};
        let bfsState = null;
        let autoTimer = null;

        function setDefaultExample() {
          const example = [
            "A: B C",
            "B: A D E",
            "C: A F",
            "D: B",
            "E: B F",
            "F: C E",
          ].join("\\n");
          adjacencyInput.value = example;
          startNodeInput.value = "A";
          targetNodeInput.value = "F";
        }

        setDefaultExample();

        exampleSelect.addEventListener("change", () => {
          const val = exampleSelect.value;
          if (!val) return;
          if (val === "line") {
            adjacencyInput.value = ["A: B", "B: A C", "C: B D", "D: C"].join(
              "\\n"
            );
            startNodeInput.value = "A";
            targetNodeInput.value = "D";
          } else if (val === "tree") {
            adjacencyInput.value = [
              "A: B C",
              "B: D E",
              "C: F G",
              "D:",
              "E:",
              "F:",
              "G:",
            ].join("\\n");
            startNodeInput.value = "A";
            targetNodeInput.value = "F";
          } else if (val === "cycle") {
            adjacencyInput.value = [
              "A: B D",
              "B: A C",
              "C: B D",
              "D: C A",
            ].join("\\n");
            startNodeInput.value = "A";
            targetNodeInput.value = "C";
          } else if (val === "grid") {
            adjacencyInput.value = [
              "A: B D",
              "B: A C E",
              "C: B F",
              "D: A E",
              "E: B D F",
              "F: C E",
            ].join("\\n");
            startNodeInput.value = "A";
            targetNodeInput.value = "F";
          }
        });

        function parseAdjacency() {
          const lines = adjacencyInput.value
            .split(/\\r?\\n/)
            .map((l) => l.trim())
            .filter((l) => l.length > 0);
          const directed = graphTypeSelect.value === "directed";
          const g = {};
          for (const line of lines) {
            const parts = line.split(":");
            if (parts.length === 0) continue;
            const node = parts[0].trim();
            if (!node) continue;
            if (!g[node]) g[node] = new Set();
            if (parts.length > 1) {
              const neighbors = parts[1].trim().split(/\\s+/).filter(Boolean);
              for (const nb of neighbors) {
                if (!g[node]) g[node] = new Set();
                g[node].add(nb);
                if (!directed) {
                  if (!g[nb]) g[nb] = new Set();
                  g[nb].add(node);
                }
              }
            }
          }
          const gFinal = {};
          Object.keys(g).forEach((k) => {
            gFinal[k] = Array.from(g[k]);
          });
          return gFinal;
        }

        function buildGraph() {
          try {
            graph = parseAdjacency();
            nodes = Object.keys(graph).sort();
            edges = [];
            const directed = graphTypeSelect.value === "directed";
            const seen = new Set();
            for (const u of nodes) {
              for (const v of graph[u]) {
                const key = directed ? u + "->" + v : [u, v].sort().join("--");
                if (seen.has(key)) continue;
                seen.add(key);
                edges.push({ u, v });
              }
            }
            layoutNodes();
            renderGraph();
            clearBfsState();
          } catch (e) {
            alert(
              "Error parsing graph. Please check the adjacency list format."
            );
          }
        }

        function layoutNodes() {
          positions = {};
          const n = nodes.length;
          const width = 560;
          const height = 280;
          const cx = width / 2;
          const cy = height / 2;
          const radius = Math.min(width, height) / 2 - 40;
          if (n === 0) return;
          if (n <= 6) {
            const rows = Math.ceil(Math.sqrt(n));
            const cols = Math.ceil(n / rows);
            const cellW = width / (cols + 1);
            const cellH = height / (rows + 1);
            nodes.forEach((node, idx) => {
              const r = Math.floor(idx / cols);
              const c = idx % cols;
              positions[node] = {
                x: (c + 1) * cellW,
                y: (r + 1) * cellH,
              };
            });
          } else {
            nodes.forEach((node, idx) => {
              const angle = (2 * Math.PI * idx) / n;
              positions[node] = {
                x: cx + radius * Math.cos(angle),
                y: cy + radius * Math.sin(angle),
              };
            });
          }
        }

        function renderGraph() {
          while (svg.firstChild) svg.removeChild(svg.firstChild);

          const edgeGroup = document.createElementNS(
            "http://www.w3.org/2000/svg",
            "g"
          );
          const nodeGroup = document.createElementNS(
            "http://www.w3.org/2000/svg",
            "g"
          );
          svg.appendChild(edgeGroup);
          svg.appendChild(nodeGroup);

          edges.forEach((e, idx) => {
            const line = document.createElementNS(
              "http://www.w3.org/2000/svg",
              "line"
            );
            line.setAttribute("x1", positions[e.u].x);
            line.setAttribute("y1", positions[e.u].y);
            line.setAttribute("x2", positions[e.v].x);
            line.setAttribute("y2", positions[e.v].y);
            line.setAttribute("class", "edge");
            line.dataset.u = e.u;
            line.dataset.v = e.v;
            line.dataset.index = idx;
            edgeGroup.appendChild(line);
          });

          nodes.forEach((node) => {
            const g = document.createElementNS(
              "http://www.w3.org/2000/svg",
              "g"
            );
            g.setAttribute("class", "node");
            g.dataset.node = node;

            const circle = document.createElementNS(
              "http://www.w3.org/2000/svg",
              "circle"
            );
            circle.setAttribute("cx", positions[node].x);
            circle.setAttribute("cy", positions[node].y);
            circle.setAttribute("r", 18);
            circle.setAttribute("fill", "#ffffff");
            circle.setAttribute("stroke", "#4b5563");
            circle.setAttribute("stroke-width", "2");
            g.appendChild(circle);

            const text = document.createElementNS(
              "http://www.w3.org/2000/svg",
              "text"
            );
            text.setAttribute("x", positions[node].x);
            text.setAttribute("y", positions[node].y + 4);
            text.setAttribute("text-anchor", "middle");
            text.setAttribute("font-size", "12");
            text.setAttribute("fill", "#111827");
            text.textContent = node;
            g.appendChild(text);

            g.addEventListener("click", () => {
              if (!startNodeInput.value) {
                startNodeInput.value = node;
              } else if (
                !targetNodeInput.value &&
                node !== startNodeInput.value
              ) {
                targetNodeInput.value = node;
              } else {
                startNodeInput.value = node;
                targetNodeInput.value = "";
              }
              updateNodeStyles();
            });

            nodeGroup.appendChild(g);
          });

          updateNodeStyles();
        }

        function updateNodeStyles() {
          const start = startNodeInput.value.trim();
          const target = targetNodeInput.value.trim();
          const nodeGroups = svg.querySelectorAll(".node");
          nodeGroups.forEach((g) => {
            const node = g.dataset.node;
            const circle = g.querySelector("circle");
            circle.setAttribute("class", "");
            circle.classList.add("transition-all");
            circle.setAttribute("fill", "#ffffff");
            circle.setAttribute("stroke", "#4b5563");
            circle.setAttribute("stroke-width", "2");
            if (node === start) {
              circle.classList.add("node-start");
            }
            if (node === target) {
              circle.classList.add("node-target");
            }
          });
        }

        function clearBfsState() {
          bfsState = null;
          if (autoTimer) {
            clearInterval(autoTimer);
            autoTimer = null;
          }
          stepBtn.disabled = true;
          autoPlayBtn.disabled = true;
          autoPlayBtn.textContent = "Auto-play";
          queueView.innerHTML = "";
          queueSizeLabel.textContent = "(0 items)";
          levelsView.innerHTML = "";
          parentsView.innerHTML = "";
          pathOutput.textContent =
            "Run BFS with a start and target node to see the shortest path here.";
          pathOutput.classList.remove("text-red-700");
          pathOutput.classList.add("text-emerald-900");

          svg.querySelectorAll(".edge").forEach((line) => {
            line.classList.remove("edge-tree", "edge-path");
            line.setAttribute("stroke", "#9ca3af");
            line.setAttribute("stroke-width", "2");
          });
          svg.querySelectorAll(".node").forEach((g) => {
            const circle = g.querySelector("circle");
            circle.classList.remove(
              "node-visited",
              "node-frontier",
              "node-current"
            );
            circle.setAttribute("fill", "#ffffff");
            circle.setAttribute("stroke", "#4b5563");
            circle.setAttribute("stroke-width", "2");
          });
          updateNodeStyles();
        }

        function initBfs() {
          clearBfsState();
          const start = startNodeInput.value.trim();
          if (!start) {
            alert("Please specify a start node.");
            return false;
          }
          if (!graph[start]) {
            alert("Start node not found in graph.");
            return false;
          }
          const queue = [start];
          const visited = new Set([start]);
          const dist = {};
          const parent = {};
          dist[start] = 0;
          parent[start] = null;
          bfsState = {
            queue,
            visited,
            dist,
            parent,
            current: null,
            finished: false,
            target: targetNodeInput.value.trim() || null,
            treeEdges: new Set(),
          };
          updateViews();
          highlightState();
          stepBtn.disabled = false;
          autoPlayBtn.disabled = false;
          return true;
        }

        function bfsStep() {
          if (!bfsState) {
            if (!initBfs()) return;
          }
          const state = bfsState;
          if (state.finished) return;

          if (state.queue.length === 0) {
            state.current = null;
            state.finished = true;
            updateViews();
            highlightState();
            finalizePath();
            return;
          }

          const u = state.queue.shift();
          state.current = u;

          const neighbors = graph[u] || [];
          for (const v of neighbors) {
            if (!state.visited.has(v)) {
              state.visited.add(v);
              state.dist[v] = (state.dist[u] || 0) + 1;
              state.parent[v] = u;
              state.queue.push(v);
              const key = [u, v].sort().join("--");
              state.treeEdges.add(key);
            }
          }

          if (state.target && u === state.target) {
            state.finished = true;
          }

          updateViews();
          highlightState();
          if (state.finished) finalizePath();
        }

        function updateViews() {
          if (!bfsState) return;
          const state = bfsState;

          queueView.innerHTML = "";
          state.queue.forEach((node, idx) => {
            const span = document.createElement("span");
            span.textContent = node;
            span.className =
              "px-2 py-0.5 rounded-full text-xs font-mono " +
              (idx === 0
                ? "bg-yellow-500 text-white"
                : "bg-yellow-100 text-yellow-800");
            queueView.appendChild(span);
          });
          queueSizeLabel.textContent = `(${state.queue.length} item${
            state.queue.length === 1 ? "" : "s"
          })`;

          const levels = {};
          Object.keys(state.dist).forEach((node) => {
            const d = state.dist[node];
            if (!levels[d]) levels[d] = [];
            levels[d].push(node);
          });
          levelsView.innerHTML = "";
          Object.keys(levels)
            .sort((a, b) => a - b)
            .forEach((d) => {
              const div = document.createElement("div");
              div.innerHTML =
                `<span class="font-semibold mr-1">Level ${d}:</span>` +
                levels[d]
                  .sort()
                  .map(
                    (n) =>
                      `<span class="inline-block px-1.5 py-0.5 bg-blue-50 text-blue-800 rounded text-xs font-mono">${n}</span>`
                  )
                  .join(" ");
              levelsView.appendChild(div);
            });

          parentsView.innerHTML = "";
          Object.keys(state.parent)
            .sort()
            .forEach((node) => {
              const p = state.parent[node];
              const div = document.createElement("div");
              div.textContent = `${node}: ${p === null ? "root" : p}`;
              parentsView.appendChild(div);
            });
        }

        function highlightState() {
          const state = bfsState;
          svg.querySelectorAll(".edge").forEach((line) => {
            const u = line.dataset.u;
            const v = line.dataset.v;
            const key = [u, v].sort().join("--");
            if (state && state.treeEdges.has(key)) {
              line.classList.add("edge-tree");
              line.setAttribute("stroke", "#2563eb");
              line.setAttribute("stroke-width", "3");
            } else {
              line.classList.remove("edge-tree", "edge-path");
              line.setAttribute("stroke", "#9ca3af");
              line.setAttribute("stroke-width", "2");
            }
          });

          svg.querySelectorAll(".node").forEach((g) => {
            const node = g.dataset.node;
            const circle = g.querySelector("circle");
            circle.classList.remove(
              "node-visited",
              "node-frontier",
              "node-current"
            );
            circle.setAttribute("fill", "#ffffff");
            circle.setAttribute("stroke", "#4b5563");
            circle.setAttribute("stroke-width", "2");

            if (state) {
              if (state.visited.has(node)) {
                circle.classList.add("node-visited");
                circle.setAttribute("fill", "#bfdbfe");
              }
              if (state.queue.includes(node)) {
                circle.classList.add("node-frontier");
                circle.setAttribute("fill", "#facc15");
              }
              if (state.current === node) {
                circle.classList.add("node-current");
                circle.setAttribute("stroke", "#2563eb");
                circle.setAttribute("stroke-width", "3");
              }
            }
          });

          updateNodeStyles();
        }

        function finalizePath() {
          const state = bfsState;
          if (!state || !state.target) return;
          const start = startNodeInput.value.trim();
          const target = state.target;
          if (!state.visited.has(target)) {
            pathOutput.textContent = `Target node "${target}" is not reachable from start node "${start}".`;
            pathOutput.classList.remove("text-emerald-900");
            pathOutput.classList.add("text-red-700");
            return;
          }
          const path = [];
          let cur = target;
          while (cur !== null && cur !== undefined) {
            path.push(cur);
            cur = state.parent[cur];
          }
          path.reverse();
          pathOutput.textContent =
            `Shortest path from ${start} to ${target} (length ${
              path.length - 1
            } edges): ` + path.join(" -> ");
          pathOutput.classList.remove("text-red-700");
          pathOutput.classList.add("text-emerald-900");

          svg.querySelectorAll(".edge").forEach((line) => {
            const u = line.dataset.u;
            const v = line.dataset.v;
            const idxU = path.indexOf(u);
            const idxV = path.indexOf(v);
            if (idxU !== -1 && idxV !== -1 && Math.abs(idxU - idxV) === 1) {
              line.classList.add("edge-path");
              line.setAttribute("stroke", "#22c55e");
              line.setAttribute("stroke-width", "3");
            }
          });

          svg.querySelectorAll(".node").forEach((g) => {
            const node = g.dataset.node;
            const circle = g.querySelector("circle");
            if (path.includes(node)) {
              circle.setAttribute("fill", "#bbf7d0");
            }
          });
        }

        parseGraphBtn.addEventListener("click", buildGraph);
        runBtn.addEventListener("click", () => {
          if (!graph || Object.keys(graph).length === 0) buildGraph();
          if (!initBfs()) return;
          while (bfsState && !bfsState.finished) {
            bfsStep();
          }
        });
        stepBtn.addEventListener("click", () => {
          if (!graph || Object.keys(graph).length === 0) buildGraph();
          bfsStep();
        });
        autoPlayBtn.addEventListener("click", () => {
          if (autoTimer) {
            clearInterval(autoTimer);
            autoTimer = null;
            autoPlayBtn.textContent = "Auto-play";
            return;
          }
          if (!graph || Object.keys(graph).length === 0) buildGraph();
          if (!bfsState) {
            if (!initBfs()) return;
          }
          autoPlayBtn.textContent = "Pause";
          const getDelay = () => parseInt(speedRange.value, 10) || 800;
          autoTimer = setInterval(() => {
            if (!bfsState || bfsState.finished) {
              clearInterval(autoTimer);
              autoTimer = null;
              autoPlayBtn.textContent = "Auto-play";
              return;
            }
            bfsStep();
          }, getDelay());
        });
        resetBtn.addEventListener("click", () => {
          clearBfsState();
          renderGraph();
        });

        buildGraph();
      })();
    </script>
  
