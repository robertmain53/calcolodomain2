    <script>
      // NJ tax brackets (simplified, same for 2023/2024 in this model)
      // Thresholds differ by filing status; rates are the same.
      const njBrackets = {
        single: [
          { upTo: 20000, rate: 0.014 },
          { upTo: 35000, rate: 0.0175 },
          { upTo: 40000, rate: 0.035 },
          { upTo: 75000, rate: 0.05525 },
          { upTo: 500000, rate: 0.0637 },
          { upTo: 1000000, rate: 0.0897 },
          { upTo: Infinity, rate: 0.1075 },
        ],
        married_joint: [
          { upTo: 20000, rate: 0.014 },
          { upTo: 50000, rate: 0.0175 },
          { upTo: 70000, rate: 0.0245 },
          { upTo: 80000, rate: 0.035 },
          { upTo: 150000, rate: 0.05525 },
          { upTo: 500000, rate: 0.0637 },
          { upTo: 1000000, rate: 0.0897 },
          { upTo: Infinity, rate: 0.1075 },
        ],
        married_separate: [
          // Use single brackets as an approximation
          { upTo: 20000, rate: 0.014 },
          { upTo: 35000, rate: 0.0175 },
          { upTo: 40000, rate: 0.035 },
          { upTo: 75000, rate: 0.05525 },
          { upTo: 500000, rate: 0.0637 },
          { upTo: 1000000, rate: 0.0897 },
          { upTo: Infinity, rate: 0.1075 },
        ],
        hoh: [
          // Approximate using married_joint thresholds
          { upTo: 20000, rate: 0.014 },
          { upTo: 50000, rate: 0.0175 },
          { upTo: 70000, rate: 0.0245 },
          { upTo: 80000, rate: 0.035 },
          { upTo: 150000, rate: 0.05525 },
          { upTo: 500000, rate: 0.0637 },
          { upTo: 1000000, rate: 0.0897 },
          { upTo: Infinity, rate: 0.1075 },
        ],
      };

      function formatCurrency(value) {
        if (!isFinite(value)) return "$0";
        return value.toLocaleString("en-US", {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 0,
        });
      }

      function formatPercent(value, digits = 2) {
        if (!isFinite(value)) return "0%";
        return (value * 100).toFixed(digits) + "%";
      }

      function calculateNjTax(taxableIncome, statusKey) {
        const brackets = njBrackets[statusKey] || njBrackets.single;
        let remaining = Math.max(0, taxableIncome);
        let lastLimit = 0;
        let totalTax = 0;
        let marginalRate = 0;
        const breakdown = [];

        for (const bracket of brackets) {
          if (remaining <= 0) break;
          const bracketIncome = Math.min(remaining, bracket.upTo - lastLimit);
          const taxForBracket = bracketIncome * bracket.rate;
          if (bracketIncome > 0) {
            breakdown.push({
              from: lastLimit,
              to: bracket.upTo,
              rate: bracket.rate,
              income: bracketIncome,
              tax: taxForBracket,
            });
            totalTax += taxForBracket;
            marginalRate = bracket.rate;
            remaining -= bracketIncome;
            lastLimit = bracket.upTo;
          }
        }

        return { totalTax, marginalRate, breakdown };
      }

      function updateNjCalculator() {
        const statusEl = document.getElementById("nj-filing-status");
        const yearEl = document.getElementById("nj-tax-year");
        const grossEl = document.getElementById("nj-gross-income");
        const preTaxEl = document.getElementById("nj-pre-tax");
        const otherDedEl = document.getElementById("nj-other-deductions");
        const freqEl = document.getElementById("nj-pay-frequency");
        const fedRateEl = document.getElementById("nj-fed-withholding-rate");

        const status = statusEl.value;
        const year = yearEl.value;
        const grossIncome = Math.max(0, Number(grossEl.value) || 0);
        const preTax = Math.max(0, Number(preTaxEl.value) || 0);
        const otherDed = Math.max(0, Number(otherDedEl.value) || 0);
        const fedRate = Math.max(0, Number(fedRateEl.value) || 0) / 100;

        const taxableIncome = Math.max(0, grossIncome - preTax - otherDed);
        const { totalTax, marginalRate, breakdown } = calculateNjTax(
          taxableIncome,
          status
        );

        const effectiveRate = grossIncome > 0 ? totalTax / grossIncome : 0;

        // Estimated federal withholding and net pay
        const estFederal = grossIncome * fedRate;
        const netAnnual = Math.max(0, grossIncome - estFederal - totalTax);

        let periodsPerYear = 1;
        switch (freqEl.value) {
          case "monthly":
            periodsPerYear = 12;
            break;
          case "biweekly":
            periodsPerYear = 26;
            break;
          case "weekly":
            periodsPerYear = 52;
            break;
          default:
            periodsPerYear = 1;
        }
        const netPerPeriod = netAnnual / periodsPerYear;

        // Update summary UI
        document.getElementById("nj-summary-year").textContent = year;
        const statusTextMap = {
          single: "Single",
          married_joint: "Married filing jointly",
          married_separate: "Married filing separately",
          hoh: "Head of household",
        };
        document.getElementById("nj-summary-status").textContent =
          statusTextMap[status] || "Single";
        document.getElementById("nj-summary-taxable").textContent =
          formatCurrency(taxableIncome);
        document.getElementById("nj-summary-tax").textContent =
          formatCurrency(totalTax);
        document.getElementById("nj-summary-effective-rate").textContent =
          formatPercent(effectiveRate);
        document.getElementById("nj-summary-marginal-rate").textContent =
          formatPercent(marginalRate);

        document.getElementById("nj-summary-net-annual").textContent =
          formatCurrency(netAnnual);
        document.getElementById("nj-summary-net-period").textContent =
          formatCurrency(netPerPeriod);
        document.getElementById(
          "nj-summary-fed-withholding"
        ).textContent = `${formatCurrency(estFederal)} (${(
          fedRate * 100
        ).toFixed(0)}%)`;

        // Update bracket table
        const tableBody = document.getElementById("nj-bracket-rows");
        tableBody.innerHTML = "";
        if (breakdown.length === 0) {
          document
            .getElementById("nj-bracket-breakdown")
            .classList.add("hidden");
        } else {
          document
            .getElementById("nj-bracket-breakdown")
            .classList.remove("hidden");
          breakdown.forEach((row) => {
            const tr = document.createElement("tr");
            tr.className = "border-t border-gray-200";
            const rangeText =
              row.to === Infinity
                ? `${formatCurrency(row.from + 0.01)}+`
                : `${formatCurrency(row.from + 0.01)} – ${formatCurrency(
                    row.to
                  )}`;
            tr.innerHTML = `
          <td class="px-3 py-2 whitespace-nowrap text-gray-700">${rangeText}</td>
          <td class="px-3 py-2 text-right text-gray-700">${formatPercent(
            row.rate,
            2
          )}</td>
          <td class="px-3 py-2 text-right text-gray-700">${formatCurrency(
            row.income
          )}</td>
          <td class="px-3 py-2 text-right text-gray-900 font-medium">${formatCurrency(
            row.tax
          )}</td>
        `;
            tableBody.appendChild(tr);
          });
        }
      }

      document.addEventListener("DOMContentLoaded", () => {
        const calcBtn = document.getElementById("nj-calc-btn");
        const fedSlider = document.getElementById("nj-fed-withholding-rate");
        const fedDisplay = document.getElementById(
          "nj-fed-withholding-display"
        );

        fedSlider.addEventListener("input", () => {
          fedDisplay.textContent = fedSlider.value + "%";
        });

        calcBtn.addEventListener("click", (e) => {
          e.preventDefault();
          updateNjCalculator();
        });

        // Auto-calc on load with default values
        updateNjCalculator();
      });
    </script>

