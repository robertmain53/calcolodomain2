<script>
  (function() {
    const HP_H_TO_J = 2684520; // precise factor based on 745.7 W * 3600 s

    const fromUnitEl = document.getElementById('from-unit');
    const toUnitEl = document.getElementById('to-unit');
    const inputEl = document.getElementById('input-value');
    const outputEl = document.getElementById('output-value');
    const inputLabelEl = document.getElementById('input-unit-label');
    const outputLabelEl = document.getElementById('output-unit-label');
    const detailEl = document.getElementById('conversion-detail');
    const swapBtn = document.getElementById('swap-units');
    const exampleBtn = document.getElementById('fill-example');
    const clearBtn = document.getElementById('clear-fields');

    function formatNumber(value) {
      if (!isFinite(value)) return '';
      const abs = Math.abs(value);
      if (abs !== 0 && (abs < 0.001 || abs >= 1e7)) {
        return value.toExponential(6);
      }
      return value.toLocaleString(undefined, { maximumFractionDigits: 6 });
    }

    function updateUnitLabels() {
      inputLabelEl.textContent = fromUnitEl.value === 'hph' ? 'hp·h' : 'J';
      outputLabelEl.textContent = toUnitEl.value === 'hph' ? 'hp·h' : 'J';

      if (fromUnitEl.value === 'hph' && toUnitEl.value === 'J') {
        detailEl.textContent = '1 horsepower hour (hp·h) = ' + formatNumber(HP_H_TO_J) + ' joules (J)';
      } else if (fromUnitEl.value === 'J' && toUnitEl.value === 'hph') {
        detailEl.textContent = '1 joule (J) = ' + (1 / HP_H_TO_J).toExponential(6) + ' horsepower hours (hp·h)';
      } else {
        detailEl.textContent = 'Please select different units to convert between horsepower hours and joules.';
      }
    }

    function convert() {
      const raw = parseFloat(inputEl.value);
      if (isNaN(raw)) {
        outputEl.value = '';
        return;
      }

      let result;
      if (fromUnitEl.value === toUnitEl.value) {
        result = raw;
      } else if (fromUnitEl.value === 'hph' && toUnitEl.value === 'J') {
        result = raw * HP_H_TO_J;
      } else if (fromUnitEl.value === 'J' && toUnitEl.value === 'hph') {
        result = raw / HP_H_TO_J;
      } else {
        result = NaN;
      }

      outputEl.value = isNaN(result) ? '' : formatNumber(result);
    }

    function swapUnits() {
      const tmp = fromUnitEl.value;
      fromUnitEl.value = toUnitEl.value;
      toUnitEl.value = tmp;
      updateUnitLabels();
      convert();
    }

    function fillExample() {
      fromUnitEl.value = 'hph';
      toUnitEl.value = 'J';
      updateUnitLabels();
      inputEl.value = '5';
      convert();
    }

    function clearFields() {
      inputEl.value = '';
      outputEl.value = '';
    }

    fromUnitEl.addEventListener('change', () => {
      if (fromUnitEl.value === toUnitEl.value) {
        toUnitEl.value = fromUnitEl.value === 'hph' ? 'J' : 'hph';
      }
      updateUnitLabels();
      convert();
    });

    toUnitEl.addEventListener('change', () => {
      if (fromUnitEl.value === toUnitEl.value) {
        fromUnitEl.value = toUnitEl.value === 'hph' ? 'J' : 'hph';
      }
      updateUnitLabels();
      convert();
    });

    inputEl.addEventListener('input', convert);
    swapBtn.addEventListener('click', swapUnits);
    exampleBtn.addEventListener('click', fillExample);
    clearBtn.addEventListener('click', clearFields);

    updateUnitLabels();
  })();
</script>

