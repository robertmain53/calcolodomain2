    <script>
      // Utility functions
      function mean(arr) {
        const vals = arr.filter((v) => !isNaN(v));
        if (!vals.length) return NaN;
        return vals.reduce((a, b) => a + b, 0) / vals.length;
      }

      function stdDev(arr) {
        const vals = arr.filter((v) => !isNaN(v));
        if (vals.length < 2) return 0;
        const m = mean(vals);
        const variance =
          vals.reduce((sum, v) => sum + Math.pow(v - m, 2), 0) /
          (vals.length - 1);
        return Math.sqrt(variance);
      }

      function linearRegression(xs, ys) {
        const n = xs.length;
        const xMean = mean(xs);
        const yMean = mean(ys);
        let num = 0,
          den = 0;
        for (let i = 0; i < n; i++) {
          num += (xs[i] - xMean) * (ys[i] - yMean);
          den += Math.pow(xs[i] - xMean, 2);
        }
        const m = num / den;
        const b = yMean - m * xMean;

        // R^2
        let ssTot = 0,
          ssRes = 0;
        for (let i = 0; i < n; i++) {
          const yPred = m * xs[i] + b;
          ssTot += Math.pow(ys[i] - yMean, 2);
          ssRes += Math.pow(ys[i] - yPred, 2);
        }
        const r2 = ssTot === 0 ? 1 : 1 - ssRes / ssTot;
        return { m, b, r2 };
      }

      // Simple 4PL fit using non-linear least squares (gradient-free, Nelder–Mead-like search)
      function fit4PL(xs, ys) {
        // Initial guesses
        const yMin = Math.min(...ys);
        const yMax = Math.max(...ys);
        const A0 = yMin;
        const D0 = yMax;
        const C0 = xs[Math.floor(xs.length / 2)] || 1;
        const B0 = 1;

        function model(x, A, B, C, D) {
          return D + (A - D) / (1 + Math.pow(x / C, B));
        }

        function sse(params) {
          const [A, B, C, D] = params;
          let sum = 0;
          for (let i = 0; i < xs.length; i++) {
            const yPred = model(xs[i], A, B, C, D);
            sum += Math.pow(ys[i] - yPred, 2);
          }
          return sum;
        }

        // Simple coordinate descent around starting point
        let params = [A0, B0, C0, D0];
        let step = [
          Math.abs(A0) || 0.1,
          0.5,
          Math.abs(C0) * 0.5 || 0.5,
          Math.abs(D0) || 0.1,
        ];
        let bestError = sse(params);

        for (let iter = 0; iter < 400; iter++) {
          let improved = false;
          for (let j = 0; j < 4; j++) {
            for (const dir of [-1, 1]) {
              const trial = params.slice();
              trial[j] += dir * step[j];
              // enforce positive C and reasonable B
              if (j === 1 && Math.abs(trial[j]) < 0.1) continue;
              if (j === 2 && trial[j] <= 0) continue;
              const err = sse(trial);
              if (err < bestError) {
                bestError = err;
                params = trial;
                improved = true;
              }
            }
          }
          if (!improved) {
            // reduce step sizes
            step = step.map((s) => s * 0.5);
            if (step.every((s) => s < 1e-6)) break;
          }
        }

        const [A, B, C, D] = params;

        // Compute R^2
        const yMean = mean(ys);
        let ssTot = 0,
          ssRes = 0;
        for (let i = 0; i < xs.length; i++) {
          const yPred = model(xs[i], A, B, C, D);
          ssTot += Math.pow(ys[i] - yMean, 2);
          ssRes += Math.pow(ys[i] - yPred, 2);
        }
        const r2 = ssTot === 0 ? 1 : 1 - ssRes / ssTot;

        return { A, B, C, D, r2, model };
      }

      function createStandardsLayout() {
        const n =
          parseInt(document.getElementById("num-standards").value, 10) || 6;
        const reps =
          parseInt(document.getElementById("replicates").value, 10) || 2;
        const tbody = document.getElementById("standards-body");
        tbody.innerHTML = "";

        for (let i = 0; i < n; i++) {
          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="border px-2 py-1">
          <input type="text" class="border rounded px-2 py-1 w-full text-xs" value="Std ${
            i + 1
          }">
        </td>
        <td class="border px-2 py-1">
          <input type="number" step="any" class="border rounded px-2 py-1 w-full text-xs" ${
            i === 0 ? 'value="0"' : ""
          }>
        </td>
        <td class="border px-2 py-1">
          <input type="text" class="border rounded px-2 py-1 w-full text-xs" value="ng/mL">
        </td>
        <td class="border px-2 py-1">
          <select class="border rounded px-2 py-1 w-full text-xs">
            <option value="standard"${
              i === 0 ? "" : " selected"
            }>Standard</option>
            <option value="blank"${i === 0 ? " selected" : ""}>Blank</option>
          </select>
        </td>
        <td class="border px-2 py-1">
          <div class="flex flex-wrap gap-1">
            ${Array.from({ length: reps })
              .map(
                () =>
                  '<input type="number" step="any" class="border rounded px-1 py-0.5 w-16 text-xs" placeholder="OD">'
              )
              .join("")}
          </div>
        </td>
      `;
          tbody.appendChild(tr);
        }
      }

      function createSamplesLayout(initial = 4) {
        const reps =
          parseInt(document.getElementById("replicates").value, 10) || 2;
        const tbody = document.getElementById("samples-body");
        tbody.innerHTML = "";
        for (let i = 0; i < initial; i++) {
          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="border px-2 py-1">
          <input type="text" class="border rounded px-2 py-1 w-full text-xs" value="Sample ${
            i + 1
          }">
        </td>
        <td class="border px-2 py-1">
          <input type="number" step="any" class="border rounded px-2 py-1 w-full text-xs" value="1">
        </td>
        <td class="border px-2 py-1">
          <div class="flex flex-wrap gap-1">
            ${Array.from({ length: reps })
              .map(
                () =>
                  '<input type="number" step="any" class="border rounded px-1 py-0.5 w-16 text-xs" placeholder="OD">'
              )
              .join("")}
          </div>
        </td>
      `;
          tbody.appendChild(tr);
        }
      }

      let curveChart = null;

      function updateChart(xs, ys, fitFunc, modelLabel) {
        const ctx = document.getElementById("curve-chart").getContext("2d");
        const sorted = xs
          .map((x, i) => ({ x, y: ys[i] }))
          .sort((a, b) => a.x - b.x);
        const dataPoints = sorted.map((p) => ({ x: p.x, y: p.y }));

        let curvePoints = [];
        if (fitFunc) {
          const minX = Math.min(...xs);
          const maxX = Math.max(...xs);
          const steps = 50;
          for (let i = 0; i <= steps; i++) {
            const x = minX + ((maxX - minX) * i) / steps;
            curvePoints.push({ x, y: fitFunc(x) });
          }
        }

        if (curveChart) {
          curveChart.destroy();
        }

        curveChart = new Chart(ctx, {
          type: "scatter",
          data: {
            datasets: [
              {
                label: "Standards",
                data: dataPoints,
                backgroundColor: "rgba(37, 99, 235, 0.9)",
                pointRadius: 4,
              },
              {
                label: modelLabel,
                data: curvePoints,
                type: "line",
                borderColor: "rgba(16, 185, 129, 0.9)",
                borderWidth: 2,
                pointRadius: 0,
                tension: 0.2,
              },
            ],
          },
          options: {
            responsive: true,
            maintainAspectRatio: false,
            scales: {
              x: {
                type: "linear",
                title: { display: true, text: "Concentration" },
              },
              y: {
                title: { display: true, text: "OD" },
              },
            },
            plugins: {
              legend: { position: "bottom" },
            },
          },
        });
      }

      function runAnalysis() {
        const statusEl = document.getElementById("status-message");
        statusEl.textContent = "";

        const model = document.getElementById("curve-model").value;
        const standardsRows = Array.from(
          document.querySelectorAll("#standards-body tr")
        );

        const stdXs = [];
        const stdYs = [];
        const stdLabels = [];
        const blanks = [];

        standardsRows.forEach((row) => {
          const cells = row.querySelectorAll("td");
          const label = cells[0].querySelector("input").value || "";
          const conc = parseFloat(cells[1].querySelector("input").value);
          const type = cells[3].querySelector("select").value;
          const odInputs = cells[4].querySelectorAll("input");
          const ods = Array.from(odInputs).map((i) => parseFloat(i.value));
          const m = mean(ods);
          if (type === "blank") {
            if (!isNaN(m)) blanks.push(m);
            return;
          }
          if (!isNaN(conc) && !isNaN(m)) {
            stdXs.push(conc);
            stdYs.push(m);
            stdLabels.push(label);
          }
        });

        if (stdXs.length < 3) {
          statusEl.textContent =
            "Please enter at least 3 valid standard points (non-blank).";
          statusEl.className = "text-sm text-red-600";
          return;
        }

        // Fit curve
        let fitInfo;
        let fitFunc;
        let modelLabel = "";
        try {
          if (model === "linear") {
            fitInfo = linearRegression(stdXs, stdYs);
            fitFunc = (x) => fitInfo.m * x + fitInfo.b;
            modelLabel = "Linear fit";
          } else if (model === "log-linear") {
            const logXs = stdXs.map((x) => Math.log10(x));
            fitInfo = linearRegression(logXs, stdYs);
            fitFunc = (x) => {
              if (x <= 0) return NaN;
              return fitInfo.m * Math.log10(x) + fitInfo.b;
            };
            modelLabel = "Log-linear fit";
          } else {
            fitInfo = fit4PL(stdXs, stdYs);
            fitFunc = (x) =>
              fitInfo.model(x, fitInfo.A, fitInfo.B, fitInfo.C, fitInfo.D);
            modelLabel = "4PL fit";
          }
        } catch (e) {
          statusEl.textContent =
            "Curve fitting failed. Check your standard values.";
          statusEl.className = "text-sm text-red-600";
          return;
        }

        // Update chart
        updateChart(stdXs, stdYs, fitFunc, modelLabel);

        // Fit summary
        const fitSummary = document.getElementById("fit-summary");
        let summaryHtml = `<div><strong>Model:</strong> ${modelLabel}</div>`;
        if (model === "linear" || model === "log-linear") {
          summaryHtml += `<div><strong>Slope (m):</strong> ${fitInfo.m.toFixed(
            4
          )}</div>`;
          summaryHtml += `<div><strong>Intercept (b):</strong> ${fitInfo.b.toFixed(
            4
          )}</div>`;
          summaryHtml += `<div><strong>R²:</strong> ${fitInfo.r2.toFixed(
            4
          )}</div>`;
        } else {
          summaryHtml += `<div><strong>A (bottom):</strong> ${fitInfo.A.toFixed(
            4
          )}</div>`;
          summaryHtml += `<div><strong>B (slope):</strong> ${fitInfo.B.toFixed(
            4
          )}</div>`;
          summaryHtml += `<div><strong>C (EC50):</strong> ${fitInfo.C.toExponential(
            3
          )}</div>`;
          summaryHtml += `<div><strong>D (top):</strong> ${fitInfo.D.toFixed(
            4
          )}</div>`;
          summaryHtml += `<div><strong>R²:</strong> ${fitInfo.r2.toFixed(
            4
          )}</div>`;
        }
        if (blanks.length) {
          summaryHtml += `<div><strong>Blank mean OD:</strong> ${mean(
            blanks
          ).toFixed(4)}</div>`;
        }
        fitSummary.innerHTML = summaryHtml;

        // Analyze samples
        const resultsBody = document.getElementById("results-body");
        resultsBody.innerHTML = "";
        const sampleRows = Array.from(
          document.querySelectorAll("#samples-body tr")
        );

        sampleRows.forEach((row) => {
          const cells = row.querySelectorAll("td");
          const id = cells[0].querySelector("input").value || "";
          const dilution =
            parseFloat(cells[1].querySelector("input").value) || 1;
          const odInputs = cells[2].querySelectorAll("input");
          const ods = Array.from(odInputs).map((i) => parseFloat(i.value));
          const mOD = mean(ods);
          const sdOD = stdDev(ods);
          const cv = isNaN(mOD) || mOD === 0 ? NaN : (sdOD / mOD) * 100;

          let conc = NaN;
          let finalConc = NaN;
          let flag = "";

          if (!isNaN(mOD)) {
            if (model === "linear") {
              conc = (mOD - fitInfo.b) / fitInfo.m;
            } else if (model === "log-linear") {
              const logX = (mOD - fitInfo.b) / fitInfo.m;
              conc = Math.pow(10, logX);
            } else {
              // invert 4PL
              const A = fitInfo.A,
                B = fitInfo.B,
                C = fitInfo.C,
                D = fitInfo.D;
              const denom = mOD - D;
              if (denom === 0) {
                conc = NaN;
              } else {
                const inner = (A - D) / denom - 1;
                if (inner <= 0 || B === 0) {
                  conc = NaN;
                } else {
                  conc = C * Math.pow(inner, 1 / B);
                }
              }
            }
            finalConc = conc * dilution;

            const minX = Math.min(...stdXs);
            const maxX = Math.max(...stdXs);
            if (conc < minX || conc > maxX) {
              flag = "Out of range";
            }
          } else {
            flag = "No valid OD";
          }

          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="border px-2 py-1">${id}</td>
        <td class="border px-2 py-1">${isNaN(mOD) ? "" : mOD.toFixed(4)}</td>
        <td class="border px-2 py-1">${isNaN(cv) ? "" : cv.toFixed(1)}</td>
        <td class="border px-2 py-1">${
          isNaN(conc) ? "" : conc.toExponential(3)
        }</td>
        <td class="border px-2 py-1">${
          isNaN(finalConc) ? "" : finalConc.toExponential(3)
        }</td>
        <td class="border px-2 py-1 text-red-600 text-xs">${flag}</td>
      `;
          resultsBody.appendChild(tr);
        });

        statusEl.textContent = "Analysis complete.";
        statusEl.className = "text-sm text-green-700";
      }

      function exportCSV() {
        const rows = [];
        rows.push([
          "Sample",
          "Mean OD",
          "CV %",
          "Calculated concentration",
          "Final concentration (dilution-corrected)",
          "Flag",
        ]);

        document.querySelectorAll("#results-body tr").forEach((tr) => {
          const cells = tr.querySelectorAll("td");
          rows.push(Array.from(cells).map((td) => td.textContent.trim()));
        });

        const csvContent = rows
          .map((r) => r.map((v) => `"${v.replace(/"/g, '""')}"`).join(","))
          .join("\n");
        const blob = new Blob([csvContent], {
          type: "text/csv;charset=utf-8;",
        });
        const url = URL.createObjectURL(blob);
        const a = document.createElement("a");
        a.href = url;
        a.download = "elisa_results.csv";
        document.body.appendChild(a);
        a.click();
        document.body.removeChild(a);
        URL.revokeObjectURL(url);
      }

      function loadDemoData() {
        document.getElementById("num-standards").value = 6;
        document.getElementById("replicates").value = 2;
        createStandardsLayout();
        createSamplesLayout(4);

        // Demo standards (ng/mL) with OD values
        const demoConc = [0, 1, 2, 4, 8, 16];
        const demoOD = [
          [0.05, 0.06],
          [0.12, 0.13],
          [0.25, 0.27],
          [0.52, 0.5],
          [0.95, 0.98],
          [1.6, 1.55],
        ];

        const stdRows = document.querySelectorAll("#standards-body tr");
        stdRows.forEach((row, i) => {
          const cells = row.querySelectorAll("td");
          cells[0].querySelector("input").value =
            i === 0 ? "Blank" : `Std ${i}`;
          cells[1].querySelector("input").value = demoConc[i];
          cells[3].querySelector("select").value =
            i === 0 ? "blank" : "standard";
          const odInputs = cells[4].querySelectorAll("input");
          odInputs.forEach((inp, j) => {
            inp.value = demoOD[i][j];
          });
        });

        const sampleRows = document.querySelectorAll("#samples-body tr");
        const sampleODs = [
          [0.3, 0.32],
          [0.8, 0.78],
          [1.4, 1.38],
          [0.15, 0.16],
        ];
        sampleRows.forEach((row, i) => {
          const cells = row.querySelectorAll("td");
          cells[0].querySelector("input").value = `Sample ${i + 1}`;
          cells[1].querySelector("input").value = i === 2 ? 10 : 1;
          const odInputs = cells[2].querySelectorAll("input");
          odInputs.forEach((inp, j) => {
            inp.value = sampleODs[i][j];
          });
        });

        document.getElementById("status-message").textContent =
          'Demo data loaded. Click "Fit curve & calculate concentrations".';
        document.getElementById("status-message").className =
          "text-sm text-gray-700";
      }

      document.addEventListener("DOMContentLoaded", () => {
        createStandardsLayout();
        createSamplesLayout();

        document
          .getElementById("apply-layout")
          .addEventListener("click", () => {
            createStandardsLayout();
            createSamplesLayout();
          });

        document
          .getElementById("run-analysis")
          .addEventListener("click", runAnalysis);
        document
          .getElementById("export-csv")
          .addEventListener("click", exportCSV);
        document
          .getElementById("load-demo")
          .addEventListener("click", loadDemoData);

        document.getElementById("add-sample").addEventListener("click", () => {
          const reps =
            parseInt(document.getElementById("replicates").value, 10) || 2;
          const tbody = document.getElementById("samples-body");
          const index = tbody.querySelectorAll("tr").length + 1;
          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="border px-2 py-1">
          <input type="text" class="border rounded px-2 py-1 w-full text-xs" value="Sample ${index}">
        </td>
        <td class="border px-2 py-1">
          <input type="number" step="any" class="border rounded px-2 py-1 w-full text-xs" value="1">
        </td>
        <td class="border px-2 py-1">
          <div class="flex flex-wrap gap-1">
            ${Array.from({ length: reps })
              .map(
                () =>
                  '<input type="number" step="any" class="border rounded px-1 py-0.5 w-16 text-xs" placeholder="OD">'
              )
              .join("")}
          </div>
        </td>
      `;
          tbody.appendChild(tr);
        });

        document
          .getElementById("remove-sample")
          .addEventListener("click", () => {
            const tbody = document.getElementById("samples-body");
            const rows = tbody.querySelectorAll("tr");
            if (rows.length > 0) {
              tbody.removeChild(rows[rows.length - 1]);
            }
          });
      });

      // Mobile menu script (from template)
      document.addEventListener("DOMContentLoaded", function () {
        const toggle = document.getElementById("mobile-menu-toggle");
        const menu = document.getElementById("mobile-menu");
        if (toggle && menu) {
          toggle.addEventListener("click", () => {
            const isHidden = menu.classList.contains("hidden");
            menu.classList.toggle("hidden", !isHidden);
            toggle.setAttribute("aria-expanded", String(isHidden));
          });
        }
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
