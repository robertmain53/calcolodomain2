    <script>
      // Mode presets (km/h)
      const modeSpeeds = {
        car: 90,
        bike: 18,
        walk: 5,
        flight: 800,
      };

      const modeButtons = document.querySelectorAll(".mode-btn");
      const speedInput = document.getElementById("speed");
      const speedUnitSelect = document.getElementById("speed-unit");
      const modeHint = document.getElementById("mode-hint");

      modeButtons.forEach((btn) => {
        btn.addEventListener("click", () => {
          modeButtons.forEach((b) => {
            b.classList.remove(
              "border-blue-500",
              "bg-blue-50",
              "text-blue-700"
            );
            b.classList.add("border-gray-300", "bg-white", "text-gray-700");
          });
          btn.classList.add("border-blue-500", "bg-blue-50", "text-blue-700");
          btn.classList.remove("border-gray-300", "bg-white", "text-gray-700");

          const mode = btn.getAttribute("data-mode");
          const preset = modeSpeeds[mode];
          speedInput.value = preset;
          speedUnitSelect.value = mode === "flight" ? "kmh" : "kmh";
          modeHint.textContent = `Preset speed for ${mode}: ${preset} km/h. Adjust if needed.`;
        });
      });

      function toKm(distance, unit) {
        if (!isFinite(distance)) return NaN;
        switch (unit) {
          case "km":
            return distance;
          case "mi":
            return distance * 1.609344;
          case "nmi":
            return distance * 1.852;
          default:
            return NaN;
        }
      }

      function toKmh(speed, unit) {
        if (!isFinite(speed)) return NaN;
        switch (unit) {
          case "kmh":
            return speed;
          case "mph":
            return speed * 1.609344;
          case "ms":
            return speed * 3.6;
          case "knots":
            return speed * 1.852;
          default:
            return NaN;
        }
      }

      function hoursToHMS(hours) {
        if (!isFinite(hours) || hours < 0) return null;
        const totalSeconds = Math.round(hours * 3600);
        const h = Math.floor(totalSeconds / 3600);
        const m = Math.floor((totalSeconds % 3600) / 60);
        const s = totalSeconds % 60;
        return { h, m, s };
      }

      function formatHMS(hms) {
        if (!hms) return "";
        const parts = [];
        if (hms.h) parts.push(`${hms.h} h`);
        if (hms.m) parts.push(`${hms.m} min`);
        if (hms.s && !hms.h && !hms.m) parts.push(`${hms.s} s`);
        if (!parts.length) return "0 min";
        return parts.join(" ");
      }

      const calcBtn = document.getElementById("calculate-btn");
      const resetBtn = document.getElementById("reset-btn");
      const distanceInput = document.getElementById("distance");
      const distanceUnitSelect = document.getElementById("distance-unit");
      const extraTimeInput = document.getElementById("extra-time");
      const extraTimeUnitSelect = document.getElementById("extra-time-unit");
      const depDateInput = document.getElementById("departure-date");
      const depTimeInput = document.getElementById("departure-time");

      const resultsPlaceholder = document.getElementById("results-placeholder");
      const resultsContent = document.getElementById("results-content");
      const resultDurationMain = document.getElementById(
        "result-duration-main"
      );
      const resultDurationAlt = document.getElementById("result-duration-alt");
      const resultDrivingTime = document.getElementById("result-driving-time");
      const resultExtraTime = document.getElementById("result-extra-time");
      const resultAverageSpeed = document.getElementById(
        "result-average-speed"
      );
      const arrivalBlock = document.getElementById("arrival-block");
      const resultArrival = document.getElementById("result-arrival");

      calcBtn.addEventListener("click", () => {
        const distance = parseFloat(distanceInput.value);
        const speed = parseFloat(speedInput.value);
        const extra = parseFloat(extraTimeInput.value || "0");

        if (!isFinite(distance) || distance <= 0) {
          alert("Please enter a valid distance greater than 0.");
          return;
        }
        if (!isFinite(speed) || speed <= 0) {
          alert("Please enter a valid average speed greater than 0.");
          return;
        }

        const dKm = toKm(distance, distanceUnitSelect.value);
        const vKmh = toKmh(speed, speedUnitSelect.value);
        if (!isFinite(dKm) || !isFinite(vKmh) || vKmh <= 0) {
          alert("Invalid distance or speed units.");
          return;
        }

        const baseHours = dKm / vKmh;

        let extraHours = 0;
        if (isFinite(extra) && extra > 0) {
          extraHours = extraTimeUnitSelect.value === "h" ? extra : extra / 60;
        }

        const totalHours = baseHours + extraHours;
        const hmsBase = hoursToHMS(baseHours);
        const hmsTotal = hoursToHMS(totalHours);

        resultsPlaceholder.classList.add("hidden");
        resultsContent.classList.remove("hidden");

        resultDurationMain.textContent = formatHMS(hmsTotal);
        resultDurationAlt.textContent = `Base travel: ${formatHMS(
          hmsBase
        )} · Extra: ${formatHMS(hoursToHMS(extraHours))}`;

        resultDrivingTime.textContent = `Base travel time: ${formatHMS(
          hmsBase
        )} (${baseHours.toFixed(2)} h)`;
        resultExtraTime.textContent = `Extra time: ${formatHMS(
          hoursToHMS(extraHours)
        )} (${extraHours.toFixed(2)} h)`;
        const avgSpeedDisplay =
          distanceUnitSelect.value === "km"
            ? dKm / baseHours
            : distance / baseHours;
        const unitLabel = distanceUnitSelect.value === "km" ? "km/h" : "mi/h";
        resultAverageSpeed.textContent = `Average speed over distance: ${avgSpeedDisplay.toFixed(
          1
        )} ${unitLabel}`;

        // Arrival time
        const depDate = depDateInput.value;
        const depTime = depTimeInput.value;
        if (depDate && depTime) {
          const depDateTime = new Date(`${depDate}T${depTime}`);
          if (!isNaN(depDateTime.getTime())) {
            const arrivalTime = new Date(
              depDateTime.getTime() + totalHours * 3600 * 1000
            );
            resultArrival.textContent = arrivalTime.toLocaleString(undefined, {
              year: "numeric",
              month: "short",
              day: "numeric",
              hour: "2-digit",
              minute: "2-digit",
            });
            arrivalBlock.classList.remove("hidden");
          } else {
            arrivalBlock.classList.add("hidden");
          }
        } else {
          arrivalBlock.classList.add("hidden");
        }
      });

      resetBtn.addEventListener("click", () => {
        distanceInput.value = "";
        speedInput.value = "";
        extraTimeInput.value = "";
        depDateInput.value = "";
        depTimeInput.value = "";
        distanceUnitSelect.value = "km";
        speedUnitSelect.value = "kmh";
        extraTimeUnitSelect.value = "min";
        modeHint.textContent =
          "Mode presets set a typical speed. You can always override it manually.";
        modeButtons.forEach((b) => {
          b.classList.remove("border-blue-500", "bg-blue-50", "text-blue-700");
          b.classList.add("border-gray-300", "bg-white", "text-gray-700");
        });
        resultsPlaceholder.classList.remove("hidden");
        resultsContent.classList.add("hidden");
        arrivalBlock.classList.add("hidden");
      });

      // Multi-leg helper
      const legDistanceInput = document.getElementById("leg-distance");
      const legDistanceUnitSelect =
        document.getElementById("leg-distance-unit");
      const legSpeedInput = document.getElementById("leg-speed");
      const legSpeedUnitSelect = document.getElementById("leg-speed-unit");
      const addLegBtn = document.getElementById("add-leg-btn");
      const clearLegsBtn = document.getElementById("clear-legs-btn");
      const legsSummary = document.getElementById("legs-summary");

      let legs = [];

      function updateLegsSummary() {
        if (!legs.length) {
          legsSummary.textContent = "No legs added yet.";
          return;
        }
        const totalKm = legs.reduce((sum, l) => sum + l.km, 0);
        const totalHours = legs.reduce((sum, l) => sum + l.hours, 0);
        const hms = hoursToHMS(totalHours);
        legsSummary.innerHTML = `
      <p class="mb-1"><strong>Legs added:</strong> ${legs.length}</p>
      <p>Total distance: ${totalKm.toFixed(1)} km</p>
      <p>Total time: ${formatHMS(hms)} (${totalHours.toFixed(2)} h)</p>
      <p class="mt-1 text-gray-500">You can copy these totals into the main calculator.</p>
    `;
      }

      addLegBtn.addEventListener("click", () => {
        const d = parseFloat(legDistanceInput.value);
        const v = parseFloat(legSpeedInput.value);
        if (!isFinite(d) || d <= 0 || !isFinite(v) || v <= 0) {
          alert("Please enter a valid leg distance and speed greater than 0.");
          return;
        }
        const km = toKm(d, legDistanceUnitSelect.value);
        const kmh = toKmh(v, legSpeedUnitSelect.value);
        if (!isFinite(km) || !isFinite(kmh) || kmh <= 0) {
          alert("Invalid leg units.");
          return;
        }
        const hours = km / kmh;
        legs.push({ km, hours });
        updateLegsSummary();
        legDistanceInput.value = "";
        legSpeedInput.value = "";
      });

      clearLegsBtn.addEventListener("click", () => {
        legs = [];
        updateLegsSummary();
      });

      // Mobile menu script (simple)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", () => {
          const menu = document.getElementById("mobile-menu");
          const expanded = menu.classList.contains("hidden") ? "false" : "true";
          menu.classList.toggle("hidden");
          document
            .getElementById("mobile-menu-toggle")
            .setAttribute(
              "aria-expanded",
              expanded === "true" ? "false" : "true"
            );
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
