    <script>
      const inputEl = document.getElementById('data-input');
      const btnCalc = document.getElementById('btn-calc');
      const btnExample = document.getElementById('btn-example');
      const btnClear = document.getElementById('btn-clear');

      const summaryWrap = document.getElementById('summary-wrap');
      const varianceWrap = document.getElementById('variance-wrap');
      const stepsWrap = document.getElementById('steps-wrap');

      const resCount = document.getElementById('res-count');
      const resMean = document.getElementById('res-mean');
      const resSum = document.getElementById('res-sum');
      const resMinMax = document.getElementById('res-minmax');
      const resRange = document.getElementById('res-range');
      const resQQQ = document.getElementById('res-qqq');

      const resVarPop = document.getElementById('res-var-pop');
      const resStdPop = document.getElementById('res-std-pop');
      const resVarSample = document.getElementById('res-var-sample');
      const resStdSample = document.getElementById('res-std-sample');

      const stepsTableBody = document.querySelector('#steps-table tbody');
      const resSumSq = document.getElementById('res-sum-sq');

      function parseNumbers(text) {
        const parts = text.split(/[\s,;]+/);
        const nums = [];
        for (const p of parts) {
          if (!p.trim()) continue;
          const v = parseFloat(p);
          if (!isNaN(v)) nums.push(v);
        }
        return nums;
      }

      function mean(nums) {
        if (!nums.length) return NaN;
        return nums.reduce((a,b) => a + b, 0) / nums.length;
      }

      function quartiles(sorted) {
        const n = sorted.length;
        if (n === 0) return {q1: NaN, q3: NaN};
        const mid = Math.floor(n/2);
        let lower, upper;
        if (n % 2 === 0) {
          lower = sorted.slice(0, mid);
          upper = sorted.slice(mid);
        } else {
          lower = sorted.slice(0, mid);
          upper = sorted.slice(mid+1);
        }
        function median(arr) {
          const m = arr.length;
          if (!m) return NaN;
          if (m % 2 === 1) return arr[(m-1)/2];
          return (arr[m/2 - 1] + arr[m/2]) / 2;
        }
        return { q1: median(lower), q3: median(upper) };
      }

      function calculateVariance() {
        const nums = parseNumbers(inputEl.value);
        if (nums.length === 0) {
          summaryWrap.classList.add('hidden');
          varianceWrap.classList.add('hidden');
          stepsWrap.classList.add('hidden');
          stepsTableBody.innerHTML = '';
          return;
        }

        const n = nums.length;
        const sorted = nums.slice().sort((a,b) => a - b);
        const m = mean(nums);
        const sum = nums.reduce((a,b) => a + b, 0);
        const min = sorted[0];
        const max = sorted[sorted.length - 1];
        const range = max - min;
        const { q1, q3 } = quartiles(sorted);
        const iqr = q3 - q1;

        // sum of squared deviations
        let sumSq = 0;
        stepsTableBody.innerHTML = '';
        nums.forEach((val, idx) => {
          const diff = val - m;
          const sq = diff * diff;
          sumSq += sq;
          const tr = document.createElement('tr');
          const tdIdx = document.createElement('td');
          const tdVal = document.createElement('td');
          const tdDiff = document.createElement('td');
          const tdSq = document.createElement('td');
          tdIdx.textContent = idx + 1;
          tdVal.textContent = val;
          tdDiff.textContent = diff.toFixed(6);
          tdSq.textContent = sq.toFixed(6);
          tr.appendChild(tdIdx);
          tr.appendChild(tdVal);
          tr.appendChild(tdDiff);
          tr.appendChild(tdSq);
          stepsTableBody.appendChild(tr);
        });

        // population variance
        const varPop = sumSq / n;
        const stdPop = Math.sqrt(varPop);

        // sample variance
        let varSample = NaN;
        let stdSample = NaN;
        if (n > 1) {
          varSample = sumSq / (n - 1);
          stdSample = Math.sqrt(varSample);
        }

        // fill UI
        resCount.textContent = n;
        resMean.textContent = m.toFixed(6).replace(/\.?0+$/,'');
        resSum.textContent = sum;
        resMinMax.textContent = min + ' / ' + max;
        resRange.textContent = range;
        resQQQ.textContent = (isNaN(q1) ? '—' : q1) + ' / ' + (isNaN(q3) ? '—' : q3) + ' / ' + (isNaN(iqr) ? '—' : iqr);
        resVarPop.textContent = varPop.toFixed(6).replace(/\.?0+$/,'');
        resStdPop.textContent = stdPop.toFixed(6).replace(/\.?0+$/,'');
        resVarSample.textContent = isNaN(varSample) ? '—' : varSample.toFixed(6).replace(/\.?0+$/,'');
        resStdSample.textContent = isNaN(stdSample) ? '—' : stdSample.toFixed(6).replace(/\.?0+$/,'');
        resSumSq.textContent = sumSq.toFixed(6).replace(/\.?0+$/,'');

        summaryWrap.classList.remove('hidden');
        varianceWrap.classList.remove('hidden');
        stepsWrap.classList.remove('hidden');
      }

      btnCalc.addEventListener('click', calculateVariance);

      btnExample.addEventListener('click', function() {
        inputEl.value = '12, 15, 18, 21, 22, 25, 27';
        calculateVariance();
      });

      btnClear.addEventListener('click', function() {
        inputEl.value = '';
        summaryWrap.classList.add('hidden');
        varianceWrap.classList.add('hidden');
        stepsWrap.classList.add('hidden');
        stepsTableBody.innerHTML = '';
      });

      // preload sample
      inputEl.value = '12, 15, 18, 21, 22, 25, 27';
      calculateVariance();
    </script>

