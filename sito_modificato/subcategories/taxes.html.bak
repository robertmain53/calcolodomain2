  <!-- Tiny filter script (no framework) -->
  <script>
    (function(){
      const input = document.getElementById('filter');
      const tiles = Array.from(document.querySelectorAll('#taxes-collection a.link-tile'));
      if(!input || !tiles.length) return;

      const normalize = s => s.toLowerCase().normalize('NFKD').replace(/[^\w\s-]/g,'');
      input.addEventListener('input', () => {
        const q = normalize(input.value);
        tiles.forEach(t => {
          const txt = normalize(t.textContent || '');
          t.parentElement.style.display = (!q || txt.includes(q)) ? '' : 'none';
        });
      });
      input.addEventListener('keydown', (e)=>{ if(e.key==='Escape'){ input.value=''; input.dispatchEvent(new Event('input')); }});
    })();
  </script>
