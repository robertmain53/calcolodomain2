    <script>
      // Mobile menu toggle
      document
        .getElementById("mobile-menu-toggle")
        ?.addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          if (!menu) return;
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          this.setAttribute("aria-expanded", String(isHidden));
        });

      // BSA & dosing calculator logic
      (function () {
        const unitButtons = document.querySelectorAll(".unit-toggle");
        const weightInput = document.getElementById("weight");
        const heightInput = document.getElementById("height");
        const weightUnitSpan = document.getElementById("weight-unit");
        const heightUnitSpan = document.getElementById("height-unit");
        const formulaSelect = document.getElementById("bsa-formula");
        const bsaCapInput = document.getElementById("bsa-cap");
        const drugNameInput = document.getElementById("drug-name");
        const dosePerM2Input = document.getElementById("dose-per-m2");
        const maxDoseInput = document.getElementById("max-dose");
        const roundingSelect = document.getElementById("rounding");
        const calculateBtn = document.getElementById("calculate-btn");
        const resetBtn = document.getElementById("reset-btn");

        const resultsContainer = document.getElementById("results-container");
        const bsaValueSpan = document.getElementById("bsa-value");
        const bsaFormulaLabel = document.getElementById("bsa-formula-label");
        const bsaCapNote = document.getElementById("bsa-cap-note");
        const resultDrugName = document.getElementById("result-drug-name");
        const resultDosePerM2 = document.getElementById("result-dose-per-m2");
        const resultUncappedDose = document.getElementById(
          "result-uncapped-dose"
        );
        const resultCappedDose = document.getElementById("result-capped-dose");
        const resultRoundedDose = document.getElementById(
          "result-rounded-dose"
        );
        const bsaComparisonBody = document.getElementById(
          "bsa-comparison-body"
        );
        const historyContainer = document.getElementById("history-container");
        const historyBody = document.getElementById("history-body");

        let currentUnits = "metric"; // 'metric' or 'us'
        let history = [];

        function setUnits(units) {
          currentUnits = units;
          if (units === "metric") {
            weightUnitSpan.textContent = "kg";
            heightUnitSpan.textContent = "cm";
          } else {
            weightUnitSpan.textContent = "lb";
            heightUnitSpan.textContent = "in";
          }
          unitButtons.forEach((btn) => {
            const isActive = btn.dataset.units === units;
            btn.classList.toggle("bg-blue-600", isActive);
            btn.classList.toggle("text-white", isActive);
            btn.classList.toggle("border-blue-600", isActive);
            btn.classList.toggle("bg-white", !isActive);
            btn.classList.toggle("text-gray-700", !isActive);
            btn.classList.toggle("border-gray-300", !isActive);
          });
        }

        unitButtons.forEach((btn) => {
          btn.addEventListener("click", () => setUnits(btn.dataset.units));
        });

        function toMetric(weight, height) {
          let w = weight;
          let h = height;
          if (currentUnits === "us") {
            // lb to kg, in to cm
            w = weight * 0.45359237;
            h = height * 2.54;
          }
          return { weightKg: w, heightCm: h };
        }

        function calcBSA(formula, weightKg, heightCm) {
          const w = weightKg;
          const h = heightCm;
          switch (formula) {
            case "mosteller":
              return Math.sqrt((h * w) / 3600);
            case "dubois":
              return 0.007184 * Math.pow(h, 0.725) * Math.pow(w, 0.425);
            case "haycock":
              return 0.024265 * Math.pow(h, 0.3964) * Math.pow(w, 0.5378);
            case "boyd":
              const logW = Math.log10(w + 0.0001);
              return (
                0.0003207 *
                Math.pow(h, 0.3) *
                Math.pow(w + 0.0001, 0.7285 - 0.0188 * logW)
              );
            case "gehan":
              return 0.0235 * Math.pow(h, 0.42246) * Math.pow(w, 0.51456);
            default:
              return NaN;
          }
        }

        function formatNumber(value, decimals) {
          if (!isFinite(value)) return "–";
          return value.toFixed(decimals);
        }

        function roundToStep(value, step) {
          if (!isFinite(value) || step <= 0) return value;
          return Math.round(value / step) * step;
        }

        function validateInputs() {
          const weight = parseFloat(weightInput.value);
          const height = parseFloat(heightInput.value);
          const dosePerM2 = parseFloat(dosePerM2Input.value);

          if (!isFinite(weight) || weight <= 0) {
            alert("Please enter a valid weight.");
            return null;
          }
          if (!isFinite(height) || height <= 0) {
            alert("Please enter a valid height.");
            return null;
          }

          // Basic sanity checks (in metric equivalents)
          const { weightKg, heightCm } = toMetric(weight, height);
          if (weightKg < 2 || weightKg > 300) {
            if (
              !confirm(
                "The weight is outside typical clinical ranges (2–300 kg). Continue?"
              )
            ) {
              return null;
            }
          }
          if (heightCm < 30 || heightCm > 250) {
            if (
              !confirm(
                "The height is outside typical clinical ranges (30–250 cm). Continue?"
              )
            ) {
              return null;
            }
          }

          if (!isFinite(dosePerM2) || dosePerM2 < 0) {
            alert("Please enter a valid dose per m² (mg/m²).");
            return null;
          }

          return { weight, height, dosePerM2 };
        }

        calculateBtn.addEventListener("click", () => {
          const base = validateInputs();
          if (!base) return;

          const { weight, height, dosePerM2 } = base;
          const { weightKg, heightCm } = toMetric(weight, height);

          const primaryFormula = formulaSelect.value;
          const primaryBSA = calcBSA(primaryFormula, weightKg, heightCm);
          if (!isFinite(primaryBSA) || primaryBSA <= 0) {
            alert("Unable to compute BSA with the given inputs.");
            return;
          }

          // Apply BSA cap if provided
          let effectiveBSA = primaryBSA;
          const bsaCap = parseFloat(bsaCapInput.value);
          let capNote = "";
          if (isFinite(bsaCap) && bsaCap > 0) {
            if (primaryBSA > bsaCap) {
              effectiveBSA = bsaCap;
              capNote = `BSA capped at ${bsaCap.toFixed(
                2
              )} m² (calculated: ${primaryBSA.toFixed(2)} m²).`;
            } else {
              capNote = `BSA cap set to ${bsaCap.toFixed(
                2
              )} m² (no effect; calculated BSA is lower).`;
            }
          }

          // Dose calculations
          const uncappedDose = effectiveBSA * dosePerM2;
          let cappedDose = uncappedDose;
          const maxDose = parseFloat(maxDoseInput.value);
          if (isFinite(maxDose) && maxDose > 0 && cappedDose > maxDose) {
            cappedDose = maxDose;
          }

          let roundedDose = cappedDose;
          const roundingStep =
            roundingSelect.value === "none"
              ? null
              : parseFloat(roundingSelect.value);
          if (roundingStep && isFinite(roundingStep) && roundingStep > 0) {
            roundedDose = roundToStep(cappedDose, roundingStep);
          }

          // Update primary results
          bsaValueSpan.textContent = formatNumber(primaryBSA, 2);
          const formulaLabels = {
            mosteller: "Mosteller formula",
            dubois: "DuBois & DuBois formula",
            haycock: "Haycock formula",
            boyd: "Boyd formula",
            gehan: "Gehan & George formula",
          };
          bsaFormulaLabel.textContent =
            formulaLabels[primaryFormula] || "Selected BSA formula";
          bsaCapNote.textContent = capNote;

          const drugName = drugNameInput.value.trim() || "Unnamed regimen";
          resultDrugName.textContent = drugName;
          resultDosePerM2.textContent = isFinite(dosePerM2)
            ? dosePerM2.toFixed(2)
            : "–";
          resultUncappedDose.textContent = isFinite(uncappedDose)
            ? uncappedDose.toFixed(2)
            : "–";
          resultCappedDose.textContent = isFinite(cappedDose)
            ? cappedDose.toFixed(2)
            : "–";
          resultRoundedDose.textContent = isFinite(roundedDose)
            ? roundedDose.toFixed(2)
            : "–";

          // BSA comparison table
          const formulas = ["mosteller", "dubois", "haycock", "boyd", "gehan"];
          const mostellerBSA = calcBSA("mosteller", weightKg, heightCm);
          bsaComparisonBody.innerHTML = "";
          formulas.forEach((f) => {
            const bsa = calcBSA(f, weightKg, heightCm);
            if (!isFinite(bsa) || bsa <= 0) return;
            const diff = ((bsa - mostellerBSA) / mostellerBSA) * 100;
            const tr = document.createElement("tr");
            tr.className = "border-t border-gray-100";
            const nameCell = document.createElement("td");
            nameCell.className = "px-2 py-1";
            nameCell.textContent = formulaLabels[f] || f;
            const bsaCell = document.createElement("td");
            bsaCell.className = "px-2 py-1 text-right";
            bsaCell.textContent = bsa.toFixed(3);
            const diffCell = document.createElement("td");
            diffCell.className = "px-2 py-1 text-right";
            diffCell.textContent =
              diff === 0 ? "0%" : `${diff > 0 ? "+" : ""}${diff.toFixed(1)}%`;
            tr.appendChild(nameCell);
            tr.appendChild(bsaCell);
            tr.appendChild(diffCell);
            bsaComparisonBody.appendChild(tr);
          });

          resultsContainer.classList.remove("hidden");

          // Update history
          const now = new Date();
          history.push({
            time: now.toLocaleTimeString(),
            drug: drugName,
            bsa: primaryBSA,
            dosePerM2,
            uncappedDose,
            cappedDose,
            roundedDose,
          });

          historyBody.innerHTML = "";
          history.forEach((entry) => {
            const tr = document.createElement("tr");
            tr.className = "border-t border-gray-100";
            tr.innerHTML = `
          <td class="px-2 py-1">${entry.time}</td>
          <td class="px-2 py-1">${entry.drug}</td>
          <td class="px-2 py-1 text-right">${entry.bsa.toFixed(2)}</td>
          <td class="px-2 py-1 text-right">${entry.dosePerM2.toFixed(2)}</td>
          <td class="px-2 py-1 text-right">${entry.uncappedDose.toFixed(2)}</td>
          <td class="px-2 py-1 text-right">${entry.cappedDose.toFixed(2)}</td>
          <td class="px-2 py-1 text-right">${entry.roundedDose.toFixed(2)}</td>
        `;
            historyBody.appendChild(tr);
          });
          if (history.length > 0) {
            historyContainer.classList.remove("hidden");
          }
        });

        resetBtn.addEventListener("click", () => {
          weightInput.value = "";
          heightInput.value = "";
          document.getElementById("age").value = "";
          formulaSelect.value = "mosteller";
          bsaCapInput.value = "";
          drugNameInput.value = "";
          dosePerM2Input.value = "";
          maxDoseInput.value = "";
          roundingSelect.value = "none";
          resultsContainer.classList.add("hidden");
