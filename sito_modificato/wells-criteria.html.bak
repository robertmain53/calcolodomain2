    <script>
      // Tab switching
      const tabPe = document.getElementById("tab-pe");
      const tabDvt = document.getElementById("tab-dvt");
      const panelPe = document.getElementById("panel-pe");
      const panelDvt = document.getElementById("panel-dvt");
      const resultSummary = document.getElementById("result-summary");

      function setActiveTab(type) {
        const isPE = type === "pe";
        tabPe.classList.toggle("bg-white", isPE);
        tabPe.classList.toggle("bg-gray-100", !isPE);
        tabPe.classList.toggle("text-blue-600", isPE);
        tabPe.classList.toggle("text-gray-700", !isPE);
        tabPe.setAttribute("aria-selected", isPE ? "true" : "false");

        tabDvt.classList.toggle("bg-white", !isPE);
        tabDvt.classList.toggle("bg-gray-100", isPE);
        tabDvt.classList.toggle("text-blue-600", !isPE);
        tabDvt.classList.toggle("text-gray-700", isPE);
        tabDvt.setAttribute("aria-selected", !isPE ? "true" : "false");

        panelPe.classList.toggle("hidden", !isPE);
        panelDvt.classList.toggle("hidden", isPE);
      }

      tabPe.addEventListener("click", () => setActiveTab("pe"));
      tabDvt.addEventListener("click", () => setActiveTab("dvt"));

      // Calculation helpers
      function calculateScore(formId, scoreSpanId, type) {
        const form = document.getElementById(formId);
        const scoreSpan = document.getElementById(scoreSpanId);
        let total = 0;
        form.querySelectorAll('input[type="checkbox"]').forEach((cb) => {
          if (cb.checked) {
            const pts = parseFloat(cb.getAttribute("data-points") || "0");
            total += pts;
          }
        });
        total = Math.round(total * 10) / 10;
        scoreSpan.textContent = total.toFixed(1);
        updateSummary(type, total);
      }

      function interpretPE(score) {
        let threeLevel, twoLevel;
        if (score <= 1) threeLevel = "Low";
        else if (score >= 7) threeLevel = "High";
        else threeLevel = "Intermediate";

        twoLevel = score <= 4 ? "PE unlikely" : "PE likely";
        return { threeLevel, twoLevel };
      }

      function interpretDVT(score) {
        let threeLevel, twoLevel;
        if (score <= 0) threeLevel = "Low";
        else if (score >= 3) threeLevel = "High";
        else threeLevel = "Moderate";

        twoLevel = score <= 1 ? "DVT unlikely" : "DVT likely";
        return { threeLevel, twoLevel };
      }

      function updateSummary(type, score) {
        const isPE = type === "pe";
        const { threeLevel, twoLevel } = isPE
          ? interpretPE(score)
          : interpretDVT(score);
        const label = isPE ? "PE" : "DVT";

        let suggestion = "";
        if (threeLevel === "Low") {
          suggestion = `Low pre-test probability of ${label}. In many patients, a negative high-sensitivity D-dimer can safely exclude ${label}.`;
        } else if (threeLevel === "Intermediate" || threeLevel === "Moderate") {
          suggestion = `Intermediate pre-test probability of ${label}. Consider D-dimer vs. direct imaging based on clinical context and local protocol.`;
        } else {
          suggestion = `High pre-test probability of ${label}. Imaging is usually indicated; consider empiric anticoagulation if imaging is delayed and bleeding risk is acceptable.`;
        }

        resultSummary.innerHTML = `
      <h2 class="font-semibold mb-1 text-blue-800 text-sm">${label} Wells score: ${score.toFixed(
          1
        )}</h2>
      <p class="text-sm text-blue-900">
        <span class="font-medium">3-level category:</span> ${threeLevel}<br>
        <span class="font-medium">2-level category:</span> ${twoLevel}<br>
        <span class="block mt-1">${suggestion}</span>
      </p>
    `;
      }

      // Attach listeners
      document
        .getElementById("btn-calc-pe")
        .addEventListener("click", () =>
          calculateScore("form-pe", "pe-score", "pe")
        );
      document
        .getElementById("btn-calc-dvt")
        .addEventListener("click", () =>
          calculateScore("form-dvt", "dvt-score", "dvt")
        );

      document.getElementById("btn-reset-pe").addEventListener("click", () => {
        document.getElementById("form-pe").reset();
        document.getElementById("pe-score").textContent = "0.0";
        resultSummary.innerHTML = `
      <h2 class="font-semibold mb-1 text-blue-800 text-sm">Current Result</h2>
      <p class="text-sm text-blue-900">
        Select criteria below to calculate the Wells score for PE or DVT.
      </p>
    `;
      });

      document.getElementById("btn-reset-dvt").addEventListener("click", () => {
        document.getElementById("form-dvt").reset();
        document.getElementById("dvt-score").textContent = "0.0";
        resultSummary.innerHTML = `
      <h2 class="font-semibold mb-1 text-blue-800 text-sm">Current Result</h2>
      <p class="text-sm text-blue-900">
        Select criteria below to calculate the Wells score for PE or DVT.
      </p>
    `;
      });

      // Auto-update on checkbox change for better UX
      document
        .getElementById("form-pe")
        .addEventListener("change", () =>
          calculateScore("form-pe", "pe-score", "pe")
        );
      document
        .getElementById("form-dvt")
        .addEventListener("change", () =>
          calculateScore("form-dvt", "dvt-score", "dvt")
        );
    </script>

