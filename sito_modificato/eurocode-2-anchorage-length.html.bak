    <script>
      // Eurocode 2 anchorage length calculator logic
      function calcFctk005(fck) {
        // EN 1992-1-1 Table 3.1: approximate lower characteristic tensile strength
        if (fck <= 50) {
          const fctm = 0.3 * Math.pow(fck, 2 / 3); // MPa
          return 0.7 * fctm; // fctk,0.05 ≈ 0.7 fctm
        } else {
          // Simplified extension for high strength concrete
          const fctm50 = 0.3 * Math.pow(50, 2 / 3);
          const fctm = fctm50 + 0.3 * (fck - 50);
          return 0.7 * fctm;
        }
      }

      function roundTo(value, decimals) {
        const factor = Math.pow(10, decimals);
        return Math.round(value * factor) / factor;
      }

      function updateAnchorage() {
        const fck = parseFloat(document.getElementById("fck").value) || 0;
        const fyk = parseFloat(document.getElementById("fyk").value) || 0;
        const phi = parseFloat(document.getElementById("phi").value) || 0;
        const sigmaSd =
          parseFloat(document.getElementById("sigmaSd").value) || 0;
        const cover = parseFloat(document.getElementById("cover").value) || 0;

        const stressState =
          document.querySelector('input[name="stressState"]:checked')?.value ||
          "tension";
        const bondCondition = document.getElementById("bondCondition").value;
        const anchorageType = document.getElementById("anchorageType").value;
        const confinement = document.getElementById("confinement").value;
        const lapMode = document.getElementById("lapMode").value;

        if (!fck || !fyk || !phi || !sigmaSd) {
          return;
        }

        // 1) Bond strength fbd
        const fctk005 = calcFctk005(fck);
        const gammaC = 1.5;
        const fctd = fctk005 / gammaC;

        let eta1 = bondCondition === "good" ? 1.0 : 0.7;
        let eta2 = 1.0; // φ <= 32 mm
        let fbd = 2.25 * eta1 * eta2 * fctd; // MPa

        // Slightly higher bond in compression
        if (stressState === "compression") {
          fbd *= 1.25;
        }

        // 2) Basic required anchorage length lb,rqd
        let lbRqd = (phi / 4.0) * (sigmaSd / fbd); // mm

        // 3) Modification factors α1..α5 (simplified)
        let alpha1 = 1.0; // bar shape
        if (anchorageType === "hook90") alpha1 = 0.7;
        if (anchorageType === "hook135") alpha1 = 0.6;

        let alpha2 = 1.0; // cover & confinement
        const cdOverPhi = cover / phi;
        if (cdOverPhi >= 3.0) alpha2 = 0.9;
        if (confinement === "links") alpha2 *= 0.9;
        if (confinement === "welded") alpha2 *= 0.8;

        let alpha3 = 1.0; // transverse pressure (ignored)
        let alpha4 = 1.0; // welded transverse bars (already in confinement)
        let alpha5 = 1.0; // excess reinforcement (assume required ≈ provided)

        let lbDesign = alpha1 * alpha2 * alpha3 * alpha4 * alpha5 * lbRqd;

        // 4) Minimum anchorage length lb,min
        const lbMin = Math.max(0.3 * lbRqd, 10 * phi, 100);

        if (lbDesign < lbMin) lbDesign = lbMin;

        // 5) Lap length (tension) if requested
        let lapLength = null;
        if (lapMode === "yes") {
          // Simplified: start from lb,rqd and apply conservative factor
          let alpha6 = 1.0;
          // assume 50-100% of bars lapped in a section → increase
          alpha6 = 1.3;
          lapLength = alpha6 * lbRqd;

          const lapMin = Math.max(0.3 * lbRqd, 15 * phi, 200);
          if (lapLength < lapMin) lapLength = lapMin;
        }

        // Update UI
        document.getElementById("lbRqdVal").textContent = isFinite(lbRqd)
          ? roundTo(lbRqd, 0)
          : "–";
        document.getElementById("lbDesignVal").textContent = isFinite(lbDesign)
          ? roundTo(lbDesign, 0)
          : "–";
        document.getElementById("lbMinVal").textContent = isFinite(lbMin)
          ? roundTo(lbMin, 0)
          : "–";

        const lapBox = document.getElementById("lapResultBox");
        if (lapMode === "yes" && lapLength !== null && isFinite(lapLength)) {
          lapBox.classList.remove("hidden");
          document.getElementById("lapLengthVal").textContent = roundTo(
            lapLength,
            0
          );
        } else {
          lapBox.classList.add("hidden");
        }
      }

      document
        .getElementById("calcBtn")
        .addEventListener("click", function (e) {
          e.preventDefault();
          updateAnchorage();
        });

      // Auto-calc on input change for better UX
      document
        .querySelectorAll(
          '#fck,#fyk,#phi,#sigmaSd,#cover,#bondCondition,#anchorageType,#confinement,#lapMode,input[name="stressState"]'
        )
        .forEach((el) => {
          el.addEventListener("change", updateAnchorage);
        });

      // Initial calculation
      document.addEventListener("DOMContentLoaded", updateAnchorage);
    </script>

