    <script>
      // Mobile menu toggle
      document
        .getElementById("mobile-menu-toggle")
        ?.addEventListener("click", () => {
          const menu = document.getElementById("mobile-menu");
          if (!menu) return;
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
        });

      // Nominal voltage custom handling
      const nominalSelect = document.getElementById("nominal-voltage");
      const customVoltageInput = document.getElementById("custom-voltage");
      nominalSelect.addEventListener("change", () => {
        if (nominalSelect.value === "custom") {
          customVoltageInput.classList.remove("hidden");
        } else {
          customVoltageInput.classList.add("hidden");
        }
      });

      // Load type custom limit
      const loadTypeSelect = document.getElementById("load-type");
      const customLimitWrapper = document.getElementById(
        "custom-limit-wrapper"
      );
      loadTypeSelect.addEventListener("change", () => {
        if (loadTypeSelect.value === "custom") {
          customLimitWrapper.classList.remove("hidden");
        } else {
          customLimitWrapper.classList.add("hidden");
        }
      });

      // Cable type custom mV/A/m
      const cableTypeSelect = document.getElementById("cable-type");
      const customMvWrapper = document.getElementById("custom-mv-wrapper");
      cableTypeSelect.addEventListener("change", () => {
        if (cableTypeSelect.value === "custom") {
          customMvWrapper.classList.remove("hidden");
        } else {
          customMvWrapper.classList.add("hidden");
        }
      });

      function getNominalVoltage() {
        const sel = nominalSelect.value;
        if (sel === "custom") {
          const v = parseFloat(customVoltageInput.value);
          return isFinite(v) && v > 0 ? v : NaN;
        }
        return parseFloat(sel);
      }

      function getCableMv() {
        const map = {
          "2.5twin": 18,
          "4twin": 11,
          "6twin": 7.3,
          "10twin": 4.4,
          "16cu3ph": 2.8,
          "25cu3ph": 1.75,
          "35alu3ph": 3.1,
        };
        if (cableTypeSelect.value === "custom") {
          const v = parseFloat(document.getElementById("custom-mv").value);
          return isFinite(v) && v > 0 ? v : NaN;
        }
        return map[cableTypeSelect.value] ?? NaN;
      }

      document.getElementById("calc-btn").addEventListener("click", () => {
        const Ib = parseFloat(document.getElementById("design-current").value);
        const L = parseFloat(document.getElementById("circuit-length").value);
        const Un = getNominalVoltage();
        const mv = getCableMv();
        const includeNeutral =
          document.getElementById("include-neutral").checked;

        const resultsContent = document.getElementById("results-content");
        const neutralNote = document.getElementById("neutral-note");
        const statusBadge = document.getElementById("status-badge");
        const limitText = document.getElementById("limit-text");
        const deltaVSpan = document.getElementById("delta-v");
        const deltaVPctSpan = document.getElementById("delta-v-pct");
        const endVoltageSpan = document.getElementById("end-voltage");
        const mvUsedSpan = document.getElementById("mv-used");
        const iblSpan = document.getElementById("ib-l");
        const stepsText = document.getElementById("steps-text");

        // Basic validation
        if (
          !isFinite(Ib) ||
          Ib <= 0 ||
          !isFinite(L) ||
          L <= 0 ||
          !isFinite(Un) ||
          Un <= 0 ||
          !isFinite(mv) ||
          mv <= 0
        ) {
          alert(
            "Please enter valid values for design current, circuit length, nominal voltage and mV/A/m."
          );
          resultsContent.classList.add("hidden");
          return;
        }

        // Determine limit
        let limitPct;
        if (loadTypeSelect.value === "lighting") {
          limitPct = 3;
        } else if (loadTypeSelect.value === "other") {
          limitPct = 5;
        } else {
          const customLimit = parseFloat(
            document.getElementById("custom-limit").value
          );
          limitPct =
            isFinite(customLimit) && customLimit > 0 ? customLimit : NaN;
        }
        if (!isFinite(limitPct)) {
          alert("Please enter a valid custom voltage drop limit (%).");
          resultsContent.classList.add("hidden");
          return;
        }

        // Core BS 7671 formula
        let deltaV = (mv * Ib * L) / 1000; // in volts

        // Simple neutral / harmonics allowance (10% uplift)
        if (includeNeutral) {
          deltaV *= 1.1;
          neutralNote.classList.remove("hidden");
        } else {
          neutralNote.classList.add("hidden");
        }

        const deltaVPct = (deltaV / Un) * 100;
        const endVoltage = Un - deltaV;

        // Status
        const withinLimit = deltaVPct <= limitPct + 1e-6;
        statusBadge.textContent = withinLimit
          ? "Within BS 7671 design limit"
          : "Exceeds BS 7671 design limit";
        statusBadge.className =
          "badge " +
          (withinLimit
            ? "bg-green-100 text-green-800"
            : "bg-red-100 text-red-800");
        limitText.textContent = `Calculated drop: ${deltaVPct.toFixed(
          2
        )}% (limit ${limitPct.toFixed(1)}%)`;

        // Populate values
        deltaVSpan.textContent = deltaV.toFixed(2);
        deltaVPctSpan.textContent = deltaVPct.toFixed(2);
        endVoltageSpan.textContent = endVoltage.toFixed(1);
        mvUsedSpan.textContent = mv.toFixed(3);
        iblSpan.textContent = (Ib * L).toFixed(1);

        // Step-by-step text
        const sysType =
          document.getElementById("system-type").value === "single"
            ? "single‑phase"
            : "three‑phase";
        const loadLabel =
          loadTypeSelect.options[loadTypeSelect.selectedIndex].textContent;
        stepsText.innerHTML = `
      <p><strong>1.</strong> System type: ${sysType}, nominal voltage \(U_n = ${Un.toFixed(
          1
        )}\) V.</p>
      <p><strong>2.</strong> Design current \(I_b = ${Ib.toFixed(
        2
      )}\) A, circuit length \(L = ${L.toFixed(1)}\) m.</p>
      <p><strong>3.</strong> Tabulated value from BS 7671 / manufacturer: ${mv.toFixed(
        3
      )} mV/A/m.</p>
      <p><strong>4.</strong> Voltage drop (before any neutral allowance):</p>
      <p>&nbsp;&nbsp;&nbsp;&nbsp;\\( \\Delta V = \\dfrac{${mv.toFixed(
        3
      )} \\times ${Ib.toFixed(2)} \\times ${L.toFixed(1)}}{1000} = ${(
          (mv * Ib * L) /
          1000
        ).toFixed(3)} \\text{ V} \\)</p>
      ${
        includeNeutral
          ? `<p><strong>5.</strong> Neutral / harmonic allowance: 10% uplift → final \\( \\Delta V = ${deltaV.toFixed(
              3
            )} \\) V.</p>`
          : ""
      }
      <p><strong>${
        includeNeutral ? "6" : "5"
      }.</strong> Percentage voltage drop:</p>
      <p>&nbsp;&nbsp;&nbsp;&nbsp;\\( \\%\\Delta V = \\dfrac{${deltaV.toFixed(
        3
      )}}{${Un.toFixed(1)}} \\times 100 = ${deltaVPct.toFixed(2)}\\% \\)</p>
      <p><strong>${
        includeNeutral ? "7" : "6"
      }.</strong> End‑of‑line voltage:</p>
      <p>&nbsp;&nbsp;&nbsp;&nbsp;\\( U_{end} = ${Un.toFixed(
        1
      )} - ${deltaV.toFixed(3)} = ${endVoltage.toFixed(2)} \\text{ V} \\)</p>
      <p><strong>${
        includeNeutral ? "8" : "7"
      }.</strong> BS 7671 design check for ${loadLabel}: limit = ${limitPct.toFixed(
          1
        )}%, result = ${deltaVPct.toFixed(2)}% → <strong>${
          withinLimit ? "PASS" : "FAIL"
        }</strong>.</p>
    `;

        resultsContent.classList.remove("hidden");

        if (window.MathJax && window.MathJax.typesetPromise) {
          window.MathJax.typesetPromise([stepsText]);
        }
      });
    </script>

