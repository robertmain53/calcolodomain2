    <script src="/assets/js/mobile-menu.js"></script>
    <script src="/search.js"></script>

    <script>
      // Core rating factors
      const stateBaseRates = {
        "US-avg": 1500,
        CA: 1900,
        TX: 1700,
        FL: 2200,
        NY: 2100,
        MI: 2600,
        NJ: 2000,
        IL: 1500,
        PA: 1500,
        OH: 1200,
        GA: 1700,
        NC: 1300,
        VA: 1300,
        WA: 1400,
        AZ: 1500,
        CO: 1600,
        MA: 1800,
        Other: 1400,
      };

      function getAgeFactor(age) {
        switch (age) {
          case "18-24":
            return 1.6;
          case "25-34":
            return 1.2;
          case "35-44":
            return 1.0;
          case "45-64":
            return 0.95;
          case "65+":
            return 1.1;
          default:
            return 1.0;
        }
      }

      function getYearsLicensedFactor(years) {
        switch (years) {
          case "0-2":
            return 1.3;
          case "3-5":
            return 1.1;
          case "6-9":
            return 1.0;
          case "10+":
            return 0.95;
          default:
            return 1.0;
        }
      }

      function getCreditFactor(credit) {
        switch (credit) {
          case "excellent":
            return 0.85;
          case "good":
            return 0.95;
          case "fair":
            return 1.1;
          case "poor":
            return 1.3;
          case "no-credit":
            return 1.15;
          default:
            return 1.0;
        }
      }

      function getMaritalFactor(marital) {
        if (marital === "married") return 0.95;
        return 1.0;
      }

      function getPriorInsuranceFactor(prior) {
        switch (prior) {
          case "12+":
            return 0.95;
          case "6-11":
            return 1.0;
          case "1-5":
            return 1.1;
          case "lapse":
            return 1.25;
          case "new":
            return 1.2;
          default:
            return 1.0;
        }
      }

      function getVehicleAgeFactor(age) {
        switch (age) {
          case "new":
            return 1.2;
          case "mid":
            return 1.0;
          case "old":
            return 0.8;
          default:
            return 1.0;
        }
      }

      function getVehicleTypeFactor(type) {
        switch (type) {
          case "sedan":
            return 1.0;
          case "suv":
            return 1.05;
          case "truck":
            return 1.1;
          case "sports":
            return 1.3;
          case "minivan":
            return 0.95;
          default:
            return 1.0;
        }
      }

      function getMileageFactor(miles) {
        switch (miles) {
          case "0-7500":
            return 0.9;
          case "7501-12000":
            return 1.0;
          case "12001-15000":
            return 1.1;
          case "15001+":
            return 1.2;
          default:
            return 1.0;
        }
      }

      function getUsageFactor(usage) {
        switch (usage) {
          case "pleasure":
            return 0.95;
          case "commute":
            return 1.05;
          case "business":
            return 1.2;
          default:
            return 1.0;
        }
      }

      function getGaragingFactor(garaging) {
        switch (garaging) {
          case "garage":
            return 0.95;
          case "street":
            return 1.05;
          case "mixed":
            return 1.0;
          default:
            return 1.0;
        }
      }

      function getOwnershipFactor(ownership) {
        // Ownership mostly affects coverage requirements; small factor here
        switch (ownership) {
          case "owned":
            return 1.0;
          case "financed":
            return 1.05;
          case "leased":
            return 1.08;
          default:
            return 1.0;
        }
      }

      function getCoverageFactor(level, collisionDed, compDed, vehicleAge) {
        let factor = 1.0;
        switch (level) {
          case "basic":
            factor = 0.7;
            break;
          case "recommended":
            factor = 1.0;
            break;
          case "premium":
            factor = 1.25;
            break;
          default:
            factor = 1.0;
        }

        // Adjust for deductibles if collision/comp present
        function dedFactor(ded) {
          if (ded === 0) return 0.0; // no coverage
          if (ded <= 100) return 1.15;
          if (ded <= 250) return 1.05;
          if (ded <= 500) return 1.0;
          return 0.9; // 1000+
        }

        const collF = dedFactor(collisionDed);
        const compF = dedFactor(compDed);

        // Weight collision/comp more for newer cars
        const ageWeight =
          vehicleAge === "new" ? 0.25 : vehicleAge === "mid" ? 0.18 : 0.1;
        factor *= 1 + ageWeight * (collF - 1 + (compF - 1));

        return factor;
      }

      function getHistoryFactor(tickets, accidents, dui) {
        let factor = 1.0;
        factor *= 1 + 0.08 * Math.min(tickets, 5);
        factor *= 1 + 0.25 * Math.min(accidents, 3);
        if (dui === "recent") factor *= 1.8;
        else if (dui === "older") factor *= 1.3;
        return factor;
      }

      function getDiscountFactor(discounts) {
        switch (discounts) {
          case "none":
            return 1.0;
          case "multi":
            return 0.9;
          case "telematics":
            return 0.88;
          case "good-student":
            return 0.92;
          case "stacked":
            return 0.8;
          default:
            return 1.0;
        }
      }

      function formatCurrency(value) {
        return (
          "$" + value.toLocaleString("en-US", { maximumFractionDigits: 0 })
        );
      }

      function updateCoverageBreakdown(level, collisionDed, compDed) {
        let liability = 0.5,
          collision = 0.3,
          comp = 0.15,
          other = 0.05;

        if (level === "basic") {
          liability = 0.8;
          collision = 0.0;
          comp = 0.0;
          other = 0.2;
        } else {
          if (collisionDed === 0) collision = 0;
          if (compDed === 0) comp = 0;
          const total = liability + collision + comp + other;
          liability / total;
          collision / total;
          comp / total;
          other / total;
        }

        document.getElementById("liability-share").textContent =
          Math.round(liability * 100) + "%";
        document.getElementById("collision-share").textContent =
          Math.round(collision * 100) + "%";
        document.getElementById("comp-share").textContent =
          Math.round(comp * 100) + "%";
        document.getElementById("other-share").textContent =
          Math.round(other * 100) + "%";
      }

      function buildCoverageRecommendation(level, age, ownership, vehicleAge) {
        const recEl = document.getElementById("coverage-recommendation");
        const bulletsEl = document.getElementById("coverage-bullets");
        bulletsEl.innerHTML = "";

        let title = "";
        let summary = "";

        if (level === "basic") {
          title = "Basic liability-only coverage";
          summary =
            "You selected a basic package focused on meeting legal requirements at the lowest cost.";
        } else if (level === "recommended") {
          title = "Balanced protection with higher limits and full coverage";
          summary =
            "You selected a recommended package that balances cost and protection for most drivers.";
        } else {
          title = "High-limit, low-deductible coverage";
          summary =
            "You selected a premium package designed to protect significant assets and minimize out-of-pocket costs.";
        }

        recEl.textContent = title + " " + summary;

        const suggestions = [];

        if (age === "18-24") {
          suggestions.push(
            "Consider higher liability limits (e.g., 100/300/50) due to higher claim frequency for young drivers."
          );
        } else {
          suggestions.push(
            "Aim for at least 100/300/50 liability limits; higher if you own a home or have substantial savings."
          );
        }

        if (ownership !== "owned" || vehicleAge === "new") {
          suggestions.push(
            "Maintain collision and comprehensive coverage while the vehicle is new or financed/leased."
          );
        } else if (vehicleAge === "old") {
          suggestions.push(
            "For older, low-value vehicles, consider whether collision/comprehensive are still cost-effective."
          );
        }

        suggestions.push(
          "Add uninsured/underinsured motorist coverage where available, ideally matching your liability limits."
        );
        suggestions.push(
          "Review medical payments (MedPay) or personal injury protection (PIP) options based on your health insurance."
        );

        suggestions.forEach((text) => {
          const li = document.createElement("li");
          li.textContent = text;
          bulletsEl.appendChild(li);
        });
      }

      function buildSavingsTips(
        historyFactor,
        discountFactor,
        coverageLevel,
        collisionDed,
        compDed,
        mileageFactor,
        usageFactor
      ) {
        const tipsEl = document.getElementById("savings-tips");
        tipsEl.innerHTML = "";
        const tips = [];

        if (
          coverageLevel !== "basic" &&
          (collisionDed < 1000 || compDed < 1000)
        ) {
          tips.push(
            "Raising your collision and comprehensive deductibles (for example from $500 to $1,000) can lower your premium if you can afford a higher out-of-pocket cost after a claim."
          );
        }

        if (coverageLevel === "premium") {
          tips.push(
            "If budget is tight, consider stepping down from a premium package to a recommended package with slightly lower limits and higher deductibles."
          );
        }

        if (
          mileageFactor > 1.0 ||
          usageFactor === "commute" ||
          usageFactor === "business"
        ) {
          tips.push(
            "Reducing annual mileage, carpooling, or using public transit a few days a week can qualify you for low-mileage discounts with many insurers."
          );
        }

        if (discountFactor === 1.0) {
          tips.push(
            "Ask about discounts for bundling auto with home or renters insurance, enrolling in a telematics program, or completing a defensive driving course."
          );
        }

        if (historyFactor > 1.0) {
          tips.push(
            "Keeping a clean record going forward can significantly reduce your premium once tickets and accidents fall off your record (typically after 3–5 years)."
          );
        }

        if (!tips.length) {
          tips.push(
            "You already appear to have a relatively efficient coverage setup. Shop around with multiple insurers and consider bundling policies to find additional savings."
          );
        }

        tips.forEach((text) => {
          const li = document.createElement("li");
          li.textContent = text;
          tipsEl.appendChild(li);
        });
      }

      document
        .getElementById("calculate-btn")
        .addEventListener("click", function () {
          const state = document.getElementById("state").value;
          const age = document.getElementById("age").value;
          const yearsLicensed = document.getElementById("years-licensed").value;
          const credit = document.getElementById("credit").value;
          const marital = document.getElementById("marital").value;
          const priorInsurance =
            document.getElementById("prior-insurance").value;

          const vehicleAge = document.getElementById("vehicle-age").value;
          const vehicleType = document.getElementById("vehicle-type").value;
          const annualMileage = document.getElementById("annual-mileage").value;
          const usage = document.getElementById("usage").value;
          const ownership = document.getElementById("ownership").value;
          const garaging = document.getElementById("garaging").value;

          const coverageLevel =
            document.querySelector("input[name='coverage-level']:checked")
              ?.value || "recommended";
          const collisionDed = parseInt(
            document.getElementById("collision-ded").value,
            10
          );
          const compDed = parseInt(
            document.getElementById("comp-ded").value,
            10
          );

          const tickets = parseInt(
            document.getElementById("tickets").value || "0",
            10
          );
          const accidents = parseInt(
            document.getElementById("accidents").value || "0",
            10
          );
          const dui = document.getElementById("dui").value;
          const discounts = document.getElementById("discounts").value;

          const base = stateBaseRates[state] || stateBaseRates["US-avg"];

          const ageF = getAgeFactor(age);
          const yearsF = getYearsLicensedFactor(yearsLicensed);
          const creditF = getCreditFactor(credit);
          const maritalF = getMaritalFactor(marital);
          const priorF = getPriorInsuranceFactor(priorInsurance);

          const vehicleAgeF = getVehicleAgeFactor(vehicleAge);
          const vehicleTypeF = getVehicleTypeFactor(vehicleType);
          const mileageF = getMileageFactor(annualMileage);
          const usageF = getUsageFactor(usage);
          const garagingF = getGaragingFactor(garaging);
          const ownershipF = getOwnershipFactor(ownership);

          const coverageF = getCoverageFactor(
            coverageLevel,
            collisionDed,
            compDed,
            vehicleAge
          );
          const historyF = getHistoryFactor(tickets, accidents, dui);
          const discountF = getDiscountFactor(discounts);

          let annual = base;
          annual *= ageF * yearsF * creditF * maritalF * priorF;
          annual *=
            vehicleAgeF *
            vehicleTypeF *
            mileageF *
            usageF *
            garagingF *
            ownershipF;
          annual *= coverageF * historyF * discountF;

          // Clamp to a reasonable range
          annual = Math.max(400, Math.min(annual, 8000));

          const low = Math.round(annual * 0.85);
          const high = Math.round(annual * 1.15);
          const monthlyLow = Math.round(low / 12);
          const monthlyHigh = Math.round(high / 12);

          document.getElementById("monthly-range").textContent =
            formatCurrency(monthlyLow) + " – " + formatCurrency(monthlyHigh);
          document.getElementById("annual-range").textContent =
            formatCurrency(low) + " – " + formatCurrency(high);

          // Risk label
          const riskLabel = document.getElementById("risk-label");
          const compositeRisk = ageF * yearsF * historyF;
          if (compositeRisk < 1.0) {
            riskLabel.textContent =
              "Profile: lower-than-average risk based on age, experience, and clean driving history.";
          } else if (compositeRisk < 1.4) {
            riskLabel.textContent =
              "Profile: average risk. Small changes in coverage or discounts could meaningfully change your price.";
          } else {
            riskLabel.textContent =
              "Profile: higher-than-average risk due to age or driving history. Expect above-average premiums.";
          }

          updateCoverageBreakdown(coverageLevel, collisionDed, compDed);
          buildCoverageRecommendation(
            coverageLevel,
            age,
            ownership,
            vehicleAge
          );
          buildSavingsTips(
            historyF,
            discountF,
            coverageLevel,
            collisionDed,
            compDed,
            mileageF,
            usageF
          );

          document.getElementById("results-section").classList.remove("hidden");
          document
            .getElementById("results-section")
            .scrollIntoView({ behavior: "smooth", block: "start" });
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
