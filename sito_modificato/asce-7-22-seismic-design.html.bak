    <script src="https://cdn.jsdelivr.net/npm/chart.js@4.4.1/dist/chart.umd.min.js"></script>
    <script>
      (function () {
        const ssEl = document.getElementById("input-ss");
        const s1El = document.getElementById("input-s1");
        const siteClassEl = document.getElementById("site-class");
        const riskCatEl = document.getElementById("risk-category");
        const tlEl = document.getElementById("tl");
        const btnCalc = document.getElementById("btn-calc");
        const btnReset = document.getElementById("btn-reset");
        const errorEl = document.getElementById("calc-error");

        const resContainer = document.getElementById("results-container");
        const resPlaceholder = document.getElementById("results-placeholder");
        const resFa = document.getElementById("res-fa");
        const resFv = document.getElementById("res-fv");
        const resSms = document.getElementById("res-sms");
        const resSm1 = document.getElementById("res-sm1");
        const resSds = document.getElementById("res-sds");
        const resSd1 = document.getElementById("res-sd1");
        const resSdc = document.getElementById("res-sdc");
        const resSdcNote = document.getElementById("res-sdc-note");
        const spectrumTableBody = document.getElementById(
          "spectrum-table-body"
        );

        let chart;

        function round(val, digits) {
          const f = Math.pow(10, digits);
          return Math.round(val * f) / f;
        }

        function getFa(siteClass) {
          switch (siteClass) {
            case "A":
              return 0.8;
            case "B":
              return 0.9;
            case "C":
              return 1.0;
            case "D":
              return 1.2;
            case "E":
              return 2.5;
            default:
              return 1.0;
          }
        }

        function getFv(siteClass) {
          switch (siteClass) {
            case "A":
              return 0.8;
            case "B":
              return 0.9;
            case "C":
              return 1.0;
            case "D":
              return 1.7;
            case "E":
              return 3.5;
            default:
              return 1.0;
          }
        }

        function estimateSDC(SDS, SD1, riskCat) {
          // Very simplified approximation inspired by ASCE tables
          let sdc = "B";
          if (SDS < 0.167 && SD1 < 0.067) {
            sdc = "A";
          } else if (SDS < 0.33 && SD1 < 0.133) {
            sdc = "B";
          } else if (SDS < 0.5 && SD1 < 0.2) {
            sdc = "C";
          } else if (SDS < 0.75 && SD1 < 0.3) {
            sdc = "D";
          } else {
            sdc = "D";
          }

          if (
            (riskCat === "III" || riskCat === "IV") &&
            (SDS >= 0.33 || SD1 >= 0.133)
          ) {
            if (sdc === "B") sdc = "C";
            else if (sdc === "C") sdc = "D";
          }
          return sdc;
        }

        function buildSpectrum(SDS, SD1, TL) {
          const T0 = (0.2 * SD1) / SDS;
          const Ts = SD1 / SDS;

          const points = [];
          const maxT = Math.max(TL, Ts * 3);
          const n = 40;
          for (let i = 0; i <= n; i++) {
            const T = (maxT * i) / n + 0.01; // avoid zero
            let Sa;
            if (T <= T0) {
              Sa = SDS * (0.4 + 0.6 * (T / T0));
            } else if (T <= Ts) {
              Sa = SDS;
            } else if (T <= TL) {
              Sa = SD1 / T;
            } else {
              Sa = (SD1 * TL) / (T * T);
            }
            points.push({ T, Sa });
          }
          return points;
        }

        function updateChart(points) {
          const ctx = document
            .getElementById("spectrum-chart")
            .getContext("2d");
          const labels = points.map((p) => round(p.T, 2));
          const data = points.map((p) => round(p.Sa, 3));

          if (chart) {
            chart.destroy();
          }

          chart = new Chart(ctx, {
            type: "line",
            data: {
              labels,
              datasets: [
                {
                  label: "Sa(T) (g)",
                  data,
                  borderColor: "#2563eb",
                  backgroundColor: "rgba(37, 99, 235, 0.1)",
                  tension: 0,
                  pointRadius: 0,
                },
              ],
            },
            options: {
              responsive: true,
              maintainAspectRatio: false,
              scales: {
                x: {
                  title: { display: true, text: "Period T (s)" },
                },
                y: {
                  title: { display: true, text: "Sa(T) (g)" },
                  beginAtZero: true,
                },
              },
              plugins: {
                legend: { display: false },
              },
            },
          });
        }

        function updateSpectrumTable(points) {
          spectrumTableBody.innerHTML = "";
          const step = Math.max(1, Math.floor(points.length / 12));
          for (let i = 0; i < points.length; i += step) {
            const p = points[i];
            const tr = document.createElement("tr");
            tr.className = i % (2 * step) === 0 ? "bg-white" : "bg-gray-50";
            tr.innerHTML = `
        <td class="px-3 py-1 border-b font-mono">${round(p.T, 3)}</td>
        <td class="px-3 py-1 border-b font-mono">${round(p.Sa, 4)}</td>
      `;
            spectrumTableBody.appendChild(tr);
          }
        }

        btnCalc.addEventListener("click", function () {
          errorEl.textContent = "";
          const Ss = parseFloat(ssEl.value);
          const S1 = parseFloat(s1El.value);

          if (isNaN(Ss) || Ss <= 0 || isNaN(S1) || S1 <= 0) {
            errorEl.textContent =
              "Please enter positive numeric values for Ss and S1.";
            return;
          }

          const siteClass = siteClassEl.value;
          const riskCat = riskCatEl.value;
          let TL = parseFloat(tlEl.value);
          if (isNaN(TL) || TL <= 0) {
            TL = 8.0; // generic default for plotting
          }

          const Fa = getFa(siteClass);
          const Fv = getFv(siteClass);
          const SMS = Fa * Ss;
          const SM1 = Fv * S1;
          const SDS = (2 / 3) * SMS;
          const SD1 = (2 / 3) * SM1;

          const sdc = estimateSDC(SDS, SD1, riskCat);

          resFa.textContent = round(Fa, 3);
          resFv.textContent = round(Fv, 3);
          resSms.textContent = round(SMS, 3);
          resSm1.textContent = round(SM1, 3);
          resSds.textContent = round(SDS, 3);
          resSd1.textContent = round(SD1, 3);
          resSdc.textContent = sdc;
          resSdcNote.textContent =
            "Approximate SDC based on SDS, SD1, and risk category. Confirm using full ASCE 7‑22 tables.";

          const spectrumPoints = buildSpectrum(SDS, SD1, TL);
          updateChart(spectrumPoints);
          updateSpectrumTable(spectrumPoints);

          resPlaceholder.classList.add("hidden");
          resContainer.classList.remove("hidden");
        });

        btnReset.addEventListener("click", function () {
          ssEl.value = "";
          s1El.value = "";
          tlEl.value = "";
          siteClassEl.value = "D";
          riskCatEl.value = "II";
          errorEl.textContent = "";
          resContainer.classList.add("hidden");
          resPlaceholder.classList.remove("hidden");
          if (chart) {
            chart.destroy();
            chart = null;
          }
          spectrumTableBody.innerHTML = "";
        });
      })();
    </script>

