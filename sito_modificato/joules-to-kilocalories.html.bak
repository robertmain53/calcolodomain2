    <script>
      // Core conversion constants
      const J_PER_KCAL = 4184; // exact

      const fromInput = document.getElementById("from-value");
      const dirJtoKcalBtn = document.getElementById("dir-j-to-kcal");
      const dirKcalToJBtn = document.getElementById("dir-kcal-to-j");
      const decimalsSelect = document.getElementById("decimals");
      const resultText = document.getElementById("result-text");
      const resultFormula = document.getElementById("result-formula");
      const inputError = document.getElementById("input-error");
      const copyBtn = document.getElementById("copy-result-btn");
      const resetBtn = document.getElementById("reset-btn");
      const swapBtn = document.getElementById("swap-btn");
      const quickTableBody = document.getElementById("quick-table-body");

      let direction = "J_TO_KCAL"; // or 'KCAL_TO_J'

      function parseNumber(value) {
        if (typeof value !== "string") return NaN;
        const trimmed = value.trim();
        if (!trimmed) return NaN;
        const normalized = trimmed.replace(/,/g, "");
        return Number(normalized);
      }

      function formatNumber(value, decimals) {
        if (!isFinite(value)) return "—";
        return value.toLocaleString(undefined, {
          minimumFractionDigits: decimals,
          maximumFractionDigits: decimals,
        });
      }

      function updateDirection(newDir) {
        direction = newDir;
        if (direction === "J_TO_KCAL") {
          dirJtoKcalBtn.classList.add(
            "bg-blue-600",
            "text-white",
            "border-blue-600"
          );
          dirJtoKcalBtn.classList.remove(
            "border-gray-300",
            "text-gray-700",
            "hover:bg-gray-100"
          );
          dirKcalToJBtn.classList.remove(
            "bg-blue-600",
            "text-white",
            "border-blue-600"
          );
          dirKcalToJBtn.classList.add(
            "border-gray-300",
            "text-gray-700",
            "hover:bg-gray-100"
          );
        } else {
          dirKcalToJBtn.classList.add(
            "bg-blue-600",
            "text-white",
            "border-blue-600"
          );
          dirKcalToJBtn.classList.remove(
            "border-gray-300",
            "text-gray-700",
            "hover:bg-gray-100"
          );
          dirJtoKcalBtn.classList.remove(
            "bg-blue-600",
            "text-white",
            "border-blue-600"
          );
          dirJtoKcalBtn.classList.add(
            "border-gray-300",
            "text-gray-700",
            "hover:bg-gray-100"
          );
        }
        compute();
      }

      function compute() {
        const raw = fromInput.value;
        const num = parseNumber(raw);
        const decimals = parseInt(decimalsSelect.value, 10);

        if (isNaN(num)) {
          inputError.classList.remove("hidden");
          fromInput.classList.add("input-error");
          resultText.textContent = "—";
          resultFormula.textContent = "";
          return;
        } else {
          inputError.classList.add("hidden");
          fromInput.classList.remove("input-error");
        }

        let result, formulaStr;

        if (direction === "J_TO_KCAL") {
          result = num / J_PER_KCAL;
          resultText.textContent = `${formatNumber(result, decimals)} kcal`;
          formulaStr = `${num.toLocaleString()} J ÷ 4184 = ${formatNumber(
            result,
            decimals
          )} kcal`;
        } else {
          result = num * J_PER_KCAL;
          resultText.textContent = `${formatNumber(result, decimals)} J`;
          formulaStr = `${num.toLocaleString()} kcal × 4184 = ${formatNumber(
            result,
            decimals
          )} J`;
        }

        resultFormula.textContent = formulaStr;
      }

      function resetAll() {
        fromInput.value = "";
        resultText.textContent = "—";
        resultFormula.textContent = "";
        inputError.classList.add("hidden");
        fromInput.classList.remove("input-error");
      }

      function copyResult() {
        const text = resultText.textContent;
        if (!text || text === "—") return;
        navigator.clipboard.writeText(text).catch(() => {});
      }

      function swapUnits() {
        if (!fromInput.value.trim()) {
          updateDirection(
            direction === "J_TO_KCAL" ? "KCAL_TO_J" : "J_TO_KCAL"
          );
          return;
        }
        const currentResult = resultText.textContent;
        const numMatch = currentResult.match(/[-+]?[\d.,]+(?:e[-+]?\d+)?/i);
        if (!numMatch) {
          updateDirection(
            direction === "J_TO_KCAL" ? "KCAL_TO_J" : "J_TO_KCAL"
          );
          return;
        }
        const numeric = parseNumber(numMatch[0]);
        if (isNaN(numeric)) {
          updateDirection(
            direction === "J_TO_KCAL" ? "KCAL_TO_J" : "J_TO_KCAL"
          );
          return;
        }
        fromInput.value = numeric;
        updateDirection(direction === "J_TO_KCAL" ? "KCAL_TO_J" : "J_TO_KCAL");
      }

      function buildQuickTable() {
        const baseValues = [1, 10, 50, 100, 500, 1000, 5000, 10000];
        quickTableBody.innerHTML = "";
        baseValues.forEach((j) => {
          const kcal = j / J_PER_KCAL;
          const row = document.createElement("tr");
          row.innerHTML = `
        <td class="px-3 py-2 border-t border-gray-200">${j.toLocaleString()} J</td>
        <td class="px-3 py-2 border-t border-gray-200">${kcal.toFixed(
          4
        )} kcal</td>
      `;
          quickTableBody.appendChild(row);
        });
      }

      // Event listeners
      fromInput.addEventListener("input", compute);
      decimalsSelect.addEventListener("change", compute);
      dirJtoKcalBtn.addEventListener("click", () =>
        updateDirection("J_TO_KCAL")
      );
      dirKcalToJBtn.addEventListener("click", () =>
        updateDirection("KCAL_TO_J")
      );
      copyBtn.addEventListener("click", copyResult);
      resetBtn.addEventListener("click", resetAll);
      if (swapBtn) swapBtn.addEventListener("click", swapUnits);

      // Initialize
      buildQuickTable();
      compute();
    </script>

