    <script>
      (function () {
        const PSI_TO_KGCM2 = 0.07030697;
        const KGCM2_TO_PSI = 14.2233433;

        const psiInput = document.getElementById("psiInput");
        const kgcm2Input = document.getElementById("kgcm2Input");
        const swapBtn = document.getElementById("swapBtn");
        const clearBtn = document.getElementById("clearBtn");
        const errorEl = document.getElementById("converterError");
        const precisionNote = document.getElementById("precisionNote");
        const tableBody = document.getElementById("psiTableBody");

        let psiIsSource = true;
        let isUpdating = false;

        function formatNumber(value, decimals) {
          if (!isFinite(value)) return "";
          return Number(value.toFixed(decimals)).toString();
        }

        function showError(msg) {
          if (!errorEl) return;
          if (!msg) {
            errorEl.classList.add("hidden");
            errorEl.textContent = "";
          } else {
            errorEl.textContent = msg;
            errorEl.classList.remove("hidden");
          }
        }

        function convertFromPsi() {
          if (isUpdating) return;
          isUpdating = true;
          showError("");
          const v = parseFloat(psiInput.value);
          if (isNaN(v)) {
            kgcm2Input.value = "";
            isUpdating = false;
            return;
          }
          kgcm2Input.value = formatNumber(v * PSI_TO_KGCM2, 6);
          isUpdating = false;
        }

        function convertFromKgcm2() {
          if (isUpdating) return;
          isUpdating = true;
          showError("");
          const v = parseFloat(kgcm2Input.value);
          if (isNaN(v)) {
            psiInput.value = "";
            isUpdating = false;
            return;
          }
          psiInput.value = formatNumber(v * KGCM2_TO_PSI, 6);
          isUpdating = false;
        }

        function handleInput(e) {
          const value = e.target.value;
          if (value && !/^[-+]?\d*\.?\d*$/.test(value)) {
            showError("Please enter a valid numeric value.");
            return;
          }
          showError("");
          if (psiIsSource) {
            convertFromPsi();
          } else {
            convertFromKgcm2();
          }
        }

        function swapDirection() {
          psiIsSource = !psiIsSource;
          if (psiIsSource) {
            swapBtn.textContent = "Swap directions";
            precisionNote.textContent =
              "Using factor 1 psi = 0.07030697 kg/cm²";
          } else {
            swapBtn.textContent = "Swap directions";
            precisionNote.textContent =
              "Using factor 1 kg/cm² = 14.2233433 psi";
          }
          if (psiIsSource) {
            convertFromPsi();
          } else {
            convertFromKgcm2();
          }
        }

        function clearFields() {
          psiInput.value = "";
          kgcm2Input.value = "";
          showError("");
        }

        function buildTable() {
          if (!tableBody) return;
          const rows = [];
          const values = [
            1, 2, 5, 10, 20, 30, 40, 50, 75, 100, 150, 200, 250, 300, 500, 1000,
          ];
          values.forEach((psi) => {
            const kgcm2 = psi * PSI_TO_KGCM2;
            rows.push(
              "<tr>" +
                '<td class="px-3 py-1.5 text-gray-800">' +
                psi +
                "</td>" +
                '<td class="px-3 py-1.5 text-gray-800">' +
                formatNumber(kgcm2, 4) +
                "</td>" +
                "</tr>"
            );
          });
          tableBody.innerHTML = rows.join("");
        }

        if (psiInput && kgcm2Input) {
          psiInput.addEventListener("input", function (e) {
            psiIsSource = true;
            handleInput(e);
          });
          kgcm2Input.addEventListener("input", function (e) {
            psiIsSource = false;
            handleInput(e);
          });
        }

        if (swapBtn) swapBtn.addEventListener("click", swapDirection);
        if (clearBtn) clearBtn.addEventListener("click", clearFields);

        buildTable();
      })();
    </script>

