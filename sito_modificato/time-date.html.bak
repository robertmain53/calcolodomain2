    <script>
      // Tabs
      document.querySelectorAll(".tab-btn").forEach((btn) => {
        btn.addEventListener("click", () => {
          const target = btn.getAttribute("data-tab");
          document
            .querySelectorAll(".tab-btn")
            .forEach((b) => b.classList.remove("tab-active"));
          document
            .querySelectorAll(".tab-panel")
            .forEach((p) => p.classList.add("hidden"));
          btn.classList.add("tab-active");
          document.getElementById(target).classList.remove("hidden");
        });
      });

      // World clock
      const localTimeEl = document.getElementById("local-time");
      const localDateEl = document.getElementById("local-date");
      const localTzLabel = document.getElementById("local-tz-label");
      const cityListEl = document.getElementById("city-list");
      const citySelectEl = document.getElementById("city-select");

      function getTimeZoneName() {
        try {
          return (
            Intl.DateTimeFormat().resolvedOptions().timeZone || "Local time"
          );
        } catch {
          return "Local time";
        }
      }

      localTzLabel.textContent = getTimeZoneName();

      function formatTime(d) {
        return d.toLocaleTimeString([], {
          hour: "2-digit",
          minute: "2-digit",
          second: "2-digit",
        });
      }
      function formatDate(d) {
        return d.toLocaleDateString([], {
          weekday: "short",
          year: "numeric",
          month: "short",
          day: "numeric",
        });
      }

      function updateLocalClock() {
        const now = new Date();
        localTimeEl.textContent = formatTime(now);
        localDateEl.textContent = formatDate(now);
        // update city clocks
        document.querySelectorAll("[data-city-tz]").forEach((row) => {
          const tz = row.getAttribute("data-city-tz");
          try {
            const fmt = new Intl.DateTimeFormat([], {
              hour: "2-digit",
              minute: "2-digit",
              second: "2-digit",
              timeZone: tz,
            });
            row.querySelector(".city-time").textContent = fmt.format(now);
          } catch {
            row.querySelector(".city-time").textContent = "--:--:--";
          }
        });
      }
      setInterval(updateLocalClock, 1000);
      updateLocalClock();

      citySelectEl.addEventListener("change", () => {
        const tz = citySelectEl.value;
        if (!tz) return;
        const label =
          citySelectEl.options[citySelectEl.selectedIndex].textContent;
        // avoid duplicates
        if (cityListEl.querySelector('[data-city-tz="' + tz + '"]')) return;
        const div = document.createElement("div");
        div.className =
          "flex items-center justify-between border rounded px-2 py-1 bg-white";
        div.setAttribute("data-city-tz", tz);
        div.innerHTML =
          '<div><div class="font-medium">' +
          label +
          '</div><div class="text-xs text-gray-500">' +
          tz +
          '</div></div><div class="flex items-center gap-2"><span class="city-time font-mono text-sm">--:--:--</span><button type="button" class="text-xs text-red-500 hover:underline">Remove</button></div>';
        div
          .querySelector("button")
          .addEventListener("click", () => div.remove());
        cityListEl.appendChild(div);
        citySelectEl.value = "";
        updateLocalClock();
      });

      // Time zone list
      const tzFrom = document.getElementById("tz-from");
      const tzTo = document.getElementById("tz-to");
      const tzResult = document.getElementById("tz-result");
      const tzResultText = document.getElementById("tz-result-text");

      const commonTimeZones = [
        "UTC",
        "Europe/London",
        "Europe/Berlin",
        "Europe/Rome",
        "Europe/Paris",
        "America/New_York",
        "America/Chicago",
        "America/Denver",
        "America/Los_Angeles",
        "America/Sao_Paulo",
        "Asia/Tokyo",
        "Asia/Shanghai",
        "Asia/Kolkata",
        "Australia/Sydney",
      ];

      function populateTzSelect(sel) {
        commonTimeZones.forEach((tz) => {
          const opt = document.createElement("option");
          opt.value = tz;
          opt.textContent = tz;
          sel.appendChild(opt);
        });
        try {
          const localTz = Intl.DateTimeFormat().resolvedOptions().timeZone;
          if (localTz && !commonTimeZones.includes(localTz)) {
            const opt = document.createElement("option");
            opt.value = localTz;
            opt.textContent = localTz + " (local)";
            sel.insertBefore(opt, sel.firstChild);
          }
        } catch {}
      }
      populateTzSelect(tzFrom);
      populateTzSelect(tzTo);
      tzFrom.selectedIndex = 0;
      tzTo.selectedIndex = 1;

      document
        .getElementById("btn-convert-tz")
        .addEventListener("click", () => {
          const fromTz = tzFrom.value;
          const toTz = tzTo.value;
          const dateStr = document.getElementById("tz-from-date").value;
          const timeStr =
            document.getElementById("tz-from-time").value || "00:00:00";
          if (!fromTz || !toTz || !dateStr) {
            alert("Please select both time zones and a date.");
            return;
          }
          const [h, m, s] = timeStr
            .split(":")
            .map((v) => parseInt(v || "0", 10));
          const [y, mo, d] = dateStr.split("-").map((v) => parseInt(v, 10));
          // Create date in source TZ by interpreting components as that TZ
          const src = new Date(Date.UTC(y, mo - 1, d, h, m, s || 0));
          // Adjust for source TZ offset at that moment
          try {
            const fmt = new Intl.DateTimeFormat("en-US", {
              timeZone: fromTz,
              hour12: false,
              year: "numeric",
              month: "2-digit",
              day: "2-digit",
              hour: "2-digit",
              minute: "2-digit",
              second: "2-digit",
            });
            const parts = fmt.formatToParts(src);
            const map = {};
            parts.forEach((p) => {
              if (p.type !== "literal") map[p.type] = p.value;
            });
            const localLike = new Date(
              parseInt(map.year, 10),
              parseInt(map.month, 10) - 1,
              parseInt(map.day, 10),
              parseInt(map.hour, 10),
              parseInt(map.minute, 10),
              parseInt(map.second, 10)
            );
            // Now format in target TZ
            const outFmt = new Intl.DateTimeFormat([], {
              timeZone: toTz,
              year: "numeric",
              month: "short",
              day: "2-digit",
              weekday: "short",
              hour: "2-digit",
              minute: "2-digit",
              second: "2-digit",
            });
            tzResultText.textContent =
              outFmt.format(localLike) + " (" + toTz + ")";
            tzResult.classList.remove("hidden");
          } catch (e) {
            alert("Time zone conversion is not supported in this browser.");
          }
        });

      // Date difference
      function parseDateInput(id) {
        const v = document.getElementById(id).value;
        if (!v) return null;
        const [y, m, d] = v.split("-").map((n) => parseInt(n, 10));
        return new Date(y, m - 1, d);
      }

      document.getElementById("btn-diff").addEventListener("click", () => {
        const start = parseDateInput("diff-start");
        const end = parseDateInput("diff-end");
        const includeEnd = document.getElementById("diff-include-end").checked;
        if (!start || !end) {
          alert("Please choose both start and end dates.");
          return;
        }
        const msPerDay = 24 * 60 * 60 * 1000;
        const diffMs = end.getTime() - start.getTime();
        let days = Math.round(diffMs / msPerDay);
        if (includeEnd) days += diffMs >= 0 ? 1 : -1;
        const absDays = Math.abs(days);
        const weeks = absDays / 7;
        const months = absDays / 30.4375;
        const years = absDays / 365.25;

        document.getElementById("diff-days").textContent =
          days + " day" + (Math.abs(days) === 1 ? "" : "s");
        document.getElementById("diff-weeks").textContent =
          weeks.toFixed(2) + " week(s) (approx.)";
        document.getElementById("diff-months").textContent =
          months.toFixed(2) + " month(s) (approx.)";
        document.getElementById("diff-years").textContent =
          years.toFixed(2) + " year(s) (approx.)";
        document.getElementById("diff-result").classList.remove("hidden");
      });

      // Add/Subtract days
      document.getElementById("btn-as").addEventListener("click", () => {
        const base = parseDateInput("as-base-date");
        const days = parseInt(document.getElementById("as-days").value, 10);
        const op = document.getElementById("as-operation").value;
        if (!base || isNaN(days)) {
          alert("Please choose a start date and enter a number of days.");
          return;
        }
        const delta = op === "add" ? days : -days;
        const result = new Date(base.getTime());
        result.setDate(result.getDate() + delta);
        document.getElementById("as-result-text").textContent =
          result.toLocaleDateString();
        document.getElementById("as-result-weekday").textContent =
          result.toLocaleDateString([], { weekday: "long" });
        document.getElementById("as-result").classList.remove("hidden");
      });

      // Weekday finder
      document.getElementById("btn-wd").addEventListener("click", () => {
        const d = parseDateInput("wd-date");
        if (!d) {
          alert("Please choose a date.");
          return;
        }
        document.getElementById("wd-result-text").textContent =
          d.toLocaleDateString([], {
            weekday: "long",
            year: "numeric",
            month: "long",
            day: "numeric",
          });
        document.getElementById("wd-result").classList.remove("hidden");
      });

      // Unix & ISO
      document
        .getElementById("btn-unix-to-date")
        .addEventListener("click", () => {
          const v = parseInt(document.getElementById("unix-input").value, 10);
          if (isNaN(v)) {
            alert("Please enter a valid Unix timestamp in seconds.");
            return;
          }
          const d = new Date(v * 1000);
          if (isNaN(d.getTime())) {
            alert("Invalid Unix timestamp.");
            return;
          }
          const localStr = d.toLocaleString();
          const utcStr = d.toUTCString();
          document.getElementById("unix-date-text").textContent =
            localStr + " (local) | " + utcStr + " (UTC)";
          document
            .getElementById("unix-date-result")
            .classList.remove("hidden");
        });

      document
        .getElementById("btn-date-to-unix")
        .addEventListener("click", () => {
          const dateStr = document.getElementById("unix-date").value;
          const timeStr =
            document.getElementById("unix-time").value || "00:00:00";
          if (!dateStr) {
            alert("Please choose a date.");
            return;
          }
          const [y, m, d] = dateStr.split("-").map((n) => parseInt(n, 10));
          const [hh, mm, ss] = timeStr
            .split(":")
            .map((n) => parseInt(n || "0", 10));
          const local = new Date(y, m - 1, d, hh, mm, ss || 0);
          if (isNaN(local.getTime())) {
            alert("Invalid date or time.");
            return;
          }
          const unix = Math.floor(local.getTime() / 1000);
          document.getElementById("date-unix-text").textContent = unix;
          document.getElementById("date-iso-local").textContent = local
            .toISOString()
            .replace("Z", "");
          document.getElementById("date-iso-utc").textContent = new Date(
            local.getTime()
          ).toISOString();
          document
            .getElementById("date-unix-result")
            .classList.remove("hidden");
        });

      document
        .querySelectorAll("#faq-container .faq-question")
        .forEach((btn) => {
          btn.addEventListener("click", () => {
            const ans = btn.parentElement.querySelector(".faq-answer");
            const isHidden = ans.classList.contains("hidden");
            ans.classList.toggle("hidden");
            btn.querySelector("span:last-child").textContent = isHidden
              ? "−"
              : "+";
          });
        });
    </script>

