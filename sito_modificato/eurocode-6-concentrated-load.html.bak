    <script>
      (function () {
        const fkdEl = document.getElementById("fkd");
        const tEl = document.getElementById("t");
        const bEl = document.getElementById("b");
        const wEffEl = document.getElementById("w_eff");
        const gammaMEl = document.getElementById("gamma_m");
        const gammaFEl = document.getElementById("gamma_f");
        const nEdCharEl = document.getElementById("n_ed");
        const etaEl = document.getElementById("eta");

        const errorMsg = document.getElementById("errorMsg");
        const resultsCard = document.getElementById("results-card");
        const nrdValueEl = document.getElementById("nrdValue");
        const nkAllowEl = document.getElementById("nkAllow");
        const aEffEl = document.getElementById("aEff");
        const nrdStepEl = document.getElementById("nrdStep");
        const utilRow = document.getElementById("utilisation-row");
        const nedDesignEl = document.getElementById("nedDesign");
        const utilPercentEl = document.getElementById("utilPercent");
        const utilStatusEl = document.getElementById("utilStatus");

        function formatNumber(val, decimals) {
          if (!isFinite(val)) return "–";
          return val.toFixed(decimals);
        }

        function calculate() {
          errorMsg.textContent = "";
          utilRow.classList.add("hidden");

          const fkd = parseFloat(fkdEl.value);
          const t = parseFloat(tEl.value);
          const b = parseFloat(bEl.value);
          const wEff = parseFloat(wEffEl.value);
          const gammaF = parseFloat(gammaFEl.value) || 1.0;
          const nK = parseFloat(nEdCharEl.value);
          const eta = parseFloat(etaEl.value) || 1.0;

          if ([fkd, t, b, wEff].some((v) => !isFinite(v) || v <= 0)) {
            errorMsg.textContent =
              "Please enter positive values for strength, thickness, bearing length and effective width.";
            resultsCard.classList.add("hidden");
            return;
          }

          if (eta < 1) {
            errorMsg.textContent = "Dispersion factor ηd should be ≥ 1.0.";
            resultsCard.classList.add("hidden");
            return;
          }

          // Effective bearing area (simplified): b * t * ηd
          const aEff = b * t * eta; // mm²

          // Design resistance in kN
          const nRd = (fkd * aEff) / 1000.0;

          // Approx allowable characteristic load
          const nkAllow = nRd / (gammaF || 1.0);

          aEffEl.textContent = formatNumber(aEff, 0);
          nrdValueEl.textContent = formatNumber(nRd, 1);
          nrdStepEl.textContent = formatNumber(nRd, 1);
          nkAllowEl.textContent = formatNumber(nkAllow, 1);

          // Utilisation if Nk provided
          if (isFinite(nK) && nK > 0) {
            const nEd = nK * gammaF;
            const util = (nEd / nRd) * 100.0;
            nedDesignEl.textContent = formatNumber(nEd, 1);
            utilPercentEl.textContent = formatNumber(util, 1);

            if (util <= 100) {
              utilStatusEl.textContent = "OK (within resistance)";
              utilStatusEl.classList.remove("text-red-700");
              utilStatusEl.classList.add("text-green-700");
            } else {
              utilStatusEl.textContent = "Not OK (exceeds resistance)";
              utilStatusEl.classList.remove("text-green-700");
              utilStatusEl.classList.add("text-red-700");
            }
            utilRow.classList.remove("hidden");
          }

          resultsCard.classList.remove("hidden");
        }

        document
          .getElementById("calculateBtn")
          .addEventListener("click", calculate);
        document
          .getElementById("resetBtn")
          .addEventListener("click", function () {
            errorMsg.textContent = "";
            resultsCard.classList.add("hidden");
            utilRow.classList.add("hidden");
          });
      })();
    </script>

