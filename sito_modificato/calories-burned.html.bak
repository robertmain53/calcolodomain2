        const customMetInput = document.getElementById("custom-met");

        const distanceActivity = document.getElementById("distance-activity");
        const distanceInput = document.getElementById("distance");
        const distanceUnit = document.getElementById("distance-unit");
        const paceMin = document.getElementById("pace-min");
        const paceSec = document.getElementById("pace-sec");
        const paceUnitLabel = document.getElementById("pace-unit-label");

        const avgHr = document.getElementById("avg-hr");
        const hrDurationMin = document.getElementById("hr-duration-min");
        const hrDurationSec = document.getElementById("hr-duration-sec");

        // Activity list with METs (simplified from Compendium)
        const activities = [
          { id: "sitting", name: "Sitting quietly", met: 1.3 },
          { id: "standing", name: "Standing (light)", met: 1.8 },
          {
            id: "walk_slow",
            name: "Walking, slow (2 mph / 3.2 km/h)",
            met: 2.8,
          },
          {
            id: "walk_brisk",
            name: "Walking, brisk (3.5 mph / 5.6 km/h)",
            met: 4.3,
          },
          { id: "walk_hill", name: "Walking, uphill", met: 6.5 },
          { id: "run_jog", name: "Jogging (5 mph / 8 km/h)", met: 8.3 },
          { id: "run_fast", name: "Running (6 mph / 9.7 km/h)", met: 9.8 },
          { id: "run_sprint", name: "Running (7.5 mph / 12 km/h)", met: 11.5 },
          {
            id: "cycle_easy",
            name: "Cycling, easy (10–12 mph / 16–19 km/h)",
            met: 6.8,
          },
          {
            id: "cycle_mod",
            name: "Cycling, moderate (12–14 mph / 19–23 km/h)",
            met: 8.0,
          },
          {
            id: "cycle_fast",
            name: "Cycling, vigorous (16–19 mph / 26–30 km/h)",
            met: 12.0,
          },
          { id: "swim_easy", name: "Swimming, easy", met: 6.0 },
          { id: "swim_fast", name: "Swimming, vigorous laps", met: 9.8 },
          { id: "elliptical", name: "Elliptical trainer", met: 5.0 },
          { id: "stair_climb", name: "Stair climbing machine", met: 8.8 },
          { id: "hiit", name: "HIIT / circuit training", met: 8.0 },
          { id: "weights_light", name: "Weight training, light", met: 3.5 },
          { id: "weights_heavy", name: "Weight training, vigorous", met: 6.0 },
          { id: "yoga", name: "Yoga (Hatha)", met: 2.5 },
          { id: "pilates", name: "Pilates", met: 3.0 },
          { id: "dance", name: "Dancing, aerobic", met: 7.3 },
          { id: "sports_soccer", name: "Soccer, casual", met: 7.0 },
          { id: "sports_basketball", name: "Basketball game", met: 8.0 },
          { id: "sports_tennis", name: "Tennis, singles", met: 7.3 },
        ];

        function populateActivities() {
          activities.forEach((act) => {
            const opt = document.createElement("option");
            opt.value = act.id;
            opt.textContent = act.name;
            activitySelect.appendChild(opt);
          });
          activitySelect.value = "walk_brisk";
          updateActivityMetLabel();
        }

        function populateActivityTable() {
          const sampleWeightKg = 70;
          activityTableBody.innerHTML = "";
          activities.slice(2, 12).forEach((act) => {
            const kcalPerMin = (act.met * 3.5 * sampleWeightKg) / 200;
            const kcalPerHour = kcalPerMin * 60;
            const tr = document.createElement("tr");
            tr.className = "activity-row";
            tr.innerHTML = `
          <td class="px-3 py-2 text-gray-800">${act.name}</td>
          <td class="px-3 py-2 text-right text-gray-700">${act.met.toFixed(
            1
          )}</td>
          <td class="px-3 py-2 text-right text-gray-700">${kcalPerHour.toFixed(
            0
          )}</td>
        `;
            activityTableBody.appendChild(tr);
          });
        }

        function updateActivityMetLabel() {
          const act = activities.find((a) => a.id === activitySelect.value);
          if (act) {
            activityMetLabel.textContent = `Typical intensity: ${act.met.toFixed(
              1
            )} METs`;
          } else {
            activityMetLabel.textContent = "";
          }
        }

        activitySelect.addEventListener("change", updateActivityMetLabel);

        // Tab switching
        function setActiveTab(tab) {
          [tabActivity, tabDistance, tabHeart].forEach((t) => {
            t.classList.remove("tab-active");
            t.classList.add("tab-inactive");
          });
          [panelActivity, panelDistance, panelHeart].forEach((p) =>
            p.classList.add("hidden")
          );

          if (tab === "activity") {
            tabActivity.classList.add("tab-active");
            tabActivity.classList.remove("tab-inactive");
            panelActivity.classList.remove("hidden");
          } else if (tab === "distance") {
            tabDistance.classList.add("tab-active");
            tabDistance.classList.remove("tab-inactive");
            panelDistance.classList.remove("hidden");
          } else if (tab === "heart") {
            tabHeart.classList.add("tab-active");
            tabHeart.classList.remove("tab-inactive");
            panelHeart.classList.remove("hidden");
          }
          errorMsg.textContent = "";
        }

        tabActivity.addEventListener("click", () => setActiveTab("activity"));
        tabDistance.addEventListener("click", () => setActiveTab("distance"));
        tabHeart.addEventListener("click", () => setActiveTab("heart"));

        // Distance unit change updates pace label
        distanceUnit.addEventListener("change", () => {
          paceUnitLabel.textContent =
            distanceUnit.value === "km" ? "per km" : "per mile";
        });

        function getWeightKg() {
          const w = parseFloat(weightInput.value);
          if (!isFinite(w) || w <= 0) return null;
          return weightUnit.value === "kg" ? w : w * 0.45359237;
        }

        function getDurationMinutes(minInput, secInput) {
          const min = parseFloat(minInput.value) || 0;
          const sec = parseFloat(secInput.value) || 0;
          const total = min + sec / 60;
          return total > 0 ? total : null;
        }

        function calcMetCalories(met, weightKg, durationMin) {
          const kcalPerMin = (met * 3.5 * weightKg) / 200;
          return { kcalPerMin, totalKcal: kcalPerMin * durationMin };
        }

        function estimateMetFromDistance(activityType, speedKph) {
          // Rough mapping based on speed
          if (activityType === "walk") {
            if (speedKph < 3) return 2.0;
            if (speedKph < 4) return 2.8;
            if (speedKph < 5) return 3.5;
            if (speedKph < 6) return 4.3;
            return 5.0;
          }
          if (activityType === "run") {
            if (speedKph < 7) return 7.0;
            if (speedKph < 9) return 8.3;
            if (speedKph < 11) return 9.8;
            if (speedKph < 13) return 11.0;
            return 12.5;
          }
          if (activityType === "cycle") {
            if (speedKph < 16) return 4.0;
            if (speedKph < 20) return 6.8;
            if (speedKph < 24) return 8.0;
            if (speedKph < 28) return 10.0;
            return 12.0;
          }
          return 4.0;
        }

        function calcDistanceMode(weightKg) {
          const dist = parseFloat(distanceInput.value);
          if (!isFinite(dist) || dist <= 0) {
            throw new Error("Please enter a valid distance.");
          }
          const distKm = distanceUnit.value === "km" ? dist : dist * 1.60934;
          const activityType = distanceActivity.value;

          let durationMin = null;
          const paceMinVal = parseFloat(paceMin.value);
          const paceSecVal = parseFloat(paceSec.value);
          if (isFinite(paceMinVal) || isFinite(paceSecVal)) {
            const paceTotalMin = (paceMinVal || 0) + (paceSecVal || 0) / 60;
            if (paceTotalMin > 0) {
              durationMin = paceTotalMin * dist;
            }
          }
          if (!durationMin) {
            // Estimate duration from typical speeds
            let speedKph;
            if (activityType === "walk") speedKph = 5;
            else if (activityType === "run") speedKph = 9;
            else speedKph = 20;
            durationMin = (distKm / speedKph) * 60;
          }

          const speedKph = distKm / (durationMin / 60);
          const met = estimateMetFromDistance(activityType, speedKph);

          const { kcalPerMin, totalKcal } = calcMetCalories(
            met,
            weightKg,
            durationMin
          );
          return { totalKcal, kcalPerMin, met, durationMin, distKm, speedKph };
        }

        function calcActivityMode(weightKg) {
          const durationMinVal = getDurationMinutes(durationMin, durationSec);
          if (!durationMinVal)
            throw new Error("Please enter a valid duration.");
          const customMet = parseFloat(customMetInput.value);
          let met;
          if (isFinite(customMet) && customMet > 0) {
            met = customMet;
          } else {
            const act = activities.find((a) => a.id === activitySelect.value);
            met = act ? act.met : 4.0;
          }
          const { kcalPerMin, totalKcal } = calcMetCalories(
            met,
            weightKg,
            durationMinVal
          );
          return { totalKcal, kcalPerMin, met, durationMin: durationMinVal };
        }

        function calcHeartMode(weightKg) {
          const hr = parseFloat(avgHr.value);
          if (!isFinite(hr) || hr <= 0)
            throw new Error("Please enter a valid average heart rate.");
          const durationMinVal = getDurationMinutes(
            hrDurationMin,
            hrDurationSec
          );
          if (!durationMinVal)
            throw new Error("Please enter a valid duration.");

          const age = parseFloat(ageInput.value);
          const sex = sexInput.value;
          let kcalPerMin;

          if (sex === "male" && isFinite(age)) {
            // Keytel male
            kcalPerMin =
              ((-55.0969 + 0.6309 * hr + 0.1988 * weightKg + 0.2017 * age) /
                4.184 /
                60) *
              60;
            // simplified to per minute already
          } else if (sex === "female" && isFinite(age)) {
            // Keytel female
            kcalPerMin =
              ((-20.4022 + 0.4472 * hr - 0.1263 * weightKg + 0.074 * age) /
                4.184 /
                60) *
              60;
          } else {
            // Fallback: estimate MET from HR reserve
            const estMaxHr = 208 - 0.7 * (isFinite(age) ? age : 40);
            const restingHr = 60;
            const hrReserve = estMaxHr - restingHr;
            const intensity = Math.max(
              0,
              Math.min(1, (hr - restingHr) / (hrReserve || 1))
            );
            const met = 1 + intensity * 11; // from 1 to ~12 METs
            const res = calcMetCalories(met, weightKg, durationMinVal);
            return {
              totalKcal: res.totalKcal,
              kcalPerMin: res.kcalPerMin,
              met,
              durationMin: durationMinVal,
              hrBased: true,
            };
          }

          if (!isFinite(kcalPerMin) || kcalPerMin <= 0) {
            throw new Error(
              "Heart rate inputs produced an invalid result. Try MET or distance mode instead."
            );
          }
          const totalKcal = kcalPerMin * durationMinVal;
          // Approximate MET from kcalPerMin
          const met = (kcalPerMin * 200) / (3.5 * weightKg);
          return {
            totalKcal,
            kcalPerMin,
            met,
            durationMin: durationMinVal,
            hrBased: true,
          };
        }

        function buildEquivalents(totalKcal) {
          const items = [];
          const sliceBread = 80; // kcal
          const banana = 105;
          const chocolateBar = 230;
          const oliveOilTbsp = 120;

          items.push(
            `${(totalKcal / sliceBread).toFixed(
              1
            )} slices of bread (~80 kcal each)`
          );
          items.push(
            `${(totalKcal / banana).toFixed(1)} medium bananas (~105 kcal each)`
          );
          items.push(
            `${(totalKcal / chocolateBar).toFixed(
              1
            )} small chocolate bars (~230 kcal each)`
          );
          items.push(
            `${(totalKcal / oliveOilTbsp).toFixed(
              1
            )} tbsp of olive oil (~120 kcal each)`
          );

          equivalentsList.innerHTML = "";
          items.forEach((text) => {
            const li = document.createElement("li");
            li.textContent = text;
            equivalentsList.appendChild(li);
          });
        }

        function formatDetails(mode, data, weightKg) {
          let html = "";
          html += `<p><strong>Mode:</strong> ${mode}</p>`;
          html += `<p><strong>Weight:</strong> ${weightKg.toFixed(1)} kg</p>`;
          html += `<p><strong>Duration:</strong> ${data.durationMin.toFixed(
            1
          )} minutes</p>`;
          if (mode === "Distance & pace") {
            html += `<p><strong>Distance:</strong> ${data.distKm.toFixed(
              2
            )} km (${(data.distKm / 1.60934).toFixed(2)} miles)</p>`;
            html += `<p><strong>Average speed:</strong> ${data.speedKph.toFixed(
              1
            )} km/h</p>`;
          }
          if (data.hrBased) {
            html += `<p><strong>Heart rate based estimate</strong> using research regression formulas.</p>`;
          }
          html += `<p class="mt-2">Calories per minute are calculated as: <code>(MET × 3.5 × weight_kg) ÷ 200</code>.</p>`;
          return html;
        }

        calcBtn.addEventListener("click", function () {
          errorMsg.textContent = "";
          let weightKg = getWeightKg();
          if (!weightKg) {
            errorMsg.textContent = "Please enter a valid body weight.";
            resultsSection.classList.add("hidden");
            return;
          }

          let mode;
          let data;
          try {
            if (!panelActivity.classList.contains("hidden")) {
              mode = "Activity & duration";
              data = calcActivityMode(weightKg);
            } else if (!panelDistance.classList.contains("hidden")) {
              mode = "Distance & pace";
              data = calcDistanceMode(weightKg);
            } else {
              mode = "Heart rate";
              data = calcHeartMode(weightKg);
            }
          } catch (e) {
            errorMsg.textContent = e.message;
            resultsSection.classList.add("hidden");
            return;
          }

          const totalKcal = Math.max(0, data.totalKcal);
          const kcalPerMin = Math.max(0, data.kcalPerMin);
          const met = Math.max(0, data.met);

          resultTotal.textContent = `${totalKcal.toFixed(0)} kcal`;
          resultPerMin.textContent = `${kcalPerMin.toFixed(1)} kcal/min`;
          resultMet.textContent = met ? met.toFixed(1) : "–";

          buildEquivalents(totalKcal);
          detailsText.innerHTML = formatDetails(mode, data, weightKg);

          resultsSection.classList.remove("hidden");
        });

        resetBtn.addEventListener("click", function () {
          weightInput.value = 70;
          weightUnit.value = "kg";
          ageInput.value = "";
          sexInput.value = "";
          durationMin.value = 30;
          durationSec.value = 0;
          customMetInput.value = "";
          distanceActivity.value = "walk";
          distanceInput.value = 5;
          distanceUnit.value = "km";
          paceMin.value = "";
          paceSec.value = "";
          avgHr.value = "";
          hrDurationMin.value = 30;
          hrDurationSec.value = 0;
          setActiveTab("activity");
          errorMsg.textContent = "";
          resultsSection.classList.add("hidden");
        });

        // Initialize
        populateActivities();
        populateActivityTable();
        setActiveTab("activity");
      });
    </script>

