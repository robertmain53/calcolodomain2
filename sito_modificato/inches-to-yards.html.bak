    <script>
      (function () {
        const inchesInput = document.getElementById("inchesInput");
        const yardsInput = document.getElementById("yardsInput");

        function roundTo(value, decimals) {
          if (!isFinite(value)) return "";
          const factor = Math.pow(10, decimals);
          return Math.round(value * factor) / factor;
        }

        function convertInchesToYards() {
          const inches = parseFloat(inchesInput.value);
          if (isNaN(inches)) {
            yardsInput.value = "";
            return;
          }
          const yards = inches / 36;
          yardsInput.value = roundTo(yards, 6);
        }

        function convertYardsToInches() {
          const yards = parseFloat(yardsInput.value);
          if (isNaN(yards)) {
            inchesInput.value = "";
            return;
          }
          const inches = yards * 36;
          inchesInput.value = roundTo(inches, 6);
        }

        if (inchesInput && yardsInput) {
          inchesInput.addEventListener("input", function () {
            convertInchesToYards();
          });

          yardsInput.addEventListener("input", function () {
            convertYardsToInches();
          });
        }
      })();
    </script>

