    <!-- Data transfer rate converter logic -->
    <script>
      (function () {
        const rateUnits = {
          /* base: bits per second */
          bps: { label: "bits per second (bps)", factor: 1 },
          kbps: { label: "kilobits per second (kbps, 10^3)", factor: 1e3 },
          mbps: { label: "megabits per second (Mbps, 10^6)", factor: 1e6 },
          gbps: { label: "gigabits per second (Gbps, 10^9)", factor: 1e9 },
          tbps: { label: "terabits per second (Tbps, 10^12)", factor: 1e12 },

          kibps: {
            label: "kibibits per second (Kib/s, 2^10)",
            factor: Math.pow(2, 10),
          },
          mibps: {
            label: "mebibits per second (Mib/s, 2^20)",
            factor: Math.pow(2, 20),
          },
          gibps: {
            label: "gibibits per second (Gib/s, 2^30)",
            factor: Math.pow(2, 30),
          },
          tibps: {
            label: "tebibits per second (Tib/s, 2^40)",
            factor: Math.pow(2, 40),
          },

          Bps: { label: "bytes per second (B/s)", factor: 8 },
          KBps: { label: "kilobytes per second (KB/s, 10^3)", factor: 8e3 },
          MBps: { label: "megabytes per second (MB/s, 10^6)", factor: 8e6 },
          GBps: { label: "gigabytes per second (GB/s, 10^9)", factor: 8e9 },
          TBps: { label: "terabytes per second (TB/s, 10^12)", factor: 8e12 },

          KiBps: {
            label: "kibibytes per second (KiB/s, 2^10)",
            factor: 8 * Math.pow(2, 10),
          },
          MiBps: {
            label: "mebibytes per second (MiB/s, 2^20)",
            factor: 8 * Math.pow(2, 20),
          },
          GiBps: {
            label: "gibibytes per second (GiB/s, 2^30)",
            factor: 8 * Math.pow(2, 30),
          },
          TiBps: {
            label: "tebibytes per second (TiB/s, 2^40)",
            factor: 8 * Math.pow(2, 40),
          },
        };

        const rateUnitOrder = [
          {
            group: "Bits per second (decimal)",
            units: ["bps", "kbps", "mbps", "gbps", "tbps"],
          },
          {
            group: "Bits per second (binary)",
            units: ["kibps", "mibps", "gibps", "tibps"],
          },
          {
            group: "Bytes per second (decimal)",
            units: ["Bps", "KBps", "MBps", "GBps", "TBps"],
          },
          {
            group: "Bytes per second (binary)",
            units: ["KiBps", "MiBps", "GiBps", "TiBps"],
          },
        ];

        const fileSizeUnits = {
          B: 1,
          KB: 1e3,
          MB: 1e6,
          GB: 1e9,
          TB: 1e12,
          KiB: Math.pow(2, 10),
          MiB: Math.pow(2, 20),
          GiB: Math.pow(2, 30),
          TiB: Math.pow(2, 40),
        };

        const rateValueInput = document.getElementById("rate-value");
        const rateFromSelect = document.getElementById("rate-from");
        const rateToSelect = document.getElementById("rate-to");
        const rateConvertBtn = document.getElementById("rate-convert-btn");
        const rateResultMain = document.getElementById("rate-result-main");
        const rateResultAlt = document.getElementById("rate-result-alt");
        const rateResultBps = document.getElementById("rate-result-bps");
        const rateResultBpsBytes = document.getElementById("rate-result-Bps");

        const fileSizeValueInput = document.getElementById("file-size-value");
        const fileSizeUnitSelect = document.getElementById("file-size-unit");
        const useMainRateCheckbox =
          document.getElementById("time-use-main-rate");
        const timeRateValueInput = document.getElementById("time-rate-value");
        const timeRateUnitSelect = document.getElementById("time-rate-unit");
        const timeCustomWrapper = document.getElementById(
          "time-custom-rate-wrapper"
        );
        const timeCalcBtn = document.getElementById("time-calc-btn");
        const timeResultMain = document.getElementById("time-result-main");
        const timeResultDetail = document.getElementById("time-result-detail");
        const timeResultData = document.getElementById("time-result-data");
        const timeResultRate = document.getElementById("time-result-rate");
        const timeResultSeconds = document.getElementById(
          "time-result-seconds"
        );

        function populateRateSelect(selectEl, defaultUnit) {
          rateUnitOrder.forEach((group) => {
            const optgroup = document.createElement("optgroup");
            optgroup.label = group.group;
            group.units.forEach((u) => {
              const opt = document.createElement("option");
              opt.value = u;
              opt.textContent = rateUnits[u].label;
              if (u === defaultUnit) opt.selected = true;
              optgroup.appendChild(opt);
            });
            selectEl.appendChild(optgroup);
          });
        }

        function formatNumber(value, decimals) {
          if (!isFinite(value)) return "—";
          return value.toLocaleString(undefined, {
            maximumFractionDigits: decimals,
            minimumFractionDigits: 0,
          });
        }

        function formatBps(bps) {
          if (!isFinite(bps) || bps <= 0) return "—";
          const units = ["bps", "Kbps", "Mbps", "Gbps", "Tbps"];
          let index = 0;
          let val = bps;
          while (val >= 1000 && index < units.length - 1) {
            val / 1000;
            index++;
          }
          return formatNumber(val, 3) + " " + units[index];
        }

        function formatBpsToBytesPerSec(bps) {
          if (!isFinite(bps) || bps <= 0) return "—";
          const Bps = bps / 8;
          const units = ["B/s", "KB/s", "MB/s", "GB/s", "TB/s"];
          let index = 0;
          let val = Bps;
          while (val >= 1000 && index < units.length - 1) {
            val / 1000;
            index++;
          }
          return formatNumber(val, 3) + " " + units[index];
        }

        function convertRate() {
          const v = parseFloat(rateValueInput.value);
          const from = rateFromSelect.value;
          const to = rateToSelect.value;

          if (isNaN(v)) {
            rateResultMain.textContent =
              "Please enter a numeric data rate value.";
            rateResultAlt.textContent = "";
            rateResultBps.textContent = "—";
            rateResultBpsBytes.textContent = "—";
            return;
          }

          const fromFactor = rateUnits[from].factor;
          const toFactor = rateUnits[to].factor;

          const bps = v * fromFactor;
          const converted = bps / toFactor;

          rateResultMain.textContent =
            formatNumber(v, 6) + " " + rateUnits[from].label + " =";
          rateResultAlt.textContent =
            formatNumber(converted, 6) + " " + rateUnits[to].label;
          rateResultBps.textContent = formatNumber(bps, 6) + " bps";
          rateResultBpsBytes.textContent = formatBpsToBytesPerSec(bps);
        }

        function toggleCustomRate() {
          const useMain = useMainRateCheckbox.checked;
          if (useMain) {
            timeCustomWrapper.classList.add("opacity-40");
            timeCustomWrapper.classList.add("pointer-events-none");
          } else {
            timeCustomWrapper.classList.remove("opacity-40");
            timeCustomWrapper.classList.remove("pointer-events-none");
          }
        }

        function formatDuration(seconds) {
          if (!isFinite(seconds) || seconds <= 0) return "0 seconds";

          let s = Math.round(seconds);
          const days = Math.floor(s / 86400);
          s %= 86400;
          const hours = Math.floor(s / 3600);
          s %= 3600;
          const minutes = Math.floor(s / 60);
          s %= 60;

          const parts = [];
          if (days) parts.push(days + " day" + (days !== 1 ? "s" : ""));
          if (hours) parts.push(hours + " hour" + (hours !== 1 ? "s" : ""));
          if (minutes)
            parts.push(minutes + " minute" + (minutes !== 1 ? "s" : ""));
          if (s || parts.length === 0)
            parts.push(s + " second" + (s !== 1 ? "s" : ""));
          return parts.join(", ");
        }

        function estimateTime() {
          const fileVal = parseFloat(fileSizeValueInput.value);
          const fileUnit = fileSizeUnitSelect.value;

          if (isNaN(fileVal) || fileVal <= 0) {
            timeResultMain.textContent = "Please enter a positive file size.";
            timeResultDetail.textContent = "";
            timeResultData.textContent = "—";
            timeResultRate.textContent = "—";
            timeResultSeconds.textContent = "—";
            return;
          }

          const bytes = fileVal * fileSizeUnits[fileUnit];
          const bitsTotal = bytes * 8;

          let bps;
          let rateLabel;
          if (useMainRateCheckbox.checked) {
            const v = parseFloat(rateValueInput.value);
            if (isNaN(v) || v <= 0) {
              timeResultMain.textContent =
                "Please enter a valid data rate in the main converter or disable the checkbox.";
              timeResultDetail.textContent = "";
              timeResultData.textContent = "—";
              timeResultRate.textContent = "—";
              timeResultSeconds.textContent = "—";
              return;
            }
            const fromUnit = rateFromSelect.value;
            bps = v * rateUnits[fromUnit].factor;
            rateLabel = formatNumber(v, 6) + " " + rateUnits[fromUnit].label;
          } else {
            const v = parseFloat(timeRateValueInput.value);
            const u = timeRateUnitSelect.value;
            if (isNaN(v) || v <= 0) {
              timeResultMain.textContent =
                "Please enter a positive custom link speed.";
              timeResultDetail.textContent = "";
              timeResultData.textContent = "—";
              timeResultRate.textContent = "—";
              timeResultSeconds.textContent = "—";
              return;
            }
            bps = v * rateUnits[u].factor;
            rateLabel = formatNumber(v, 6) + " " + rateUnits[u].label;
          }

          const seconds = bitsTotal / bps;

          timeResultMain.textContent = formatDuration(seconds);
          timeResultDetail.textContent =
            "Idealised transfer time (no protocol overhead or congestion).";
          timeResultData.textContent =
            formatNumber(bytes, 3) +
            " bytes (" +
            formatNumber(bitsTotal, 3) +
            " bits)";
          timeResultRate.textContent = rateLabel + " ≈ " + formatBps(bps);
          timeResultSeconds.textContent = formatNumber(seconds, 3) + " s";
        }

        populateRateSelect(rateFromSelect, "mbps");
        populateRateSelect(rateToSelect, "MBps");
        populateRateSelect(timeRateUnitSelect, "mbps");

        rateConvertBtn.addEventListener("click", convertRate);
        rateValueInput.addEventListener("keyup", function (e) {
          if (e.key === "Enter") convertRate();
        });
        rateFromSelect.addEventListener("change", convertRate);
        rateToSelect.addEventListener("change", convertRate);

        useMainRateCheckbox.addEventListener("change", toggleCustomRate);
        timeCalcBtn.addEventListener("click", estimateTime);

        toggleCustomRate();
      })();
    </script>
  
