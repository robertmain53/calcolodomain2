    <script>
      // Quick Trip & Fuel Planner logic
      function toNumber(value) {
        const n = parseFloat(value);
        return isNaN(n) ? null : n;
      }

      document
        .getElementById("calcPlannerBtn")
        .addEventListener("click", function () {
          const distance = toNumber(document.getElementById("distance").value);
          const distanceUnit = document.getElementById("distanceUnit").value;
          const consumption = toNumber(
            document.getElementById("consumption").value
          );
          const consumptionUnit =
            document.getElementById("consumptionUnit").value;
          const fuelPrice = toNumber(
            document.getElementById("fuelPrice").value
          );
          const fuelPriceUnit = document.getElementById("fuelPriceUnit").value;

          const resultsEl = document.getElementById("planner-results");
          const fuelUsedEl = document.getElementById("plannerFuelUsed");
          const fuelCostEl = document.getElementById("plannerFuelCost");
          const costPerUnitEl = document.getElementById("plannerCostPerUnit");

          if (
            distance === null ||
            distance <= 0 ||
            consumption === null ||
            consumption <= 0 ||
            fuelPrice === null ||
            fuelPrice <= 0
          ) {
            resultsEl.classList.remove("hidden");
            fuelUsedEl.textContent = "—";
            fuelCostEl.textContent =
              "Please enter positive values for distance, efficiency and fuel price.";
            costPerUnitEl.textContent = "—";
            return;
          }

          // Normalize distance to kilometers
          let distanceKm =
            distanceUnit === "mi" ? distance * 1.609344 : distance;

          // Convert efficiency to L/100km
          let lPer100km;
          if (consumptionUnit === "l_100km") {
            lPer100km = consumption;
          } else if (consumptionUnit === "mpg_us") {
            lPer100km = 235.215 / consumption;
          } else {
            // mpg_uk
            lPer100km = 282.481 / consumption;
          }

          // Fuel used in liters
          const fuelLiters = (distanceKm / 100) * lPer100km;

          // Convert fuel price to price per liter
          let pricePerLiter;
          if (fuelPriceUnit === "per_liter") {
            pricePerLiter = fuelPrice;
          } else if (fuelPriceUnit === "per_gallon_us") {
            pricePerLiter = fuelPrice / 3.785411784;
          } else {
            // per_gallon_uk
            pricePerLiter = fuelPrice / 4.54609;
          }

          const totalCost = fuelLiters * pricePerLiter;

          // Cost per 100 km and per 100 mi
          const costPer100km = lPer100km * pricePerLiter;
          const costPer100mi = costPer100km * (160.9344 / 100); // 100 miles ≈ 160.9344 km

          resultsEl.classList.remove("hidden");
          fuelUsedEl.textContent = fuelLiters.toFixed(2) + " L";
          fuelCostEl.textContent =
            totalCost.toFixed(2) + " (same currency as input)";
          costPerUnitEl.textContent =
            costPer100km.toFixed(2) +
            " /100 km · " +
            costPer100mi.toFixed(2) +
            " /100 mi";
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
