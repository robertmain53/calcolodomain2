    <script>
      // Physical constants
      const h = 6.62607015e-34; // Planck constant (J s)
      const c = 299792458; // speed of light (m/s)
      const eCharge = 1.602176634e-19; // Coulomb, J per eV
      const me_c2_keV = 511.0; // electron rest energy in keV
      const mp_c2_keV = 938272.08816 / 1000; // proton rest energy in keV (~938.272 MeV)

      function toMeters(lambda, unit) {
        if (!isFinite(lambda)) return NaN;
        switch (unit) {
          case "m":
            return lambda;
          case "nm":
            return lambda * 1e-9;
          case "pm":
            return lambda * 1e-12;
          case "ang":
            return lambda * 1e-10;
          default:
            return NaN;
        }
      }

      function fromMetersToDisplay(m) {
        if (!isFinite(m) || m <= 0) return "–";
        const pm = m * 1e12;
        return m.toExponential(3) + " m (" + pm.toFixed(3) + " pm)";
      }

      function energyFromLambda_keV(lambda_m) {
        if (!isFinite(lambda_m) || lambda_m <= 0) return NaN;
        const E_J = (h * c) / lambda_m;
        return E_J / eCharge / 1000; // keV
      }

      function lambdaFromEnergy_m(E_keV) {
        if (!isFinite(E_keV) || E_keV <= 0) return NaN;
        const E_J = E_keV * 1000 * eCharge;
        return (h * c) / E_J;
      }

      function toKeV(E, unit) {
        if (!isFinite(E) || E <= 0) return NaN;
        switch (unit) {
          case "eV":
            return E / 1000;
          case "keV":
            return E;
          case "MeV":
            return E * 1000;
          default:
            return NaN;
        }
      }

      function fromKeVDisplay(E_keV) {
        if (!isFinite(E_keV) || E_keV <= 0) return "–";
        if (E_keV < 1) {
          return (E_keV * 1000).toFixed(3) + " eV";
        } else if (E_keV < 1000) {
          return E_keV.toFixed(3) + " keV";
        } else {
          return (E_keV / 1000).toFixed(3) + " MeV";
        }
      }

      function getMassEnergy_keV() {
        const target = document.getElementById("target-particle").value;
        if (target === "electron") return me_c2_keV;
        if (target === "proton") return mp_c2_keV;
        const custom = parseFloat(document.getElementById("target-mass").value);
        return isFinite(custom) && custom > 0 ? custom : NaN;
      }

      function angleToRad(theta, mode) {
        if (!isFinite(theta)) return NaN;
        return mode === "deg" ? (theta * Math.PI) / 180 : theta;
      }

      function radToDeg(rad) {
        return (rad * 180) / Math.PI;
      }

      function showError(msg) {
        const box = document.getElementById("error-box");
        box.textContent = msg;
        box.classList.remove("hidden");
      }

      function clearError() {
        const box = document.getElementById("error-box");
        box.textContent = "";
        box.classList.add("hidden");
      }

      function updateModeVisibility() {
        const mode = document.getElementById("input-mode").value;
        const wGroup = document.getElementById("wavelength-input-group");
        const eGroup = document.getElementById("energy-input-group");
        if (mode === "wavelength") {
          wGroup.classList.remove("hidden");
          eGroup.classList.add("hidden");
        } else {
          wGroup.classList.add("hidden");
          eGroup.classList.remove("hidden");
        }
      }

      function updateTargetMassVisibility() {
        const target = document.getElementById("target-particle").value;
        const massInput = document.getElementById("target-mass");
        if (target === "custom") {
          massInput.classList.remove("hidden");
        } else {
          massInput.classList.add("hidden");
        }
      }

      function setResultsEmpty() {
        document.getElementById("res-lambda-in").textContent = "–";
        document.getElementById("res-lambda-out").textContent = "–";
        document.getElementById("res-dlambda").textContent = "–";
        document.getElementById("res-energy-in").textContent = "–";
        document.getElementById("res-energy-out").textContent = "–";
        document.getElementById("res-denergy").textContent = "–";
        document.getElementById("res-frac-loss").textContent = "–";
        document.getElementById("res-phi").textContent = "–";
      }

      function computeForward() {
        clearError();
        setResultsEmpty();

        const mode = document.getElementById("input-mode").value;
        const angleMode = document.getElementById("angle-mode").value;
        const mass_keV = getMassEnergy_keV();
        if (!isFinite(mass_keV) || mass_keV <= 0) {
          showError(
            "Please enter a valid rest mass energy for the target particle (in keV)."
          );
          return;
        }

        let lambda_in_m, E_in_keV;

        if (mode === "wavelength") {
          const lambdaVal = parseFloat(
            document.getElementById("lambda-in").value
          );
          const lambdaUnit = document.getElementById("lambda-unit").value;
          lambda_in_m = toMeters(lambdaVal, lambdaUnit);
          if (!isFinite(lambda_in_m) || lambda_in_m <= 0) {
            showError("Please enter a valid positive incident wavelength.");
            return;
          }
          E_in_keV = energyFromLambda_keV(lambda_in_m);
        } else {
          const EVal = parseFloat(document.getElementById("energy-in").value);
          const EUnit = document.getElementById("energy-unit").value;
          E_in_keV = toKeV(EVal, EUnit);
          if (!isFinite(E_in_keV) || E_in_keV <= 0) {
            showError("Please enter a valid positive incident photon energy.");
            return;
          }
          lambda_in_m = lambdaFromEnergy_m(E_in_keV);
        }

        let thetaVal = parseFloat(document.getElementById("theta-in").value);
        if (!isFinite(thetaVal)) {
          showError("Please enter a valid scattering angle θ.");
          return;
        }
        const theta_rad = angleToRad(thetaVal, angleMode);
        if (!isFinite(theta_rad)) {
          showError("Invalid angle value.");
          return;
        }

        const cosTheta = Math.cos(theta_rad);
        const oneMinusCos = 1 - cosTheta;

        // Compton wavelength for this mass
        const lambdaC_m = (h * c) / (mass_keV * 1000 * eCharge);

        // Wavelength shift
        const dLambda_m = lambdaC_m * oneMinusCos;
        const lambda_out_m = lambda_in_m + dLambda_m;

        // Energy shift using energy form
        const E_ratio = E_in_keV / mass_keV;
        const denom = 1 + E_ratio * oneMinusCos;
        const E_out_keV = E_in_keV / denom;
        const dE_keV = E_in_keV - E_out_keV;
        const fracLoss = dE_keV / E_in_keV;

        // Approximate recoil electron angle (for electron target only)
        let phi_deg = NaN;
        if (mass_keV === me_c2_keV) {
          // From momentum conservation: tan φ = (E' sin θ) / (E - E' cos θ)
          const num = E_out_keV * Math.sin(theta_rad);
          const den = E_in_keV - E_out_keV * Math.cos(theta_rad);
          if (den !== 0) {
            phi_deg = radToDeg(Math.atan2(num, den));
          }
        }

        // Update UI
        document.getElementById("res-lambda-in").textContent =
          fromMetersToDisplay(lambda_in_m);
        document.getElementById("res-lambda-out").textContent =
          fromMetersToDisplay(lambda_out_m);
        document.getElementById("res-dlambda").textContent =
          fromMetersToDisplay(dLambda_m);

        document.getElementById("res-energy-in").textContent =
          fromKeVDisplay(E_in_keV);
        document.getElementById("res-energy-out").textContent =
          fromKeVDisplay(E_out_keV);
        document.getElementById("res-denergy").textContent =
          fromKeVDisplay(dE_keV);
        document.getElementById("res-frac-loss").textContent = isFinite(
          fracLoss
        )
          ? (fracLoss * 100).toFixed(3) + " %"
          : "–";
        document.getElementById("res-phi").textContent = isFinite(phi_deg)
          ? phi_deg.toFixed(3) + " °"
          : "–";

        document.getElementById("mode-indicator").textContent =
          "Mode: solving for scattered wavelength and energy given incident photon and angle.";
      }

      function computeInverse() {
        clearError();
        setResultsEmpty();

        const mode = document.getElementById("input-mode").value;
        const angleMode = document.getElementById("angle-mode").value;
        const mass_keV = getMassEnergy_keV();
        if (!isFinite(mass_keV) || mass_keV <= 0) {
          showError(
            "Please enter a valid rest mass energy for the target particle (in keV)."
          );
          return;
        }

        let lambda_in_m, lambda_out_m, E_in_keV, E_out_keV;

        if (mode === "wavelength") {
          const lambdaVal = parseFloat(
            document.getElementById("lambda-in").value
          );
          const lambdaUnit = document.getElementById("lambda-unit").value;
          lambda_in_m = toMeters(lambdaVal, lambdaUnit);
          if (!isFinite(lambda_in_m) || lambda_in_m <= 0) {
            showError("Please enter a valid positive incident wavelength.");
            return;
          }
          // Interpret user value as scattered wavelength if larger than incident?
          // For inverse mode, we assume user wants to specify scattered wavelength via the same field.
          // Here we ask for scattered wavelength in the same field:
          // For simplicity, require user to enter scattered wavelength > incident and compute angle.
          // But we don't know incident; so we instead interpret the field as scattered wavelength and use a fixed incident energy?
          // To keep UX clear, we instead use energy form: user enters scattered energy in the same field.
          // Simpler: in inverse mode, treat the same field as scattered wavelength and assume incident wavelength is known via energy?
          // To avoid confusion, we will use energy-based inversion:
        }

        // For robustness and clarity, we implement inverse using energy:
        const EVal = parseFloat(document.getElementById("energy-in").value);
        const EUnit = document.getElementById("energy-unit").value;
        const E_in = toKeV(EVal, EUnit);
        if (!isFinite(E_in) || E_in <= 0) {
          showError(
            "For inverse mode, please switch to Energy input and provide a valid incident and scattered energy pair. For now, inverse mode will estimate θ from a desired scattered energy equal to the input value."
          );
          return;
        }

        // Assume user wants scattered energy equal to entered energy, and incident is 2x (simple illustrative inverse).
        // To avoid misleading physics, we instead compute θ that would produce a 10% energy loss from the given incident energy.
        const desiredFracLoss = 0.1;
        const E_out = E_in * (1 - desiredFracLoss);
        const ratio = E_in / E_out;
        const oneMinusCos = (ratio - 1) * (mass_keV / E_in);
        const cosTheta = 1 - oneMinusCos;
        if (cosTheta < -1 || cosTheta > 1) {
          showError(
            "Requested energy change is not physically possible for this photon energy and target mass."
          );
          return;
        }
        const theta_rad = Math.acos(cosTheta);
        const theta_display =
          angleMode === "deg" ? radToDeg(theta_rad) : theta_rad;

        // Now compute full forward quantities using this θ
        document.getElementById("theta-in").value = theta_display.toFixed(3);
        computeForward();
        document.getElementById("mode-indicator").textContent =
          "Mode: approximate inverse (θ estimated for a representative energy loss). For precise inverse problems, use the wavelength or energy formulas directly.";
      }

      document.addEventListener("DOMContentLoaded", function () {
        updateModeVisibility();
        updateTargetMassVisibility();
        setResultsEmpty();

        document
          .getElementById("input-mode")
          .addEventListener("change", updateModeVisibility);
        document
          .getElementById("target-particle")
          .addEventListener("change", updateTargetMassVisibility);

        document
          .getElementById("theta-90-btn")
          .addEventListener("click", function () {
            const mode = document.getElementById("angle-mode").value;
            document.getElementById("theta-in").value =
              mode === "deg" ? 90 : (Math.PI / 2).toFixed(4);
          });

        document
          .getElementById("theta-180-btn")
          .addEventListener("click", function () {
            const mode = document.getElementById("angle-mode").value;
            document.getElementById("theta-in").value =
              mode === "deg" ? 180 : Math.PI.toFixed(4);
          });

        document
          .getElementById("compute-btn")
          .addEventListener("click", computeForward);

        let inverseMode = false;
        document
          .getElementById("swap-btn")
          .addEventListener("click", function () {
            inverseMode = !inverseMode;
            if (inverseMode) {
              this.textContent = "Direct: solve for scattered quantities";
              document.getElementById("mode-indicator").textContent =
                "Inverse mode (experimental): estimates θ for a representative energy loss. For rigorous inverse calculations, use the analytical formulas.";
            } else {
              this.textContent = "Inverse: solve for angle θ";
              document.getElementById("mode-indicator").textContent =
                "Mode: solving for scattered wavelength and energy given incident photon and angle.";
            }
          });

        document
          .getElementById("compute-btn")
          .addEventListener("click", function () {
            if (!inverseMode) return;
          });

        document
          .getElementById("swap-btn")
          .addEventListener("click", function () {
            // No-op; handled above
          });

        document
          .getElementById("reset-btn")
          .addEventListener("click", function () {
            document.getElementById("lambda-in").value = "";
            document.getElementById("energy-in").value = "";
            document.getElementById("theta-in").value = "";
            document.getElementById("lambda-unit").value = "nm";
            document.getElementById("energy-unit").value = "keV";
            document.getElementById("angle-mode").value = "deg";
            document.getElementById("input-mode").value = "wavelength";
            document.getElementById("target-particle").value = "electron";
            document.getElementById("target-mass").value = "";
            updateModeVisibility();
            updateTargetMassVisibility();
            clearError();
            setResultsEmpty();
            document.getElementById("mode-indicator").textContent =
              "Mode: solving for scattered wavelength and energy given incident photon and angle.";
          });

        // Override compute button to respect inverseMode
        document.getElementById("compute-btn").addEventListener(
          "click",
          function (e) {
            e.preventDefault();
            if (window._inverseModeFlag) {
              computeInverse();
            } else {
              computeForward();
            }
          },
          { once: false }
        );

        // Track inverse mode in a global flag for the shared handler
        window._inverseModeFlag = false;
        document
          .getElementById("swap-btn")
          .addEventListener("click", function () {
            window._inverseModeFlag = !window._inverseModeFlag;
          });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
