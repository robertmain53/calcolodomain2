  <script>
    // mobile menu
    const mobileToggle = document.getElementById('mobile-menu-toggle');
    const mobileMenu = document.getElementById('mobile-menu');
    if (mobileToggle && mobileMenu) {
      mobileToggle.addEventListener('click', () => {
        const isHidden = mobileMenu.classList.contains('hidden');
        mobileMenu.classList.toggle('hidden', !isHidden);
        mobileToggle.setAttribute('aria-expanded', String(isHidden));
      });
    }

    // tabs
    const viewButtons = document.querySelectorAll('[data-view]');
    const viewSections = {
      "list-view": document.getElementById('list-view'),
      "json-view": document.getElementById('json-view'),
      "matrix-view": document.getElementById('matrix-view')
    };
    viewButtons.forEach(btn => {
      btn.addEventListener('click', () => {
        viewButtons.forEach(b => b.classList.remove('active'));
        btn.classList.add('active');
        const view = btn.dataset.view;
        Object.keys(viewSections).forEach(k => {
          viewSections[k].classList.toggle('hidden', k !== view);
        });
      });
    });

    function parseEdges(raw) {
      const lines = raw.split(/\r?\n/).map(l => l.trim()).filter(l => l.length > 0);
      const edges = [];
      lines.forEach(line => {
        const parts = line.split(/[\s,;]+/).filter(p => p.length > 0);
        if (parts.length >= 2) {
          const u = parts[0];
          const v = parts[1];
          const w = parts.length >= 3 ? parseFloat(parts[2]) : null;
          edges.push({u, v, w});
        } else if (parts.length === 1) {
          // single node, no edge
          const u = parts[0];
          edges.push({u, v: null, w: null});
        }
      });
      return edges;
    }

    function buildAdjacency(edges, directed, weighted, sortNodes) {
      const adj = {};
      edges.forEach(e => {
        const u = e.u;
        const v = e.v;
        if (!adj[u]) adj[u] = [];
        if (v !== null && v !== undefined && v !== "") {
          if (weighted && !isNaN(e.w)) {
            adj[u].push({node: v, weight: e.w});
          } else {
            adj[u].push({node: v});
          }
          if (!directed) {
            if (!adj[v]) adj[v] = [];
            if (weighted && !isNaN(e.w)) {
              adj[v].push({node: u, weight: e.w});
            } else {
              adj[v].push({node: u});
            }
          }
        }
      });

      if (sortNodes) {
        const sortedKeys = Object.keys(adj).sort();
        const sortedAdj = {};
        sortedKeys.forEach(k => {
          sortedAdj[k] = adj[k].slice().sort((a, b) => a.node.localeCompare(b.node));
        });
        return sortedAdj;
      }
      return adj;
    }

    function renderAdjacency(adj, weighted) {
      const container = document.getElementById('adjacency-container');
      const keys = Object.keys(adj);
      if (keys.length === 0) {
        container.innerHTML = "<p class='text-sm text-gray-500'>No edges or nodes.</p>";
        return;
      }
      let html = "<table class='adj-table'><thead><tr><th>Node</th><th>Neighbors</th></tr></thead><tbody>";
      keys.forEach(k => {
        const neighbors = adj[k].map(n => {
          if (weighted && typeof n.weight === "number" && !isNaN(n.weight)) {
            return n.node + "(" + n.weight + ")";
          }
          return n.node;
        }).join(", ");
        html += "<tr><td>"+k+"</td><td>"+neighbors+"</td></tr>";
      });
      html += "</tbody></table>";
      container.innerHTML = html;
    }

    function renderJSON(adj, weighted) {
      const obj = {};
      Object.keys(adj).forEach(k => {
        if (weighted) {
          obj[k] = adj[k].map(n => {
            if (typeof n.weight === "number" && !isNaN(n.weight)) {
              return {node: n.node, weight: n.weight};
            }
            return {node: n.node};
          });
        } else {
          obj[k] = adj[k].map(n => n.node);
        }
      });
      document.getElementById('json-output').textContent = JSON.stringify(obj, null, 2);
    }

    function renderMatrix(adj) {
      const container = document.getElementById('matrix-container');
      const nodes = Object.keys(adj);
      if (nodes.length === 0) {
        container.innerHTML = "<p class='text-sm text-gray-500'>No nodes to display.</p>";
        return;
      }
      // also include nodes that appear only as targets
      const seen = new Set(nodes);
      nodes.forEach(n => {
        adj[n].forEach(nb => { if (!seen.has(nb.node)) seen.add(nb.node); });
      });
      const allNodes = Array.from(seen).sort();
      let html = "<table class='adj-table'><thead><tr><th></th>";
      allNodes.forEach(n => { html += "<th>"+n+"</th>"; });
      html += "</tr></thead><tbody>";
      allNodes.forEach(rowNode => {
        html += "<tr><th>"+rowNode+"</th>";
        allNodes.forEach(colNode => {
          const list = adj[rowNode] || [];
          const exists = list.some(nb => nb.node === colNode);
          html += "<td style='text-align:center'>" + (exists ? "1" : "0") + "</td>";
        });
        html += "</tr>";
      });
      html += "</tbody></table>";
      container.innerHTML = html;
    }

    document.getElementById('build-btn').addEventListener('click', () => {
      const raw = document.getElementById('edge-input').value;
      const directed = document.getElementById('directed').checked;
      const weighted = document.getElementById('weighted').checked;
      const sortNodes = document.getElementById('sort-nodes').checked;
      const errBox = document.getElementById('err-box');

      errBox.textContent = "";
      const edges = parseEdges(raw);
      if (edges.length === 0) {
        errBox.textContent = "No edges or nodes found. Please check your input.";
        return;
      }
      const adj = buildAdjacency(edges, directed, weighted, sortNodes);
      renderAdjacency(adj, weighted);
      renderJSON(adj, weighted);
      renderMatrix(adj);
    });

    // initial auto-build
    document.getElementById('build-btn').click();
  </script>

