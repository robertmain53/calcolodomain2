    <script>
      // Utility: standard normal CDF (approximation)
      function normCdf(x) {
        // Abramowitz & Stegun approximation
        var a1 = 0.254829592,
          a2 = -0.284496736,
          a3 = 1.421413741,
          a4 = -1.453152027,
          a5 = 1.061405429,
          p = 0.3275911;
        var sign = x < 0 ? -1 : 1;
        x = Math.abs(x) / Math.sqrt(2.0);
        var t = 1.0 / (1.0 + p * x);
        var y =
          1.0 -
          ((((a5 * t + a4) * t + a3) * t + a2) * t + a1) * t * Math.exp(-x * x);
        return 0.5 * (1.0 + sign * y);
      }

      // Utility: inverse standard normal CDF (Acklam approximation)
      function normInv(p) {
        if (p <= 0 || p >= 1) return NaN;
        var a = [
          -3.969683028665376e1, 2.209460984245205e2, -2.759285104469687e2,
          1.38357751867269e2, -3.066479806614716e1, 2.506628277459239,
        ];

        var b = [
          -5.447609879822406e1, 1.615858368580409e2, -1.556989798598866e2,
          6.680131188771972e1, -1.328068155288572e1,
        ];

        var c = [
          -7.784894002430293e-3, -3.223964580411365e-1, -2.400758277161838,
          -2.549732539343734, 4.374664141464968, 2.938163982698783,
        ];

        var d = [
          7.784695709041462e-3, 3.224671290700398e-1, 2.445134137142996,
          3.754408661907416,
        ];

        var plow = 0.02425;
        var phigh = 1 - plow;
        var q, r;

        if (p < plow) {
          q = Math.sqrt(-2 * Math.log(p));
          return (
            (((((c[0] * q + c[1]) * q + c[2]) * q + c[3]) * q + c[4]) * q +
              c[5]) /
            ((((d[0] * q + d[1]) * q + d[2]) * q + d[3]) * q + 1)
          );
        }
        if (phigh < p) {
          q = Math.sqrt(-2 * Math.log(1 - p));
          return (
            -(
              ((((c[0] * q + c[1]) * q + c[2]) * q + c[3]) * q + c[4]) * q +
              c[5]
            ) /
            ((((d[0] * q + d[1]) * q + d[2]) * q + d[3]) * q + 1)
          );
        }
        q = p - 0.5;
        r = q * q;
        return (
          ((((((a[0] * r + a[1]) * r + a[2]) * r + a[3]) * r + a[4]) * r +
            a[5]) *
            q) /
          (((((b[0] * r + b[1]) * r + b[2]) * r + b[3]) * r + b[4]) * r + 1)
        );
      }

      function formatNumber(x, decimals) {
        if (!isFinite(x)) return "–";
        return x.toLocaleString(undefined, {
          minimumFractionDigits: decimals,
          maximumFractionDigits: decimals,
        });
      }

      function parseDiscreteDemand(text) {
        var lines = text
          .split(/\r?\n/)
          .map(function (l) {
            return l.trim();
          })
          .filter(Boolean);
        var scenarios = [];
        lines.forEach(function (line, idx) {
          var parts = line.split(/[,;\t]+/).map(function (p) {
            return p.trim();
          });
          if (parts.length < 2) return;
          var q = parseFloat(parts[0]);
          var p = parseFloat(parts[1]);
          if (isNaN(q) || isNaN(p) || p < 0) {
            throw new Error("Invalid line " + (idx + 1) + ": '" + line + "'");
          }
          scenarios.push({ demand: q, prob: p });
        });
        if (!scenarios.length)
          throw new Error("Please enter at least one demand scenario.");
        var sumP = scenarios.reduce(function (acc, s) {
          return acc + s.prob;
        }, 0);
        if (sumP <= 0)
          throw new Error("Probabilities must sum to a positive value.");
        // normalize if not exactly 1
        scenarios.forEach(function (s) {
          s.prob = s.prob / sumP;
        });
        return scenarios;
      }

      function computeDiscreteMetrics(Q, scenarios, prices) {
        var expSales = 0,
          expLeftover = 0,
          expLost = 0;
        scenarios.forEach(function (s) {
          var d = s.demand,
            p = s.prob;
          var sales = Math.min(d, Q);
          var leftover = Math.max(Q - d, 0);
          var lost = Math.max(d - Q, 0);
          expSales += sales * p;
          expLeftover += leftover * p;
          expLost += lost * p;
        });
        var revenue = expSales * prices.selling;
        var purchaseCost = Q * prices.cost;
        var salvage = expLeftover * prices.salvage;
        var profit = revenue - purchaseCost + salvage;
        return {
          expSales: expSales,
          expLeftover: expLeftover,
          expLost: expLost,
          revenue: revenue,
          purchaseCost: purchaseCost,
          salvage: salvage,
          profit: profit,
        };
      }

      function computeNormalMetrics(Q, mu, sigma, prices) {
        if (sigma <= 0) {
          // degenerate: demand = mu
          var d = mu;
          var sales = Math.min(d, Q);
          var leftover = Math.max(Q - d, 0);
          var lost = Math.max(d - Q, 0);
          var revenue = sales * prices.selling;
          var purchaseCost = Q * prices.cost;
          var salvage = leftover * prices.salvage;
          var profit = revenue - purchaseCost + salvage;
          return {
            expSales: sales,
            expLeftover: leftover,
            expLost: lost,
            revenue: revenue,
            purchaseCost: purchaseCost,
            salvage: salvage,
            profit: profit,
          };
        }
        var z1 = (Q - mu) / sigma;
        var phi = Math.exp(-0.5 * z1 * z1) / Math.sqrt(2 * Math.PI);
        var Phi = normCdf(z1);

        // E[min(D,Q)] = mu*Phi + Q*(1-Phi) - sigma*phi
        var expSales = mu * Phi + Q * (1 - Phi) - sigma * phi;

        // E[(Q-D)^+] = (Q - mu) * Phi + sigma * phi
        var expLeftover = (Q - mu) * Phi + sigma * phi;

        // E[(D-Q)^+] = (mu - Q) * (1 - Phi) + sigma * phi
        var expLost = (mu - Q) * (1 - Phi) + sigma * phi;

        var revenue = expSales * prices.selling;
        var purchaseCost = Q * prices.cost;
        var salvage = expLeftover * prices.salvage;
        var profit = revenue - purchaseCost + salvage;

        return {
          expSales: expSales,
          expLeftover: expLeftover,
          expLost: expLost,
          revenue: revenue,
          purchaseCost: purchaseCost,
          salvage: salvage,
          profit: profit,
        };
      }

      document.addEventListener("DOMContentLoaded", function () {
        var demandRadios = document.querySelectorAll(
          'input[name="demandModel"]'
        );
        var normalInputs = document.getElementById("normalInputs");
        var discreteInputs = document.getElementById("discreteInputs");
        var errorMsg = document.getElementById("errorMsg");
        var modelBadge = document.getElementById("modelBadge");

        demandRadios.forEach(function (radio) {
          radio.addEventListener("change", function () {
            if (this.value === "normal") {
              normalInputs.classList.remove("hidden");
              discreteInputs.classList.add("hidden");
              modelBadge.textContent = "Normal";
            } else {
              normalInputs.classList.add("hidden");
              discreteInputs.classList.remove("hidden");
              modelBadge.textContent = "Discrete";
            }
            errorMsg.textContent = "";
          });
        });

        document
          .getElementById("resetBtn")
          .addEventListener("click", function () {
            document.getElementById("sellingPrice").value = 10;
            document.getElementById("purchaseCost").value = 6;
            document.getElementById("salvageValue").value = 2;
            document.getElementById("penaltyCost").value = "";
            document.getElementById("meanDemand").value = 100;
            document.getElementById("stdDemand").value = 20;
            document.getElementById("discreteDemand").value = "";
            document.querySelector(
              'input[name="demandModel"][value="normal"]'
            ).checked = true;
            normalInputs.classList.remove("hidden");
            discreteInputs.classList.add("hidden");
            modelBadge.textContent = "Normal";
            errorMsg.textContent = "";
            // Clear results
            [
              "criticalRatio",
              "optimalQ",
              "serviceLevel",
              "expSales",
              "expLeftover",
              "expLostSales",
              "expRevenue",
              "expPurchaseCost",
              "expSalvage",
              "expProfit",
            ].forEach(function (id) {
              document.getElementById(id).textContent = "–";
            });
          });

        document
          .getElementById("computeBtn")
          .addEventListener("click", function () {
            errorMsg.textContent = "";
            try {
              var p = parseFloat(document.getElementById("sellingPrice").value);
              var c = parseFloat(document.getElementById("purchaseCost").value);
              var v = parseFloat(document.getElementById("salvageValue").value);
              var penaltyStr = document
                .getElementById("penaltyCost")
                .value.trim();
              var penalty = penaltyStr === "" ? null : parseFloat(penaltyStr);

              if (
                [p, c, v].some(function (x) {
                  return isNaN(x) || x < 0;
                })
              ) {
                throw new Error(
                  "Prices and costs must be non-negative numbers."
                );
              }
              if (penaltyStr !== "" && (isNaN(penalty) || penalty < 0)) {
                throw new Error("Penalty cost must be a non-negative number.");
              }

              var Co = c - v;
              if (Co < 0) {
                throw new Error(
                  "Salvage value cannot exceed purchase cost (overage cost would be negative)."
                );
              }
              var Cu = penaltyStr === "" ? p - c : penalty;
              if (Cu <= 0) {
                throw new Error(
                  "Underage cost must be positive. Check selling price, purchase cost, or penalty."
                );
              }

              var CR = Cu / (Cu + Co);
              if (!(CR > 0 && CR < 1)) {
                throw new Error(
                  "Critical ratio must be between 0 and 1. Check your cost parameters."
                );
              }

              var demandModel = document.querySelector(
                'input[name="demandModel"]:checked'
              ).value;
              var Qstar, serviceLevel, metrics;

              if (demandModel === "normal") {
                var mu = parseFloat(
                  document.getElementById("meanDemand").value
                );
                var sigma = parseFloat(
                  document.getElementById("stdDemand").value
                );
                if (isNaN(mu) || mu < 0 || isNaN(sigma) || sigma < 0) {
                  throw new Error(
                    "Mean demand and standard deviation must be non-negative numbers."
                  );
                }
                var z = normInv(CR);
                if (!isFinite(z)) {
                  throw new Error(
                    "Could not compute normal quantile for the given critical ratio."
                  );
                }
                Qstar = mu + z * sigma;
                if (Qstar < 0) Qstar = 0;
                serviceLevel = normCdf((Qstar - mu) / (sigma || 1));
                metrics = computeNormalMetrics(Qstar, mu, sigma, {
                  selling: p,
                  cost: c,
                  salvage: v,
                });
              } else {
                var scenarios = parseDiscreteDemand(
                  document.getElementById("discreteDemand").value
                );
                // sort by demand
                scenarios.sort(function (a, b) {
                  return a.demand - b.demand;
                });
                var cum = 0,
                  chosenQ = scenarios[scenarios.length - 1].demand;
                for (var i = 0; i < scenarios.length; i++) {
                  cum += scenarios[i].prob;
                  if (cum >= CR) {
                    chosenQ = scenarios[i].demand;
                    break;
                  }
                }
                Qstar = chosenQ;
                serviceLevel = cum;
                metrics = computeDiscreteMetrics(Qstar, scenarios, {
                  selling: p,
                  cost: c,
                  salvage: v,
                });
              }

              // Update results
              document.getElementById("criticalRatio").textContent =
                formatNumber(CR, 3);
              document.getElementById("optimalQ").textContent = formatNumber(
                Qstar,
                2
              );
              document.getElementById("serviceLevel").textContent =
                formatNumber(serviceLevel, 3);

              document.getElementById("expSales").textContent = formatNumber(
                metrics.expSales,
                2
              );
              document.getElementById("expLeftover").textContent = formatNumber(
                metrics.expLeftover,
                2
              );
              document.getElementById("expLostSales").textContent =
                formatNumber(metrics.expLost, 2);
              document.getElementById("expRevenue").textContent = formatNumber(
                metrics.revenue,
                2
              );
              document.getElementById("expPurchaseCost").textContent =
                formatNumber(metrics.purchaseCost, 2);
              document.getElementById("expSalvage").textContent = formatNumber(
                metrics.salvage,
                2
              );
              document.getElementById("expProfit").textContent = formatNumber(
                metrics.profit,
                2
              );
            } catch (e) {
              console.error(e);
              errorMsg.textContent =
                e.message || "An error occurred. Please check your inputs.";
            }
          });
      });

