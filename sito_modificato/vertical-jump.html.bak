    <script>
      // Mobile menu toggle
      document
        .getElementById("mobile-menu-toggle")
        ?.addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          if (!menu) return;
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          this.setAttribute("aria-expanded", String(isHidden));
        });

      // Vertical jump calculator logic
      (function () {
        const measureRadios = document.querySelectorAll(
          'input[name="measure-mode"]'
        );
        const directGroup = document.getElementById("direct-height-group");
        const reachGroup = document.getElementById("reach-group");
        const errorMsg = document.getElementById("error-msg");

        function setMeasureMode(mode) {
          if (mode === "direct") {
            directGroup.classList.remove("hidden");
            reachGroup.classList.add("hidden");
          } else {
            directGroup.classList.add("hidden");
            reachGroup.classList.remove("hidden");
          }
          errorMsg.classList.add("hidden");
        }

        measureRadios.forEach((r) => {
          r.addEventListener("change", () => setMeasureMode(r.value));
        });

        function toNumber(id) {
          const el = document.getElementById(id);
          if (!el) return NaN;
          const v = parseFloat(el.value);
          return isNaN(v) ? NaN : v;
        }

        function showError(msg) {
          errorMsg.textContent = msg;
          errorMsg.classList.remove("hidden");
        }

        function clearError() {
          errorMsg.classList.add("hidden");
        }

        function convertLbToKg(lb) {
          return lb * 0.45359237;
        }
        function convertInToCm(inch) {
          return inch * 2.54;
        }

        function estimatePercentile(sex, age, vjCm) {
          // Very rough heuristic bands based on general adult data
          let p = 50;
          const isMale = sex === "male";

          // Age adjustment: peak around 20–29, decline after 30
          let ageFactor = 0;
          if (age < 18) ageFactor = -5;
          else if (age >= 30 && age < 40) ageFactor = -5;
          else if (age >= 40 && age < 50) ageFactor = -10;
          else if (age >= 50) ageFactor = -20;

          // Baseline thresholds (cm)
          const thresholds = isMale
            ? { low: 35, avg: 45, good: 60, exc: 75 }
            : { low: 25, avg: 35, good: 50, exc: 60 };

          if (vjCm < thresholds.low) p = 20;
          else if (vjCm < thresholds.avg) p = 40;
          else if (vjCm < thresholds.good) p = 60;
          else if (vjCm < thresholds.exc) p = 80;
          else p = 95;

          p = Math.max(1, Math.min(99, p + ageFactor));
          return p;
        }

        function labelFromPercentile(p) {
          if (p < 25) return "Below average";
          if (p < 50) return "Average";
          if (p < 75) return "Above average";
          if (p < 90) return "Excellent";
          return "Elite";
        }

        document
          .getElementById("calculate-btn")
          ?.addEventListener("click", function () {
            clearError();

            const sex = document.getElementById("sex").value;
            const age = toNumber("age");
            const weightUnit = document.getElementById("weight-unit").value;
            let bodyWeight = toNumber("body-weight");
            const measureMode =
              document.querySelector('input[name="measure-mode"]:checked')
                ?.value || "direct";

            if (!age || age < 10 || age > 80) {
              showError("Please enter a valid age between 10 and 80.");
              return;
            }
            if (!bodyWeight || bodyWeight < 20 || bodyWeight > 200) {
              showError("Please enter a realistic body weight.");
              return;
            }

            if (weightUnit === "lb") {
              bodyWeight = convertLbToKg(bodyWeight);
            }

            let vjCm;

            if (measureMode === "direct") {
              const heightUnit = document.getElementById("height-unit").value;
              let vj = toNumber("jump-height");
              if (!vj || vj <= 0) {
                showError("Please enter your vertical jump height.");
                return;
              }
              if (heightUnit === "in") vj = convertInToCm(vj);
              vjCm = vj;
            } else {
              const reachUnit = document.getElementById("reach-unit").value;
              const jumpReachUnit =
                document.getElementById("jump-reach-unit").value;
              let sr = toNumber("standing-reach");
              let jr = toNumber("jump-reach");

              if (!sr || !jr || sr <= 0 || jr <= 0) {
                showError("Please enter both standing reach and jump reach.");
                return;
              }

              if (reachUnit === "in") sr = convertInToCm(sr);
              if (jumpReachUnit === "in") jr = convertInToCm(jr);

              const diff = jr - sr;
              if (diff <= 0) {
                showError("Jump reach must be higher than standing reach.");
                return;
              }
              vjCm = diff;
              // Also update direct height field for transparency
              document.getElementById("jump-height").value = vjCm.toFixed(1);
              document.getElementById("height-unit").value = "cm";
            }

            // Sayers equation
            const peakPower = 60.7 * vjCm + 45.3 * bodyWeight - 2055;
            const powerPerKg = peakPower / bodyWeight;

            const vjIn = vjCm / 2.54;

            // Update results
            document.getElementById("result-height-main").textContent =
              vjCm.toFixed(1) + " cm";
            document.getElementById("result-height-alt").textContent =
              vjIn.toFixed(1) + " in";

            if (peakPower > 0) {
              document.getElementById("result-power-main").textContent =
                peakPower.toFixed(0) + " W";
              document.getElementById("result-power-alt").textContent =
                powerPerKg.toFixed(1) + " W/kg";
            } else {
              document.getElementById("result-power-main").textContent = "–";
              document.getElementById("result-power-alt").textContent =
                "Jump height/body weight too low for Sayers equation.";
            }

            const percentile = estimatePercentile(sex, age, vjCm);
            const label = labelFromPercentile(percentile);
            document.getElementById("result-percentile").textContent =
              "Approx. " + percentile.toFixed(0) + "th percentile";
            document.getElementById("result-percentile-desc").textContent =
              "Performance level: " +
              label +
              " for your sex and age group (approximate).";

            const badge = document.getElementById("result-level-badge");
            badge.textContent = label;
            badge.classList.remove("hidden");
            badge.classList.remove(
              "bg-gray-100",
              "text-gray-700",
              "bg-yellow-100",
              "text-yellow-800",
              "bg-green-100",
              "text-green-800",
              "bg-blue-100",
              "text-blue-800",
              "bg-purple-100",
              "text-purple-800"
            );
            if (label === "Below average")
              badge.classList.add("bg-gray-100", "text-gray-700");
            else if (label === "Average")
              badge.classList.add("bg-yellow-100", "text-yellow-800");
            else if (label === "Above average")
              badge.classList.add("bg-green-100", "text-green-800");
            else if (label === "Excellent")
              badge.classList.add("bg-blue-100", "text-blue-800");
            else badge.classList.add("bg-purple-100", "text-purple-800");
          });

        document
          .getElementById("reset-btn")
          ?.addEventListener("click", function () {
            document.getElementById("sex").value = "male";
            document.getElementById("age").value = 20;
            document.getElementById("body-weight").value = 75;
            document.getElementById("weight-unit").value = "kg";
            document.getElementById("test-type").value = "cmj";
            document.querySelector(
              'input[name="measure-mode"][value="direct"]'
            ).checked = true;
            setMeasureMode("direct");
            document.getElementById("jump-height").value = 50;
            document.getElementById("height-unit").value = "cm";
            document.getElementById("standing-reach").value = "";
            document.getElementById("jump-reach").value = "";
            document.getElementById("reach-unit").value = "cm";
            document.getElementById("jump-reach-unit").value = "cm";
            document.getElementById("result-height-main").textContent = "–";
            document.getElementById("result-height-alt").textContent =
              "Enter your data and click calculate.";
            document.getElementById("result-power-main").textContent = "–";
            document.getElementById("result-power-alt").textContent =
              "Sayers equation (W) and W/kg.";
            document.getElementById("result-percentile").textContent = "–";
            document.getElementById("result-percentile-desc").textContent =
              "Approximate percentile vs. general population of same sex and age.";
            document
              .getElementById("result-level-badge")
              .classList.add("hidden");
            clearError();
          });
      })();
    </script>

