    <script>
      // Atomic weights (u) - condensed table for common elements + full periodic subset
      // Values are approximate IUPAC standard atomic weights.
      const ATOMIC_WEIGHTS = {
        H: 1.008,
        He: 4.0026,
        Li: 6.94,
        Be: 9.0122,
        B: 10.81,
        C: 12.011,
        N: 14.007,
        O: 15.999,
        F: 18.998,
        Ne: 20.18,
