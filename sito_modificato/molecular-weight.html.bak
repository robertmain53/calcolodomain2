        Ca: 40.078,
        Sc: 44.956,
        Ti: 47.867,
        V: 50.942,
        Cr: 51.996,
        Mn: 54.938,
        Fe: 55.845,
        Co: 58.933,
        Ni: 58.693,
        Cu: 63.546,
        Zn: 65.38,
        Ga: 69.723,
        Ge: 72.63,
        As: 74.922,
        Se: 78.971,
        Br: 79.904,
        Kr: 83.798,
        Rb: 85.468,
        Sr: 87.62,
        Y: 88.906,
        Zr: 91.224,
        Nb: 92.906,
        Mo: 95.95,
        Tc: 98,
        Ru: 101.07,
        Rh: 102.91,
        Pd: 106.42,
        Ag: 107.87,
        Cd: 112.41,
        In: 114.82,
        Sn: 118.71,
        Sb: 121.76,
        Te: 127.6,
        I: 126.9,
        Xe: 131.29,
        Cs: 132.91,
        Ba: 137.33,
        La: 138.91,
        Ce: 140.12,
        Pr: 140.91,
        Nd: 144.24,
        Sm: 150.36,
        Eu: 151.96,
        Gd: 157.25,
        Tb: 158.93,
        Dy: 162.5,
        Ho: 164.93,
        Er: 167.26,
        Tm: 168.93,
        Yb: 173.05,
        Lu: 174.97,
        Hf: 178.49,
        Ta: 180.95,
        W: 183.84,
        Re: 186.21,
        Os: 190.23,
        Ir: 192.22,
        Pt: 195.08,
        Au: 196.97,
        Hg: 200.59,
        Tl: 204.38,
        Pb: 207.2,
        Bi: 208.98,
        Po: 209,
        At: 210,
        Rn: 222,
        Fr: 223,
        Ra: 226,
        Ac: 227,
        Th: 232.04,
        Pa: 231.04,
        U: 238.03,
      };

      function parseFormula(formula) {
        if (!formula || typeof formula !== "string") {
          throw new Error("Please enter a chemical formula.");
        }

        // Normalize: replace middle dot variants with standard dot
        formula = formula.replace(/·|\u00b7/g, "·").trim();

        // Split hydrates: e.g., "CuSO4·5H2O"
        const hydrateParts = formula.split("·");
        let totalCounts = {};

        hydrateParts.forEach((part) => {
          part = part.trim();
          if (!part) return;

          // If part starts with a leading coefficient (e.g., 5H2O), extract it
          let i = 0;
          let coeffStr = "";
          while (i < part.length && /[0-9]/.test(part[i])) {
            coeffStr += part[i];
            i++;
          }
          const multiplier = coeffStr ? parseInt(coeffStr, 10) : 1;
          const core = coeffStr ? part.slice(i) : part;

          const counts = parseCoreFormula(core);
          for (const el in counts) {
            totalCounts[el] = (totalCounts[el] || 0) + counts[el] * multiplier;
          }
        });

        if (Object.keys(totalCounts).length === 0) {
          throw new Error(
            "Could not parse the formula. Please check the syntax."
          );
        }

        return totalCounts;
      }

      function parseCoreFormula(formula) {
        // Replace square brackets with parentheses for parsing
        formula = formula.replace(/\[/g, "(").replace(/\]/g, ")");

        let index = 0;

        function parseGroup() {
          let counts = {};
          while (index < formula.length) {
            const char = formula[index];

            if (char === "(") {
              index++;
              const innerCounts = parseGroup();
              const mult = parseNumber();
              for (const el in innerCounts) {
                counts[el] = (counts[el] || 0) + innerCounts[el] * mult;
              }
            } else if (char === ")") {
              index++;
              break;
            } else if (/[A-Z]/.test(char) || /[0-9]/.test(char)) {
              // Could be isotope (e.g., 13C) or element
              const { element, count } = parseElementWithCount();
              counts[element] = (counts[element] || 0) + count;
            } else if (/\s/.test(char)) {
              index++;
            } else {
              throw new Error(
                `Unexpected character "${char}" at position ${index + 1}.`
              );
            }
          }
          return counts;
        }

        function parseElementWithCount() {
          // Optional leading digits for isotope mass number, e.g., 13C
          let massStr = "";
          while (index < formula.length && /[0-9]/.test(formula[index])) {
            massStr += formula[index++];
          }

          if (index >= formula.length || !/[A-Z]/.test(formula[index])) {
            throw new Error(
              `Expected element symbol at position ${index + 1}.`
            );
          }

          let symbol = formula[index++];
          if (index < formula.length && /[a-z]/.test(formula[index])) {
            symbol += formula[index++];
          }

          const count = parseNumber();

          // If isotope mass specified, we treat element as normal symbol but override atomic weight later
          if (massStr) {
            const massNumber = parseInt(massStr, 10);
            if (!ATOMIC_WEIGHTS[symbol]) {
              throw new Error(`Unknown element symbol "${symbol}".`);
            }
            return { element: `${massNumber}${symbol}`, count };
          }

          if (!ATOMIC_WEIGHTS[symbol]) {
            throw new Error(`Unknown element symbol "${symbol}".`);
          }

          return { element: symbol, count };
        }

        function parseNumber() {
          let numStr = "";
          while (index < formula.length && /[0-9]/.test(formula[index])) {
            numStr += formula[index++];
          }
          return numStr ? parseInt(numStr, 10) : 1;
        }

        const result = parseGroup();
        return result;
      }

      function computeMolecularWeight(counts) {
        let total = 0;
        const breakdown = [];

        for (const key in counts) {
          const count = counts[key];

          let symbol = key;
          let atomicWeight;

          // Handle isotopes like "13C"
          const isotopeMatch = key.match(/^(\d+)([A-Z][a-z]?)$/);
          if (isotopeMatch) {
            const massNumber = parseInt(isotopeMatch[1], 10);
            symbol = isotopeMatch[2];
            if (!ATOMIC_WEIGHTS[symbol]) {
              throw new Error(`Unknown element symbol "${symbol}".`);
            }
            atomicWeight = massNumber; // approximate isotope mass as integer
          } else {
            atomicWeight = ATOMIC_WEIGHTS[symbol];
          }

          if (typeof atomicWeight !== "number") {
            throw new Error(`Missing atomic weight for element "${symbol}".`);
          }

          const contribution = atomicWeight * count;
          total += contribution;
          breakdown.push({
            symbol: key,
            baseSymbol: symbol,
            count,
            atomicWeight,
            contribution,
          });
        }

        return { total, breakdown };
      }

      function formatNumber(value, decimals) {
        return value.toFixed(decimals);
      }

      function renderResults(formula, total, breakdown, decimals) {
        const resultContainer = document.getElementById("result-container");
        const resultFormula = document.getElementById("result-formula");
        const resultValue = document.getElementById("result-value");
        const breakdownBody = document.getElementById("breakdown-body");
        const stepsContainer = document.getElementById("steps-container");

        resultFormula.textContent = formula;
        resultValue.textContent = formatNumber(total, decimals);

        breakdownBody.innerHTML = "";
        breakdown.sort((a, b) => a.baseSymbol.localeCompare(b.baseSymbol));

        breakdown.forEach((item) => {
          const row = document.createElement("tr");
          const percent = (item.contribution / total) * 100;

          row.innerHTML = `
        <td class="border-b border-gray-100">${item.symbol}</td>
        <td class="border-b border-gray-100 text-right">${item.count}</td>
        <td class="border-b border-gray-100 text-right">${item.atomicWeight.toFixed(
          4
        )}</td>
        <td class="border-b border-gray-100 text-right">${formatNumber(
          item.contribution,
          decimals
        )}</td>
        <td class="border-b border-gray-100 text-right">${percent.toFixed(
          2
        )}%</td>
      `;
          breakdownBody.appendChild(row);
        });

        // Step-by-step text
        const lines = [];
        lines.push(`1. Parse the formula and count atoms of each element.`);
        lines.push(`2. Multiply each element's atomic weight by its count:`);
        breakdown.forEach((item) => {
          lines.push(
            `   • ${item.symbol}: ${item.count} × ${item.atomicWeight.toFixed(
              4
            )} = ${formatNumber(item.contribution, decimals)} g/mol`
          );
        });
        lines.push(`3. Add all contributions:`);
        const sumExpr = breakdown
          .map((item) => formatNumber(item.contribution, decimals))
          .join(" + ");
        lines.push(`   ${sumExpr} = ${formatNumber(total, decimals)} g/mol`);

        stepsContainer.innerHTML = "";
        lines.forEach((text) => {
          const p = document.createElement("p");
          p.textContent = text;
          stepsContainer.appendChild(p);
        });

        resultContainer.classList.remove("hidden");
      }

      function showError(message) {
        const errorBox = document.getElementById("error-box");
        errorBox.textContent = message;
        errorBox.classList.remove("hidden");
      }

      function clearError() {
        const errorBox = document.getElementById("error-box");
        errorBox.classList.add("hidden");
        errorBox.textContent = "";
      }

      document.addEventListener("DOMContentLoaded", () => {
        const formulaInput = document.getElementById("formula-input");
        const precisionSelect = document.getElementById("precision-select");
        const calculateBtn = document.getElementById("calculate-btn");
        const clearBtn = document.getElementById("clear-btn");
        const resultContainer = document.getElementById("result-container");

        calculateBtn.addEventListener("click", () => {
          clearError();
          const formula = formulaInput.value.trim();
          const decimals = parseInt(precisionSelect.value, 10);

          try {
            const counts = parseFormula(formula);
            const { total, breakdown } = computeMolecularWeight(counts);
            renderResults(formula, total, breakdown, decimals);
          } catch (err) {
            resultContainer.classList.add("hidden");
            showError(
              err.message || "An error occurred while parsing the formula."
            );
          }
        });

        clearBtn.addEventListener("click", () => {
          formulaInput.value = "";
          resultContainer.classList.add("hidden");
          clearError();
        });

        // Example buttons
        document.querySelectorAll(".example-btn").forEach((btn) => {
          btn.addEventListener("click", () => {
            formulaInput.value = btn.getAttribute("data-example");
            clearError();
            resultContainer.classList.add("hidden");
          });
        });

        // Enter key triggers calculation
        formulaInput.addEventListener("keydown", (e) => {
          if (e.key === "Enter") {
            e.preventDefault();
            calculateBtn.click();
          }
        });

        // Simple FAQ toggle (in addition to global script_faq, safe to keep)
        document.querySelectorAll("#faq .faq-question").forEach((btn) => {
          btn.addEventListener("click", () => {
            const content = btn.nextElementSibling;
            const icon = btn.querySelector("span:last-child");
            const isHidden = content.classList.contains("hidden");
            document
              .querySelectorAll("#faq .faq-question + div")
              .forEach((div) => div.classList.add("hidden"));
            document
              .querySelectorAll("#faq .faq-question span:last-child")
              .forEach((i) => (i.textContent = "+"));
            if (isHidden) {
              content.classList.remove("hidden");
              icon.textContent = "−";
            }
          });
        });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
