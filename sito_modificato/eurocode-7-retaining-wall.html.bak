        function calculateStability() {
            const wallHeight = parseFloat(document.getElementById('wallHeight').value);
            const surchargeLoad = parseFloat(document.getElementById('surchargeLoad').value);
            const soilType = document.getElementById('soilType').value;

            if (!wallHeight || !surchargeLoad) {
                alert('Please enter all required fields.');
                return;
            }

            let safetyFactor;
            switch (soilType) {
                case 'sandy':
                    safetyFactor = (wallHeight * 1.5) / surchargeLoad;
                    break;
                case 'clay':
                    safetyFactor = (wallHeight * 1.2) / surchargeLoad;
                    break;
                case 'silt':
                    safetyFactor = (wallHeight * 1.3) / surchargeLoad;
                    break;
                default:
                    safetyFactor = 0;
            }

            document.getElementById('safetyFactor').textContent = safetyFactor.toFixed(2);
        }
    </script>


