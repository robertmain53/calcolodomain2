    <script>
      // Simplified geologic time data (Ma, older = larger number)
      const geoData = [
        // Eons (broad)
        { level: "eon", name: "Phanerozoic", startMa: 541, endMa: 0 },
        { level: "eon", name: "Proterozoic", startMa: 2500, endMa: 541 },
        { level: "eon", name: "Archean", startMa: 4000, endMa: 2500 },
        { level: "eon", name: "Hadean", startMa: 4600, endMa: 4000 },

        // Eras (Phanerozoic)
        {
          level: "era",
          name: "Cenozoic",
          startMa: 66,
          endMa: 0,
          parent: "Phanerozoic",
        },
        {
          level: "era",
          name: "Mesozoic",
          startMa: 252,
          endMa: 66,
          parent: "Phanerozoic",
        },
        {
          level: "era",
          name: "Paleozoic",
          startMa: 541,
          endMa: 252,
          parent: "Phanerozoic",
        },

        // Periods (Cenozoic)
        {
          level: "period",
          name: "Quaternary",
          startMa: 2.58,
          endMa: 0,
          parent: "Cenozoic",
        },
        {
          level: "period",
          name: "Neogene",
          startMa: 23.03,
          endMa: 2.58,
          parent: "Cenozoic",
        },
        {
          level: "period",
          name: "Paleogene",
          startMa: 66,
          endMa: 23.03,
          parent: "Cenozoic",
        },

        // Periods (Mesozoic)
        {
          level: "period",
          name: "Cretaceous",
          startMa: 145,
          endMa: 66,
          parent: "Mesozoic",
        },
        {
          level: "period",
          name: "Jurassic",
          startMa: 201.3,
          endMa: 145,
          parent: "Mesozoic",
        },
        {
          level: "period",
          name: "Triassic",
          startMa: 252.17,
          endMa: 201.3,
          parent: "Mesozoic",
        },

        // Periods (Paleozoic)
        {
          level: "period",
          name: "Permian",
          startMa: 298.9,
          endMa: 252.17,
          parent: "Paleozoic",
        },
        {
          level: "period",
          name: "Carboniferous",
          startMa: 358.9,
          endMa: 298.9,
          parent: "Paleozoic",
        },
        {
          level: "period",
          name: "Devonian",
          startMa: 419.2,
          endMa: 358.9,
          parent: "Paleozoic",
        },
        {
          level: "period",
          name: "Silurian",
          startMa: 443.8,
          endMa: 419.2,
          parent: "Paleozoic",
        },
        {
          level: "period",
          name: "Ordovician",
          startMa: 485.4,
          endMa: 443.8,
          parent: "Paleozoic",
        },
        {
          level: "period",
          name: "Cambrian",
          startMa: 541,
          endMa: 485.4,
          parent: "Paleozoic",
        },

        // Epochs (Cenozoic – simplified)
        {
          level: "epoch",
          name: "Holocene",
          startMa: 0.0117,
          endMa: 0,
          parent: "Quaternary",
        },
        {
          level: "epoch",
          name: "Pleistocene",
          startMa: 2.58,
          endMa: 0.0117,
          parent: "Quaternary",
        },
        {
          level: "epoch",
          name: "Pliocene",
          startMa: 5.333,
          endMa: 2.58,
          parent: "Neogene",
        },
        {
          level: "epoch",
          name: "Miocene",
          startMa: 23.03,
          endMa: 5.333,
          parent: "Neogene",
        },
        {
          level: "epoch",
          name: "Oligocene",
          startMa: 33.9,
          endMa: 23.03,
          parent: "Paleogene",
        },
        {
          level: "epoch",
          name: "Eocene",
          startMa: 56.0,
          endMa: 33.9,
          parent: "Paleogene",
        },
        {
          level: "epoch",
          name: "Paleocene",
          startMa: 66.0,
          endMa: 56.0,
          parent: "Paleogene",
        },

        // A few illustrative stages (Cenozoic only, for brevity)
        {
          level: "stage",
          name: "Meghalayan",
          startMa: 0.0042,
          endMa: 0,
          parent: "Holocene",
        },
        {
          level: "stage",
          name: "Northgrippian",
          startMa: 0.0082,
          endMa: 0.0042,
          parent: "Holocene",
        },
        {
          level: "stage",
          name: "Greenlandian",
          startMa: 0.0117,
          endMa: 0.0082,
          parent: "Holocene",
        },
        {
          level: "stage",
          name: "Late Pleistocene",
          startMa: 0.126,
          endMa: 0.0117,
          parent: "Pleistocene",
        },
        {
          level: "stage",
          name: "Middle Pleistocene",
          startMa: 0.774,
          endMa: 0.126,
          parent: "Pleistocene",
        },
        {
          level: "stage",
          name: "Early Pleistocene",
          startMa: 2.58,
          endMa: 0.774,
          parent: "Pleistocene",
        },
      ];

      function findContainingInterval(ageMa, level) {
        return geoData
          .filter((d) => d.level === level)
          .find((d) => ageMa <= d.startMa && ageMa > d.endMa);
      }

      function findIntervalByName(name, level) {
        return geoData.find((d) => d.level === level && d.name === name);
      }

      function getParent(interval, parentLevel) {
        if (!interval) return null;
        if (!interval.parent) return null;
        return geoData.find(
          (d) => d.level === parentLevel && d.name === interval.parent
        );
      }

      function formatRange(startMa, endMa) {
        const dur = startMa - endMa;
        return `${startMa.toFixed(2)} – ${endMa.toFixed(
          2
        )} Ma (duration ≈ ${dur.toFixed(2)} Myr)`;
      }

      // Populate dropdowns
      function populateDropdowns() {
        const selectEon = document.getElementById("selectEon");
        const selectEra = document.getElementById("selectEra");
        const selectPeriod = document.getElementById("selectPeriod");
        const selectEpoch = document.getElementById("selectEpoch");

        ["eon", "era", "period", "epoch"].forEach((level) => {
          const list = geoData
            .filter((d) => d.level === level)
            .sort((a, b) => b.startMa - a.startMa); // older first

          const target =
            level === "eon"
              ? selectEon
              : level === "era"
              ? selectEra
              : level === "period"
              ? selectPeriod
              : selectEpoch;

          list.forEach((d) => {
            const opt = document.createElement("option");
            opt.value = d.name;
            opt.textContent = `${d.name} (${d.startMa.toFixed(
              1
            )}–${d.endMa.toFixed(1)} Ma)`;
            target.appendChild(opt);
          });
        });
      }

      // Build simple horizontal timeline
      function buildTimeline() {
        const container = document.getElementById("timelineContainer");
        container.innerHTML = "";

        const eons = geoData
          .filter((d) => d.level === "eon")
          .sort((a, b) => b.startMa - a.startMa);
        const eras = geoData
          .filter((d) => d.level === "era")
          .sort((a, b) => b.startMa - a.startMa);
        const periods = geoData
          .filter((d) => d.level === "period")
          .sort((a, b) => b.startMa - a.startMa);

        const maxMa = 4600;
        const scale = 0.04; // px per Ma (for visual width)

        function makeRow(label, items, color) {
          const row = document.createElement("div");
          row.className =
            "timeline-row flex items-center border-b border-gray-100 px-3 py-2 text-xs";
          const labelDiv = document.createElement("div");
          labelDiv.className = "w-24 font-semibold text-gray-700";
          labelDiv.textContent = label;
          row.appendChild(labelDiv);

          const barContainer = document.createElement("div");
          barContainer.className = "relative flex-1 h-6 flex items-center";
          items.forEach((item) => {
            const width = (item.startMa - item.endMa) * scale;
            const left = (maxMa - item.startMa) * scale;
            const bar = document.createElement("button");
            bar.type = "button";
            bar.className =
              "absolute h-4 rounded-sm text-[10px] text-white flex items-center justify-center overflow-hidden whitespace-nowrap px-1";
            bar.style.left = `${left}px`;
            bar.style.width = `${Math.max(width, 24)}px`;
            bar.style.backgroundColor = color;
            bar.dataset.level = item.level;
            bar.dataset.name = item.name;
            bar.title = `${item.name} (${item.startMa.toFixed(
              2
            )}–${item.endMa.toFixed(2)} Ma)`;
            bar.textContent = item.name;
            bar.addEventListener("click", () => {
              const mid = (item.startMa + item.endMa) / 2;
              document.getElementById("ageMa").value = mid.toFixed(2);
              handleAgeInput();
            });
            barContainer.appendChild(bar);
          });
          row.appendChild(barContainer);
          container.appendChild(row);
        }

        makeRow("Eons", eons, "#4b5563");
        makeRow("Eras", eras, "#2563eb");
        makeRow("Periods", periods, "#059669");
      }

      function clearResults() {
        document.getElementById("resultEon").textContent = "–";
        document.getElementById("resultEra").textContent = "–";
        document.getElementById("resultPeriod").textContent = "–";
        document.getElementById("resultEpoch").textContent = "–";
        document.getElementById("resultStage").textContent = "–";
        document.getElementById("resultRange").textContent = "Age range: –";
        document.getElementById("resultNote").textContent = "";
      }

      function handleAgeInput() {
        const val = parseFloat(document.getElementById("ageMa").value);
        if (isNaN(val)) {
          clearResults();
          return;
        }
        if (val < 0 || val > 4600) {
          clearResults();
          document.getElementById("resultNote").textContent =
            "Age must be between 0 and 4,600 Ma.";
          return;
        }

        const ageMa = val;

        const stage = findContainingInterval(ageMa, "stage");
        const epoch = findContainingInterval(ageMa, "epoch");
        const period = findContainingInterval(ageMa, "period");
        const era = findContainingInterval(ageMa, "era");
        const eon = findContainingInterval(ageMa, "eon");

        document.getElementById("resultEon").textContent = eon ? eon.name : "–";
        document.getElementById("resultEra").textContent = era ? era.name : "–";
        document.getElementById("resultPeriod").textContent = period
          ? period.name
          : "–";
        document.getElementById("resultEpoch").textContent = epoch
          ? epoch.name
          : "–";
        document.getElementById("resultStage").textContent = stage
          ? stage.name
          : "–";

        let finest = stage || epoch || period || era || eon;
        if (finest) {
          document.getElementById(
            "resultRange"
          ).textContent = `Age range: ${formatRange(
            finest.startMa,
            finest.endMa
          )}`;
        } else {
          document.getElementById("resultRange").textContent =
            "Age range: outside simplified table.";
        }

        document.getElementById("resultNote").textContent =
          "Note: Boundaries are approximate and rounded to commonly cited Ma values based on the ICS time scale.";
      }

      function handleIntervalSelect(level, name) {
        if (!name) return;
        const interval = findIntervalByName(name, level);
        if (!interval) return;

        const mid = (interval.startMa + interval.endMa) / 2;
        document.getElementById("ageMa").value = mid.toFixed(2);
        handleAgeInput();
      }

      document.addEventListener("DOMContentLoaded", () => {
        populateDropdowns();
        buildTimeline();

        document
          .getElementById("ageMa")
          .addEventListener("input", handleAgeInput);
        document.getElementById("btnNow").addEventListener("click", () => {
          document.getElementById("ageMa").value = "0";
          handleAgeInput();
        });

        document.getElementById("selectEon").addEventListener("change", (e) => {
          handleIntervalSelect("eon", e.target.value);
        });
        document.getElementById("selectEra").addEventListener("change", (e) => {
          handleIntervalSelect("era", e.target.value);
        });
        document
          .getElementById("selectPeriod")
          .addEventListener("change", (e) => {
            handleIntervalSelect("period", e.target.value);
          });
        document
          .getElementById("selectEpoch")
          .addEventListener("change", (e) => {
            handleIntervalSelect("epoch", e.target.value);
          });
      });

      // Simple FAQ enhancer (optional, works with existing markup)
      document.addEventListener("DOMContentLoaded", () => {
        const details = document.querySelectorAll("#faq-container details");
        details.forEach((d) => {
          d.addEventListener("toggle", () => {
            if (d.open) {
              details.forEach((other) => {
                if (other !== d) other.open = false;
              });
            }
          });
        });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
