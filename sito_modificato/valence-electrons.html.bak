          period: 7,
          group: 0,
          block: "f",
          config: "[Rn] 5f3 6d1 7s2",
          valenceGroup: 6,
        },
      ];

      // Helper: build a lookup by symbol, name, and Z
      const ELEMENT_INDEX = {};
      ELEMENTS.forEach((el) => {
        ELEMENT_INDEX[el.symbol.toLowerCase()] = el;
        ELEMENT_INDEX[el.name.toLowerCase()] = el;
        ELEMENT_INDEX[String(el.z)] = el;
      });

      function findElement(query) {
        if (!query) return null;
        const q = String(query).trim().toLowerCase();
        if (!q) return null;
        // direct lookup
        if (ELEMENT_INDEX[q]) return ELEMENT_INDEX[q];

        // try capitalized symbol (first letter upper, rest lower)
        const sym = q.charAt(0).toUpperCase() + q.slice(1).toLowerCase();
        if (ELEMENT_INDEX[sym.toLowerCase()])
          return ELEMENT_INDEX[sym.toLowerCase()];

        return null;
      }

      function computeValence(el, mode) {
        // Mode "group": main-group rule
        if (mode === "group") {
          if (el.group === 1 || el.group === 2)
            return el.valenceGroup ?? el.group;
          if (el.group >= 13 && el.group <= 18) {
            if (el.symbol === "He") return 2;
            return el.group - 10;
          }
          // transition / others: fall back to stored typical value or outer-shell
          if (typeof el.valenceGroup === "number") return el.valenceGroup;
          mode = "outer";
        }

        // Mode "outer": count electrons in highest n shell from config string (approximate)
        // We approximate by parsing numbers before s/p/d/f and summing exponents for max n.
        const cfg = el.config.replace(/\[.*?\]/g, "").trim();
        const parts = cfg.split(/\s+/).filter(Boolean);
        let shells = {};
        for (const part of parts) {
          const m = part.match(/^(\d)([spdf])(\d+)$/i);
          if (!m) continue;
          const n = parseInt(m[1], 10);
          const electrons = parseInt(m[3], 10);
          shells[n] = (shells[n] || 0) + electrons;
        }
        const ns = Object.keys(shells).map(Number);
        if (!ns.length) return el.valenceGroup ?? null;
        const maxN = Math.max(...ns);
        return shells[maxN] || el.valenceGroup || null;
      }

      function renderLewisDots(symbol, valence) {
        const container = document.getElementById("lewis-diagram");
        container.innerHTML = "";
        if (!symbol || !Number.isFinite(valence) || valence <= 0) {
          container.innerHTML =
            '<span class="text-gray-400 text-sm">Enter an element to see its Lewis dots</span>';
          return;
        }
        const wrapper = document.createElement("div");
        wrapper.className = "flex flex-col items-center";

        const symEl = document.createElement("div");
        symEl.textContent = symbol;
        symEl.className = "text-2xl font-semibold mb-2";
        wrapper.appendChild(symEl);

        const dotsContainer = document.createElement("div");
        dotsContainer.className = "grid grid-cols-4 gap-1";
        const maxDots = Math.min(valence, 8); // simple octet-style diagram
        for (let i = 0; i < maxDots; i++) {
          const d = document.createElement("span");
          d.className = "dot";
          dotsContainer.appendChild(d);
        }
        wrapper.appendChild(dotsContainer);

        const note = document.createElement("p");
        note.className = "text-xs text-gray-500 mt-2 text-center";
        note.textContent =
          valence > 8
            ? `Showing first 8 of ${valence} valence electrons (expanded octet).`
            : `${valence} valence electron${valence === 1 ? "" : "s"}.`;
        wrapper.appendChild(note);

        container.appendChild(wrapper);
      }

      function updateUI(el, mode) {
        const nameEl = document.getElementById("res-name");
        const symEl = document.getElementById("res-symbol");
        const zEl = document.getElementById("res-z");
        const periodEl = document.getElementById("res-period");
        const groupEl = document.getElementById("res-group");
        const blockEl = document.getElementById("res-block");
        const valEl = document.getElementById("res-valence");
        const valNoteEl = document.getElementById("res-valence-note");
        const cfgEl = document.getElementById("res-config");
        const cfgShortEl = document.getElementById("res-config-short");
        const errEl = document.getElementById("element-error");

        if (!el) {
          nameEl.textContent = "–";
          symEl.textContent = "–";
          zEl.textContent = "–";
          periodEl.textContent = "–";
          groupEl.textContent = "–";
          blockEl.textContent = "–";
          valEl.textContent = "–";
          valNoteEl.textContent = "";
          cfgEl.textContent = "–";
          cfgShortEl.textContent = "–";
          renderLewisDots(null, null);
          errEl.classList.add("hidden");
          return;
        }

        nameEl.textContent = el.name;
        symEl.textContent = el.symbol;
        zEl.textContent = el.z;
        periodEl.textContent = el.period ?? "–";
        groupEl.textContent = el.group || (el.group === 0 ? "—" : "–");
        blockEl.textContent = el.block || "–";

        const val = computeValence(el, mode);
        if (val == null) {
          valEl.textContent = "–";
          valNoteEl.textContent =
            "Valence electrons not defined for this element in the selected mode.";
        } else {
          valEl.textContent = val;
          if (mode === "group") {
            if (el.group >= 1 && el.group <= 2) {
              valNoteEl.textContent = `Main-group rule: group ${
                el.group
              } → ${val} valence electron${val === 1 ? "" : "s"}.`;
            } else if (el.group >= 13 && el.group <= 18) {
              valNoteEl.textContent = `Main-group rule: group ${el.group} → ${val} valence electrons (group − 10).`;
            } else {
              valNoteEl.textContent = `Typical valence electron count for ${el.block}-block element (may vary by oxidation state).`;
            }
          } else {
            valNoteEl.textContent = `Outer-shell definition: electrons in the highest n shell from the electron configuration.`;
          }
        }

        cfgEl.textContent = el.config || "–";
        const shortCfg =
          el.config && el.config.includes("]")
            ? el.config
            : `[${el.symbol}] core configuration`;
        cfgShortEl.textContent = shortCfg;

        renderLewisDots(el.symbol, val);
        errEl.classList.add("hidden");
      }

      function handleInput() {
        const input = document.getElementById("element-input").value;
        const mode = document.getElementById("valence-mode").value;
        const errEl = document.getElementById("element-error");
        const el = findElement(input);

        if (!input.trim()) {
          updateUI(null, mode);
          return;
        }

        if (!el) {
          errEl.textContent =
            "Element not found. Try a valid symbol (e.g., C, Fe), name (carbon), or atomic number (6).";
          errEl.classList.remove("hidden");
          updateUI(null, mode);
          return;
        }

        updateUI(el, mode);
      }

      document.addEventListener("DOMContentLoaded", () => {
        const inputEl = document.getElementById("element-input");
        const clearBtn = document.getElementById("element-clear");
        const modeEl = document.getElementById("valence-mode");

        inputEl.addEventListener("input", handleInput);
        modeEl.addEventListener("change", handleInput);
        clearBtn.addEventListener("click", () => {
          inputEl.value = "";
          handleInput();
          inputEl.focus();
        });

        // Pre-fill with carbon as a friendly default
        inputEl.value = "C";
        handleInput();

        // Build quick reference table for a curated subset
        const quickIds = [
          "H",
          "He",
          "Li",
          "Be",
          "B",
          "C",
          "N",
          "O",
          "F",
          "Ne",
          "Na",
          "Mg",
          "Al",
          "Si",
          "P",
          "S",
          "Cl",
          "Ar",
        ];
        const tbody = document.getElementById("quick-table-body");
        quickIds.forEach((sym) => {
          const el = ELEMENTS.find((e) => e.symbol === sym);
          if (!el) return;
          const tr = document.createElement("tr");
          const val = computeValence(el, "group");
          tr.innerHTML = `
        <td class="px-3 py-2 whitespace-nowrap">${el.name}</td>
        <td class="px-3 py-2 font-mono">${el.symbol}</td>
        <td class="px-3 py-2">${el.group ?? "–"}</td>
        <td class="px-3 py-2">${val ?? "–"}</td>
      `;
          tbody.appendChild(tr);
        });

        document
          .querySelectorAll("#faq-container .faq-question")
          .forEach((btn) => {
            btn.addEventListener("click", () => {
              const answer = btn.parentElement.querySelector(".faq-answer");
              const isHidden = answer.classList.contains("hidden");
              document
                .querySelectorAll("#faq-container .faq-answer")
                .forEach((a) => a.classList.add("hidden"));
              document
                .querySelectorAll(
                  "#faq-container .faq-question span:last-child"
                )
                .forEach((icon) => (icon.textContent = "+"));
              if (isHidden) {
