      ];

      const timeSelect = document.getElementById("time-per-week");
      const budgetSelect = document.getElementById("budget-level");
      const energySelect = document.getElementById("energy-level");
      const socialSelect = document.getElementById("social-level");
      const goalChips = document.querySelectorAll(".goal-chip");
      const applyBtn = document.getElementById("apply-filters");
      const resetBtn = document.getElementById("reset-filters");
      const hobbyListEl = document.getElementById("hobby-list");
      const resultsCountEl = document.getElementById("results-count");
      const savedListEl = document.getElementById("saved-hobbies");
      const clearSavedBtn = document.getElementById("clear-saved");

      let activeGoals = new Set();
      let savedHobbies = new Set(
        JSON.parse(localStorage.getItem("savedHobbies") || "[]")
      );

      function renderSaved() {
        savedListEl.innerHTML = "";
        if (savedHobbies.size === 0) {
          const li = document.createElement("li");
          li.className = "text-gray-500";
          li.textContent =
            "No hobbies saved yet. Click the heart icon on a hobby to add it here.";
          savedListEl.appendChild(li);
          return;
        }
        [...savedHobbies].forEach((name) => {
          const li = document.createElement("li");
          li.textContent = name;
          savedListEl.appendChild(li);
        });
      }

      function matchesFilters(hobby) {
        const timeVal = timeSelect.value;
        const budgetVal = budgetSelect.value;
        const energyVal = energySelect.value;
        const socialVal = socialSelect.value;

        if (timeVal !== "any" && hobby.time !== timeVal) return false;
        if (budgetVal !== "any" && hobby.budget !== budgetVal) return false;
        if (energyVal !== "any" && hobby.energy !== energyVal) return false;
        if (socialVal !== "any" && hobby.social !== socialVal) return false;

        if (activeGoals.size > 0) {
          const hasGoal = hobby.goals.some((g) => activeGoals.has(g));
          if (!hasGoal) return false;
        }
        return true;
      }

      function renderHobbies() {
        hobbyListEl.innerHTML = "";
        const filtered = HOBBIES.filter(matchesFilters);
        resultsCountEl.textContent = `${filtered.length} hobby${
          filtered.length === 1 ? "" : "ies"
        } found`;

        if (filtered.length === 0) {
          const div = document.createElement("div");
          div.className = "col-span-full text-sm text-gray-600";
          div.textContent =
            "No hobbies match these filters. Try relaxing one or two filters or clearing goals.";
          hobbyListEl.appendChild(div);
          return;
        }

        filtered.forEach((hobby) => {
          const card = document.createElement("article");
          card.className =
            "card-hover border border-gray-200 rounded-lg p-3 flex flex-col justify-between";

          const header = document.createElement("div");
          header.className = "flex items-start justify-between mb-2";

          const title = document.createElement("h3");
          title.className = "font-semibold text-sm md:text-base";
          title.textContent = hobby.name;

          const favBtn = document.createElement("button");
          favBtn.type = "button";
          favBtn.className = "ml-2 text-gray-400 hover:text-red-500";
          favBtn.setAttribute("aria-label", "Save hobby");
          const isSaved = savedHobbies.has(hobby.name);
          favBtn.innerHTML = isSaved
            ? '<svg class="w-5 h-5 fill-red-500" viewBox="0 0 20 20"><path d="M3.172 5.172a4 4 0 015.656 0L10 6.343l1.172-1.171a4 4 0 115.656 5.656L10 18.657l-6.828-6.829a4 4 0 010-5.656z"/></svg>'
            : '<svg class="w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4.318 6.318a4.5 4.5 0 016.364 0L12 7.636l1.318-1.318a4.5 4.5 0 116.364 6.364L12 20.364l-7.682-7.682a4.5 4.5 0 010-6.364z"/></svg>';

          favBtn.addEventListener("click", () => {
            if (savedHobbies.has(hobby.name)) {
              savedHobbies.delete(hobby.name);
            } else {
              savedHobbies.add(hobby.name);
            }
            localStorage.setItem(
              "savedHobbies",
              JSON.stringify([...savedHobbies])
            );
            renderSaved();
            renderHobbies();
          });

          header.appendChild(title);
          header.appendChild(favBtn);

          const meta = document.createElement("div");
          meta.className = "text-[11px] text-gray-500 mb-2 space-y-1";
          meta.innerHTML = `
        <div><strong>Category:</strong> ${hobby.category}</div>
        <div><strong>Time:</strong> ${describeTime(
          hobby.time
        )} · <strong>Budget:</strong> ${describeBudget(hobby.budget)}</div>
        <div><strong>Energy:</strong> ${describeEnergy(
          hobby.energy
        )} · <strong>Social:</strong> ${describeSocial(hobby.social)}</div>
      `;

          const goals = document.createElement("div");
          goals.className = "mb-2";
          hobby.goals.forEach((g) => {
            const span = document.createElement("span");
            span.className =
              "inline-flex items-center px-2 py-0.5 rounded-full text-[10px] font-medium bg-blue-50 text-blue-700 mr-1 mb-1";
            span.textContent = goalLabel(g);
            goals.appendChild(span);
          });

          const notes = document.createElement("p");
          notes.className = "text-xs text-gray-700";
          notes.textContent = hobby.notes;

          card.appendChild(header);
          card.appendChild(meta);
          card.appendChild(goals);
          card.appendChild(notes);

          hobbyListEl.appendChild(card);
        });
      }

      function describeTime(t) {
        switch (t) {
          case "very-low":
            return "< 1 h/week";
          case "low":
            return "1–3 h/week";
          case "medium":
            return "3–6 h/week";
          case "high":
            return "> 6 h/week";
          default:
            return "Varies";
        }
      }
      function describeBudget(b) {
        switch (b) {
          case "free":
            return "Free / almost free";
          case "low":
            return "Low";
          case "medium":
            return "Medium";
          case "high":
            return "High";
          default:
            return "Any";
        }
      }
      function describeEnergy(e) {
        switch (e) {
          case "calm":
            return "Calm";
          case "moderate":
            return "Moderate";
          case "intense":
            return "High energy";
          default:
            return "Any";
        }
      }
      function describeSocial(s) {
        switch (s) {
          case "solo":
            return "Mostly solo";
          case "either":
            return "Solo or social";
          case "social":
            return "Mostly social";
          default:
            return "Any";
        }
      }
      function goalLabel(g) {
        switch (g) {
          case "relax":
            return "Relax";
          case "fitness":
            return "Move / Fitness";
          case "creative":
            return "Creative";
          case "social":
            return "Social";
          case "learn":
            return "Learn";
          default:
            return g;
        }
      }

      goalChips.forEach((chip) => {
        chip.addEventListener("click", () => {
          const goal = chip.dataset.goal;
          if (activeGoals.has(goal)) {
            activeGoals.delete(goal);
            chip.classList.remove(
              "bg-blue-600",
              "text-white",
              "border-blue-600"
            );
            chip.classList.add("border-gray-300", "text-gray-700");
          } else {
            activeGoals.add(goal);
            chip.classList.add("bg-blue-600", "text-white", "border-blue-600");
            chip.classList.remove("border-gray-300", "text-gray-700");
          }
        });
      });

      applyBtn.addEventListener("click", renderHobbies);

      resetBtn.addEventListener("click", () => {
        timeSelect.value = "any";
        budgetSelect.value = "any";
        energySelect.value = "any";
        socialSelect.value = "any";
        activeGoals.clear();
        goalChips.forEach((chip) => {
          chip.classList.remove("bg-blue-600", "text-white", "border-blue-600");
          chip.classList.add("border-gray-300", "text-gray-700");
        });
        renderHobbies();
      });

      clearSavedBtn.addEventListener("click", () => {
        savedHobbies.clear();
        localStorage.removeItem("savedHobbies");
        renderSaved();
        renderHobbies();
      });

      // Initial render
      renderSaved();
      renderHobbies();
    </script>

