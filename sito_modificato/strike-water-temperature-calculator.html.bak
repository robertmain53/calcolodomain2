    <script>
      // Utility: clamp and parse
      function num(val) {
        const n = parseFloat(val);
        return isNaN(n) ? null : n;
      }

      const els = {
        unitSystem: document.getElementById("unit-system"),
        mode: document.getElementById("mode"),
        grainWeight: document.getElementById("grain-weight"),
        grainTemp: document.getElementById("grain-temp"),
        mashTemp: document.getElementById("mash-temp"),
        mashThickness: document.getElementById("mash-thickness"),
        totalWater: document.getElementById("total-water"),
        mashThicknessGroup: document.getElementById("mash-thickness-group"),
        totalWaterGroup: document.getElementById("total-water-group"),
        tunEnable: document.getElementById("tun-enable"),
        tunWeight: document.getElementById("tun-weight"),
        tunTemp: document.getElementById("tun-temp"),
        tunCp: document.getElementById("tun-cp"),
        tunPreset: document.getElementById("tun-preset"),
        grainAbsorption: document.getElementById("grain-absorption"),
        deadspace: document.getElementById("deadspace"),
        calcBtn: document.getElementById("calculate-btn"),
        resetBtn: document.getElementById("reset-btn"),
        error: document.getElementById("calc-error"),
        resultStrikeTemp: document.getElementById("result-strike-temp"),
        resultStrikeTempAlt: document.getElementById("result-strike-temp-alt"),
        resultStrikeVolume: document.getElementById("result-strike-volume"),
        resultStrikeVolumeDetail: document.getElementById(
          "result-strike-volume-detail"
        ),
        resultNotes: document.getElementById("result-notes"),
        // unit labels
        grainWeightUnit: document.getElementById("grain-weight-unit"),
        grainTempUnit: document.getElementById("grain-temp-unit"),
        mashTempUnit: document.getElementById("mash-temp-unit"),
        mashThicknessUnit: document.getElementById("mash-thickness-unit"),
        totalWaterUnit: document.getElementById("total-water-unit"),
        tunWeightUnit: document.getElementById("tun-weight-unit"),
        tunTempUnit: document.getElementById("tun-temp-unit"),
        grainAbsorptionUnit: document.getElementById("grain-absorption-unit"),
        deadspaceUnit: document.getElementById("deadspace-unit"),
      };

      function updateUnits() {
        const us = els.unitSystem.value === "us";

        els.grainWeightUnit.textContent = us ? "lb" : "kg";
        els.grainTempUnit.textContent = us ? "°F" : "°C";
        els.mashTempUnit.textContent = us ? "°F" : "°C";
        els.mashThicknessUnit.textContent = us ? "qt/lb" : "L/kg";
        els.totalWaterUnit.textContent = us ? "gal" : "L";
        els.tunWeightUnit.textContent = us ? "lb" : "kg";
        els.tunTempUnit.textContent = us ? "°F" : "°C";
        els.grainAbsorptionUnit.textContent = us ? "gal/lb" : "L/kg";
        els.deadspaceUnit.textContent = us ? "gal" : "L";

        // Adjust default values when switching units (simple approximate conversion)
        if (us) {
          // Metric -> US
          els.grainWeight.value = (num(els.grainWeight.value) || 4.5) * 2.20462;
          els.grainTemp.value = ((num(els.grainTemp.value) || 21) * 9) / 5 + 32;
          els.mashTemp.value = ((num(els.mashTemp.value) || 67) * 9) / 5 + 32;
          els.mashThickness.value = (num(els.mashThickness.value) || 3) / 1.043; // approx L/kg to qt/lb
          els.totalWater.value = (num(els.totalWater.value) || 26) / 3.785;
          els.tunWeight.value = (num(els.tunWeight.value) || 4) * 2.20462;
          els.tunTemp.value = ((num(els.tunTemp.value) || 21) * 9) / 5 + 32;
          els.grainAbsorption.value =
            (num(els.grainAbsorption.value) || 0.9) / 3.785 / 2.20462;
          els.deadspace.value = (num(els.deadspace.value) || 1) / 3.785;
        } else {
          // US -> Metric
          els.grainWeight.value = (num(els.grainWeight.value) || 10) / 2.20462;
          els.grainTemp.value =
            (((num(els.grainTemp.value) || 70) - 32) * 5) / 9;
          els.mashTemp.value =
            (((num(els.mashTemp.value) || 152) - 32) * 5) / 9;
          els.mashThickness.value =
            (num(els.mashThickness.value) || 1.5) * 1.043; // approx qt/lb to L/kg
          els.totalWater.value = (num(els.totalWater.value) || 7) * 3.785;
          els.tunWeight.value = (num(els.tunWeight.value) || 9) / 2.20462;
          els.tunTemp.value = (((num(els.tunTemp.value) || 70) - 32) * 5) / 9;
          els.grainAbsorption.value =
            (num(els.grainAbsorption.value) || 0.12) * 3.785 * 2.20462;
          els.deadspace.value = (num(els.deadspace.value) || 0.25) * 3.785;
        }
      }

      function updateMode() {
        const biab = els.mode.value === "biab";
        els.mashThicknessGroup.classList.toggle("hidden", biab);
        els.totalWaterGroup.classList.toggle("hidden", !biab);
      }

      function applyTunPreset() {
        const preset = els.tunPreset.value;
        if (preset === "cooler") {
          els.tunCp.value = 0.3;
          els.tunEnable.checked = true;
        } else if (preset === "stainless") {
          els.tunCp.value = 0.5;
          els.tunEnable.checked = true;
        } else if (preset === "none") {
          els.tunCp.value = 0.0;
          els.tunEnable.checked = false;
        }
      }

      function resetForm() {
        els.unitSystem.value = "us";
        els.mode.value = "standard";
        els.grainWeight.value = 10;
        els.grainTemp.value = 70;
        els.mashTemp.value = 152;
        els.mashThickness.value = 1.5;
        els.totalWater.value = 7.0;
        els.tunEnable.checked = true;
        els.tunWeight.value = 9;
        els.tunTemp.value = 70;
        els.tunCp.value = 0.3;
        els.tunPreset.value = "cooler";
        els.grainAbsorption.value = 0.12;
        els.deadspace.value = 0.25;
        updateUnits();
        updateMode();
        els.error.classList.add("hidden");
        els.resultStrikeTemp.textContent = "–";
        els.resultStrikeTempAlt.textContent = "";
        els.resultStrikeVolume.textContent = "–";
        els.resultStrikeVolumeDetail.textContent = "";
        els.resultNotes.textContent = "";
      }

      function calculate() {
        els.error.classList.add("hidden");
        const us = els.unitSystem.value === "us";
        const biab = els.mode.value === "biab";

        const G = num(els.grainWeight.value);
        const Tg = num(els.grainTemp.value);
        const Tm = num(els.mashTemp.value);
        const thickness = num(els.mashThickness.value);
        const totalWater = num(els.totalWater.value);
        const tunEnabled = els.tunEnable.checked;
        const Mt = tunEnabled ? num(els.tunWeight.value) : 0;
        const Tt = tunEnabled ? num(els.tunTemp.value) : 0;
        const Ct_rel = tunEnabled ? num(els.tunCp.value) : 0;
        const absorption = num(els.grainAbsorption.value);
        const deadspace = num(els.deadspace.value);

        if (!G || !Tg || !Tm || (biab ? !totalWater : !thickness)) {
          els.error.textContent =
            "Please enter grain weight, temperatures, and mash thickness or total water.";
          els.error.classList.remove("hidden");
          return;
        }

        // Convert everything to metric internally for consistent physics
        let G_kg,
          Tg_C,
          Tm_C,
          thickness_L_per_kg,
          Vw_L,
          Mt_kg,
          Tt_C,
          absorption_L_per_kg,
          deadspace_L;
        if (us) {
          G_kg = G * 0.453592;
          Tg_C = ((Tg - 32) * 5) / 9;
          Tm_C = ((Tm - 32) * 5) / 9;
          thickness_L_per_kg = biab ? null : thickness * 1.043; // approx qt/lb -> L/kg
          Vw_L = biab ? totalWater * 3.78541 : thickness_L_per_kg * G_kg;
          Mt_kg = Mt * 0.453592;
          Tt_C = ((Tt - 32) * 5) / 9;
          absorption_L_per_kg = absorption * 3.78541 * 2.20462;
          deadspace_L = deadspace * 3.78541;
        } else {
          G_kg = G;
          Tg_C = Tg;
          Tm_C = Tm;
          thickness_L_per_kg = biab ? null : thickness;
          Vw_L = biab ? totalWater : thickness_L_per_kg * G_kg;
          Mt_kg = Mt;
          Tt_C = Tt;
          absorption_L_per_kg = absorption;
          deadspace_L = deadspace;
        }

        if (Vw_L <= 0) {
          els.error.textContent =
            "Strike water volume must be greater than zero.";
          els.error.classList.remove("hidden");
          return;
        }

        // Specific heats (relative): water = 1, grain ≈ 0.38, tun = Ct_rel
        const Cw = 1.0;
        const Cg = 0.38;
        const Ct = Math.max(0, Ct_rel || 0);

        // Mass of water in kg (approx 1 kg/L)
        const Mw_kg = Vw_L;

        // Heat balance in °C
        const numerator =
          G_kg * Cg * (Tm_C - Tg_C) + Mt_kg * Ct * (Tm_C - Tt_C);
        const denominator = Mw_kg * Cw;
        const Tstrike_C =
          Tm_C + (denominator > 0 ? numerator / denominator : 0);

        // Convert back to display units
        const Tstrike_display = us ? (Tstrike_C * 9) / 5 + 32 : Tstrike_C;
        const Tstrike_alt = us ? Tstrike_C : (Tstrike_C * 9) / 5 + 32;

        const Vw_display = us ? Vw_L / 3.78541 : Vw_L;

        // First runnings estimate
        const absorbed_L = absorption_L_per_kg * G_kg;
        const firstRunnings_L = Math.max(0, Vw_L - absorbed_L - deadspace_L);
        const firstRunnings_display = us
          ? firstRunnings_L / 3.78541
          : firstRunnings_L;

        // Output formatting
        const tempUnit = us ? "°F" : "°C";
        const tempAltUnit = us ? "°C" : "°F";
        const volUnit = us ? "gal" : "L";

        els.resultStrikeTemp.textContent = isFinite(Tstrike_display)
          ? Tstrike_display.toFixed(1) + " " + tempUnit
          : "–";
        els.resultStrikeTempAlt.textContent = isFinite(Tstrike_alt)
          ? "(" + Tstrike_alt.toFixed(1) + " " + tempAltUnit + ")"
          : "";

        els.resultStrikeVolume.textContent =
          Vw_display.toFixed(2) + " " + volUnit;
        els.resultStrikeVolumeDetail.textContent =
          "Estimated first runnings: " +
          firstRunnings_display.toFixed(2) +
          " " +
          volUnit +
          " (after grain absorption and deadspace).";

        let notes =
          "Based on " +
          G.toFixed(2) +
          (us ? " lb" : " kg") +
          " of grain at " +
          Tg.toFixed(1) +
          " " +
          tempUnit +
          " and a target mash of " +
          Tm.toFixed(1) +
          " " +
          tempUnit +
          ".";

        if (tunEnabled && Mt_kg > 0 && Ct > 0) {
          notes += " Mash tun thermal mass is included in the calculation.";
        } else {
          notes +=
            " Mash tun thermal mass is ignored (assumed pre-heated or negligible).";
        }

        els.resultNotes.textContent = notes;
      }

      els.unitSystem.addEventListener("change", () => {
        updateUnits();
        calculate();
      });
      els.mode.addEventListener("change", () => {
        updateMode();
        calculate();
      });
      els.tunPreset.addEventListener("change", () => {
        applyTunPreset();
        calculate();
      });
      els.calcBtn.addEventListener("click", calculate);
      els.resetBtn.addEventListener("click", resetForm);

      // Recalculate on Enter key in any input
      document.querySelectorAll("input").forEach((inp) => {
        inp.addEventListener("keydown", (e) => {
          if (e.key === "Enter") {
            e.preventDefault();
            calculate();
          }
        });
      });

      // Initialize
      resetForm();
    </script>

