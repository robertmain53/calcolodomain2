    <script>
      // Utility formatting
      function fmtMoney(value) {
        if (!isFinite(value)) return "$0";
        return (
          "$" + value.toLocaleString(undefined, { maximumFractionDigits: 0 })
        );
      }
      function fmtMoney2(value) {
        if (!isFinite(value)) return "$0";
        return (
          "$" +
          value.toLocaleString(undefined, {
            minimumFractionDigits: 2,
            maximumFractionDigits: 2,
          })
        );
      }
      function fmtPsf(value) {
        if (!isFinite(value)) return "$0";
        return (
          "$" +
          value.toLocaleString(undefined, {
            minimumFractionDigits: 2,
            maximumFractionDigits: 2,
          })
        );
      }

      // Mode toggle
      const modePsfBtn = document.getElementById("mode-psf");
      const modeMonthlyBtn = document.getElementById("mode-monthly");
      const rentPsfGroup = document.getElementById("rent-psf-group");
      const rentMonthlyGroup = document.getElementById("rent-monthly-group");
      let rentMode = "psf";

      modePsfBtn.addEventListener("click", () => {
        rentMode = "psf";
        modePsfBtn.classList.remove(
          "border-gray-300",
          "text-gray-700",
          "bg-white"
        );
        modePsfBtn.classList.add(
          "border-blue-600",
          "text-blue-600",
          "bg-blue-50"
        );
        modeMonthlyBtn.classList.remove(
          "border-blue-600",
          "text-blue-600",
          "bg-blue-50"
        );
        modeMonthlyBtn.classList.add(
          "border-gray-300",
          "text-gray-700",
          "bg-white"
        );
        rentPsfGroup.classList.remove("hidden");
        rentMonthlyGroup.classList.add("hidden");
      });

      modeMonthlyBtn.addEventListener("click", () => {
        rentMode = "monthly";
        modeMonthlyBtn.classList.remove(
          "border-gray-300",
          "text-gray-700",
          "bg-white"
        );
        modeMonthlyBtn.classList.add(
          "border-blue-600",
          "text-blue-600",
          "bg-blue-50"
        );
        modePsfBtn.classList.remove(
          "border-blue-600",
          "text-blue-600",
          "bg-blue-50"
        );
        modePsfBtn.classList.add(
          "border-gray-300",
          "text-gray-700",
          "bg-white"
        );
        rentPsfGroup.classList.add("hidden");
        rentMonthlyGroup.classList.remove("hidden");
      });

      document.getElementById("calculateBtn").addEventListener("click", () => {
        const errorEl = document.getElementById("errorMsg");
        errorEl.textContent = "";

        const rentableSqft =
          parseFloat(document.getElementById("rentableSqft").value) || 0;
        const usableSqft =
          parseFloat(document.getElementById("usableSqft").value) || 0;
        const baseRentPsfInput =
          parseFloat(document.getElementById("baseRentPsf").value) || 0;
        const baseRentMonthlyInput =
          parseFloat(document.getElementById("baseRentMonthly").value) || 0;
        const nnnPsf = parseFloat(document.getElementById("nnnPsf").value) || 0;
        const termYears =
          parseFloat(document.getElementById("termYears").value) || 0;
        const termMonths =
          parseFloat(document.getElementById("termMonths").value) || 0;
        const freeRentMonths =
          parseFloat(document.getElementById("freeRentMonths").value) || 0;
        const escalationRate =
          (parseFloat(document.getElementById("escalationRate").value) || 0) /
          100;
        const tiAllowancePsf =
          parseFloat(document.getElementById("tiAllowancePsf").value) || 0;
        const tenantUpfrontCosts =
          parseFloat(document.getElementById("tenantUpfrontCosts").value) || 0;
        const discountRate =
          (parseFloat(document.getElementById("discountRate").value) || 0) /
          100;

        if (rentableSqft <= 0) {
          errorEl.textContent = "Please enter rentable square footage.";
          return;
        }
        const totalMonths = termYears * 12 + termMonths;
        if (totalMonths <= 0) {
          errorEl.textContent = "Please enter a positive lease term.";
          return;
        }

        // Normalize base rent to $/sf/year
        let baseRentPsf = 0;
        if (rentMode === "psf") {
          baseRentPsf = baseRentPsfInput;
        } else {
          if (baseRentMonthlyInput <= 0) {
            errorEl.textContent = "Please enter a positive monthly base rent.";
            return;
          }
          const annualBase = baseRentMonthlyInput * 12;
          baseRentPsf = annualBase / rentableSqft;
        }
        if (baseRentPsf < 0) baseRentPsf = 0;

        const termYearsExact = totalMonths / 12;

        // Year-by-year schedule
        const scheduleBody = document.getElementById("scheduleBody");
        scheduleBody.innerHTML = "";

        let remainingFreeMonths = Math.min(freeRentMonths, totalMonths);
        const freeMonthsPerYear =
          remainingFreeMonths / Math.ceil(termYearsExact); // simple spread
        let totalLeaseCost = 0;
        let totalBaseRentAllYears = 0;
        let totalPV = 0;

        for (let year = 1; year <= Math.ceil(termYearsExact); year++) {
          const yearFraction =
            year < Math.ceil(termYearsExact)
              ? 1
              : termYearsExact - Math.floor(termYearsExact) || 1;
          const monthsInYear = 12 * yearFraction;

          const yearBasePsf =
            baseRentPsf * Math.pow(1 + escalationRate, year - 1);
          const annualBase = yearBasePsf * rentableSqft * yearFraction;
          const annualNNN = nnnPsf * rentableSqft * yearFraction;

          const freeMonthsThisYear = Math.min(
            freeMonthsPerYear,
            remainingFreeMonths,
            monthsInYear
          );
          remainingFreeMonths -= freeMonthsThisYear;

          const paidFraction =
            (monthsInYear - freeMonthsThisYear) / monthsInYear;
          const paidBase = annualBase * paidFraction;
          const netPaid = paidBase + annualNNN;

          totalLeaseCost += netPaid;
          totalBaseRentAllYears += annualBase;

          if (discountRate > 0) {
            const t = year - 0.5; // mid-year discounting
            totalPV += netPaid / Math.pow(1 + discountRate, t);
          }

          const row = document.createElement("tr");
          row.innerHTML = `
        <td class="px-3 py-2 border-b text-right">${year}</td>
        <td class="px-3 py-2 border-b text-right">${fmtPsf(yearBasePsf)}</td>
        <td class="px-3 py-2 border-b text-right">${fmtMoney(annualBase)}</td>
        <td class="px-3 py-2 border-b text-right">${fmtMoney(annualNNN)}</td>
        <td class="px-3 py-2 border-b text-right">${freeMonthsThisYear.toFixed(
          1
        )}</td>
        <td class="px-3 py-2 border-b text-right">${fmtMoney(netPaid)}</td>
      `;
          scheduleBody.appendChild(row);
        }

        // TI and net cost
        const totalTiAllowance = tiAllowancePsf * rentableSqft;
        const netTotalCost =
          totalLeaseCost + tenantUpfrontCosts - totalTiAllowance;

        // Face and effective rent
        const facePsf = totalBaseRentAllYears / (rentableSqft * termYearsExact);
        const effectivePsf = netTotalCost / (rentableSqft * termYearsExact);
        const effectiveUsablePsf =
          usableSqft > 0 ? netTotalCost / (usableSqft * termYearsExact) : NaN;

        // Year 1 monthly numbers
        const year1BaseAnnual = baseRentPsf * rentableSqft;
        const year1NnnAnnual = nnnPsf * rentableSqft;
        const year1TotalAnnual = year1BaseAnnual + year1NnnAnnual;
        const year1BaseMonthly = year1BaseAnnual / 12;
        const year1NnnMonthly = year1NnnAnnual / 12;
        const year1TotalMonthly = year1TotalAnnual / 12;

        document.getElementById("resBaseMonthly").textContent =
          fmtMoney2(year1BaseMonthly);
        document.getElementById("resNnnMonthly").textContent =
          fmtMoney2(year1NnnMonthly);
        document.getElementById("resTotalMonthly").textContent =
          fmtMoney2(year1TotalMonthly);
        document.getElementById("resTotalAnnual").textContent =
          fmtMoney(year1TotalAnnual);

        document.getElementById("resFacePsf").textContent = fmtPsf(facePsf);
        document.getElementById("resEffectivePsf").textContent =
          fmtPsf(effectivePsf);
        document.getElementById("resEffectiveUsablePsf").textContent = isNaN(
          effectiveUsablePsf
        )
          ? "—"
          : fmtPsf(effectiveUsablePsf);
        document.getElementById("resTotalLeaseCost").textContent =
          fmtMoney(netTotalCost);
        document.getElementById("resPV").textContent =
          discountRate > 0 ? fmtMoney(totalPV) : "—";
      });

      document.getElementById("resetBtn").addEventListener("click", () => {
        [
          "rentableSqft",
          "usableSqft",
          "baseRentPsf",
          "baseRentMonthly",
          "nnnPsf",
          "termYears",
          "termMonths",
          "freeRentMonths",
          "escalationRate",
          "tiAllowancePsf",
          "tenantUpfrontCosts",
          "discountRate",
        ].forEach((id) => (document.getElementById(id).value = ""));
        document.getElementById("errorMsg").textContent = "";
        document.getElementById("scheduleBody").innerHTML =
          '<tr><td class="px-3 py-2 border-b text-center text-gray-500" colspan="6">Enter lease details and click Calculate to see the schedule.</td></tr>';
        document.getElementById("resBaseMonthly").textContent = "$0";
        document.getElementById("resNnnMonthly").textContent = "$0";
        document.getElementById("resTotalMonthly").textContent = "$0";
        document.getElementById("resTotalAnnual").textContent = "$0";
        document.getElementById("resFacePsf").textContent = "$0";
        document.getElementById("resEffectivePsf").textContent = "$0";
        document.getElementById("resEffectiveUsablePsf").textContent = "$0";
        document.getElementById("resTotalLeaseCost").textContent = "$0";
        document.getElementById("resPV").textContent = "$0";
      });

      // Mobile menu script (simple)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", () => {
          const menu = document.getElementById("mobile-menu");
          const expanded = menu.classList.contains("hidden");
          menu.classList.toggle("hidden");
          document
            .getElementById("mobile-menu-toggle")
            .setAttribute("aria-expanded", expanded ? "true" : "false");
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
