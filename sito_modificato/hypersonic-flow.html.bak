    <script>
      // Standard atmosphere model (0–80 km), simplified ISA
      function standardAtmosphere(hMeters) {
        // Clamp altitude
        const h = Math.max(0, Math.min(80000, hMeters));

        // Layer base values (up to 80 km)
        const layers = [
          { hb: 0, Tb: 288.15, pb: 101325, L: -0.0065 },
          { hb: 11000, Tb: 216.65, pb: 22632.06, L: 0.0 },
          { hb: 20000, Tb: 216.65, pb: 5474.889, L: 0.001 },
          { hb: 32000, Tb: 228.65, pb: 868.019, L: 0.0028 },
          { hb: 47000, Tb: 270.65, pb: 110.906, L: 0.0 },
          { hb: 51000, Tb: 270.65, pb: 66.9389, L: -0.0028 },
          { hb: 71000, Tb: 214.65, pb: 3.95639, L: -0.002 },
        ];

        const g0 = 9.80665;
        const R = 287.05;

        let T, p;
        for (let i = layers.length - 1; i >= 0; i--) {
          const layer = layers[i];
          if (h >= layer.hb) {
            const { hb, Tb, pb, L } = layer;
            if (L === 0) {
              T = Tb;
              p = pb * Math.exp((-g0 * (h - hb)) / (R * Tb));
            } else {
              T = Tb + L * (h - hb);
              p = pb * Math.pow(T / Tb, -g0 / (L * R));
            }
            break;
          }
        }

        const rho = p / (287.05 * T);
        return { T, p, rho };
      }

      function formatNumber(value, decimals) {
        if (!isFinite(value)) return "–";
        if (
          Math.abs(value) >= 1e5 ||
          (Math.abs(value) <= 1e-2 && value !== 0)
        ) {
          return value.toExponential(decimals);
        }
        return value.toFixed(decimals);
      }

      function determineRegime(M) {
        if (!isFinite(M) || M <= 0)
          return { name: "–", color: "bg-gray-100 text-gray-700" };
        if (M < 0.8)
          return { name: "Subsonic", color: "bg-green-100 text-green-800" };
        if (M < 1.2)
          return { name: "Transonic", color: "bg-yellow-100 text-yellow-800" };
        if (M < 5)
          return { name: "Supersonic", color: "bg-blue-100 text-blue-800" };
        if (M < 10)
          return { name: "Hypersonic", color: "bg-purple-100 text-purple-800" };
        return { name: "High-hypersonic", color: "bg-red-100 text-red-800" };
      }

      document
        .getElementById("calculate-btn")
        .addEventListener("click", function () {
          const speedVal = parseFloat(
            document.getElementById("speed-value").value
          );
          const speedUnit = document.getElementById("speed-unit").value;
          const altVal = parseFloat(
            document.getElementById("altitude-value").value
          );
          const altUnit = document.getElementById("altitude-unit").value;
          const gammaVal = parseFloat(
            document.getElementById("gamma-value").value
          );
          const errorEl = document.getElementById("input-error");

          errorEl.classList.add("hidden");
          errorEl.textContent = "";

          if (!isFinite(speedVal) || speedVal <= 0) {
            errorEl.textContent = "Please enter a positive speed value.";
            errorEl.classList.remove("hidden");
            return;
          }
          if (!isFinite(altVal) || altVal < 0) {
            errorEl.textContent = "Please enter a non-negative altitude.";
            errorEl.classList.remove("hidden");
            return;
          }
          if (!isFinite(gammaVal) || gammaVal <= 1.0 || gammaVal > 2.0) {
            errorEl.textContent =
              "Please enter a realistic γ between 1.0 and 2.0.";
            errorEl.classList.remove("hidden");
            return;
          }

          // Convert speed to m/s
          let V;
          switch (speedUnit) {
            case "mps":
              V = speedVal;
              break;
            case "kmh":
              V = speedVal / 3.6;
              break;
            case "mph":
              V = speedVal * 0.44704;
              break;
            case "knots":
              V = speedVal * 0.514444;
              break;
            default:
              V = speedVal;
          }

          // Convert altitude to meters
          const h = altUnit === "ft" ? altVal * 0.3048 : altVal;

          if (h > 80000) {
            errorEl.textContent =
              "Altitude is above 80 km. Results may be unreliable with this simple atmosphere model.";
            errorEl.classList.remove("hidden");
          }

          const atm = standardAtmosphere(h);
          const T = atm.T;
          const p = atm.p;
          const rho = atm.rho;

          const R = 287.05;
          const a = Math.sqrt(gammaVal * R * T);
          const M = V / a;
          const q = 0.5 * rho * V * V;
          const T0 = T * (1 + ((gammaVal - 1) / 2) * M * M);
          const p0 =
            p *
            Math.pow(
              1 + ((gammaVal - 1) / 2) * M * M,
              gammaVal / (gammaVal - 1)
            );

          // Update outputs
          document.getElementById("mach-output").textContent = isFinite(M)
            ? M.toFixed(2)
            : "–";
          document.getElementById("a-output").textContent = formatNumber(a, 2);
          document.getElementById("rho-output").textContent = formatNumber(
            rho,
            4
          );
          document.getElementById("T-output").textContent = formatNumber(T, 1);
          document.getElementById("q-output").textContent = formatNumber(q, 1);
          document.getElementById("T0-output").textContent = formatNumber(
            T0,
            1
          );
          document.getElementById("p0-output").textContent = formatNumber(
            p0,
            1
          );

          const regime = determineRegime(M);
          const badge = document.getElementById("regime-badge");
          badge.textContent = "Regime: " + regime.name;
          badge.className = "result-badge " + regime.color;

          const machBar = document.getElementById("mach-bar");
          const Mclamped = Math.max(0, Math.min(25, M));
          machBar.style.width = ((Mclamped / 25) * 100).toFixed(1) + "%";

          const regimeText = document.getElementById("regime-text");
          if (regime.name === "–") {
            regimeText.textContent =
              "Enter values and click Calculate to see the flow regime.";
          } else {
            regimeText.textContent =
              "At Mach " +
              M.toFixed(2) +
              ", the flow is classified as " +
              regime.name +
              ".";
          }
        });
    </script>

