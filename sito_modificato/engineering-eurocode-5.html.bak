    <script>
      // Strength class database (simplified typical values)
      const strengthClasses = {
        C16: { fm: 16, ft0: 10, fc0: 17, fv: 2.0, E0: 8000, rho: 310 },
        C24: { fm: 24, ft0: 14, fc0: 21, fv: 2.5, E0: 11000, rho: 350 },
        C30: { fm: 30, ft0: 18, fc0: 24, fv: 3.0, E0: 12000, rho: 380 },
        GL24h: { fm: 24, ft0: 16, fc0: 24, fv: 3.5, E0: 11500, rho: 385 },
        GL28h: { fm: 28, ft0: 19, fc0: 26, fv: 3.5, E0: 13000, rho: 410 },
      };

      function updateStrengthClass() {
        const cls = document.getElementById("strengthClass").value;
        if (cls === "custom") return;
        const data = strengthClasses[cls];
        if (!data) return;
        document.getElementById("fk_bend").value = data.fm;
        document.getElementById("fk_tens").value = data.ft0;
        document.getElementById("fk_comp").value = data.fc0;
        document.getElementById("fk_shear").value = data.fv;
        document.getElementById("E0mean").value = data.E0;
        document.getElementById("rho_k").value = data.rho;
        // Adjust default gammaM for glulam
        const gammaInput = document.getElementById("gammaM");
        gammaInput.value = cls.startsWith("GL") ? 1.25 : 1.3;
      }

      function updateKmodKdef() {
        const sc = document.getElementById("serviceClass").value;
        const ld = document.getElementById("loadDuration").value;
        let kmod = 0.6,
          kdef = 0.6;

        // Very simplified mapping based on EC5 tables
        if (sc === "1") {
          if (ld === "permanent") kmod = 0.6;
          else if (ld === "long") kmod = 0.7;
          else if (ld === "medium") kmod = 0.8;
          else if (ld === "short") kmod = 0.9;
          else kmod = 1.1;
          kdef = 0.6;
        } else if (sc === "2") {
          if (ld === "permanent") kmod = 0.55;
          else if (ld === "long") kmod = 0.65;
          else if (ld === "medium") kmod = 0.7;
          else if (ld === "short") kmod = 0.8;
          else kmod = 0.9;
          kdef = 0.8;
        } else {
          // service class 3
          if (ld === "permanent") kmod = 0.45;
          else if (ld === "long") kmod = 0.5;
          else if (ld === "medium") kmod = 0.55;
          else if (ld === "short") kmod = 0.65;
          else kmod = 0.7;
          kdef = 2.0;
        }

        document.getElementById("kmod").value = kmod.toFixed(2);
        document.getElementById("kdef").value = kdef.toFixed(2);
      }

      function setBadge(el, value) {
        el.textContent = isFinite(value) ? value.toFixed(2) : "–";
        el.classList.remove("result-badge-ok", "result-badge-ng");
        if (!isFinite(value)) return;
        if (value <= 1.0) {
          el.classList.add("result-badge-ok");
        } else {
          el.classList.add("result-badge-ng");
        }
      }

      function calculateEC5() {
        const status = document.getElementById("statusText");
        status.textContent = "Calculating…";

        const memberType = document.getElementById("memberType").value;
        const b = parseFloat(document.getElementById("b").value); // mm
        const h = parseFloat(document.getElementById("h").value); // mm
        const Lm = parseFloat(document.getElementById("length").value); // m
        const L = Lm * 1000; // mm

        const fk_bend = parseFloat(document.getElementById("fk_bend").value);
        const fk_tens = parseFloat(document.getElementById("fk_tens").value);
        const fk_comp = parseFloat(document.getElementById("fk_comp").value);
        const fk_shear = parseFloat(document.getElementById("fk_shear").value);
        const E0 = parseFloat(document.getElementById("E0mean").value);

        const kmod = parseFloat(document.getElementById("kmod").value);
        const kdef = parseFloat(document.getElementById("kdef").value);
        const gammaM = parseFloat(document.getElementById("gammaM").value);

        const MEd_kNm = parseFloat(document.getElementById("MEd").value);
        const VEd_kN = parseFloat(document.getElementById("VEd").value);
        const NEd_kN = parseFloat(document.getElementById("NEd").value);
        const qk_kNm = parseFloat(document.getElementById("qk").value);

        if (
          [b, h, Lm, fk_bend, fk_shear, E0, kmod, gammaM].some(
            (v) => !isFinite(v) || v <= 0
          )
        ) {
          status.textContent =
            "Please enter valid positive values for geometry and material.";
          return;
        }

        // Section properties (mm, mm2, mm3, mm4)
        const A = b * h; // mm²
        const W = (b * h * h) / 6; // mm³
        const I = (b * Math.pow(h, 3)) / 12; // mm⁴

        // Design strengths
        const fmd = (kmod * fk_bend) / gammaM;
        const fvd = (kmod * fk_shear) / gammaM;
        const fcd = (kmod * fk_comp) / gammaM;
        const ftd = (kmod * fk_tens) / gammaM;

        // Resistances
        const MRd_Nmm = fmd * W;
        const VRd_N = fvd * A;
        const NRd_comp_N = fcd * A;
        const NRd_tens_N = ftd * A;

        // Design effects
        const MEd_Nmm = MEd_kNm * 1e6;
        const VEd_N = VEd_kN * 1e3;
        const NEd_N = NEd_kN * 1e3;

        const etaM = MRd_Nmm > 0 ? MEd_Nmm / MRd_Nmm : NaN;
        const etaV = VRd_N > 0 ? VEd_N / VRd_N : NaN;
        let etaN = NaN;
        if (NEd_N > 0 && NRd_comp_N > 0) {
          etaN = NEd_N / NRd_comp_N;
        } else if (NEd_N < 0 && NRd_tens_N > 0) {
          etaN = Math.abs(NEd_N) / NRd_tens_N;
        } else {
          etaN = 0;
        }

        const etaComb =
          (isFinite(etaM) ? etaM : 0) + (isFinite(etaN) ? etaN : 0);

        // Deflection for simply supported beam with uniform load (only if beam)
        let wInst_mm = NaN;
        if (memberType === "beam" && isFinite(qk_kNm) && qk_kNm > 0) {
          const q_Nmm = (qk_kNm * 1e3) / 1e3; // kN/m -> N/mm (1 kN/m = 1 N/mm)
          // w = 5 q L^4 / (384 E I)
          wInst_mm = (5 * q_Nmm * Math.pow(L, 4)) / (384 * E0 * I);
        }
        const wFin_mm = isFinite(wInst_mm) ? wInst_mm * (1 + kdef) : NaN;

        // Deflection limit
        const deflType = document.getElementById("deflLimitType").value;
        let wLimit_mm;
        if (deflType === "L300") wLimit_mm = L / 300;
        else if (deflType === "L250") wLimit_mm = L / 250;
        else if (deflType === "L200") wLimit_mm = L / 200;
        else
          wLimit_mm =
            parseFloat(document.getElementById("deflLimitCustom").value) || NaN;

        const etaDefl =
          isFinite(wFin_mm) && isFinite(wLimit_mm) && wLimit_mm > 0
            ? wFin_mm / wLimit_mm
            : NaN;

        // Update UI
        document.getElementById("resA").textContent = (A / 100).toFixed(1); // cm²
        document.getElementById("resW").textContent = (W / 1000).toFixed(1); // cm³
        document.getElementById("resI").textContent = (I / 1e4).toFixed(1); // cm⁴

        document.getElementById("resFmd").textContent = fmd.toFixed(2);
        document.getElementById("resFvd").textContent = fvd.toFixed(2);
        document.getElementById("resFcd").textContent = fcd.toFixed(2);

        setBadge(document.getElementById("resEtaM"), etaM);
        setBadge(document.getElementById("resEtaV"), etaV);
        setBadge(document.getElementById("resEtaN"), etaN);
        setBadge(document.getElementById("resEtaComb"), etaComb);
        setBadge(document.getElementById("resEtaDefl"), etaDefl);

        document.getElementById("resDeflInst").textContent = isFinite(wInst_mm)
          ? wInst_mm.toFixed(2)
          : "–";
        document.getElementById("resDeflFin").textContent = isFinite(wFin_mm)
          ? wFin_mm.toFixed(2)
          : "–";
        document.getElementById("resDeflLimit").textContent = isFinite(
          wLimit_mm
        )
          ? wLimit_mm.toFixed(1)
          : "–";

        let verdict = [];
        if (isFinite(etaM)) verdict.push(`Bending: η = ${etaM.toFixed(2)}`);
        if (isFinite(etaV)) verdict.push(`Shear: η = ${etaV.toFixed(2)}`);
        if (isFinite(etaN)) verdict.push(`Axial: η = ${etaN.toFixed(2)}`);
        if (isFinite(etaDefl))
          verdict.push(`Deflection: η = ${etaDefl.toFixed(2)}`);

        const maxEta = Math.max(
          isFinite(etaM) ? etaM : 0,
          isFinite(etaV) ? etaV : 0,
          isFinite(etaN) ? etaN : 0,
          isFinite(etaDefl) ? etaDefl : 0
        );

        const verdictEl = document.getElementById("resVerdict");
        if (maxEta <= 1 && maxEta > 0) {
          verdictEl.textContent = `All checks OK (max utilization η = ${maxEta.toFixed(
            2
          )} ≤ 1.0).`;
        } else if (maxEta > 1) {
          verdictEl.textContent = `Member fails at least one check (max utilization η = ${maxEta.toFixed(
            2
          )} > 1.0). Consider increasing section size, changing grade, or reducing span.`;
        } else {
          verdictEl.textContent =
            "Unable to compute a clear verdict. Check your inputs.";
        }

        status.textContent = "Done.";
      }

      function resetForm() {
        document.getElementById("memberType").value = "beam";
        document.getElementById("b").value = 100;
        document.getElementById("h").value = 300;
        document.getElementById("length").value = 4.0;
        document.getElementById("supportType").value = "simply";
        document.getElementById("strengthClass").value = "C24";
        document.getElementById("serviceClass").value = "2";
        document.getElementById("loadDuration").value = "medium";
        document.getElementById("gammaM").value = 1.3;
        updateStrengthClass();
        updateKmodKdef();
        document.getElementById("MEd").value = 30;
        document.getElementById("VEd").value = 25;
        document.getElementById("NEd").value = 0;
        document.getElementById("qk").value = 5;
        document.getElementById("deflLimitType").value = "L300";
        document.getElementById("deflLimitCustom").value = 15;
        document.getElementById("statusText").textContent = "";
        document.getElementById("resVerdict").textContent =
          "Run the calculation to see if the member passes Eurocode 5 checks.";
        ["resEtaM", "resEtaV", "resEtaN", "resEtaComb", "resEtaDefl"].forEach(
          (id) => {
            const el = document.getElementById(id);
            el.textContent = "–";
            el.classList.remove("result-badge-ok", "result-badge-ng");
          }
        );
        [
          "resA",
          "resW",
          "resI",
          "resFmd",
          "resFvd",
          "resFcd",
          "resDeflInst",
          "resDeflFin",
          "resDeflLimit",
        ].forEach((id) => {
          document.getElementById(id).textContent = "–";
        });
      }

      document.addEventListener("DOMContentLoaded", () => {
        document
          .getElementById("strengthClass")
          .addEventListener("change", updateStrengthClass);
        document
          .getElementById("serviceClass")
          .addEventListener("change", updateKmodKdef);
        document
          .getElementById("loadDuration")
          .addEventListener("change", updateKmodKdef);
        document
          .getElementById("calcBtn")
          .addEventListener("click", calculateEC5);
        document
          .getElementById("resetBtn")
          .addEventListener("click", resetForm);
        updateStrengthClass();
        updateKmodKdef();
      });

