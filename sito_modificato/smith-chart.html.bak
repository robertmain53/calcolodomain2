    <script>
      // Utility: format complex number a + jb
      function formatComplex(re, im, digits = 3) {
        if (!isFinite(re) || !isFinite(im)) return "–";
        const r = re.toFixed(digits);
        const i = Math.abs(im).toFixed(digits);
        const sign = im >= 0 ? "+" : "−";
        return r + " " + sign + " j" + i;
      }

      function computeSmith() {
        const z0 = parseFloat(document.getElementById("z0").value);
        const real = parseFloat(document.getElementById("realPart").value);
        const imag = parseFloat(document.getElementById("imagPart").value);
        const inputType = document.querySelector(
          'input[name="inputType"]:checked'
        ).value;

        if (!isFinite(z0) || z0 === 0 || !isFinite(real) || !isFinite(imag)) {
          return null;
        }

        let Z, Y;
        if (inputType === "impedance") {
          Z = { re: real, im: imag };
          const denom = Z.re * Z.re + Z.im * Z.im || 1e-30;
          Y = { re: Z.re / denom, im: -Z.im / denom };
        } else {
          Y = { re: real, im: imag };
          const denom = Y.re * Y.re + Y.im * Y.im || 1e-30;
          Z = { re: Y.re / denom, im: -Y.im / denom };
        }

        // Normalize
        const z = { re: Z.re / z0, im: Z.im / z0 };
        const denomZ = (z.re + 1) * (z.re + 1) + z.im * z.im || 1e-30;
        const gamma = {
          re: ((z.re - 1) * (z.re + 1) + z.im * z.im) / denomZ,
          im: (2 * z.im) / denomZ,
        };

        const yDenom = z.re * z.re + z.im * z.im || 1e-30;
        const y = { re: z.re / yDenom, im: -z.im / yDenom };

        const gammaMag = Math.sqrt(gamma.re * gamma.re + gamma.im * gamma.im);
        const vswr = gammaMag >= 1 ? Infinity : (1 + gammaMag) / (1 - gammaMag);
        const rl = gammaMag <= 0 ? Infinity : -20 * Math.log10(gammaMag);

        return { Z, Y, z, y, gamma, gammaMag, vswr, rl };
      }

      function updateDisplays(result) {
        if (!result) return;
        const { z, y, gamma, gammaMag, vswr, rl } = result;

        document.getElementById("normZDisplay").textContent = formatComplex(
          z.re,
          z.im
        );
        document.getElementById("normYDisplay").textContent = formatComplex(
          y.re,
          y.im
        );
        document.getElementById("gammaDisplay").textContent = formatComplex(
          gamma.re,
          gamma.im
        );

        document.getElementById("gammaMagDisplay").textContent = isFinite(
          gammaMag
        )
          ? gammaMag.toFixed(4)
          : "–";
        document.getElementById("vswrDisplay").textContent = isFinite(vswr)
          ? vswr.toFixed(3)
          : "∞";
        document.getElementById("rlDisplay").textContent = isFinite(rl)
          ? rl.toFixed(2)
          : "∞";
      }

      function plotCurrentPoint(result) {
        if (!result) return;
        const { gamma } = result;
        const pt = document.getElementById("currentPoint");
        pt.setAttribute("cx", gamma.re);
        pt.setAttribute("cy", -gamma.im); // SVG y-axis is down
        pt.setAttribute("visibility", "visible");
      }

      function addMarker(result) {
        if (!result) return;
        const { gamma, z } = result;
        const group = document.getElementById("markerGroup");
        const marker = document.createElementNS(
          "http://www.w3.org/2000/svg",
          "circle"
        );
        marker.setAttribute("cx", gamma.re);
        marker.setAttribute("cy", -gamma.im);
        marker.setAttribute("r", 0.02);
        marker.setAttribute("fill", "#10b981");
        marker.setAttribute("class", "smith-point match");
        marker.setAttribute("opacity", "0.9");
        marker.setAttribute("data-tooltip", formatComplex(z.re, z.im));
        group.appendChild(marker);
      }

      function clearMarkers() {
        const group = document.getElementById("markerGroup");
        while (group.firstChild) group.removeChild(group.firstChild);
      }

      document.addEventListener("DOMContentLoaded", function () {
        const realLabel = document.getElementById("realLabel");
        const imagLabel = document.getElementById("imagLabel");

        document.querySelectorAll('input[name="inputType"]').forEach((r) => {
          r.addEventListener("change", () => {
            if (r.checked) {
              if (r.value === "impedance") {
                realLabel.textContent = "R";
                imagLabel.textContent = "X";
              } else {
                realLabel.textContent = "G";
                imagLabel.textContent = "B";
              }
              const res = computeSmith();
              if (res) {
                updateDisplays(res);
                plotCurrentPoint(res);
              }
            }
          });
        });

        ["z0", "realPart", "imagPart"].forEach((id) => {
          document.getElementById(id).addEventListener("input", () => {
            const res = computeSmith();
            if (res) {
              updateDisplays(res);
              plotCurrentPoint(res);
            }
          });
        });

        document
          .getElementById("plotPointBtn")
          .addEventListener("click", () => {
            const res = computeSmith();
            if (res) {
              updateDisplays(res);
              plotCurrentPoint(res);
            }
          });

        document
          .getElementById("addMarkerBtn")
          .addEventListener("click", () => {
            const res = computeSmith();
            if (res) addMarker(res);
          });

        document
          .getElementById("clearMarkersBtn")
          .addEventListener("click", clearMarkers);

        // Initial compute
        const initial = computeSmith();
        if (initial) {
          updateDisplays(initial);
          plotCurrentPoint(initial);
        }
      });
    </script>

