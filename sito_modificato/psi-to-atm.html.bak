    <script>
      // Core conversion constants
      const ATM_PER_PSI = 1 / 14.6959; // ≈ 0.068046
      const PSI_PER_ATM = 14.6959;

      const psiInput = document.getElementById("psiInput");
      const atmInput = document.getElementById("atmInput");
      const swapBtn = document.getElementById("swapBtn");
      const clearBtn = document.getElementById("clearBtn");
      const resultSummary = document.getElementById("resultSummary");
      const tableBody = document.getElementById("psiAtmTableBody");

      function isValidNumber(value) {
        return value !== "" && !isNaN(value);
      }

      function formatNumber(value, decimals = 6) {
        if (!isFinite(value)) return "";
        const abs = Math.abs(value);
        if (abs !== 0 && (abs < 1e-4 || abs >= 1e6)) {
          return value.toExponential(4);
        }
        return value.toFixed(decimals).replace(/\.?0+$/, "");
      }

      function updateFromPsi() {
        const psiVal = psiInput.value.trim();
        if (!isValidNumber(psiVal)) {
          atmInput.value = "";
          resultSummary.classList.add("hidden");
          return;
        }
        const psi = parseFloat(psiVal);
        const atm = psi * ATM_PER_PSI;
        atmInput.value = formatNumber(atm, 6);
        showSummary("psi", psi, atm);
      }

      function updateFromAtm() {
        const atmVal = atmInput.value.trim();
        if (!isValidNumber(atmVal)) {
          psiInput.value = "";
          resultSummary.classList.add("hidden");
          return;
        }
        const atm = parseFloat(atmVal);
        const psi = atm * PSI_PER_ATM;
        psiInput.value = formatNumber(psi, 6);
        showSummary("atm", atm, psi);
      }

      function showSummary(fromUnit, fromVal, toVal) {
        let text = "";
        if (fromUnit === "psi") {
          text = `${formatNumber(fromVal, 6)} psi = ${formatNumber(
            toVal,
            6
          )} atm`;
        } else {
          text = `${formatNumber(fromVal, 6)} atm = ${formatNumber(
            toVal,
            6
          )} psi`;
        }
        resultSummary.textContent = text;
        resultSummary.classList.remove("hidden");
      }

      psiInput.addEventListener("input", function () {
        atmInput.removeEventListener("input", updateFromAtm);
        updateFromPsi();
        setTimeout(() => atmInput.addEventListener("input", updateFromAtm), 0);
      });

      atmInput.addEventListener("input", function () {
        psiInput.removeEventListener("input", updateFromPsi);
        updateFromAtm();
        setTimeout(() => psiInput.addEventListener("input", updateFromPsi), 0);
      });

      swapBtn.addEventListener("click", () => {
        const psiVal = psiInput.value;
        const atmVal = atmInput.value;
        psiInput.value = atmVal;
        atmInput.value = psiVal;
        if (isValidNumber(psiInput.value)) {
          updateFromPsi();
        } else if (isValidNumber(atmInput.value)) {
          updateFromAtm();
        } else {
          resultSummary.classList.add("hidden");
        }
      });

      clearBtn.addEventListener("click", () => {
        psiInput.value = "";
        atmInput.value = "";
        resultSummary.classList.add("hidden");
        psiInput.focus();
      });

      // Fill conversion table
      (function buildTable() {
        const baseValues = [
          1, 5, 10, 14.7, 20, 30, 40, 50, 60, 80, 100, 150, 200,
        ];
        const fragment = document.createDocumentFragment();
        baseValues.forEach((v) => {
          const tr = document.createElement("tr");
          const atm = v * ATM_PER_PSI;
          tr.innerHTML = `
        <td class="px-3 py-1.5 font-mono">${formatNumber(v, 4)}</td>
        <td class="px-3 py-1.5 font-mono">${formatNumber(atm, 4)}</td>
      `;
          fragment.appendChild(tr);
        });
        tableBody.appendChild(fragment);
      })();

      // Copy buttons for formulas
      document.querySelectorAll(".copy-btn").forEach((btn) => {
        btn.addEventListener("click", () => {
          const text = btn.getAttribute("data-copy");
          if (!text) return;
          navigator.clipboard
            .writeText(text)
            .then(() => {
              const original = btn.textContent;
              btn.textContent = "Copied!";
              btn.disabled = true;
              setTimeout(() => {
                btn.textContent = original;
                btn.disabled = false;
              }, 1200);
            })
            .catch(() => {});
        });
      });

      // Initialize with a common example
      psiInput.value = "14.7";
      updateFromPsi();
    </script>

