    <script>
      function fmtCurrency(value) {
        if (!isFinite(value)) return "$0";
        const abs = Math.abs(value);
        const sign = value < 0 ? "-" : "";
        return (
          sign +
          "$" +
          abs.toLocaleString(undefined, { maximumFractionDigits: 0 })
        );
      }

      function fmtPercent(value) {
        if (!isFinite(value)) return "0%";
        return value.toFixed(1).replace(/\.0$/, "") + "%";
      }

      function fmtNumber(value, decimals = 1) {
        if (!isFinite(value)) return "–";
        return value.toFixed(decimals).replace(/\.0$/, "");
      }

      function getNumber(id) {
        const el = document.getElementById(id);
        if (!el) return 0;
        const v = parseFloat(el.value);
        return isNaN(v) ? 0 : v;
      }

      function setText(id, text) {
        const el = document.getElementById(id);
        if (el) el.textContent = text;
      }

      function calculateBrrrr() {
        const purchasePrice = getNumber("purchasePrice");
        const purchaseClosingCosts = getNumber("purchaseClosingCosts");
        const purchaseLtv = getNumber("purchaseLtv") / 100;

        const rehabCosts = getNumber("rehabCosts");
        const holdingMonths = getNumber("holdingMonths");
        const monthlyHoldingCosts = getNumber("monthlyHoldingCosts");

        const monthlyRent = getNumber("monthlyRent");
        const vacancyRate = getNumber("vacancyRate") / 100;
        const operatingExpensesRate = getNumber("operatingExpensesRate") / 100;

        const arv = getNumber("afterRepairValue");
        const refiLtv = getNumber("refiLtv") / 100;
        const refiRate = getNumber("refiRate") / 100;
        const refiTermYears = getNumber("refiTerm");

        const errorEl = document.getElementById("calcError");
        if (
          purchasePrice <= 0 ||
          arv <= 0 ||
          monthlyRent < 0 ||
          refiTermYears <= 0
        ) {
          if (errorEl) {
            errorEl.classList.remove("hidden");
          }
          return;
        } else if (errorEl) {
          errorEl.classList.add("hidden");
        }

        const holdingCosts = holdingMonths * monthlyHoldingCosts;
        const totalProjectCost =
          purchasePrice + purchaseClosingCosts + rehabCosts + holdingCosts;
        const initialLoan = purchasePrice * purchaseLtv;
        const cashInvested = totalProjectCost - initialLoan;

        const equityAtArv = arv - initialLoan;

        const maxRefiLoan = arv * refiLtv;
        const cashOutRefi = Math.max(0, maxRefiLoan - initialLoan);
        const cashLeftInDeal = cashInvested - cashOutRefi;

        const monthlyRate = refiRate / 12;
        const nPayments = refiTermYears * 12;
        let monthlyRefiPayment = 0;
        if (monthlyRate > 0 && maxRefiLoan > 0 && nPayments > 0) {
          monthlyRefiPayment =
            maxRefiLoan *
            (monthlyRate / (1 - Math.pow(1 + monthlyRate, -nPayments)));
        } else if (nPayments > 0) {
          monthlyRefiPayment = maxRefiLoan / nPayments;
        }

        const vacancyAmount = monthlyRent * vacancyRate;
        const operatingExpenses = monthlyRent * operatingExpensesRate;
        const noiMonthly = monthlyRent - vacancyAmount - operatingExpenses;
        const monthlyCashFlow = noiMonthly - monthlyRefiPayment;
        const annualCashFlow = monthlyCashFlow * 12;

        let cocReturn = 0;
        let equityMultiple = NaN;
        let paybackPeriod = NaN;

        if (cashLeftInDeal > 0) {
          cocReturn = (annualCashFlow / cashLeftInDeal) * 100;
          equityMultiple = equityAtArv / cashLeftInDeal;
          if (annualCashFlow > 0) {
            paybackPeriod = cashLeftInDeal / annualCashFlow;
          }
        }

        setText("totalProjectCost", fmtCurrency(totalProjectCost));
        setText("initialLoan", fmtCurrency(initialLoan));
        setText("cashInvested", fmtCurrency(cashInvested));
        setText("arvDisplay", fmtCurrency(arv));
        setText("equityAtArv", fmtCurrency(equityAtArv));

        setText("maxRefiLoan", fmtCurrency(maxRefiLoan));
        setText("cashOutRefi", fmtCurrency(cashOutRefi));
        setText("cashLeftInDeal", fmtCurrency(cashLeftInDeal));
        setText("monthlyRefiPayment", fmtCurrency(monthlyRefiPayment));

        setText("grossRent", fmtCurrency(monthlyRent));
        setText("vacancyAmount", fmtCurrency(vacancyAmount));
        setText("operatingExpenses", fmtCurrency(operatingExpenses));
        setText("noiMonthly", fmtCurrency(noiMonthly));
        setText("monthlyCashFlow", fmtCurrency(monthlyCashFlow));

        setText("annualCashFlow", fmtCurrency(annualCashFlow));
        setText(
          "cocReturn",
          isFinite(cocReturn) ? fmtPercent(cocReturn) : "0%"
        );
        setText(
          "equityMultiple",
          isFinite(equityMultiple) ? fmtNumber(equityMultiple, 2) + "×" : "–"
        );
        setText(
          "paybackPeriod",
          isFinite(paybackPeriod) ? fmtNumber(paybackPeriod, 1) + " yrs" : "–"
        );
      }

      function resetBrrrr() {
        document.getElementById("purchasePrice").value = 150000;
        document.getElementById("purchaseClosingCosts").value = 5000;
        document.getElementById("purchaseLtv").value = 80;

        document.getElementById("rehabCosts").value = 30000;
        document.getElementById("holdingMonths").value = 6;
        document.getElementById("monthlyHoldingCosts").value = 800;

        document.getElementById("monthlyRent").value = 1800;
        document.getElementById("vacancyRate").value = 5;
        document.getElementById("operatingExpensesRate").value = 35;

        document.getElementById("afterRepairValue").value = 240000;
        document.getElementById("refiLtv").value = 75;
        document.getElementById("refiRate").value = 6.5;
        document.getElementById("refiTerm").value = 30;

        const errorEl = document.getElementById("calcError");
        if (errorEl) errorEl.classList.add("hidden");

        calculateBrrrr();
      }

      document.addEventListener("DOMContentLoaded", function () {
        const calcBtn = document.getElementById("calculateBtn");
        const resetBtn = document.getElementById("resetBtn");

        if (calcBtn) calcBtn.addEventListener("click", calculateBrrrr);
        if (resetBtn) resetBtn.addEventListener("click", resetBrrrr);

        // Auto-calc on load
        calculateBrrrr();
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
