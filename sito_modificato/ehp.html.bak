    <script>
      // Mobile menu toggle
      document
        .getElementById("mobile-menu-toggle")
        ?.addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          if (!menu) return;
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          this.setAttribute("aria-expanded", String(isHidden));
        });

      function clamp(value, min, max) {
        return Math.min(Math.max(value, min), max);
      }

      function formatNumber(n) {
        if (!isFinite(n)) return "–";
        if (n === 0) return "0";
        if (Math.abs(n) >= 1000000) return n.toExponential(2);
        if (Math.abs(n) >= 1000)
          return n.toLocaleString(undefined, { maximumFractionDigits: 0 });
        return n.toLocaleString(undefined, { maximumFractionDigits: 2 });
      }

      function calculateEHP() {
        const health = Math.max(
          1,
          Number(document.getElementById("health").value) || 1
        );
        const armor = Math.max(
          0,
          Number(document.getElementById("armor").value) || 0
        );
        const physResPct = clamp(
          Number(document.getElementById("physRes").value) || 0,
          0,
          95
        );
        const elemResPct = clamp(
          Number(document.getElementById("elemRes").value) || 0,
          0,
          95
        );
        const flatDR = Math.max(
          0,
          Number(document.getElementById("flatDR").value) || 0
        );
        const extraDRPct = clamp(
          Number(document.getElementById("percentDR").value) || 0,
          0,
          95
        );

        const hitSize = Math.max(
          1,
          Number(document.getElementById("hitSize").value) || 1
        );
        let physShare = Number(document.getElementById("physShare").value) || 0;
        let elemShare = Number(document.getElementById("elemShare").value) || 0;
        const armorK = Math.max(
          1,
          Number(document.getElementById("armorK").value) || 100
        );

        // Normalize shares
        let totalShare = physShare + elemShare;
        if (totalShare <= 0) {
          physShare = 1;
          elemShare = 0;
          totalShare = 1;
        }
        const pPhys = physShare / totalShare;
        const pElem = elemShare / totalShare;

        // Armor DR
        const armorDR = armor > 0 ? armor / (armor + armorK) : 0;

        const physResDR = physResPct / 100;
        const elemResDR = elemResPct / 100;
        const extraDR = extraDRPct / 100;

        // Physical total DR
        const physMultBeforeExtra = (1 - armorDR) * (1 - physResDR);
        const physMultAfterExtra = physMultBeforeExtra * (1 - extraDR);
        let physDRTotal = 1 - physMultAfterExtra;
        physDRTotal = clamp(physDRTotal, 0, 0.99);

        // Elemental total DR
        const elemMultBeforeExtra = 1 - elemResDR;
        const elemMultAfterExtra = elemMultBeforeExtra * (1 - extraDR);
        let elemDRTotal = 1 - elemMultAfterExtra;
        elemDRTotal = clamp(elemDRTotal, 0, 0.99);

        // Damage per type
        const rawPhys = hitSize * pPhys;
        const rawElem = hitSize * pElem;

        let takenPhys = rawPhys * (1 - physDRTotal);
        let takenElem = rawElem * (1 - elemDRTotal);

        // Apply flat DR per hit to total
        let totalTaken = takenPhys + takenElem - flatDR;
        if (totalTaken < 0) totalTaken = 0;

        const totalRaw = hitSize;
        const overallMult = totalRaw > 0 ? totalTaken / totalRaw : 1;
        const overallDR = 1 - overallMult;

        const ehpOverall = overallMult > 0 ? health / overallMult : Infinity;
        const hitsToDie = totalTaken > 0 ? health / totalTaken : Infinity;

        // Per-type EHP ignoring flat DR (to keep it interpretable)
        const physMultNoFlat = 1 - physDRTotal;
        const elemMultNoFlat = 1 - elemDRTotal;
        const ehpPhys = physMultNoFlat > 0 ? health / physMultNoFlat : Infinity;
        const ehpElem = elemMultNoFlat > 0 ? health / elemMultNoFlat : Infinity;

        document.getElementById("ehpOverall").textContent =
          formatNumber(ehpOverall);
        document.getElementById("mitigationOverall").textContent = isFinite(
          overallDR
        )
          ? (overallDR * 100).toFixed(1) + "%"
          : "100%+";
        document.getElementById("hitsToDie").textContent = isFinite(hitsToDie)
          ? formatNumber(hitsToDie)
          : "Immortal (for this hit size)";
        document.getElementById("ehpPerType").textContent =
          "Physical EHP: " +
          formatNumber(ehpPhys) +
          " | Elemental EHP: " +
          formatNumber(ehpElem);
      }

      document
        .getElementById("calcBtn")
        ?.addEventListener("click", calculateEHP);

      document.getElementById("resetBtn")?.addEventListener("click", () => {
        document.getElementById("health").value = 1000;
        document.getElementById("armor").value = 0;
        document.getElementById("physRes").value = 0;
        document.getElementById("elemRes").value = 0;
        document.getElementById("flatDR").value = 0;
        document.getElementById("percentDR").value = 0;
        document.getElementById("hitSize").value = 500;
        document.getElementById("physShare").value = 70;
        document.getElementById("elemShare").value = 30;
        document.getElementById("armorK").value = 100;
        document.getElementById("ehpOverall").textContent = "–";
        document.getElementById("mitigationOverall").textContent = "–";
        document.getElementById("hitsToDie").textContent = "–";
        document.getElementById("ehpPerType").textContent = "–";
      });

      // Auto-calc on load
      window.addEventListener("DOMContentLoaded", calculateEHP);
    </script>

