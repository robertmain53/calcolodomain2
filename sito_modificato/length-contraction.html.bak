    <script>
      // Physical constant
      const C = 299792458; // m/s

      const modeTabs = document.querySelectorAll(".mode-tab");
      const fieldL0 = document.getElementById("field-L0");
      const fieldL = document.getElementById("field-L");
      const fieldV = document.getElementById("field-v");

      const inputL0 = document.getElementById("L0");
      const unitL0 = document.getElementById("L0-unit");
      const inputL = document.getElementById("L");
      const unitL = document.getElementById("L-unit");
      const inputBeta = document.getElementById("beta");
      const sliderBeta = document.getElementById("beta-slider");
      const sliderBetaValue = document.getElementById("beta-slider-value");
      const vSi = document.getElementById("v-si");

      const resultsSection = document.getElementById("results-section");
      const gammaResult = document.getElementById("gamma-result");
      const gammaNote = document.getElementById("gamma-note");
      const ratioResult = document.getElementById("ratio-result");
      const ratioNote = document.getElementById("ratio-note");
      const L0Display = document.getElementById("L0-display");
      const LDisplay = document.getElementById("L-display");
      const percentDisplay = document.getElementById("percent-display");

      const calcBtn = document.getElementById("calculate-btn");
      const resetBtn = document.getElementById("reset-btn");

      let currentMode = "L"; // 'L', 'L0', 'v'

      function setMode(mode) {
        currentMode = mode;
        modeTabs.forEach((tab) => {
          if (tab.dataset.mode === mode) {
            tab.classList.add("border-blue-600", "text-blue-600");
            tab.classList.remove("border-transparent", "text-gray-600");
          } else {
            tab.classList.remove("border-blue-600", "text-blue-600");
            tab.classList.add("border-transparent", "text-gray-600");
          }
        });

        // Show/hide fields depending on mode
        if (mode === "L") {
          fieldL0.classList.remove("opacity-50");
          fieldL0.querySelector("input").disabled = false;
          fieldL.classList.add("opacity-50");
          fieldL.querySelector("input").disabled = true;
          fieldV.classList.remove("opacity-50");
          inputBeta.disabled = false;
          sliderBeta.disabled = false;
        } else if (mode === "L0") {
          fieldL0.classList.add("opacity-50");
          fieldL0.querySelector("input").disabled = true;
          fieldL.classList.remove("opacity-50");
          fieldL.querySelector("input").disabled = false;
          fieldV.classList.remove("opacity-50");
          inputBeta.disabled = false;
          sliderBeta.disabled = false;
        } else if (mode === "v") {
          fieldL0.classList.remove("opacity-50");
          fieldL0.querySelector("input").disabled = false;
          fieldL.classList.remove("opacity-50");
          fieldL.querySelector("input").disabled = false;
          fieldV.classList.add("opacity-50");
          inputBeta.disabled = true;
          sliderBeta.disabled = true;
        }
      }

      modeTabs.forEach((tab) => {
        tab.addEventListener("click", () => {
          setMode(tab.dataset.mode);
          resultsSection.classList.add("hidden");
        });
      });

      function updateVDisplay(beta) {
        if (beta < 0) beta = 0;
        if (beta >= 1) beta = 0.999999;
        const v = beta * C;
        const sci = v.toExponential(2);
        sliderBetaValue.textContent = beta.toFixed(2) + " c";
        vSi.innerHTML =
          "v ≈ " +
          beta.toFixed(2) +
          " c ≈ " +
          sci.replace("e", " × 10^") +
          " m/s";
      }

      sliderBeta.addEventListener("input", () => {
        const beta = parseFloat(sliderBeta.value);
        inputBeta.value = beta.toFixed(2);
        updateVDisplay(beta);
      });

      inputBeta.addEventListener("input", () => {
        let beta = parseFloat(inputBeta.value);
        if (isNaN(beta)) return;
        if (beta < 0) beta = 0;
        if (beta > 0.99) beta = 0.99;
        sliderBeta.value = beta;
        updateVDisplay(beta);
      });

      function unitToMetersFactor(unit) {
        switch (unit) {
          case "km":
            return 1000;
          case "cm":
            return 0.01;
          case "ft":
            return 0.3048;
          case "ly":
            return 9.4607e15;
          case "m":
          default:
            return 1;
        }
      }

      function formatNumber(x, digits = 4) {
        if (!isFinite(x)) return "—";
        const abs = Math.abs(x);
        if (abs !== 0 && (abs < 1e-3 || abs >= 1e5)) {
          return x.toExponential(3);
        }
        return parseFloat(x.toFixed(digits)).toString();
      }

      function calculate() {
        let beta, L0_m, L_m;

        if (currentMode === "L" || currentMode === "L0") {
          beta = parseFloat(inputBeta.value);
          if (!isFinite(beta) || beta < 0 || beta >= 1) {
            alert(
              "Please enter a speed v as a fraction of c between 0 and 1 (not including 1)."
            );
            return;
          }
        }

        if (currentMode === "L" || currentMode === "v") {
          const L0_val = parseFloat(inputL0.value);
          if (!isFinite(L0_val) || L0_val <= 0) {
            alert("Please enter a positive proper length L₀.");
            return;
          }
          L0_m = L0_val * unitToMetersFactor(unitL0.value);
        }

        if (currentMode === "L0" || currentMode === "v") {
          const L_val = parseFloat(inputL.value);
          if (!isFinite(L_val) || L_val <= 0) {
            alert("Please enter a positive contracted length L.");
            return;
          }
          L_m = L_val * unitToMetersFactor(unitL.value);
        }

        let gamma, ratio;

        if (currentMode === "L") {
          const oneMinusB2 = 1 - beta * beta;
          if (oneMinusB2 <= 0) {
            alert("Speed must be less than c.");
            return;
          }
          const sqrtTerm = Math.sqrt(oneMinusB2);
          L_m = L0_m * sqrtTerm;
          gamma = 1 / sqrtTerm;
          ratio = L_m / L0_m;
          // write back in chosen units
          const L_unitFactor = unitToMetersFactor(unitL.value);
          inputL.value = (L_m / L_unitFactor).toPrecision(6);
        } else if (currentMode === "L0") {
          const oneMinusB2 = 1 - beta * beta;
          if (oneMinusB2 <= 0) {
            alert("Speed must be less than c.");
            return;
          }
          const sqrtTerm = Math.sqrt(oneMinusB2);
          L0_m = L_m / sqrtTerm;
          gamma = 1 / sqrtTerm;
          ratio = L_m / L0_m;
          const L0_unitFactor = unitToMetersFactor(unitL0.value);
          inputL0.value = (L0_m / L0_unitFactor).toPrecision(6);
        } else if (currentMode === "v") {
          // solve for beta from L and L0
          const r = L_m / L0_m;
          if (r <= 0 || r > 1) {
            alert(
              "For physical length contraction, L must be less than or equal to L₀ and greater than 0."
            );
            return;
          }
          const betaSq = 1 - r * r;
          if (betaSq < 0) {
            alert("Invalid combination of L and L₀.");
            return;
          }
          beta = Math.sqrt(betaSq);
          sliderBeta.value = beta;
          inputBeta.value = beta.toFixed(4);
          updateVDisplay(beta);
          const oneMinusB2 = 1 - beta * beta;
          const sqrtTerm = Math.sqrt(oneMinusB2);
          gamma = 1 / sqrtTerm;
          ratio = r;
        }

        // Update results
        gammaResult.textContent = formatNumber(gamma, 4);
        gammaNote.textContent =
          gamma > 1.01
            ? "γ > 1 indicates significant relativistic effects."
            : "γ is very close to 1; relativistic effects are tiny.";

        ratioResult.textContent = formatNumber(ratio, 4);
        ratioNote.textContent =
          "L / L₀ = " + formatNumber(ratio * 100, 2) + "% of the rest length.";

        // Display lengths in chosen units
        const L0_unitFactor = unitToMetersFactor(unitL0.value);
        const L_unitFactor = unitToMetersFactor(unitL.value);
        const L0_user = L0_m / L0_unitFactor;
        const L_user = L_m / L_unitFactor;

        L0Display.innerHTML = formatNumber(L0_user, 4) + " " + unitL0.value;
        LDisplay.innerHTML = formatNumber(L_user, 4) + " " + unitL.value;
        const percent = (L_m / L0_m) * 100;
        percentDisplay.textContent =
          formatNumber(percent, 2) + "% of the proper length";

        resultsSection.classList.remove("hidden");

        // Update chart marker
        if (window.contractionChart) {
          window.contractionChart.data.datasets[1].data = [
            { x: beta, y: ratio },
          ];
          window.contractionChart.update();
        }
      }

      calcBtn.addEventListener("click", calculate);

      resetBtn.addEventListener("click", () => {
        inputL0.value = "";
        inputL.value = "";
        unitL0.value = "m";
        unitL.value = "m";
        sliderBeta.value = 0.8;
        inputBeta.value = "0.8";
        updateVDisplay(0.8);
        resultsSection.classList.add("hidden");
        if (window.contractionChart) {
          window.contractionChart.data.datasets[1].data = [];
          window.contractionChart.update();
        }
      });

      // Initialize defaults
      setMode("L");
      inputBeta.value = "0.8";
      updateVDisplay(0.8);

      // Chart.js: contraction factor vs beta
      const ctx = document.getElementById("contraction-chart").getContext("2d");
      const betaValues = [];
      const ratioValues = [];
      for (let b = 0; b <= 0.99; b += 0.01) {
        betaValues.push(parseFloat(b.toFixed(2)));
        ratioValues.push(Math.sqrt(1 - b * b));
      }

      window.contractionChart = new Chart(ctx, {
        type: "line",
        data: {
          datasets: [
            {
              label: "L / L₀",
              data: betaValues.map((b, i) => ({ x: b, y: ratioValues[i] })),
              borderColor: "#2563eb",
              backgroundColor: "rgba(37, 99, 235, 0.1)",
              tension: 0.15,
              pointRadius: 0,
              borderWidth: 2,
            },
            {
              label: "Your scenario",
              data: [],
              type: "scatter",
              pointRadius: 5,
              pointBackgroundColor: "#dc2626",
              pointBorderColor: "#b91c1c",
              showLine: false,
            },
          ],
        },
        options: {
          responsive: true,
          maintainAspectRatio: false,
          scales: {
            x: {
              type: "linear",
              title: { display: true, text: "Speed v / c" },
              min: 0,
              max: 1,
              ticks: { stepSize: 0.1 },
            },
            y: {
              title: { display: true, text: "Contraction factor L / L₀" },
              min: 0,
              max: 1,
              ticks: { stepSize: 0.1 },
            },
          },
          plugins: {
            legend: { display: true },
          },
        },
      });
    </script>
  
