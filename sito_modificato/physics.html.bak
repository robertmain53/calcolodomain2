          document
            .querySelectorAll(".topic-panel")
            .forEach((panel) => panel.classList.add("hidden"));
          const active = document.getElementById("topic-" + topic);
          if (active) active.classList.remove("hidden");
        });
      });

      function toNumber(value) {
        const v = parseFloat(value);
        return isNaN(v) ? null : v;
      }

      // Unit helpers
      function convertLengthToMeters(value, unit) {
        if (unit === "m") return value;
        if (unit === "km") return value * 1000;
        if (unit === "cm") return value / 100;
        return value;
      }
      function convertMetersToLength(value, unit) {
        if (unit === "m") return value;
        if (unit === "km") return value / 1000;
        if (unit === "cm") return value * 100;
        return value;
      }
      function convertSpeedToMS(value, unit) {
        if (unit === "m/s") return value;
        if (unit === "km/h") return value * (1000 / 3600);
        return value;
      }
      function convertMSToSpeed(value, unit) {
        if (unit === "m/s") return value;
        if (unit === "km/h") return value * (3600 / 1000);
        return value;
      }
      function convertTimeToSeconds(value, unit) {
        if (unit === "s") return value;
        if (unit === "min") return value * 60;
        if (unit === "h") return value * 3600;
        if (unit === "ms") return value / 1000;
        return value;
      }
      function convertSecondsToTime(value, unit) {
        if (unit === "s") return value;
        if (unit === "min") return value / 60;
        if (unit === "h") return value / 3600;
        if (unit === "ms") return value * 1000;
        return value;
      }
      function convertAccelToMS2(value, unit) {
        if (unit === "m/s2") return value;
        if (unit === "km/h2") return value * (1000 / 3600 / 3600);
        return value;
      }
      function convertMS2ToAccel(value, unit) {
        if (unit === "m/s2") return value;
        if (unit === "km/h2") return value * ((3600 * 3600) / 1000);
        return value;
      }
      function convertMassToKg(value, unit) {
        if (unit === "kg") return value;
        if (unit === "g") return value / 1000;
        return value;
      }
      function convertKgToMass(value, unit) {
        if (unit === "kg") return value;
        if (unit === "g") return value * 1000;
        return value;
      }
      function convertForceToN(value, unit) {
        if (unit === "N") return value;
        if (unit === "kN") return value * 1000;
        return value;
      }
      function convertNToForce(value, unit) {
        if (unit === "N") return value;
        if (unit === "kN") return value / 1000;
        return value;
      }
      function convertEnergyToJ(value, unit) {
        if (unit === "J") return value;
        if (unit === "kJ") return value * 1000;
        return value;
      }
      function convertJToEnergy(value, unit) {
        if (unit === "J") return value;
        if (unit === "kJ") return value / 1000;
        return value;
      }

      // Kinematics
      document.getElementById("kin-calc").addEventListener("click", () => {
        const solveFor = document.getElementById("kin-solve-for").value;
        const eq = document.getElementById("kin-equation").value;

        const s0Val = toNumber(document.getElementById("kin-s0").value) || 0;
        const sUnit = document.getElementById("kin-s-unit").value;
        const v0Val = toNumber(document.getElementById("kin-v0").value);
        const vUnit = document.getElementById("kin-v-unit").value;
        const tVal = toNumber(document.getElementById("kin-t").value);
        const tUnit = document.getElementById("kin-t-unit").value;
        const aVal = toNumber(document.getElementById("kin-a").value);
        const aUnit = document.getElementById("kin-a-unit").value;

        const s0 = convertLengthToMeters(s0Val, sUnit);
        const v0 = v0Val != null ? convertSpeedToMS(v0Val, vUnit) : null;
        const t = tVal != null ? convertTimeToSeconds(tVal, tUnit) : null;
        const a = aVal != null ? convertAccelToMS2(aVal, aUnit) : null;

        let resultText = "";
        let formulaDisplay = "";
        let steps = "";

        try {
          if (eq === "1") {
            // v = v0 + a t
            if (solveFor === "v") {
              if (v0 == null || a == null || t == null)
                throw new Error("Enter v₀, a, and t.");
              const v = v0 + a * t;
              const vOut = convertMSToSpeed(v, vUnit);
              resultText = `Final velocity v = ${vOut.toFixed(3)} ${vUnit}`;
              formulaDisplay = "v = v₀ + a t";
              steps = `v = ${v0.toFixed(3)} + ${a.toFixed(3)} · ${t.toFixed(
                3
              )} = ${v.toFixed(3)} m/s`;
            } else if (solveFor === "t") {
              if (v0 == null || a == null || tVal == null) {
                // here t input is used as v, to allow solving for t; keep UI simple: require v in "Time t" field when solving for t
                throw new Error(
                  "To solve for t with equation v = v₀ + a t, enter v in the Time field and provide v₀ and a."
                );
              }
            } else if (solveFor === "a") {
              if (v0 == null || aVal == null || t == null) {
                throw new Error(
                  "To solve for a, enter v in the Acceleration field, plus v₀ and t."
                );
              }
            } else if (solveFor === "s") {
              throw new Error(
                "Equation 1 does not include s. Choose equation 2 or 3 to solve for displacement."
              );
            }
          } else if (eq === "2") {
            // s = s0 + v0 t + 1/2 a t^2
            if (solveFor === "s") {
              if (v0 == null || a == null || t == null)
                throw new Error("Enter v₀, a, and t.");
              const s = s0 + v0 * t + 0.5 * a * t * t;
              const sOut = convertMetersToLength(s, sUnit);
              resultText = `Final position s = ${sOut.toFixed(3)} ${sUnit}`;
              formulaDisplay = "s = s₀ + v₀ t + ½ a t²";
              steps = `s = ${s0.toFixed(3)} + ${v0.toFixed(3)}·${t.toFixed(
                3
              )} + 0.5·${a.toFixed(3)}·${t.toFixed(3)}² = ${s.toFixed(3)} m`;
            } else {
              throw new Error(
                "For this simplified interface, equation 2 only solves for s. Use equation 1 or 3 for other variables."
              );
            }
          } else if (eq === "3") {
            // v^2 = v0^2 + 2 a (s - s0)
            if (solveFor === "v") {
              if (v0 == null || a == null || tVal == null) {
                throw new Error(
                  "To solve for v with equation 3, enter s in the Time field (interpreted as s), plus v₀ and a."
                );
              }
            } else {
              throw new Error(
                "For this simplified interface, equation 3 is mainly for solving v given s, v₀, a (not fully exposed here)."
              );
            }
          }

          const resEl = document.getElementById("kin-result");
          const textEl = document.getElementById("kin-result-text");
          const fEl = document.getElementById("kin-formula-display");
          const sEl = document.getElementById("kin-formula-steps");

          if (!resultText) {
            textEl.textContent =
              "Currently this configuration is limited. Try solving for v with equation 1 or s with equation 2.";
          } else {
            textEl.textContent = resultText;
          }
          fEl.textContent = formulaDisplay;
          sEl.textContent = steps;
          resEl.classList.remove("hidden");
        } catch (e) {
          alert(e.message);
        }
      });

      // Newton's second law
      document.getElementById("n2-calc").addEventListener("click", () => {
        const solveFor = document.getElementById("n2-solve-for").value;
        const mVal = toNumber(document.getElementById("n2-m").value);
        const mUnit = document.getElementById("n2-m-unit").value;
        const aVal = toNumber(document.getElementById("n2-a").value);
        const aUnit = document.getElementById("n2-a-unit").value;
        const FVal = toNumber(document.getElementById("n2-F").value);
        const FUnit = document.getElementById("n2-F-unit").value;

        const m = mVal != null ? convertMassToKg(mVal, mUnit) : null;
        const a = aVal != null ? convertAccelToMS2(aVal, aUnit) : null;
        const F = FVal != null ? convertForceToN(FVal, FUnit) : null;

        let resultText = "";
        let formulaDisplay = "F = m a";
        let steps = "";

        try {
          if (solveFor === "F") {
            if (m == null || a == null)
              throw new Error("Enter mass and acceleration.");
            const Fcalc = m * a;
            const Fout = convertNToForce(Fcalc, FUnit);
            resultText = `Force F = ${Fout.toFixed(3)} ${FUnit}`;
            steps = `F = ${m.toFixed(3)} · ${a.toFixed(3)} = ${Fcalc.toFixed(
              3
            )} N`;
          } else if (solveFor === "m") {
            if (F == null || a == null)
              throw new Error("Enter force and acceleration.");
            const mcalc = F / a;
            const mout = convertKgToMass(mcalc, mUnit);
            resultText = `Mass m = ${mout.toFixed(3)} ${mUnit}`;
            steps = `m = F / a = ${F.toFixed(3)} / ${a.toFixed(
              3
            )} = ${mcalc.toFixed(3)} kg`;
          } else if (solveFor === "a") {
            if (F == null || m == null)
              throw new Error("Enter force and mass.");
            const acalc = F / m;
            const aout = convertMS2ToAccel(acalc, aUnit);
            resultText = `Acceleration a = ${aout.toFixed(3)} ${aUnit}`;
            steps = `a = F / m = ${F.toFixed(3)} / ${m.toFixed(
              3
            )} = ${acalc.toFixed(3)} m/s²`;
          }

          document.getElementById("n2-result-text").textContent = resultText;
          document.getElementById("n2-formula-display").textContent =
            formulaDisplay;
          document.getElementById("n2-formula-steps").textContent = steps;
          document.getElementById("n2-result").classList.remove("hidden");
        } catch (e) {
          alert(e.message);
        }
      });

      // Work
      document.getElementById("work-calc").addEventListener("click", () => {
        const FVal = toNumber(document.getElementById("work-F").value);
        const FUnit = document.getElementById("work-F-unit").value;
        const dVal = toNumber(document.getElementById("work-d").value);
        const dUnit = document.getElementById("work-d-unit").value;
        const thetaDeg =
          toNumber(document.getElementById("work-theta").value) || 0;

        try {
          if (FVal == null || dVal == null)
            throw new Error("Enter force and displacement.");
          const F = convertForceToN(FVal, FUnit);
          const d = convertLengthToMeters(dVal, dUnit);
          const thetaRad = (thetaDeg * Math.PI) / 180;
          const W = F * d * Math.cos(thetaRad);
          const WkJ = convertJToEnergy(W, "kJ");

          document.getElementById(
            "work-result-text"
          ).textContent = `Work W = ${W.toFixed(3)} J (${WkJ.toFixed(3)} kJ)`;
          document.getElementById("work-formula-display").textContent =
            "W = F d cos(θ)";
          document.getElementById(
            "work-formula-steps"
          ).textContent = `W = ${F.toFixed(3)} · ${d.toFixed(
            3
          )} · cos(${thetaDeg.toFixed(3)}°) = ${W.toFixed(3)} J`;
          document.getElementById("work-result").classList.remove("hidden");
        } catch (e) {
          alert(e.message);
        }
      });

      // Kinetic energy
      document.getElementById("ke-calc").addEventListener("click", () => {
        const solveFor = document.getElementById("ke-solve-for").value;
        const mVal = toNumber(document.getElementById("ke-m").value);
        const mUnit = document.getElementById("ke-m-unit").value;
        const vVal = toNumber(document.getElementById("ke-v").value);
        const vUnit = document.getElementById("ke-v-unit").value;

        const m = mVal != null ? convertMassToKg(mVal, mUnit) : null;
        const v = vVal != null ? convertSpeedToMS(vVal, vUnit) : null;

        let resultText = "";
        let formulaDisplay = "K = ½ m v²";
        let steps = "";

        try {
          if (solveFor === "K") {
            if (m == null || v == null)
              throw new Error("Enter mass and speed.");
            const K = 0.5 * m * v * v;
            const KkJ = convertJToEnergy(K, "kJ");
            resultText = `Kinetic energy K = ${K.toFixed(3)} J (${KkJ.toFixed(
              3
            )} kJ)`;
            steps = `K = 0.5 · ${m.toFixed(3)} · ${v.toFixed(3)}² = ${K.toFixed(
              3
            )} J`;
          } else if (solveFor === "m") {
            if (v == null || mVal == null)
              throw new Error(
                "To solve for m, enter K in the Mass field (J) and v in the Speed field."
              );
          } else if (solveFor === "v") {
            if (m == null || vVal == null)
              throw new Error(
                "To solve for v, enter K in the Speed field (J) and m in the Mass field."
              );
          }

          document.getElementById("ke-result-text").textContent =
            resultText ||
            "Currently this configuration only solves for K given m and v.";
          document.getElementById("ke-formula-display").textContent =
            formulaDisplay;
          document.getElementById("ke-formula-steps").textContent = steps;
          document.getElementById("ke-result").classList.remove("hidden");
        } catch (e) {
          alert(e.message);
        }
      });

      // Momentum
      document.getElementById("mom-calc").addEventListener("click", () => {
        const solveFor = document.getElementById("mom-solve-for").value;
        const mVal = toNumber(document.getElementById("mom-m").value);
        const mUnit = document.getElementById("mom-m-unit").value;
        const vVal = toNumber(document.getElementById("mom-v").value);
        const vUnit = document.getElementById("mom-v-unit").value;

        const m = mVal != null ? convertMassToKg(mVal, mUnit) : null;
        const v = vVal != null ? convertSpeedToMS(vVal, vUnit) : null;

        let resultText = "";
        let formulaDisplay = "p = m v";
        let steps = "";

        try {
          if (solveFor === "p") {
            if (m == null || v == null)
              throw new Error("Enter mass and velocity.");
            const p = m * v;
            resultText = `Momentum p = ${p.toFixed(3)} kg·m/s`;
            steps = `p = ${m.toFixed(3)} · ${v.toFixed(3)} = ${p.toFixed(
              3
            )} kg·m/s`;
          } else if (solveFor === "m") {
            if (v == null || mVal == null)
              throw new Error(
                "To solve for m, enter p in the Mass field (kg·m/s) and v in the Velocity field."
              );
          } else if (solveFor === "v") {
            if (m == null || vVal == null)
              throw new Error(
                "To solve for v, enter p in the Velocity field (kg·m/s) and m in the Mass field."
              );
          }

          document.getElementById("mom-result-text").textContent =
            resultText ||
            "Currently this configuration only solves for p given m and v.";
          document.getElementById("mom-formula-display").textContent =
            formulaDisplay;
          document.getElementById("mom-formula-steps").textContent = steps;
          document.getElementById("mom-result").classList.remove("hidden");
        } catch (e) {
          alert(e.message);
        }
      });

      // Impulse
      document.getElementById("imp-calc").addEventListener("click", () => {
        const FVal = toNumber(document.getElementById("imp-F").value);
        const FUnit = document.getElementById("imp-F-unit").value;
        const dtVal = toNumber(document.getElementById("imp-dt").value);
        const dtUnit = document.getElementById("imp-dt-unit").value;

        try {
          if (FVal == null || dtVal == null)
            throw new Error("Enter force and time interval.");
          const F = convertForceToN(FVal, FUnit);
          const dt = convertTimeToSeconds(dtVal, dtUnit);
          const J = F * dt;

          document.getElementById(
            "imp-result-text"
          ).textContent = `Impulse J = ${J.toFixed(3)} N·s (kg·m/s)`;
          document.getElementById("imp-formula-display").textContent =
            "J = F Δt";
          document.getElementById(
            "imp-formula-steps"
          ).textContent = `J = ${F.toFixed(3)} · ${dt.toFixed(3)} = ${J.toFixed(
            3
          )} N·s`;
          document.getElementById("imp-result").classList.remove("hidden");
        } catch (e) {
          alert(e.message);
        }
      });

      // Circular motion
      document.getElementById("circ-calc").addEventListener("click", () => {
        const solveFor = document.getElementById("circ-solve-for").value;
        const vVal = toNumber(document.getElementById("circ-v").value);
        const vUnit = document.getElementById("circ-v-unit").value;
        const rVal = toNumber(document.getElementById("circ-r").value);
        const rUnit = document.getElementById("circ-r-unit").value;

        const v = vVal != null ? convertSpeedToMS(vVal, vUnit) : null;
        const r = rVal != null ? convertLengthToMeters(rVal, rUnit) : null;

        let resultText = "";
        let formulaDisplay = "a_c = v² / r";
        let steps = "";

        try {
          if (solveFor === "a") {
            if (v == null || r == null)
              throw new Error("Enter speed and radius.");
            const a = (v * v) / r;
            resultText = `Centripetal acceleration a_c = ${a.toFixed(3)} m/s²`;
            steps = `a_c = ${v.toFixed(3)}² / ${r.toFixed(3)} = ${a.toFixed(
              3
            )} m/s²`;
          } else if (solveFor === "v") {
            if (r == null || vVal == null)
              throw new Error(
                "To solve for v, enter a_c in the Speed field (m/s²) and r in the Radius field."
              );
          } else if (solveFor === "r") {
            if (v == null || rVal == null)
              throw new Error(
                "To solve for r, enter a_c in the Radius field (m/s²) and v in the Speed field."
              );
          }

          document.getElementById("circ-result-text").textContent =
            resultText ||
            "Currently this configuration only solves for a_c given v and r.";
          document.getElementById("circ-formula-display").textContent =
            formulaDisplay;
          document.getElementById("circ-formula-steps").textContent = steps;
          document.getElementById("circ-result").classList.remove("hidden");
        } catch (e) {
          alert(e.message);
        }
      });

      // Simple FAQ toggle (in addition to global script_faq)
      document
        .querySelectorAll("#faq-container .faq-question")
        .forEach((btn) => {
          btn.addEventListener("click", () => {
            const answer = btn.parentElement.querySelector(".faq-answer");
            const isHidden = answer.classList.contains("hidden");
            document
              .querySelectorAll("#faq-container .faq-answer")
              .forEach((a) => a.classList.add("hidden"));
            document
              .querySelectorAll("#faq-container .faq-question span:last-child")
              .forEach((icon) => (icon.textContent = "+"));
            if (isHidden) {
              answer.classList.remove("hidden");
              btn.querySelector("span:last-child").textContent = "−";
            }
          });
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
