    <script>
      // Tab switching
      document.querySelectorAll(".qm-tab").forEach((btn) => {
        btn.addEventListener("click", () => {
          const target = btn.getAttribute("data-tab");
          document.querySelectorAll(".qm-tab").forEach((b) => {
            b.classList.remove("bg-blue-600", "text-white");
            b.classList.add("bg-gray-100", "text-gray-700");
          });
          btn.classList.add("bg-blue-600", "text-white");
          btn.classList.remove("bg-gray-100", "text-gray-700");

          document
            .querySelectorAll(".qm-tab-panel")
            .forEach((p) => p.classList.add("hidden"));
          document.getElementById(target).classList.remove("hidden");
        });
      });

      function toPounds(value, unit) {
        return unit === "kg" ? value * 2.2046226218 : value;
      }

      function round(value, decimals) {
        const factor = Math.pow(10, decimals);
        return Math.round(value * factor) / factor;
      }

      // Tab 1: ET & Trap from Weight & HP
      document.getElementById("btn-et-calc").addEventListener("click", () => {
        const weight = parseFloat(document.getElementById("et-weight").value);
        const weightUnit = document.getElementById("et-weight-unit").value;
        const hpInput = parseFloat(document.getElementById("et-hp").value);
        const hpType = document.getElementById("et-hp-type").value;
        const lossPct =
          parseFloat(document.getElementById("et-drivetrain-loss").value) || 0;

        if (!weight || !hpInput || weight <= 0 || hpInput <= 0) {
          alert(
            "Please enter valid positive values for weight and horsepower."
          );
          return;
        }

        const W = toPounds(weight, weightUnit);
        let Pwheel;

        if (hpType === "whp") {
          Pwheel = hpInput;
        } else {
          const loss = Math.min(Math.max(lossPct, 0), 40) / 100;
          Pwheel = hpInput * (1 - loss);
        }

        if (Pwheel <= 0) {
          alert(
            "Effective wheel horsepower must be greater than zero. Check your drivetrain loss setting."
          );
          return;
        }

        // ET and trap speed formulas
        const etQuarter = 5.825 * Math.cbrt(W / Pwheel);
        const mphQuarter = 234 * Math.cbrt(Pwheel / W);

        // 1/8 mile estimates using common factors
        const etEighth = etQuarter / 1.57;
        const mphEighth = mphQuarter / 1.25;

        document.getElementById("et-quarter").textContent = round(etQuarter, 2);
        document.getElementById("mph-quarter").textContent = round(
          mphQuarter,
          1
        );
        document.getElementById("et-eighth").textContent = round(etEighth, 2);
        document.getElementById("mph-eighth").textContent = round(mphEighth, 1);

        document.getElementById("et-results").classList.remove("hidden");
      });

      // Tab 2: HP from ET & Weight
      document.getElementById("btn-hp-calc").addEventListener("click", () => {
        const weight = parseFloat(document.getElementById("hp-weight").value);
        const weightUnit = document.getElementById("hp-weight-unit").value;
        const et = parseFloat(document.getElementById("hp-et").value);
        const lossPct =
          parseFloat(document.getElementById("hp-drivetrain-loss").value) || 0;

        if (!weight || !et || weight <= 0 || et <= 0) {
          alert("Please enter valid positive values for weight and ET.");
          return;
        }

        const W = toPounds(weight, weightUnit);
        const etNorm = et / 5.825;
        const Pwheel = W / Math.pow(etNorm, 3);

        const loss = Math.min(Math.max(lossPct, 0), 40) / 100;
        const Pcrank = Pwheel / (1 - loss);

        document.getElementById("hp-wheel").textContent = round(Pwheel, 1);
        document.getElementById("hp-crank").textContent = round(Pcrank, 1);
        document.getElementById("hp-results").classList.remove("hidden");
      });

      // Tab 3: 1/8 ↔ 1/4 converter
      document.getElementById("btn-conv-calc").addEventListener("click", () => {
        const et8 = parseFloat(document.getElementById("conv-eighth-et").value);
        const mph8 = parseFloat(
          document.getElementById("conv-eighth-mph").value
        );

        if (!et8 || !mph8 || et8 <= 0 || mph8 <= 0) {
          alert(
            "Please enter valid positive values for 1/8 mile ET and trap speed."
          );
          return;
        }

        const etQuarter = et8 * 1.57;
        const mphQuarter = mph8 * 1.25;

        document.getElementById("conv-quarter-et").textContent = round(
          etQuarter,
          2
        );
        document.getElementById("conv-quarter-mph").textContent = round(
          mphQuarter,
          1
        );
        document.getElementById("conv-results").classList.remove("hidden");
      });

      // Mobile menu script safety (if not already in mobile-menu.js)
      const mobileToggle = document.getElementById("mobile-menu-toggle");
      const mobileMenu = document.getElementById("mobile-menu");
      if (mobileToggle && mobileMenu) {
        mobileToggle.addEventListener("click", () => {
          const expanded =
            mobileToggle.getAttribute("aria-expanded") === "true";
          mobileToggle.setAttribute("aria-expanded", String(!expanded));
          mobileMenu.classList.toggle("hidden");
        });
      }

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
