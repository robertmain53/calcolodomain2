    <script>
      // Tab handling
      const tabButtons = document.querySelectorAll(".tab-btn");
      const tabPanels = document.querySelectorAll(".tab-panel");
      tabButtons.forEach((btn) => {
        btn.addEventListener("click", () => {
          const target = btn.getAttribute("data-tab");
          tabButtons.forEach((b) => b.classList.remove("tab-active"));
          btn.classList.add("tab-active");
          tabPanels.forEach((p) => {
            p.classList.toggle("hidden", p.id !== target);
          });
        });
      });
      // Set initial active tab
      if (tabButtons.length) tabButtons[0].classList.add("tab-active");

      // Unit system handling
      const unitSystemEl = document.getElementById("unit-system");
      const kerfEl = document.getElementById("kerf");
      const trimEl = document.getElementById("trim-allowance");
      const kerfUnitEl = document.getElementById("kerf-unit");
      const trimUnitEl = document.getElementById("trim-unit");
      const bfThickUnit = document.getElementById("bf-thick-unit");
      const bfWidthUnit = document.getElementById("bf-width-unit");
      const bfLengthUnit = document.getElementById("bf-length-unit");
      const panelWidthUnit = document.getElementById("panel-width-unit");
      const panelLengthUnit = document.getElementById("panel-length-unit");
      const panelBoardWidthUnit = document.getElementById(
        "panel-board-width-unit"
      );
      const panelEdgeLossUnit = document.getElementById("panel-edge-loss-unit");

      function updateUnits() {
        const isMetric = unitSystemEl.value === "metric";
        if (isMetric) {
          kerfUnitEl.textContent = "mm";
          trimUnitEl.textContent = "mm";
          bfThickUnit.textContent = "mm";
          bfWidthUnit.textContent = "mm";
          bfLengthUnit.textContent = "mm";
          panelWidthUnit.textContent = "mm";
          panelLengthUnit.textContent = "mm";
          panelBoardWidthUnit.textContent = "mm";
          panelEdgeLossUnit.textContent = "mm per edge";
          if (parseFloat(kerfEl.value) === 0.125) kerfEl.value = 3;
          if (parseFloat(trimEl.value) === 0) trimEl.value = 0;
        } else {
          kerfUnitEl.textContent = "in";
          trimUnitEl.textContent = "in";
          bfThickUnit.textContent = "in";
          bfWidthUnit.textContent = "in";
          bfLengthUnit.textContent = "in";
          panelWidthUnit.textContent = "in";
          panelLengthUnit.textContent = "in";
          panelBoardWidthUnit.textContent = "in";
          panelEdgeLossUnit.textContent = "in per edge";
        }
      }
      unitSystemEl.addEventListener("change", updateUnits);
      updateUnits();

      // Helpers
      function createInput(type, cls, value) {
        const input = document.createElement("input");
        input.type = type;
        input.className = cls;
        if (value !== undefined) input.value = value;
        return input;
      }
      function parseNum(v) {
        const n = parseFloat(v);
        return isNaN(n) ? 0 : n;
      }

      // Stock and parts tables
      const stockBody = document.getElementById("stock-body");
      const partsBody = document.getElementById("parts-body");

      function addStockRow(data = {}) {
        const tr = document.createElement("tr");
        tr.className = "border-t border-gray-100";
        tr.innerHTML = `
      <td class="px-2 py-1"><input type="number" min="1" step="1" value="${
        data.qty || 1
      }" class="border-gray-300 rounded-md w-14 text-xs"></td>
      <td class="px-2 py-1"><input type="number" step="0.01" value="${
        data.thick || 1
      }" class="border-gray-300 rounded-md w-16 text-xs"></td>
      <td class="px-2 py-1"><input type="number" step="0.01" value="${
        data.width || 6
      }" class="border-gray-300 rounded-md w-16 text-xs"></td>
      <td class="px-2 py-1"><input type="number" step="0.1" value="${
        data.length || 96
      }" class="border-gray-300 rounded-md w-20 text-xs"></td>
      <td class="px-2 py-1 text-right"><button type="button" class="text-red-600 text-xs remove-row">✕</button></td>
    `;
        stockBody.appendChild(tr);
      }
      function addPartRow(data = {}) {
        const tr = document.createElement("tr");
        tr.className = "border-t border-gray-100";
        tr.innerHTML = `
      <td class="px-2 py-1"><input type="text" value="${
        data.label || ""
      }" class="border-gray-300 rounded-md w-20 text-xs"></td>
      <td class="px-2 py-1"><input type="number" min="1" step="1" value="${
        data.qty || 1
      }" class="border-gray-300 rounded-md w-14 text-xs"></td>
      <td class="px-2 py-1"><input type="number" step="0.01" value="${
        data.thick || 0.75
      }" class="border-gray-300 rounded-md w-16 text-xs"></td>
      <td class="px-2 py-1"><input type="number" step="0.01" value="${
        data.width || 3
      }" class="border-gray-300 rounded-md w-16 text-xs"></td>
      <td class="px-2 py-1"><input type="number" step="0.1" value="${
        data.length || 30
      }" class="border-gray-300 rounded-md w-20 text-xs"></td>
      <td class="px-2 py-1 text-right"><button type="button" class="text-red-600 text-xs remove-row">✕</button></td>
    `;
        partsBody.appendChild(tr);
      }

      document
        .getElementById("add-stock")
        .addEventListener("click", () => addStockRow());
      document
        .getElementById("add-part")
        .addEventListener("click", () => addPartRow());
      [stockBody, partsBody].forEach((tbody) => {
        tbody.addEventListener("click", (e) => {
          if (e.target.classList.contains("remove-row")) {
            e.target.closest("tr").remove();
          }
        });
      });

      // Seed with one example row each
      addStockRow();
      addPartRow();

      // Cut list optimizer
      const runOptimizerBtn = document.getElementById("run-optimizer");
      const clearAllBtn = document.getElementById("clear-all");
      const errorEl = document.getElementById("cutlist-error");
      const cutlistResults = document.getElementById("cutlist-results");
      const layoutContainer = document.getElementById("layout-container");
      const summaryBf = document.getElementById("summary-bf");
      const summaryPartsBf = document.getElementById("summary-parts-bf");
      const summaryWaste = document.getElementById("summary-waste");

      function getStockBoards() {
        const boards = [];
        stockBody.querySelectorAll("tr").forEach((tr) => {
          const tds = tr.querySelectorAll("td input");
          const qty = parseNum(tds[0].value);
          const thick = parseNum(tds[1].value);
          const width = parseNum(tds[2].value);
          const length = parseNum(tds[3].value);
          if (qty > 0 && thick > 0 && width > 0 && length > 0) {
            for (let i = 0; i < qty; i++) {
              boards.push({
                thick,
                width,
                length,
                remaining: length,
                parts: [],
              });
            }
          }
        });
        return boards;
      }

      function getParts() {
        const parts = [];
        partsBody.querySelectorAll("tr").forEach((tr) => {
          const inputs = tr.querySelectorAll("td input");
          const label = inputs[0].value || "";
          const qty = parseNum(inputs[1].value);
          const thick = parseNum(inputs[2].value);
          const width = parseNum(inputs[3].value);
          const length = parseNum(inputs[4].value);
          if (qty > 0 && thick > 0 && width > 0 && length > 0) {
            for (let i = 0; i < qty; i++) {
              parts.push({ label, thick, width, length });
            }
          }
        });
        return parts;
      }

      function boardFeetImperial(thick, width, length) {
        return (thick * width * length) / 144;
      }
      function volumeMetric(thickMm, widthMm, lengthMm) {
        return (thickMm / 1000) * (widthMm / 1000) * (lengthMm / 1000);
      }

      runOptimizerBtn.addEventListener("click", () => {
        errorEl.textContent = "";
        const boards = getStockBoards();
        const parts = getParts();
        const isMetric = unitSystemEl.value === "metric";
        const kerf = parseNum(kerfEl.value);
        const trim = parseNum(trimEl.value);

        if (!boards.length) {
          errorEl.textContent = "Add at least one stock board.";
          cutlistResults.classList.add("hidden");
          return;
        }
        if (!parts.length) {
          errorEl.textContent = "Add at least one required part.";
          cutlistResults.classList.add("hidden");
          return;
        }

        // Sort parts by length descending
        parts.sort((a, b) => b.length + trim - (a.length + trim));

        // Simple first-fit decreasing along length
        let boardIndex = 0;
        parts.forEach((part) => {
          const needed = part.length + trim;
          let placed = false;
          for (let i = 0; i < boards.length; i++) {
            const b = boards[i];
            const kerfCost = b.parts.length > 0 ? kerf : 0;
            if (b.remaining >= needed + kerfCost) {
              b.parts.push({ ...part, usedLength: needed });
              b.remaining -= needed + kerfCost;
              placed = true;
              break;
            }
          }
          if (!placed && boardIndex < boards.length) {
            const b = boards[boardIndex];
            const kerfCost = b.parts.length > 0 ? kerf : 0;
            if (b.remaining >= needed + kerfCost) {
              b.parts.push({ ...part, usedLength: needed });
              b.remaining -= needed + kerfCost;
            }
          }
        });

        // Compute volumes
        let totalStockVol = 0;
        let totalPartsVol = 0;
        boards.forEach((b) => {
          if (isMetric) {
            totalStockVol += volumeMetric(b.thick, b.width, b.length);
            b.parts.forEach((p) => {
              totalPartsVol += volumeMetric(p.thick, p.width, p.usedLength);
            });
          } else {
            totalStockVol += boardFeetImperial(b.thick, b.width, b.length);
            b.parts.forEach((p) => {
              totalPartsVol += boardFeetImperial(
                p.thick,
                p.width,
                p.usedLength
              );
            });
          }
        });

        const unitLabel = isMetric ? "m³ (approx bf shown)" : "bd ft";
        summaryBf.textContent = isMetric
          ? `${totalStockVol.toFixed(3)} m³`
          : `${totalStockVol.toFixed(2)} bd ft`;
        summaryPartsBf.textContent = isMetric
          ? `${totalPartsVol.toFixed(3)} m³`
          : `${totalPartsVol.toFixed(2)} bd ft`;
        const wastePct =
          totalStockVol > 0 ? (1 - totalPartsVol / totalStockVol) * 100 : 0;
        summaryWaste.textContent = `${wastePct.toFixed(1)}%`;

        // Render layout
        layoutContainer.innerHTML = "";
        boards.forEach((b, idx) => {
          if (!b.parts.length) return;
          const div = document.createElement("div");
          div.className = "border border-gray-200 rounded-md p-3 bg-white";
          const usedLength = b.parts.reduce(
            (sum, p, i) => sum + p.usedLength + (i > 0 ? kerf : 0),
            0
          );
          const usedPct = (usedLength / b.length) * 100;
          div.innerHTML = `
        <div class="flex justify-between items-center mb-1 text-sm">
          <div class="font-semibold">Board ${idx + 1}</div>
          <div class="text-xs text-gray-600">
            ${b.thick} × ${b.width} × ${b.length} ${isMetric ? "mm" : "in"}
          </div>
        </div>
        <div class="w-full bg-gray-100 rounded-full h-3 mb-2 overflow-hidden">
          <div class="bg-blue-500 h-3" style="width:${Math.min(
            100,
            usedPct
          ).toFixed(1)}%"></div>
        </div>
        <div class="text-xs text-gray-600 mb-2">
          Used length: ${usedLength.toFixed(1)} / ${b.length.toFixed(1)} ${
            isMetric ? "mm" : "in"
          } (${usedPct.toFixed(1)}%)
        </div>
        <table class="w-full text-xs">
          <thead class="bg-gray-50">
            <tr>
              <th class="px-1 py-1 text-left">Part</th>
              <th class="px-1 py-1 text-left">T</th>
              <th class="px-1 py-1 text-left">W</th>
              <th class="px-1 py-1 text-left">L (used)</th>
            </tr>
          </thead>
          <tbody>
            ${b.parts
              .map(
                (p) => `
              <tr class="border-t border-gray-100">
                <td class="px-1 py-1">${p.label || "-"}</td>
                <td class="px-1 py-1">${p.thick}</td>
                <td class="px-1 py-1">${p.width}</td>
                <td class="px-1 py-1">${p.usedLength.toFixed(1)}</td>
              </tr>
            `
              )
              .join("")}
          </tbody>
        </table>
      `;
          layoutContainer.appendChild(div);
        });

        cutlistResults.classList.remove("hidden");
      });

      clearAllBtn.addEventListener("click", () => {
        stockBody.innerHTML = "";
        partsBody.innerHTML = "";
        addStockRow();
        addPartRow();
        cutlistResults.classList.add("hidden");
        errorEl.textContent = "";
      });

      // Board feet calculator
      document.getElementById("bf-calc").addEventListener("click", () => {
        const isMetric = unitSystemEl.value === "metric";
        const t = parseNum(document.getElementById("bf-thick").value);
        const w = parseNum(document.getElementById("bf-width").value);
        const l = parseNum(document.getElementById("bf-length").value);
        const qty = parseNum(document.getElementById("bf-qty").value) || 1;
        const resultEl = document.getElementById("bf-result");
        if (t <= 0 || w <= 0 || l <= 0) {
          resultEl.textContent = "Enter positive thickness, width, and length.";
          return;
        }
        if (isMetric) {
          const v = volumeMetric(t, w, l) * qty;
          const bfApprox = v * 423.776; // 1 m³ ≈ 423.776 bd ft
          resultEl.textContent = `${v.toFixed(4)} m³ (≈ ${bfApprox.toFixed(
            1
          )} bd ft)`;
        } else {
          const bf = boardFeetImperial(t, w, l) * qty;
          resultEl.textContent = `${bf.toFixed(2)} board feet`;
        }
      });

      // Panel glue-up
      document.getElementById("panel-calc").addEventListener("click", () => {
        const isMetric = unitSystemEl.value === "metric";
        const panelW = parseNum(document.getElementById("panel-width").value);
        const panelL = parseNum(document.getElementById("panel-length").value);
        const boardW = parseNum(
          document.getElementById("panel-board-width").value
        );
        const edgeLoss = parseNum(
          document.getElementById("panel-edge-loss").value
        );
        const resultEl = document.getElementById("panel-result");
        const detailsEl = document.getElementById("panel-details");
        detailsEl.innerHTML = "";
        if (panelW <= 0 || panelL <= 0 || boardW <= 0) {
          resultEl.textContent = "Enter positive panel and board dimensions.";
          return;
        }
        const effectiveBoardW = Math.max(boardW - 2 * edgeLoss, 0);
        if (effectiveBoardW <= 0) {
          resultEl.textContent =
            "Edge loss is too large for the given board width.";
          return;
        }
        const boardsNeeded = Math.ceil(panelW / effectiveBoardW);
        const totalWidth = boardsNeeded * effectiveBoardW;
        const overhang = totalWidth - panelW;
        const unit = isMetric ? "mm" : "in";
        resultEl.textContent = `${boardsNeeded} boards required across the width.`;
        const li1 = document.createElement("li");
        li1.textContent = `Effective board width after jointing: ${effectiveBoardW.toFixed(
          2
        )} ${unit}.`;
        const li2 = document.createElement("li");
        li2.textContent = `Total glued-up width: ${totalWidth.toFixed(
          2
        )} ${unit} (about ${overhang.toFixed(2)} ${unit} extra to trim).`;
        const li3 = document.createElement("li");
        li3.textContent = `Cut boards slightly longer than ${panelL.toFixed(
          1
        )} ${unit} to allow for end trimming.`;
        detailsEl.append(li1, li2, li3);
      });

      // Dimension helper: fraction to decimal/mm
      document.getElementById("frac-calc").addEventListener("click", () => {
        const whole = parseNum(document.getElementById("frac-whole").value);
        const num = parseNum(document.getElementById("frac-num").value);
        const den = parseNum(document.getElementById("frac-den").value);
        const resultEl = document.getElementById("frac-result");
        if (den === 0) {
          resultEl.textContent = "Denominator must be greater than zero.";
          return;
        }
        const decimalIn = whole + (den ? num / den : 0);
        const mm = decimalIn * 25.4;
        resultEl.textContent = `${decimalIn.toFixed(3)} in  ≈  ${mm.toFixed(
          2
        )} mm`;
      });

      // Decimal/mm to nearest fraction
      function toNearestFraction(decimalIn, maxDen) {
        const sign = decimalIn < 0 ? -1 : 1;
        const abs = Math.abs(decimalIn);
        const whole = Math.floor(abs);
        const frac = abs - whole;
        let bestNum = 0;
        let bestDen = 1;
        let bestErr = Infinity;
        for (let den = 1; den <= maxDen; den++) {
          const num = Math.round(frac * den);
          const approx = num / den;
          const err = Math.abs(frac - approx);
          if (err < bestErr) {
            bestErr = err;
            bestNum = num;
            bestDen = den;
          }
        }
        const finalWhole = sign * whole;
        return { whole: finalWhole, num: bestNum, den: bestDen };
      }

      document.getElementById("dec-calc").addEventListener("click", () => {
        const decIn = parseFloat(document.getElementById("dec-in").value);
        const mmVal = parseFloat(document.getElementById("dec-mm").value);
        const maxDen =
          parseInt(document.getElementById("dec-denom").value, 10) || 16;
        const resultEl = document.getElementById("dec-result");

        let inches = NaN;
        if (!isNaN(decIn)) {
          inches = decIn;
        } else if (!isNaN(mmVal)) {
          inches = mmVal / 25.4;
        }

        if (isNaN(inches)) {
          resultEl.textContent = "Enter either decimal inches or millimeters.";
          return;
        }

        const frac = toNearestFraction(inches, maxDen);
        const mm = inches * 25.4;
        let fracStr = "";
        if (frac.num === 0) {
          fracStr = `${frac.whole}"`;
        } else if (frac.whole === 0) {
          fracStr = `${frac.num}/${frac.den}"`;
        } else {
          fracStr = `${frac.whole} ${frac.num}/${frac.den}"`;
        }
        resultEl.textContent = `${inches.toFixed(3)} in  ≈  ${mm.toFixed(
          2
        )} mm  ≈  ${fracStr}`;
      });

      // Simple FAQ toggle (in addition to global script_faq)
      document
        .querySelectorAll("#faq-container .faq-question")
        .forEach((btn) => {
          btn.addEventListener("click", () => {
            const ans = btn.nextElementSibling;
            const sign = btn.querySelector("span:last-child");
            const isHidden = ans.classList.contains("hidden");
            document
              .querySelectorAll("#faq-container .faq-answer")
              .forEach((a) => a.classList.add("hidden"));
            document
              .querySelectorAll("#faq-container .faq-question span:last-child")
              .forEach((s) => (s.textContent = "+"));
            if (isHidden) {
              ans.classList.remove("hidden");
              sign.textContent = "−";
            }
          });
        });

      // Mobile menu script (fallback if mobile-menu.js not yet loaded)
      const mobileToggle = document.getElementById("mobile-menu-toggle");
      const mobileMenu = document.getElementById("mobile-menu");
      if (mobileToggle && mobileMenu) {
        mobileToggle.addEventListener("click", () => {
          const isHidden = mobileMenu.classList.contains("hidden");
          mobileMenu.classList.toggle("hidden", !isHidden);
          mobileToggle.setAttribute("aria-expanded", String(isHidden));
        });
      }

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
