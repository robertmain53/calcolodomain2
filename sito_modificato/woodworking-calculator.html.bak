            <!-- TAB: Dimension Helper -->
            <section id="tab-dimensions" class="tab-panel hidden">
              <h2 class="text-xl font-semibold mb-3">
                Dimension Helper & Unit Converter
              </h2>
              <p class="text-sm text-gray-700 mb-3">
                Convert between fractional inches, decimals, and millimeters.
                Great for setting tools and reading plans.
              </p>

              <div class="grid md:grid-cols-2 gap-6 text-sm">
                <div>
                  <h3 class="font-semibold mb-2">
                    Fractional inches → decimal & mm
                  </h3>
                  <div class="flex items-center gap-2 mb-2">
                    <input
                      id="frac-whole"
                      type="number"
                      step="1"
                      class="border-gray-300 rounded-md w-16"
                      placeholder="in"
                    />
                    <span class="text-gray-500">+</span>
                    <input
                      id="frac-num"
                      type="number"
                      step="1"
                      class="border-gray-300 rounded-md w-16"
                      placeholder="num"
                    />
                    <span class="text-gray-500">/</span>
                    <input
                      id="frac-den"
                      type="number"
                      step="1"
                      class="border-gray-300 rounded-md w-16"
                      placeholder="den"
                    />
                  </div>
                  <button
                    id="frac-calc"
                    type="button"
                    class="px-3 py-2 bg-blue-600 text-white rounded-md text-sm hover:bg-blue-700"
                  >
                    Convert
                  </button>
                  <div
                    class="mt-3 bg-gray-50 border border-gray-200 rounded-md p-3"
                  >
                    <div class="text-xs text-gray-500 mb-1">Result</div>
                    <div id="frac-result" class="font-semibold">–</div>
                  </div>
                </div>

                <div>
                  <h3 class="font-semibold mb-2">
                    Decimal / mm → nearest fraction
                  </h3>
                  <div class="space-y-2">
                    <div class="flex items-center gap-2">
                      <label class="w-20">Decimal in</label>
                      <input
                        id="dec-in"
                        type="number"
                        step="0.001"
                        class="border-gray-300 rounded-md w-28"
                      />
                    </div>
                    <div class="flex items-center gap-2">
                      <label class="w-20">Millimeters</label>
                      <input
                        id="dec-mm"
                        type="number"
                        step="0.1"
                        class="border-gray-300 rounded-md w-28"
                      />
                    </div>
                    <div class="flex items-center gap-2">
                      <label class="w-20">Max denom</label>
                      <select
                        id="dec-denom"
                        class="border-gray-300 rounded-md w-28"
                      >
                        <option value="8">1/8</option>
                        <option value="16" selected>1/16</option>
                        <option value="32">1/32</option>
                        <option value="64">1/64</option>
                      </select>
                    </div>
                    <button
                      id="dec-calc"
                      type="button"
