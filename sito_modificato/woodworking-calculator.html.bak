            <section id="tab-boardfeet" class="tab-panel hidden">
              <h2 class="text-xl font-semibold mb-3">
                Board Feet & Volume Calculator
              </h2>
              <p class="text-sm text-gray-700 mb-3">
                Quickly calculate board feet for rough lumber or finished parts.
              </p>

              <div class="grid md:grid-cols-2 gap-6">
                <div>
                  <h3 class="font-semibold mb-2">Single Board / Part</h3>
                  <div class="space-y-2 text-sm">
                    <div class="flex items-center gap-2">
                      <label class="w-24">Thickness</label>
                      <input
                        id="bf-thick"
                        type="number"
                        step="0.01"
                        class="border-gray-300 rounded-md w-24"
                      />
                      <span id="bf-thick-unit" class="text-gray-600 text-xs"
                        >in</span
                      >
                    </div>
                    <div class="flex items-center gap-2">
                      <label class="w-24">Width</label>
                      <input
                        id="bf-width"
                        type="number"
                        step="0.01"
                        class="border-gray-300 rounded-md w-24"
                      />
                      <span id="bf-width-unit" class="text-gray-600 text-xs"
                        >in</span
                      >
                    </div>
                    <div class="flex items-center gap-2">
                      <label class="w-24">Length</label>
                      <input
                        id="bf-length"
                        type="number"
                        step="0.01"
                        class="border-gray-300 rounded-md w-24"
                      />
                      <span id="bf-length-unit" class="text-gray-600 text-xs"
                        >in</span
                      >
                    </div>
                    <div class="flex items-center gap-2">
                      <label class="w-24">Quantity</label>
                      <input
                        id="bf-qty"
                        type="number"
                        step="1"
                        value="1"
                        class="border-gray-300 rounded-md w-24"
                      />
                    </div>
                    <button
                      id="bf-calc"
                      type="button"
                      class="mt-2 px-3 py-2 bg-blue-600 text-white rounded-md text-sm hover:bg-blue-700"
                    >
                      Calculate
                    </button>
                  </div>
                  <div
                    class="mt-3 text-sm bg-gray-50 border border-gray-200 rounded-md p-3"
                  >
                    <div class="text-xs text-gray-500 mb-1">Result</div>
                    <div id="bf-result" class="font-semibold">–</div>
                  </div>
                </div>

                <div>
                  <h3 class="font-semibold mb-2">Formula</h3>
                  <div class="formula-box text-sm">
                    <p class="mb-1 font-medium">Imperial (board feet)</p>
                    <p>
                      \( \text{Board feet} = \dfrac{T_{\text{in}} \times
                      W_{\text{in}} \times L_{\text{in}}}{144} \times
                      \text{Quantity} \)
                    </p>
                    <p class="mt-2 mb-1 font-medium">Metric (cubic meters)</p>
                    <p>
                      \( V_{\text{m}^3} =
                      \left(\dfrac{T_{\text{mm}}}{1000}\right) \times
                      \left(\dfrac{W_{\text{mm}}}{1000}\right) \times
                      \left(\dfrac{L_{\text{mm}}}{1000}\right) \times
                      \text{Quantity} \)
                    </p>
                    <p class="mt-2 text-xs text-gray-600">
                      The calculator automatically switches formulas based on
                      the selected unit system.
                    </p>
                  </div>
                </div>
              </div>
            </section>

