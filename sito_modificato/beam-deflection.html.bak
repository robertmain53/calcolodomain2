    <script>
      // Utility: unit conversions
      const lengthToMeters = (value, unit) => {
        const v = Number(value);
        if (!isFinite(v)) return NaN;
        switch (unit) {
          case "m":
            return v;
          case "cm":
            return v / 100;
          case "mm":
            return v / 1000;
          case "ft":
            return v * 0.3048;
          case "in":
            return v * 0.0254;
          default:
            return v;
        }
      };

      const metersToLength = (value, unit) => {
        const v = Number(value);
        if (!isFinite(v)) return NaN;
        switch (unit) {
          case "m":
            return v;
          case "cm":
            return v * 100;
          case "mm":
            return v * 1000;
          case "ft":
            return v / 0.3048;
          case "in":
            return v / 0.0254;
          default:
            return v;
        }
      };

      const forceToNewtons = (value, unit) => {
        const v = Number(value);
        if (!isFinite(v)) return NaN;
        switch (unit) {
          case "N":
            return v;
          case "kN":
            return v * 1000;
          case "kgf":
            return v * 9.80665;
          case "lb":
            return v * 4.4482216153;
          default:
            return v;
        }
      };

      const udlToNPerM = (value, unit, lengthUnit) => {
        const v = Number(value);
        if (!isFinite(v)) return NaN;
        // unit is force per length; we assume:
        // kN/m, kN/ft, lb/ft, lb/in
        switch (unit) {
          case "kN/m":
            return v * 1000;
          case "kN/ft":
            return (v * 1000) / 0.3048;
          case "lb/ft":
            return (v * 4.4482216153) / 0.3048;
          case "lb/in":
            return (v * 4.4482216153) / 0.0254;
          default:
            return v;
        }
      };

      const EToPa = (value, unit) => {
        const v = Number(value);
        if (!isFinite(v)) return NaN;
        switch (unit) {
          case "Pa":
            return v;
          case "kPa":
            return v * 1e3;
          case "MPa":
            return v * 1e6;
          case "GPa":
            return v * 1e9;
          case "psi":
            return v * 6894.757293;
          case "ksi":
            return v * 6894.757293 * 1000;
          default:
            return v * 1e6; // assume MPa if unknown
        }
      };

      const IToM4 = (value, unit) => {
        const v = Number(value);
        if (!isFinite(v)) return NaN;
        switch (unit) {
          case "m4":
            return v;
          case "cm4":
            return v * 1e-8; // (1 cm = 0.01 m)^4
          case "mm4":
            return v * 1e-12;
          case "in4":
            return v * Math.pow(0.0254, 4);
          default:
            return v;
        }
      };

      // Load row templates
      function createPointLoadRow(index) {
