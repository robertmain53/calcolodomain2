        <button type="button" class="remove-load text-xs text-red-600 hover:underline">Remove</button>
      </div>
    `;
        return row;
      }

      function createUDLRow(index) {
        const row = document.createElement("div");
        row.className =
          "grid md:grid-cols-6 gap-2 items-end border border-gray-100 rounded-md p-2 bg-white";
        row.dataset.type = "udl";
        row.innerHTML = `
      <div class="md:col-span-2">
        <label class="block text-xs font-medium text-gray-700 mb-1">UDL w</label>
        <div class="flex gap-2">
          <input type="number" step="0.01" class="w-full border-gray-300 rounded-md shadow-sm focus:ring-blue-500 focus:border-blue-500" value="5">
          <select class="border-gray-300 rounded-md shadow-sm focus:ring-blue-500 focus:border-blue-500">
            <option value="kN/m">kN/m</option>
            <option value="kN/ft">kN/ft</option>
            <option value="lb/ft">lb/ft</option>
            <option value="lb/in">lb/in</option>
          </select>
        </div>
      </div>
      <div>
        <label class="block text-xs font-medium text-gray-700 mb-1">Start a</label>
        <div class="flex gap-2">
          <input type="number" step="0.01" class="w-full border-gray-300 rounded-md shadow-sm focus:ring-blue-500 focus:border-blue-500" value="0">
          <span class="text-xs text-gray-600 span-unit-label">m</span>
        </div>
      </div>
      <div>
        <label class="block text-xs font-medium text-gray-700 mb-1">End b</label>
        <div class="flex gap-2">
          <input type="number" step="0.01" class="w-full border-gray-300 rounded-md shadow-sm focus:ring-blue-500 focus:border-blue-500" value="5">
          <span class="text-xs text-gray-600 span-unit-label">m</span>
        </div>
      </div>
      <div class="flex items-center justify-between md:justify-end gap-2 mt-2 md:mt-0 md:col-span-1">
        <span class="inline-flex items-center px-2 py-0.5 rounded text-xs bg-green-50 text-green-700">w</span>
        <button type="button" class="remove-load text-xs text-red-600 hover:underline">Remove</button>
      </div>
    `;
        return row;
      }

      function updateSpanUnitLabels() {
        const spanUnit = document.getElementById("span-unit").value;
        document.querySelectorAll(".span-unit-label").forEach((el) => {
          el.textContent = spanUnit;
        });
        document.getElementById("probe-x-unit").textContent = spanUnit;
      }

      function updateBeamDiagramSupports() {
        const type = document.getElementById("beam-type").value;
        const fixed = document.getElementById("support-fixed");
        const left = document.getElementById("support-left");
        const right = document.getElementById("support-right");
        if (type === "cantilever") {
          fixed.classList.remove("hidden");
          left.classList.add("hidden");
          right.classList.add("hidden");
        } else {
          fixed.classList.add("hidden");
          left.classList.remove("hidden");
          right.classList.remove("hidden");
        }
      }

      function renderLoadMarkers() {
        const container = document.getElementById("load-markers");
        container.innerHTML = "";
        const span = Number(document.getElementById("span").value) || 0;
        const spanUnit = document.getElementById("span-unit").value;
        const rows = document.querySelectorAll("#loads-container > div");
        rows.forEach((row) => {
          const type = row.dataset.type;
          if (type === "point") {
            const posInput = row.querySelector("div:nth-child(2) input");
            const a = Number(posInput.value);
            if (!isFinite(a) || span <= 0) return;
            const rel = Math.min(Math.max(a / span, 0), 1);
            const xPercent = 5 + rel * 90;
            const arrow = document.createElement("div");
            arrow.className = "load-arrow";
            arrow.style.left = xPercent + "%";
            arrow.style.top = "20%";
            arrow.style.transform = "translateX(-50%)";
            const line = document.createElement("div");
            line.className = "load-arrow-line";
            line.style.left = xPercent + "%";
            line.style.top = "20%";
            line.style.height = "35%";
            line.style.transform = "translateX(-50%)";
            container.appendChild(line);
            container.appendChild(arrow);
          } else if (type === "udl") {
            const aInput = row.querySelector("div:nth-child(2) input");
            const bInput = row.querySelector("div:nth-child(3) input");
            const a = Number(aInput.value);
            const b = Number(bInput.value);
            if (!isFinite(a) || !isFinite(b) || span <= 0) return;
            const startRel = Math.min(Math.max(a / span, 0), 1);
            const endRel = Math.min(Math.max(b / span, 0), 1);
            const leftPercent = 5 + Math.min(startRel, endRel) * 90;
            const rightPercent = 5 + Math.max(startRel, endRel) * 90;
            const band = document.createElement("div");
            band.style.position = "absolute";
            band.style.left = leftPercent + "%";
            band.style.right = 100 - rightPercent + "%";
            band.style.top = "18%";
            band.style.height = "6px";
            band.style.background =
              "repeating-linear-gradient(90deg, #16a34a, #16a34a 4px, transparent 4px, transparent 8px)";
            container.appendChild(band);
          }
        });
      }

      // Beam deflection functions
      // For simplicity and robustness, we use standard closed-form formulas for common cases:
      // - Simply supported: point load anywhere, UDL over segment
      // - Cantilever: point load anywhere, UDL over segment
      // We compute reactions and integrate M/EI piecewise.

      function deflectionSimplySupportedPoint(EI, L, P, a, x) {
        // Point load P at distance a from left, b = L - a
        const b = L - a;
        const R1 = (P * b) / L;
        const R2 = (P * a) / L;
        if (x <= a) {
          // region 1: 0 <= x <= a
          // v(x) = R1 x^3 / (6EI) - P (x - a)^3 / (6EI) * 0 (since x<a, term zero)
          return (R1 * Math.pow(x, 3)) / (6 * EI);
        } else {
          // region 2: a <= x <= L
          // v(x) = R1 x^3 / (6EI) - P (x - a)^3 / (6EI)
          return (R1 * Math.pow(x, 3) - P * Math.pow(x - a, 3)) / (6 * EI);
        }
      }

      function deflectionSimplySupportedUDL(EI, L, w, a, b, x) {
        // UDL w from a to b (0<=a<b<=L). Use superposition of equivalent loads:
        // We integrate M/EI piecewise. For simplicity, approximate by splitting into many small point loads.
        const n = 40;
        const dx = (b - a) / n;
        let v = 0;
        for (let i = 0; i < n; i++) {
          const xi = a + (i + 0.5) * dx;
          const Pi = w * dx;
          v += deflectionSimplySupportedPoint(EI, L, Pi, xi, x);
        }
        return v;
      }

      function deflectionCantileverPoint(EI, L, P, a, x) {
        // Cantilever fixed at x=0, free at x=L. Point load P at distance a from fixed end.
        // Use standard formulas via superposition: treat as load at a with equivalent end moment and shear.
        // For x <= a:
        // v(x) = P x^2 (3a - x) / (6EI)
        // For x >= a:
        // v(x) = P a^2 (3x - a) / (6EI)
        if (x <= a) {
          return (P * x * x * (3 * a - x)) / (6 * EI);
        } else {
          return (P * a * a * (3 * x - a)) / (6 * EI);
        }
      }

      function deflectionCantileverUDL(EI, L, w, a, b, x) {
        // UDL w from a to b on cantilever. Approximate by many point loads.
        const n = 40;
        const dx = (b - a) / n;
        let v = 0;
        for (let i = 0; i < n; i++) {
          const xi = a + (i + 0.5) * dx;
          const Pi = w * dx;
          v += deflectionCantileverPoint(EI, L, Pi, xi, x);
        }
        return v;
      }

      function computeDeflectionAt(xMeters, state) {
        const { type, L, EI, pointLoads, udls } = state;
        let v = 0;
        if (type === "simply-supported") {
          pointLoads.forEach((pl) => {
            v += deflectionSimplySupportedPoint(EI, L, pl.P, pl.a, xMeters);
          });
          udls.forEach((u) => {
            v += deflectionSimplySupportedUDL(EI, L, u.w, u.a, u.b, xMeters);
          });
        } else {
          pointLoads.forEach((pl) => {
            v += deflectionCantileverPoint(EI, L, pl.P, pl.a, xMeters);
          });
          udls.forEach((u) => {
            v += deflectionCantileverUDL(EI, L, u.w, u.a, u.b, xMeters);
          });
        }
        return v; // meters (downward positive)
      }

      function gatherState() {
        const type = document.getElementById("beam-type").value;
        const Luser = Number(document.getElementById("span").value);
        const Lunit = document.getElementById("span-unit").value;
        const L = lengthToMeters(Luser, Lunit);
        const Euser = Number(document.getElementById("E").value);
        const Eunit = document.getElementById("E-unit").value;
        const E = EToPa(Euser, Eunit);
        const Iuser = Number(document.getElementById("I").value);
        const Iunit = document.getElementById("I-unit").value;
        const I = IToM4(Iuser, Iunit);
        const EI = E * I;

        const pointLoads = [];
        const udls = [];
        document.querySelectorAll("#loads-container > div").forEach((row) => {
          const typeRow = row.dataset.type;
          if (typeRow === "point") {
            const Pval = row.querySelector("div:nth-child(1) input").value;
            const Punit = row.querySelector("div:nth-child(1) select").value;
            const aVal = row.querySelector("div:nth-child(2) input").value;
            const P = forceToNewtons(Pval, Punit);
            const a = lengthToMeters(aVal, Lunit);
            if (isFinite(P) && isFinite(a)) {
              pointLoads.push({ P, a });
            }
          } else if (typeRow === "udl") {
            const wVal = row.querySelector("div:nth-child(1) input").value;
            const wUnit = row.querySelector("div:nth-child(1) select").value;
            const aVal = row.querySelector("div:nth-child(2) input").value;
            const bVal = row.querySelector("div:nth-child(3) input").value;
            const w = udlToNPerM(wVal, wUnit, Lunit);
            let a = lengthToMeters(aVal, Lunit);
            let b = lengthToMeters(bVal, Lunit);
            if (b < a) [a, b] = [b, a];
            a = Math.max(0, Math.min(a, L));
            b = Math.max(0, Math.min(b, L));
            if (isFinite(w) && b > a) {
              udls.push({ w, a, b });
            }
          }
        });

        return { type, L, EI, pointLoads, udls, Lunit };
      }

      function calculateDeflection() {
        const state = gatherState();
        const { L, EI, Lunit } = state;
        if (!isFinite(L) || L <= 0 || !isFinite(EI) || EI <= 0) {
          alert("Please enter valid span, E, and I values.");
          return;
        }
        if (state.pointLoads.length === 0 && state.udls.length === 0) {
          alert("Please add at least one load.");
          return;
        }

        const n = 80;
        let maxDefl = 0;
        let maxX = 0;
        const path = document.getElementById("deflection-path");
        let d = "";
        for (let i = 0; i <= n; i++) {
          const x = (L * i) / n;
          const v = computeDeflectionAt(x, state); // meters
          if (Math.abs(v) > Math.abs(maxDefl)) {
            maxDefl = v;
            maxX = x;
          }
        }

        // Build SVG path (scaled)
        const scale =
          maxDefl === 0 ? 0 : 20 / Math.max(Math.abs(maxDefl), 1e-9);
        for (let i = 0; i <= n; i++) {
          const x = (L * i) / n;
          const v = computeDeflectionAt(x, state);
          const X = (x / L) * 100;
          const Y = 30 + v * scale * 100; // center at 30
          d += (i === 0 ? "M" : "L") + X.toFixed(2) + "," + Y.toFixed(2) + " ";
        }
        path.setAttribute("d", d.trim());

        // Display max deflection
        const maxDeflAbs = Math.abs(maxDefl);
        const maxDeflUser = metersToLength(maxDeflAbs, Lunit);
        const dir = maxDefl < 0 ? "upward" : "downward";
        document.getElementById("max-deflection-display").textContent =
          maxDeflUser.toFixed(3) + " " + Lunit;
        document.getElementById("max-deflection-location").textContent =
          "Maximum " +
          dir +
          " deflection at x ≈ " +
          metersToLength(maxX, Lunit).toFixed(3) +
          " " +
          Lunit;

        // Deflection limit check
        const limitType = document.getElementById("limit-type").value;
        const limitDisplay = document.getElementById("limit-check-display");
        const ratioDisplay = document.getElementById("limit-ratio-display");
        if (limitType === "none") {
          limitDisplay.textContent = "No limit selected.";
          ratioDisplay.textContent = "";
        } else {
          let deltaAllow;
          if (limitType.startsWith("L/")) {
            const N = Number(limitType.split("/")[1]);
            deltaAllow = L / N;
          } else {
            const customVal = Number(
              document.getElementById("custom-limit").value
            );
            const customUnit =
              document.getElementById("custom-limit-unit").value;
            deltaAllow = lengthToMeters(customVal, customUnit);
          }
          if (!isFinite(deltaAllow) || deltaAllow <= 0) {
            limitDisplay.textContent = "Invalid deflection limit.";
            ratioDisplay.textContent = "";
          } else {
            const ratio = L / maxDeflAbs;
            const pass = maxDeflAbs <= deltaAllow;
            const status = pass ? "PASS" : "FAIL";
            limitDisplay.innerHTML =
              `<span class="font-semibold ${
                pass ? "text-green-700" : "text-red-700"
              }">${status}</span> &mdash; ` +
              `δ<sub>max</sub> = ${maxDeflUser.toFixed(3)} ${Lunit}, ` +
              `δ<sub>allow</sub> = ${metersToLength(deltaAllow, Lunit).toFixed(
                3
              )} ${Lunit}`;
            ratioDisplay.textContent =
              "Equivalent span ratio: L/δ ≈ " + ratio.toFixed(1);
          }
        }

        // Store state globally for probe
        window.__beamDeflectionState = state;
      }

      function probeDeflection() {
        const state = window.__beamDeflectionState || gatherState();
        const { L, Lunit } = state;
        const xUser = Number(document.getElementById("probe-x").value);
        if (!isFinite(xUser)) return;
        const x = lengthToMeters(xUser, Lunit);
        if (x < 0 || x > L) {
          document.getElementById("probe-result").textContent =
            "Position is outside the span.";
          return;
        }
        const v = computeDeflectionAt(x, state);
        const vUser = metersToLength(Math.abs(v), Lunit);
        const dir = v < 0 ? "upward" : "downward";
        document.getElementById("probe-result").textContent =
          "δ(x) ≈ " + vUser.toFixed(4) + " " + Lunit + " (" + dir + ").";
      }

      function resetForm() {
        document.getElementById("beam-type").value = "simply-supported";
        document.getElementById("span").value = 5;
        document.getElementById("span-unit").value = "m";
        document.getElementById("E").value = 200000;
        document.getElementById("E-unit").value = "MPa";
        document.getElementById("I").value = 8.33e-6;
        document.getElementById("I-unit").value = "m4";
        document.getElementById("loads-container").innerHTML = "";
        document.getElementById("limit-type").value = "L/360";
        document.getElementById("custom-limit-wrapper").classList.add("hidden");
        document.getElementById("max-deflection-display").textContent = "–";
        document.getElementById("max-deflection-location").textContent =
          "Enter data and click Calculate.";
        document.getElementById("limit-check-display").textContent =
          "No limit selected.";
        document.getElementById("limit-ratio-display").textContent = "";
        document
          .getElementById("deflection-path")
          .setAttribute("d", "M0,30 Q50,30 100,30");
        document.getElementById("load-markers").innerHTML = "";
        updateSpanUnitLabels();
        updateBeamDiagramSupports();
      }

      document.addEventListener("DOMContentLoaded", () => {
        const loadsContainer = document.getElementById("loads-container");
        document
          .getElementById("add-point-load")
          .addEventListener("click", () => {
            loadsContainer.appendChild(createPointLoadRow());
            updateSpanUnitLabels();
            renderLoadMarkers();
          });
        document.getElementById("add-udl").addEventListener("click", () => {
          loadsContainer.appendChild(createUDLRow());
          updateSpanUnitLabels();
          renderLoadMarkers();
        });
        loadsContainer.addEventListener("click", (e) => {
          if (e.target.classList.contains("remove-load")) {
            e.target.closest("div[data-type]").remove();
            renderLoadMarkers();
          }
        });
        loadsContainer.addEventListener("input", (e) => {
          if (e.target.tagName === "INPUT" || e.target.tagName === "SELECT") {
            renderLoadMarkers();
          }
        });

        document.getElementById("span-unit").addEventListener("change", () => {
          updateSpanUnitLabels();
          renderLoadMarkers();
        });
        document
          .getElementById("span")
          .addEventListener("input", renderLoadMarkers);
        document.getElementById("beam-type").addEventListener("change", () => {
          updateBeamDiagramSupports();
          renderLoadMarkers();
        });

        document
          .getElementById("limit-type")
          .addEventListener("change", (e) => {
            const val = e.target.value;
            const wrapper = document.getElementById("custom-limit-wrapper");
            if (val === "custom") {
              wrapper.classList.remove("hidden");
            } else {
              wrapper.classList.add("hidden");
            }
          });

        document
          .getElementById("calculate-btn")
          .addEventListener("click", calculateDeflection);
        document
          .getElementById("probe-btn")
          .addEventListener("click", probeDeflection);
        document
          .getElementById("reset-btn")
          .addEventListener("click", resetForm);

        // Initialize with one UDL example
        loadsContainer.appendChild(createUDLRow());
        updateSpanUnitLabels();
        updateBeamDiagramSupports();
        renderLoadMarkers();
      });
    </script>

