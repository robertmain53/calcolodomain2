    <script>
      // --- Utility functions ---
      function parseItems(text, delimiter, caseSensitive, ignoreDuplicates) {
        let raw;
        if (delimiter === "newline") {
          raw = text.split(/\r?\n/);
        } else if (delimiter === "comma") {
          raw = text.split(/,/);
        } else {
          raw = text.split(/\s+/);
        }
        let items = raw.map((s) => s.trim()).filter(Boolean);
        if (!caseSensitive) {
          items = items.map((s) => s.toLowerCase());
        }
        if (ignoreDuplicates) {
          const seen = new Set();
          items = items.filter((x) => {
            if (seen.has(x)) return false;
            seen.add(x);
            return true;
          });
        }
        return items;
      }

      function computeIntersections(sets, labels) {
        const n = sets.length;
        const results = [];
        const allKeys = new Set();
        sets.forEach((s) => s.forEach((v) => allKeys.add(v)));

        const keys = Array.from(allKeys);
        const regionMap = new Map(); // mask -> items

        keys.forEach((item) => {
          let mask = 0;
          sets.forEach((s, idx) => {
            if (s.has(item)) mask |= 1 << idx;
          });
          if (mask === 0) return;
          if (!regionMap.has(mask)) regionMap.set(mask, []);
          regionMap.get(mask).push(item);
        });

        regionMap.forEach((items, mask) => {
          const active = [];
          for (let i = 0; i < n; i++) {
            if (mask & (1 << i))
              active.push(labels[i] || String.fromCharCode(65 + i));
          }
          results.push({
            mask,
            key: active.join(" ∩ "),
            sets: active,
            count: items.length,
            items: items.slice().sort(),
          });
        });

        results.sort((a, b) => {
          const bitsA = a.sets.length;
          const bitsB = b.sets.length;
          if (bitsA !== bitsB) return bitsB - bitsA;
          return a.key.localeCompare(b.key);
        });

        return results;
      }

      // --- UI setup ---
      const setCountSelect = document.getElementById("set-count");
      const delimiterSelect = document.getElementById("delimiter");
      const ignoreDuplicatesCheckbox =
        document.getElementById("ignore-duplicates");
      const caseSensitiveCheckbox = document.getElementById("case-sensitive");
      const setTextareasContainer = document.getElementById("set-textareas");
      const btnSample = document.getElementById("btn-sample");
      const btnClear = document.getElementById("btn-clear");
      const btnGenerate = document.getElementById("btn-generate");
      const statusMessage = document.getElementById("status-message");
      const vennSvg = document.getElementById("venn-svg");
      const selectedRegionLabel = document.getElementById(
        "selected-region-label"
      );
      const selectedRegionItems = document.getElementById(
        "selected-region-items"
      );
      const summaryTableBody = document.getElementById("summary-table-body");
      const btnDownloadPng = document.getElementById("btn-download-png");
      const btnDownloadSvg = document.getElementById("btn-download-svg");
      const btnDownloadCsv = document.getElementById("btn-download-csv");

      let currentResults = [];
      let currentLabels = [];
      let currentSetCount = 3;

      // Create textareas dynamically
      function renderSetInputs(count) {
        setTextareasContainer.innerHTML = "";
        const colors = ["#60a5fa", "#34d399", "#fbbf24", "#a855f7"];
        for (let i = 0; i < count; i++) {
          const wrapper = document.createElement("div");
          wrapper.innerHTML = `
      <label class="block text-sm font-medium text-gray-700 mb-1" for="set-${i}">
        Set ${String.fromCharCode(65 + i)} label
      </label>
      <input id="label-${i}" type="text" class="w-full mb-2 border-gray-300 rounded-md shadow-sm focus:ring-blue-500 focus:border-blue-500 text-sm" value="Set ${String.fromCharCode(
            65 + i
          )}">
      <label class="block text-xs font-medium text-gray-500 mb-1" for="set-${i}">
        Items for set ${String.fromCharCode(65 + i)}
      </label>
      <textarea id="set-${i}" rows="5" class="w-full border-gray-300 rounded-md shadow-sm focus:ring-blue-500 focus:border-blue-500 text-xs" placeholder="Enter items for set ${String.fromCharCode(
            65 + i
          )}..."></textarea>
      <p class="mt-1 text-[11px] text-gray-400 flex items-center gap-1">
        <span class="inline-block w-3 h-3 rounded-full" style="background:${
          colors[i]
        }"></span>
        Color for set ${String.fromCharCode(65 + i)}
      </p>
    `;
          setTextareasContainer.appendChild(wrapper);
        }
        currentSetCount = count;
      }

      setCountSelect.addEventListener("change", () => {
        const count = parseInt(setCountSelect.value, 10);
        renderSetInputs(count);
        clearDiagram();
      });

      btnSample.addEventListener("click", () => {
        const count = parseInt(setCountSelect.value, 10);
        renderSetInputs(count);
        const samples = [
          ["apple", "banana", "cherry", "date", "fig"],
          ["banana", "cherry", "grape", "kiwi"],
          ["cherry", "fig", "kiwi", "lemon", "mango"],
          ["banana", "lemon", "pear", "fig"],
        ];
        for (let i = 0; i < count; i++) {
          const ta = document.getElementById(`set-${i}`);
          ta.value = samples[i].join("\n");
        }
        statusMessage.textContent =
          "Sample data loaded. Click “Generate Venn diagram”.";
      });

      btnClear.addEventListener("click", () => {
        const count = parseInt(setCountSelect.value, 10);
        for (let i = 0; i < count; i++) {
          const ta = document.getElementById(`set-${i}`);
          if (ta) ta.value = "";
        }
        clearDiagram();
        statusMessage.textContent = "Inputs cleared.";
      });

      function clearDiagram() {
        currentResults = [];
        vennSvg.innerHTML = "";
        summaryTableBody.innerHTML = "";
        selectedRegionLabel.textContent = "No region selected yet.";
        selectedRegionItems.innerHTML = "";
      }

      // Generate diagram and table
      btnGenerate.addEventListener("click", () => {
        const count = parseInt(setCountSelect.value, 10);
        const delimiter = delimiterSelect.value;
        const ignoreDuplicates = ignoreDuplicatesCheckbox.checked;
        const caseSensitive = caseSensitiveCheckbox.checked;

        const sets = [];
        const labels = [];
        for (let i = 0; i < count; i++) {
          const labelInput = document.getElementById(`label-${i}`);
          const ta = document.getElementById(`set-${i}`);
          if (!ta) continue;
          const label =
            (labelInput && labelInput.value.trim()) ||
            `Set ${String.fromCharCode(65 + i)}`;
          labels.push(label);
          const items = parseItems(
            ta.value || "",
            delimiter,
            caseSensitive,
            ignoreDuplicates
          );
          sets.push(new Set(items));
        }

        const totalItems = sets.reduce((acc, s) => acc + s.size, 0);
        if (totalItems === 0) {
          statusMessage.textContent =
            "Please enter at least one item in any set.";
          clearDiagram();
          return;
        }

        currentResults = computeIntersections(sets, labels);
        currentLabels = labels;
        drawVenn(count, labels, currentResults);
        renderSummaryTable(currentResults);
        statusMessage.textContent = `Computed ${currentResults.length} non-empty region(s).`;
      });

      // Draw Venn diagram (approximate geometry)
      function drawVenn(count, labels, results) {
        vennSvg.innerHTML = "";
        const width = 600;
        const height = 400;
        const cx = width / 2;
        const cy = height / 2;
        const baseRadius = count === 2 ? 120 : count === 3 ? 130 : 120;

        const colors = ["#60a5fa", "#34d399", "#fbbf24", "#a855f7"];
        const positions = [];

        if (count === 2) {
          positions.push({ x: cx - 70, y: cy });
          positions.push({ x: cx + 70, y: cy });
        } else if (count === 3) {
          positions.push({ x: cx - 80, y: cy + 30 });
          positions.push({ x: cx + 80, y: cy + 30 });
          positions.push({ x: cx, y: cy - 80 });
        } else {
          const r = 110;
          for (let i = 0; i < 4; i++) {
            const angle = (Math.PI / 2) * i + Math.PI / 4;
            positions.push({
              x: cx + r * Math.cos(angle),
              y: cy + r * Math.sin(angle),
            });
          }
        }

        // Draw circles
        positions.forEach((pos, i) => {
          const circle = document.createElementNS(
            "http://www.w3.org/2000/svg",
            "circle"
          );
          circle.setAttribute("cx", pos.x);
          circle.setAttribute("cy", pos.y);
          circle.setAttribute("r", baseRadius);
          circle.setAttribute("fill", colors[i] + "33");
          circle.setAttribute("stroke", colors[i]);
          circle.setAttribute("stroke-width", "2");
          vennSvg.appendChild(circle);

          const label = document.createElementNS(
            "http://www.w3.org/2000/svg",
            "text"
          );
          label.setAttribute("x", pos.x);
          label.setAttribute("y", pos.y - baseRadius - 10);
          label.setAttribute("text-anchor", "middle");
          label.setAttribute("class", "venn-set-label");
          label.setAttribute("fill", "#111827");
          label.setAttribute("font-size", "13");
          label.setAttribute("font-weight", "600");
          label.textContent = labels[i] || `Set ${String.fromCharCode(65 + i)}`;
          vennSvg.appendChild(label);
        });

        // Region label positions (approximate)
        const regionPositions = {};
        function regionCenter(mask) {
          const active = [];
          for (let i = 0; i < count; i++) {
            if (mask & (1 << i)) active.push(i);
          }
          if (active.length === 1) {
            const i = active[0];
            const pos = positions[i];
            const angle = Math.atan2(pos.y - cy, pos.x - cx);
            const r = baseRadius * 0.6;
            return {
              x: pos.x + r * Math.cos(angle),
              y: pos.y + r * Math.sin(angle),
            };
          } else {
            let x = 0,
              y = 0;
            active.forEach((i) => {
              x += positions[i].x;
              y += positions[i].y;
            });
            return { x: x / active.length, y: y / active.length };
          }
        }

        results.forEach((region) => {
          const { mask, count: c } = region;
          const center = regionCenter(mask);
          regionPositions[mask] = center;

          const g = document.createElementNS("http://www.w3.org/2000/svg", "g");
          g.setAttribute("data-mask", mask);
          g.style.cursor = "pointer";

          const bg = document.createElementNS(
            "http://www.w3.org/2000/svg",
            "circle"
          );
          bg.setAttribute("cx", center.x);
          bg.setAttribute("cy", center.y - 6);
          bg.setAttribute("r", 16);
          bg.setAttribute("fill", "#ffffffcc");
          g.appendChild(bg);

          const text = document.createElementNS(
            "http://www.w3.org/2000/svg",
            "text"
          );
          text.setAttribute("x", center.x);
          text.setAttribute("y", center.y);
          text.setAttribute("text-anchor", "middle");
          text.setAttribute("class", "venn-region-label");
          text.textContent = c;
          g.appendChild(text);

          g.addEventListener("mouseenter", () => highlightRegion(mask));
          g.addEventListener("mouseleave", () => highlightRegion(null));
          g.addEventListener("click", () => selectRegion(mask));

          vennSvg.appendChild(g);
        });

        // Default: select full intersection if exists, else first region
        if (results.length > 0) {
          const fullMask = (1 << count) - 1;
          const full = results.find((r) => r.mask === fullMask) || results[0];
          selectRegion(full.mask);
        }
      }

      function renderSummaryTable(results) {
        summaryTableBody.innerHTML = "";
        results.forEach((region) => {
          const tr = document.createElement("tr");
          tr.className = "hover:bg-gray-50 cursor-pointer";
          tr.dataset.mask = region.mask;

          const tdRegion = document.createElement("td");
          tdRegion.className = "px-2 py-1 border-b border-gray-100";
          tdRegion.textContent = region.key;
          const tdCount = document.createElement("td");
          tdCount.className = "px-2 py-1 border-b border-gray-100 text-right";
          tdCount.textContent = region.count;

          tr.appendChild(tdRegion);
          tr.appendChild(tdCount);

          tr.addEventListener("click", () => selectRegion(region.mask));
          summaryTableBody.appendChild(tr);
        });
      }

      function selectRegion(mask) {
        const region = currentResults.find((r) => r.mask === mask);
        if (!region) {
          selectedRegionLabel.textContent = "No region selected.";
          selectedRegionItems.innerHTML = "";
          return;
        }
        selectedRegionLabel.textContent = `Region: ${region.key} (${
          region.count
        } item${region.count === 1 ? "" : "s"})`;
        if (region.items.length === 0) {
          selectedRegionItems.innerHTML =
            '<p class="text-gray-400">No items in this intersection.</p>';
        } else {
          const ul = document.createElement("ul");
          region.items.forEach((item) => {
            const li = document.createElement("li");
            li.textContent = item;
            ul.appendChild(li);
          });
          selectedRegionItems.innerHTML = "";
          selectedRegionItems.appendChild(ul);
        }
        highlightRegion(mask);
      }

      function highlightRegion(mask) {
        const groups = vennSvg.querySelectorAll("g[data-mask]");
        groups.forEach((g) => {
          const thisMask = parseInt(g.getAttribute("data-mask"), 10);
          if (mask == null) {
            g.style.opacity = "1";
          } else {
            g.style.opacity = thisMask === mask ? "1" : "0.25";
          }
        });
      }

      // Export SVG
      btnDownloadSvg.addEventListener("click", () => {
        if (!currentResults.length) return;
        const serializer = new XMLSerializer();
        const source = serializer.serializeToString(vennSvg);
        const blob = new Blob([source], {
          type: "image/svg+xml;charset=utf-8",
        });
        const url = URL.createObjectURL(blob);
        const a = document.createElement("a");
        a.href = url;
        a.download = "venn-diagram.svg";
        document.body.appendChild(a);
        a.click();
        document.body.removeChild(a);
        URL.revokeObjectURL(url);
      });

      // Export PNG using canvas
      btnDownloadPng.addEventListener("click", () => {
        if (!currentResults.length) return;
        const serializer = new XMLSerializer();
        const source = serializer.serializeToString(vennSvg);
        const svgBlob = new Blob([source], {
          type: "image/svg+xml;charset=utf-8",
        });
        const url = URL.createObjectURL(svgBlob);
        const img = new Image();
        img.onload = function () {
          const canvas = document.createElement("canvas");
          canvas.width = 1200;
          canvas.height = 800;
          const ctx = canvas.getContext("2d");
          ctx.fillStyle = "#ffffff";
          ctx.fillRect(0, 0, canvas.width, canvas.height);
          ctx.drawImage(img, 0, 0, canvas.width, canvas.height);
          URL.revokeObjectURL(url);
          canvas.toBlob((blob) => {
            const pngUrl = URL.createObjectURL(blob);
            const a = document.createElement("a");
            a.href = pngUrl;
            a.download = "venn-diagram.png";
            document.body.appendChild(a);
            a.click();
            document.body.removeChild(a);
            URL.revokeObjectURL(pngUrl);
          }, "image/png");
        };
        img.src = url;
      });

      // Export CSV of intersections
      btnDownloadCsv.addEventListener("click", () => {
        if (!currentResults.length) return;
        const rows = [["Region", "Count", "Items"]];
        currentResults.forEach((r) => {
          rows.push([
            r.key,
            String(r.count),
            '"' + r.items.join("; ").replace(/"/g, '""') + '"',
          ]);
        });
        const csv = rows.map((r) => r.join(",")).join("\n");
        const blob = new Blob([csv], { type: "text/csv;charset=utf-8;" });
        const url = URL.createObjectURL(blob);
        const a = document.createElement("a");
        a.href = url;
        a.download = "venn-intersections.csv";
        document.body.appendChild(a);
        a.click();
        document.body.removeChild(a);
        URL.revokeObjectURL(url);
      });

      // Initial render
      renderSetInputs(currentSetCount);
    </script>
  
