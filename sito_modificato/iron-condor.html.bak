    <script>
      // Simple payoff chart without external libraries
      const payoffCanvas = document.getElementById("payoffChart");
      const ctx = payoffCanvas.getContext
        ? payoffCanvas.getContext("2d")
        : null;

      function computePayoffAtPrice(S, params) {
        const {
          longPutStrike,
          shortPutStrike,
          shortCallStrike,
          longCallStrike,
          longPutPremium,
          shortPutPremium,
          shortCallPremium,
          longCallPremium,
          contracts,
        } = params;

        const size = 100 * contracts;

        // Long put
        const longPut = Math.max(longPutStrike - S, 0) - longPutPremium;
        // Short put
        const shortPut = shortPutPremium - Math.max(shortPutStrike - S, 0);
        // Short call
        const shortCall = shortCallPremium - Math.max(S - shortCallStrike, 0);
        // Long call
        const longCall = Math.max(S - longCallStrike, 0) - longCallPremium;

        return (longPut + shortPut + shortCall + longCall) * size;
      }

      function getInputs() {
        const val = (id) => parseFloat(document.getElementById(id).value);

        return {
          underlyingPrice: val("underlyingPrice"),
          contracts: Math.max(
            1,
            parseInt(document.getElementById("contracts").value, 10) || 1
          ),
          longPutStrike: val("longPutStrike"),
          shortPutStrike: val("shortPutStrike"),
          longPutPremium: val("longPutPremium"),
          shortPutPremium: val("shortPutPremium"),
          shortCallStrike: val("shortCallStrike"),
          longCallStrike: val("longCallStrike"),
          shortCallPremium: val("shortCallPremium"),
          longCallPremium: val("longCallPremium"),
        };
      }

      function formatMoney(x) {
        if (!isFinite(x)) return "–";
        const sign = x < 0 ? "-" : "";
        const v = Math.abs(x).toFixed(2);
        return sign + "$" + v.replace(/\B(?=(\d{3})+(?!\d))/g, ",");
      }

      function calculateIronCondor() {
        const inp = getInputs();

        // Net credit per share
        const netCreditPerShare =
          inp.shortPutPremium +
          inp.shortCallPremium -
          (inp.longPutPremium + inp.longCallPremium);
        const netCreditPerCondor = netCreditPerShare * 100;
        const totalNetCredit = netCreditPerCondor * inp.contracts;

        // Spread widths
        const putWidth = Math.abs(inp.shortPutStrike - inp.longPutStrike);
        const callWidth = Math.abs(inp.longCallStrike - inp.shortCallStrike);
        const maxWidth = Math.max(putWidth, callWidth);

        const maxLossPerShare = Math.max(0, maxWidth - netCreditPerShare);
        const maxLossTotal = maxLossPerShare * 100 * inp.contracts;

        const maxProfitTotal = totalNetCredit;

        // Break-evens
        const lowerBE = inp.shortPutStrike - netCreditPerShare;
        const upperBE = inp.shortCallStrike + netCreditPerShare;

        // Risk / reward
        let rrText = "–";
        if (maxLossTotal > 0 && isFinite(maxLossTotal)) {
          const rr = maxLossTotal / maxProfitTotal;
          rrText = rr.toFixed(2) + " : 1 (risk : reward)";
        }

        // Update summary
        document.getElementById("netCredit").textContent =
          formatMoney(netCreditPerCondor);
        document.getElementById("maxProfit").textContent =
          formatMoney(maxProfitTotal);
        document.getElementById("maxLoss").textContent = formatMoney(
          -maxLossTotal
        );
        document.getElementById("rrRatio").textContent = rrText;
        document.getElementById("lowerBE").textContent = isFinite(lowerBE)
          ? lowerBE.toFixed(2)
          : "–";
        document.getElementById("upperBE").textContent = isFinite(upperBE)
          ? upperBE.toFixed(2)
          : "–";
        document.getElementById("profitZone").textContent =
          isFinite(lowerBE) && isFinite(upperBE)
            ? `${lowerBE.toFixed(2)} – ${upperBE.toFixed(2)}`
            : "–";
        document.getElementById("capitalAtRisk").textContent =
          formatMoney(maxLossTotal);

        // Scenario P&L
        const scenarioPrice = parseFloat(
          document.getElementById("scenarioPrice").value
        );
        const scenarioPL = computePayoffAtPrice(scenarioPrice, {
          ...inp,
          contracts: inp.contracts,
        });
        document.getElementById("scenarioPL").textContent =
          formatMoney(scenarioPL);

        const plLowerBE = computePayoffAtPrice(lowerBE, {
          ...inp,
          contracts: inp.contracts,
        });
        const plUpperBE = computePayoffAtPrice(upperBE, {
          ...inp,
          contracts: inp.contracts,
        });
        document.getElementById("plAtLowerBE").textContent =
          formatMoney(plLowerBE);
        document.getElementById("plAtUpperBE").textContent =
          formatMoney(plUpperBE);

        // Draw payoff chart
        if (ctx) {
          drawPayoffChart(inp, lowerBE, upperBE);
        }
      }

      function drawPayoffChart(inp, lowerBE, upperBE) {
        const width = (payoffCanvas.width = payoffCanvas.clientWidth || 600);
        const height = (payoffCanvas.height = 320);

        // Build price range
        const minStrike = Math.min(
          inp.longPutStrike,
          inp.shortPutStrike,
          inp.shortCallStrike,
          inp.longCallStrike
        );
        const maxStrike = Math.max(
          inp.longPutStrike,
          inp.shortPutStrike,
          inp.shortCallStrike,
          inp.longCallStrike
        );
        const padding = (maxStrike - minStrike) * 0.4 || 10;

        const xMin = minStrike - padding;
        const xMax = maxStrike + padding;

        const points = [];
        let yMin = Infinity,
          yMax = -Infinity;

        const steps = 120;
        for (let i = 0; i <= steps; i++) {
          const S = xMin + (i / steps) * (xMax - xMin);
          const pl = computePayoffAtPrice(S, {
            ...inp,
            contracts: inp.contracts,
          });
          points.push({ S, pl });
          if (pl < yMin) yMin = pl;
          if (pl > yMax) yMax = pl;
        }

        if (!isFinite(yMin) || !isFinite(yMax)) {
          yMin = -100;
          yMax = 100;
        }
        if (yMin === yMax) {
          yMin -= 100;
          yMax += 100;
        }

        const margin = { left: 50, right: 20, top: 20, bottom: 40 };

        const xScale = (S) =>
          margin.left +
          ((S - xMin) / (xMax - xMin)) * (width - margin.left - margin.right);
        const yScale = (pl) =>
          height -
          margin.bottom -
          ((pl - yMin) / (yMax - yMin)) * (height - margin.top - margin.bottom);

        ctx.clearRect(0, 0, width, height);

        // Axes
        ctx.strokeStyle = "#9CA3AF";
        ctx.lineWidth = 1;

        // x-axis at P&L = 0
        const yZero = yScale(0);
        ctx.beginPath();
        ctx.moveTo(margin.left, yZero);
        ctx.lineTo(width - margin.right, yZero);
        ctx.stroke();

        // y-axis at underlying ~ current price
        const xSpot = xScale(inp.underlyingPrice);
        ctx.beginPath();
        ctx.moveTo(xSpot, margin.top);
        ctx.lineTo(xSpot, height - margin.bottom);
        ctx.strokeStyle = "#E5E7EB";
        ctx.setLineDash([4, 4]);
        ctx.stroke();
        ctx.setLineDash([]);

        // Payoff line
        ctx.beginPath();
        points.forEach((p, i) => {
          const x = xScale(p.S);
          const y = yScale(p.pl);
          if (i === 0) ctx.moveTo(x, y);
          else ctx.lineTo(x, y);
        });
        ctx.strokeStyle = "#2563EB";
        ctx.lineWidth = 2;
        ctx.stroke();

        // Mark break-evens
        ctx.fillStyle = "#10B981";
        ctx.strokeStyle = "#10B981";
        ctx.lineWidth = 1.5;

        [lowerBE, upperBE].forEach((be) => {
          if (!isFinite(be)) return;
          const x = xScale(be);
          const y = yScale(0);
          ctx.beginPath();
          ctx.moveTo(x, margin.top);
          ctx.lineTo(x, height - margin.bottom);
          ctx.setLineDash([4, 4]);
          ctx.stroke();
          ctx.setLineDash([]);

          ctx.beginPath();
          ctx.arc(x, y, 3, 0, 2 * Math.PI);
          ctx.fill();
        });

        // Labels
        ctx.fillStyle = "#4B5563";
        ctx.font = "11px Inter, system-ui, sans-serif";
        ctx.textAlign = "center";
        ctx.fillText("Underlying price at expiration", width / 2, height - 8);

        ctx.save();
        ctx.translate(12, height / 2);
        ctx.rotate(-Math.PI / 2);
        ctx.textAlign = "center";
        ctx.fillText("P&L (total position)", 0, 0);
        ctx.restore();
      }

      document
        .getElementById("calculateBtn")
        .addEventListener("click", calculateIronCondor);
      document
        .getElementById("scenarioPrice")
        .addEventListener("input", calculateIronCondor);

      document.getElementById("resetBtn").addEventListener("click", () => {
        document.getElementById("underlyingPrice").value = 100;
        document.getElementById("contracts").value = 1;
        document.getElementById("longPutStrike").value = 90;
        document.getElementById("shortPutStrike").value = 95;
        document.getElementById("longPutPremium").value = 0.8;
        document.getElementById("shortPutPremium").value = 1.5;
        document.getElementById("shortCallStrike").value = 105;
        document.getElementById("longCallStrike").value = 110;
        document.getElementById("shortCallPremium").value = 1.4;
        document.getElementById("longCallPremium").value = 0.7;
        document.getElementById("scenarioPrice").value = 100;
        calculateIronCondor();
      });

      // Initial calculation
      window.addEventListener("load", calculateIronCondor);
    </script>

