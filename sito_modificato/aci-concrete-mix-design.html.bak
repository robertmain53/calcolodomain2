    <script>
      // Simple helpers
      function clamp(val, min, max) {
        return Math.min(Math.max(val, min), max);
      }

      const state = {
        unit: "metric", // 'metric' or 'us'
      };

      const els = {
        btnMetric: document.getElementById("btn-metric"),
        btnUS: document.getElementById("btn-us"),
        fcUnit: document.getElementById("fc-unit"),
        slumpUnit: document.getElementById("slump-unit"),
        unitWeightCoarseUnit: document.getElementById(
          "unit-weight-coarse-unit"
        ),
        thMass: document.getElementById("th-mass"),
        thMassUS: document.getElementById("th-mass-us"),
        form: document.getElementById("aci-mix-form"),
        btnCalc: document.getElementById("btn-calc"),
        btnReset: document.getElementById("btn-reset"),
        btnSuggestVca: document.getElementById("btn-suggest-vca"),
        error: document.getElementById("calc-error"),
        resultsContainer: document.getElementById("results-container"),
        resultsBody: document.getElementById("results-body"),
        summaryList: document.getElementById("summary-list"),
        checkList: document.getElementById("check-list"),
        resultsPlaceholder: document.getElementById("results-placeholder"),
      };

      function setUnitSystem(unit) {
        state.unit = unit;
        if (unit === "metric") {
          els.btnMetric.classList.add("bg-blue-600", "text-white");
          els.btnMetric.classList.remove("bg-white", "text-gray-700");
          els.btnUS.classList.remove("bg-blue-600", "text-white");
          els.btnUS.classList.add("bg-white", "text-gray-700");
          els.fcUnit.textContent = "MPa";
          els.slumpUnit.textContent = "mm";
          els.unitWeightCoarseUnit.textContent = "kg/m³";
          els.thMass.textContent = "Mass (kg/m³)";
          els.thMassUS.classList.add("hidden");
        } else {
          els.btnUS.classList.add("bg-blue-600", "text-white");
          els.btnUS.classList.remove("bg-white", "text-gray-700");
          els.btnMetric.classList.remove("bg-blue-600", "text-white");
          els.btnMetric.classList.add("bg-white", "text-gray-700");
          els.fcUnit.textContent = "psi";
          els.slumpUnit.textContent = "in";
          els.unitWeightCoarseUnit.textContent = "lb/ft³";
          els.thMass.textContent = "Mass (kg/m³)";
          els.thMassUS.classList.remove("hidden");
        }
      }

      els.btnMetric.addEventListener("click", () => setUnitSystem("metric"));
      els.btnUS.addEventListener("click", () => setUnitSystem("us"));

      // Suggest coarse aggregate volume fraction based on size & FM (approximate ACI 211.1)
      els.btnSuggestVca.addEventListener("click", () => {
        const size =
          parseFloat(document.getElementById("agg-size").value) || 19;
        const fm = parseFloat(document.getElementById("fm-sand").value) || 2.7;
        let base;
        if (size <= 10) base = 0.5;
        else if (size <= 13) base = 0.56;
        else if (size <= 20) base = 0.62;
        else if (size <= 26) base = 0.66;
        else base = 0.7;
        // Adjust slightly for sand fineness modulus
        const delta = (fm - 2.7) * 0.02;
        const vca = clamp(base + delta, 0.45, 0.72);
        document.getElementById("vca-factor").value = vca.toFixed(2);
      });

      function estimateWaterContent(slump, aggSize, airPercent) {
        // Very approximate ACI-style water content for non-air-entrained concrete, metric (kg/m³)
        // Base for 75 mm slump:
        let base;
        if (aggSize <= 10) base = 205;
        else if (aggSize <= 13) base = 200;
        else if (aggSize <= 20) base = 185;
        else if (aggSize <= 26) base = 180;
        else base = 175;

        // Adjust for slump difference from 75 mm (3 in): +3 kg/m³ per 25 mm
        const deltaSlump = clamp(slump, 25, 200) - 75;
        base += (deltaSlump / 25) * 3;

        // Adjust for air-entrained: reduce water ~3% per 1% air above 2%
        const extraAir = Math.max(0, airPercent - 2);
        base *= 1 - 0.03 * extraAir;

        return base;
      }

      function calculateMix() {
        els.error.textContent = "";
        const fc = parseFloat(document.getElementById("fc").value);
        const slump = parseFloat(document.getElementById("slump").value);
        const aggSize = parseFloat(document.getElementById("agg-size").value);
        const air = parseFloat(document.getElementById("air-content").value);
        const wc = parseFloat(document.getElementById("wc-ratio").value);
        const fm = parseFloat(document.getElementById("fm-sand").value);
        const sgCement = parseFloat(document.getElementById("sg-cement").value);
        const sgCoarse = parseFloat(document.getElementById("sg-coarse").value);
        const sgFine = parseFloat(document.getElementById("sg-fine").value);
        const unitWeightCoarse = parseFloat(
          document.getElementById("unit-weight-coarse").value
        );
        const vcaFactor = parseFloat(
          document.getElementById("vca-factor").value
        );

        if (!wc || wc <= 0 || wc > 0.8) {
          els.error.textContent =
            "Please enter a realistic water–cement ratio (0.30–0.80).";
          return;
        }
        if (!sgCement || !sgCoarse || !sgFine) {
          els.error.textContent =
            "Please provide specific gravities for cement, coarse and fine aggregates.";
          return;
        }
        if (!vcaFactor || vcaFactor <= 0 || vcaFactor >= 0.9) {
          els.error.textContent =
            "Please enter a realistic coarse aggregate volume fraction (e.g., 0.55–0.70).";
          return;
        }

        // Work internally in metric per 1 m³
        const gammaW = 1000; // kg/m³
        const vAir = clamp(air / 100, 0, 0.1);

        // Estimate water content (kg/m³)
        let waterKg = estimateWaterContent(
          state.unit === "metric" ? slump : slump * 25.4,
          aggSize,
          air
        );
        waterKg = clamp(waterKg, 140, 230);

        // Cement content (kg/m³)
        const cementKg = waterKg / wc;

        // Absolute volumes
        const vWater = waterKg / gammaW;
        const vCement = cementKg / (sgCement * gammaW);

        // Coarse aggregate absolute volume
        const vCoarse = vcaFactor; // fraction of 1 m³

        // Remaining volume for fine aggregate
        const vFine = 1.0 - (vAir + vWater + vCement + vCoarse);

        if (vFine <= 0) {
          els.error.textContent =
            "Parameters lead to negative fine aggregate volume. Reduce coarse aggregate fraction or water/cement contents.";
          return;
        }

        const fineKg = vFine * sgFine * gammaW;
        const coarseKg = vCoarse * sgCoarse * gammaW;

        // US conversion (approx)
        const kgPerM3ToLbPerYd3 = 1.68555; // 1 kg/m³ ≈ 1.68555 lb/yd³
        const waterLbYd3 = waterKg * kgPerM3ToLbPerYd3;
        const cementLbYd3 = cementKg * kgPerM3ToLbPerYd3;
        const fineLbYd3 = fineKg * kgPerM3ToLbPerYd3;
        const coarseLbYd3 = coarseKg * kgPerM3ToLbPerYd3;

        // Populate table
        els.resultsBody.innerHTML = "";
        function addRow(name, massKg, vol, fraction) {
          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="px-3 py-2 border-b">${name}</td>
        <td class="px-3 py-2 border-b text-right">${massKg.toFixed(1)}</td>
        <td class="px-3 py-2 border-b text-right ${
          state.unit === "us" ? "" : "hidden"
        }">
          ${(massKg * kgPerM3ToLbPerYd3).toFixed(1)}
        </td>
        <td class="px-3 py-2 border-b text-right">${vol.toFixed(4)}</td>
        <td class="px-3 py-2 border-b text-right">${(fraction * 100).toFixed(
          1
        )}%</td>
      `;
          els.resultsBody.appendChild(tr);
        }

        addRow("Cement", cementKg, vCement, vCement);
        addRow("Water", waterKg, vWater, vWater);
        addRow("Air", 0, vAir, vAir);
        addRow("Coarse aggregate", coarseKg, vCoarse, vCoarse);
        addRow("Fine aggregate", fineKg, vFine, vFine);

        // Summary
        els.summaryList.innerHTML = `
      <li><strong>Water:</strong> ${waterKg.toFixed(
        1
      )} kg/m³ (${waterLbYd3.toFixed(1)} lb/yd³)</li>
      <li><strong>Cement:</strong> ${cementKg.toFixed(
        1
      )} kg/m³ (${cementLbYd3.toFixed(1)} lb/yd³)</li>
      <li><strong>Coarse aggregate:</strong> ${coarseKg.toFixed(
        1
      )} kg/m³ (${coarseLbYd3.toFixed(1)} lb/yd³)</li>
      <li><strong>Fine aggregate:</strong> ${fineKg.toFixed(
        1
      )} kg/m³ (${fineLbYd3.toFixed(1)} lb/yd³)</li>
      <li><strong>Total solids (excl. air):</strong> ${(
        cementKg +
        waterKg +
        fineKg +
        coarseKg
      ).toFixed(1)} kg/m³</li>
    `;

        const actualWc = waterKg / cementKg;
        const volSum = vAir + vWater + vCement + vCoarse + vFine;

        els.checkList.innerHTML = `
      <li><strong>Input w/c:</strong> ${wc.toFixed(3)}</li>
      <li><strong>Calculated w/c:</strong> ${actualWc.toFixed(3)}</li>
      <li><strong>Air content:</strong> ${air.toFixed(
        1
      )}% (volume fraction ${vAir.toFixed(3)})</li>
      <li><strong>Volume check:</strong> V<sub>total</sub> = ${volSum.toFixed(
        3
      )} m³ (should be ≈ 1.000)</li>
    `;

        els.resultsContainer.classList.remove("hidden");
        els.resultsPlaceholder.classList.add("hidden");
      }

      els.btnCalc.addEventListener("click", calculateMix);

      els.btnReset.addEventListener("click", () => {
        els.form.reset();
        document.getElementById("fc").value = 30;
        document.getElementById("slump").value = 75;
        document.getElementById("agg-size").value = "19";
        document.getElementById("air-content").value = 2;
        document.getElementById("wc-ratio").value = 0.5;
        document.getElementById("fm-sand").value = 2.7;
        document.getElementById("sg-cement").value = 3.15;
        document.getElementById("sg-coarse").value = 2.65;
        document.getElementById("sg-fine").value = 2.6;
        document.getElementById("unit-weight-coarse").value = 1600;
        document.getElementById("vca-factor").value = 0.62;
        els.error.textContent = "";
        els.resultsContainer.classList.add("hidden");
        els.resultsPlaceholder.classList.remove("hidden");
      });

      // Initialize
      setUnitSystem("metric");
    </script>

