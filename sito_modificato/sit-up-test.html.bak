<script>
  // Sit-up test calculator logic
  (function() {
    const ageInput = document.getElementById('age');
    const sexInput = document.getElementById('sex');
    const repsInput = document.getElementById('reps');
    const durationInput = document.getElementById('duration');

    const rpmOutput = document.getElementById('rpmOutput');
    const ratingOutput = document.getElementById('ratingOutput');
    const ratingBadge = document.getElementById('ratingBadge');
    const percentileOutput = document.getElementById('percentileOutput');
    const relativeOutput = document.getElementById('relativeOutput');
    const summaryBox = document.getElementById('summaryBox');
    const summaryText = document.getElementById('summaryText');

    const calculateBtn = document.getElementById('calculateBtn');
    const resetBtn = document.getElementById('resetBtn');

    function clamp(val, min, max) {
      return Math.min(Math.max(val, min), max);
    }

    function getAgeBand(age) {
      if (age < 20) return 20;
      if (age < 30) return 20;
      if (age < 40) return 30;
      if (age < 50) return 40;
      if (age < 60) return 50;
      return 60;
    }

    // Thresholds for 1-min sit-ups by age band & sex (approximate)
    const thresholds = {
      male: {
        20: { excellent: 55, good: 45, average: 35, below: 25 },
        30: { excellent: 50, good: 40, average: 30, below: 20 },
        40: { excellent: 45, good: 35, average: 25, below: 15 },
        50: { excellent: 40, good: 30, average: 20, below: 12 },
        60: { excellent: 35, good: 25, average: 18, below: 10 }
      },
      female: {
        20: { excellent: 50, good: 40, average: 30, below: 22 },
        30: { excellent: 45, good: 35, average: 25, below: 18 },
        40: { excellent: 40, good: 30, average: 22, below: 15 },
        50: { excellent: 35, good: 27, average: 20, below: 12 },
        60: { excellent: 30, good: 22, average: 16, below: 10 }
      }
    };

    function ratePerformance(rpm, sex, ageBand) {
      const t = thresholds[sex][ageBand];
      let rating, percentile;

      if (rpm <= 0) {
        rating = 'Invalid';
        percentile = 0;
      } else if (rpm < t.below) {
        rating = 'Poor';
        percentile = 10;
      } else if (rpm < t.average) {
        rating = 'Below average';
        percentile = 30;
      } else if (rpm < t.good) {
        rating = 'Average';
        percentile = 50;
      } else if (rpm < t.excellent) {
        rating = 'Good';
        percentile = 75;
      } else {
        rating = 'Excellent';
        percentile = 90;
      }

      // Smooth percentile based on distance within bands
      const span = t.excellent - t.below || 1;
      const pos = clamp((rpm - t.below) / span, 0, 1);
      percentile = clamp(Math.round(10 + pos * 80), 1, 99);

      return { rating, percentile, thresholds: t };
    }

    function updateResults() {
      const age = parseInt(ageInput.value, 10);
      const sex = sexInput.value === 'female' ? 'female' : 'male';
      const reps = parseInt(repsInput.value, 10);
      const duration = parseFloat(durationInput.value);

      if (!age || !reps || !duration || duration <= 0) {
        rpmOutput.textContent = '—';
        ratingBadge.textContent = '—';
        ratingBadge.className = 'badge bg-gray-100 text-gray-700';
        percentileOutput.textContent = '—';
        relativeOutput.textContent = '—';
        summaryBox.classList.add('hidden');
        return;
      }

      const ageBand = getAgeBand(age);
      const rpm = reps * (60 / duration);
      const rpmRounded = Math.round(rpm * 10) / 10;

      const { rating, percentile, thresholds: t } = ratePerformance(rpmRounded, sex, ageBand);

      rpmOutput.textContent = isFinite(rpmRounded) ? rpmRounded.toString() : '—';
      percentileOutput.textContent = percentile + 'th';

      let badgeClass = 'badge ';
      let relativeText = '';

      switch (rating) {
        case 'Excellent':
          badgeClass += 'bg-green-100 text-green-800';
          relativeText = 'Your abdominal endurance is well above typical values for your age and sex.';
          break;
        case 'Good':
          badgeClass += 'bg-emerald-100 text-emerald-800';
          relativeText = 'You are above average compared with people of your age and sex.';
          break;
        case 'Average':
          badgeClass += 'bg-blue-100 text-blue-800';
          relativeText = 'You are around the average range for your age and sex.';
          break;
        case 'Below average':
          badgeClass += 'bg-yellow-100 text-yellow-800';
          relativeText = 'Your score is below typical values. With regular training you can improve quickly.';
          break;
        case 'Poor':
          badgeClass += 'bg-red-100 text-red-800';
          relativeText = 'Your score is well below norms. Focus on technique and gradual progression.';
          break;
        default:
          badgeClass += 'bg-gray-100 text-gray-700';
          relativeText = 'Please check your inputs.';
      }

      ratingBadge.textContent = rating;
      ratingBadge.className = badgeClass;
      relativeOutput.textContent = relativeText;

      const sexLabel = sex === 'male' ? 'men' : 'women';
      summaryText.textContent =
        `You completed approximately ${rpmRounded} sit-ups per minute. ` +
        `For ${sexLabel} aged around ${ageBand}, “average” is about ${t.average} sit-ups, ` +
        `“good” starts around ${t.good}, and “excellent” from ${t.excellent}+ sit-ups.`;
      summaryBox.classList.remove('hidden');
    }

    calculateBtn.addEventListener('click', function(e) {
      e.preventDefault();
      updateResults();
    });

    resetBtn.addEventListener('click', function(e) {
      e.preventDefault();
      sexInput.value = 'male';
      ageInput.value = 30;
      repsInput.value = 35;
      durationInput.value = 60;
      rpmOutput.textContent = '—';
      ratingBadge.textContent = '—';
      ratingBadge.className = 'badge bg-gray-100 text-gray-700';
      percentileOutput.textContent = '—';
      relativeOutput.textContent = '—';
      summaryBox.classList.add('hidden');
    });

    // Auto-calc on load with defaults
    document.addEventListener('DOMContentLoaded', updateResults);
  })();

  window.MathJax = {
    tex: { inlineMath: [['\\(','\\)'], ['$', '$']], displayMath: [['$','$'], ['\\[','\\]']] },
    svg: { fontCache: 'global' }
  };
</script>
<script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>

