    <script>
      // Tab switching
      document.querySelectorAll(".ratio-tab").forEach((btn) => {
        btn.addEventListener("click", () => {
          const category = btn.getAttribute("data-category");
          document.querySelectorAll(".ratio-tab").forEach((b) => {
            b.classList.remove(
              "border-blue-600",
              "text-blue-600",
              "bg-blue-50"
            );
            b.classList.add("border-gray-300", "text-gray-700", "bg-white");
          });
          btn.classList.add("border-blue-600", "text-blue-600", "bg-blue-50");
          btn.classList.remove("border-gray-300", "text-gray-700", "bg-white");

          document
            .querySelectorAll(".ratio-panel")
            .forEach((p) => p.classList.add("hidden"));
          const panel = document.getElementById("panel-" + category);
          if (panel) panel.classList.remove("hidden");
        });
      });

      function formatRatio(value, decimals = 2) {
        if (!isFinite(value)) return "–";
        return value.toFixed(decimals);
      }
      function formatPercent(value, decimals = 1) {
        if (!isFinite(value)) return "–";
        return (value * 100).toFixed(decimals) + "%";
      }

      // Current Ratio
      document
        .getElementById("calcCurrentRatio")
        .addEventListener("click", () => {
          const a = parseFloat(document.getElementById("currentAssets").value);
          const l = parseFloat(
            document.getElementById("currentLiabilities").value
          );
          const resEl = document.getElementById("currentRatioResult");
          const intEl = document.getElementById("currentRatioInterpretation");
          if (!a || !l || l === 0) {
            resEl.textContent = "–";
            intEl.textContent =
              "Enter positive values for both current assets and current liabilities.";
            return;
          }
          const ratio = a / l;
          resEl.textContent = formatRatio(ratio, 2) + "×";
          if (ratio < 1) {
            intEl.textContent =
              "A current ratio below 1.0 suggests potential short‑term liquidity stress and reliance on external financing.";
          } else if (ratio <= 2) {
            intEl.textContent =
              "A current ratio between 1.0 and 2.0 is typical for many stable businesses, indicating adequate short‑term liquidity.";
          } else {
            intEl.textContent =
              "A very high current ratio may indicate excess idle cash or inventory; review working capital efficiency.";
          }
        });

      // Quick Ratio
      document
        .getElementById("calcQuickRatio")
        .addEventListener("click", () => {
          const cash =
            parseFloat(document.getElementById("cashEquivalents").value) || 0;
          const sti =
            parseFloat(document.getElementById("shortTermInvestments").value) ||
            0;
          const ar =
            parseFloat(document.getElementById("accountsReceivable").value) ||
            0;
          const l = parseFloat(
            document.getElementById("quickCurrentLiabilities").value
          );
          const resEl = document.getElementById("quickRatioResult");
          const intEl = document.getElementById("quickRatioInterpretation");
          if (!l || l === 0) {
            resEl.textContent = "–";
            intEl.textContent =
              "Enter a positive value for current liabilities.";
            return;
          }
          const ratio = (cash + sti + ar) / l;
          resEl.textContent = formatRatio(ratio, 2) + "×";
          if (ratio < 0.8) {
            intEl.textContent =
              "A quick ratio below ~0.8 can be a warning sign that you may struggle to meet short‑term obligations without selling inventory or raising funds.";
          } else if (ratio <= 1.5) {
            intEl.textContent =
              "A quick ratio around 1.0–1.5 is generally considered healthy, depending on your industry and business model.";
          } else {
            intEl.textContent =
              "A very strong quick ratio suggests ample liquid reserves; ensure excess cash is being put to productive use.";
          }
        });

      // Net Profit Margin
      document.getElementById("calcNetMargin").addEventListener("click", () => {
        const ni = parseFloat(document.getElementById("netIncome").value);
        const rev = parseFloat(document.getElementById("totalRevenue").value);
        const resEl = document.getElementById("netMarginResult");
        const intEl = document.getElementById("netMarginInterpretation");
        if ((!ni && ni !== 0) || !rev || rev === 0) {
          resEl.textContent = "–";
          intEl.textContent = "Enter net income and a positive revenue figure.";
          return;
        }
        const margin = ni / rev;
        resEl.textContent = formatPercent(margin, 1);
        if (margin < 0) {
          intEl.textContent =
            "Negative net margin indicates the business is currently loss‑making after all expenses.";
        } else if (margin < 0.05) {
          intEl.textContent =
            "A thin net margin under 5% leaves little room for shocks; consider pricing, cost structure, and mix.";
        } else if (margin < 0.2) {
          intEl.textContent =
            "A net margin between 5–20% is common in many industries; compare against peers for context.";
        } else {
          intEl.textContent =
            "A very high net margin suggests strong pricing power or a lean cost base; ensure it is sustainable.";
        }
      });

      // ROA & ROE
      document.getElementById("calcRoaroe").addEventListener("click", () => {
        const ni = parseFloat(document.getElementById("roaNetIncome").value);
        const assets = parseFloat(
          document.getElementById("avgTotalAssets").value
        );
        const equity = parseFloat(document.getElementById("avgEquity").value);
        const roaEl = document.getElementById("roaResult");
        const roeEl = document.getElementById("roeResult");
        const roaInt = document.getElementById("roaInterpretation");
        const roeInt = document.getElementById("roeInterpretation");

        if (
          (!ni && ni !== 0) ||
          !assets ||
          assets === 0 ||
          !equity ||
          equity === 0
        ) {
          roaEl.textContent = "–";
          roeEl.textContent = "–";
          roaInt.textContent =
            "Enter net income, average total assets, and average equity (all positive).";
          roeInt.textContent = "";
          return;
        }
        const roa = ni / assets;
        const roe = ni / equity;
        roaEl.textContent = formatPercent(roa, 1);
        roeEl.textContent = formatPercent(roe, 1);

        if (roa < 0) {
          roaInt.textContent =
            "Negative ROA means assets are not currently generating positive earnings.";
        } else if (roa < 0.05) {
          roaInt.textContent =
            "ROA under 5% is modest; asset‑heavy industries often show lower ROA than asset‑light ones.";
        } else {
          roaInt.textContent =
            "ROA above ~5% is solid for many sectors; compare to industry norms and your cost of capital.";
        }

        if (roe < 0) {
          roeInt.textContent =
            "Negative ROE indicates shareholders are currently losing value.";
        } else if (roe < 0.1) {
          roeInt.textContent =
            "ROE under 10% is on the low side; investors may expect higher returns for the risk taken.";
        } else if (roe < 0.2) {
          roeInt.textContent =
            "ROE between 10–20% is attractive in many markets, especially if consistent over time.";
        } else {
          roeInt.textContent =
            "ROE above 20% is strong; check that it is not driven solely by high leverage.";
        }
      });

      // Debt-to-Equity
      document
        .getElementById("calcDebtEquity")
        .addEventListener("click", () => {
          const debt = parseFloat(document.getElementById("totalDebt").value);
          const equity = parseFloat(
            document.getElementById("totalEquity").value
          );
          const resEl = document.getElementById("debtEquityResult");
          const intEl = document.getElementById("debtEquityInterpretation");
          if ((!debt && debt !== 0) || !equity || equity === 0) {
            resEl.textContent = "–";
            intEl.textContent =
              "Enter total interest‑bearing debt and a positive equity figure.";
            return;
          }
          const ratio = debt / equity;
          resEl.textContent = formatRatio(ratio, 2) + "×";
          if (ratio < 0.5) {
            intEl.textContent =
              "Low leverage: the business relies more on equity than debt, which reduces financial risk but may limit returns.";
          } else if (ratio <= 2) {
            intEl.textContent =
              "Moderate leverage: common for many established companies; acceptable if cash flows are stable.";
          } else {
            intEl.textContent =
              "High leverage: debt‑to‑equity above 2× can be risky, especially in cyclical or volatile industries.";
          }
        });

      // Interest Coverage
      document
        .getElementById("calcInterestCoverage")
        .addEventListener("click", () => {
          const ebit = parseFloat(document.getElementById("ebit").value);
          const interest = parseFloat(
            document.getElementById("interestExpense").value
          );
          const resEl = document.getElementById("interestCoverageResult");
          const intEl = document.getElementById(
            "interestCoverageInterpretation"
          );
          if (!ebit || !interest || interest === 0) {
            resEl.textContent = "–";
            intEl.textContent =
              "Enter positive values for EBIT and interest expense.";
            return;
          }
          const ratio = ebit / interest;
          resEl.textContent = formatRatio(ratio, 2) + "×";
          if (ratio < 1.5) {
            intEl.textContent =
              "Interest coverage below ~1.5× is a red flag; the business may struggle to meet debt service in a downturn.";
          } else if (ratio <= 4) {
            intEl.textContent =
              "Coverage between 1.5–4× is typical; lenders often prefer at least 2× for comfort.";
          } else {
            intEl.textContent =
              "Strong coverage: the business generates ample operating profit to service its interest obligations.";
          }
        });

      // Inventory Turnover
      document
        .getElementById("calcInventoryTurnover")
        .addEventListener("click", () => {
          const cogs = parseFloat(document.getElementById("cogs").value);
          const inv = parseFloat(document.getElementById("avgInventory").value);
          const resEl = document.getElementById("inventoryTurnoverResult");
          const intEl = document.getElementById(
            "inventoryTurnoverInterpretation"
          );
          if (!cogs || !inv || inv === 0) {
            resEl.textContent = "–";
            intEl.textContent =
              "Enter positive values for COGS and average inventory.";
            return;
          }
          const ratio = cogs / inv;
          resEl.textContent = formatRatio(ratio, 2) + "× per period";
          intEl.textContent =
            "Higher turnover generally indicates more efficient inventory management, but extremely high turnover may risk stock‑outs.";
        });

      // DSO
      document.getElementById("calcDSO").addEventListener("click", () => {
        const ar = parseFloat(
          document.getElementById("endingReceivables").value
        );
        const sales = parseFloat(
          document.getElementById("annualCreditSales").value
        );
        const days =
          parseFloat(document.getElementById("dsoDays").value) || 365;
        const resEl = document.getElementById("dsoResult");
        const intEl = document.getElementById("dsoInterpretation");
        if ((!ar && ar !== 0) || !sales || sales === 0) {
          resEl.textContent = "–";
          intEl.textContent =
            "Enter accounts receivable and a positive annual credit sales figure.";
          return;
        }
        const dso = (ar / sales) * days;
        resEl.textContent = formatRatio(dso, 1) + " days";
        intEl.textContent =
          "Compare DSO to your payment terms and industry norms; rising DSO can signal collection issues or looser credit policies.";
      });

      // P/E
      document.getElementById("calcPE").addEventListener("click", () => {
        const price = parseFloat(document.getElementById("sharePrice").value);
        const eps = parseFloat(document.getElementById("eps").value);
        const resEl = document.getElementById("peResult");
        const intEl = document.getElementById("peInterpretation");
        if (!price || !eps || eps === 0) {
          resEl.textContent = "–";
          intEl.textContent = "Enter a positive share price and EPS.";
          return;
        }
        const pe = price / eps;
        resEl.textContent = formatRatio(pe, 1) + "×";
        intEl.textContent =
          "P/E ratios vary widely by sector and growth expectations; compare to peers and your own growth outlook.";
      });

      // EV/EBITDA
      document.getElementById("calcEvEbitda").addEventListener("click", () => {
        const mc = parseFloat(document.getElementById("marketCap").value);
        const debt =
          parseFloat(document.getElementById("totalDebtEv").value) || 0;
        const cash = parseFloat(document.getElementById("cashEv").value) || 0;
        const ebitda = parseFloat(document.getElementById("ebitda").value);
        const resEl = document.getElementById("evEbitdaResult");
        const intEl = document.getElementById("evEbitdaInterpretation");
        if (!mc || !ebitda || ebitda === 0) {
          resEl.textContent = "–";
          intEl.textContent =
            "Enter market cap and a positive EBITDA figure; include debt and cash if available.";
          return;
        }
        const ev = mc + debt - cash;
        const multiple = ev / ebitda;
        resEl.textContent = formatRatio(multiple, 1) + "×";
        intEl.textContent =
          "EV/EBITDA is commonly used in valuation and M&A; compare this multiple to similar companies and recent deals in your sector.";
      });

      document
        .querySelectorAll("#faq-container .faq-question")
        .forEach((btn) => {
          btn.addEventListener("click", () => {
            const answer = btn.parentElement.querySelector(".faq-answer");
            const icon = btn.querySelector(".faq-icon");
            const isHidden = answer.classList.contains("hidden");
            document
              .querySelectorAll("#faq-container .faq-answer")
              .forEach((a) => a.classList.add("hidden"));
            if (isHidden) {
              answer.classList.remove("hidden");
            }
            if (icon) {
              icon.classList.toggle("rotate-180", !isHidden);
            }
          });
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
