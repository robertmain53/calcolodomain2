    <script>
      // Utility: format currency with symbol guess
      function guessSymbol(code) {
        switch (code) {
          case "USD":
            return "$";
          case "EUR":
            return "€";
          case "GBP":
            return "£";
          case "AUD":
            return "A$";
          case "CAD":
            return "C$";
          case "JPY":
            return "¥";
          default:
            return code + " ";
        }
      }
      function formatMoney(amount, code) {
        const symbol = guessSymbol(code);
        const abs = Math.abs(amount);
        const formatted = abs.toLocaleString(undefined, {
          maximumFractionDigits: 2,
          minimumFractionDigits: 2,
        });
        return (amount < 0 ? "-" : "") + symbol + formatted;
      }

      // Update FX label when currencies change
      function updateFxLabel() {
        const home = document.getElementById("home-currency").value;
        const dest = document.getElementById("dest-currency").value;
        document.getElementById("fx-label").textContent = "1 " + home + " =";
        document.getElementById("fx-label-right").textContent = dest;
      }

      document
        .getElementById("home-currency")
        .addEventListener("change", updateFxLabel);
      document
        .getElementById("dest-currency")
        .addEventListener("change", updateFxLabel);
      updateFxLabel();

      // Swap currencies
      document
        .getElementById("swap-currencies")
        .addEventListener("click", function () {
          const homeSel = document.getElementById("home-currency");
          const destSel = document.getElementById("dest-currency");
          const tmp = homeSel.value;
          homeSel.value = destSel.value;
          destSel.value = tmp;
          updateFxLabel();
        });

      // Main planner calculation
      document
        .getElementById("calc-international")
        .addEventListener("click", function () {
          const homeSalary = parseFloat(
            document.getElementById("home-salary").value
          );
          const homeTaxRate = parseFloat(
            document.getElementById("home-tax-rate").value
          );
          const homeCol = parseFloat(
            document.getElementById("home-col-index").value
          );

          const destSalary = parseFloat(
            document.getElementById("dest-salary").value
          );
          const destTaxRate = parseFloat(
            document.getElementById("dest-tax-rate").value
          );
          const destCol = parseFloat(
            document.getElementById("dest-col-index").value
          );

          const fxRate = parseFloat(document.getElementById("fx-rate").value);

          const errorEl = document.getElementById("planner-error");
          const resultsEl = document.getElementById("planner-results");
          const summaryEl = document.getElementById("planner-summary");
          const summaryTextEl = document.getElementById("planner-summary-text");

          if (
            !isFinite(homeSalary) ||
            homeSalary <= 0 ||
            !isFinite(homeTaxRate) ||
            homeTaxRate < 0 ||
            homeTaxRate > 80 ||
            !isFinite(homeCol) ||
            homeCol <= 0 ||
            !isFinite(destSalary) ||
            destSalary <= 0 ||
            !isFinite(destTaxRate) ||
            destTaxRate < 0 ||
            destTaxRate > 80 ||
            !isFinite(destCol) ||
            destCol <= 0 ||
            !isFinite(fxRate) ||
            fxRate <= 0
          ) {
            errorEl.classList.remove("hidden");
            resultsEl.classList.add("hidden");
            summaryEl.classList.add("hidden");
            return;
          }
          errorEl.classList.add("hidden");

          const homeCur = document.getElementById("home-currency").value;
          const destCur = document.getElementById("dest-currency").value;

          const homeNet = homeSalary * (1 - homeTaxRate / 100);
          const destNet = destSalary * (1 - destTaxRate / 100);

          const homeNetMonthly = homeNet / 12;
          const destNetMonthly = destNet / 12;

          const homeColAdj = homeNet * (100 / homeCol);
          const destColAdj = destNet * (100 / destCol);

          // Fill home
          document.getElementById("res-home-gross").textContent = formatMoney(
            homeSalary,
            homeCur
          );
          document.getElementById("res-home-tax").textContent = formatMoney(
            homeSalary - homeNet,
            homeCur
          );
          document.getElementById("res-home-net").textContent = formatMoney(
            homeNet,
            homeCur
          );
          document.getElementById("res-home-monthly").textContent = formatMoney(
            homeNetMonthly,
            homeCur
          );
          document.getElementById("res-home-col-adj").textContent =
            formatMoney(homeColAdj, homeCur) + " (COL‑adjusted)";

          // Destination in its own currency
          document.getElementById("res-dest-gross").textContent = formatMoney(
            destSalary,
            destCur
          );
          document.getElementById("res-dest-tax").textContent = formatMoney(
            destSalary - destNet,
            destCur
          );
          document.getElementById("res-dest-net").textContent = formatMoney(
            destNet,
            destCur
          );
          document.getElementById("res-dest-monthly").textContent = formatMoney(
            destNetMonthly,
            destCur
          );
          document.getElementById("res-dest-col-adj").textContent =
            formatMoney(destColAdj, destCur) + " (COL‑adjusted)";

          resultsEl.classList.remove("hidden");

          // Convert destination COL‑adjusted net into home currency for comparison
          const destColAdjHomeCur = destColAdj / fxRate;
          const diff = destColAdjHomeCur - homeColAdj;
          const pct = homeColAdj > 0 ? (diff / homeColAdj) * 100 : 0;

          let summary = "";
          if (Math.abs(diff) < 1e-2) {
            summary =
              "After adjusting for tax and cost of living, your effective purchasing power is roughly the same in both countries.";
          } else if (diff > 0) {
            summary = `After tax and cost of living, the destination offers about ${pct.toFixed(
              1
            )}% higher effective purchasing power than your home country (in ${homeCur} terms).`;
          } else {
            summary = `After tax and cost of living, the destination offers about ${Math.abs(
              pct
            ).toFixed(
              1
            )}% lower effective purchasing power than your home country (in ${homeCur} terms).`;
          }
          summaryTextEl.textContent = summary;
          summaryEl.classList.remove("hidden");
        });

      document
        .getElementById("reset-international")
        .addEventListener("click", function () {
          document.getElementById("home-currency").value = "USD";
          document.getElementById("dest-currency").value = "EUR";
          document.getElementById("home-salary").value = 60000;
          document.getElementById("home-tax-rate").value = 30;
          document.getElementById("home-col-index").value = 100;
          document.getElementById("dest-salary").value = 70000;
          document.getElementById("dest-tax-rate").value = 35;
          document.getElementById("dest-col-index").value = 120;
          document.getElementById("fx-rate").value = 0.92;
          updateFxLabel();
          document.getElementById("planner-results").classList.add("hidden");
          document.getElementById("planner-summary").classList.add("hidden");
          document.getElementById("planner-error").classList.add("hidden");
        });

      // FX cost calculator
      document.getElementById("fx-calc").addEventListener("click", function () {
        const amount = parseFloat(document.getElementById("fx-amount").value);
        const midRate = parseFloat(
          document.getElementById("fx-mid-rate").value
        );
        const providerRate = parseFloat(
          document.getElementById("fx-provider-rate").value
        );
        const flatFee = parseFloat(
          document.getElementById("fx-flat-fee").value
        );

        const errorEl = document.getElementById("fx-error");
        const resultsEl = document.getElementById("fx-results");

        if (
          !isFinite(amount) ||
          amount <= 0 ||
          !isFinite(midRate) ||
          midRate <= 0 ||
          !isFinite(providerRate) ||
          providerRate <= 0 ||
          !isFinite(flatFee) ||
          flatFee < 0
        ) {
          errorEl.classList.remove("hidden");
          resultsEl.classList.add("hidden");
          return;
        }
        errorEl.classList.add("hidden");

        const midAmount = amount * midRate;
        const providerAmount = amount * providerRate;
        const hiddenCost = Math.max(0, midAmount - providerAmount);
        const totalCost = hiddenCost + flatFee;
        const effectivePct = (totalCost / amount) * 100;

        document.getElementById("fx-mid-amount").textContent =
          midAmount.toLocaleString(undefined, {
            maximumFractionDigits: 2,
            minimumFractionDigits: 2,
          });
        document.getElementById("fx-provider-amount").textContent =
          providerAmount.toLocaleString(undefined, {
            maximumFractionDigits: 2,
            minimumFractionDigits: 2,
          });
        document.getElementById("fx-hidden-cost").textContent =
          hiddenCost.toLocaleString(undefined, {
            maximumFractionDigits: 2,
            minimumFractionDigits: 2,
          });
        document.getElementById("fx-flat-fee-out").textContent =
          flatFee.toLocaleString(undefined, {
            maximumFractionDigits: 2,
            minimumFractionDigits: 2,
          });
        document.getElementById("fx-total-cost").textContent =
          totalCost.toLocaleString(undefined, {
            maximumFractionDigits: 2,
            minimumFractionDigits: 2,
          });
        document.getElementById("fx-effective-pct").textContent =
          effectivePct.toFixed(2) + "% of amount sent";

        resultsEl.classList.remove("hidden");
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
