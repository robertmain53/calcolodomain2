    <script>
      // Live current ratio calculator logic
      (function () {
        const assetsInput = document.getElementById("currentAssets");
        const liabInput = document.getElementById("currentLiabilities");
        const currencyInput = document.getElementById("currency");
        const prefixAssets = document.getElementById("currency-prefix-assets");
        const prefixLiab = document.getElementById("currency-prefix-liab");
        const ratioValue = document.getElementById("ratioValue");
        const ratioBadge = document.getElementById("ratioBadge");
        const ratioSummary = document.getElementById("ratioSummary");

        const btnStrong = document.getElementById("btn-example-strong");
        const btnTight = document.getElementById("btn-example-tight");
        const btnWeak = document.getElementById("btn-example-weak");

        function updateCurrencyPrefix() {
          const cur = currencyInput.value.trim();
          const symbol = cur || "$";
          prefixAssets.textContent = symbol;
          prefixLiab.textContent = symbol;
        }

        function classifyRatio(r) {
          if (!isFinite(r) || r < 0)
            return {
              label: "Invalid",
              color: "bg-gray-100 text-gray-700",
              text: "Check your inputs. Current assets and liabilities should be non‑negative, and liabilities should not be zero.",
            };
          if (r === 0)
            return {
              label: "No coverage",
              color: "bg-red-100 text-red-700",
              text: "Current assets are zero. The business has no short‑term assets to cover its current obligations.",
            };
          if (r < 1)
            return {
              label: "At risk",
              color: "bg-red-100 text-red-700",
              text: "Current liabilities exceed current assets. This can indicate liquidity stress and a higher risk of missed payments.",
            };
          if (r < 1.5)
            return {
              label: "Tight",
              color: "bg-amber-100 text-amber-700",
              text: "Short‑term obligations are covered, but the margin of safety is thin. Monitor cash flow closely.",
            };
          if (r < 3)
            return {
              label: "Healthy",
              color: "bg-emerald-100 text-emerald-700",
              text: "The current ratio suggests a comfortable liquidity position for many industries. Compare to peers and your own history.",
            };
          return {
            label: "Very conservative",
            color: "bg-sky-100 text-sky-700",
            text: "Liquidity is strong, but a very high ratio can indicate under‑utilized cash, receivables, or inventory.",
          };
        }

        function formatRatio(r) {
          if (!isFinite(r)) return "–";
          if (r === 0) return "0.00";
          if (r >= 1000) return r.toFixed(0);
          if (r >= 100) return r.toFixed(1);
          return r.toFixed(2);
        }

        function recalc() {
          const assets = parseFloat(assetsInput.value);
          const liab = parseFloat(liabInput.value);

          if (isNaN(assets) || isNaN(liab)) {
            ratioValue.textContent = "–";
            ratioBadge.textContent = "Enter values";
            ratioBadge.className =
              "inline-flex items-center px-3 py-1 rounded-full text-xs font-medium bg-gray-100 text-gray-700";
            ratioSummary.textContent =
              "Enter current assets and liabilities to see your ratio and liquidity assessment.";
            return;
          }

          if (liab === 0) {
            ratioValue.textContent = "∞";
            ratioBadge.textContent = "No short‑term debt";
            ratioBadge.className =
              "inline-flex items-center px-3 py-1 rounded-full text-xs font-medium bg-sky-100 text-sky-700";
            ratioSummary.textContent =
              "Current liabilities are zero, so the current ratio is mathematically infinite. The business has no short‑term obligations on the balance sheet.";
            return;
          }

          const r = assets / liab;
          const cls = classifyRatio(r);
          ratioValue.textContent = formatRatio(r);
          ratioBadge.textContent = cls.label;
          ratioBadge.className =
            "inline-flex items-center px-3 py-1 rounded-full text-xs font-medium " +
            cls.color;
          ratioSummary.textContent = cls.text;
        }

        assetsInput.addEventListener("input", recalc);
        liabInput.addEventListener("input", recalc);
        currencyInput.addEventListener("input", updateCurrencyPrefix);

        btnStrong.addEventListener("click", function () {
          assetsInput.value = 600000;
          liabInput.value = 300000;
          recalc();
        });
        btnTight.addEventListener("click", function () {
          assetsInput.value = 220000;
          liabInput.value = 200000;
          recalc();
        });
        btnWeak.addEventListener("click", function () {
          assetsInput.value = 150000;
          liabInput.value = 250000;
          recalc();
        });

        updateCurrencyPrefix();
      })();

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
