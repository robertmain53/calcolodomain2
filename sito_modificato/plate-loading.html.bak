    <script>
      // Utility functions
      function convertLengthToMeters(value, unit) {
        if (isNaN(value)) return NaN;
        switch (unit) {
          case "mm":
            return value / 1000;
          case "cm":
            return value / 100;
          case "m":
          default:
            return value;
        }
      }

      function convertSettlementToMm(value, unit) {
        if (isNaN(value)) return NaN;
        switch (unit) {
          case "cm":
            return value * 10;
          case "mm":
          default:
            return value;
        }
      }

      function loadUnitIsPressure() {
        const loadUnit = document.getElementById("loadUnit").value;
        return loadUnit === "kN/m2";
      }

      function loadToKPa(load, area, unit) {
        if (isNaN(load) || load <= 0) return NaN;
        if (unit === "kN/m2") {
          return load; // already pressure
        }
        // convert load to kN
        let PkN = load;
        if (unit === "ton") {
          // metric ton to kN (approx 9.80665 kN/ton)
          PkN = load * 9.80665;
        }
        if (!area || area <= 0) return NaN;
        return PkN / area; // kN/m2 = kPa
      }

      function interpolate(x, x1, y1, x2, y2) {
        if (x2 === x1) return y1;
        return y1 + ((y2 - y1) * (x - x1)) / (x2 - x1);
      }

      // Dynamic table handling
      const loadStepsBody = document.getElementById("loadStepsBody");
      const addRowBtn = document.getElementById("addRowBtn");
      const exampleDataBtn = document.getElementById("exampleDataBtn");
      const clearDataBtn = document.getElementById("clearDataBtn");
      const loadUnitSelect = document.getElementById("loadUnit");
      const loadHeader = document.getElementById("loadHeader");

      function updateLoadHeader() {
        const unit = document.getElementById("loadUnit").value;
        if (unit === "kN/m2") {
          loadHeader.textContent = "Pressure [kPa]";
        } else if (unit === "ton") {
          loadHeader.textContent = "Load [ton]";
        } else {
          loadHeader.textContent = "Load [kN]";
        }
      }

      loadUnitSelect.addEventListener("change", updateLoadHeader);

      function addRow(loadVal = "", settVal = "") {
        const rowIndex = loadStepsBody.children.length + 1;
        const tr = document.createElement("tr");
        tr.innerHTML = `
      <td class="text-center">${rowIndex}</td>
      <td><input type="number" step="0.01" class="w-full border border-gray-300 rounded-md px-2 py-1" value="${loadVal}"></td>
      <td><input type="number" step="0.01" class="w-full border border-gray-300 rounded-md px-2 py-1" value="${settVal}"></td>
      <td class="text-center">
        <button type="button" class="text-red-600 text-xs remove-row">Remove</button>
      </td>
    `;
        loadStepsBody.appendChild(tr);
      }

      function renumberRows() {
        Array.from(loadStepsBody.children).forEach((tr, idx) => {
          tr.children[0].textContent = idx + 1;
        });
      }

      loadStepsBody.addEventListener("click", (e) => {
        if (e.target.classList.contains("remove-row")) {
          e.target.closest("tr").remove();
          renumberRows();
        }
      });

      addRowBtn.addEventListener("click", () => addRow());

      clearDataBtn.addEventListener("click", () => {
        loadStepsBody.innerHTML = "";
      });

      exampleDataBtn.addEventListener("click", () => {
        // Example: circular plate D=0.3 m, load in kN, settlement in mm
        document.getElementById("plateShape").value = "circular";
        document.getElementById("plateSize").value = 0.3;
        document.getElementById("plateSizeUnit").value = "m";
        document.getElementById("loadUnit").value = "kN";
        document.getElementById("settlementUnit").value = "mm";
        updateLoadHeader();

        loadStepsBody.innerHTML = "";
        const example = [
          [10, 0.5],
          [20, 1.1],
          [30, 1.8],
          [40, 2.7],
          [50, 3.9],
          [60, 5.5],
        ];
        example.forEach(([P, s]) => addRow(P, s));

        document.getElementById("s1").value = 1;
        document.getElementById("s2").value = 3;
        document.getElementById("allowableSettlement").value = 25;
      });

      // Toggle k-method options
      const kMethodSelect = document.getElementById("kMethod");
      const secantRangeContainer = document.getElementById(
        "secantRangeContainer"
      );
      const tangentPointContainer = document.getElementById(
        "tangentPointContainer"
      );

      kMethodSelect.addEventListener("change", () => {
        if (kMethodSelect.value === "secant") {
          secantRangeContainer.classList.remove("hidden");
          tangentPointContainer.classList.add("hidden");
        } else {
          secantRangeContainer.classList.add("hidden");
          tangentPointContainer.classList.remove("hidden");
        }
      });

      // Calculation
      const calculateBtn = document.getElementById("calculateBtn");
      const errorMsg = document.getElementById("errorMsg");

      const resArea = document.getElementById("resArea");
      const resQmax = document.getElementById("resQmax");
      const resSmax = document.getElementById("resSmax");
      const resK = document.getElementById("resK");
      const resSoilClass = document.getElementById("resSoilClass");
      const resQallow = document.getElementById("resQallow");
      const resTableBody = document.getElementById("resTableBody");
      const kBadge = document.getElementById("kBadge");

      function classifySoil(kMN) {
        if (!isFinite(kMN) || kMN <= 0) return "Unknown";
        if (kMN < 5) return "Very soft subgrade";
        if (kMN < 20) return "Soft to medium subgrade";
        if (kMN < 80) return "Stiff subgrade";
        return "Very stiff / rock-like subgrade";
      }

      function updateKBadge(kMN) {
        kBadge.className = "badge";
        if (!isFinite(kMN) || kMN <= 0) {
          kBadge.textContent = "n/a";
          kBadge.classList.add("bg-gray-100", "text-gray-700");
          return;
        }
        kBadge.textContent = kMN.toFixed(1) + " MN/m³";
        if (kMN < 5) {
          kBadge.classList.add("bg-red-100", "text-red-700");
        } else if (kMN < 20) {
          kBadge.classList.add("bg-yellow-100", "text-yellow-700");
        } else if (kMN < 80) {
          kBadge.classList.add("bg-green-100", "text-green-700");
        } else {
          kBadge.classList.add("bg-blue-100", "text-blue-700");
        }
      }

      calculateBtn.addEventListener("click", () => {
        errorMsg.classList.add("hidden");
        errorMsg.textContent = "";

        const shape = document.getElementById("plateShape").value;
        const sizeVal = parseFloat(document.getElementById("plateSize").value);
        const sizeUnit = document.getElementById("plateSizeUnit").value;
        const loadUnit = document.getElementById("loadUnit").value;
        const settlementUnit = document.getElementById("settlementUnit").value;

        const D_or_B_m = convertLengthToMeters(sizeVal, sizeUnit);
        if (!isFinite(D_or_B_m) || D_or_B_m <= 0) {
          errorMsg.textContent = "Please enter a valid plate size.";
          errorMsg.classList.remove("hidden");
          return;
        }

        let area;
        if (shape === "circular") {
          area = (Math.PI * Math.pow(D_or_B_m, 2)) / 4;
        } else {
          area = Math.pow(D_or_B_m, 2);
        }

        const rows = Array.from(loadStepsBody.querySelectorAll("tr"));
        if (rows.length < 2) {
          errorMsg.textContent = "Enter at least two load–settlement points.";
          errorMsg.classList.remove("hidden");
          return;
        }

        const data = [];
        for (const tr of rows) {
          const inputs = tr.querySelectorAll("input");
          const load = parseFloat(inputs[0].value);
          const sRaw = parseFloat(inputs[1].value);
          if (!isFinite(load) || !isFinite(sRaw) || load <= 0 || sRaw < 0) {
            errorMsg.textContent =
              "All load and settlement values must be valid and positive (settlement ≥ 0).";
            errorMsg.classList.remove("hidden");
            return;
          }
          const s_mm = convertSettlementToMm(sRaw, settlementUnit);
          const q_kPa = loadToKPa(load, area, loadUnit);
          if (!isFinite(q_kPa) || q_kPa <= 0) {
            errorMsg.textContent =
              "Could not compute pressure from load. Check plate size and load units.";
            errorMsg.classList.remove("hidden");
            return;
          }
          data.push({ load, s_mm, q_kPa });
        }

        // Sort by settlement
        data.sort((a, b) => a.s_mm - b.s_mm);

        const maxQ = data[data.length - 1].q_kPa;
        const maxS = data[data.length - 1].s_mm;

        resArea.textContent = area.toFixed(4);
        resQmax.textContent = maxQ.toFixed(1);
        resSmax.textContent = maxS.toFixed(2);

        // Compute k-value
        let k_kPa_per_mm = NaN;
        const kMethod = document.getElementById("kMethod").value;

        if (kMethod === "secant") {
          const s1 = parseFloat(document.getElementById("s1").value);
          const s2 = parseFloat(document.getElementById("s2").value);
          if (!isFinite(s1) || !isFinite(s2) || s2 <= s1) {
            errorMsg.textContent =
              "For the secant method, enter valid settlements s₁ and s₂ with s₂ > s₁.";
            errorMsg.classList.remove("hidden");
            return;
          }

          function qAtSettlement(targetS) {
            if (targetS <= data[0].s_mm) return data[0].q_kPa;
            if (targetS >= data[data.length - 1].s_mm)
              return data[data.length - 1].q_kPa;
            for (let i = 0; i < data.length - 1; i++) {
              const p1 = data[i];
              const p2 = data[i + 1];
              if (targetS >= p1.s_mm && targetS <= p2.s_mm) {
                return interpolate(
                  targetS,
                  p1.s_mm,
                  p1.q_kPa,
                  p2.s_mm,
                  p2.q_kPa
                );
              }
            }
            return data[data.length - 1].q_kPa;
          }

          const q1 = qAtSettlement(s1);
          const q2 = qAtSettlement(s2);
          k_kPa_per_mm = (q2 - q1) / (s2 - s1);
        } else {
          const sT = parseFloat(document.getElementById("sTangent").value);
          if (!isFinite(sT) || sT <= 0) {
            errorMsg.textContent =
              "For the tangent method, enter a valid settlement point.";
            errorMsg.classList.remove("hidden");
            return;
          }
          // Find nearest two points around sT
          let lower = null,
            upper = null;
          for (let i = 0; i < data.length; i++) {
            if (data[i].s_mm <= sT) lower = data[i];
            if (data[i].s_mm >= sT) {
              upper = data[i];
              break;
            }
          }
          if (!lower || !upper || lower === upper) {
            errorMsg.textContent =
              "Not enough data around the selected tangent point to estimate stiffness.";
            errorMsg.classList.remove("hidden");
            return;
          }
          k_kPa_per_mm =
            (upper.q_kPa - lower.q_kPa) / (upper.s_mm - lower.s_mm);
        }

        if (!isFinite(k_kPa_per_mm) || k_kPa_per_mm <= 0) {
          resK.textContent = "–";
          resSoilClass.textContent = "Unknown";
          updateKBadge(NaN);
        } else {
          // Convert kPa/mm to kN/m³: 1 kPa = 1 kN/m², 1 mm = 0.001 m → kN/m² / 0.001 m = 1000 kN/m³
          const k_kN_per_m3 = k_kPa_per_mm * 1000;
          const k_MN_per_m3 = k_kN_per_m3 / 1000;
          resK.textContent = k_kN_per_m3.toFixed(0);
          resSoilClass.textContent = classifySoil(k_MN_per_m3);
          updateKBadge(k_MN_per_m3);
        }

        // Allowable bearing pressure
        const sAllowRaw = parseFloat(
          document.getElementById("allowableSettlement").value
        );
        if (isFinite(sAllowRaw) && sAllowRaw > 0) {
          const sAllow_mm = convertSettlementToMm(sAllowRaw, settlementUnit);
          let qAllow = NaN;
          if (sAllow_mm <= data[0].s_mm) {
            qAllow = data[0].q_kPa;
          } else if (sAllow_mm >= data[data.length - 1].s_mm) {
            qAllow = data[data.length - 1].q_kPa;
          } else {
            for (let i = 0; i < data.length - 1; i++) {
              const p1 = data[i];
              const p2 = data[i + 1];
              if (sAllow_mm >= p1.s_mm && sAllow_mm <= p2.s_mm) {
                qAllow = interpolate(
                  sAllow_mm,
                  p1.s_mm,
                  p1.q_kPa,
                  p2.s_mm,
                  p2.q_kPa
                );
                break;
              }
            }
          }
          if (isFinite(qAllow)) {
            resQallow.textContent = qAllow.toFixed(1);
          } else {
            resQallow.textContent = "–";
          }
        } else {
          resQallow.textContent = "–";
        }

        // Fill results table
        resTableBody.innerHTML = "";
        data.forEach((p, idx) => {
          const tr = document.createElement("tr");
          const qOverS = p.s_mm > 0 ? p.q_kPa / p.s_mm : NaN;
          tr.innerHTML = `
        <td class="text-center">${idx + 1}</td>
        <td class="text-right">${p.load.toFixed(2)}</td>
        <td class="text-right">${p.q_kPa.toFixed(1)}</td>
        <td class="text-right">${p.s_mm.toFixed(2)}</td>
        <td class="text-right">${
          isFinite(qOverS) ? qOverS.toFixed(2) : "–"
        }</td>
      `;
          resTableBody.appendChild(tr);
        });
      });

      // Initialize with a couple of empty rows
      addRow();
      addRow();
      updateLoadHeader();

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
