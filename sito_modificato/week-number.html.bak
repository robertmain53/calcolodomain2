    <script>
      // Utility: clone date without mutation
      function cloneDate(d) {
        return new Date(d.getTime());
      }

      // ISO week number (ISO-8601)
      function getISOWeek(date) {
        const d = new Date(
          Date.UTC(date.getFullYear(), date.getMonth(), date.getDate())
        );
        // Thursday in current week decides the year
        d.setUTCDate(d.getUTCDate() + 4 - (d.getUTCDay() || 7));
        const yearStart = new Date(Date.UTC(d.getUTCFullYear(), 0, 1));
        const weekNo = Math.ceil(((d - yearStart) / 86400000 + 1) / 7);
        return { week: weekNo, year: d.getUTCFullYear() };
      }

      // US week number: week starts Sunday, week 1 contains Jan 1
      function getUSWeek(date) {
        const d = cloneDate(date);
        const year = d.getFullYear();
        const jan1 = new Date(year, 0, 1);
        const dayMs = 24 * 60 * 60 * 1000;
        const diffDays = Math.floor((d - jan1) / dayMs);
        const week = Math.floor((diffDays + jan1.getDay()) / 7) + 1;
        return { week: week, year: year };
      }

      // Custom week number
      // weekStart: 0=Sun..6=Sat
      // rule:
      //  - "contains-jan1": week 1 is week containing Jan 1
      //  - "first-full-week": first week fully in year
      //  - "iso-like": week with >=4 days in year
      function getCustomWeek(date, weekStart, rule) {
        const d = cloneDate(date);
        const year = d.getFullYear();
        const jan1 = new Date(year, 0, 1);
        const dayMs = 24 * 60 * 60 * 1000;

        function startOfWeek(dt) {
          const res = cloneDate(dt);
          const diff = (res.getDay() - weekStart + 7) % 7;
          res.setDate(res.getDate() - diff);
          return res;
        }

        const weekStartDate = startOfWeek(d);

        let week1Start;
        if (rule === "contains-jan1") {
          week1Start = startOfWeek(jan1);
        } else if (rule === "first-full-week") {
          // first week whose start is in this year
          week1Start = startOfWeek(jan1);
          while (week1Start.getFullYear() < year) {
            week1Start = new Date(week1Start.getTime() + 7 * dayMs);
          }
        } else if (rule === "iso-like") {
          // week with at least 4 days in this year
          week1Start = startOfWeek(jan1);
          while (true) {
            let daysInYear = 0;
            for (let i = 0; i < 7; i++) {
              const tmp = new Date(week1Start.getTime() + i * dayMs);
              if (tmp.getFullYear() === year) daysInYear++;
            }
            if (daysInYear >= 4) break;
            week1Start = new Date(week1Start.getTime() + 7 * dayMs);
          }
        } else {
          week1Start = startOfWeek(jan1);
        }

        const diffWeeks = Math.floor(
          (weekStartDate - week1Start) / (7 * dayMs)
        );
        const week = diffWeeks + 1;
        return { week: week, year: year };
      }

      // Generate year overview table for current tab
      function buildYearOverview(year, mode, customOptions) {
        const tbody = document.getElementById("year-weeks-body");
        tbody.innerHTML = "";
        if (!year || isNaN(year)) return;

        const dayMs = 24 * 60 * 60 * 1000;
        const jan1 = new Date(year, 0, 1);
        const dec31 = new Date(year, 11, 31);

        let weekStartFunc;
        if (mode === "iso") {
          weekStartFunc = function (dt) {
            const d = new Date(dt.getFullYear(), dt.getMonth(), dt.getDate());
            const day = d.getDay() || 7; // 1..7, Monday=1
            d.setDate(d.getDate() - (day - 1)); // back to Monday
            return d;
          };
        } else if (mode === "us") {
          weekStartFunc = function (dt) {
            const d = new Date(dt.getFullYear(), dt.getMonth(), dt.getDate());
            const day = d.getDay(); // 0..6, Sunday=0
            d.setDate(d.getDate() - day); // back to Sunday
            return d;
          };
        } else {
          const weekStart = customOptions.weekStart;
          weekStartFunc = function (dt) {
            const d = new Date(dt.getFullYear(), dt.getMonth(), dt.getDate());
            const diff = (d.getDay() - weekStart + 7) % 7;
            d.setDate(d.getDate() - diff);
            return d;
          };
        }

        // Find first week start that intersects the year
        let current = weekStartFunc(jan1);
        while (current.getFullYear() > year || current > dec31) {
          current = new Date(current.getTime() - 7 * dayMs);
        }
        while (
          current.getFullYear() < year &&
          current.getTime() + 6 * dayMs < jan1.getTime()
        ) {
          current = new Date(current.getTime() + 7 * dayMs);
        }

        const seenWeeks = new Set();
        for (let i = 0; i < 60; i++) {
          const weekStart = new Date(current.getTime());
          const weekEnd = new Date(current.getTime() + 6 * dayMs);

          if (weekEnd.getFullYear() < year && weekStart.getFullYear() < year) {
            current = new Date(current.getTime() + 7 * dayMs);
            continue;
          }
          if (weekStart.getFullYear() > year && weekEnd.getFullYear() > year)
            break;

          let wk;
          if (mode === "iso") {
            wk = getISOWeek(weekStart).week;
          } else if (mode === "us") {
            wk = getUSWeek(weekStart).week;
          } else {
            wk = getCustomWeek(
              weekStart,
              customOptions.weekStart,
              customOptions.rule
            ).week;
          }

          const key = wk + "-" + year;
          if (seenWeeks.has(key)) {
            current = new Date(current.getTime() + 7 * dayMs);
            continue;
          }
          seenWeeks.add(key);

          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="px-2 py-1 border-t border-gray-200 font-semibold">W${wk}</td>
        <td class="px-2 py-1 border-t border-gray-200">${weekStart
          .toISOString()
          .slice(0, 10)}</td>
        <td class="px-2 py-1 border-t border-gray-200">${weekEnd
          .toISOString()
          .slice(0, 10)}</td>
      `;
          tbody.appendChild(tr);

          current = new Date(current.getTime() + 7 * dayMs);
          if (current.getFullYear() > year && weekStart.getFullYear() > year)
            break;
        }
      }

      document.addEventListener("DOMContentLoaded", function () {
        const dateInput = document.getElementById("date-input");
        const yearInput = document.getElementById("year-input");
        const yearTodayBtn = document.getElementById("year-today-btn");

        const tabISO = document.getElementById("tab-iso");
        const tabUS = document.getElementById("tab-us");
        const tabCustom = document.getElementById("tab-custom");
        const customOptionsDiv = document.getElementById("custom-options");
        const customWeekStart = document.getElementById("custom-week-start");
        const customWeek1Rule = document.getElementById("custom-week1-rule");

        const weekNumberMain = document.getElementById("week-number-main");
        const weekSystemLabel = document.getElementById("week-system-label");
        const selectedDateLabel = document.getElementById(
          "selected-date-label"
        );

        const todayLabel = document.getElementById("today-label");
        const todayIsoWeek = document.getElementById("today-iso-week");
        const todayUsWeek = document.getElementById("today-us-week");
        const todayCustomWeek = document.getElementById("today-custom-week");

        let currentMode = "iso"; // 'iso' | 'us' | 'custom'

        const today = new Date();
        const todayStr = today.toISOString().slice(0, 10);
        dateInput.value = todayStr;
        yearInput.value = today.getFullYear();

        function updateTodayPanel() {
          const iso = getISOWeek(today);
          const us = getUSWeek(today);
          const custom = getCustomWeek(
            today,
            parseInt(customWeekStart.value, 10),
            customWeek1Rule.value
          );

          todayLabel.textContent = today.toDateString();
          todayIsoWeek.textContent = `W${iso.week} (${iso.year})`;
          todayUsWeek.textContent = `W${us.week} (${us.year})`;
          todayCustomWeek.textContent = `W${custom.week} (${custom.year})`;
        }

        function updateMainResult() {
          const val = dateInput.value;
          if (!val) {
            weekNumberMain.textContent = "—";
            selectedDateLabel.textContent = "No date selected";
            return;
          }
          const d = new Date(val + "T12:00:00"); // avoid timezone edge
          selectedDateLabel.textContent = d.toDateString();

          if (currentMode === "iso") {
            const iso = getISOWeek(d);
            weekNumberMain.textContent = `W${iso.week} (${iso.year})`;
            weekSystemLabel.textContent =
              "ISO week number (weeks start on Monday, week 1 has January 4)";
          } else if (currentMode === "us") {
            const us = getUSWeek(d);
            weekNumberMain.textContent = `W${us.week} (${us.year})`;
            weekSystemLabel.textContent =
              "US week number (weeks start on Sunday, week 1 contains January 1)";
          } else {
            const custom = getCustomWeek(
              d,
              parseInt(customWeekStart.value, 10),
              customWeek1Rule.value
            );
            weekNumberMain.textContent = `W${custom.week} (${custom.year})`;
            weekSystemLabel.textContent =
              "Custom week number (based on your week start and week 1 rule)";
          }
        }

        function setActiveTab(mode) {
          currentMode = mode;
          [tabISO, tabUS, tabCustom].forEach((btn) =>
            btn.classList.remove("tab-active")
          );
          if (mode === "iso") tabISO.classList.add("tab-active");
          if (mode === "us") tabUS.classList.add("tab-active");
          if (mode === "custom") tabCustom.classList.add("tab-active");

          customOptionsDiv.classList.toggle("hidden", mode !== "custom");

          const year = parseInt(yearInput.value, 10);
          const customOpts = {
            weekStart: parseInt(customWeekStart.value, 10),
            rule: customWeek1Rule.value,
          };
          buildYearOverview(year, currentMode, customOpts);
          updateMainResult();
        }

        // Event listeners
        dateInput.addEventListener("change", function () {
          updateMainResult();
        });

        yearInput.addEventListener("change", function () {
          const year = parseInt(yearInput.value, 10);
          if (!year || year < 1900 || year > 2100) return;
          const customOpts = {
            weekStart: parseInt(customWeekStart.value, 10),
            rule: customWeek1Rule.value,
          };
          buildYearOverview(year, currentMode, customOpts);
        });

        yearTodayBtn.addEventListener("click", function () {
          yearInput.value = today.getFullYear();
          const customOpts = {
            weekStart: parseInt(customWeekStart.value, 10),
            rule: customWeek1Rule.value,
          };
          buildYearOverview(today.getFullYear(), currentMode, customOpts);
        });

        tabISO.addEventListener("click", function () {
          setActiveTab("iso");
        });
        tabUS.addEventListener("click", function () {
          setActiveTab("us");
        });
        tabCustom.addEventListener("click", function () {
          setActiveTab("custom");
        });

        customWeekStart.addEventListener("change", function () {
          if (currentMode === "custom") {
            updateMainResult();
            const year = parseInt(yearInput.value, 10);
            const customOpts = {
              weekStart: parseInt(customWeekStart.value, 10),
              rule: customWeek1Rule.value,
            };
            buildYearOverview(year, currentMode, customOpts);
          }
          updateTodayPanel();
        });

        customWeek1Rule.addEventListener("change", function () {
          if (currentMode === "custom") {
            updateMainResult();
            const year = parseInt(yearInput.value, 10);
            const customOpts = {
              weekStart: parseInt(customWeekStart.value, 10),
              rule: customWeek1Rule.value,
            };
            buildYearOverview(year, currentMode, customOpts);
          }
          updateTodayPanel();
        });

        // Initial render
        updateTodayPanel();
        setActiveTab("iso");
      });
    </script>

