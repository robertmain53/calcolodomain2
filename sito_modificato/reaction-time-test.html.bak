          FALSE_START: "false_start",
        };

        let currentState = STATE.IDLE;
        let timeoutId = null;
        let stimulusTime = 0;
        let trials = [];
        let bestAllTime = null;
        let trialsPerRun = parseInt(trialSelect.value, 10);
        let currentTrial = 0;
        let currentMode = "simple";
        let goNoGoIsGo = true;
        let sequenceStep = 0;
        let sequenceTimes = [];

        function setPanelClass(base) {
          panel.classList.remove(
            "reaction-panel-wait",
            "reaction-panel-ready",
            "reaction-panel-go",
            "reaction-panel-false"
          );
          panel.classList.add(base);
        }

        function resetRun() {
          clearTimeout(timeoutId);
          timeoutId = null;
          currentState = STATE.IDLE;
          trials = [];
          currentTrial = 0;
          sequenceStep = 0;
          sequenceTimes = [];
          updateStats();
          setPanelClass("reaction-panel-wait");
          msg.textContent = "Click or tap to start";
          submsg.textContent =
            "When the panel turns green, react as fast as you can.";
        }

        function resetAllStats() {
          resetRun();
          bestAllTime = null;
          updateStats();
          performanceText.textContent =
            "Complete at least 5 trials to see how your reaction time compares to typical human ranges.";
        }

        function scheduleStimulus() {
          const delay = 1000 + Math.random() * 2000; // 1–3 seconds
          timeoutId = setTimeout(() => {
            if (currentMode === "go-nogo") {
              goNoGoIsGo = Math.random() < 0.7; // 70% GO, 30% NO-GO
              if (goNoGoIsGo) {
                setPanelClass("reaction-panel-go");
                msg.textContent = "GO!";
                submsg.textContent = "Click or press space as fast as you can.";
                stimulusTime = performance.now();
                currentState = STATE.READY;
              } else {
                setPanelClass("reaction-panel-ready");
                msg.textContent = "NO!";
                submsg.textContent = "Do NOT click. Wait for the next trial.";
                currentState = STATE.READY; // but we treat clicks as errors
                stimulusTime = performance.now();
              }
            } else if (currentMode === "sequence") {
              sequenceStep = 1;
              sequenceTimes = [];
              showSequenceStep();
            } else {
              setPanelClass("reaction-panel-go");
              msg.textContent = "GO!";
              submsg.textContent = "Click or press space as fast as you can.";
              stimulusTime = performance.now();
              currentState = STATE.READY;
            }
          }, delay);
        }

        function showSequenceStep() {
          setPanelClass("reaction-panel-go");
          msg.textContent = "GO " + sequenceStep;
          submsg.textContent = "React to each GO as fast as you can.";
          stimulusTime = performance.now();
          currentState = STATE.READY;
        }

        function completeSequenceTrial() {
          const avgSeq =
            sequenceTimes.reduce((a, b) => a + b, 0) / sequenceTimes.length;
          recordTrial(avgSeq);
          sequenceStep = 0;
          sequenceTimes = [];
        }

        function recordTrial(rt) {
          trials.push(rt);
          currentTrial = trials.length;
          lastTimeEl.textContent = Math.round(rt);
          const avg = trials.reduce((a, b) => a + b, 0) / trials.length;
          avgTimeEl.textContent = Math.round(avg);
          if (bestAllTime === null || rt < bestAllTime) {
            bestAllTime = rt;
          }
          bestTimeEl.textContent =
            bestAllTime !== null ? Math.round(bestAllTime) : "–";
          trialCounterEl.textContent = currentTrial + " / " + trialsPerRun;
          updatePerformanceText(avg);
          if (currentTrial >= trialsPerRun) {
            currentState = STATE.IDLE;
            setPanelClass("reaction-panel-wait");
            msg.textContent = "Run complete";
            submsg.textContent =
              "Click or tap to start a new run, or change mode.";
          } else {
            currentState = STATE.WAITING;
            setPanelClass("reaction-panel-wait");
            msg.textContent = "Get ready...";
            submsg.textContent = "Wait for the next GO signal.";
            scheduleStimulus();
          }
        }

        function updateStats() {
          if (trials.length === 0) {
            lastTimeEl.textContent = "–";
            avgTimeEl.textContent = "–";
            trialCounterEl.textContent = "0 / " + trialsPerRun;
          }
          bestTimeEl.textContent =
            bestAllTime !== null ? Math.round(bestAllTime) : "–";
        }

        function updatePerformanceText(avg) {
          if (!avg || trials.length < 5) return;
          let text =
            "Your current average reaction time is about " +
            Math.round(avg) +
            " ms. ";
          if (avg < 150) {
            text +=
              "This is extremely fast and may be influenced by device or measurement artifacts.";
          } else if (avg < 200) {
            text += "This is an elite reaction time, faster than most people.";
          } else if (avg < 250) {
            text += "This is typical for a healthy young adult.";
          } else if (avg < 300) {
            text +=
              "This is slightly slower than average, but still within a normal range.";
          } else {
            text +=
              "This is slower than average. Fatigue, distractions, age, or device lag may be contributing.";
          }
          performanceText.textContent = text;
        }

        function handleUserAction() {
          if (
            currentMode === "go-nogo" &&
            currentState === STATE.READY &&
            !goNoGoIsGo
          ) {
            // False alarm in no-go trial
            setPanelClass("reaction-panel-false");
            msg.textContent = "False alarm!";
            submsg.textContent =
              "You reacted to a NO signal. This trial will be repeated.";
            currentState = STATE.FALSE_START;
            clearTimeout(timeoutId);
            timeoutId = null;
            setTimeout(() => {
              if (currentState === STATE.FALSE_START) {
                currentState = STATE.WAITING;
                setPanelClass("reaction-panel-wait");
                msg.textContent = "Get ready...";
                submsg.textContent = "Wait for the GO signal.";
                scheduleStimulus();
              }
            }, 1000);
            return;
          }

          if (currentState === STATE.IDLE) {
            // Start new run or first trial
            trials = [];
            currentTrial = 0;
            sequenceStep = 0;
            sequenceTimes = [];
            updateStats();
            currentState = STATE.WAITING;
            setPanelClass("reaction-panel-wait");
            msg.textContent = "Wait for it...";
            submsg.textContent = "Do not click until you see GO.";
            scheduleStimulus();
          } else if (currentState === STATE.WAITING) {
            // Clicked too early
            clearTimeout(timeoutId);
            timeoutId = null;
            currentState = STATE.FALSE_START;
            setPanelClass("reaction-panel-false");
            msg.textContent = "Too early!";
            submsg.textContent =
              "You clicked before the signal. Wait for GO next time.";
            setTimeout(() => {
              if (currentState === STATE.FALSE_START) {
                currentState = STATE.WAITING;
                setPanelClass("reaction-panel-wait");
                msg.textContent = "Get ready...";
                submsg.textContent = "Wait for the GO signal.";
                scheduleStimulus();
              }
            }, 1000);
          } else if (currentState === STATE.READY) {
            const now = performance.now();
            const rt = now - stimulusTime;
            if (currentMode === "sequence") {
              sequenceTimes.push(rt);
              if (sequenceStep < 3) {
                // brief pause then next step
                sequenceStep++;
                setPanelClass("reaction-panel-wait");
                msg.textContent = "Prepare for GO " + sequenceStep;
                submsg.textContent = "Wait for the next signal.";
                currentState = STATE.WAITING;
                timeoutId = setTimeout(() => {
                  showSequenceStep();
                }, 400 + Math.random() * 400);
              } else {
                completeSequenceTrial();
              }
            } else {
              recordTrial(rt);
            }
          } else if (currentState === STATE.FALSE_START) {
            // Ignore extra clicks during false start
          }
        }

        // Event bindings
        panel.addEventListener("click", handleUserAction);
        startBtn.addEventListener("click", () => {
          resetRun();
          handleUserAction();
        });
        resetBtn.addEventListener("click", resetAllStats);
        trialSelect.addEventListener("change", () => {
          trialsPerRun = parseInt(trialSelect.value, 10);
          trialCounterEl.textContent = trials.length + " / " + trialsPerRun;
        });

        // Keyboard support (spacebar)
        document.addEventListener("keydown", (e) => {
          if (e.code === "Space" || e.key === " ") {
            e.preventDefault();
            handleUserAction();
          }
        });

        // Mode switching
        modeButtons.forEach((btn) => {
          btn.addEventListener("click", () => {
            const mode = btn.getAttribute("data-mode");
            if (mode === currentMode) return;
            currentMode = mode;
            modeButtons.forEach((b) => b.classList.remove("mode-pill-active"));
            btn.classList.add("mode-pill-active");
            resetRun();
            if (mode === "simple") {
              submsg.textContent =
                "When the panel turns green, react as fast as you can.";
            } else if (mode === "go-nogo") {
              submsg.textContent = "React only to GO. Ignore NO signals.";
            } else if (mode === "sequence") {
              submsg.textContent =
                "You will react to three GO signals in a row.";
            }
          });
        });
      })();
    </script>

