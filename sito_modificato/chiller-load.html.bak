    <script>
      // Tab handling
      const modeTabs = document.querySelectorAll(".mode-tab");
      const modeSections = document.querySelectorAll(".mode-section");
      let currentMode = "water";

      modeTabs.forEach((tab) => {
        tab.addEventListener("click", () => {
          const mode = tab.getAttribute("data-mode");
          currentMode = mode;
          modeTabs.forEach((t) => {
            t.classList.remove("bg-white", "border-gray-200", "text-blue-600");
            t.classList.add("border-transparent", "text-gray-600");
          });
          tab.classList.add("bg-white", "border-gray-200", "text-blue-600");
          tab.classList.remove("border-transparent", "text-gray-600");

          modeSections.forEach((sec) => sec.classList.add("hidden"));
          document.getElementById("mode-" + mode).classList.remove("hidden");
        });
      });

      const unitSystemEl = document.getElementById("unit-system");
      const fluidTypeEl = document.getElementById("fluid-type");
      const customFluidRow = document.getElementById("custom-fluid-row");

      function updateUnits() {
        const isSI = unitSystemEl.value === "si";
        document.getElementById("t-in-unit").textContent = isSI ? "°C" : "°F";
        document.getElementById("t-out-unit").textContent = isSI ? "°C" : "°F";
        document.getElementById("delta-t-heat-unit").textContent = isSI
          ? "K"
          : "°F";
        document.getElementById("delta-t-room-unit").textContent = isSI
          ? "K"
          : "°F";
        document.getElementById("flow-unit").textContent = isSI
          ? "m³/h"
          : "gpm";
      }

      unitSystemEl.addEventListener("change", updateUnits);

      function updateFluidRow() {
        customFluidRow.classList.toggle(
          "hidden",
          fluidTypeEl.value !== "custom"
        );
      }
      fluidTypeEl.addEventListener("change", updateFluidRow);

      updateUnits();
      updateFluidRow();

      function getFluidProps() {
        const type = fluidTypeEl.value;
        if (type === "water") {
          return { cp: 4.186, rho: 1000 }; // kJ/kgK, kg/m3
        }
        if (type === "glycol30") {
          // Approximate at typical chilled water temps
          return { cp: 3.8, rho: 1040 };
        }
        const cp = parseFloat(document.getElementById("cp-custom").value);
        const rho = parseFloat(document.getElementById("rho-custom").value);
        return {
          cp: isFinite(cp) && cp > 0 ? cp : 4.186,
          rho: isFinite(rho) && rho > 0 ? rho : 1000,
        };
      }

      function toKWFromInput(value, unit) {
        const v = parseFloat(value);
        if (!isFinite(v)) return NaN;
        if (unit === "kw") return v;
        if (unit === "btu_h") return v / 3412.142;
        if (unit === "ton") return v * 3.5168525;
        return NaN;
      }

      function formatNumber(num, decimals) {
        if (!isFinite(num)) return "–";
        return num.toLocaleString(undefined, {
          maximumFractionDigits: decimals,
          minimumFractionDigits: 0,
        });
      }

      document.getElementById("calculate-btn").addEventListener("click", () => {
        const errorEl = document.getElementById("error-msg");
        errorEl.textContent = "";
        let qKW = NaN;
        let deltaT = NaN;
        const { cp, rho } = getFluidProps();
        const isSI = unitSystemEl.value === "si";

        try {
          if (currentMode === "water") {
            const tIn = parseFloat(document.getElementById("t-in").value);
            const tOut = parseFloat(document.getElementById("t-out").value);
            const flow = parseFloat(document.getElementById("flow-rate").value);
            if (
              !isFinite(tIn) ||
              !isFinite(tOut) ||
              !isFinite(flow) ||
              flow <= 0
            ) {
              throw new Error("Enter valid temperatures and flow rate.");
            }
            deltaT = tIn - tOut;
            if (Math.abs(deltaT) < 0.01) {
              throw new Error("Temperature difference is too small.");
            }
            // Convert flow to m3/s or gpm to m3/s
            let vDot_m3s;
            if (isSI) {
              vDot_m3s = flow / 3600.0;
            } else {
              // gpm to m3/s: 1 gpm ≈ 0.0000630902 m3/s
              vDot_m3s = flow * 0.0000630902;
            }
            const mDot = rho * vDot_m3s; // kg/s
            qKW = mDot * cp * deltaT; // kW (cp in kJ/kgK)
          } else if (currentMode === "heat") {
            const qVal = document.getElementById("heat-load").value;
            const qUnit = document.getElementById("heat-load-unit").value;
            deltaT = parseFloat(document.getElementById("delta-t-heat").value);
            if (!isFinite(deltaT) || deltaT <= 0) {
              throw new Error("Enter a valid design ΔT.");
            }
            qKW = toKWFromInput(qVal, qUnit);
            if (!isFinite(qKW) || qKW <= 0) {
              throw new Error("Enter a valid total heat load.");
            }
          } else if (currentMode === "room") {
            const sVal = document.getElementById("sensible-load").value;
            const sUnit = document.getElementById("sensible-load-unit").value;
            const lVal = document.getElementById("latent-load").value;
            const lUnit = document.getElementById("latent-load-unit").value;
            deltaT = parseFloat(document.getElementById("delta-t-room").value);
            if (!isFinite(deltaT) || deltaT <= 0) {
              throw new Error("Enter a valid design ΔT.");
            }
            const sKW = toKWFromInput(sVal, sUnit);
            const lKW = toKWFromInput(lVal, lUnit);
            if (!isFinite(sKW) || sKW < 0 || !isFinite(lKW) || lKW < 0) {
              throw new Error("Enter valid sensible and latent loads.");
            }
            qKW = sKW + lKW;
          }

          const safety = parseFloat(
            document.getElementById("safety-factor").value
          );
          const sf = isFinite(safety) && safety > 0 ? 1 + safety / 100.0 : 1.0;
          qKW *= sf;

          if (!isFinite(qKW) || qKW <= 0) {
            throw new Error("Calculated load is invalid. Check your inputs.");
          }

          const qTon = qKW / 3.5168525;
          const qBTUh = qKW * 3412.142;

          // Flow calculation (for heat & room modes)
          let flowText = "–";
          if (currentMode === "water") {
            const flowInput = parseFloat(
              document.getElementById("flow-rate").value
            );
            flowText = isSI
              ? `${formatNumber(flowInput, 2)} m³/h`
              : `${formatNumber(flowInput, 2)} gpm`;
          } else {
            // derive required flow from Q and ΔT
            const mDot = qKW / (cp * deltaT); // kg/s
            const vDot_m3s = mDot / rho;
            if (isSI) {
              const vDot_m3h = vDot_m3s * 3600;
              flowText = `${formatNumber(vDot_m3h, 2)} m³/h`;
            } else {
              const gpm = vDot_m3s / 0.0000630902;
              flowText = `${formatNumber(gpm, 2)} gpm`;
            }
          }

          // Update results
          document.getElementById(
            "res-capacity-main"
          ).textContent = `${formatNumber(qKW, 2)} kW (${formatNumber(
            qTon,
            2
          )} tons)`;
          document.getElementById(
            "res-capacity-alt"
          ).textContent = `${formatNumber(qBTUh, 0)} BTU/h`;

          document.getElementById("res-flow").textContent = flowText;
          document.getElementById(
            "res-delta-t"
          ).textContent = `Design ΔT: ${formatNumber(Math.abs(deltaT), 2)} ${
            isSI ? "K (≈ °C)" : "°F"
          }`;

          // Efficiency / heat rejection
          const copVal = parseFloat(document.getElementById("cop").value);
          const effSection = document.getElementById("results-efficiency");
          if (isFinite(copVal) && copVal > 0) {
            const pCompKW = qKW / copVal;
            const qRejKW = qKW + pCompKW;
            const pCompHP = pCompKW / 0.7457;
            const qRejTon = qRejKW / 3.5168525;

            document.getElementById("res-power").textContent = `${formatNumber(
              pCompKW,
              2
            )} kW (${formatNumber(pCompHP, 2)} hp)`;
            document.getElementById(
              "res-rejection"
            ).textContent = `${formatNumber(qRejKW, 2)} kW (${formatNumber(
              qRejTon,
              2
            )} tons)`;
            effSection.classList.remove("hidden");
          } else {
            effSection.classList.add("hidden");
          }

          document.getElementById("results-section").classList.remove("hidden");
          window.scrollTo({
            top: document.getElementById("results-section").offsetTop - 80,
            behavior: "smooth",
          });
        } catch (e) {
          errorEl.textContent = e.message;
          document.getElementById("results-section").classList.add("hidden");
        }
      });

      document.getElementById("reset-btn").addEventListener("click", () => {
        document.getElementById("chiller-form").reset();
        updateUnits();
        updateFluidRow();
        document.getElementById("results-section").classList.add("hidden");
        document.getElementById("results-efficiency").classList.add("hidden");
        document.getElementById("error-msg").textContent = "";
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
