    <script>
      // WAR calculator logic
      function toNumber(id) {
        const v = parseFloat(document.getElementById(id).value);
        return isNaN(v) ? 0 : v;
      }

      function updateOutputs(values) {
        document.getElementById("war-output").textContent =
          values.war.toFixed(1);
        document.getElementById("out-bat-runs").textContent =
          values.bat.toFixed(1);
        document.getElementById("out-br-runs").textContent =
          values.br.toFixed(1);
        document.getElementById("out-field-runs").textContent =
          values.field.toFixed(1);
        document.getElementById("out-pos-runs").textContent =
          values.pos.toFixed(1);
        document.getElementById("out-pit-runs").textContent =
          values.pit.toFixed(1);
        document.getElementById("out-repl-runs").textContent =
          values.rep.toFixed(1);
        document.getElementById("out-raa").textContent = values.raa.toFixed(1);
        document.getElementById("out-rar").textContent = values.rar.toFixed(1);
        document.getElementById("out-rpw").textContent = values.rpw.toFixed(1);

        // WAR tier label
        let tier = "";
        const w = values.war;
        if (w < 0) tier = "Below replacement level.";
        else if (w < 1) tier = "Bench / up-and-down player.";
        else if (w < 2) tier = "Role player / low-end regular.";
        else if (w < 3) tier = "Solid starter.";
        else if (w < 5) tier = "All-Star level season.";
        else if (w < 7) tier = "MVP candidate season.";
        else tier = "MVP / historic season.";
        document.getElementById("war-tier").textContent = tier;
      }

      document
        .getElementById("calculate-war")
        .addEventListener("click", function () {
          const playerName = document
            .getElementById("player-name")
            .value.trim();
          document.querySelector("#player-label span").textContent =
            playerName || "—";

          const rpw = toNumber("runs-per-win") || 10;
          const repPer600 = toNumber("replacement-runs-per-600") || 20;

          const bat = toNumber("batting-runs");
          const br = toNumber("baserunning-runs");
          const field = toNumber("fielding-runs");
          const pos = toNumber("positional-runs");
          const pit = toNumber("pitching-runs");

          const pa = Math.max(0, toNumber("plate-appearances"));
          const ip = Math.max(0, toNumber("innings-pitched"));
          const role = document.getElementById("pitcher-role").value;

          // Runs above average
          const raa = bat + br + field + pos + pit;

          // Replacement runs scaling:
          // For hitters: scale by PA; for pitchers: approximate PA from IP if PA is 0.
          let effectivePA = pa;
          if (effectivePA === 0 && ip > 0) {
            // Rough conversion: ~4.3 batters per inning
            effectivePA = ip * 4.3;
          }
          const repRuns = repPer600 * (effectivePA / 600);

          const rar = raa + repRuns;
          const war = rpw !== 0 ? rar / rpw : 0;

          updateOutputs({
            bat: bat,
            br: br,
            field: field,
            pos: pos,
            pit: pit,
            rep: repRuns,
            raa: raa,
            rar: rar,
            rpw: rpw,
            war: war,
          });
        });
    </script>

