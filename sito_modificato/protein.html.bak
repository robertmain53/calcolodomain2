    <script>
      function clamp(value, min, max) {
        return Math.min(Math.max(value, min), max);
      }

      function calculateProtein() {
        const weightInput = document.getElementById("weight");
        const weightUnit = document.getElementById("weight-unit").value;
        const sex = document.getElementById("sex").value;
        const ageGroup = document.getElementById("age-group").value;
        const activity = document.getElementById("activity").value;
        const goal = document.getElementById("goal").value;
        const meals = parseInt(document.getElementById("meals").value, 10) || 3;
        const proteinQuality = document.getElementById("protein-quality").value;
        const errorMsg = document.getElementById("error-msg");

        let weight = parseFloat(weightInput.value);
        if (isNaN(weight) || weight <= 0) {
          errorMsg.textContent = "Please enter a valid weight.";
          errorMsg.classList.remove("hidden");
          return;
        }

        // Basic sanity limits
        if (weightUnit === "kg" && (weight < 30 || weight > 250)) {
          errorMsg.textContent =
            "Please enter a realistic weight in kg (30–250).";
          errorMsg.classList.remove("hidden");
          return;
        }
        if (weightUnit === "lb" && (weight < 70 || weight > 550)) {
          errorMsg.textContent =
            "Please enter a realistic weight in lb (70–550).";
          errorMsg.classList.remove("hidden");
          return;
        }
        errorMsg.classList.add("hidden");

        // Convert to kg if needed
        let weightKg = weightUnit === "kg" ? weight : weight * 0.45359237;

        // Base range by activity & goal
        let minFactor, maxFactor;

        if (goal === "maintenance") {
          if (activity === "sedentary") {
            minFactor = 0.9;
            maxFactor = 1.1;
          } else if (activity === "light") {
            minFactor = 1.0;
            maxFactor = 1.3;
          } else if (activity === "moderate") {
            minFactor = 1.2;
            maxFactor = 1.6;
          } else if (activity === "high") {
            minFactor = 1.4;
            maxFactor = 1.8;
          } else {
            // athlete
            minFactor = 1.6;
            maxFactor = 2.0;
          }
        } else if (goal === "muscle-gain") {
          if (activity === "sedentary") {
            minFactor = 1.2;
            maxFactor = 1.6;
          } else if (activity === "light") {
            minFactor = 1.4;
            maxFactor = 1.8;
          } else if (activity === "moderate") {
            minFactor = 1.6;
            maxFactor = 2.0;
          } else if (activity === "high") {
            minFactor = 1.8;
            maxFactor = 2.2;
          } else {
            // athlete
            minFactor = 2.0;
            maxFactor = 2.4;
          }
        } else {
          // fat-loss
          if (activity === "sedentary") {
            minFactor = 1.4;
            maxFactor = 1.8;
          } else if (activity === "light") {
            minFactor = 1.6;
            maxFactor = 2.0;
          } else if (activity === "moderate") {
            minFactor = 1.8;
            maxFactor = 2.2;
          } else if (activity === "high") {
            minFactor = 2.0;
            maxFactor = 2.4;
          } else {
            // athlete
            minFactor = 2.1;
            maxFactor = 2.5;
          }
        }

        // Age adjustment (older adults)
        if (ageGroup === "senior") {
          minFactor += 0.2;
          maxFactor += 0.2;
        }

        // Protein quality adjustment
        if (proteinQuality === "mostly-plant") {
          minFactor += 0.1;
          maxFactor += 0.2;
        } else if (proteinQuality === "mostly-animal") {
          // slight downward adjustment if mostly high-quality protein
          minFactor -= 0.05;
          maxFactor -= 0.05;
        }

        // Clamp to sensible global range
        minFactor = clamp(minFactor, 0.8, 2.8);
        maxFactor = clamp(maxFactor, minFactor + 0.1, 3.0);

        const minProteinG = Math.round(weightKg * minFactor);
        const maxProteinG = Math.round(weightKg * maxFactor);
        const avgProteinG = Math.round((minProteinG + maxProteinG) / 2);

        const minGkg = (minProteinG / weightKg).toFixed(2);
        const maxGkg = (maxProteinG / weightKg).toFixed(2);
        const avgGkg = (avgProteinG / weightKg).toFixed(2);

        // Update UI
        document.getElementById("protein-min-g").textContent = minProteinG;
        document.getElementById("protein-max-g").textContent = maxProteinG;
        document.getElementById("protein-avg-g").textContent = avgProteinG;

        document.getElementById("protein-min-gkg").textContent = minGkg;
        document.getElementById("protein-max-gkg").textContent = maxGkg;
        document.getElementById("protein-avg-gkg").textContent = avgGkg;

        const safeMeals = clamp(meals, 1, 8);
        document.getElementById("meals-label").textContent = safeMeals;

        const perMeal = Math.round(avgProteinG / safeMeals);
        document.getElementById("protein-per-meal").textContent = perMeal;

        // Update range marker (0.8–2.6 g/kg mapped to 0–100%)
        const marker = document.getElementById("range-marker");
        const minScale = 0.8;
        const maxScale = 2.6;
        let pos = ((avgGkg - minScale) / (maxScale - minScale)) * 100;
        pos = clamp(pos, 0, 100);
        marker.style.left = pos + "%";
        marker.title = avgGkg + " g/kg";

        // Example plan
        const examplePlan = document.getElementById("example-plan");
        examplePlan.innerHTML = "";
        const base = Math.floor(avgProteinG / safeMeals);
        let remainder = avgProteinG - base * safeMeals;
        for (let i = 1; i <= safeMeals; i++) {
          let mealProtein = base;
          if (remainder > 0) {
            mealProtein += 1;
            remainder -= 1;
          }
          const li = document.createElement("li");
          li.textContent = `Meal ${i}: ~${mealProtein} g protein`;
          examplePlan.appendChild(li);
        }
      }

      document
        .getElementById("calculate-btn")
        .addEventListener("click", calculateProtein);
      document.getElementById("reset-btn").addEventListener("click", () => {
        document.getElementById("sex").value = "female";
        document.getElementById("age-group").value = "adult";
        document.getElementById("weight").value = 70;
        document.getElementById("weight-unit").value = "kg";
        document.getElementById("activity").value = "moderate";
        document.getElementById("goal").value = "maintenance";
        document.getElementById("meals").value = 3;
        document.getElementById("protein-quality").value = "mixed";
        document.getElementById("error-msg").classList.add("hidden");
        calculateProtein();
      });

      // Initial calculation
      calculateProtein();
    </script>

