      const useTypeSelect = document.getElementById("useType");
      const customLoadWrapper = document.getElementById("customLoadWrapper");
      const customLoadInput = document.getElementById("customLoadPsf");
      const tributaryAreaInput = document.getElementById("tributaryArea");
      const influenceWidthInput = document.getElementById("influenceWidth");
      const unitSystemSelect = document.getElementById("unitSystem");
      const enableReductionCheckbox =
        document.getElementById("enableReduction");
      const calcButton = document.getElementById("calcButton");

      const resultsCard = document.getElementById("resultsCard");
      const resultsContent = document.getElementById("resultsContent");
      const llBaseImperial = document.getElementById("llBaseImperial");
      const llBaseMetric = document.getElementById("llBaseMetric");
      const llReducedImperial = document.getElementById("llReducedImperial");
      const llReducedMetric = document.getElementById("llReducedMetric");
      const lineLoadImperial = document.getElementById("lineLoadImperial");
      const lineLoadMetric = document.getElementById("lineLoadMetric");
      const llDescription = document.getElementById("llDescription");
      const resultNotes = document.getElementById("resultNotes");

      function populateUseTypes() {
        const occKey = occupancySelect.value;
        const occ = liveLoadData[occKey];
        useTypeSelect.innerHTML = "";
        Object.entries(occ.uses).forEach(([key, data]) => {
          const opt = document.createElement("option");
          opt.value = key;
          opt.textContent = data.label;
          useTypeSelect.appendChild(opt);
        });
        customLoadWrapper.classList.toggle("hidden", occKey !== "custom");
      }

      occupancySelect.addEventListener("change", populateUseTypes);

      function psfToKnm2(psf) {
        return psf * 0.04788;
      }

      function plfToKnm(plf) {
        return plf * 0.01459;
      }

      function formatNumber(value, decimals = 1) {
        if (isNaN(value) || !isFinite(value)) return "—";
        return value.toFixed(decimals);
      }

      // Simplified live load reduction (indicative only)
      function computeReducedLiveLoad(L0, A, reducible) {
        if (!reducible || !A || A <= 0) return L0;

        // Assume beam element (K_L = 4) for simplicity
        const KL = 4;
        const term = 0.25 + 15 / Math.sqrt(KL * A);
        let Lr = L0 * term;

        // Limit between 0.4L0 and L0 as a rough bound
        const minFrac = 0.4;
        if (Lr < L0 * minFrac) Lr = L0 * minFrac;
        if (Lr > L0) Lr = L0;

        return Lr;
      }

      calcButton.addEventListener("click", () => {
        const occKey = occupancySelect.value;
        const useKey = useTypeSelect.value;
        const occ = liveLoadData[occKey];
        const use = occ.uses[useKey];

        let L0 = use.psf;
        if (occKey === "custom") {
          const customVal = parseFloat(customLoadInput.value);
          if (!isNaN(customVal) && customVal > 0) {
            L0 = customVal;
          }
        }

        const A = parseFloat(tributaryAreaInput.value);
        const b = parseFloat(influenceWidthInput.value);
        const unitSystem = unitSystemSelect.value;
        const considerReduction = enableReductionCheckbox.checked;

        const Lr = considerReduction
          ? computeReducedLiveLoad(L0, A, use.reducible)
          : L0;

        // Line load based on reduced (or base) live load
        let w_plf = NaN;
        if (!isNaN(b) && b > 0) {
          w_plf = Lr * b;
        }

        // Update results
        resultsContent.classList.remove("hidden");

        // Base live load
        if (unitSystem === "imperial" || unitSystem === "both") {
          llBaseImperial.textContent = `${formatNumber(L0, 1)} psf`;
        } else {
          llBaseImperial.textContent = "";
        }
        if (unitSystem === "metric" || unitSystem === "both") {
          const L0k = psfToKnm2(L0);
          llBaseMetric.textContent = ` (${formatNumber(L0k, 2)} kN/m²)`;
        } else {
          llBaseMetric.textContent = "";
        }

        // Reduced live load
        if (considerReduction && use.reducible && A && A > 0) {
          if (unitSystem === "imperial" || unitSystem === "both") {
            llReducedImperial.textContent = `${formatNumber(Lr, 1)} psf`;
          } else {
            llReducedImperial.textContent = "";
          }
          if (unitSystem === "metric" || unitSystem === "both") {
            const Lrk = psfToKnm2(Lr);
            llReducedMetric.textContent = ` (${formatNumber(Lrk, 2)} kN/m²)`;
          } else {
            llReducedMetric.textContent = "";
          }
        } else {
          llReducedImperial.textContent = "No reduction applied";
          llReducedMetric.textContent = "";
        }

        // Line load
        if (!isNaN(w_plf)) {
          if (unitSystem === "imperial" || unitSystem === "both") {
            lineLoadImperial.textContent = `${formatNumber(w_plf, 1)} plf`;
          } else {
            lineLoadImperial.textContent = "";
          }
          if (unitSystem === "metric" || unitSystem === "both") {
            const w_kNm = plfToKnm(w_plf);
            lineLoadMetric.textContent = ` (${formatNumber(w_kNm, 2)} kN/m)`;
          } else {
            lineLoadMetric.textContent = "";
          }
        } else {
          lineLoadImperial.textContent = "Enter influence width to compute plf";
          lineLoadMetric.textContent = "";
        }

        llDescription.textContent = `${occ.label} – ${use.label}`;

        // Notes
        resultNotes.innerHTML = "";
        use.notes.forEach((n) => {
          const li = document.createElement("li");
          li.textContent = n;
          resultNotes.appendChild(li);
        });
        const liCode = document.createElement("li");
        liCode.textContent =
          "Values are indicative and based on typical ASCE 7 / IBC tables. Always verify with the actual code and local amendments.";
        resultNotes.appendChild(liCode);
        if (considerReduction) {
          const liRed = document.createElement("li");
          liRed.textContent =
            "Live load reduction shown is simplified and for educational use only. Check ASCE 7-22 Section 4.7 for exact provisions, limits, and exceptions.";
          resultNotes.appendChild(liRed);
        }
      });

      // Initialize
      populateUseTypes();

      // Simple FAQ toggle (in addition to global script)
      document
        .querySelectorAll("#faq-container .faq-question")
        .forEach((btn) => {
          btn.addEventListener("click", () => {
            const answer = btn.parentElement.querySelector(".faq-answer");
            const symbol = btn.querySelector("span:last-child");
            const isHidden = answer.classList.contains("hidden");
            document
              .querySelectorAll("#faq-container .faq-answer")
              .forEach((a) => a.classList.add("hidden"));
            document
              .querySelectorAll("#faq-container .faq-question span:last-child")
              .forEach((s) => (s.textContent = "+"));
            if (isHidden) {
              answer.classList.remove("hidden");
              symbol.textContent = "−";
            }
          });
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
