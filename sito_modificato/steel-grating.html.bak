    <script>
      // Utility conversions
      const IN_PER_FT = 12;
      const MM_PER_IN = 25.4;
      const PSI_PER_MPA = 145.0377;
      const LB_PER_KN = 224.8089;
      const PSF_PER_KN_M2 = 20.8854;
      const STEEL_E_PSI = 29000000; // modulus of elasticity
      const STEEL_DENSITY_LB_FT3 = 490;

      function toInches(value, unit) {
        if (unit === "in") return value;
        if (unit === "ft") return value * IN_PER_FT;
        if (unit === "mm") return value / MM_PER_IN;
        if (unit === "m") return (value * 1000) / MM_PER_IN;
        return value;
      }

      function fromInches(valueIn, unit) {
        if (unit === "in") return valueIn;
        if (unit === "ft") return valueIn / IN_PER_FT;
        if (unit === "mm") return valueIn * MM_PER_IN;
        if (unit === "m") return (valueIn * MM_PER_IN) / 1000;
        return valueIn;
      }

      function toPsi(value, unit) {
        if (unit === "psi") return value;
        if (unit === "MPa") return value * PSI_PER_MPA;
        return value;
      }

      function toPsf(value, unit) {
        if (unit === "psf") return value;
        if (unit === "kN_m2") return value * PSF_PER_KN_M2;
        return value;
      }

      function toLb(value, unit) {
        if (unit === "lb") return value;
        if (unit === "kN") return value * LB_PER_KN;
        return value;
      }

      function formatNumber(val, decimals = 2) {
        if (!isFinite(val)) return "–";
        return Number(val).toFixed(decimals);
      }

      function showElement(el, show) {
        el.classList.toggle("hidden", !show);
      }

      document.addEventListener("DOMContentLoaded", () => {
        const modeEl = document.getElementById("mode");
        const spanRow = document.getElementById("spanRow");
        const loadTypeEl = document.getElementById("loadType");
        const uniformLoadRow = document.getElementById("uniformLoadRow");
        const pointLoadRow = document.getElementById("pointLoadRow");
        const deflectionLimitEl = document.getElementById("deflectionLimit");
        const customDeflectionRow = document.getElementById(
          "customDeflectionRow"
        );
        const calculateBtn = document.getElementById("calculateBtn");
        const resetBtn = document.getElementById("resetBtn");
        const errorEl = document.getElementById("calcError");

        function updateModeUI() {
          const mode = modeEl.value;
          if (mode === "check") {
            showElement(spanRow, true);
          } else {
            showElement(spanRow, true); // still show span, but we will override result with max span
          }
        }

        function updateLoadTypeUI() {
          const type = loadTypeEl.value;
          showElement(uniformLoadRow, type === "uniform");
          showElement(pointLoadRow, type === "point");
        }

        function updateDeflectionUI() {
          const val = deflectionLimitEl.value;
          showElement(customDeflectionRow, val === "custom");
        }

        modeEl.addEventListener("change", updateModeUI);
        loadTypeEl.addEventListener("change", updateLoadTypeUI);
        deflectionLimitEl.addEventListener("change", updateDeflectionUI);

        resetBtn.addEventListener("click", () => {
          document.getElementById("barDepth").value = 1.25;
          document.getElementById("barThickness").value = 0.125;
          document.getElementById("barSpacing").value = 1.25;
          document.getElementById("panelWidth").value = 36;
          document.getElementById("span").value = 36;
          document.getElementById("uniformLoad").value = 250;
          document.getElementById("pointLoad").value = 1000;
          document.getElementById("fy").value = 36000;
          document.getElementById("allowableStressRatio").value = 0.6;
          deflectionLimitEl.value = "L/240";
          modeEl.value = "check";
          loadTypeEl.value = "uniform";
          updateModeUI();
          updateLoadTypeUI();
          updateDeflectionUI();
          errorEl.classList.add("hidden");
          document.getElementById("bendingStressResult").textContent = "–";
          document.getElementById("bendingStressUtil").textContent = "–";
          document.getElementById("shearStressResult").textContent = "–";
          document.getElementById("shearStressUtil").textContent = "–";
          document.getElementById("deflectionResult").textContent = "–";
          document.getElementById("deflectionLimitResult").textContent = "–";
          document.getElementById("weightResult").textContent = "–";
          document.getElementById("safetySummary").textContent =
            "Awaiting input…";
          document.getElementById("modeSpecificResult").textContent = "–";
        });

        calculateBtn.addEventListener("click", () => {
          errorEl.classList.add("hidden");
          errorEl.textContent = "";

          try {
            const barDepth = parseFloat(
              document.getElementById("barDepth").value
            );
            const barDepthUnit = document.getElementById("barDepthUnit").value;
            const barThickness = parseFloat(
              document.getElementById("barThickness").value
            );
            const barThicknessUnit =
              document.getElementById("barThicknessUnit").value;
            const barSpacing = parseFloat(
              document.getElementById("barSpacing").value
            );
            const barSpacingUnit =
              document.getElementById("barSpacingUnit").value;
            const panelWidth = parseFloat(
              document.getElementById("panelWidth").value
            );
            const panelWidthUnit =
              document.getElementById("panelWidthUnit").value;
            const spanInput = parseFloat(document.getElementById("span").value);
            const spanUnit = document.getElementById("spanUnit").value;
            const mode = modeEl.value;
            const loadType = loadTypeEl.value;
            const fy = parseFloat(document.getElementById("fy").value);
            const fyUnit = document.getElementById("fyUnit").value;
            const allowableStressRatio = parseFloat(
              document.getElementById("allowableStressRatio").value
            );
            const deflectionLimitVal = deflectionLimitEl.value;
            const customDeflection = parseFloat(
              document.getElementById("customDeflection").value
            );
            const customDeflectionUnit = document.getElementById(
              "customDeflectionUnit"
            ).value;

            if (
              [
                barDepth,
                barThickness,
                barSpacing,
                panelWidth,
                fy,
                allowableStressRatio,
              ].some((v) => !isFinite(v) || v <= 0)
            ) {
              throw new Error(
                "Please enter positive values for geometry and material properties."
              );
            }

            let spanIn = toInches(spanInput, spanUnit);
            let dIn = toInches(barDepth, barDepthUnit);
            let tIn = toInches(barThickness, barThicknessUnit);
            let sIn = toInches(barSpacing, barSpacingUnit);
            let BIn = toInches(panelWidth, panelWidthUnit);
            let fyPsi = toPsi(fy, fyUnit);

            if (sIn <= 0 || BIn <= 0) {
              throw new Error(
                "Panel width and bar spacing must be greater than zero."
              );
            }

            // Section properties in inches
            const I = (tIn * Math.pow(dIn, 3)) / 12; // in^4
            const S = (tIn * Math.pow(dIn, 2)) / 6; // in^3

            // Number of bars (approx)
            const nBars = BIn / sIn;

            // Load conversion
            let w_lb_per_in = 0; // line load per bar
            let P_lb = 0;
            let loadDescription = "";

            if (loadType === "uniform") {
              const qSurface = parseFloat(
                document.getElementById("uniformLoad").value
              );
              const qUnit = document.getElementById("uniformLoadUnit").value;
              if (!isFinite(qSurface) || qSurface <= 0) {
                throw new Error("Please enter a positive uniform load.");
              }
              const qPsf = toPsf(qSurface, qUnit); // lb/ft^2
              const sFt = sIn / IN_PER_FT;
              w_lb_per_in = (qPsf * sFt) / IN_PER_FT; // lb/in
              loadDescription = `Uniform load ${formatNumber(qPsf, 1)} psf`;
            } else {
              const P = parseFloat(document.getElementById("pointLoad").value);
              const PUnit = document.getElementById("pointLoadUnit").value;
              if (!isFinite(P) || P <= 0) {
                throw new Error("Please enter a positive point load.");
              }
              P_lb = toLb(P, PUnit);
              loadDescription = `Point load ${formatNumber(
                P_lb,
                0
              )} lb at midspan`;
            }

            const sigmaAllow = allowableStressRatio * fyPsi;

            // Deflection limit in inches
            let deflectionLimitIn;
            if (deflectionLimitVal.startsWith("L/")) {
              const N = parseFloat(deflectionLimitVal.split("/")[1]);
              deflectionLimitIn = spanIn / N;
            } else {
              deflectionLimitIn = toInches(
                customDeflection,
                customDeflectionUnit
              );
            }

            let sigmaB,
              tauV,
              delta,
              spanUsedIn = spanIn;

            function computeForSpan(Lin) {
              let Mmax, Vmax, deltaVal;
              if (loadType === "uniform") {
                const w = w_lb_per_in; // lb/in
                Mmax = (w * Math.pow(Lin, 2)) / 8; // lb*in
                Vmax = (w * Lin) / 2; // lb
                deltaVal = (5 * w * Math.pow(Lin, 4)) / (384 * STEEL_E_PSI * I); // in
              } else {
                const P = P_lb;
                Mmax = (P * Lin) / 4;
                Vmax = P / 2;
                deltaVal = (P * Math.pow(Lin, 3)) / (48 * STEEL_E_PSI * I);
              }
              const sigma = Mmax / S; // psi
              const A = tIn * dIn; // in^2
              const tau = (1.5 * Vmax) / A; // approximate max shear
              return { sigma, tau, delta: deltaVal };
            }

            if (mode === "check") {
              const res = computeForSpan(spanIn);
              sigmaB = res.sigma;
              tauV = res.tau;
              delta = res.delta;
            } else {
              // Find max allowable span based on both stress and deflection using simple iteration
              let L = 6; // start at 6 in
              const Lmax = 240; // 20 ft in inches
              let lastSafeL = L;

              while (L <= Lmax) {
                const { sigma, delta: d } = computeForSpan(L);
                if (sigma <= sigmaAllow && d <= deflectionLimitIn) {
                  lastSafeL = L;
                  L += 0.5; // increment 0.5 in
                } else {
                  break;
                }
              }

              spanUsedIn = lastSafeL;
              const res = computeForSpan(spanUsedIn);
              sigmaB = res.sigma;
              tauV = res.tau;
              delta = res.delta;
            }

            // Weight estimation
            const Lft = spanUsedIn / IN_PER_FT;
            const dFt = dIn / IN_PER_FT;
            const tFt = tIn / IN_PER_FT;
            const volumeFt3 = nBars * (tFt * dFt * Lft);
            const weightLb = volumeFt3 * STEEL_DENSITY_LB_FT3;

            // Utilization ratios
            const stressUtil = sigmaB / sigmaAllow;
            const deflUtil = delta / deflectionLimitIn;

            // Update UI
            document.getElementById(
              "bendingStressResult"
            ).textContent = `${formatNumber(
              sigmaB,
              0
            )} psi (allowable ${formatNumber(sigmaAllow, 0)} psi)`;
            document.getElementById(
              "bendingStressUtil"
            ).textContent = `Bending utilization: ${formatNumber(
              stressUtil * 100,
              1
            )}% of allowable`;

            document.getElementById(
              "shearStressResult"
            ).textContent = `${formatNumber(tauV, 0)} psi (approx.)`;
            document.getElementById("shearStressUtil").textContent =
              "Shear is rarely governing for typical bar grating; check for very short spans or heavy wheel loads.";

            const deflUnitLabel =
              spanUnit === "mm" || spanUnit === "m" ? "mm" : "in";
            const deltaDisplay =
              deflUnitLabel === "mm" ? delta * MM_PER_IN : delta;
            const deltaLimitDisplay =
              deflUnitLabel === "mm"
                ? deflectionLimitIn * MM_PER_IN
                : deflectionLimitIn;

            document.getElementById(
              "deflectionResult"
            ).textContent = `${formatNumber(deltaDisplay, 3)} ${deflUnitLabel}`;
            document.getElementById(
              "deflectionLimitResult"
            ).textContent = `Deflection limit: ${formatNumber(
              deltaLimitDisplay,
              3
            )} ${deflUnitLabel} (${
              deflectionLimitVal === "custom" ? "custom" : deflectionLimitVal
            }), utilization ${formatNumber(deflUtil * 100, 1)}%`;

            const weightKg = weightLb * 0.453592;
            document.getElementById(
              "weightResult"
            ).textContent = `${formatNumber(weightLb, 1)} lb ≈ ${formatNumber(
              weightKg,
              1
            )} kg (bearing bars only)`;

            let safetyMsg;
            if (stressUtil <= 1 && deflUtil <= 1) {
              safetyMsg =
                "OK – stresses and deflection are within the specified limits.";
            } else if (stressUtil > 1 && deflUtil <= 1) {
              safetyMsg =
                "Not OK – bending stress exceeds allowable. Increase bar size or reduce span/load.";
            } else if (stressUtil <= 1 && deflUtil > 1) {
              safetyMsg =
                "Not OK – deflection exceeds limit. Increase bar depth or reduce span/load.";
            } else {
              safetyMsg =
                "Not OK – both stress and deflection exceed limits. Redesign required.";
            }
            document.getElementById("safetySummary").textContent = safetyMsg;

            if (mode === "check") {
              const spanOut = fromInches(spanUsedIn, spanUnit);
              document.getElementById(
                "modeSpecificResult"
              ).textContent = `Checked span: ${formatNumber(
                spanOut,
                2
              )} ${spanUnit}. ${loadDescription}.`;
            } else {
              const spanOut = fromInches(spanUsedIn, spanUnit);
              document.getElementById(
                "modeSpecificResult"
              ).textContent = `Estimated maximum allowable span: ${formatNumber(
                spanOut,
                2
              )} ${spanUnit} for ${loadDescription}, limited by both stress and deflection.`;
            }
          } catch (err) {
            errorEl.textContent =
              err.message ||
              "An error occurred while calculating. Please check your inputs.";
            errorEl.classList.remove("hidden");
          }
        });

        // Initialize UI state
        updateModeUI();
        updateLoadTypeUI();
        updateDeflectionUI();
      });
    </script>

