    <script>
      // Tabs
      document.querySelectorAll(".tab-btn").forEach((btn) => {
        btn.addEventListener("click", () => {
          const target = btn.getAttribute("data-tab");
          document
            .querySelectorAll(".tab-btn")
            .forEach((b) => b.classList.remove("tab-active"));
          btn.classList.add("tab-active");
          document.querySelectorAll(".tab-panel").forEach((panel) => {
            panel.classList.toggle("hidden", panel.id !== "tab-" + target);
          });
        });
      });

      // Daily intake calculator
      const intakeContainer = document.getElementById("intake-items");
      const addItemBtn = document.getElementById("add-intake-item");
      const clearItemsBtn = document.getElementById("clear-intake-items");

      function createIntakeRow(index) {
        const row = document.createElement("div");
        row.className =
          "grid grid-cols-1 md:grid-cols-12 gap-3 items-end border rounded-lg p-3 bg-white";
        row.innerHTML = `
      <div class="md:col-span-4">
        <label class="block text-xs font-medium text-gray-700">Food / meal name (optional)</label>
        <input type="text" class="mt-1 block w-full rounded-md border-gray-300 focus:ring-blue-500 focus:border-blue-500 text-xs" placeholder="e.g. canned soup">
      </div>
      <div class="md:col-span-3">
        <label class="block text-xs font-medium text-gray-700">Sodium per serving (mg)</label>
        <input type="number" min="0" step="1" class="intake-sodium mt-1 block w-full rounded-md border-gray-300 focus:ring-blue-500 focus:border-blue-500 text-xs" placeholder="e.g. 650">
      </div>
      <div class="md:col-span-2">
        <label class="block text-xs font-medium text-gray-700">Servings eaten</label>
        <input type="number" min="0" step="0.1" class="intake-servings mt-1 block w-full rounded-md border-gray-300 focus:ring-blue-500 focus:border-blue-500 text-xs" placeholder="e.g. 1.5">
      </div>
      <div class="md:col-span-2">
        <label class="block text-xs font-medium text-gray-700">Days per week</label>
        <input type="number" min="1" max="7" step="1" class="intake-days mt-1 block w-full rounded-md border-gray-300 focus:ring-blue-500 focus:border-blue-500 text-xs" value="7">
      </div>
      <div class="md:col-span-1 flex md:justify-end">
        <button type="button" class="remove-intake-item mt-6 inline-flex items-center px-2 py-1 bg-red-50 text-red-600 text-xs font-medium rounded hover:bg-red-100">×</button>
      </div>
    `;
        return row;
      }

      function recalcIntake() {
        const rows = intakeContainer.querySelectorAll(".grid");
        let dailyTotal = 0;
        let weeklyTotal = 0;

        rows.forEach((row) => {
          const sodiumInput = row.querySelector(".intake-sodium");
          const servingsInput = row.querySelector(".intake-servings");
          const daysInput = row.querySelector(".intake-days");

          const sodium = parseFloat(sodiumInput.value);
          const servings = parseFloat(servingsInput.value);
          const days = parseFloat(daysInput.value);

          if (
            !isNaN(sodium) &&
            !isNaN(servings) &&
            sodium >= 0 &&
            servings >= 0
          ) {
            const dailyFromItem =
              sodium * servings * (isNaN(days) || days <= 0 ? 1 : days / 7);
            dailyTotal += dailyFromItem;
          }
        });

        weeklyTotal = dailyTotal * 7;

        const dailyRounded = Math.round(dailyTotal);
        const weeklyRounded = Math.round(weeklyTotal);
        document.getElementById("intake-total-mg").textContent =
          dailyRounded.toLocaleString();
        document.getElementById("intake-weekly-mg").textContent =
          weeklyRounded.toLocaleString();

        const pct2300 = dailyTotal > 0 ? (dailyTotal / 2300) * 100 : 0;
        const pct1500 = dailyTotal > 0 ? (dailyTotal / 1500) * 100 : 0;
        document.getElementById("intake-percent-2300").textContent =
          pct2300.toFixed(0);
        document.getElementById("intake-percent-1500").textContent =
          pct1500.toFixed(0);

        const interpEl = document.getElementById("intake-interpretation");
        if (dailyTotal === 0) {
          interpEl.textContent = "";
        } else if (dailyTotal <= 1500) {
          interpEl.textContent =
            "Your estimated intake is within a lower-sodium target (≤ 1,500 mg/day). Always confirm your personal goal with your clinician.";
        } else if (dailyTotal <= 2300) {
          interpEl.textContent =
            "Your estimated intake is below the general upper limit of 2,300 mg/day but above 1,500 mg/day. Many people with high blood pressure are advised to aim closer to 1,500 mg/day.";
        } else {
          interpEl.textContent =
            "Your estimated intake is above 2,300 mg/day, which is higher than most guideline recommendations. Consider discussing sodium reduction strategies with a healthcare professional.";
        }
      }

      function attachIntakeRowEvents(row) {
        row.querySelectorAll("input").forEach((input) => {
          input.addEventListener("input", recalcIntake);
        });
        row
          .querySelector(".remove-intake-item")
          .addEventListener("click", () => {
            row.remove();
            recalcIntake();
          });
      }

      addItemBtn.addEventListener("click", () => {
        const row = createIntakeRow();
        intakeContainer.appendChild(row);
        attachIntakeRowEvents(row);
      });

      clearItemsBtn.addEventListener("click", () => {
        intakeContainer.innerHTML = "";
        recalcIntake();
      });

      // Initialize with one row
      (function initIntake() {
        const row = createIntakeRow();
        intakeContainer.appendChild(row);
        attachIntakeRowEvents(row);
      })();

      // Corrected sodium calculator
      const measuredInput = document.getElementById("measured-sodium");
      const glucoseInput = document.getElementById("glucose");
      const corrCustomInput = document.getElementById("corr-custom");
      const correctedBtn = document.getElementById("corrected-calc-btn");
      const correctedBox = document.getElementById("corrected-results");
      const correctedValueEl = document.getElementById("corrected-sodium");
      const deltaEl = document.getElementById("sodium-delta");
      const noteEl = document.getElementById("corrected-note");

      function getCorrectionFactor() {
        const selected = document.querySelector(
          'input[name="corr-factor"]:checked'
        ).value;
        if (selected === "custom") {
          const v = parseFloat(corrCustomInput.value);
          return isNaN(v) || v <= 0 ? null : v;
        }
        return parseFloat(selected);
      }

      correctedBtn.addEventListener("click", () => {
        [measuredInput, glucoseInput, corrCustomInput].forEach((el) =>
          el.classList.remove("input-error")
        );

        const measured = parseFloat(measuredInput.value);
        const glucose = parseFloat(glucoseInput.value);
        let hasError = false;

        if (isNaN(measured)) {
          measuredInput.classList.add("input-error");
          hasError = true;
        }
        if (isNaN(glucose)) {
          glucoseInput.classList.add("input-error");
          hasError = true;
        }

        const factor = getCorrectionFactor();
        if (
          document.querySelector('input[name="corr-factor"]:checked').value ===
            "custom" &&
          factor === null
        ) {
          corrCustomInput.classList.add("input-error");
          hasError = true;
        }

        if (hasError) {
          correctedBox.classList.add("hidden");
          return;
        }

        const effectiveGlucose = Math.max(glucose, 100);
        const deltaGlucose = effectiveGlucose - 100;
        const correction = factor * (deltaGlucose / 100);
        const corrected = measured + correction;

        correctedValueEl.textContent = corrected.toFixed(1);
        deltaEl.textContent = correction.toFixed(1);
        correctedBox.classList.remove("hidden");

        let factorText = factor.toFixed(1);
        noteEl.textContent = `Using a correction factor of ${factorText} mEq/L per 100 mg/dL above 100, a glucose of ${glucose.toFixed(
          0
        )} mg/dL yields a corrected sodium of ${corrected.toFixed(
          1
        )} mEq/L. This is an estimate only and should be interpreted in clinical context.`;
      });

      // FAQ toggle (simple, in addition to global script_faq if present)
      document
        .querySelectorAll("#faq-container .faq-question")
        .forEach((btn) => {
          btn.addEventListener("click", () => {
            const answer = btn.parentElement.querySelector(".faq-answer");
            const symbol = btn.querySelector("span:last-child");
            const isHidden = answer.classList.contains("hidden");
            document
              .querySelectorAll("#faq-container .faq-answer")
              .forEach((a) => a.classList.add("hidden"));
            document
              .querySelectorAll("#faq-container .faq-question span:last-child")
              .forEach((s) => (s.textContent = "+"));
            if (isHidden) {
              answer.classList.remove("hidden");
              symbol.textContent = "−";
            }
          });
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
