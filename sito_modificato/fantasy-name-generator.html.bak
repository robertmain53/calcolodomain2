<script>
  // Fantasy name generator logic
  (function() {
    const resultsContainer = document.getElementById('resultsContainer');
    const generateBtn = document.getElementById('generateBtn');
    const rerollBtn = document.getElementById('rerollUnlockedBtn');
    const copyAllBtn = document.getElementById('copyAllBtn');
    const exportBtn = document.getElementById('exportBtn');
    const statusMessage = document.getElementById('statusMessage');

    const categoryEl = document.getElementById('category');
    const raceEl = document.getElementById('race');
    const toneEl = document.getElementById('tone');
    const syllablesEl = document.getElementById('syllables');
    const startsWithEl = document.getElementById('startsWith');
    const endsWithEl = document.getElementById('endsWith');
    const countEl = document.getElementById('count');
    const avoidHardEl = document.getElementById('avoidHard');
    const lowRandomnessEl = document.getElementById('lowRandomness');

    const presets = document.querySelectorAll('[data-preset]');

    // Syllable banks by race/culture
    const baseSyllables = {
      any: ["ar","bel","cor","dan","el","fae","gal","hal","is","jor","ka","lor","mir","nor","or","per","qua","rin","sar","tor","ur","val","wyn","xer","yor","zel"],
      human: ["al","ben","cal","dar","ed","fen","gar","hen","ian","jon","kar","leo","mar","ned","os","petr","quin","rob","sam","tom","ul","vin","wil","yor","zan"],
      elven: ["ae","al","ara","ari","cel","dae","ela","eri","fae","gal","ia","iel","il","ira","lae","lith","mir","na","nel","nor","phir","qua","rae","syl","thal","thel","var","wyn"],
      dwarven: ["bal","brom","dun","dwar","far","gar","gim","grim","kaz","kor","mag","mor","nor","ogr","ragn","thor","thra","ul","var","zor"],
      orcish: ["az","bag","drok","gar","grom","gruk","karg","krag","morg","narg","rag","rok","thok","ug","vor","zug"],
      dragonborn: ["ar","az","bal","dar","drak","dra","faer","ghar","ith","jhar","kyr","mir","nar","oz","qir","rax","saar","thor","vyr","zhar"],
      dark: ["az","bal","dar","dra","dred","gloom","mal","mor","nec","noct","nyx","obs","rav","sor","thar","umb","vel","vor","xan","zir"],
      celestial: ["ael","aur","cel","dea","eon","eph","gal","hel","iel","lum","lys","mir","oph","ora","rael","ser","sol","theo","ura","zel"],
      nordic: ["arn","bjorn","dag","ein","finn","gud","hal","ing","jarl","kjell","leif","magn","odin","ragn","sig","thor","ulf","vid","yr"],
      eastern: ["aki","bao","chi","dao","feng","han","jin","kai","li","mei","nao","qin","rui","shan","tai","wei","xian","yan","zhao","zhen"]
    };

    // Town / guild / artifact word parts
    const townParts = {
      prefix: ["Amber","Ash","Black","Bright","Dragon","Eagle","Frost","Gold","Green","High","Iron","Mist","Moon","Oak","Raven","Red","Silver","Star","Stone","Storm","Sun","White","Wind","Wolf"],
      suffix: ["barrow","bridge","brook","crest","dale","fall","field","ford","gate","haven","helm","hold","hollow","keep","mere","mont","moor","port","reach","rest","shire","stead","vale","watch","wick"]
    };

    const guildParts = {
      prefix: ["Order","Circle","Brotherhood","Sisterhood","Company","League","Cabal","Council","Choir","Band","Guard","Knights","Blades","Hands","Wardens","Seekers"],
      middle: ["of the","of","for the"],
      suffix: ["Silver Star","Veiled Sun","Eternal Flame","Hidden Path","Black Rose","Shattered Crown","Gilded Dawn","Whispering Wind","Obsidian Eye","Crimson Oath","Emerald Scale","Fallen Sky"]
    };

    const artifactParts = {
      prefix: ["Blade","Crown","Amulet","Orb","Staff","Ring","Lantern","Tome","Aegis","Helm","Shard","Heart","Key","Mirror","Stone","Chalice"],
      of: ["of","of the"],
      suffix: ["Ancient Kings","Fallen Empire","First Dawn","Last Light","Whispering Woods","Endless Night","Stormbound Seas","Forgotten Realms","Eternal Winter","Burning Sky","Silent Choir","Hidden Paths"]
    };

    const creatureParts = {
      base: ["wyrm","drake","hound","fiend","wraith","golem","sprite","nymph","titan","leviathan","behemoth","chimera","basilisk","manticore","griffon"],
      prefix: ["Ash","Bone","Frost","Storm","Void","Night","Sun","Star","Blood","Iron","Shadow","Thunder","Plague","Rune","Ghost"]
    };

    function rand(arr) {
      return arr[Math.floor(Math.random() * arr.length)];
    }

    function clampCount() {
      let n = parseInt(countEl.value, 10);
      if (isNaN(n) || n < 1) n = 1;
      if (n > 50) n = 50;
      countEl.value = n;
      return n;
    }

    function chooseSyllableBank(race) {
      if (!baseSyllables[race]) return baseSyllables.any;
      return baseSyllables[race];
    }

    function buildCharacterName(race, tone, syllables, lowRandom) {
      const bank = chooseSyllableBank(race);
      let targetSyllables;
      if (syllables === 'short') targetSyllables = 2;
      else if (syllables === 'medium') targetSyllables = 3;
      else if (syllables === 'long') targetSyllables = 4;
      else targetSyllables = 2 + Math.floor(Math.random() * 3); // 2–4

      const parts = [];
      for (let i = 0; i < targetSyllables; i++) {
        let s = rand(bank);
        if (lowRandom && i > 0 && Math.random() < 0.5) {
          // reuse previous syllable pattern start/end
          const prev = parts[parts.length - 1];
          if (prev && prev.length > 2) {
            if (Math.random() < 0.5) {
              s = prev.slice(0, 2);
            } else {
              s = prev.slice(-2);
            }
          }
        }
        parts.push(s);
      }

      let name = parts.join("");

      // Tone-based tweaks
      if (tone === 'dark') {
        const darkSuffixes = ["ar","ath","or","oth","ar","os","ul","azar","azar","thor","gash","mor","dred"];
        if (Math.random() < 0.7) name += rand(darkSuffixes);
      } else if (tone === 'ancient') {
        const ancientSuffixes = ["ion","ius","ar","en","or","iel","ael","anor","eth","orim"];
        if (Math.random() < 0.6) name += rand(ancientSuffixes);
      } else if (tone === 'whimsical') {
        const whimsyEndings = ["y","ie","o","u","el","i"];
        if (Math.random() < 0.5) name += rand(whimsyEndings);
      }

      // Capitalize first letter
      name = name.charAt(0).toUpperCase() + name.slice(1);

      return name;
    }

    function buildTownName(tone) {
      const pre = rand(townParts.prefix);
      const suf = rand(townParts.suffix);
      let name = pre + suf.charAt(0).toUpperCase() + suf.slice(1);

      if (tone === 'dark' && Math.random() < 0.4) {
        const darkMods = ["Black","Grim","Dread","Shadow","Raven","Night"];
        name = rand(darkMods) + name;
      } else if (tone === 'whimsical' && Math.random() < 0.4) {
        const whimsyMods = ["Jolly","Merry","Lucky","Sunny","Bright"];
        name = rand(whimsyMods) + " " + name;
      }

      return name;
    }

    function buildGuildName(tone) {
      const pre = rand(guildParts.prefix);
      const mid = rand(guildParts.middle);
      let suf = rand(guildParts.suffix);

      if (tone === 'dark') {
        const darkWords = ["Ebon Veil","Black Sun","Bleak Star","Endless Night","Crimson Shroud"];
        if (Math.random() < 0.6) suf = rand(darkWords);
      } else if (tone === 'whimsical') {
        const whimsyWords = ["Laughing Lanterns","Wandering Bards","Jolly Owls","Golden Geese"];
        if (Math.random() < 0.6) suf = rand(whimsyWords);
      }

      return `${pre} ${mid} ${suf}`;
    }

    function buildArtifactName(tone) {
      const pre = rand(artifactParts.prefix);
      const ofWord = rand(artifactParts.of);
      let suf = rand(artifactParts.suffix);

      if (tone === 'dark') {
        const darkSuf = ["the Void","the Abyss","Forgotten Souls","Broken Oaths","the Black Sun"];
        if (Math.random() < 0.6) suf = rand(darkSuf);
      } else if (tone === 'whimsical') {
        const whimsySuf = ["Lost Socks","Endless Tea","Wayward Dreams","Ticklish Stars","Happy Accidents"];
        if (Math.random() < 0.6) suf = rand(whimsySuf);
      }

      return `${pre} ${ofWord} ${suf}`;
    }

    function buildCreatureName(tone) {
      const pre = rand(creatureParts.prefix);
      const base = rand(creatureParts.base);
      let name = pre + " " + base;

      if (tone === 'dark' && Math.random() < 0.4) {
        const extra = ["of the Deep","of the Pit","of the Wastes","of the Void"];
        name += " " + rand(extra);
      }

      return name;
    }

    function applyStartEnd(name, startsWith, endsWith) {
      let n = name;
      if (startsWith) {
        const sw = startsWith.toLowerCase();
        if (!n.toLowerCase().startsWith(sw)) {
          n = sw + n;
        }
      }
      if (endsWith) {
        const ew = endsWith.toLowerCase();
        if (!n.toLowerCase().endsWith(ew)) {
          n = n + ew;
        }
      }
      // Capitalize first letter of each word
      n = n.split(" ").map(w => w.charAt(0).toUpperCase() + w.slice(1)).join(" ");
      return n;
    }

    function avoidTongueTwisters(name) {
      // Simple heuristic: collapse triple consonants/vowels
      return name.replace(/([bcdfghjklmnpqrstvwxyz])\1\1+/gi, '$1$1')
                 .replace(/([aeiou])\1\1+/gi, '$1$1');
    }

    function generateSingleName() {
      const category = categoryEl.value;
      const race = raceEl.value;
      const tone = toneEl.value;
      const syllables = syllablesEl.value;
      const startsWith = (startsWithEl.value || "").trim();
      const endsWith = (endsWithEl.value || "").trim();
      const avoidHard = avoidHardEl.checked;
      const lowRandom = lowRandomEl.checked;

      let name;
      if (category === 'character') {
        name = buildCharacterName(race, tone, syllables, lowRandom);
      } else if (category === 'town') {
        name = buildTownName(tone);
      } else if (category === 'guild') {
        name = buildGuildName(tone);
      } else if (category === 'artifact') {
        name = buildArtifactName(tone);
      } else if (category === 'creature') {
        name = buildCreatureName(tone);
      } else {
        name = buildCharacterName(race, tone, syllables, lowRandom);
      }

      name = applyStartEnd(name, startsWith, endsWith);
      if (avoidHard) {
        name = avoidTongueTwisters(name);
      }
      return name;
    }

    function clearStatus() {
      statusMessage.textContent = "";
    }

    function setStatus(msg) {
      statusMessage.textContent = msg;
      if (msg) {
        setTimeout(clearStatus, 2500);
      }
    }

    function renderNames(names, preserveLocks) {
      const existing = {};
      if (preserveLocks) {
        // Keep locked items and their lock state
        resultsContainer.querySelectorAll('[data-name-item]').forEach((el, idx) => {
          const locked = el.getAttribute('data-locked') === 'true';
          const text = el.querySelector('[data-name-text]').textContent;
          existing[idx] = { locked, text };
        });
      }

      resultsContainer.innerHTML = "";
      names.forEach((name, index) => {
        const wrapper = document.createElement('div');
        wrapper.className = "flex items-center justify-between border border-gray-200 rounded-md px-3 py-2 text-sm card-hover";
        wrapper.setAttribute('data-name-item', 'true');
        wrapper.setAttribute('data-locked', existing[index]?.locked ? 'true' : 'false');

        const textBtn = document.createElement('button');
        textBtn.type = "button";
        textBtn.className = "text-left flex-1 mr-2 focus:outline-none";
        textBtn.setAttribute('data-name-text', 'true');
        textBtn.textContent = existing[index]?.locked ? existing[index].text : name;

        const lockBtn = document.createElement('button');
        lockBtn.type = "button";
        lockBtn.className = "ml-2 text-gray-400 hover:text-blue-600 focus:outline-none";
        lockBtn.setAttribute('aria-label', 'Lock name');

        function updateLockIcon() {
          const locked = wrapper.getAttribute('data-locked') === 'true';
          lockBtn.innerHTML = locked
            ? '<svg class="w-4 h-4" fill="currentColor" viewBox="0 0 20 20" aria-hidden="true"><path fill-rule="evenodd" d="M5 8V6a5 5 0 1110 0v2a2 2 0 012 2v6a2 2 0 01-2 2H5a2 2 0 01-2-2v-6a2 2 0 012-2zm2-2a3 3 0 116 0v2H7V6z" clip-rule="evenodd"/></svg>'
            : '<svg class="w-4 h-4" fill="none" stroke="currentColor" viewBox="0 0 24 24" aria-hidden="true"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 11c-1.105 0-2 .895-2 2v2a2 2 0 104 0v-2c0-1.105-.895-2-2-2z"/><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17 11V7a5 5 0 10-10 0v4M7 11h10a2 2 0 012 2v6a2 2 0 01-2 2H7a2 2 0 01-2-2v-6a2 2 0 012-2z"/></svg>';
        }

        updateLockIcon();

        lockBtn.addEventListener('click', () => {
          const locked = wrapper.getAttribute('data-locked') === 'true';
          wrapper.setAttribute('data-locked', locked ? 'false' : 'true');
          updateLockIcon();
        });

        textBtn.addEventListener('click', async () => {
          const text = textBtn.textContent;
          try {
            await navigator.clipboard.writeText(text);
            setStatus(`Copied "${text}"`);
          } catch (e) {
            setStatus("Copied to clipboard (or press Ctrl+C)");
          }
        });

        wrapper.appendChild(textBtn);
        wrapper.appendChild(lockBtn);
        resultsContainer.appendChild(wrapper);
      });
    }

    function generateNames(preserveLocks = false) {
      const n = clampCount();
      const names = [];
      for (let i = 0; i < n; i++) {
        names.push(generateSingleName());
      }
      renderNames(names, preserveLocks);
    }

    generateBtn.addEventListener('click', () => {
      generateNames(false);
      setStatus("Generated new names");
    });

    rerollBtn.addEventListener('click', () => {
      const items = resultsContainer.querySelectorAll('[data-name-item]');
      if (!items.length) {
        generateNames(false);
        setStatus("Generated names");
        return;
      }
      items.forEach(item => {
        const locked = item.getAttribute('data-locked') === 'true';
        if (!locked) {
          const textEl = item.querySelector('[data-name-text]');
          textEl.textContent = generateSingleName();
        }
      });
      setStatus("Rerolled unlocked names");
    });

    copyAllBtn.addEventListener('click', async () => {
      const names = Array.from(resultsContainer.querySelectorAll('[data-name-text]')).map(el => el.textContent.trim()).filter(Boolean);
      if (!names.length) {
        setStatus("No names to copy");
        return;
      }
      const text = names.join(", ");
      try {
        await navigator.clipboard.writeText(text);
        setStatus("Copied all names");
      } catch (e) {
        setStatus("Unable to access clipboard");
      }
    });

    exportBtn.addEventListener('click', () => {
      const names = Array.from(resultsContainer.querySelectorAll('[data-name-text]')).map(el => el.textContent.trim()).filter(Boolean);
      if (!names.length) {
        setStatus("No names to export");
        return;
      }
      const blob = new Blob([names.join("\n")], { type: "text/plain" });
      const url = URL.createObjectURL(blob);
      const a = document.createElement('a');
      a.href = url;
      a.download = "fantasy-names.txt";
      document.body.appendChild(a);
      a.click();
      document.body.removeChild(a);
      URL.revokeObjectURL(url);
      setStatus("Exported as text file");
    });

    // Presets
    presets.forEach(btn => {
      btn.addEventListener('click', () => {
        const preset = btn.getAttribute('data-preset');
        if (preset === 'elven-hero') {
          categoryEl.value = 'character';
          raceEl.value = 'elven';
          toneEl.value = 'serious';
          syllablesEl.value = 'medium';
