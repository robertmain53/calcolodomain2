    <script>
      // Nail presets
      const nailPresets = {
        custom: null,
        "6d": { d: 0.113, L: 2.0 },
        "8d": { d: 0.131, L: 2.5 },
        "10d": { d: 0.148, L: 3.0 },
        "16d": { d: 0.162, L: 3.5 },
      };

      const presetSelect = document.getElementById("nailPreset");
      const dInput = document.getElementById("nailDiameter");
      const LInput = document.getElementById("nailLength");
      const mainMemberSelect = document.getElementById("mainMember");
      const sideMemberSelect = document.getElementById("sideMember");
      const tSideInput = document.getElementById("sideThickness");
      const pInput = document.getElementById("penetration");
      const CDSelect = document.getElementById("loadDuration");
      const phiInput = document.getElementById("phi");
      const nailCountInput = document.getElementById("nailCount");

      const refShearSpan = document.getElementById("refShear");
      const refWithdrawalSpan = document.getElementById("refWithdrawal");
      const designShearSpan = document.getElementById("designShear");
      const designWithdrawalSpan = document.getElementById("designWithdrawal");
      const totalShearSpan = document.getElementById("totalShear");
      const totalWithdrawalSpan = document.getElementById("totalWithdrawal");
      const nailCountDisplay = document.getElementById("nailCountDisplay");
      const checkPenetration = document.getElementById("checkPenetration");

      presetSelect.addEventListener("change", () => {
        const val = presetSelect.value;
        if (nailPresets[val]) {
          dInput.value = nailPresets[val].d.toFixed(3);
          LInput.value = nailPresets[val].L.toFixed(2);
          // Reasonable default penetration: about 12d but not exceeding length - side thickness
          const d = parseFloat(dInput.value) || 0.131;
          const tSide = parseFloat(tSideInput.value) || 1.5;
          const L = parseFloat(LInput.value) || 2.5;
          const targetPen = Math.min(L - tSide, 12 * d);
          if (targetPen > 0.5) {
            pInput.value = targetPen.toFixed(2);
          }
        }
      });

      document.getElementById("calcBtn").addEventListener("click", () => {
        const d = Math.max(parseFloat(dInput.value) || 0, 0.05);
        const L = Math.max(parseFloat(LInput.value) || 0, 0.5);
        const G = parseFloat(mainMemberSelect.value) || 0.5;
        const tSide = Math.max(parseFloat(tSideInput.value) || 0, 0.25);
        const p = Math.max(parseFloat(pInput.value) || 0, 0.5);
        const CD = parseFloat(CDSelect.value) || 1.0;
        const phi = Math.min(
          Math.max(parseFloat(phiInput.value) || 0.65, 0.3),
          1.0
        );
        const n = Math.max(parseInt(nailCountInput.value, 10) || 1, 1);

        // Empirical constants tuned to be in a reasonable range
        // Lateral capacity constant (approximate)
        let C1 = 900; // base
        if (sideMemberSelect.value === "osb") {
          C1 *= 0.8; // OSB/plywood slightly lower
        }

        // Reference lateral capacity per nail (lb)
        const Zref = C1 * Math.pow(G, 0.8) * Math.pow(d, 1.5) * tSide;

        // Withdrawal constant (approximate)
        const C2 = 250; // base for smooth shank nails

        // Reference withdrawal capacity per nail (lb)
        const Wref = C2 * Math.pow(G, 1.5) * d * p;

        // Design capacities
        const Rv = phi * CD * Zref;
        const Rw = phi * CD * Wref;

        // Totals
        const RvTotal = Rv * n;
        const RwTotal = Rw * n;

        // Update UI
        refShearSpan.textContent = Zref.toFixed(0);
        refWithdrawalSpan.textContent = Wref.toFixed(0);
        designShearSpan.textContent = Rv.toFixed(0);
        designWithdrawalSpan.textContent = Rw.toFixed(0);
        totalShearSpan.textContent = RvTotal.toFixed(0);
        totalWithdrawalSpan.textContent = RwTotal.toFixed(0);
        nailCountDisplay.textContent = n.toString();

        // Penetration check
        const minPen = 10 * d;
        const okPen = p >= minPen;
        checkPenetration.textContent = `• Penetration ≥ 10d? ${
          okPen ? "OK" : "Not OK"
        } (p = ${p.toFixed(2)} in, 10d = ${minPen.toFixed(2)} in)`;
        checkPenetration.classList.toggle("text-red-600", !okPen);
        checkPenetration.classList.toggle("text-green-700", okPen);
      });
    </script>

