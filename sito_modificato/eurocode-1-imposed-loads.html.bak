    <script>
      // Basic data for categories (simplified, indicative only)
      const categoryData = {
        A: {
          desc: "Residential, domestic areas (rooms, kitchens, corridors in dwellings).",
          qk: 2.0,
          Qk: 2.0,
          psi: { psi0: 0.7, psi1: 0.5, psi2: 0.3 },
        },
        B: {
          desc: "Office areas (office floors, corridors).",
          qk: 3.0,
          Qk: 3.0,
          psi: { psi0: 0.7, psi1: 0.5, psi2: 0.3 },
        },
        C1: {
          desc: "Areas with tables, e.g. restaurants, dining halls.",
          qk: 2.0,
          Qk: 3.0,
          psi: { psi0: 0.7, psi1: 0.7, psi2: 0.6 },
        },
        C2: {
          desc: "Areas with fixed seating (theatres, churches).",
          qk: 3.0,
          Qk: 4.0,
          psi: { psi0: 0.7, psi1: 0.7, psi2: 0.6 },
        },
        C3: {
          desc: "Areas without obstacles, e.g. school corridors, assembly areas.",
          qk: 4.0,
          Qk: 4.0,
          psi: { psi0: 0.7, psi1: 0.7, psi2: 0.6 },
        },
        C4: {
          desc: "Areas for physical activities (sports halls, dance halls).",
          qk: 5.0,
          Qk: 7.0,
          psi: { psi0: 0.7, psi1: 0.7, psi2: 0.6 },
        },
        C5: {
          desc: "Areas susceptible to overcrowding (stadia, concert halls).",
          qk: 7.0,
          Qk: 10.0,
          psi: { psi0: 0.7, psi1: 0.7, psi2: 0.6 },
        },
        D: {
          desc: "Shopping areas (malls, supermarkets, department stores).",
          qk: 4.0,
          Qk: 4.5,
          psi: { psi0: 0.7, psi1: 0.7, psi2: 0.6 },
        },
        E: {
          desc: "Storage and industrial areas (light storage, workshops).",
          qk: 5.0,
          Qk: 7.0,
          psi: { psi0: 0.7, psi1: 0.6, psi2: 0.6 },
        },
        F: {
          desc: "Traffic areas for vehicles ≤ 30 kN axle load (car parks, garages).",
          qk: 2.5,
          Qk: 10.0,
          psi: { psi0: 0.7, psi1: 0.5, psi2: 0.3 },
        },
        G: {
          desc: "Traffic areas for vehicles > 30 kN axle load (lorry docks, industrial yards).",
          qk: 5.0,
          Qk: 20.0,
          psi: { psi0: 0.7, psi1: 0.5, psi2: 0.3 },
        },
        H: {
          desc: "Roofs (accessible or inaccessible).",
          qk: 0.75,
          Qk: 1.0,
          psi: { psi0: 0.7, psi1: 0.5, psi2: 0.3 },
        },
      };

      function updateCategoryDescription() {
        const cat = document.getElementById("useCategory").value;
        const data = categoryData[cat];
        const descEl = document.getElementById("categoryDescription");
        if (!data) {
          descEl.textContent = "";
          return;
        }
        descEl.textContent = `${data.desc} Typical qk ≈ ${data.qk} kN/m², Qk ≈ ${data.Qk} kN (check EN 1991-1-1 & National Annex).`;

        // If user hasn't edited psi fields, suggest defaults for this category
        const psi0El = document.getElementById("psi0");
        const psi1El = document.getElementById("psi1");
        const psi2El = document.getElementById("psi2");
        if (!psi0El.dataset.userEdited) psi0El.value = data.psi.psi0;
        if (!psi1El.dataset.userEdited) psi1El.value = data.psi.psi1;
        if (!psi2El.dataset.userEdited) psi2El.value = data.psi.psi2;
      }

      document
        .getElementById("useCategory")
        .addEventListener("change", updateCategoryDescription);

      ["psi0", "psi1", "psi2"].forEach((id) => {
        const el = document.getElementById(id);
        el.addEventListener("input", () => {
          el.dataset.userEdited = "true";
        });
      });

      function roundTo(value, decimals) {
        const factor = Math.pow(10, decimals);
        return Math.round(value * factor) / factor;
      }

      document.getElementById("calcButton").addEventListener("click", () => {
        const cat = document.getElementById("useCategory").value;
        const floorType = document.getElementById("floorType").value;
        const gammaQ = parseFloat(document.getElementById("gammaQ").value);
        const psi0 = parseFloat(document.getElementById("psi0").value);
        const psi1 = parseFloat(document.getElementById("psi1").value);
        const psi2 = parseFloat(document.getElementById("psi2").value);
        const overrideQk = parseFloat(
          document.getElementById("overrideQk").value
        );
        const overrideQkPoint = parseFloat(
          document.getElementById("overrideQkPoint").value
        );

        const data = categoryData[cat];
        if (
          !data ||
          isNaN(gammaQ) ||
          isNaN(psi0) ||
          isNaN(psi1) ||
          isNaN(psi2)
        ) {
          alert("Please check category selection and factor inputs.");
          return;
        }

        // Base qk and Qk from table
        let qk = data.qk;
        let Qk = data.Qk;

        // Adjust for floor type (very simplified heuristics)
        switch (floorType) {
          case "stairs":
            qk *= 1.2;
            break;
          case "corridor":
            qk *= 1.1;
            break;
          case "balcony":
            qk *= 1.3;
            break;
          case "roofAccessible":
            if (cat !== "H") qk = Math.max(qk, 1.5);
            break;
          case "roofInaccessible":
            qk = 0.4;
            break;
          default:
            break;
        }

        // Apply overrides if provided
        if (!isNaN(overrideQk)) qk = overrideQk;
        if (!isNaN(overrideQkPoint)) Qk = overrideQkPoint;

        const qd = gammaQ * qk;
        const qCombUls = psi0 * qk;
        const qFreq = psi1 * qk;
        const qQuasi = psi2 * qk;

        document.getElementById("resQk").textContent = roundTo(qk, 2).toFixed(
          2
        );
        document.getElementById("resQkPoint").textContent = roundTo(
          Qk,
          2
        ).toFixed(2);
        document.getElementById("resQd").textContent = roundTo(qd, 2).toFixed(
          2
        );
        document.getElementById("resQcombUls").textContent = roundTo(
          qCombUls,
          2
        ).toFixed(2);
        document.getElementById("resQfreq").textContent = roundTo(
          qFreq,
          2
        ).toFixed(2);
        document.getElementById("resQquasi").textContent = roundTo(
          qQuasi,
          2
        ).toFixed(2);

        const note = [];
        note.push(`Category ${cat} – ${data.desc}`);
        note.push(
          `Floor type: ${
            document.getElementById("floorType").options[
              document.getElementById("floorType").selectedIndex
            ].text
          }`
        );
        note.push(`γQ = ${gammaQ}, ψ0 = ${psi0}, ψ1 = ${psi1}, ψ2 = ${psi2}.`);
        note.push(
          "Values are indicative only. Always verify against EN 1991-1-1 and the applicable National Annex."
        );
        document.getElementById("noteText").textContent = note.join(" ");

        document.getElementById("resultsCard").classList.remove("hidden");
      });

      document.getElementById("resetButton").addEventListener("click", () => {
        document.getElementById("useCategory").value = "A";
        document.getElementById("floorType").value = "standard";
        document.getElementById("gammaQ").value = 1.5;
        document.getElementById("psi0").value = 0.7;
        document.getElementById("psi1").value = 0.5;
        document.getElementById("psi2").value = 0.3;
        document.getElementById("overrideQk").value = "";
        document.getElementById("overrideQkPoint").value = "";
        ["psi0", "psi1", "psi2"].forEach(
          (id) => delete document.getElementById(id).dataset.userEdited
        );
        updateCategoryDescription();
        document.getElementById("resultsCard").classList.add("hidden");
      });

      // Initialize on load
      updateCategoryDescription();

      document
        .querySelectorAll("#faq-container .faq-question")
        .forEach((btn) => {
          btn.addEventListener("click", () => {
            const answer = btn.parentElement.querySelector(".faq-answer");
            const icon = btn.querySelector("span:last-child");
            const isHidden = answer.classList.contains("hidden");
            document
              .querySelectorAll("#faq-container .faq-answer")
              .forEach((a) => a.classList.add("hidden"));
            document
              .querySelectorAll("#faq-container .faq-question span:last-child")
              .forEach((i) => (i.textContent = "+"));
            if (isHidden) {
              answer.classList.remove("hidden");
              icon.textContent = "−";
            }
          });
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
