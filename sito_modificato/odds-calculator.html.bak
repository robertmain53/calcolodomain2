    <script>
      // Utility functions for odds parsing & conversion
      function parseFractional(str) {
        const parts = str.split("/");
        if (parts.length !== 2) return null;
        const num = parseFloat(parts[0]);
        const den = parseFloat(parts[1]);
        if (!isFinite(num) || !isFinite(den) || den === 0) return null;
        return num / den;
      }

      function decimalFromAmerican(american) {
        if (!isFinite(american) || american === 0) return null;
        if (american > 0) {
          return 1 + american / 100;
        } else {
          return 1 + 100 / Math.abs(american);
        }
      }

      function americanFromDecimal(decimal) {
        if (!isFinite(decimal) || decimal <= 1) return null;
        const profitPer1 = decimal - 1;
        if (profitPer1 >= 1) {
          return Math.round(profitPer1 * 100);
        } else {
          return -Math.round(100 / profitPer1);
        }
      }

      function fractionalFromDecimal(decimal) {
        if (!isFinite(decimal) || decimal <= 1) return null;
        const profit = decimal - 1;
        // approximate nice fraction using denominator up to 100
        const maxDen = 100;
        let bestNum = Math.round(profit * maxDen);
        let bestDen = maxDen;
        // reduce fraction
        function gcd(a, b) {
          a = Math.abs(a);
          b = Math.abs(b);
          while (b) {
            const t = b;
            b = a % b;
            a = t;
          }
          return a || 1;
        }
        const g = gcd(bestNum, bestDen);
        bestNum / g;
        bestDen / g;
        return bestNum + "/" + bestDen;
      }

      function decimalFromInput(format, valueStr) {
        valueStr = (valueStr || "").trim();
        if (!valueStr) return null;
        if (format === "american") {
          const val = parseFloat(valueStr);
          return decimalFromAmerican(val);
        } else if (format === "decimal") {
          const dec = parseFloat(valueStr);
          if (!isFinite(dec) || dec <= 1) return null;
          return dec;
        } else if (format === "fractional") {
          const ratio = parseFractional(valueStr);
          if (ratio === null) return null;
          return 1 + ratio;
        }
        return null;
      }

      function formatPercent(p) {
        if (!isFinite(p)) return "–";
        return (p * 100).toFixed(2).replace(/\.00$/, "") + "%";
      }

      function formatMoney(x) {
        if (!isFinite(x)) return "–";
        const abs = Math.abs(x);
        const decimals = abs < 1 ? 2 : abs < 1000 ? 2 : 2;
        return "$" + x.toFixed(decimals).replace(/\B(?=(\d{3})+(?!\d))/g, ",");
      }

      // Single bet logic
      (function () {
        const formatEl = document.getElementById("single-odds-format");
        const oddsInputEl = document.getElementById("single-odds-input");
        const stakeEl = document.getElementById("single-stake");
        const errorEl = document.getElementById("single-error");

        const impliedProbEl = document.getElementById("single-implied-prob");
        const fairProbRowEl = document.getElementById("single-fair-prob-row");
        const fairProbEl = document.getElementById("single-fair-prob");
        const totalReturnEl = document.getElementById("single-total-return");
        const profitEl = document.getElementById("single-profit");
        const breakevenEl = document.getElementById("single-breakeven");

        const americanOutEl = document.getElementById("single-american-out");
        const decimalOutEl = document.getElementById("single-decimal-out");
        const fractionalOutEl = document.getElementById(
          "single-fractional-out"
        );

        const vigToggleEl = document.getElementById("single-vig-toggle");
        const vigContainerEl = document.getElementById("single-vig-container");
        const vigPercentEl = document.getElementById("single-vig-percent");

        function updateSingle() {
          errorEl.classList.add("hidden");
          oddsInputEl.classList.remove("input-error");
          fairProbRowEl.style.display = "none";

          const format = formatEl.value;
          const oddsStr = oddsInputEl.value;
          const stake = parseFloat(stakeEl.value);

          const decimal = decimalFromInput(format, oddsStr);
          if (!decimal) {
            impliedProbEl.textContent = "–";
            totalReturnEl.textContent = "–";
            profitEl.textContent = "–";
            breakevenEl.textContent = "–";
            americanOutEl.textContent = "–";
            decimalOutEl.textContent = "–";
            fractionalOutEl.textContent = "–";
            if (oddsStr.trim() !== "") {
              errorEl.textContent =
                "Please enter valid odds for the selected format.";
              errorEl.classList.remove("hidden");
              oddsInputEl.classList.add("input-error");
            }
            return;
          }

          const impliedProb = 1 / decimal;
          impliedProbEl.textContent = formatPercent(impliedProb);
          breakevenEl.textContent = formatPercent(impliedProb);

          if (isFinite(stake) && stake > 0) {
            const totalReturn = stake * decimal;
            const profit = totalReturn - stake;
            totalReturnEl.textContent = formatMoney(totalReturn);
            profitEl.textContent = formatMoney(profit);
          } else {
            totalReturnEl.textContent = "–";
            profitEl.textContent = "–";
          }

          decimalOutEl.textContent = decimal.toFixed(3).replace(/\.000$/, "");
          const american = americanFromDecimal(decimal);
          americanOutEl.textContent = american
            ? american > 0
              ? "+" + american
              : american.toString()
            : "–";
          const frac = fractionalFromDecimal(decimal);
          fractionalOutEl.textContent = frac || "–";

          // Vig handling: simple adjustment using provided margin
          if (vigToggleEl.checked) {
            const vig = parseFloat(vigPercentEl.value);
            if (isFinite(vig) && vig > 0) {
              const fairProb = impliedProb / (1 + vig / 100);
              fairProbEl.textContent = formatPercent(fairProb);
              fairProbRowEl.style.display = "block";
            }
          }
        }

        formatEl.addEventListener("change", updateSingle);
        oddsInputEl.addEventListener("input", updateSingle);
        stakeEl.addEventListener("input", updateSingle);
        vigToggleEl.addEventListener("change", () => {
          vigContainerEl.classList.toggle("hidden", !vigToggleEl.checked);
          updateSingle();
        });
        vigPercentEl.addEventListener("input", updateSingle);

        // Initialize with a default example
        oddsInputEl.value = "+150";
        updateSingle();
      })();

      // Parlay logic
      (function () {
        const legsContainer = document.getElementById("parlay-legs");
        const addLegBtn = document.getElementById("parlay-add-leg");
        const stakeEl = document.getElementById("parlay-stake");
        const errorEl = document.getElementById("parlay-error");

        const decimalOutEl = document.getElementById("parlay-decimal-out");
        const americanOutEl = document.getElementById("parlay-american-out");
        const fractionalOutEl = document.getElementById(
          "parlay-fractional-out"
        );
        const impliedProbEl = document.getElementById("parlay-implied-prob");
        const totalReturnEl = document.getElementById("parlay-total-return");
        const profitEl = document.getElementById("parlay-profit");

        let legIdCounter = 0;

        function createLegRow() {
          const id = ++legIdCounter;
          const row = document.createElement("div");
          row.className = "grid grid-cols-12 gap-2 items-center";
          row.dataset.legId = id;

          row.innerHTML = `
        <div class="col-span-4">
          <label class="sr-only" for="parlay-format-${id}">Format</label>
          <select id="parlay-format-${id}" class="w-full border border-gray-300 rounded-md px-2 py-1 text-xs focus:outline-none focus:ring-2 focus:ring-blue-500">
            <option value="american">American</option>
            <option value="decimal">Decimal</option>
            <option value="fractional">Fractional</option>
          </select>
        </div>
        <div class="col-span-6">
          <label class="sr-only" for="parlay-odds-${id}">Odds</label>
          <input id="parlay-odds-${id}" type="text" placeholder="+150" class="w-full border border-gray-300 rounded-md px-2 py-1 text-xs focus:outline-none focus:ring-2 focus:ring-blue-500" />
        </div>
        <div class="col-span-2 flex justify-end">
          <button type="button" class="text-xs text-red-600 hover:text-red-800 remove-leg-btn">Remove</button>
        </div>
      `;

          legsContainer.appendChild(row);

          const formatEl = row.querySelector("select");
          const oddsEl = row.querySelector("input");
          const removeBtn = row.querySelector(".remove-leg-btn");

          formatEl.addEventListener("change", updateParlay);
          oddsEl.addEventListener("input", updateParlay);
          removeBtn.addEventListener("click", () => {
            row.remove();
            updateParlay();
          });

          return row;
        }

        function updateParlay() {
          errorEl.classList.add("hidden");

          const rows = Array.from(
            legsContainer.querySelectorAll("[data-leg-id]")
          );
          if (rows.length === 0) {
            decimalOutEl.textContent = "–";
            americanOutEl.textContent = "–";
            fractionalOutEl.textContent = "–";
            impliedProbEl.textContent = "–";
            totalReturnEl.textContent = "–";
            profitEl.textContent = "–";
            return;
          }

          let combinedDecimal = 1;
          let validLegs = 0;
          for (const row of rows) {
            const formatEl = row.querySelector("select");
            const oddsEl = row.querySelector("input");
            oddsEl.classList.remove("input-error");

            const dec = decimalFromInput(formatEl.value, oddsEl.value);
            if (!dec) {
              if (oddsEl.value.trim() !== "") {
                oddsEl.classList.add("input-error");
                errorEl.textContent =
                  "One or more legs have invalid odds. Please check and try again.";
                errorEl.classList.remove("hidden");
              }
              return;
            }
            combinedDecimal *= dec;
            validLegs++;
          }

          if (validLegs === 0) {
            decimalOutEl.textContent = "–";
            americanOutEl.textContent = "–";
            fractionalOutEl.textContent = "–";
            impliedProbEl.textContent = "–";
            totalReturnEl.textContent = "–";
            profitEl.textContent = "–";
            return;
          }

          decimalOutEl.textContent = combinedDecimal
            .toFixed(3)
            .replace(/\.000$/, "");
          const american = americanFromDecimal(combinedDecimal);
          americanOutEl.textContent = american
            ? american > 0
              ? "+" + american
              : american.toString()
            : "–";
          const frac = fractionalFromDecimal(combinedDecimal);
          fractionalOutEl.textContent = frac || "–";

          const impliedProb = 1 / combinedDecimal;
          impliedProbEl.textContent = formatPercent(impliedProb);

          const stake = parseFloat(stakeEl.value);
          if (isFinite(stake) && stake > 0) {
            const totalReturn = stake * combinedDecimal;
            const profit = totalReturn - stake;
            totalReturnEl.textContent = formatMoney(totalReturn);
            profitEl.textContent = formatMoney(profit);
          } else {
            totalReturnEl.textContent = "–";
            profitEl.textContent = "–";
          }
        }

        addLegBtn.addEventListener("click", () => {
          const row = createLegRow();
          // Provide a simple default for new legs
          const oddsInput = row.querySelector("input");
          if (legsContainer.children.length === 1) {
            oddsInput.value = "-110";
          }
          updateParlay();
        });

        stakeEl.addEventListener("input", updateParlay);

        // Initialize with two common -110 legs
        const leg1 = createLegRow();
        leg1.querySelector("input").value = "-110";
        const leg2 = createLegRow();
        leg2.querySelector("input").value = "-110";
        updateParlay();
      })();

      // Tabs
      (function () {
        const tabSingle = document.getElementById("tab-single");
        const tabParlay = document.getElementById("tab-parlay");
        const panelSingle = document.getElementById("panel-single");
        const panelParlay = document.getElementById("panel-parlay");

        function activate(tab) {
          if (tab === "single") {
            tabSingle.classList.add("border-blue-600", "text-blue-600");
            tabSingle.classList.remove("border-transparent", "text-gray-500");
            tabParlay.classList.remove("border-blue-600", "text-blue-600");
            tabParlay.classList.add("border-transparent", "text-gray-500");
            panelSingle.classList.remove("hidden");
            panelParlay.classList.add("hidden");
          } else {
            tabParlay.classList.add("border-blue-600", "text-blue-600");
            tabParlay.classList.remove("border-transparent", "text-gray-500");
            tabSingle.classList.remove("border-blue-600", "text-blue-600");
            tabSingle.classList.add("border-transparent", "text-gray-500");
            panelParlay.classList.remove("hidden");
            panelSingle.classList.add("hidden");
          }
        }

        tabSingle.addEventListener("click", () => activate("single"));
        tabParlay.addEventListener("click", () => activate("parlay"));
      })();

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
