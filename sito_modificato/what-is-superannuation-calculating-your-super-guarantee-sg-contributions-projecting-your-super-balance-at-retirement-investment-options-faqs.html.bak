    <script>
      // Utility: format currency
      function formatAUD(value) {
        if (!isFinite(value)) return "A$0";
        return (
          "A$" + value.toLocaleString("en-AU", { maximumFractionDigits: 0 })
        );
      }

      // Tab switching
      const tabProjection = document.getElementById("tab-projection");
      const tabSG = document.getElementById("tab-sg");
      const projectionPanel = document.getElementById("projection-panel");
      const sgPanel = document.getElementById("sg-panel");

      tabProjection.addEventListener("click", () => {
        tabProjection.classList.add(
          "border-blue-600",
          "text-blue-600",
          "font-semibold"
        );
        tabProjection.classList.remove("text-gray-600");
        tabSG.classList.remove(
          "border-blue-600",
          "text-blue-600",
          "font-semibold"
        );
        tabSG.classList.add("text-gray-600");
        projectionPanel.classList.remove("hidden");
        sgPanel.classList.add("hidden");
      });

      tabSG.addEventListener("click", () => {
        tabSG.classList.add(
          "border-blue-600",
          "text-blue-600",
          "font-semibold"
        );
        tabSG.classList.remove("text-gray-600");
        tabProjection.classList.remove(
          "border-blue-600",
          "text-blue-600",
          "font-semibold"
        );
        tabProjection.classList.add("text-gray-600");
        sgPanel.classList.remove("hidden");
        projectionPanel.classList.add("hidden");
      });

      // Range display sync
      function bindRange(idRange, idValue) {
        const r = document.getElementById(idRange);
        const v = document.getElementById(idValue);
        const update = () => {
          v.textContent = r.value;
        };
        r.addEventListener("input", update);
        update();
      }
      bindRange("returnRate", "returnRateValue");
      bindRange("feeRate", "feeRateValue");
      bindRange("wageGrowth", "wageGrowthValue");

      // Projection calculation
      document
        .getElementById("calculateProjection")
        .addEventListener("click", () => {
          const age = parseFloat(document.getElementById("age").value);
          const retirementAge = parseFloat(
            document.getElementById("retirementAge").value
          );
          const currentBalance =
            parseFloat(document.getElementById("currentBalance").value) || 0;
          const salaryStart =
            parseFloat(document.getElementById("salary").value) || 0;
          const sgRate =
            (parseFloat(document.getElementById("sgRate").value) || 0) / 100;
          const salarySacrifice =
            parseFloat(document.getElementById("salarySacrifice").value) || 0;
          const afterTax =
            parseFloat(document.getElementById("afterTax").value) || 0;
          const returnRate =
            (parseFloat(document.getElementById("returnRate").value) || 0) /
            100;
          const feeRate =
            (parseFloat(document.getElementById("feeRate").value) || 0) / 100;
          const wageGrowth =
            (parseFloat(document.getElementById("wageGrowth").value) || 0) /
            100;

          if (
            !isFinite(age) ||
            !isFinite(retirementAge) ||
            retirementAge <= age
          ) {
            alert("Please ensure retirement age is greater than current age.");
            return;
          }

          const years = Math.floor(retirementAge - age);
          let balance = currentBalance;
          let salary = salaryStart;
          let totalContrib = 0;
          const rows = [];
          for (let i = 0; i < years; i++) {
            const thisAge = age + i;
            const startBalance = balance;

            const employerSG = salary * sgRate;
            const contrib = employerSG + salarySacrifice + afterTax;
            totalContrib += contrib;

            const netRate = returnRate - feeRate;
            const endBalance = (startBalance + contrib) * (1 + netRate);

            const netReturn = endBalance - startBalance - contrib;

            rows.push({
              age: thisAge,
              startBalance,
              contrib,
              netReturn,
              endBalance,
            });

            balance = endBalance;
            salary = salary * (1 + wageGrowth);
          }

          const finalBalance = balance;
          const investEarnings = finalBalance - currentBalance - totalContrib;

          document.getElementById("proj-final-balance").textContent =
            formatAUD(finalBalance);
          document.getElementById("proj-total-contrib").textContent =
            formatAUD(totalContrib);
          document.getElementById("proj-invest-earnings").textContent =
            formatAUD(investEarnings);

          const projSummary = document.getElementById("proj-summary");
          projSummary.textContent =
            `Over approximately ${years} years, your super could grow from ${formatAUD(
              currentBalance
            )} to ` +
            `${formatAUD(finalBalance)} assuming an average return of ${(
              returnRate * 100
            ).toFixed(1)}% p.a., ` +
            `fees of ${(feeRate * 100).toFixed(
              1
            )}% p.a., and salary growth of ${(wageGrowth * 100).toFixed(
              1
            )}% p.a. ` +
            `About ${formatAUD(
              investEarnings
            )} of this comes from investment earnings.`;

          const tbody = document.getElementById("projection-table-body");
          tbody.innerHTML = "";
          rows.forEach((r) => {
            const tr = document.createElement("tr");
            tr.innerHTML = `
        <td class="px-2 py-1 border-b">${r.age}</td>
        <td class="px-2 py-1 border-b text-right">${formatAUD(
          r.startBalance
        )}</td>
        <td class="px-2 py-1 border-b text-right">${formatAUD(r.contrib)}</td>
        <td class="px-2 py-1 border-b text-right">${formatAUD(r.netReturn)}</td>
        <td class="px-2 py-1 border-b text-right">${formatAUD(
          r.endBalance
        )}</td>
      `;
            tbody.appendChild(tr);
          });

          document
            .getElementById("projection-results")
            .classList.remove("hidden");
        });

      document
        .getElementById("resetProjection")
        .addEventListener("click", () => {
          document.getElementById("age").value = 35;
          document.getElementById("retirementAge").value = 67;
          document.getElementById("currentBalance").value = 80000;
          document.getElementById("salary").value = 90000;
          document.getElementById("sgRate").value = 11.5;
          document.getElementById("salarySacrifice").value = 0;
          document.getElementById("afterTax").value = 0;
          document.getElementById("returnRate").value = 6;
          document.getElementById("feeRate").value = 0.8;
          document.getElementById("wageGrowth").value = 2.5;
          bindRange("returnRate", "returnRateValue");
          bindRange("feeRate", "feeRateValue");
          bindRange("wageGrowth", "wageGrowthValue");
          document.getElementById("projection-results").classList.add("hidden");
        });

      // SG calculator
      document.getElementById("calculateSG").addEventListener("click", () => {
        const ote = parseFloat(document.getElementById("sgSalary").value) || 0;
        const sgRate =
          (parseFloat(document.getElementById("sgRateInput").value) || 0) / 100;
        const freq = document.getElementById("sgFrequency").value;

        if (ote <= 0 || sgRate <= 0) {
          alert("Please enter a positive OTE amount and SG rate.");
          return;
        }

        const perPeriod = ote * sgRate;
        let periodsPerYear = 1;
        if (freq === "weekly") periodsPerYear = 52;
        else if (freq === "fortnightly") periodsPerYear = 26;
        else if (freq === "monthly") periodsPerYear = 12;

        const perYear = perPeriod * periodsPerYear;
        const tenYearsNoGrowth = perYear * 10;

        // 10-year with 5% p.a. growth, contributions at year-end
        const r = 0.05;
        const n = 10;
        const tenYearsGrowth = perYear * ((Math.pow(1 + r, n) - 1) / r);

        document.getElementById("sg-per-period").textContent =
          formatAUD(perPeriod);
        document.getElementById("sg-per-year").textContent = formatAUD(perYear);
        document.getElementById("sg-ten-years").textContent =
          formatAUD(tenYearsNoGrowth);
        document.getElementById("sg-ten-years-growth").textContent =
          formatAUD(tenYearsGrowth);

        document.getElementById("sg-results").classList.remove("hidden");
      });

      document.getElementById("resetSG").addEventListener("click", () => {
        document.getElementById("sgSalary").value = 2000;
        document.getElementById("sgRateInput").value = 11.5;
        document.getElementById("sgFrequency").value = "monthly";
        document.getElementById("sg-results").classList.add("hidden");
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
