    <script>
      // Density calculator logic
      (function () {
        const modeTabs = document.querySelectorAll("[data-mode]");
        const massGroup = document.getElementById("mass-input-group");
        const volumeGroup = document.getElementById("volume-input-group");
        const densityGroup = document.getElementById("density-input-group");

        const massValue = document.getElementById("mass-value");
        const massUnit = document.getElementById("mass-unit");
        const volumeValue = document.getElementById("volume-value");
        const volumeUnit = document.getElementById("volume-unit");
        const densityValue = document.getElementById("density-value");
        const densityUnit = document.getElementById("density-unit");

        const calcBtn = document.getElementById("calculate-btn");
        const resetBtn = document.getElementById("reset-btn");
        const errorEl = document.getElementById("error-message");
        const resultCard = document.getElementById("result-card");
        const resultTitle = document.getElementById("result-title");
        const resultMain = document.getElementById("result-main");
        const resultAlt = document.getElementById("result-alt");

        let currentMode = "density"; // density, mass, volume

        function setMode(mode) {
          currentMode = mode;
          modeTabs.forEach((tab) => {
            if (tab.dataset.mode === mode) {
              tab.classList.add("tab-active");
              tab.setAttribute("aria-selected", "true");
            } else {
              tab.classList.remove("tab-active");
              tab.setAttribute("aria-selected", "false");
            }
          });

          // Show/hide input groups
          if (mode === "density") {
            massGroup.classList.remove("hidden");
            volumeGroup.classList.remove("hidden");
            densityGroup.classList.add("hidden");
          } else if (mode === "mass") {
            massGroup.classList.add("hidden");
            volumeGroup.classList.remove("hidden");
            densityGroup.classList.remove("hidden");
          } else if (mode === "volume") {
            massGroup.classList.remove("hidden");
            volumeGroup.classList.add("hidden");
            densityGroup.classList.remove("hidden");
          }

          clearOutputs();
        }

        function clearOutputs() {
          errorEl.textContent = "";
          resultCard.classList.add("hidden");
          resultMain.textContent = "";
          resultAlt.textContent = "";
        }

        modeTabs.forEach((tab) => {
          tab.addEventListener("click", () => setMode(tab.dataset.mode));
        });

        // Unit conversion helpers
        function massToKg(value, unit) {
          const v = Number(value);
          switch (unit) {
            case "kg":
              return v;
            case "g":
              return v / 1000;
            case "mg":
              return v / 1e6;
            case "lb":
              return v * 0.45359237;
            case "oz":
              return v * 0.028349523125;
            default:
              return NaN;
          }
        }

        function volumeToM3(value, unit) {
          const v = Number(value);
          switch (unit) {
            case "m3":
              return v;
            case "cm3":
              return v * 1e-6;
            case "mm3":
              return v * 1e-9;
            case "L":
              return v / 1000;
            case "mL":
              return v / 1e6;
            case "ft3":
              return v * 0.028316846592;
            case "in3":
              return v * 1.6387064e-5;
            case "gal_us":
              return v * 0.003785411784;
            case "gal_uk":
              return v * 0.00454609;
            default:
              return NaN;
          }
        }

        function densityToKgM3(value, unit) {
          const v = Number(value);
          switch (unit) {
            case "kg_m3":
              return v;
            case "g_cm3":
              return v * 1000; // 1 g/cm3 = 1000 kg/m3
            case "g_mL":
              return v * 1000; // same as g/cm3
            case "lb_ft3":
              return v * 16.01846337;
            case "lb_in3":
              return v * 27679.90471;
            case "lb_gal_us":
              return v * 119.8264273;
            default:
              return NaN;
          }
        }

        function kgM3ToUnits(rho) {
          return {
            kg_m3: rho,
            g_cm3: rho / 1000,
            lb_ft3: rho / 16.01846337,
          };
        }

        function formatNumber(x, digits = 6) {
          if (!isFinite(x)) return "—";
          const abs = Math.abs(x);
          if (abs !== 0 && (abs < 1e-3 || abs >= 1e5)) {
            return x.toExponential(3);
          }
          return Number(x.toPrecision(digits)).toString();
        }

        calcBtn.addEventListener("click", () => {
          clearOutputs();

          try {
            if (currentMode === "density") {
              const mVal = parseFloat(massValue.value);
              const vVal = parseFloat(volumeValue.value);
              if (isNaN(mVal) || isNaN(vVal)) {
                errorEl.textContent = "Enter both mass and volume.";
                return;
              }
              if (vVal === 0) {
                errorEl.textContent = "Volume cannot be zero.";
                return;
              }
              const mKg = massToKg(mVal, massUnit.value);
              const vM3 = volumeToM3(vVal, volumeUnit.value);
              if (!isFinite(mKg) || !isFinite(vM3) || vM3 === 0) {
                errorEl.textContent = "Invalid units or values.";
                return;
              }
              const rho = mKg / vM3; // kg/m3
              const conv = kgM3ToUnits(rho);

              resultTitle.textContent = "Density result";
              resultMain.textContent = `ρ = ${formatNumber(rho)} kg/m³`;
              resultAlt.textContent = `≈ ${formatNumber(
                conv.g_cm3
              )} g/cm³  |  ≈ ${formatNumber(conv.lb_ft3)} lb/ft³`;
              resultCard.classList.remove("hidden");
            } else if (currentMode === "mass") {
              const rhoVal = parseFloat(densityValue.value);
              const vVal = parseFloat(volumeValue.value);
              if (isNaN(rhoVal) || isNaN(vVal)) {
                errorEl.textContent = "Enter both density and volume.";
                return;
              }
              const rho = densityToKgM3(rhoVal, densityUnit.value);
              const vM3 = volumeToM3(vVal, volumeUnit.value);
              if (!isFinite(rho) || !isFinite(vM3)) {
                errorEl.textContent = "Invalid units or values.";
                return;
              }
              const mKg = rho * vM3;
              const mG = mKg * 1000;
              const mLb = mKg / 0.45359237;

              resultTitle.textContent = "Mass result";
              resultMain.textContent = `m = ${formatNumber(mKg)} kg`;
              resultAlt.textContent = `≈ ${formatNumber(
                mG
              )} g  |  ≈ ${formatNumber(mLb)} lb`;
              resultCard.classList.remove("hidden");
            } else if (currentMode === "volume") {
              const mVal = parseFloat(massValue.value);
              const rhoVal = parseFloat(densityValue.value);
              if (isNaN(mVal) || isNaN(rhoVal)) {
                errorEl.textContent = "Enter both mass and density.";
                return;
              }
              const mKg = massToKg(mVal, massUnit.value);
              const rho = densityToKgM3(rhoVal, densityUnit.value);
              if (!isFinite(mKg) || !isFinite(rho) || rho === 0) {
                errorEl.textContent = "Invalid units or values.";
                return;
              }
              const vM3 = mKg / rho;
              const vL = vM3 * 1000;
              const vCm3 = vM3 * 1e6;

              resultTitle.textContent = "Volume result";
              resultMain.textContent = `V = ${formatNumber(vM3)} m³`;
              resultAlt.textContent = `≈ ${formatNumber(
                vL
              )} L  |  ≈ ${formatNumber(vCm3)} cm³`;
              resultCard.classList.remove("hidden");
            }
          } catch (e) {
            errorEl.textContent =
              "An unexpected error occurred. Please check your inputs.";
          }
        });

        resetBtn.addEventListener("click", () => {
          massValue.value = "";
          volumeValue.value = "";
          densityValue.value = "";
          massUnit.value = "kg";
          volumeUnit.value = "m3";
          densityUnit.value = "kg_m3";
          clearOutputs();
        });

        // Initialize default mode
        setMode("density");
      })();

      // Simple FAQ toggle (in addition to global script_faq if present)
      document.addEventListener("DOMContentLoaded", function () {
        const faqQs = document.querySelectorAll("#faq-container .faq-question");
        faqQs.forEach((btn) => {
          btn.addEventListener("click", () => {
            const answer = btn.parentElement.querySelector(".faq-answer");
            const icon = btn.querySelector("span:last-child");
            const isHidden = answer.classList.contains("hidden");
            document
              .querySelectorAll("#faq-container .faq-answer")
              .forEach((a) => a.classList.add("hidden"));
            document
              .querySelectorAll("#faq-container .faq-question span:last-child")
              .forEach((i) => (i.textContent = "+"));
            if (isHidden) {
              answer.classList.remove("hidden");
              icon.textContent = "−";
            }
          });
        });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
