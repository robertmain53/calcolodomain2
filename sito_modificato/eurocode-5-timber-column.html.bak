    <script>
      // Strength class presets (EN 338 typical values)
      const strengthClasses = {
        C16: { fc0k: 16, fm0k: 16, E0mean: 8000, Gmean: 530 },
        C24: { fc0k: 21, fm0k: 24, E0mean: 11000, Gmean: 690 },
        C30: { fc0k: 24, fm0k: 30, E0mean: 12000, Gmean: 750 },
        GL24h: { fc0k: 24, fm0k: 24, E0mean: 11500, Gmean: 650 },
        GL28h: { fc0k: 26, fm0k: 28, E0mean: 12600, Gmean: 720 },
      };

      function applyStrengthClass() {
        const sel = document.getElementById("strength-class").value;
        if (sel === "custom") return;
        const data = strengthClasses[sel];
        if (!data) return;
        document.getElementById("fc0k").value = data.fc0k;
        document.getElementById("fm0k").value = data.fm0k;
        document.getElementById("e0mean").value = data.E0mean;
        document.getElementById("gmean").value = data.Gmean;
      }

      document
        .getElementById("strength-class")
        .addEventListener("change", applyStrengthClass);

      function formatNumber(value, decimals) {
        if (!isFinite(value)) return "–";
        return Number(value).toFixed(decimals);
      }

      function calculateEurocode5TimberColumn() {
        const statusEl = document.getElementById("calc-status");
        statusEl.textContent = "Calculating...";

        const b = parseFloat(document.getElementById("width").value); // mm
        const h = parseFloat(document.getElementById("depth").value); // mm
        const L = parseFloat(document.getElementById("length").value); // mm
        const ky = parseFloat(document.getElementById("ky").value);
        const kz = parseFloat(document.getElementById("kz").value);

        const fc0k = parseFloat(document.getElementById("fc0k").value); // N/mm2
        const fm0k = parseFloat(document.getElementById("fm0k").value);
        const E0mean = parseFloat(document.getElementById("e0mean").value);
        const kmod = parseFloat(document.getElementById("kmod").value);
        const gammaM = parseFloat(document.getElementById("gamma-m").value);
        const kcrit = parseFloat(document.getElementById("kcrit").value);

        const Nd = parseFloat(document.getElementById("nd").value) * 1000; // kN -> N
        const Myd = parseFloat(document.getElementById("mdz").value) * 1e6; // kNm -> Nmm (major axis)
        const Mzd = parseFloat(document.getElementById("mdy").value) * 1e6; // kNm -> Nmm (minor axis)

        if (
          [b, h, L, ky, kz, fc0k, fm0k, E0mean, kmod, gammaM, kcrit].some(
            (v) => !isFinite(v) || v <= 0
          )
        ) {
          statusEl.textContent =
            "Please enter valid positive values for all required inputs.";
          return;
        }

        // Section properties
        const A = b * h; // mm2
        const Iy = (b * Math.pow(h, 3)) / 12;
        const Iz = (h * Math.pow(b, 3)) / 12;
        const iy = Math.sqrt(Iy / A);
        const iz = Math.sqrt(Iz / A);

        // Effective lengths
        const lcry = ky * L;
        const lcrz = kz * L;

        // Non-dimensional slenderness (simplified EC5 expression)
        const pi2 = Math.PI * Math.PI;
        const lambdaY = (lcry / iy) * Math.sqrt(fc0k / (pi2 * E0mean));
        const lambdaZ = (lcrz / iz) * Math.sqrt(fc0k / (pi2 * E0mean));

        function chiFromLambda(lambda) {
          if (lambda <= 0) return 1.0;
          const phi = 0.5 * (1 + kcrit * (lambda - 0.3) + lambda * lambda);
          const disc = phi * phi - lambda * lambda;
          if (disc <= 0) return 1.0;
          const chi = 1 / (phi + Math.sqrt(disc));
          return Math.min(1, Math.max(0, chi));
        }

        const chiY = chiFromLambda(lambdaY);
        const chiZ = chiFromLambda(lambdaZ);

        // Design strengths
        const fcd = (kmod * fc0k) / gammaM;
        const fmd = (kmod * fm0k) / gammaM;

        // Compression resistances (N)
        const NcRdy = chiY * A * fcd;
        const NcRdz = chiZ * A * fcd;
        const NcRd = Math.min(NcRdy, NcRdz);

        // Bending resistances (Nmm)
        const Wy = Iy / (h / 2);
        const Wz = Iz / (b / 2);
        const MyRd = Wy * fmd;
        const MzRd = Wz * fmd;

        // Utilizations
        const etaN = NcRd > 0 ? Nd / NcRd : NaN;
        const etaY = NcRdy > 0 && MyRd > 0 ? Nd / NcRdy + Myd / MyRd : NaN;
        const etaZ = NcRdz > 0 && MzRd > 0 ? Nd / NcRdz + Mzd / MzRd : NaN;

        // Update UI
        document.getElementById("res-area").textContent = formatNumber(A, 0);
        document.getElementById("res-iy").textContent = formatNumber(Iy, 0);
        document.getElementById("res-iz").textContent = formatNumber(Iz, 0);
        document.getElementById("res-iyr").textContent = formatNumber(iy, 1);
        document.getElementById("res-izr").textContent = formatNumber(iz, 1);

        document.getElementById("res-lcry").textContent = formatNumber(lcry, 0);
        document.getElementById("res-lcrz").textContent = formatNumber(lcrz, 0);
        document.getElementById("res-lambday").textContent = formatNumber(
          lambdaY,
          3
        );
        document.getElementById("res-lambdaz").textContent = formatNumber(
          lambdaZ,
          3
        );
        document.getElementById("res-chiy").textContent = formatNumber(chiY, 3);
        document.getElementById("res-chiz").textContent = formatNumber(chiZ, 3);

        document.getElementById("res-fcd").textContent = formatNumber(fcd, 2);
        document.getElementById("res-fmd").textContent = formatNumber(fmd, 2);
        document.getElementById("res-ncrd").textContent = isFinite(NcRd)
          ? formatNumber(NcRd / 1000, 1)
          : "–";

        document.getElementById("res-eta-n").textContent = isFinite(etaN)
          ? formatNumber(etaN, 2)
          : "–";
        document.getElementById("res-eta-y").textContent = isFinite(etaY)
          ? formatNumber(etaY, 2)
          : "–";
        document.getElementById("res-eta-z").textContent = isFinite(etaZ)
          ? formatNumber(etaZ, 2)
          : "–";

        const badge = document.getElementById("res-status-badge");
        badge.classList.remove("hidden", "result-badge-ok", "result-badge-ng");

        const maxEta = Math.max(
          isFinite(etaN) ? etaN : 0,
          isFinite(etaY) ? etaY : 0,
          isFinite(etaZ) ? etaZ : 0
        );

        if (!isFinite(maxEta)) {
          badge.textContent = "No valid utilization could be computed";
          badge.classList.add("result-badge-ng");
        } else if (maxEta <= 1.0) {
          badge.textContent =
            "OK – Governing utilization η = " + formatNumber(maxEta, 2);
          badge.classList.add("result-badge-ok");
        } else {
          badge.textContent =
            "NOT OK – Governing utilization η = " + formatNumber(maxEta, 2);
          badge.classList.add("result-badge-ng");
        }

        statusEl.textContent = "Calculation completed.";
      }

      document
        .getElementById("calculate-btn")
        .addEventListener("click", calculateEurocode5TimberColumn);

      document.getElementById("reset-btn").addEventListener("click", () => {
        document.getElementById("strength-class").value = "C24";
        document.getElementById("gamma-m").value = 1.3;
        document.getElementById("kmod").value = 0.8;
        document.getElementById("kcrit").value = 0.2;
        document.getElementById("width").value = 140;
        document.getElementById("depth").value = 240;
        document.getElementById("length").value = 3000;
        document.getElementById("ky").value = 1.0;
        document.getElementById("kz").value = 0.7;
        document.getElementById("nd").value = 400;
        document.getElementById("mdz").value = 20;
        document.getElementById("mdy").value = 0;
        applyStrengthClass();
        document.getElementById("calc-status").textContent = "";
        document.getElementById("res-status-badge").classList.add("hidden");
        [
          "res-area",
          "res-iy",
          "res-iz",
          "res-iyr",
          "res-izr",
          "res-lcry",
          "res-lcrz",
          "res-lambday",
          "res-lambdaz",
          "res-chiy",
          "res-chiz",
          "res-fcd",
          "res-fmd",
          "res-ncrd",
          "res-eta-n",
          "res-eta-y",
          "res-eta-z",
        ].forEach((id) => {
          document.getElementById(id).textContent = "–";
        });
      });

      // Initial preset
      applyStrengthClass();
    </script>

