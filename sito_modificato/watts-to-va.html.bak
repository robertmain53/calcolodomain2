    <script>
      // Helper: get selected system type
      function getSystemType() {
        const radios = document.querySelectorAll('input[name="systemType"]');
        for (const r of radios) {
          if (r.checked) return r.value; // 'single' or 'three'
        }
        return "single";
      }

      function setMode(mode) {
        const btnWtoVA = document.getElementById("mode-w-to-va");
        const btnVAtoW = document.getElementById("mode-va-to-w");
        const inputWGroup = document.getElementById("input-watts-group");
        const inputVAGroup = document.getElementById("input-va-group");
        const errorMsg = document.getElementById("error-msg");
        const results = document.getElementById("results-container");

        errorMsg.textContent = "";
        results.classList.add("hidden");

        if (mode === "w-to-va") {
          btnWtoVA.classList.add("bg-blue-600", "text-white");
          btnWtoVA.classList.remove(
            "bg-white",
            "text-blue-600",
            "border",
            "border-blue-600"
          );
          btnVAtoW.classList.remove("bg-blue-600", "text-white");
          btnVAtoW.classList.add(
            "bg-white",
            "text-blue-600",
            "border",
            "border-blue-600"
          );
          inputWGroup.classList.remove("hidden");
          inputVAGroup.classList.add("hidden");
          document.getElementById("input-va").value = "";
        } else {
          btnVAtoW.classList.add("bg-blue-600", "text-white");
          btnVAtoW.classList.remove(
            "bg-white",
            "text-blue-600",
            "border",
            "border-blue-600"
          );
          btnWtoVA.classList.remove("bg-blue-600", "text-white");
          btnWtoVA.classList.add(
            "bg-white",
            "text-blue-600",
            "border",
            "border-blue-600"
          );
          inputVAGroup.classList.remove("hidden");
          inputWGroup.classList.add("hidden");
          document.getElementById("input-watts").value = "";
        }
        document.getElementById("watts-va-form").dataset.mode = mode;
      }

      document
        .getElementById("mode-w-to-va")
        .addEventListener("click", () => setMode("w-to-va"));
      document
        .getElementById("mode-va-to-w")
        .addEventListener("click", () => setMode("va-to-w"));

      document.getElementById("btn-calc").addEventListener("click", () => {
        const mode =
          document.getElementById("watts-va-form").dataset.mode || "w-to-va";
        const pfInput = document.getElementById("power-factor");
        const wattsInput = document.getElementById("input-watts");
        const vaInput = document.getElementById("input-va");
        const errorMsg = document.getElementById("error-msg");
        const results = document.getElementById("results-container");
        const resW = document.getElementById("result-watts");
        const resVA = document.getElementById("result-va");
        const resNotes = document.getElementById("result-notes");

        errorMsg.textContent = "";
        results.classList.add("hidden");

        const pf = parseFloat(pfInput.value);
        if (isNaN(pf) || pf <= 0 || pf > 1) {
          errorMsg.textContent = "Enter a valid power factor between 0 and 1.";
          return;
        }

        let W = null,
          VA = null;

        if (mode === "w-to-va") {
          const wVal = parseFloat(wattsInput.value);
          if (isNaN(wVal) || wVal < 0) {
            errorMsg.textContent = "Enter a valid real power in watts.";
            return;
          }
          W = wVal;
          VA = W / pf;
        } else {
          const vaVal = parseFloat(vaInput.value);
          if (isNaN(vaVal) || vaVal < 0) {
            errorMsg.textContent =
              "Enter a valid apparent power in volt-amperes.";
            return;
          }
          VA = vaVal;
          W = VA * pf;
        }

        resW.textContent = W.toFixed(2) + " W";
        resVA.textContent = VA.toFixed(2) + " VA";

        const system = getSystemType();
        let sysLabel = system === "three" ? "three‑phase" : "single‑phase";
        resNotes.textContent = `Assuming ${sysLabel} AC with power factor PF = ${pf.toFixed(
          2
        )}. Relationship used: PF = W / VA.`;

        results.classList.remove("hidden");
      });

      document.getElementById("btn-clear").addEventListener("click", () => {
        document.getElementById("input-watts").value = "";
        document.getElementById("input-va").value = "";
        document.getElementById("power-factor").value = "0.8";
        document.getElementById("voltage").value = "";
        document.getElementById("current").value = "";
        document.getElementById("error-msg").textContent = "";
        document.getElementById("results-container").classList.add("hidden");
      });

      document.getElementById("btn-from-v-i").addEventListener("click", () => {
        const V = parseFloat(document.getElementById("voltage").value);
        const I = parseFloat(document.getElementById("current").value);
        const pf = parseFloat(document.getElementById("power-factor").value);
        const errorMsg = document.getElementById("error-msg");
        errorMsg.textContent = "";

        if (isNaN(V) || V <= 0 || isNaN(I) || I <= 0) {
          errorMsg.textContent = "Enter valid voltage and current values.";
          return;
        }
        if (isNaN(pf) || pf <= 0 || pf > 1) {
          errorMsg.textContent = "Enter a valid power factor between 0 and 1.";
          return;
        }

        const system = getSystemType();
        const sqrt3 = Math.sqrt(3);
        let VA, W;

        if (system === "single") {
          VA = V * I;
          W = V * I * pf;
        } else {
          VA = sqrt3 * V * I;
          W = sqrt3 * V * I * pf;
        }

        // Fill the currently active main input
        const mode =
          document.getElementById("watts-va-form").dataset.mode || "w-to-va";
        if (mode === "w-to-va") {
          document.getElementById("input-watts").value = W.toFixed(2);
        } else {
          document.getElementById("input-va").value = VA.toFixed(2);
        }

        const results = document.getElementById("results-container");
        const resW = document.getElementById("result-watts");
        const resVA = document.getElementById("result-va");
        const resNotes = document.getElementById("result-notes");

        resW.textContent = W.toFixed(2) + " W";
        resVA.textContent = VA.toFixed(2) + " VA";
        const sysLabel = system === "three" ? "three‑phase" : "single‑phase";
        resNotes.textContent = `Computed from V and A for ${sysLabel} AC: VA = V × A${
          system === "three" ? " × √3" : ""
        }, W = VA × PF (PF = ${pf.toFixed(2)}).`;
        results.classList.remove("hidden");
      });

      // Initialize default mode
      setMode("w-to-va");

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
