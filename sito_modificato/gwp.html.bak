<script>
        function calculateGWP() {
            const gasType = document.getElementById('gasType').value;
            const amount = parseFloat(document.getElementById('amount').value);
            const gwpValues = { co2: 1, ch4: 28, n2o: 265 }; // Example GWP values
            const gwpValue = gwpValues[gasType] * amount;
            document.getElementById('gwpValue').textContent = gwpValue.toFixed(2);
        }
    </script>


