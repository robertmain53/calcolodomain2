    <script>
      // Tab switching
      const tabCount = document.getElementById("tab-count");
      const tabConf = document.getElementById("tab-confluency");
      const panelCount = document.getElementById("panel-count");
      const panelConf = document.getElementById("panel-confluency");

      function setActiveTab(tab) {
        if (tab === "count") {
          tabCount.classList.add("border-blue-500", "text-blue-600");
          tabCount.classList.remove("border-transparent", "text-gray-500");
          tabConf.classList.add("border-transparent", "text-gray-500");
          tabConf.classList.remove("border-blue-500", "text-blue-600");
          panelCount.classList.remove("hidden");
          panelConf.classList.add("hidden");
        } else {
          tabConf.classList.add("border-blue-500", "text-blue-600");
          tabConf.classList.remove("border-transparent", "text-gray-500");
          tabCount.classList.add("border-transparent", "text-gray-500");
          tabCount.classList.remove("border-blue-500", "text-blue-600");
          panelConf.classList.remove("hidden");
          panelCount.classList.add("hidden");
        }
      }

      tabCount.addEventListener("click", () => setActiveTab("count"));
      tabConf.addEventListener("click", () => setActiveTab("conf"));

      // Utility: safe log with chosen base
      function logWithBase(x, base) {
        if (base === "10") return Math.log10(x);
        return Math.log(x); // natural log
      }

      // Core calculation
      function computeDoubling({ N0, Nf, t, timeUnit, base }) {
        const result = {
          dt: NaN,
          k: NaN,
          pd: NaN,
          gf: NaN,
          dtUnitLabel: "",
          kUnitLabel: "",
        };

        if (N0 <= 0 || Nf <= 0 || t <= 0) {
          throw new Error("All values must be positive.");
        }
        if (N0 === Nf) {
          throw new Error(
            "Initial and final values are equal; growth factor is 1, doubling time is undefined."
          );
        }

        const gf = Nf / N0;
        const lnRatio = Math.log(gf);
        const k = lnRatio / t; // per timeUnit

        const ln2 = Math.log(2);
        const dt = ln2 / k; // in same timeUnit as t

        const pd = lnRatio / ln2;

        result.gf = gf;
        result.k = k;
        result.dt = dt;
        result.pd = pd;
        result.dtUnitLabel = timeUnit === "days" ? "days" : "hours";
        result.kUnitLabel = timeUnit === "days" ? "per day" : "per hour";

        return result;
      }

      function formatNumber(x, digits = 3) {
        if (!isFinite(x)) return "–";
        if (Math.abs(x) >= 1e4 || Math.abs(x) <= 1e-3) {
          return x.toExponential(digits - 1);
        }
        return x.toFixed(digits);
      }

      function updateResults(res) {
        const dtValue = document.getElementById("dt-value");
        const dtUnitLabel = document.getElementById("dt-unit-label");
        const dtNote = document.getElementById("dt-note");
        const kValue = document.getElementById("k-value");
        const kUnitLabel = document.getElementById("k-unit-label");
        const pdValue = document.getElementById("pd-value");
        const gfValue = document.getElementById("gf-value");

        if (!res) {
          dtValue.textContent = "–";
          dtUnitLabel.textContent = "–";
          dtNote.textContent = "";
          kValue.textContent = "–";
          kUnitLabel.textContent = "per hour";
          pdValue.textContent = "–";
          gfValue.textContent = "–";
          return;
        }

        dtValue.textContent = isFinite(res.dt) ? formatNumber(res.dt, 3) : "–";
        dtUnitLabel.textContent = res.dtUnitLabel || "–";
        dtNote.textContent = isFinite(res.dt)
          ? `Approximate population doubling time in ${res.dtUnitLabel}.`
          : "";

        kValue.textContent = isFinite(res.k) ? formatNumber(res.k, 4) : "–";
        kUnitLabel.textContent = res.kUnitLabel || "per hour";

        pdValue.textContent = isFinite(res.pd) ? formatNumber(res.pd, 3) : "–";
        gfValue.textContent = isFinite(res.gf) ? formatNumber(res.gf, 3) : "–";
      }

      // Count mode handlers
      document
        .getElementById("btn-calc-count")
        .addEventListener("click", () => {
          const errEl = document.getElementById("error-count");
          errEl.textContent = "";
          try {
            const N0 = parseFloat(document.getElementById("n0").value);
            const Nf = parseFloat(document.getElementById("nf").value);
            const t = parseFloat(document.getElementById("delta-t").value);
            const timeUnit = document.getElementById("time-unit").value;
            const base = document.getElementById("base-log").value;

            if (isNaN(N0) || isNaN(Nf) || isNaN(t)) {
              throw new Error(
                "Please enter initial count, final count, and time."
              );
            }

            const res = computeDoubling({ N0, Nf, t, timeUnit, base });
            updateResults(res);
          } catch (e) {
            errEl.textContent = e.message;
            updateResults(null);
          }
        });

      document
        .getElementById("btn-reset-count")
        .addEventListener("click", () => {
          document.getElementById("n0").value = "";
          document.getElementById("nf").value = "";
          document.getElementById("delta-t").value = "";
          document.getElementById("time-unit").value = "hours";
          document.getElementById("base-log").value = "e";
          document.getElementById("error-count").textContent = "";
          updateResults(null);
        });

      // Confluency mode handlers
      document.getElementById("btn-calc-conf").addEventListener("click", () => {
        const errEl = document.getElementById("error-conf");
        errEl.textContent = "";
        try {
          const C0 = parseFloat(document.getElementById("c0").value);
          const Cf = parseFloat(document.getElementById("cf").value);
          const t = parseFloat(document.getElementById("delta-t-conf").value);
          const timeUnit = document.getElementById("time-unit-conf").value;
          const base = document.getElementById("base-log-conf").value;

          if (isNaN(C0) || isNaN(Cf) || isNaN(t)) {
            throw new Error(
              "Please enter initial confluency, final confluency, and time."
            );
          }
          if (C0 <= 0 || Cf <= 0) {
            throw new Error("Confluency values must be greater than 0%.");
          }

          const res = computeDoubling({ N0: C0, Nf: Cf, t, timeUnit, base });
          updateResults(res);
        } catch (e) {
          errEl.textContent = e.message;
          updateResults(null);
        }
      });

      document
        .getElementById("btn-reset-conf")
        .addEventListener("click", () => {
          document.getElementById("c0").value = "";
          document.getElementById("cf").value = "";
          document.getElementById("delta-t-conf").value = "";
          document.getElementById("time-unit-conf").value = "hours";
          document.getElementById("base-log-conf").value = "e";
          document.getElementById("error-conf").textContent = "";
          updateResults(null);
        });

      // Initialize empty results
      updateResults(null);

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
