        document.getElementById('sagTensionForm').addEventListener('submit', function(event) {
            event.preventDefault();
            let spanLength = parseFloat(document.getElementById('spanLength').value);
            let conductorWeight = parseFloat(document.getElementById('conductorWeight').value);
            let tension = parseFloat(document.getElementById('tension').value);

            if (isNaN(spanLength) || spanLength <= 0) {
                document.getElementById('spanLengthError').textContent = "Please enter a valid span length.";
                return;
            } else {
                document.getElementById('spanLengthError').textContent = "";
            }

            if (isNaN(conductorWeight) || conductorWeight <= 0) {
                document.getElementById('conductorWeightError').textContent = "Please enter a valid conductor weight.";
                return;
            } else {
                document.getElementById('conductorWeightError').textContent = "";
            }

            if (isNaN(tension) || tension <= 0) {
                document.getElementById('tensionError').textContent = "Please enter a valid tension.";
                return;
            } else {
                document.getElementById('tensionError').textContent = "";
            }

            let sag = (conductorWeight * Math.pow(spanLength, 2)) / (8 * tension);
            document.getElementById('sagResult').textContent = sag.toFixed(2) + ' m';
            document.getElementById('tensionResult').textContent = tension.toFixed(2) + ' N';
        });
    </script>


