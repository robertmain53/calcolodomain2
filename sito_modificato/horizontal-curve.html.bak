    <script>
      // Angle format toggle
      const angleFormatEl = document.getElementById("angleFormat");
      const angleDeltaDegWrapper = document.getElementById(
        "angleDeltaDegWrapper"
      );
      const angleDeltaDmsWrapper = document.getElementById(
        "angleDeltaDmsWrapper"
      );

      angleFormatEl.addEventListener("change", () => {
        if (angleFormatEl.value === "deg") {
          angleDeltaDegWrapper.classList.remove("hidden");
          angleDeltaDmsWrapper.classList.add("hidden");
        } else {
          angleDeltaDegWrapper.classList.add("hidden");
          angleDeltaDmsWrapper.classList.remove("hidden");
        }
      });

      // Units sync
      const unitSelect = document.getElementById("unitSelect");
      const unitSpans = [
        "radiusUnit",
        "lengthUnit",
        "tangentUnit",
        "chordUnit",
      ].map((id) => document.getElementById(id));

      unitSelect.addEventListener("change", () => {
        const u = unitSelect.value === "ft" ? "ft" : "m";
        unitSpans.forEach((s) => {
          if (s) s.textContent = u;
        });
      });

      // Helpers: DMS <-> decimal
      function dmsToDeg(d, m, s) {
        d = Number(d) || 0;
        m = Number(m) || 0;
        s = Number(s) || 0;
        const sign = d < 0 ? -1 : 1;
        return sign * (Math.abs(d) + m / 60 + s / 3600);
      }

      function degToDms(decDeg) {
        const sign = decDeg < 0 ? -1 : 1;
        let d = Math.floor(Math.abs(decDeg));
        let rem = (Math.abs(decDeg) - d) * 60;
        let m = Math.floor(rem);
        let s = (rem - m) * 60;
        d *= sign;
        return { d, m, s };
      }

      // Station parsing/formatting
      function parseStation(str) {
        if (!str) return null;
        str = String(str).trim();
        if (str.includes("+")) {
          const parts = str.split("+");
          const a = parseFloat(parts[0].replace(/[^\d.-]/g, "")) || 0;
          const b = parseFloat(parts[1].replace(/[^\d.-]/g, "")) || 0;
          return a * 100 + b;
        }
        const n = parseFloat(str.replace(/[^\d.-]/g, ""));
        return isNaN(n) ? null : n;
      }

      function formatStation(val) {
        if (val == null || isNaN(val)) return "";
        const sign = val < 0 ? "-" : "";
        val = Math.abs(val);
        const main = Math.floor(val / 100);
        const plus = Math.round(val - main * 100);
        return sign + main + "+" + plus.toString().padStart(2, "0");
      }

      // Main calculation
      const calcBtn = document.getElementById("calculateBtn");
      const resetBtn = document.getElementById("resetBtn");
      const errorMsg = document.getElementById("errorMsg");

      calcBtn.addEventListener("click", () => {
        errorMsg.textContent = "";
        document.getElementById("resultsSection").classList.add("hidden");
        document.getElementById("stakingSection").classList.add("hidden");
        document.getElementById("stakingTableBody").innerHTML = "";

        let R = parseFloat(document.getElementById("radius").value);
        let L = parseFloat(document.getElementById("curveLength").value);
        let T = parseFloat(document.getElementById("tangent").value);

        let deltaDeg;
        if (angleFormatEl.value === "deg") {
          deltaDeg = parseFloat(document.getElementById("deltaDeg").value);
        } else {
          const d = document.getElementById("deltaD").value;
          const m = document.getElementById("deltaM").value;
          const s = document.getElementById("deltaS").value;
          if (d || m || s) {
            deltaDeg = dmsToDeg(d, m, s);
          }
        }

        let knownCount = 0;
        if (!isNaN(R)) knownCount++;
        if (!isNaN(L)) knownCount++;
        if (!isNaN(T)) knownCount++;
        if (!isNaN(deltaDeg)) knownCount++;

        if (knownCount < 2) {
          errorMsg.textContent = "Please enter at least two of: R, Δ, L, T.";
          return;
        }

        const degToRad = Math.PI / 180;
        const radToDeg = 180 / Math.PI;
        let deltaRad = !isNaN(deltaDeg) ? deltaDeg * degToRad : undefined;

        // Solve using available combinations
        try {
          if (!isNaN(R) && !isNaN(deltaRad)) {
            // R & Δ known
            L = R * deltaRad;
            T = R * Math.tan(deltaRad / 2);
          } else if (!isNaN(R) && !isNaN(L)) {
            // R & L known
            deltaRad = L / R;
            deltaDeg = deltaRad * radToDeg;
            T = R * Math.tan(deltaRad / 2);
          } else if (!isNaN(R) && !isNaN(T)) {
            // R & T known
            deltaRad = 2 * Math.atan(T / R);
            deltaDeg = deltaRad * radToDeg;
            L = R * deltaRad;
          } else if (!isNaN(L) && !isNaN(deltaRad)) {
            // L & Δ known
            R = L / deltaRad;
            T = R * Math.tan(deltaRad / 2);
          } else if (!isNaN(T) && !isNaN(deltaRad)) {
            // T & Δ known
            R = T / Math.tan(deltaRad / 2);
            L = R * deltaRad;
          } else if (!isNaN(L) && !isNaN(T)) {
            // L & T known -> solve R, Δ numerically
            // Use iterative approach: delta from geometry
            // L = R*Δ, T = R*tan(Δ/2) => T/L = tan(Δ/2)/Δ
            const ratio = T / L;
            if (ratio <= 0) throw new Error("Invalid L and T combination.");
            let d = 0.5; // initial guess in radians
            for (let i = 0; i < 50; i++) {
              const f = Math.tan(d / 2) / d - ratio;
              const df =
                (0.5 * (1 / Math.cos(d / 2)) ** 2 * d - Math.tan(d / 2)) /
                (d * d);
              const step = f / df;
              d -= step;
              if (Math.abs(step) < 1e-10) break;
            }
            if (!isFinite(d) || d <= 0)
              throw new Error("Could not solve for Δ from L and T.");
            deltaRad = d;
            deltaDeg = deltaRad * radToDeg;
            R = L / deltaRad;
          } else {
            throw new Error("Unsupported combination of inputs.");
          }

          if (!(R > 0) || !(L > 0) || !(T > 0) || !(deltaRad > 0)) {
            throw new Error(
              "Computed values are not physically valid. Check your inputs."
            );
          }

          // External and middle ordinate
          const E = R * (1 / Math.cos(deltaRad / 2) - 1);
          const M = R * (1 - Math.cos(deltaRad / 2));

          // Stationing
          const piStationStr = document
            .getElementById("piStation")
            .value.trim();
          const piStationVal = parseStation(piStationStr);
          let pcStationVal = null,
            ptStationVal = null;
          if (piStationStr && piStationVal != null) {
            pcStationVal = piStationVal - T;
            ptStationVal = pcStationVal + L;
          }

          // Display results
          const unitLabel = unitSelect.value === "ft" ? "ft" : "m";
          const angleFmt = angleFormatEl.value;

          function fmtLen(x) {
            return x.toFixed(3) + " " + unitLabel;
          }

          function fmtAngleDeg(x) {
            if (angleFmt === "deg") {
              return x.toFixed(4) + "°";
            } else {
              const dms = degToDms(x);
              return `${dms.d}° ${dms.m}' ${dms.s.toFixed(1)}"`;
            }
          }

          document.getElementById("resRadius").textContent = fmtLen(R);
          document.getElementById("resDelta").textContent =
            fmtAngleDeg(deltaDeg);
          document.getElementById("resLength").textContent = fmtLen(L);
          document.getElementById("resTangent").textContent = fmtLen(T);
          document.getElementById("resExternal").textContent = fmtLen(E);
          document.getElementById("resMiddleOrd").textContent = fmtLen(M);
          document.getElementById("resPC").textContent =
            pcStationVal != null ? formatStation(pcStationVal) : "—";
          document.getElementById("resPT").textContent =
            ptStationVal != null ? formatStation(ptStationVal) : "—";

          document.getElementById("resultsSection").classList.remove("hidden");

          // Staking table
          const chordLen = parseFloat(
            document.getElementById("chordLength").value
          );
          if (!isNaN(chordLen) && chordLen > 0) {
            const tbody = document.getElementById("stakingTableBody");
            let remaining = L;
            let cumL = 0;
            let station = pcStationVal != null ? pcStationVal : 0;
            let pointIndex = 0;
            const maxPoints = 1000;

            while (remaining > 1e-6 && pointIndex < maxPoints) {
              const thisChord = remaining > chordLen ? chordLen : remaining;
              const deltaChordRad = 2 * Math.asin(thisChord / (2 * R));
              const deflRad = deltaChordRad / 2;
              cumL += thisChord;
              station += thisChord;
              remaining -= thisChord;
              pointIndex++;

              const tr = document.createElement("tr");
              tr.className = pointIndex % 2 === 0 ? "bg-white" : "bg-gray-50";

              const ptName =
                pointIndex === 1
                  ? "PC"
                  : remaining <= 1e-6
                  ? "PT"
                  : `P${pointIndex}`;
              const deflDeg = deflRad * radToDeg;

              const deflStr =
                angleFmt === "deg"
                  ? deflDeg.toFixed(4) + "°"
                  : (() => {
                      const dms = degToDms(deflDeg);
                      return `${dms.d}° ${dms.m}' ${dms.s.toFixed(1)}"`;
                    })();

              tr.innerHTML = `
            <td class="px-3 py-1 border-b">${ptName}</td>
            <td class="px-3 py-1 border-b">${
              pcStationVal != null ? formatStation(station) : station.toFixed(3)
            }</td>
            <td class="px-3 py-1 border-b text-right">${thisChord.toFixed(
              3
            )} ${unitLabel}</td>
            <td class="px-3 py-1 border-b text-right">${cumL.toFixed(
              3
            )} ${unitLabel}</td>
            <td class="px-3 py-1 border-b text-right">${deflStr}</td>
          `;
              tbody.appendChild(tr);
            }

            document
              .getElementById("stakingSection")
              .classList.remove("hidden");
          }
        } catch (e) {
          errorMsg.textContent =
            e.message || "Unable to compute curve. Please check your inputs.";
        }
      });

      resetBtn.addEventListener("click", () => {
        [
          "radius",
          "curveLength",
          "tangent",
          "deltaDeg",
          "deltaD",
          "deltaM",
          "deltaS",
          "piStation",
          "chordLength",
        ].forEach((id) => {
          const el = document.getElementById(id);
          if (el) el.value = "";
        });
        errorMsg.textContent = "";
        document.getElementById("resultsSection").classList.add("hidden");
        document.getElementById("stakingSection").classList.add("hidden");
        document.getElementById("stakingTableBody").innerHTML = "";
      });

      document.querySelectorAll("#faq .faq-question").forEach((btn) => {
        btn.addEventListener("click", () => {
          const ans = btn.parentElement.querySelector(".faq-answer");
          const icon = btn.querySelector("span:last-child");
          const isHidden = ans.classList.contains("hidden");
          document
            .querySelectorAll("#faq .faq-answer")
            .forEach((a) => a.classList.add("hidden"));
          document
            .querySelectorAll("#faq .faq-question span:last-child")
            .forEach((i) => (i.textContent = "+"));
          if (isHidden) {
            ans.classList.remove("hidden");
            icon.textContent = "−";
          }
        });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
