    <script>
      function fmtVA(value) {
        if (!isFinite(value)) return "0 VA";
        return (
          value.toLocaleString(undefined, { maximumFractionDigits: 0 }) + " VA"
        );
      }
      function fmtA(value) {
        if (!isFinite(value)) return "0 A";
        return (
          value.toLocaleString(undefined, { maximumFractionDigits: 1 }) + " A"
        );
      }

      function getNumber(id) {
        const el = document.getElementById(id);
        if (!el) return 0;
        const v = parseFloat(el.value);
        return isNaN(v) ? 0 : v;
      }

      function calculateNECLoad() {
        const errorEl = document.getElementById("calcError");
        errorEl.classList.add("hidden");

        // Basic inputs
        const phase =
          parseInt(document.getElementById("systemPhase").value, 10) || 1;
        const voltage = getNumber("systemVoltage");
        const marginPct = Math.max(0, getNumber("designMargin"));

        // 2. Lighting
        const area = Math.max(0, getNumber("floorArea"));
        const vaPerSqft = Math.max(0, getNumber("lightingVAperSqft"));
        const lightingConnected = area * vaPerSqft;
        const lightingDemand = Math.max(
          0,
          parseFloat(document.getElementById("lightingDemand").value) || 100
        );
        const lightingCalc = lightingConnected * (lightingDemand / 100);

        // 3. Receptacles
        const genReceptVA = Math.max(0, getNumber("genReceptVA"));
        const genReceptDemand = Math.max(
          0,
          parseFloat(document.getElementById("genReceptDemand").value) || 100
        );
        const genReceptCalc = genReceptVA * (genReceptDemand / 100);

        const dedicatedReceptVA = Math.max(0, getNumber("dedicatedReceptVA"));
        const dedicatedReceptCalc = dedicatedReceptVA; // 100%

        // 4. Fixed appliances
        const fixedApplianceVA = Math.max(0, getNumber("fixedApplianceVA"));
        const fixedApplianceCount = Math.max(
          0,
          getNumber("fixedApplianceCount")
        );
        let fixedDemand =
          parseFloat(document.getElementById("fixedApplianceDemand").value) ||
          100;
        if (fixedApplianceCount < 4 && fixedDemand < 100) {
          // If user left default but count < 4, bump to 100% for safety
          fixedDemand = 100;
        }
        const fixedApplianceCalc = fixedApplianceVA * (fixedDemand / 100);

        // 5. HVAC
        const coolingVA = Math.max(0, getNumber("coolingVA"));
        const heatingVA = Math.max(0, getNumber("heatingVA"));
        const hvacMode = document.getElementById("hvacSelection").value;
        let hvacConnected = 0;
        let hvacCalc = 0;
        let hvacLabel = "Larger of heating or cooling";

        if (hvacMode === "both") {
          hvacConnected = coolingVA + heatingVA;
          hvacCalc = hvacConnected;
          hvacLabel = "Heating + cooling (both)";
        } else if (hvacMode === "cooling") {
          hvacConnected = coolingVA;
          hvacCalc = coolingVA;
          hvacLabel = "Cooling only";
        } else if (hvacMode === "heating") {
          hvacConnected = heatingVA;
          hvacCalc = heatingVA;
          hvacLabel = "Heating only";
        } else {
          // auto
          if (coolingVA >= heatingVA) {
            hvacConnected = coolingVA;
            hvacCalc = coolingVA;
            hvacLabel = "Cooling (larger of heating/cooling)";
          } else {
            hvacConnected = heatingVA;
            hvacCalc = heatingVA;
            hvacLabel = "Heating (larger of heating/cooling)";
          }
        }

        // 6. Motors
        const motorSumVA = Math.max(0, getNumber("motorSumVA"));
        const largestMotorVA = Math.max(0, getNumber("largestMotorVA"));
        const motorFactor = Math.max(
          1,
          parseFloat(document.getElementById("motorFactor").value) || 1.25
        );

        let motorConnected = motorSumVA;
        let motorCalc = 0;
        if (motorSumVA === 0) {
          motorCalc = 0;
        } else {
          const largest = Math.min(largestMotorVA, motorSumVA);
          const others = Math.max(0, motorSumVA - largest);
          motorCalc = others + largest * motorFactor;
        }

        // 7. Other loads
        const signVA = Math.max(0, getNumber("signVA"));
        const elevatorVA = Math.max(0, getNumber("elevatorVA"));
        const otherVA = Math.max(0, getNumber("otherVA"));
        const otherConnected = signVA + elevatorVA + otherVA;
        const otherCalc = otherConnected; // 100%

        // Check for invalids
        if (voltage <= 0 || phase <= 0) {
          errorEl.textContent =
            "Please select a valid system voltage and phase.";
          errorEl.classList.remove("hidden");
          return;
        }

        // Total VA before margin
        const totalBeforeMargin =
          lightingCalc +
          genReceptCalc +
          dedicatedReceptCalc +
          fixedApplianceCalc +
          hvacCalc +
          motorCalc +
          otherCalc;

        if (!isFinite(totalBeforeMargin) || totalBeforeMargin < 0) {
          errorEl.textContent =
            "Please check your inputs. Negative or invalid values detected.";
          errorEl.classList.remove("hidden");
          return;
        }

        const totalVAWithMargin = totalBeforeMargin * (1 + marginPct / 100);

        // Service amps
        let serviceAmps = 0;
        if (phase === 1) {
          serviceAmps = totalVAWithMargin / voltage;
        } else {
          serviceAmps = totalVAWithMargin / (Math.sqrt(3) * voltage);
        }

        // Update summary
        document.getElementById("resultsSection").classList.remove("hidden");
        document.getElementById("totalVA").textContent =
          fmtVA(totalVAWithMargin);
        document.getElementById("totalVADetail").textContent =
          "Base load " +
          fmtVA(totalBeforeMargin) +
          (marginPct > 0
            ? " with " + marginPct.toFixed(1) + "% design margin applied."
            : " (no additional design margin).");

        document.getElementById("serviceAmps").textContent = fmtA(serviceAmps);
        document.getElementById("serviceDetail").textContent =
          (phase === 1 ? "Single‑phase" : "Three‑phase") +
          " at " +
          voltage +
          " V. Use this as a minimum ampacity; select conductors and OCPD per NEC tables.";

        // Breakdown table
        const breakdown = [
          {
            label: "General lighting",
            connected: lightingConnected,
            demand: lightingDemand,
            calc: lightingCalc,
          },
          {
            label: "General‑use receptacles",
            connected: genReceptVA,
            demand: genReceptDemand,
            calc: genReceptCalc,
          },
          {
            label: "Dedicated / small appliances",
            connected: dedicatedReceptVA,
            demand: 100,
            calc: dedicatedReceptCalc,
          },
          {
            label: "Fixed appliances",
            connected: fixedApplianceVA,
            demand: fixedDemand,
            calc: fixedApplianceCalc,
          },
          {
            label: "HVAC (" + hvacLabel + ")",
            connected: hvacConnected,
            demand: 100,
            calc: hvacCalc,
          },
          {
            label: "Motors (incl. largest × " + motorFactor.toFixed(2) + ")",
            connected: motorConnected,
            demand: 100,
            calc: motorCalc,
          },
          {
            label: "Other loads (signs, elevators, misc.)",
            connected: otherConnected,
            demand: 100,
            calc: otherCalc,
          },
        ];

        const tbody = document.getElementById("breakdownBody");
        tbody.innerHTML = "";
        breakdown.forEach((row) => {
          if (row.connected === 0 && row.calc === 0) return;
          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="px-3 py-2 text-gray-800">${row.label}</td>
        <td class="px-3 py-2 text-right text-gray-700">${fmtVA(
          row.connected
        )}</td>
        <td class="px-3 py-2 text-right text-gray-700">${
          row.demand.toFixed ? row.demand.toFixed(0) : row.demand
        }%</td>
        <td class="px-3 py-2 text-right font-medium text-gray-900">${fmtVA(
          row.calc
        )}</td>
      `;
          tbody.appendChild(tr);
        });
      }

      document.addEventListener("DOMContentLoaded", function () {
        document
          .getElementById("calcBtn")
          .addEventListener("click", calculateNECLoad);
        document
          .getElementById("resetBtn")
          .addEventListener("click", function () {
            document.getElementById("necForm").reset();
            document.getElementById("resultsSection").classList.add("hidden");
            document.getElementById("calcError").classList.add("hidden");
            // Restore some sensible defaults
            document.getElementById("systemPhase").value = "3";
            document.getElementById("systemVoltage").value = "277";
            document.getElementById("lightingDemand").value = "80";
            document.getElementById("genReceptDemand").value = "80";
            document.getElementById("fixedApplianceDemand").value = "75";
            document.getElementById("hvacSelection").value = "auto";
            document.getElementById("motorFactor").value = "1.25";
          });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
