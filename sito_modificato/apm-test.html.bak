    <script>
      // APM Test Logic
      (function () {
        const modeButtons = document.querySelectorAll(".mode-btn");
        const durationButtons = document.querySelectorAll(".duration-btn");
        const startBtn = document.getElementById("start-btn");
        const resetBtn = document.getElementById("reset-btn");
        const timeLeftEl = document.getElementById("time-left");
        const totalActionsEl = document.getElementById("total-actions");
        const apmValueEl = document.getElementById("apm-value");
        const accuracyValueEl = document.getElementById("accuracy-value");
        const mouseArea = document.getElementById("mouse-area");
        const keyboardArea = document.getElementById("keyboard-area");
        const statusText = document.getElementById("status-text");
        const currentKeyEl = document.getElementById("current-key");
        const resultsPanel = document.getElementById("results-panel");
        const resultApmEl = document.getElementById("result-apm");
        const resultActionsEl = document.getElementById("result-actions");
        const resultAccuracyEl = document.getElementById("result-accuracy");
        const resultCommentEl = document.getElementById("result-comment");

        const dotButtons = Array.from(document.querySelectorAll(".dot-btn"));
        const keyboardKeys = ["a", "s", "d", "j", "k", "l", " "]; // space last
        let currentMode = "mouse";
        let duration = 30; // seconds
        let timerId = null;
        let running = false;
        let startTime = null;
        let endTime = null;
        let timeLeft = duration;
        let correctActions = 0;
        let wrongActions = 0;
        let activeDotIndex = null;
        let activeKey = "a";

        function setMode(mode) {
          currentMode = mode;
          modeButtons.forEach((btn) => {
            if (btn.dataset.mode === mode) {
              btn.classList.remove(
                "bg-white",
                "text-gray-700",
                "border-gray-300"
              );
              btn.classList.add("bg-blue-600", "text-white", "border-blue-600");
            } else {
              btn.classList.add("bg-white", "text-gray-700", "border-gray-300");
              btn.classList.remove(
                "bg-blue-600",
                "text-white",
                "border-blue-600"
              );
            }
          });
          if (mode === "mouse") {
            mouseArea.classList.remove("hidden");
            keyboardArea.classList.add("hidden");
          } else if (mode === "keyboard") {
            mouseArea.classList.add("hidden");
            keyboardArea.classList.remove("hidden");
          } else {
            mouseArea.classList.remove("hidden");
            keyboardArea.classList.remove("hidden");
          }
          resetTest();
        }

        function setDuration(sec) {
          duration = sec;
          durationButtons.forEach((btn) => {
            if (Number(btn.dataset.duration) === sec) {
              btn.classList.add(
                "bg-blue-50",
                "text-blue-700",
                "border-blue-300"
              );
              btn.classList.remove(
                "bg-white",
                "text-gray-700",
                "border-gray-300"
              );
            } else {
              btn.classList.remove(
                "bg-blue-50",
                "text-blue-700",
                "border-blue-300"
              );
              btn.classList.add("bg-white", "text-gray-700", "border-gray-300");
            }
          });
          resetTest();
        }

        function resetDots() {
          dotButtons.forEach((btn) => {
            btn.classList.remove("bg-blue-500", "border-blue-600");
            btn.classList.add("bg-gray-100", "border-gray-300");
          });
          activeDotIndex = null;
        }

        function activateRandomDot() {
          if (dotButtons.length === 0) return;
          let newIndex;
          do {
            newIndex = Math.floor(Math.random() * dotButtons.length);
          } while (newIndex === activeDotIndex && dotButtons.length > 1);
          resetDots();
          const btn = dotButtons[newIndex];
          btn.classList.remove("bg-gray-100", "border-gray-300");
          btn.classList.add("bg-blue-500", "border-blue-600");
          activeDotIndex = newIndex;
        }

        function setRandomKey() {
          const idx = Math.floor(Math.random() * keyboardKeys.length);
          activeKey = keyboardKeys[idx];
          currentKeyEl.textContent =
            activeKey === " " ? "Space" : activeKey.toUpperCase();
        }

        function updateStats() {
          const totalActions = correctActions + wrongActions;
          const elapsed = startTime ? (Date.now() - startTime) / 1000 : 0;
          const effectiveTime = Math.max(elapsed, 0.001);
          const apm =
            totalActions > 0 ? (totalActions / effectiveTime) * 60 : 0;
          const accuracy =
            totalActions > 0 ? (correctActions / totalActions) * 100 : 100;

          totalActionsEl.textContent = totalActions.toString();
          apmValueEl.textContent = apm.toFixed(0);
          accuracyValueEl.textContent = accuracy.toFixed(0) + "%";
        }

        function updateTimer() {
          if (!running) return;
          const now = Date.now();
          const elapsed = (now - startTime) / 1000;
          timeLeft = Math.max(duration - elapsed, 0);
          timeLeftEl.textContent = timeLeft.toFixed(1) + " s";
          if (timeLeft <= 0) {
            finishTest();
          }
        }

        function startTimerLoop() {
          if (timerId) cancelAnimationFrame(timerId);
          const loop = () => {
            updateTimer();
            if (running) {
              timerId = requestAnimationFrame(loop);
            }
          };
          timerId = requestAnimationFrame(loop);
        }

        function startTest() {
          if (running) return;
          resetTest(false);
          running = true;
          startTime = Date.now();
          endTime = startTime + duration * 1000;
          statusText.textContent =
            "Test running... focus on fast, accurate actions.";
          startBtn.disabled = true;
          startBtn.classList.add("opacity-60", "cursor-not-allowed");

          if (currentMode === "mouse" || currentMode === "mixed") {
            activateRandomDot();
          }
          if (currentMode === "keyboard" || currentMode === "mixed") {
            setRandomKey();
          }
          startTimerLoop();
        }

        function finishTest() {
          if (!running) return;
          running = false;
          if (timerId) cancelAnimationFrame(timerId);
          timerId = null;
          timeLeft = 0;
          timeLeftEl.textContent = "0.0 s";
          resetDots();
          startBtn.disabled = false;
          startBtn.classList.remove("opacity-60", "cursor-not-allowed");

          const totalActions = correctActions + wrongActions;
          const elapsed = duration; // full duration
          const apm = totalActions > 0 ? (totalActions / elapsed) * 60 : 0;
          const accuracy =
            totalActions > 0 ? (correctActions / totalActions) * 100 : 100;

          resultApmEl.textContent = apm.toFixed(0);
          resultActionsEl.textContent = totalActions.toString();
          resultAccuracyEl.textContent = accuracy.toFixed(0) + "%";

          let comment;
          if (totalActions === 0) {
            comment =
              "You did not perform any actions. Try running the test again and start clicking or pressing keys as soon as it begins.";
          } else if (apm < 60) {
            comment =
              "Your APM is in the beginner range. With a bit of practice and focus on accuracy, you can quickly improve.";
          } else if (apm < 120) {
            comment =
              "Solid performance! You are around the average range for regular computer users and casual gamers.";
          } else if (apm < 200) {
            comment =
              "Great job – your APM is in the advanced range. With consistent training you are approaching esports-level speed.";
          } else {
            comment =
              "Excellent! Your APM is extremely high. Make sure you maintain good accuracy and avoid strain while training.";
          }
          if (accuracy < 80) {
            comment +=
              " Your accuracy is on the low side; try to slow down slightly and prioritize precise actions.";
          }
          resultsPanel.classList.remove("hidden");
          resultCommentEl.textContent = comment;
          statusText.textContent =
            "Test finished. Review your stats and run it again to track your progress.";
        }

        function resetTest(clearResults = false) {
          if (timerId) cancelAnimationFrame(timerId);
          timerId = null;
          running = false;
          startTime = null;
          endTime = null;
          timeLeft = duration;
          correctActions = 0;
          wrongActions = 0;
          resetDots();
          setRandomKey();
          timeLeftEl.textContent = duration.toFixed(1) + " s";
          totalActionsEl.textContent = "0";
          apmValueEl.textContent = "0";
          accuracyValueEl.textContent = "100%";
          startBtn.disabled = false;
          startBtn.classList.remove("opacity-60", "cursor-not-allowed");
          statusText.textContent =
            "Press “Start test” and then perform actions as fast and accurately as you can.";
          if (clearResults) {
            resultsPanel.classList.add("hidden");
          }
        }

        // Event listeners
        modeButtons.forEach((btn) => {
          btn.addEventListener("click", () => setMode(btn.dataset.mode));
        });

        durationButtons.forEach((btn) => {
          btn.addEventListener("click", () =>
            setDuration(Number(btn.dataset.duration))
          );
        });

        startBtn.addEventListener("click", startTest);
        resetBtn.addEventListener("click", () => resetTest(true));

        dotButtons.forEach((btn) => {
          btn.addEventListener("click", () => {
            if (!running) return;
            const idx = Number(btn.dataset.index);
            if (idx === activeDotIndex) {
              correctActions++;
              activateRandomDot();
            } else {
              wrongActions++;
            }
            updateStats();
          });
        });

        document.addEventListener("keydown", (e) => {
          if (!running) return;
          const key = e.key.toLowerCase();
          // prevent space from scrolling
          if (key === " " || key === "spacebar") {
            e.preventDefault();
          }
          const normalized = key === " " || key === "spacebar" ? " " : key;
          if (!keyboardKeys.includes(normalized)) {
            // ignore keys outside our set
            return;
          }
          if (normalized === activeKey) {
            correctActions++;
            setRandomKey();
          } else {
            wrongActions++;
          }
          updateStats();
        });

        // Initialize defaults
        setMode("mouse");
        setDuration(30);
      })();
    </script>

