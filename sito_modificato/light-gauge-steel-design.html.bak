    <script>
      // Light gauge steel design calculator logic
      function parseNumber(id) {
        const v = parseFloat(document.getElementById(id).value);
        return isNaN(v) ? 0 : v;
      }

      function setText(id, text) {
        document.getElementById(id).textContent = text;
      }

      function formatNumber(val, decimals) {
        if (!isFinite(val)) return "–";
        return val.toFixed(decimals);
      }

      document.getElementById("calculate-btn").addEventListener("click", () => {
        const designType = document.getElementById("design-type").value;

        const fy = parseNumber("fy"); // MPa
        const phi = parseNumber("phi");
        const A = parseNumber("area"); // mm2
        const Ix = parseNumber("ix"); // mm4
        const Iy = parseNumber("iy"); // mm4
        const Sx = parseNumber("sx"); // mm3

        const Lm = parseNumber("length"); // m
        const kx = parseNumber("kx");
        const ky = parseNumber("ky");
        const defLimit = parseNumber("deflection-limit");

        const Pu = parseNumber("axial-load"); // kN
        const Mu = parseNumber("moment"); // kNm
        const w = parseNumber("uniform-load"); // kN/m
        const loadType = document.getElementById("load-type").value;

        const E = 200000; // MPa

        const Lmm = Lm * 1000;
        let rx = 0,
          ry = 0,
          klrX = 0,
          klrY = 0;

        if (A > 0 && Ix > 0 && Iy > 0) {
          rx = Math.sqrt(Ix / A);
          ry = Math.sqrt(Iy / A);
          klrX = rx > 0 ? (kx * Lmm) / rx : 0;
          klrY = ry > 0 ? (ky * Lmm) / ry : 0;
        }

        setText("rx", rx ? formatNumber(rx, 1) : "–");
        setText("ry", ry ? formatNumber(ry, 1) : "–");
        setText("klr-x", klrX ? formatNumber(klrX, 0) : "–");
        setText("klr-y", klrY ? formatNumber(klrY, 0) : "–");

        const slendernessNote = [];
        if (klrX > 200 || klrY > 200) {
          slendernessNote.push(
            "Warning: kL/r exceeds 200 in at least one axis. Member is very slender for compression."
          );
        } else if (klrX && klrY) {
          slendernessNote.push(
            "kL/r is within a typical limit of 200 for compression members, but always verify with your design code."
          );
        }
        document.getElementById("slenderness-note").textContent =
          slendernessNote.join(" ");

        let phiPn = 0;
        if (A > 0 && fy > 0) {
          const lambda = Math.max(klrX || 0, klrY || 0);
          let reduction = 1.0;
          if (lambda > 0) {
            reduction = Math.pow(0.658, Math.pow(lambda / 120, 2));
            if (reduction < 0.1) reduction = 0.1;
          }
          const Pn = (reduction * fy * A) / 1000; // kN
          phiPn = phi * Pn;
        }

        let phiMn = 0;
        if (Sx > 0 && fy > 0) {
          const Mn = (0.9 * fy * Sx) / 1e6; // kNm
          phiMn = phi * Mn;
        }

        setText("phi-pn", phiPn ? formatNumber(phiPn, 1) : "–");
        setText("phi-mn", phiMn ? formatNumber(phiMn, 2) : "–");

        let utilAxial = 0,
          utilMoment = 0,
          interaction = 0;

        if (phiPn > 0 && Pu > 0) utilAxial = Pu / phiPn;
        if (phiMn > 0 && Mu > 0) utilMoment = Mu / phiMn;

        if (designType === "axial") {
          interaction = utilAxial;
          utilMoment = 0;
        } else if (designType === "bending") {
          interaction = utilMoment;
          utilAxial = 0;
        } else {
          interaction = utilAxial + utilMoment;
        }

        setText("util-axial", utilAxial ? formatNumber(utilAxial, 2) : "–");
        setText("util-moment", utilMoment ? formatNumber(utilMoment, 2) : "–");
        setText(
          "interaction",
          interaction ? formatNumber(interaction, 2) : "–"
        );

        const strengthStatus = document.getElementById("strength-status");
        strengthStatus.className =
          "mt-3 inline-flex items-center px-2.5 py-1 rounded-full text-xs font-semibold";
        if (interaction && interaction > 0) {
          if (interaction <= 1.0) {
            strengthStatus.classList.add("result-badge-ok");
            strengthStatus.textContent = "Strength OK (interaction ≤ 1.0)";
          } else {
            strengthStatus.classList.add("result-badge-ng");
            strengthStatus.textContent = "NG: Strength demand exceeds capacity";
          }
        } else {
          strengthStatus.textContent = "";
        }

        let delta = 0;
        let deltaAllow = 0;
        let ratio = 0;

        if (Ix > 0 && w > 0 && Lm > 0) {
          const wNmm = (w * 1e3) / 1e3; // kN/m -> N/mm (approx)
          const Lmm4 = Math.pow(Lmm, 4);
          if (loadType === "simple") {
            delta = (5 * wNmm * Lmm4) / (384 * E * Ix);
          } else {
            delta = (wNmm * Lmm4) / (8 * E * Ix);
          }
          deltaAllow = Lmm / defLimit;
          ratio = delta > 0 ? Lmm / delta : 0;
        }

        setText("deflection", delta ? formatNumber(delta, 1) : "–");
        setText(
          "deflection-allow",
          deltaAllow ? formatNumber(deltaAllow, 1) : "–"
        );
        setText(
          "deflection-ratio",
          ratio ? "L/" + formatNumber(ratio, 0) : "–"
        );

        const defStatus = document.getElementById("deflection-status");
        defStatus.className =
          "mt-3 inline-flex items-center px-2.5 py-1 rounded-full text-xs font-semibold";
        if (delta && deltaAllow) {
          if (delta <= deltaAllow) {
            defStatus.classList.add("result-badge-ok");
            defStatus.textContent = "Deflection OK";
          } else {
            defStatus.classList.add("result-badge-ng");
            defStatus.textContent = "NG: Deflection exceeds limit";
          }
        } else {
          defStatus.textContent = "";
        }
      });
    </script>

