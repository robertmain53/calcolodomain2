    <script>
      (function () {
        const L_PER_100KM_TO_MPG_CONST = 235.214583;

        const lInput = document.getElementById("l100-input");
        const mpgInput = document.getElementById("mpg-input");
        const resultText = document.getElementById("result-text");
        const swapBtn = document.getElementById("swap-direction");
        const resetBtn = document.getElementById("reset-btn");
        const directionLabel = document.getElementById("direction-label");

        // true: L/100km -> MPG, false: MPG -> L/100km
        let lToMpg = true;
        let isUpdating = false;

        function formatNumber(value) {
          if (!isFinite(value)) return "";
          if (value === 0) return "0";
          if (Math.abs(value) >= 1000) return value.toFixed(1);
          if (Math.abs(value) >= 10) return value.toFixed(2);
          return value.toFixed(3);
        }

        function updateFromL() {
          if (isUpdating || !lToMpg) return;
          const val = parseFloat(lInput.value);
          if (!isFinite(val) || val <= 0) {
            mpgInput.value = "";
            resultText.classList.add("hidden");
            return;
          }
          const mpg = L_PER_100KM_TO_MPG_CONST / val;
          isUpdating = true;
          mpgInput.value = formatNumber(mpg);
          isUpdating = false;
          resultText.textContent = `${val.toFixed(2)} L/100km ≈ ${formatNumber(
            mpg
          )} MPG (US).`;
          resultText.classList.remove("hidden");
        }

        function updateFromMpg() {
          if (isUpdating || lToMpg) return;
          const val = parseFloat(mpgInput.value);
          if (!isFinite(val) || val <= 0) {
            lInput.value = "";
            resultText.classList.add("hidden");
            return;
          }
          const l100 = L_PER_100KM_TO_MPG_CONST / val;
          isUpdating = true;
          lInput.value = formatNumber(l100);
          isUpdating = false;
          resultText.textContent = `${val.toFixed(2)} MPG (US) ≈ ${formatNumber(
            l100
          )} L/100km.`;
          resultText.classList.remove("hidden");
        }

        lInput.addEventListener("input", updateFromL);
        mpgInput.addEventListener("input", updateFromMpg);

        swapBtn.addEventListener("click", () => {
          lToMpg = !lToMpg;
          directionLabel.textContent = lToMpg
            ? "Currently: L/100km → MPG US"
            : "Currently: MPG US → L/100km";

          // Swap values logically
          if (lToMpg) {
            updateFromL();
          } else {
            updateFromMpg();
          }
        });

        resetBtn.addEventListener("click", () => {
          lInput.value = "";
          mpgInput.value = "";
          resultText.classList.add("hidden");
        });
      })();
    </script>

