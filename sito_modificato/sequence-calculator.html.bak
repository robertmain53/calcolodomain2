            <!-- JavaScript for the calculator -->
            <script>
              document.addEventListener('DOMContentLoaded', () => {
                const numberInput = document.getElementById('number-input');
                const calculateBtn = document.getElementById('calculate-btn');
                const resultsBox = document.getElementById('results-box');

                function formatNum(num) {
                    // Formatta per evitare errori di precisione di JS
                    return parseFloat(num.toPrecision(10));
                }

                function getGCD(a, b) {
                    return b === 0 ? a : getGCD(b, a % b);
                }

                function asFraction(num) {
                    if (Math.abs(num - Math.round(num)) < 1e-9) return `${num}`; // È un intero
                    let den = 1;
                    while (Math.abs((num * den) - Math.round(num * den)) > 1e-9 && den < 1000) {
                        den++;
                    }
                    let nom = Math.round(num * den);
                    const common = getGCD(nom, den);
                    return `${nom / common} / ${den / common}`;
                }

                function analyzeSequence(nums) {
                    if (nums.length < 3) return { type: 'Error', message: 'Please enter at least 3 numbers to detect a pattern.' };
                    
                    // Controlla Aritmetica
                    let isArithmetic = true;
                    const d = formatNum(nums[1] - nums[0]);
                    for (let i = 2; i < nums.length; i++) {
                        if (formatNum(nums[i] - nums[i-1]) !== d) {
                            isArithmetic = false;
                            break;
                        }
                    }
                    if (isArithmetic) {
                        const a1 = nums[0];
                        return { type: 'Arithmetic', a1: a1, d: d };
                    }
                    
                    // Controlla Geometrica
                    let isGeometric = true;
                    const r = formatNum(nums[1] / nums[0]);
                    if (r === 0) isGeometric = false; // Cannot have a zero ratio
                    
                    for (let i = 2; i < nums.length; i++) {
                        if (formatNum(nums[i] / nums[i-1]) !== r) {
                            isGeometric = false;
                            break;
                        }
                    }
                    if (isGeometric) {
                        const a1 = nums[0];
                        return { type: 'Geometric', a1: a1, r: r };
                    }
                    
                    // Controlla Quadratica (differenze seconde)
                    let diffs1 = [];
                    for (let i = 0; i < nums.length - 1; i++) {
                        diffs1.push(nums[i+1] - nums[i]);
                    }
                    let diffs2 = [];
                    for (let i = 0; i < diffs1.length - 1; i++) {
                        diffs2.push(diffs1[i+1] - diffs1[i]);
                    }
                    
                    const isQuadratic = diffs2.length > 0 && diffs2.every(v => formatNum(v) === formatNum(diffs2[0]));
                    if (isQuadratic) {
                        const a = formatNum(diffs2[0] / 2);
                        const b = formatNum(diffs1[0] - 3*a);
                        const c = formatNum(nums[0] - a - b);
                        return { type: 'Quadratic', a: a, b: b, c: c };
                    }

                    return { type: 'Unknown', message: 'This sequence is not arithmetic, geometric, or quadratic.' };
                }
                
                function generateNextTerms(result, n, count = 5) {
                    let terms = [];
                    for (let i = 1; i <= count; i++) {
                        const termN = n + i;
                        if (result.type === 'Arithmetic') {
                            terms.push(formatNum(result.a1 + (termN - 1) * result.d));
                        } else if (result.type === 'Geometric') {
                            terms.push(formatNum(result.a1 * Math.pow(result.r, termN - 1)));
                        } else if (result.type === 'Quadratic') {
                            terms.push(formatNum((result.a * termN * termN) + (result.b * termN) + result.c));
                        }
                    }
                    return terms.join(', ');
                }
                
                function getFormula(result) {
                    if (result.type === 'Arithmetic') return `aₙ = ${result.a1} + (n-1) * ${result.d}`;
                    if (result.type === 'Geometric') return `aₙ = ${result.a1} * ${result.r}ⁿ⁻¹`;
                    if (result.type === 'Quadratic') return `aₙ = ${result.a}n² + ${result.b}n + ${result.c}`;
                    return '-';
                }

                calculateBtn.addEventListener('click', () => {
                    const text = numberInput.value;
                    const nums = text
                        .split(/[,\s\n]+/)
                        .filter(n => n.trim() !== '')
                        .map(n => parseFloat(n))
                        .filter(n => !isNaN(n));

                    if (nums.length < 3) {
                        resultsBox.innerHTML = `<h3 class="text-lg font-semibold text-red-700">Error</h3><p>Please enter at least 3 valid numbers to detect a pattern.</p>`;
                        resultsBox.hidden = false;
                        return;
                    }
                    
                    const result = analyzeSequence(nums);
                    
                    if (result.type === 'Error' || result.type === 'Unknown') {
                        resultsBox.innerHTML = `<h3 class="text-lg font-semibold text-gray-800">Analysis Result</h3><p>${result.message}</p>`;
                        resultsBox.hidden = false;
                        return;
                    }
                    
                    const formula = getFormula(result);
                    const nextTerms = generateNextTerms(result, nums.length, 5);

                    let sumHtml = '';
                    if (result.type === 'Arithmetic') {
                        sumHtml = `
                            <h3 class="text-lg font-semibold text-gray-800 mt-6 mb-2">Sum of first 'n' terms (Arithmetic Series)</h3>
                            <p>Formula: <code>Sₙ = n/2 * (2a₁ + (n-1)d)</code></p>
                            <div class="flex gap-4 items-center">
                                <input type="number" id="sum-n" class="converter-input w-1/3" placeholder="n" value="${nums.length}">
                                <button id="calc-sum-btn" class="bg-blue-500 text-white font-bold py-2 px-4 rounded hover:bg-blue-600">Calculate Sum</button>
                                <div id="sum-output" class="text-lg font-bold"></div>
                            </div>`;
                    } else if (result.type === 'Geometric') {
                         sumHtml = `
                            <h3 class="text-lg font-semibold text-gray-800 mt-6 mb-2">Sum of first 'n' terms (Geometric Series)</h3>
                            <p>Formula: <code>Sₙ = a₁ * (1 - rⁿ) / (1 - r)</code></p>
                            <div class="flex gap-4 items-center">
                                <input type="number" id="sum-n" class="converter-input w-1/3" placeholder="n" value="${nums.length}">
                                <button id="calc-sum-btn" class="bg-blue-500 text-white font-bold py-2 px-4 rounded hover:bg-blue-600">Calculate Sum</button>
                                <div id="sum-output" class="text-lg font-bold"></div>
                            </div>`;
                    }

                    resultsBox.innerHTML = `
                        <h3 class="text-lg font-semibold text-gray-800 mb-2">Sequence Analysis</h3>
                        <div class="grid grid-cols-2 gap-4">
                            <div class="stat-result"><div class="stat-label">Type Detected</div><div class="stat-value">${result.type}</div></div>
                            <div class="stat-result"><div class="stat-label">Explicit Formula (aₙ)</div><div class="stat-value" style="font-size: 1.1rem;">${formula}</div></div>
                        </div>
                        <div class="mt-4">
                            <label class="converter-label">Next 5 Terms:</label>
                            <div class="sequence-output">${nextTerms}</div>
                        </div>
                        ${sumHtml}
                    `;
                    resultsBox.hidden = false;
                    
                    // Add event listener for the new sum button
                    const calcSumBtn = document.getElementById('calc-sum-btn');
                    if (calcSumBtn) {
                        calcSumBtn.addEventListener('click', () => {
                            const n = parseInt(document.getElementById('sum-n').value);
                            if (isNaN(n) || n < 1) return;
                            
                            let sum = 0;
                            if (result.type === 'Arithmetic') {
                                sum = (n / 2) * (2 * result.a1 + (n - 1) * result.d);
                            } else if (result.type === 'Geometric') {
                                if (result.r === 1) {
                                    sum = result.a1 * n;
                                } else {
                                    sum = result.a1 * (1 - Math.pow(result.r, n)) / (1 - result.r);
                                }
                            }
                            document.getElementById('sum-output').textContent = `= ${formatNum(sum)}`;
                        });
                        // Calculate the initial sum
                        calcSumBtn.click();
                    }
                });
              });
            </script>
            
