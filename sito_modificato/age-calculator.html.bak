    <script>
      // Utility: pad with leading zeros
      function pad(num, size) {
        let s = String(num);
        while (s.length < size) s = "0" + s;
        return s;
      }

      function parseDateInput(value) {
        if (!value) return null;
        const parts = value.split("-");
        if (parts.length !== 3) return null;
        const [y, m, d] = parts.map(Number);
        if (!y || !m || !d) return null;
        const date = new Date(y, m - 1, d);
        if (
          date.getFullYear() !== y ||
          date.getMonth() !== m - 1 ||
          date.getDate() !== d
        ) {
          return null; // invalid date like 2023-02-30
        }
        return date;
      }

      function applyTimeToDate(date, timeStr, defaultToEndOfDay = false) {
        if (!date) return null;
        if (!timeStr) {
          // If no time provided, use start of day for birth, end of day for reference when detailed units matter
          if (defaultToEndOfDay) {
            date.setHours(23, 59, 59, 999);
          } else {
            date.setHours(0, 0, 0, 0);
          }
          return date;
        }
        const parts = timeStr.split(":").map(Number);
        const h = parts[0] || 0;
        const m = parts[1] || 0;
        const s = parts[2] || 0;
        date.setHours(h, m, s, 0);
        return date;
      }

      function diffCalendar(d1, d2) {
        // d1 <= d2 assumed
        let years = d2.getFullYear() - d1.getFullYear();
        let months = d2.getMonth() - d1.getMonth();
        let days = d2.getDate() - d1.getDate();

        if (days < 0) {
          // borrow days from previous month
          const prevMonth = new Date(d2.getFullYear(), d2.getMonth(), 0); // last day of previous month
          days += prevMonth.getDate();
          months -= 1;
        }

        if (months < 0) {
          months += 12;
          years -= 1;
        }

        return { years, months, days };
      }

      function formatPlural(value, unit) {
        const v = Math.abs(value);
        return v + " " + unit + (v === 1 ? "" : "s");
      }

      function calculateAge(event) {
        event.preventDefault();
        const dobInput = document.getElementById("dob").value;
        const refDateInput = document.getElementById("ref-date").value;
        const dobTimeInput = document.getElementById("dob-time").value;
        const refTimeInput = document.getElementById("ref-time").value;
        const mode = document.getElementById("mode").value;
        const errorMsg = document.getElementById("error-msg");
        errorMsg.textContent = "";

        const dobDate = parseDateInput(dobInput);
        if (!dobDate) {
          errorMsg.textContent = "Please enter a valid date of birth.";
          return;
        }

        let refDate = refDateInput ? parseDateInput(refDateInput) : new Date();
        if (!refDate) {
          errorMsg.textContent = "Please enter a valid reference date.";
          return;
        }

        // Clone dates before applying times
        let dob = new Date(dobDate.getTime());
        let ref = new Date(refDate.getTime());

        // For total units, we want precise timestamps; if no time, assume midnight for both
        applyTimeToDate(dob, dobTimeInput, false);
        applyTimeToDate(ref, refTimeInput, false);

        let forward = true;
        if (dob.getTime() > ref.getTime()) {
          forward = false;
          // Swap to always compute positive difference
          const tmp = dob;
          dob = ref;
          ref = tmp;
        }

        // Calendar difference uses date-only (ignore time)
        const dobCal = new Date(
          dob.getFullYear(),
          dob.getMonth(),
          dob.getDate()
        );
        const refCal = new Date(
          ref.getFullYear(),
          ref.getMonth(),
          ref.getDate()
        );
        const calDiff = diffCalendar(dobCal, refCal);

        // Total difference in ms
        const msDiff = Math.abs(ref.getTime() - dob.getTime());
        const totalSeconds = Math.floor(msDiff / 1000);
        const totalMinutes = Math.floor(totalSeconds / 60);
        const totalHours = Math.floor(totalSeconds / 3600);
        const totalDays = Math.floor(totalSeconds / 86400);
        const totalWeeks = Math.floor(totalDays / 7);

        // Update UI
        document.getElementById("results-section").classList.remove("hidden");

        const years = calDiff.years;
        const months = calDiff.months;
        const days = calDiff.days;

        const directionText = forward
          ? mode === "age"
            ? "old"
            : "between the two dates"
          : mode === "age"
          ? "until this date"
          : "between the two dates (future to past)";

        let summary = "";
        if (mode === "age") {
          summary = forward
            ? `You are ${years} years, ${months} months, and ${days} days old.`
            : `There are ${years} years, ${months} months, and ${days} days until this date.`;
        } else {
          summary = `The difference is ${years} years, ${months} months, and ${days} days.`;
        }

        document.getElementById("age-summary").textContent = summary;

        const contextParts = [];
        contextParts.push(
          `${formatPlural(totalDays, "day")} (${formatPlural(
            totalWeeks,
            "week"
          )})`
        );
        contextParts.push(
          `${formatPlural(totalHours, "hour")}, ${formatPlural(
            totalMinutes,
            "minute"
          )}, and ${formatPlural(totalSeconds, "second")}`
        );

        document.getElementById("age-context").textContent =
          (mode === "age"
            ? forward
              ? "You have lived "
              : "Time remaining: "
            : "Total span: ") + contextParts.join(" ‚Äî ");

        document.getElementById("years-only").textContent = years;
        document.getElementById("months-only").textContent =
          years * 12 + months;
        document.getElementById("days-only").textContent = totalDays;
        document.getElementById("weeks-only").textContent = totalWeeks;

        document.getElementById("hours-total").textContent =
          totalHours.toLocaleString();
        document.getElementById("minutes-total").textContent =
          totalMinutes.toLocaleString();
        document.getElementById("seconds-total").textContent =
          totalSeconds.toLocaleString();

        // Birthday insights only make sense in age mode
        const birthdaySection = document.getElementById("birthday-section");
        if (mode === "age") {
          birthdaySection.classList.remove("hidden");

          const birthWeekday = dobCal.toLocaleDateString(undefined, {
            weekday: "long",
          });
          document.getElementById("birth-weekday").textContent = birthWeekday;

          // Next birthday based on reference calendar date (not time)
          const refForBirthday = forward ? refCal : new Date(); // if counting down to future, use now for "next birthday"
          let nextBirthdayYear = refForBirthday.getFullYear();
          const thisYearBirthday = new Date(
            nextBirthdayYear,
            dobCal.getMonth(),
            dobCal.getDate()
          );

          if (thisYearBirthday < refForBirthday) {
            nextBirthdayYear += 1;
          }

          let nextBirthday = new Date(
            nextBirthdayYear,
            dobCal.getMonth(),
            dobCal.getDate()
          );

          // Handle Feb 29 birthdays in non-leap years: default to Feb 28
          if (dobCal.getMonth() === 1 && dobCal.getDate() === 29) {
            if (
              nextBirthday.getMonth() !== 1 ||
              nextBirthday.getDate() !== 29
            ) {
              nextBirthday = new Date(nextBirthdayYear, 1, 28);
            }
          }

          const nbWeekday = nextBirthday.toLocaleDateString(undefined, {
            weekday: "long",
          });
          const nbDateStr = nextBirthday.toLocaleDateString(undefined, {
            year: "numeric",
            month: "long",
            day: "numeric",
          });
          document.getElementById(
            "next-birthday"
          ).textContent = `${nbWeekday}, ${nbDateStr}`;

          const nbDiffMs = nextBirthday.getTime() - refForBirthday.getTime();
          const nbDays = Math.round(nbDiffMs / 86400000);
          if (nbDays === 0) {
            document.getElementById("next-birthday-countdown").textContent =
              "Happy birthday! üéâ";
          } else if (nbDays > 0) {
            document.getElementById(
              "next-birthday-countdown"
            ).textContent = `${nbDays} day${
              nbDays === 1 ? "" : "s"
            } until your next birthday.`;
          } else {
            document.getElementById("next-birthday-countdown").textContent = "";
          }
        } else {
          birthdaySection.classList.add("hidden");
        }
      }

      document
        .getElementById("age-form")
        .addEventListener("submit", calculateAge);

      document
        .getElementById("reset-btn")
        .addEventListener("click", function () {
          document.getElementById("age-form").reset();
          document.getElementById("results-section").classList.add("hidden");
          document.getElementById("error-msg").textContent = "";
        });

      // Pre-fill reference date with today
      (function initDates() {
        const today = new Date();
        const yyyy = today.getFullYear();
        const mm = pad(today.getMonth() + 1, 2);
        const dd = pad(today.getDate(), 2);
        const todayStr = `${yyyy}-${mm}-${dd}`;
        const refInput = document.getElementById("ref-date");
        if (refInput && !refInput.value) {
          refInput.value = todayStr;
        }
      })();
    </script>

