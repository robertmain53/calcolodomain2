    <script>
      // Standard resistor series (E12 + some E24 for finer suggestions)
      const STANDARD_RESISTORS = [
        1.0, 1.1, 1.2, 1.3, 1.5, 1.6, 1.8, 2.0, 2.2, 2.4, 2.7, 3.0, 3.3, 3.6,
        3.9, 4.3, 4.7, 5.1, 5.6, 6.2, 6.8, 7.5, 8.2, 9.1,
      ];

      function nearestStandardResistor(value) {
        if (!isFinite(value) || value <= 0) return null;
        const decade = Math.pow(10, Math.floor(Math.log10(value)));
        const norm = value / decade;
        let best = STANDARD_RESISTORS[0],
          bestDiff = Math.abs(norm - STANDARD_RESISTORS[0]);
        for (const r of STANDARD_RESISTORS) {
          const diff = Math.abs(norm - r);
          if (diff < bestDiff) {
            bestDiff = diff;
            best = r;
          }
        }
        // Round up slightly to avoid going under the target resistance
        if (best < norm) {
          const idx = STANDARD_RESISTORS.indexOf(best);
          if (idx < STANDARD_RESISTORS.length - 1) {
            best = STANDARD_RESISTORS[idx + 1];
          } else {
            best = best;
          }
        }
        return +(best * decade).toPrecision(3);
      }

      function recommendPowerRating(P) {
        if (!isFinite(P) || P <= 0) return null;
        const safety = P * 2; // 2x margin
        const ratings = [0.125, 0.25, 0.5, 1, 2, 3, 5, 10, 25, 50, 100];
        for (const r of ratings) {
          if (safety <= r) return r;
        }
        return ratings[ratings.length - 1];
      }

      // Tab switching
      document.querySelectorAll(".mode-tab").forEach((btn) => {
        btn.addEventListener("click", () => {
          const mode = btn.dataset.mode;
          document.querySelectorAll(".mode-tab").forEach((b) => {
            b.classList.remove("border-blue-600", "text-blue-600");
            b.classList.add("border-transparent", "text-gray-600");
          });
          btn.classList.add("border-blue-600", "text-blue-600");
          btn.classList.remove("text-gray-600");

          document
            .querySelectorAll(".mode-panel")
            .forEach((p) => p.classList.add("hidden"));
          document.getElementById("mode-" + mode).classList.remove("hidden");
        });
      });

      // BASIC / SERIES CALC
      document.getElementById("calcBasic").addEventListener("click", () => {
        const Vs = parseFloat(document.getElementById("supplyVoltage").value);
        const Vf = parseFloat(
          document.getElementById("ledForwardVoltage").value
        );
        const If_mA = parseFloat(document.getElementById("ledCurrent").value);
        const N = parseInt(document.getElementById("ledCountSeries").value, 10);
        const warn = document.getElementById("basicWarning");
        warn.classList.add("hidden");
        warn.textContent = "";

        if (
          !isFinite(Vs) ||
          !isFinite(Vf) ||
          !isFinite(If_mA) ||
          !isFinite(N) ||
          Vs <= 0 ||
          Vf <= 0 ||
          If_mA <= 0 ||
          N <= 0
        ) {
          warn.textContent = "Please enter positive values for all fields.";
          warn.classList.remove("hidden");
          return;
        }

        const Vf_total = Vf * N;
        if (Vf_total >= Vs) {
          warn.textContent =
            "Total LED forward voltage is greater than or equal to supply voltage. Increase supply voltage or reduce LEDs in series.";
          warn.classList.remove("hidden");
          return;
        }

        const If = If_mA / 1000; // A
        const Vr = Vs - Vf_total;
        const R = Vr / If;
        const P = If * If * R;

        const Rstd = nearestStandardResistor(R);
        const Preco = recommendPowerRating(P);

        document.getElementById("basicRValue").textContent = R.toFixed(1);
        document.getElementById("basicRStandard").textContent = Rstd
          ? Rstd.toString()
          : "–";
        document.getElementById("basicPValue").textContent = P.toFixed(3);
        document.getElementById("basicPRecommended").textContent = Preco
          ? Preco + " W"
          : "–";
        document.getElementById("basicVfTotal").textContent =
          Vf_total.toFixed(2);
        document.getElementById("basicVr").textContent = Vr.toFixed(2);
        document.getElementById("basicResults").classList.remove("hidden");
      });

      document.getElementById("resetBasic").addEventListener("click", () => {
        document.getElementById("supplyVoltage").value = 5;
        document.getElementById("ledForwardVoltage").value = 2;
        document.getElementById("ledCurrent").value = 20;
        document.getElementById("ledCountSeries").value = 1;
        document.getElementById("basicResults").classList.add("hidden");
        const warn = document.getElementById("basicWarning");
        warn.classList.add("hidden");
        warn.textContent = "";
      });

      // PARALLEL BRANCH UI
      const branchContainer = document.getElementById("branchContainer");
      function renderBranches() {
        const count = parseInt(
          document.getElementById("parBranches").value,
          10
        );
        branchContainer.innerHTML = "";
        const n = Math.min(Math.max(count || 1, 1), 8);
        for (let i = 1; i <= n; i++) {
          const div = document.createElement("div");
          div.className = "border rounded-md p-3";
          div.innerHTML = `
        <div class="flex justify-between items-center mb-2">
          <span class="font-semibold text-sm">Branch ${i}</span>
          <span class="badge bg-gray-100 text-gray-600 border border-gray-200">R<sub>${i}</sub></span>
        </div>
        <div class="grid md:grid-cols-2 gap-3">
          <div>
            <label class="block text-xs font-medium text-gray-700">LED forward voltage (per LED)</label>
            <div class="mt-1 flex">
              <input type="number" step="0.01" class="branch-vf flex-1 border rounded-l-md px-2 py-1.5 focus:ring-blue-500 focus:border-blue-500" value="2">
              <span class="inline-flex items-center px-2 rounded-r-md border border-l-0 border-gray-300 bg-gray-50 text-gray-700 text-xs">V</span>
            </div>
          </div>
          <div>
            <label class="block text-xs font-medium text-gray-700">LEDs in series (this branch)</label>
            <input type="number" min="1" step="1" class="branch-n mt-1 w-full border rounded-md px-2 py-1.5 focus:ring-blue-500 focus:border-blue-500" value="1">
          </div>
          <div>
            <label class="block text-xs font-medium text-gray-700">Branch current</label>
            <div class="mt-1 flex">
              <input type="number" step="0.1" class="branch-i flex-1 border rounded-l-md px-2 py-1.5 focus:ring-blue-500 focus:border-blue-500" value="20">
              <span class="inline-flex items-center px-2 rounded-r-md border border-l-0 border-gray-300 bg-gray-50 text-gray-700 text-xs">mA</span>
            </div>
          </div>
        </div>
      `;
          branchContainer.appendChild(div);
        }
      }
      renderBranches();
      document
        .getElementById("parBranches")
        .addEventListener("change", renderBranches);

      document.getElementById("calcParallel").addEventListener("click", () => {
        const Vs = parseFloat(
          document.getElementById("parSupplyVoltage").value
        );
        const warn = document.getElementById("parWarning");
        warn.classList.add("hidden");
        warn.textContent = "";

        if (!isFinite(Vs) || Vs <= 0) {
          warn.textContent = "Please enter a valid positive supply voltage.";
          warn.classList.remove("hidden");
          return;
        }

        const branches = Array.from(
          branchContainer.querySelectorAll(".border.rounded-md.p-3")
        );
        const rows = [];
        let totalCurrent_mA = 0;
        let totalPower = 0;
        for (let idx = 0; idx < branches.length; idx++) {
          const b = branches[idx];
          const Vf = parseFloat(b.querySelector(".branch-vf").value);
          const N = parseInt(b.querySelector(".branch-n").value, 10);
          const I_mA = parseFloat(b.querySelector(".branch-i").value);
          if (
            !isFinite(Vf) ||
            !isFinite(N) ||
            !isFinite(I_mA) ||
            Vf <= 0 ||
            N <= 0 ||
            I_mA <= 0
          ) {
            warn.textContent =
              "Please enter positive values for all branch fields.";
            warn.classList.remove("hidden");
            return;
          }
          const Vf_total = Vf * N;
          if (Vf_total >= Vs) {
            warn.textContent = `Branch ${
              idx + 1
            }: total LED forward voltage is ≥ supply voltage. Reduce LEDs or increase supply.`;
            warn.classList.remove("hidden");
            return;
          }
          const I = I_mA / 1000;
          const Vr = Vs - Vf_total;
          const R = Vr / I;
          const P = I * I * R;
          const Rstd = nearestStandardResistor(R);
          const Preco = recommendPowerRating(P);

          totalCurrent_mA += I_mA;
          totalPower += P;

          rows.push({
            idx: idx + 1,
            Vf_total,
            I_mA,
            R,
            Rstd,
            P,
            Preco,
          });
        }

        let html = `
      <table class="min-w-full text-left border border-gray-200">
        <thead class="bg-gray-50">
          <tr>
            <th class="px-3 py-2 border-b text-xs font-semibold text-gray-700">Branch</th>
            <th class="px-3 py-2 border-b text-xs font-semibold text-gray-700">Vf,total (V)</th>
            <th class="px-3 py-2 border-b text-xs font-semibold text-gray-700">Current (mA)</th>
            <th class="px-3 py-2 border-b text-xs font-semibold text-gray-700">R (Ω)</th>
            <th class="px-3 py-2 border-b text-xs font-semibold text-gray-700">Std R (Ω)</th>
            <th class="px-3 py-2 border-b text-xs font-semibold text-gray-700">P (W)</th>
            <th class="px-3 py-2 border-b text-xs font-semibold text-gray-700">Rec. W</th>
          </tr>
        </thead>
        <tbody class="divide-y divide-gray-100">
    `;
        rows.forEach((r) => {
          html += `
        <tr>
          <td class="px-3 py-1.5 text-xs text-gray-700">#${r.idx}</td>
          <td class="px-3 py-1.5 text-xs font-mono">${r.Vf_total.toFixed(
            2
          )}</td>
          <td class="px-3 py-1.5 text-xs font-mono">${r.I_mA.toFixed(1)}</td>
          <td class="px-3 py-1.5 text-xs font-mono">${r.R.toFixed(1)}</td>
          <td class="px-3 py-1.5 text-xs font-mono">${r.Rstd ?? "–"}</td>
          <td class="px-3 py-1.5 text-xs font-mono">${r.P.toFixed(3)}</td>
          <td class="px-3 py-1.5 text-xs font-mono">${
            r.Preco ? r.Preco + " W" : "–"
          }</td>
        </tr>
      `;
        });
        html += "</tbody></table>";

        document.getElementById("parallelResultsTable").innerHTML = html;
        document.getElementById("parTotalCurrent").textContent =
          totalCurrent_mA.toFixed(1);
        document.getElementById("parTotalPower").textContent =
          totalPower.toFixed(3);
        document.getElementById("parallelResults").classList.remove("hidden");
      });

      // AUTOMOTIVE LOAD RESISTOR CALC
      document.getElementById("calcAuto").addEventListener("click", () => {
        const V = parseFloat(
          document.getElementById("autoSupplyVoltage").value
        );
        const Ph = parseFloat(
          document.getElementById("autoHalogenPower").value
        );
        const Pl = parseFloat(document.getElementById("autoLedPower").value);
        const bulbs = parseInt(
          document.getElementById("autoBulbsPerSide").value,
          10
        );
        const warn = document.getElementById("autoWarning");
        warn.classList.add("hidden");
        warn.textContent = "";

        if (
          !isFinite(V) ||
          !isFinite(Ph) ||
          !isFinite(Pl) ||
          !isFinite(bulbs) ||
          V <= 0 ||
          Ph <= 0 ||
          Pl < 0 ||
          bulbs <= 0
        ) {
          warn.textContent = "Please enter valid positive values.";
          warn.classList.remove("hidden");
          return;
        }

        const P_total_halogen = Ph * bulbs;
        const P_total_led = Pl * bulbs;
        const I_halogen = P_total_halogen / V;
        const I_led = P_total_led / V;
        let I_res = I_halogen - I_led;
        if (I_res <= 0) {
          warn.textContent =
            "LED power is already equal to or higher than halogen power; no load resistor is needed.";
          warn.classList.remove("hidden");
          return;
        }

        const R = V / I_res;
        const P = V * I_res;
        const Rstd = nearestStandardResistor(R);
        const Preco = recommendPowerRating(P);

        document.getElementById("autoRValue").textContent = R.toFixed(2);
        document.getElementById("autoRStandard").textContent = Rstd
          ? Rstd.toString()
          : "–";
        document.getElementById("autoPValue").textContent = P.toFixed(1);
        document.getElementById("autoPRecommended").textContent = Preco
          ? Preco + " W"
          : "–";
        document.getElementById("autoResults").classList.remove("hidden");
      });

      // Simple FAQ toggle (in addition to global script_faq if present)
      document
        .querySelectorAll("#faq-container .faq-question")
        .forEach((btn) => {
          btn.addEventListener("click", () => {
            const answer = btn.nextElementSibling;
            const icon = btn.querySelector("span:last-child");
            const isHidden = answer.classList.contains("hidden");
            document
              .querySelectorAll("#faq-container .faq-question + div")
              .forEach((div) => div.classList.add("hidden"));
            document
              .querySelectorAll("#faq-container .faq-question span:last-child")
              .forEach((i) => (i.textContent = "+"));
            if (isHidden) {
              answer.classList.remove("hidden");
              icon.textContent = "−";
            }
          });
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
