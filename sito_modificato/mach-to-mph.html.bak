  <script>
    // mobile menu toggle
    const mobileToggle = document.getElementById('mobile-menu-toggle');
    const mobileMenu = document.getElementById('mobile-menu');
    if (mobileToggle && mobileMenu) {
      mobileToggle.addEventListener('click', () => {
        const isHidden = mobileMenu.classList.contains('hidden');
        mobileMenu.classList.toggle('hidden', !isHidden);
        mobileToggle.setAttribute('aria-expanded', String(isHidden));
      });
    }

    function formatNumber(num) {
      if (isNaN(num)) return '–';
      return Number(num.toFixed(2)).toString();
    }

    const atmSelect = document.getElementById('atm');
    const customWrapper = document.getElementById('custom-sos-wrapper');

    atmSelect.addEventListener('change', function() {
      if (this.value === 'custom') {
        customWrapper.classList.remove('hidden');
      } else {
        customWrapper.classList.add('hidden');
      }
    });

    document.getElementById('convert-btn').addEventListener('click', function() {
      const mach = parseFloat(document.getElementById('mach').value);
      const atm = document.getElementById('atm').value;

      if (isNaN(mach)) {
        alert('Please enter a valid Mach number.');
        return;
      }

      let sosMph;
      switch (atm) {
        case 'sea':
          sosMph = 761.207;
          break;
        case '10000':
          sosMph = 734.0;
          break;
        case '20000':
          sosMph = 707.0;
          break;
        case '30000':
          sosMph = 679.0;
          break;
        case '36000':
          sosMph = 661.0;
          break;
        case 'custom':
          const customVal = parseFloat(document.getElementById('custom-sos').value);
          if (isNaN(customVal) || customVal <= 0) {
            alert('Please enter a valid custom speed of sound in mph.');
            return;
          }
          sosMph = customVal;
          break;
        default:
          sosMph = 761.207;
      }

      const mph = mach * sosMph;

      document.getElementById('res-mph').textContent = formatNumber(mph) + ' mph';
      document.getElementById('res-sos').textContent = formatNumber(sosMph) + ' mph';
    });
  </script>

