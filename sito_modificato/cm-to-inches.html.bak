            <script>
              document.addEventListener('DOMContentLoaded', () => {
                const cmInput = document.getElementById('cm-input');
                const inDecimalInput = document.getElementById('in-input');
                const ftInput = document.getElementById('ft-input');
                const inFracInput = document.getElementById('in-frac-input');
                const fractionOutput = document.getElementById('fraction-output');

                const inputs = [cmInput, inDecimalInput, ftInput, inFracInput];

                const CM_IN_INCH = 2.54;
                const IN_IN_FOOT = 12.0;
                
                // Funzione per formattare i numeri
                function formatNum(num, precision = 4) {
                    if (isNaN(num) || num === 0) return '';
                    return parseFloat(num.toFixed(precision)).toString();
                }

                // Funzione per calcolare il Massimo Comun Divisore (GCD)
                function gcd(a, b) {
                  return b === 0 ? a : gcd(b, a % b);
                }

                // Funzione per convertire decimali in frazioni
                function decimalToFraction(totalInches, maxDenominator = 64) {
                    if (isNaN(totalInches) || totalInches === 0) return '-';

                    const feet = Math.floor(totalInches / IN_IN_FOOT);
                    const inches = totalInches % IN_IN_FOOT;
                    
                    const wholeInches = Math.floor(inches);
                    let frac = inches - wholeInches;

                    let bestNum = 0;
                    let bestDen = 1;
                    
                    if (frac > 1/maxDenominator) {
                        let minError = Infinity;
                        for (let den = 2; den <= maxDenominator; den++) {
                            let num = Math.round(frac * den);
                            if (num === 0) continue;
                            let error = Math.abs(frac - (num / den));
                            if (error < minError) {
                                minError = error;
                                bestNum = num;
                                bestDen = den;
                            }
                        }
                        // Semplifica la frazione
                        let common = gcd(bestNum, bestDen);
                        bestNum / common;
                        bestDen / common;
                    }

                    let output = "";
                    if (feet > 0) {
                        output += `${feet}' `;
                    }
                    if (wholeInches > 0) {
                         output += `${wholeInches} `;
                    }
                    if (bestNum > 0) {
                        output += `${bestNum}/${bestDen}"`;
                    }
                    if (output === "") {
                        return "0\"";
                    }
                    return output.trim();
                }

                function calculate(source, value) {
                    let totalInches = 0;
                    let cm = 0;

                    if (isNaN(value) || value === 0) {
                        // If the input is 0 or empty, check if it is an ft-in input
                        if (source !== 'ft-in' || (parseFloat(ftInput.value) === 0 && parseFloat(inFracInput.value) === 0)) {
                             inputs.forEach(input => input.value = '');
                             fractionOutput.textContent = '-';
                             return;
                        }
                    }

                    if (source === 'cm') {
                        cm = value;
                        totalInches = cm / CM_IN_INCH;
                    } else if (source === 'in_decimal') {
                        totalInches = value;
                        cm = totalInches * CM_IN_INCH;
                    } else if (source === 'ft-in') {
                        const feet = parseFloat(ftInput.value) || 0;
                        const inches = parseFloat(inFracInput.value) || 0;
                        totalInches = (feet * IN_IN_FOOT) + inches;
                        cm = totalInches * CM_IN_INCH;
                    }

                    // Aggiorna tutti i campi tranne quelli sorgente
                    if (source !== 'cm') {
                        cmInput.value = formatNum(cm);
                    }
                    if (source !== 'in_decimal') {
                        inDecimalInput.value = formatNum(totalInches);
                    }
                    if (source !== 'ft-in') {
                        const feet = Math.floor(totalInches / IN_IN_FOOT);
                        const inches = totalInches % IN_IN_FOOT;
                        ftInput.value = formatNum(feet, 0);
                        inFracInput.value = formatNum(inches);
                    }
                    
                    // Aggiorna l'output frazionario
                    fractionOutput.textContent = decimalToFraction(totalInches);
                }

                inputs.forEach(input => {
                  input.addEventListener('input', (e) => {
                    const source = e.target.dataset.source;
                    const value = parseFloat(e.target.value);
                    calculate(source, value);
                  });
                });
              });
            </script>
            
