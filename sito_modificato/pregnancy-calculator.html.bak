    <script>
      // Utility functions
      function parseDateInput(id) {
        const val = document.getElementById(id).value;
        if (!val) return null;
        const d = new Date(val + "T12:00:00");
        return isNaN(d.getTime()) ? null : d;
      }

      function formatDate(date) {
        if (!date) return "–";
        const opts = { year: "numeric", month: "short", day: "numeric" };
        return date.toLocaleDateString(undefined, opts);
      }

      function addDays(date, days) {
        const d = new Date(date.getTime());
        d.setDate(d.getDate() + days);
        return d;
      }

      function diffInDays(a, b) {
        const msPerDay = 24 * 60 * 60 * 1000;
        const utcA = Date.UTC(a.getFullYear(), a.getMonth(), a.getDate());
        const utcB = Date.UTC(b.getFullYear(), b.getMonth(), b.getDate());
        return Math.round((utcA - utcB) / msPerDay);
      }

      function clamp(num, min, max) {
        return Math.min(Math.max(num, min), max);
      }

      // Tab switching
      const methodTabs = document.querySelectorAll(".method-tab");
      const methodForms = document.querySelectorAll(".method-form");
      let currentMethod = "lmp";

      methodTabs.forEach((tab) => {
        tab.addEventListener("click", () => {
          currentMethod = tab.dataset.method;
          methodTabs.forEach((t) => t.classList.remove("tab-active"));
          tab.classList.add("tab-active");
          methodForms.forEach((form) => {
            form.classList.toggle(
              "hidden",
              form.id !== "form-" + currentMethod
            );
          });
          document.getElementById("error-message").textContent = "";
        });
      });

      // Calculation logic
      document.getElementById("calculate-btn").addEventListener("click", () => {
        const errorEl = document.getElementById("error-message");
        errorEl.textContent = "";

        let estLmp = null;
        let edd = null;

        const today = new Date();
        today.setHours(12, 0, 0, 0);

        if (currentMethod === "lmp") {
          const lmp = parseDateInput("lmp-date");
          const cycleLen = parseInt(
            document.getElementById("cycle-length").value,
            10
          );
          if (!lmp || isNaN(cycleLen) || cycleLen < 20 || cycleLen > 45) {
            errorEl.textContent =
              "Please enter a valid LMP date and cycle length between 20 and 45 days.";
            return;
          }
          const adjustment = cycleLen - 28;
          estLmp = addDays(lmp, adjustment);
          edd = addDays(estLmp, 280);
        } else if (currentMethod === "conception") {
          const conc = parseDateInput("conception-date");
          if (!conc) {
            errorEl.textContent =
              "Please enter a valid conception or ovulation date.";
            return;
          }
          edd = addDays(conc, 266);
          estLmp = addDays(edd, -280);
        } else if (currentMethod === "ultrasound") {
          const usDate = parseDateInput("us-date");
          const usWeeks = parseInt(
            document.getElementById("us-weeks").value,
            10
          );
          const usDays = parseInt(
            document.getElementById("us-days").value || "0",
            10
          );
          if (
            !usDate ||
            isNaN(usWeeks) ||
            isNaN(usDays) ||
            usWeeks < 0 ||
            usWeeks > 42 ||
            usDays < 0 ||
            usDays > 6
          ) {
            errorEl.textContent =
              "Please enter a valid ultrasound date and gestational age (weeks and days).";
            return;
          }
          const gaDays = usWeeks * 7 + usDays;
          estLmp = addDays(usDate, -gaDays);
          edd = addDays(estLmp, 280);
        } else if (currentMethod === "ivf") {
          const transfer = parseDateInput("ivf-transfer-date");
          const embryoAge = parseInt(
            document.getElementById("ivf-embryo-age").value,
            10
          );
          if (!transfer || isNaN(embryoAge)) {
            errorEl.textContent =
              "Please enter a valid embryo transfer date and embryo age.";
            return;
          }
          const lmpOffset = 14 + embryoAge;
          estLmp = addDays(transfer, -lmpOffset);
          edd = addDays(estLmp, 280);
        }

        if (!estLmp || !edd) {
          errorEl.textContent =
            "Unable to calculate. Please check your inputs.";
          return;
        }

        // Gestational age today
        const gaDaysToday = diffInDays(today, estLmp);
        const gaWeeks = Math.floor(gaDaysToday / 7);
        const gaRemDays = gaDaysToday - gaWeeks * 7;

        // Weeks to due date
        const daysToDue = diffInDays(edd, today);
        const weeksToDue = daysToDue / 7;

        // Percent complete
        const percentComplete = clamp((gaDaysToday / 280) * 100, 0, 100);

        // Trimester
        let trimesterText = "";
        if (gaDaysToday < 0) {
          trimesterText = "Pregnancy not yet started (based on estimated LMP).";
        } else if (gaDaysToday <= 13 * 7 + 6) {
          trimesterText = "You are in the 1st trimester.";
        } else if (gaDaysToday <= 27 * 7 + 6) {
          trimesterText = "You are in the 2nd trimester.";
        } else {
          trimesterText = "You are in the 3rd trimester.";
        }

        // Milestone dates
        const endT1 = addDays(estLmp, 13 * 7 + 6);
        const endT2 = addDays(estLmp, 27 * 7 + 6);
        const startT3 = addDays(estLmp, 28 * 7);
        const viability = addDays(estLmp, 24 * 7);
        const fullTerm = addDays(estLmp, 39 * 7);

        // Update DOM
        document.getElementById("results-section").classList.remove("hidden");
        document.getElementById("result-edd").textContent = formatDate(edd);
        document.getElementById("result-edd-range").textContent =
          "Most babies arrive between " +
          formatDate(addDays(edd, -21)) +
          " and " +
          formatDate(addDays(edd, 14)) +
          ".";

        if (gaDaysToday < 0) {
          document.getElementById("result-ga").textContent =
            "Not yet pregnant (based on estimated LMP)";
        } else {
          document.getElementById("result-ga").textContent =
            gaWeeks + " weeks " + gaRemDays + " days";
        }
        document.getElementById("result-trimester").textContent = trimesterText;

        document.getElementById("result-lmp").textContent = formatDate(estLmp);
        document.getElementById("result-conception").textContent = formatDate(
          addDays(estLmp, 14)
        );
        document.getElementById("result-end-t1").textContent =
          formatDate(endT1);
        document.getElementById("result-end-t2").textContent =
          formatDate(endT2);

        document.getElementById("result-today").textContent = formatDate(today);
        document.getElementById("result-weeks-to-due").textContent =
          (weeksToDue >= 0 ? weeksToDue.toFixed(1) : "0.0") + " weeks";
        document.getElementById("result-percent-complete").textContent =
          percentComplete.toFixed(1) + "%";

        document.getElementById("result-viability").textContent =
          formatDate(viability);
        document.getElementById("result-start-t3").textContent =
          formatDate(startT3);
        document.getElementById("result-full-term").textContent =
          formatDate(fullTerm);

        document.getElementById("progress-bar").style.width =
          percentComplete + "%";

        // Trimester timeline text
        const t1Start = estLmp;
        const t2Start = addDays(estLmp, 14 * 7);
        const t3Start = startT3;
        const t1End = endT1;
        const t2End = endT2;
        const t3End = edd;

        document.getElementById("timeline-t1").textContent =
          formatDate(t1Start) + " – " + formatDate(t1End) + " (0–13w6d)";
        document.getElementById("timeline-t2").textContent =
          formatDate(t2Start) + " – " + formatDate(t2End) + " (14–27w6d)";
        document.getElementById("timeline-t3").textContent =
          formatDate(t3Start) + " – " + formatDate(t3End) + " (28–40w)";
      });

      document.getElementById("reset-btn").addEventListener("click", () => {
        document
          .querySelectorAll('input[type="date"], input[type="number"]')
          .forEach((el) => {
            if (el.id === "cycle-length") {
              el.value = 28;
            } else {
              el.value = "";
            }
          });
        document.getElementById("results-section").classList.add("hidden");
        document.getElementById("error-message").textContent = "";
        document.getElementById("progress-bar").style.width = "0%";
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
