    <script>
      // Mobile menu toggle
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          this.setAttribute("aria-expanded", isHidden ? "true" : "false");
        });

      // Mode switching
      const modeSelect = document.getElementById("mode");
      const beamSection = document.getElementById("beam-section");
      const colSection = document.getElementById("column-section");

      modeSelect.addEventListener("change", () => {
        if (modeSelect.value === "beam") {
          beamSection.classList.remove("hidden");
          colSection.classList.add("hidden");
        } else {
          beamSection.classList.add("hidden");
          colSection.classList.remove("hidden");
        }
      });

      // Helper: safe number
      function val(id) {
        const v = parseFloat(document.getElementById(id).value);
        return isFinite(v) ? v : 0;
      }

      // Beam calculations
      document.getElementById("beam-calc").addEventListener("click", () => {
        const fy = val("beam-fy"); // MPa
        const Wpl = val("beam-wpl"); // cm^3 (not directly used in simplified plastic method here)
        const Aa = val("beam-Aa"); // cm^2
        const za = val("beam-za"); // cm

        const fck = val("beam-fck"); // MPa
        const beff = val("beam-beff"); // mm
        const hc = val("beam-hc"); // mm
        const gammac = val("beam-gammac");
        const Prd = val("beam-Prd"); // kN
        const nstud = val("beam-nstud");
        const Med = val("beam-Med"); // kNm
        const gammam = val("beam-gammam");

        if (fy <= 0 || fck <= 0 || Aa <= 0 || beff <= 0 || hc <= 0 || za <= 0) {
          alert("Please enter positive values for all key inputs.");
          return;
        }

        // Unit conversions
        const Aa_m2 = Aa * 1e-4; // cm^2 -> m^2
        const Ac_eff_m2 = (beff / 1000) * (hc / 1000); // mm*mm -> m^2
        const za_m = za / 100; // cm -> m

        const fyd = fy / gammam; // MPa
        const fcd = (0.85 * fck) / gammac; // MPa

        // Forces in kN (1 MPa = 1 N/mm^2 = 1e6 N/m^2; 1 kN = 1e3 N)
        const NaRd = (Aa_m2 * fyd * 1e6) / 1e3; // kN
        const NcRd = (Ac_eff_m2 * fcd * 1e6) / 1e3; // kN

        const NplRd = Math.min(NaRd, NcRd); // kN
        const MplRd = NplRd * za_m; // kNm

        const sumPrd = Prd * nstud; // kN
        const eta = NcRd > 0 ? sumPrd / NcRd : 0;
        const util = MplRd > 0 ? Med / MplRd : 0;

        // Update UI
        document.getElementById("beam-NcRd").textContent = NcRd.toFixed(1);
        document.getElementById("beam-NaRd").textContent = NaRd.toFixed(1);
        document.getElementById("beam-NplRd").textContent = NplRd.toFixed(1);
        document.getElementById("beam-MplRd").textContent = MplRd.toFixed(1);
        document.getElementById("beam-sumPrd").textContent = sumPrd.toFixed(1);
        document.getElementById("beam-NcReq").textContent = NcRd.toFixed(1);
        document.getElementById("beam-eta").textContent = eta.toFixed(2);
        document.getElementById("beam-util").textContent = util.toFixed(2);

        const status = document.getElementById("beam-status");
        let msg = `Moment utilisation = ${(util * 100).toFixed(
          0
        )}%. Degree of shear connection η = ${eta.toFixed(2)}. `;
        if (util <= 1 && eta >= 0.8) {
          msg +=
            "For preliminary design, the section and shear connection look adequate, subject to full Eurocode 4 checks.";
          status.className = "mt-3 text-sm font-medium text-green-700";
        } else {
          msg +=
            "Adjust section size, slab dimensions or number of studs and re‑check according to EN 1994‑1‑1.";
          status.className = "mt-3 text-sm font-medium text-red-700";
        }

        document.getElementById("beam-results").classList.remove("hidden");
      });

      document.getElementById("beam-reset").addEventListener("click", () => {
        [
          "beam-fy",
          "beam-wpl",
          "beam-Aa",
          "beam-za",
          "beam-fck",
          "beam-beff",
          "beam-hc",
          "beam-gammac",
          "beam-Prd",
          "beam-nstud",
          "beam-Med",
          "beam-gammam",
        ].forEach(
          (id) =>
            (document.getElementById(id).value =
              document.getElementById(id).defaultValue)
        );
        document.getElementById("beam-results").classList.add("hidden");
      });

      // Column calculations
      document.getElementById("col-calc").addEventListener("click", () => {
        const fy = val("col-fy");
        const fck = val("col-fck");
        const gammaa = val("col-gammaa");
        const gammac = val("col-gammac");
        const Aa = val("col-Aa"); // cm^2
        const Ac = val("col-Ac"); // cm^2
        const Ned = val("col-Ned"); // kN
        const etaC = val("col-etaC");

        if (fy <= 0 || fck <= 0 || Aa <= 0 || Ac <= 0) {
          alert("Please enter positive values for all key inputs.");
          return;
        }

        const Aa_m2 = Aa * 1e-4;
        const Ac_m2 = Ac * 1e-4;

        const fyd = fy / gammaa;
        const fcd = fck / gammac;

        const NaRd = (Aa_m2 * fyd * 1e6) / 1e3; // kN
        const NcRd = (etaC * Ac_m2 * fcd * 1e6) / 1e3; // kN
        const NRd = NaRd + NcRd;
        const util = NRd > 0 ? Ned / NRd : 0;

        document.getElementById("col-NaRd").textContent = NaRd.toFixed(1);
        document.getElementById("col-NcRd").textContent = NcRd.toFixed(1);
        document.getElementById("col-NRd").textContent = NRd.toFixed(1);
        document.getElementById("col-util").textContent = util.toFixed(2);

        const status = document.getElementById("col-status");
        let msg = `Axial load utilisation = ${(util * 100).toFixed(0)}%. `;
        if (util <= 1) {
          msg +=
            "For a short column and ignoring buckling, the axial capacity appears sufficient. Perform full EN 1994‑1‑1 interaction and buckling checks.";
          status.className = "mt-3 text-sm font-medium text-green-700";
        } else {
          msg +=
            "Increase section size or concrete area, or reduce axial load, and re‑check with full Eurocode 4 interaction curves.";
          status.className = "mt-3 text-sm font-medium text-red-700";
        }

        document.getElementById("col-results").classList.remove("hidden");
      });

      document.getElementById("col-reset").addEventListener("click", () => {
        [
          "col-fy",
          "col-fck",
          "col-gammaa",
          "col-gammac",
          "col-Aa",
          "col-Ac",
          "col-Ned",
          "col-etaC",
        ].forEach(
          (id) =>
            (document.getElementById(id).value =
              document.getElementById(id).defaultValue)
        );
        document.getElementById("col-results").classList.add("hidden");
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
