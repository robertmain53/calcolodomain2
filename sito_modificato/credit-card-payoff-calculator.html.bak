    <script>
      // Utility: parse number safely
      function toNumber(val) {
        const n = parseFloat(val);
        return isNaN(n) ? 0 : n;
      }

      // Strategy tabs
      const tabs = {
        minimum: document.getElementById("tab-minimum"),
        snowball: document.getElementById("tab-snowball"),
        avalanche: document.getElementById("tab-avalanche"),
        custom: document.getElementById("tab-custom"),
      };
      let currentStrategy = "minimum";

      function setStrategy(strategy) {
        currentStrategy = strategy;
        Object.entries(tabs).forEach(([key, btn]) => {
          if (key === strategy) {
            btn.classList.remove("tab-inactive");
            btn.classList.add("tab-active");
            btn.setAttribute("aria-selected", "true");
          } else {
            btn.classList.remove("tab-active");
            btn.classList.add("tab-inactive");
            btn.setAttribute("aria-selected", "false");
          }
        });
        document.getElementById("custom-controls").style.display =
          strategy === "custom" ? "block" : "block"; // always visible but mainly used for custom
      }

      tabs.minimum.addEventListener("click", () => setStrategy("minimum"));
      tabs.snowball.addEventListener("click", () => setStrategy("snowball"));
      tabs.avalanche.addEventListener("click", () => setStrategy("avalanche"));
      tabs.custom.addEventListener("click", () => setStrategy("custom"));

      // Add/remove cards
      const cardsBody = document.getElementById("cards-body");
      document.getElementById("add-card-btn").addEventListener("click", () => {
        const rowCount = cardsBody.querySelectorAll("tr").length;
        const tr = document.createElement("tr");
        tr.innerHTML = `
      <td class="px-3 py-2">
        <input type="text" value="Card ${
          rowCount + 1
        }" class="w-full border-gray-300 rounded px-2 py-1 text-sm" aria-label="Card name">
      </td>
      <td class="px-3 py-2 text-right">
        <input type="number" min="0" step="0.01" value="1000" class="w-24 border-gray-300 rounded px-2 py-1 text-right" aria-label="Card balance">
      </td>
      <td class="px-3 py-2 text-right">
        <input type="number" min="0" step="0.01" value="18.99" class="w-20 border-gray-300 rounded px-2 py-1 text-right" aria-label="Card APR">
      </td>
      <td class="px-3 py-2 text-right">
        <input type="number" min="0" step="0.1" value="2" class="w-16 border-gray-300 rounded px-2 py-1 text-right" aria-label="Minimum payment percent">
      </td>
      <td class="px-3 py-2 text-right">
        <input type="number" min="0" step="1" value="25" class="w-16 border-gray-300 rounded px-2 py-1 text-right" aria-label="Minimum payment floor">
      </td>
      <td class="px-3 py-2 text-right">
        <input type="number" min="0" step="1" value="0" class="w-20 border-gray-300 rounded px-2 py-1 text-right custom-pay-input" aria-label="Custom payment amount">
      </td>
      <td class="px-3 py-2 text-center">
        <button type="button" class="text-red-500 hover:text-red-700 text-xs remove-card-btn">Remove</button>
      </td>
    `;
        cardsBody.appendChild(tr);
      });

      cardsBody.addEventListener("click", (e) => {
        if (e.target.classList.contains("remove-card-btn")) {
          const rows = cardsBody.querySelectorAll("tr");
          if (rows.length > 1) {
            e.target.closest("tr").remove();
          }
        }
      });

      // Slider sync with extra payment
      const sliderExtra = document.getElementById("slider-extra");
      const sliderExtraLabel = document.getElementById("slider-extra-label");
      const extraPaymentInput = document.getElementById("extra-payment-input");

      function syncSliderFromInput() {
        const val = Math.max(0, toNumber(extraPaymentInput.value));
        sliderExtra.value = Math.min(1000, val);
        sliderExtraLabel.textContent = sliderExtra.value;
      }

      function syncInputFromSlider() {
        extraPaymentInput.value = sliderExtra.value;
        sliderExtraLabel.textContent = sliderExtra.value;
      }

      sliderExtra.addEventListener("input", syncInputFromSlider);
      extraPaymentInput.addEventListener("input", syncSliderFromInput);
      syncSliderFromInput();

      // Core payoff simulation
      function getCardsData() {
        const rows = Array.from(cardsBody.querySelectorAll("tr"));
        return rows
          .map((row, index) => {
            const inputs = row.querySelectorAll("input");
            return {
              id: index,
              name: inputs[0].value || `Card ${index + 1}`,
              balance: toNumber(inputs[1].value),
              apr: toNumber(inputs[2].value),
              minPercent: toNumber(inputs[3].value) / 100,
              minFloor: toNumber(inputs[4].value),
              customPayment: toNumber(inputs[5].value),
            };
          })
          .filter((c) => c.balance > 0);
      }

      function simulatePayoff(strategy, extraMonthly, targetMonths) {
        const cards = getCardsData();
        if (!cards.length) return null;

        const monthlyRates = cards.map((c) => c.apr / 100 / 12);
        const originalBalances = cards.map((c) => c.balance);
        const minScenario = simulateMinimumOnly(cards, monthlyRates);

        // If targetMonths is provided, estimate required extraMonthly using simple search
        if (strategy === "custom" && targetMonths && targetMonths > 0) {
          let low = 0,
            high = 5000,
            best = 0;
          for (let i = 0; i < 20; i++) {
            const mid = (low + high) / 2;
            const res = simulateWithStrategy(
              cards,
              monthlyRates,
              "avalanche",
              mid
            ); // use avalanche for target
            if (!res) break;
            if (res.months <= targetMonths) {
              best = mid;
              high = mid;
            } else {
              low = mid;
            }
          }
          extraMonthly = best;
          extraPaymentInput.value = best.toFixed(0);
          syncSliderFromInput();
        }

        const mainStrategy =
          strategy === "minimum"
            ? "minimum"
            : strategy === "custom"
            ? "avalanche"
            : strategy;
        const result = simulateWithStrategy(
          cards,
          monthlyRates,
          mainStrategy,
          extraMonthly
        );

        if (!result) return null;

        // Attach baseline for comparison
        result.baseline = minScenario;
        result.originalBalances = originalBalances;
        return result;
      }

      function simulateMinimumOnly(cards, monthlyRates) {
        const balances = cards.map((c) => c.balance);
        const schedule = [];
        let month = 0;
        let totalInterest = 0;

        while (balances.some((b) => b > 0.01) && month < 600) {
          month++;
          for (let i = 0; i < cards.length; i++) {
            if (balances[i] <= 0.01) continue;
            const r = monthlyRates[i];
            const interest = balances[i] * r;
            let minPay = Math.max(
              balances[i] * cards[i].minPercent,
              cards[i].minFloor
            );
            if (minPay > balances[i] + interest)
              minPay = balances[i] + interest;
            const principal = minPay - interest;
            balances[i] = Math.max(0, balances[i] - principal);
            totalInterest += interest;
            schedule.push({
              month,
              cardIndex: i,
              payment: minPay,
              interest,
              principal,
              balance: balances[i],
            });
          }
        }

        return {
          months: month,
          totalInterest,
          schedule,
          perCard: cards.map((c, i) => {
            const cardSched = schedule.filter((s) => s.cardIndex === i);
            const last = cardSched[cardSched.length - 1];
            const cardInterest = cardSched.reduce(
              (sum, s) => sum + s.interest,
              0
            );
            const totalPaid = cardSched.reduce((sum, s) => sum + s.payment, 0);
            return {
              name: c.name,
              months: last ? last.month : 0,
              interest: cardInterest,
              totalPaid,
            };
          }),
        };
      }

      function simulateWithStrategy(
        cards,
        monthlyRates,
        strategy,
        extraMonthly
      ) {
        const balances = cards.map((c) => c.balance);
        const schedule = [];
        let month = 0;
        let totalInterest = 0;

        while (balances.some((b) => b > 0.01) && month < 600) {
          month++;
          // Determine order for snowball/avalanche
          let order = cards.map((c, i) => i);
          if (strategy === "snowball") {
            order.sort((a, b) => balances[a] - balances[b]);
          } else if (strategy === "avalanche") {
            order.sort((a, b) => cards[b].apr - cards[a].apr);
          }

          // First compute minimums
          const minPayments = balances.map((bal, i) => {
            if (bal <= 0.01) return 0;
            const interest = bal * monthlyRates[i];
            let minPay = Math.max(bal * cards[i].minPercent, cards[i].minFloor);
            if (minPay > bal + interest) minPay = bal + interest;
            return minPay;
          });

          let totalMin = minPayments.reduce((a, b) => a + b, 0);
          let extraPool = Math.max(0, extraMonthly || 0);

          // Apply custom per-card payments if provided (for 'custom' strategy)
          const customExtra = new Array(cards.length).fill(0);
          if (strategy === "custom") {
            cards.forEach((c, i) => {
              if (c.customPayment > 0 && balances[i] > 0.01) {
                const base = minPayments[i];
                const target = Math.max(base, c.customPayment);
                const diff = target - base;
                if (diff > 0) {
                  customExtra[i] = diff;
                }
              }
            });
          }

          // Apply payments per card
          for (const i of order) {
            if (balances[i] <= 0.01) continue;
            const r = monthlyRates[i];
            const interest = balances[i] * r;
            let payment = minPayments[i];

            // Add custom extra for this card
            if (strategy === "custom" && customExtra[i] > 0) {
              payment += customExtra[i];
            }

            // Add shared extra pool focusing on current card
            if (extraPool > 0 && strategy !== "minimum") {
              const maxExtra = balances[i] + interest - payment;
              if (maxExtra > 0) {
                const applied = Math.min(extraPool, maxExtra);
                payment += applied;
                extraPool -= applied;
              }
            }

            if (payment <= 0) continue;
            if (payment > balances[i] + interest)
              payment = balances[i] + interest;

            const principal = payment - interest;
            balances[i] = Math.max(0, balances[i] - principal);
            totalInterest += interest;

            schedule.push({
              month,
              cardIndex: i,
              payment,
              interest,
              principal,
              balance: balances[i],
            });
          }

          // Safety break if payments are too low to cover interest
          const totalBalance = balances.reduce((a, b) => a + b, 0);
          if (
            totalBalance > 0 &&
            totalMin + (extraMonthly || 0) <
              totalBalance * Math.min(...monthlyRates)
          ) {
            break;
          }
        }

        const lastMonth = schedule.reduce(
          (max, s) => Math.max(max, s.month),
          0
        );

        return {
          months: lastMonth,
          totalInterest,
          schedule,
          perCard: cards.map((c, i) => {
            const cardSched = schedule.filter((s) => s.cardIndex === i);
            const last = cardSched[cardSched.length - 1];
            const cardInterest = cardSched.reduce(
              (sum, s) => sum + s.interest,
              0
            );
            const totalPaid = cardSched.reduce((sum, s) => sum + s.payment, 0);
            return {
              name: c.name,
              months: last ? last.month : 0,
              interest: cardInterest,
              totalPaid,
            };
          }),
        };
      }

      function formatCurrency(n) {
        return "$" + n.toFixed(2).replace(/\B(?=(\d{3})+(?!\d))/g, ",");
      }

      function monthsToYearsMonths(m) {
        const years = Math.floor(m / 12);
        const months = m % 12;
        const parts = [];
        if (years > 0) parts.push(years + " year" + (years > 1 ? "s" : ""));
        if (months > 0) parts.push(months + " month" + (months > 1 ? "s" : ""));
        return parts.length ? parts.join(" ") : "0 months";
      }

      function estimateDebtFreeDate(months) {
        const now = new Date();
        const future = new Date(
          now.getFullYear(),
          now.getMonth() + months,
          now.getDate()
        );
        return future.toLocaleDateString(undefined, {
          year: "numeric",
          month: "short",
        });
      }

      document.getElementById("calculate-btn").addEventListener("click", () => {
        const extraMonthly =
          currentStrategy === "minimum" ? 0 : toNumber(extraPaymentInput.value);
        const targetMonths = toNumber(
          document.getElementById("target-months-input").value
        );
        const result = simulatePayoff(
          currentStrategy,
          extraMonthly,
          targetMonths
        );

        const resultsSection = document.getElementById("results-section");
        if (!result) {
          resultsSection.classList.add("hidden");
          return;
        }

        const totalOriginal = result.originalBalances.reduce(
          (a, b) => a + b,
          0
        );
        const totalPaid = totalOriginal + result.totalInterest;

        document.getElementById("result-debtfree-date").textContent =
          estimateDebtFreeDate(result.months);
        document.getElementById("result-debtfree-months").textContent =
          monthsToYearsMonths(result.months);
        document.getElementById("result-total-interest").textContent =
          formatCurrency(result.totalInterest);
        document.getElementById("result-total-paid").textContent =
          "Total paid: " + formatCurrency(totalPaid);

        if (result.baseline) {
          const interestSaved = Math.max(
            0,
            result.baseline.totalInterest - result.totalInterest
          );
          const monthsSaved = Math.max(
            0,
            result.baseline.months - result.months
          );
          document.getElementById("result-interest-saved").textContent =
            formatCurrency(interestSaved);
          document.getElementById("result-months-saved").textContent =
            "Months saved: " + monthsSaved;
        } else {
          document.getElementById("result-interest-saved").textContent = "$0";
          document.getElementById("result-months-saved").textContent =
            "Months saved: –";
        }

        // Per-card table
        const perCardTbody = document.getElementById("per-card-results");
        perCardTbody.innerHTML = "";
        result.perCard.forEach((card) => {
          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="px-3 py-1.5">${card.name}</td>
        <td class="px-3 py-1.5 text-right">${monthsToYearsMonths(
          card.months
        )}</td>
        <td class="px-3 py-1.5 text-right">${formatCurrency(card.interest)}</td>
        <td class="px-3 py-1.5 text-right">${formatCurrency(
          card.totalPaid
        )}</td>
      `;
          perCardTbody.appendChild(tr);
        });

        // Amortization table (limit rows for performance)
        const amortBody = document.getElementById("amortization-body");
        amortBody.innerHTML = "";
        const maxRows = 600; // cap for display
        result.schedule.slice(0, maxRows).forEach((row) => {
          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="px-2 py-1">${row.month}</td>
        <td class="px-2 py-1">${result.perCard[row.cardIndex].name}</td>
        <td class="px-2 py-1 text-right">${formatCurrency(row.payment)}</td>
        <td class="px-2 py-1 text-right">${formatCurrency(row.interest)}</td>
        <td class="px-2 py-1 text-right">${formatCurrency(row.principal)}</td>
        <td class="px-2 py-1 text-right">${formatCurrency(row.balance)}</td>
      `;
          amortBody.appendChild(tr);
        });
        if (result.schedule.length > maxRows) {
          const tr = document.createElement("tr");
          tr.innerHTML = `<td colspan="6" class="px-2 py-2 text-center text-xs text-gray-500">Schedule truncated to first ${maxRows} rows for display.</td>`;
          amortBody.appendChild(tr);
        }

        resultsSection.classList.remove("hidden");
        resultsSection.scrollIntoView({ behavior: "smooth", block: "start" });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
