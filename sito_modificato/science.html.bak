    <script>
      // Science Explorer logic
      (function () {
        const topicSelect = document.getElementById("science-topic");
        const panels = {
          gibbs: document.getElementById("panel-gibbs"),
          arrhenius: document.getElementById("panel-arrhenius"),
          centripetal: document.getElementById("panel-centripetal"),
          hydrostatic: document.getElementById("panel-hydrostatic"),
          stoichiometry: document.getElementById("panel-stoich"),
        };
        const calcBtn = document.getElementById("science-calc-btn");
        const resetBtn = document.getElementById("science-reset-btn");
        const errorEl = document.getElementById("science-error");
        const resultMain = document.getElementById("science-result-main");
        const resultDetail = document.getElementById("science-result-detail");

        function showPanel(key) {
          Object.keys(panels).forEach((k) => {
            panels[k].classList.toggle("hidden", k !== key);
          });
          errorEl.classList.add("hidden");
          resultMain.textContent = "Enter values and click “Calculate”.";
          resultDetail.textContent = "";
        }

        topicSelect.addEventListener("change", () => {
          showPanel(topicSelect.value);
        });

        function isInvalidNumber(v) {
          return typeof v !== "number" || !isFinite(v);
        }

        calcBtn.addEventListener("click", () => {
          const topic = topicSelect.value;
          errorEl.classList.add("hidden");

          try {
            if (topic === "gibbs") {
              const dH = parseFloat(document.getElementById("gibbs-dh").value); // kJ/mol
              const dS_J = parseFloat(
                document.getElementById("gibbs-ds").value
              ); // J/molK
              const T = parseFloat(document.getElementById("gibbs-T").value); // K
              if ([dH, dS_J, T].some(isInvalidNumber)) throw new Error();
              const dS_kJ = dS_J / 1000;
              const dG = dH - T * dS_kJ;
              resultMain.textContent = `ΔG = ${dG.toFixed(3)} kJ/mol`;
              resultDetail.textContent = `Using ΔG = ΔH − TΔS with ΔH = ${dH} kJ/mol, ΔS = ${dS_J} J/mol·K (${dS_kJ.toFixed(
                4
              )} kJ/mol·K), T = ${T} K.`;
            } else if (topic === "arrhenius") {
              const A = parseFloat(document.getElementById("arr-A").value);
              const Ea_kJ = parseFloat(document.getElementById("arr-Ea").value);
              const T = parseFloat(document.getElementById("arr-T").value);
              if ([A, Ea_kJ, T].some(isInvalidNumber)) throw new Error();
              const R = 8.314; // J/molK
              const Ea_J = Ea_kJ * 1000;
              const exponent = -Ea_J / (R * T);
              const k = A * Math.exp(exponent);
              resultMain.textContent = `k ≈ ${k.toExponential(3)} s⁻¹`;
              resultDetail.textContent = `Using k = A·e^(−Ea/RT) with A = ${A}, Ea = ${Ea_kJ} kJ/mol, T = ${T} K, R = 8.314 J/mol·K.`;
            } else if (topic === "centripetal") {
              const m = parseFloat(document.getElementById("cent-m").value);
              const v = parseFloat(document.getElementById("cent-v").value);
              const r = parseFloat(document.getElementById("cent-r").value);
              if ([m, v, r].some(isInvalidNumber) || r === 0) throw new Error();
              const F = (m * v * v) / r;
              resultMain.textContent = `F = ${F.toFixed(3)} N`;
              resultDetail.textContent = `Using F = mv²/r with m = ${m} kg, v = ${v} m/s, r = ${r} m.`;
            } else if (topic === "hydrostatic") {
              const rho = parseFloat(
                document.getElementById("hydro-rho").value
              );
              const h = parseFloat(document.getElementById("hydro-h").value);
              const g = parseFloat(document.getElementById("hydro-g").value);
              if ([rho, h, g].some(isInvalidNumber)) throw new Error();
              const P = rho * g * h;
              resultMain.textContent = `P = ${P.toFixed(1)} Pa`;
              resultDetail.textContent = `Using P = ρgh with ρ = ${rho} kg/m³, g = ${g} m/s², h = ${h} m.`;
            } else if (topic === "stoichiometry") {
              const nA = parseFloat(document.getElementById("st-a").value);
              const nB = parseFloat(document.getElementById("st-b").value);
              if ([nA, nB].some(isInvalidNumber) || nA < 0 || nB < 0)
                throw new Error();
              const limiting =
                nA < nB
                  ? "A"
                  : nB < nA
                  ? "B"
                  : "A and B (exactly stoichiometric)";
              const nC = Math.min(nA, nB);
              resultMain.textContent = `Theoretical moles of C = ${nC.toFixed(
                4
              )} mol`;
              resultDetail.textContent = `Reaction: A + B → C (1:1). Limiting reagent: ${limiting}. Starting with n(A) = ${nA} mol, n(B) = ${nB} mol.`;
            }
          } catch (e) {
            errorEl.classList.remove("hidden");
            resultMain.textContent =
              "Calculation failed – please check your inputs.";
            resultDetail.textContent = "";
          }
        });

        resetBtn.addEventListener("click", () => {
          // Reset to defaults
          document.getElementById("gibbs-dh").value = -100;
          document.getElementById("gibbs-ds").value = 50;
          document.getElementById("gibbs-T").value = 298;

          document.getElementById("arr-A").value = 1e12;
          document.getElementById("arr-Ea").value = 50;
          document.getElementById("arr-T").value = 298;

          document.getElementById("cent-m").value = 1;
          document.getElementById("cent-v").value = 5;
          document.getElementById("cent-r").value = 2;

          document.getElementById("hydro-rho").value = 1000;
          document.getElementById("hydro-h").value = 5;
          document.getElementById("hydro-g").value = 9.81;

          document.getElementById("st-a").value = 0.5;
          document.getElementById("st-b").value = 0.2;

          errorEl.classList.add("hidden");
          resultMain.textContent =
            "Choose a topic, enter values, and click “Calculate”.";
          resultDetail.textContent = "";
        });

        // Initialize default panel
        showPanel("gibbs");
      })();

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
