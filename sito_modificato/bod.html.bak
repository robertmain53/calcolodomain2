    <script>
      // Tab switching
      document.querySelectorAll(".bod-tab").forEach((btn) => {
        btn.addEventListener("click", () => {
          const target = btn.getAttribute("data-tab");
          document.querySelectorAll(".bod-tab").forEach((b) => {
            b.classList.remove("border-blue-600", "text-blue-600");
            b.classList.add("border-transparent", "text-gray-600");
          });
          btn.classList.add("border-blue-600", "text-blue-600");
          btn.classList.remove("text-gray-600");

          document.querySelectorAll(".bod-panel").forEach((panel) => {
            panel.classList.add("hidden");
          });
          document.getElementById(target).classList.remove("hidden");
        });
      });

      function toLiters(value, unit) {
        if (!isFinite(value)) return NaN;
        return unit === "mL" ? value / 1000 : value;
      }

      // BOD5 calculation
      function calcBOD5() {
        const Vb = parseFloat(
          document.getElementById("bod5-bottle-volume").value
        );
        const VbUnit = document.getElementById("bod5-bottle-unit").value;
        const Vs = parseFloat(
          document.getElementById("bod5-sample-volume").value
        );
        const VsUnit = document.getElementById("bod5-sample-unit").value;
        const D1 = parseFloat(document.getElementById("bod5-d1").value);
        const D2 = parseFloat(document.getElementById("bod5-d2").value);
        const B1 = parseFloat(document.getElementById("bod5-blank-d1").value);
        const B2 = parseFloat(document.getElementById("bod5-blank-d2").value);
        const f = parseFloat(document.getElementById("bod5-seed-factor").value);

        if (!isFinite(Vb) || !isFinite(Vs) || !isFinite(D1) || !isFinite(D2)) {
          alert(
            "Please enter bottle volume, sample volume, and both DO values."
          );
          return;
        }

        const VbL = toLiters(Vb, VbUnit);
        const VsL = toLiters(Vs, VsUnit);
        if (!isFinite(VbL) || !isFinite(VsL) || VbL <= 0 || VsL <= 0) {
          alert("Volumes must be positive numbers.");
          return;
        }

        let B = 0;
        let useBlank = false;
        if (isFinite(B1) && isFinite(B2)) {
          B = B1 - B2;
          useBlank = true;
        }
        let seedFactor = 0;
        if (useBlank && isFinite(f)) {
          seedFactor = B * f;
        }

        const depletion = D1 - D2 - seedFactor;
        if (depletion <= 0) {
          alert(
            "Computed DO depletion is zero or negative. Check your inputs."
          );
          return;
        }

        const bod5 = depletion * (VbL / VsL);
        const resultBox = document.getElementById("bod5-result");
        const main = document.getElementById("bod5-main");
        const details = document.getElementById("bod5-details");

        main.textContent = `BOD₅ ≈ ${bod5.toFixed(1)} mg/L`;
        let detailText = `Depletion used: (D₁ − D₂`;
        if (useBlank && isFinite(f)) {
          detailText += ` − B × f) = (${D1.toFixed(2)} − ${D2.toFixed(
            2
          )} − ${B.toFixed(2)} × ${f.toFixed(2)})`;
        } else {
          detailText += `) = (${D1.toFixed(2)} − ${D2.toFixed(2)})`;
        }
        detailText += ` = ${depletion.toFixed(
          2
        )} mg/L. Dilution factor = Vbottle / Vsample = ${(VbL / VsL).toFixed(
          2
        )}.`;
        details.textContent = detailText;

        resultBox.classList.remove("hidden");
      }

      document
        .getElementById("bod5-calc-btn")
        .addEventListener("click", calcBOD5);
      document
        .getElementById("bod5-reset-btn")
        .addEventListener("click", () => {
          document.getElementById("bod5-bottle-volume").value = 300;
          document.getElementById("bod5-bottle-unit").value = "mL";
          document.getElementById("bod5-sample-volume").value = 15;
          document.getElementById("bod5-sample-unit").value = "mL";
          document.getElementById("bod5-d1").value = 8.5;
          document.getElementById("bod5-d2").value = 2.1;
          document.getElementById("bod5-blank-d1").value = "";
          document.getElementById("bod5-blank-d2").value = "";
          document.getElementById("bod5-seed-factor").value = "";
          document.getElementById("bod5-result").classList.add("hidden");
        });

      // BODu & k calculation
      function calcBODuK() {
        const BODt = parseFloat(document.getElementById("bodu-bod5").value);
        const t = parseFloat(document.getElementById("bodu-time").value);
        const kInput = parseFloat(document.getElementById("bodu-k").value);
        const BODuInput = parseFloat(
          document.getElementById("bodu-bodu").value
        );

        if (!isFinite(BODt) || !isFinite(t) || t <= 0) {
          alert("Please enter a valid BOD at time t and a positive time.");
          return;
        }

        const hasK = isFinite(kInput) && kInput > 0;
        const hasBODu = isFinite(BODuInput) && BODuInput > 0;

        if (!hasK && !hasBODu) {
          alert(
            "Enter either a positive k or a positive BODu to solve for the other."
          );
          return;
        }

        let k = kInput;
        let BODu = BODuInput;

        if (hasK && !hasBODu) {
          const factor = 1 - Math.exp(-k * t);
          if (factor <= 0 || factor >= 1) {
            alert(
              "The combination of k and t leads to an invalid factor. Adjust inputs."
            );
            return;
          }
          BODu = BODt / factor;
        } else if (!hasK && hasBODu) {
          const ratio = BODt / BODu;
          if (ratio <= 0 || ratio >= 1) {
            alert("BODt must be between 0 and BODu to solve for k.");
            return;
          }
          k = -Math.log(1 - ratio) / t;
        } else if (hasK && hasBODu) {
          // Both provided: just report and check consistency
          const ratio = BODt / BODu;
          if (ratio <= 0 || ratio >= 1) {
            alert(
              "Provided BODt and BODu are inconsistent (BODt must be between 0 and BODu)."
            );
            return;
          }
        }

        const resultBox = document.getElementById("bodu-result");
        const main = document.getElementById("bodu-main");
        const details = document.getElementById("bodu-details");

        main.textContent = `Ultimate BOD (BODu) ≈ ${BODu.toFixed(
          1
        )} mg/L,  k ≈ ${k.toFixed(3)} day⁻¹`;
        const fraction = BODt / BODu;
        details.textContent =
          `Using first‑order kinetics: BODₜ = BODu × (1 − e^(−k t)). At t = ${t.toFixed(
            2
          )} days, ` +
          `BODₜ = ${BODt.toFixed(1)} mg/L, which is ${(fraction * 100).toFixed(
            1
          )}% of BODu.`;

        resultBox.classList.remove("hidden");
      }

      document
        .getElementById("bodu-calc-btn")
        .addEventListener("click", calcBODuK);
      document
        .getElementById("bodu-reset-btn")
        .addEventListener("click", () => {
          document.getElementById("bodu-bod5").value = 200;
          document.getElementById("bodu-time").value = 5;
          document.getElementById("bodu-k").value = 0.23;
          document.getElementById("bodu-bodu").value = "";
          document.getElementById("bodu-result").classList.add("hidden");
        });

      // BOD vs time calculation
      function calcBODvsTime() {
        const BODu = parseFloat(document.getElementById("time-bodu").value);
        const k = parseFloat(document.getElementById("time-k").value);
        const t = parseFloat(document.getElementById("time-t").value);
        const tMax = parseFloat(document.getElementById("time-max").value);

        if (
          !isFinite(BODu) ||
          BODu <= 0 ||
          !isFinite(k) ||
          k <= 0 ||
          !isFinite(t) ||
          t < 0
        ) {
          alert(
            "Please enter positive values for BODu, k, and a non‑negative time t."
          );
          return;
        }

        const BODt = BODu * (1 - Math.exp(-k * t));
        const resultBox = document.getElementById("time-result");
        const main = document.getElementById("time-main");
        const tableWrapper = document.getElementById("time-table-wrapper");
        const tbody = document.getElementById("time-table-body");

        main.textContent =
          `At t = ${t.toFixed(2)} days, BODₜ ≈ ${BODt.toFixed(1)} mg/L ` +
          `(${((BODt / BODu) * 100).toFixed(1)}% of BODu = ${BODu.toFixed(
            1
          )} mg/L).`;

        tbody.innerHTML = "";
        if (isFinite(tMax) && tMax > 0) {
          const steps = 10;
          const dt = tMax / steps;
          for (let i = 0; i <= steps; i++) {
            const ti = dt * i;
            const Bi = BODu * (1 - Math.exp(-k * ti));
            const tr = document.createElement("tr");
            tr.innerHTML =
              `<td class="px-2 py-1 border-b border-gray-200">${ti.toFixed(
                1
              )}</td>` +
              `<td class="px-2 py-1 border-b border-gray-200">${Bi.toFixed(
                1
              )}</td>` +
              `<td class="px-2 py-1 border-b border-gray-200">${(
                (Bi / BODu) *
                100
              ).toFixed(1)}%</td>`;
            tbody.appendChild(tr);
          }
          tableWrapper.classList.remove("hidden");
        } else {
          tableWrapper.classList.add("hidden");
        }

        resultBox.classList.remove("hidden");
      }

      document
        .getElementById("time-calc-btn")
        .addEventListener("click", calcBODvsTime);
      document
        .getElementById("time-reset-btn")
        .addEventListener("click", () => {
          document.getElementById("time-bodu").value = 250;
          document.getElementById("time-k").value = 0.23;
          document.getElementById("time-t").value = 5;
          document.getElementById("time-max").value = 20;
          document.getElementById("time-result").classList.add("hidden");
          document.getElementById("time-table-wrapper").classList.add("hidden");
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
