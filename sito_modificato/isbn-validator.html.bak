  <script>
    // mobile menu toggle
    const mobileToggle = document.getElementById('mobile-menu-toggle');
    const mobileMenu = document.getElementById('mobile-menu');
    if (mobileToggle && mobileMenu) {
      mobileToggle.addEventListener('click', () => {
        const isHidden = mobileMenu.classList.contains('hidden');
        mobileMenu.classList.toggle('hidden', !isHidden);
        mobileToggle.setAttribute('aria-expanded', String(isHidden));
      });
    }

    const isbnInput = document.getElementById('isbn-input');
    const validateBtn = document.getElementById('validate-btn');
    const inputError = document.getElementById('input-error');
    const isbnType = document.getElementById('isbn-type');
    const isbnValid = document.getElementById('isbn-valid');
    const isbnNormalized = document.getElementById('isbn-normalized');
    const isbn10to13 = document.getElementById('isbn10-to-13');
    const isbn13to10 = document.getElementById('isbn13-to-10');
    const stepsContainer = document.getElementById('steps-container');

    function normalizeISBN(str) {
      return str.replace(/[^0-9Xx]/g, '').toUpperCase();
    }

    function isISBN10(str) {
      return str.length === 10;
    }
    function isISBN13(str) {
      return str.length === 13;
    }

    function checksumISBN10(str) {
      // str: 10 chars, last might be X
      let sum = 0;
      const steps = [];
      for (let i = 0; i < 10; i++) {
        const char = str[i];
        const digit = (i === 9 && char === 'X') ? 10 : parseInt(char, 10);
        if (isNaN(digit)) return {valid:false, steps:[]};
        const term = (i+1) * digit;
        sum += term;
        steps.push({i: i+1, digit, term});
      }
      return {valid: sum % 11 === 0, sum, steps};
    }

    function checksumISBN13(str) {
      let sum = 0;
      const steps = [];
      for (let i = 0; i < 13; i++) {
        const digit = parseInt(str[i], 10);
        if (isNaN(digit)) return {valid:false, steps:[]};
        const weight = (i % 2 === 0) ? 1 : 3;
        const term = digit * weight;
        sum += term;
        steps.push({i: i+1, digit, weight, term});
      }
      return {valid: sum % 10 === 0, sum, steps};
    }

    function computeISBN13CheckDigit(first12) {
      let sum = 0;
      for (let i = 0; i < 12; i++) {
        const d = parseInt(first12[i], 10);
        sum += d * (i % 2 === 0 ? 1 : 3);
      }
      const mod = sum % 10;
      return mod === 0 ? 0 : 10 - mod;
    }

    function convert10to13(isbn10) {
      // take first 9 digits, prepend 978
      const core9 = isbn10.slice(0, 9);
      const base12 = "978" + core9;
      const check = computeISBN13CheckDigit(base12);
      return base12 + check.toString();
    }

    function convert13to10(isbn13) {
      // only if starts with 978
      if (!isbn13.startsWith("978")) return null;
      const core9 = isbn13.slice(3, 12); // positions 4..12
      // compute isbn10 check digit
      let sum = 0;
      for (let i = 0; i < 9; i++) {
        sum += (i+1) * parseInt(core9[i], 10);
      }
      let check = sum % 11;
      const checkChar = check === 10 ? 'X' : check.toString();
      return core9 + checkChar;
    }

    function renderStepsFor10(res) {
      const {steps, sum, valid} = res;
      let html = '<p class="font-semibold">ISBN-10 checksum steps</p>';
      html += '<div class="overflow-x-auto"><table class="calc-table mt-2"><thead><tr><th>i</th><th>digit</th><th>i × digit</th></tr></thead><tbody>';
      steps.forEach(s => {
        html += `<tr><td>${s.i}</td><td>${s.digit}</td><td>${s.term}</td></tr>`;
      });
      html += `</tbody><tfoot><tr><th colspan="2" class="text-right">Sum</th><th>${sum}</th></tr></tfoot></table></div>`;
      html += `<p class="mt-2 text-sm">${sum} mod 11 = ${sum % 11} → ${valid ? '<span class="text-green-600 font-semibold">VALID</span>' : '<span class="text-red-600 font-semibold">NOT valid</span>'}</p>`;
      stepsContainer.innerHTML = html;
    }

    function renderStepsFor13(res) {
      const {steps, sum, valid} = res;
      let html = '<p class="font-semibold">ISBN-13 checksum steps</p>';
      html += '<div class="overflow-x-auto"><table class="calc-table mt-2"><thead><tr><th>pos</th><th>digit</th><th>weight</th><th>digit × weight</th></tr></thead><tbody>';
      steps.forEach(s => {
        html += `<tr><td>${s.i}</td><td>${s.digit}</td><td>${s.weight}</td><td>${s.term}</td></tr>`;
      });
      html += `</tbody><tfoot><tr><th colspan="3" class="text-right">Sum</th><th>${sum}</th></tr></tfoot></table></div>`;
      html += `<p class="mt-2 text-sm">${sum} mod 10 = ${sum % 10} → ${valid ? '<span class="text-green-600 font-semibold">VALID</span>' : '<span class="text-red-600 font-semibold">NOT valid</span>'}</p>`;
      stepsContainer.innerHTML = html;
    }

    function validateISBN() {
      inputError.textContent = '';
      stepsContainer.innerHTML = '<p class="text-gray-500 text-sm">—</p>';
      isbn10to13.textContent = '—';
      isbn13to10.textContent = '—';

      const raw = isbnInput.value.trim();
      if (!raw) {
        inputError.textContent = 'Please enter an ISBN first.';
        isbnType.textContent = '—';
        isbnValid.textContent = '—';
        isbnNormalized.textContent = '—';
        return;
      }

      const normalized = normalizeISBN(raw);
      isbnNormalized.textContent = normalized || '—';

      if (isISBN10(normalized)) {
        isbnType.textContent = 'ISBN-10';
        const res = checksumISBN10(normalized);
        isbnValid.textContent = res.valid ? 'Yes' : 'No';
        renderStepsFor10(res);
        if (res.valid) {
          const conv13 = convert10to13(normalized);
          isbn10to13.textContent = conv13;
        }
      } else if (isISBN13(normalized)) {
        isbnType.textContent = 'ISBN-13';
        const res = checksumISBN13(normalized);
        isbnValid.textContent = res.valid ? 'Yes' : 'No';
        renderStepsFor13(res);
        if (res.valid) {
          const conv10 = convert13to10(normalized);
          if (conv10) {
            isbn13to10.textContent = conv10;
          } else {
            isbn13to10.textContent = 'Cannot convert (not 978-prefixed).';
          }
        }
      } else {
        isbnType.textContent = 'Unknown / invalid length';
        isbnValid.textContent = 'No';
        inputError.textContent = 'ISBN length must be 10 or 13 digits (after removing hyphens).';
      }
    }

    validateBtn.addEventListener('click', validateISBN);
    // initial
    validateISBN();
  </script>

