    <script>
      // Container presets (internal dimensions in mm)
      const containerPresets = {
        "20gp": { L: 5898, W: 2350, H: 2390 },
        "40gp": { L: 12032, W: 2350, H: 2390 },
        "40hc": { L: 12032, W: 2350, H: 2695 },
        "45hc": { L: 13556, W: 2350, H: 2695 },
      };

      const containerPresetSelect = document.getElementById("container-preset");
      const containerLengthInput = document.getElementById("container-length");
      const containerWidthInput = document.getElementById("container-width");
      const containerHeightInput = document.getElementById("container-height");
      const cargoTbody = document.getElementById("cargo-tbody");
      const addItemBtn = document.getElementById("add-item-btn");
      const cargoTypeButtons = document.querySelectorAll(".cargo-type-btn");
      const calculateBtn = document.getElementById("calculate-btn");
      const resetBtn = document.getElementById("reset-btn");
      const utilizationVolumeEl = document.getElementById("utilization-volume");
      const utilizationWeightEl = document.getElementById("utilization-weight");
      const itemsLoadedEl = document.getElementById("items-loaded");
      const itemsLoadedDetailEl = document.getElementById(
        "items-loaded-detail"
      );
      const detailsTbody = document.getElementById("details-tbody");
      const warningsBox = document.getElementById("warnings-box");
      const layoutSvg = document.getElementById("layout-svg");
      const toggleLayerViewBtn = document.getElementById("toggle-layer-view");
      const maxGrossWeightInput = document.getElementById("max-gross-weight");
      const packingStrategySelect = document.getElementById("packing-strategy");

      let currentCargoType = "cartons";
      let currentLayerIndex = 0;
      let layoutLayers = [];

      function setContainerFromPreset() {
        const preset = containerPresets[containerPresetSelect.value];
        if (!preset) return;
        containerLengthInput.value = preset.L;
        containerWidthInput.value = preset.W;
        containerHeightInput.value = preset.H;
      }

      containerPresetSelect.addEventListener("change", () => {
        if (containerPresetSelect.value !== "custom") {
          setContainerFromPreset();
        }
      });

      // Initialize default preset
      setContainerFromPreset();

      // Cargo type toggle
      cargoTypeButtons.forEach((btn) => {
        btn.addEventListener("click", () => {
          cargoTypeButtons.forEach((b) => {
            b.classList.remove(
              "bg-blue-50",
              "border-blue-500",
              "text-blue-600"
            );
            b.classList.add("border-gray-300", "text-gray-700");
          });
          btn.classList.add("bg-blue-50", "border-blue-500", "text-blue-600");
          btn.classList.remove("border-gray-300", "text-gray-700");
          currentCargoType = btn.dataset.cargoType || "cartons";
        });
      });

      // Add cargo row
      let itemCounter = 0;
      function addCargoRow(defaults = {}) {
        itemCounter += 1;
        const tr = document.createElement("tr");
        tr.innerHTML = `
      <td class="px-2 py-1 text-xs text-gray-500">${itemCounter}</td>
      <td class="px-2 py-1">
        <input type="text" class="w-32 md:w-40 border border-gray-200 rounded px-2 py-1 text-xs" placeholder="Item ${itemCounter}" value="${
          defaults.description || ""
        }">
      </td>
      <td class="px-2 py-1">
        <input type="number" class="w-20 border border-gray-200 rounded px-2 py-1 text-xs" value="${
          defaults.L || ""
        }">
      </td>
      <td class="px-2 py-1">
        <input type="number" class="w-20 border border-gray-200 rounded px-2 py-1 text-xs" value="${
          defaults.W || ""
        }">
      </td>
      <td class="px-2 py-1">
        <input type="number" class="w-20 border border-gray-200 rounded px-2 py-1 text-xs" value="${
          defaults.H || ""
        }">
      </td>
      <td class="px-2 py-1">
        <input type="number" class="w-20 border border-gray-200 rounded px-2 py-1 text-xs" value="${
          defaults.qty || 0
        }">
      </td>
      <td class="px-2 py-1">
        <input type="number" class="w-24 border border-gray-200 rounded px-2 py-1 text-xs" value="${
          defaults.weight || 0
        }">
      </td>
      <td class="px-2 py-1">
        <input type="number" class="w-20 border border-gray-200 rounded px-2 py-1 text-xs" value="${
          defaults.stack || 1
        }">
      </td>
      <td class="px-2 py-1 text-center">
        <input type="checkbox" class="h-4 w-4" ${
          defaults.rotation === false ? "" : "checked"
        }>
      </td>
      <td class="px-2 py-1 text-center">
        <button type="button" class="text-xs text-red-600 hover:underline">Remove</button>
      </td>
    `;
        cargoTbody.appendChild(tr);

        tr.querySelector("button").addEventListener("click", () => {
          tr.remove();
        });
      }

      addItemBtn.addEventListener("click", () => addCargoRow());

      // Add one default row for convenience
      addCargoRow({
        description: "Carton A",
        L: 400,
        W: 300,
        H: 250,
        qty: 500,
        weight: 10,
        stack: 4,
      });

      function readCargoRows() {
        const rows = [];
        cargoTbody.querySelectorAll("tr").forEach((tr, index) => {
          const inputs = tr.querySelectorAll("input");
          if (inputs.length < 8) return;
          const description = inputs[0].value || `Item ${index + 1}`;
          const L = parseFloat(inputs[1].value);
          const W = parseFloat(inputs[2].value);
          const H = parseFloat(inputs[3].value);
          const qty = parseInt(inputs[4].value, 10);
          const weight = parseFloat(inputs[5].value);
          const stack = parseInt(inputs[6].value, 10) || 1;
          const rotation = inputs[7].checked;

          if (!L || !W || !H || !qty) return;

          rows.push({
            id: index,
            description,
            L,
            W,
            H,
            qty,
            weight: weight || 0,
            stack,
            rotation,
          });
        });
        return rows;
      }

      function resetResults() {
        utilizationVolumeEl.textContent = "–";
        utilizationWeightEl.textContent = "–";
        itemsLoadedEl.textContent = "–";
        itemsLoadedDetailEl.textContent = "–";
        detailsTbody.innerHTML = "";
        warningsBox.classList.add("hidden");
        warningsBox.textContent = "";
        layoutSvg.innerHTML = "";
        layoutLayers = [];
        currentLayerIndex = 0;
      }

      function calculatePlan() {
        resetResults();

        const Lc = parseFloat(containerLengthInput.value);
        const Wc = parseFloat(containerWidthInput.value);
        const Hc = parseFloat(containerHeightInput.value);
        if (!Lc || !Wc || !Hc) {
          alert("Please enter valid container dimensions.");
          return;
        }

        const items = readCargoRows();
        if (!items.length) {
          alert(
            "Please add at least one cargo item with valid dimensions and quantity."
          );
          return;
        }

        const maxGross = parseFloat(maxGrossWeightInput.value) || null;
        const strategy = packingStrategySelect.value;

        // Sort items according to strategy
        if (strategy === "volume") {
          items.sort((a, b) => b.L * b.W * b.H - a.L * a.W * a.H);
        } else if (strategy === "weight") {
          items.sort((a, b) => (b.weight || 0) - (a.weight || 0));
        } // 'priority' keeps original order

        const containerVolume = (Lc * Wc * Hc) / 1e9; // m³
        let totalLoadedVolume = 0;
        let totalLoadedWeight = 0;
        let totalRequested = 0;
        let totalLoaded = 0;
        const details = [];
        const warnings = [];

        layoutLayers = [];
        let currentZ = 0;

        items.forEach((item) => {
          totalRequested += item.qty;

          // Try orientations
          const orientations = [];
          orientations.push({ L: item.L, W: item.W, H: item.H, label: "L×W" });
          if (item.rotation && item.L !== item.W) {
            orientations.push({
              L: item.W,
              W: item.L,
              H: item.H,
              label: "W×L",
            });
          }

          let best = null;
          orientations.forEach((o) => {
            const nL = Math.floor(Lc / o.L);
            const nW = Math.floor(Wc / o.W);
            const perLayer = nL * nW;
            if (perLayer <= 0) return;
            const footprint = perLayer * o.L * o.W;
            if (!best || footprint > best.footprint) {
              best = { ...o, nL, nW, perLayer, footprint };
            }
          });

          if (!best || best.perLayer <= 0) {
            details.push({
              item,
              loadedQty: 0,
              fullLayers: 0,
              unitsPerLayer: 0,
              reason: "Item footprint larger than container floor.",
            });
            return;
          }

          const maxLayersByHeight = Math.floor(Hc / best.H);
          const maxLayersByStack = item.stack;
          const theoreticalMaxLayers = Math.min(
            maxLayersByHeight,
            maxLayersByStack
          );
          if (theoreticalMaxLayers <= 0) {
            details.push({
              item,
              loadedQty: 0,
              fullLayers: 0,
              unitsPerLayer: best.perLayer,
              reason:
                "Stacking height exceeds container height or stack limit.",
            });
            return;
          }

          const maxUnitsByVolume = theoreticalMaxLayers * best.perLayer;
          const unitsToLoad = Math.min(item.qty, maxUnitsByVolume);
          const fullLayers = Math.floor(unitsToLoad / best.perLayer);
          const remainingUnits = unitsToLoad % best.perLayer;

          const itemVolume = (item.L * item.W * item.H) / 1e9; // m³
          const loadedVolume = unitsToLoad * itemVolume;
          const loadedWeight = unitsToLoad * (item.weight || 0);

          totalLoadedVolume += loadedVolume;
          totalLoadedWeight += loadedWeight;
          totalLoaded += unitsToLoad;

          // Build layout layers for visualization (simplified)
          let unitsLeft = unitsToLoad;
          let z = 0;
          while (unitsLeft > 0 && z < theoreticalMaxLayers) {
            const unitsThisLayer = Math.min(best.perLayer, unitsLeft);
            const layer = {
              itemDescription: item.description,
              color: pickColorForIndex(item.id),
              zIndex: z,
              boxes: [],
            };
            let count = 0;
            for (let i = 0; i < best.nL; i++) {
              for (let j = 0; j < best.nW; j++) {
                if (count >= unitsThisLayer) break;
                const x = i * best.L;
                const y = j * best.W;
                layer.boxes.push({
                  x,
                  y,
                  L: best.L,
                  W: best.W,
                });
                count++;
              }
              if (count >= unitsThisLayer) break;
            }
            layoutLayers.push(layer);
            unitsLeft -= unitsThisLayer;
            z++;
          }

          let reason = "";
          if (unitsToLoad < item.qty) {
            if (maxUnitsByVolume < item.qty) {
              reason = "Limited by container height or floor area.";
            } else {
              reason = "Limited by stacking constraint.";
            }
          }

          details.push({
            item,
            loadedQty: unitsToLoad,
            fullLayers,
            unitsPerLayer: best.perLayer,
            reason: reason || "Fully loaded.",
          });
        });

        // Utilization
        const volumeUtil =
          containerVolume > 0 ? (totalLoadedVolume / containerVolume) * 100 : 0;
        utilizationVolumeEl.textContent = isFinite(volumeUtil)
          ? volumeUtil.toFixed(1) + "%"
          : "–";

        if (maxGross) {
          const weightUtil = (totalLoadedWeight / maxGross) * 100;
          utilizationWeightEl.textContent = isFinite(weightUtil)
            ? weightUtil.toFixed(1) + "%"
            : "–";
          if (totalLoadedWeight > maxGross) {
            warnings.push(
              `Total cargo weight (${totalLoadedWeight.toFixed(
                0
              )} kg) exceeds the specified max gross weight (${maxGross.toFixed(
                0
              )} kg).`
            );
          }
        } else {
          utilizationWeightEl.textContent =
            totalLoadedWeight > 0 ? totalLoadedWeight.toFixed(0) + " kg" : "–";
        }

        itemsLoadedEl.textContent = totalLoaded.toString();
        if (totalRequested > 0) {
          itemsLoadedDetailEl.textContent = `${totalLoaded} of ${totalRequested} requested units loaded (${(
            (totalLoaded / totalRequested) *
            100
          ).toFixed(1)}%).`;
        } else {
          itemsLoadedDetailEl.textContent = "–";
        }

        // Fill details table
        detailsTbody.innerHTML = "";
        details.forEach((d) => {
          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="px-2 py-1">${escapeHtml(d.item.description)}</td>
        <td class="px-2 py-1">${d.item.qty}</td>
        <td class="px-2 py-1">${d.loadedQty}</td>
        <td class="px-2 py-1">${d.fullLayers}</td>
        <td class="px-2 py-1">${d.unitsPerLayer}</td>
        <td class="px-2 py-1 text-xs text-gray-600">${escapeHtml(d.reason)}</td>
      `;
          detailsTbody.appendChild(tr);
        });

        if (warnings.length) {
          warningsBox.textContent = warnings.join(" ");
          warningsBox.classList.remove("hidden");
        }

        // Draw layout
        drawLayout(Lc, Wc);
      }

      function pickColorForIndex(i) {
        const palette = [
          "#3b82f6",
          "#10b981",
          "#f97316",
          "#ec4899",
          "#6366f1",
          "#14b8a6",
          "#f59e0b",
          "#ef4444",
        ];
        return palette[i % palette.length];
      }

      function drawLayout(Lc, Wc) {
        layoutSvg.innerHTML = "";
        if (!layoutLayers.length) return;

        const layer = layoutLayers[currentLayerIndex] || layoutLayers[0];

        const padding = 10;
        const viewWidth = 1000 - 2 * padding;
        const viewHeight = 400 - 2 * padding;

        const scaleX = viewWidth / Lc;
        const scaleY = viewHeight / Wc;
        const scale = Math.min(scaleX, scaleY);

        // Container outline
        const containerWidthPx = Lc * scale;
        const containerHeightPx = Wc * scale;
        const offsetX = (1000 - containerWidthPx) / 2;
        const offsetY = (400 - containerHeightPx) / 2;

        const rect = document.createElementNS(
          "http://www.w3.org/2000/svg",
          "rect"
        );
        rect.setAttribute("x", offsetX);
        rect.setAttribute("y", offsetY);
        rect.setAttribute("width", containerWidthPx);
        rect.setAttribute("height", containerHeightPx);
        rect.setAttribute("fill", "#f9fafb");
        rect.setAttribute("stroke", "#9ca3af");
        rect.setAttribute("stroke-width", "2");
        layoutSvg.appendChild(rect);

        // Boxes
        layoutLayers.forEach((layerObj, idx) => {
          if (idx !== currentLayerIndex) return;
          layerObj.boxes.forEach((box) => {
            const r = document.createElementNS(
              "http://www.w3.org/2000/svg",
              "rect"
            );
            r.setAttribute("x", offsetX + box.x * scale);
            r.setAttribute("y", offsetY + box.y * scale);
            r.setAttribute("width", box.L * scale);
            r.setAttribute("height", box.W * scale);
            r.setAttribute("fill", layerObj.color);
            r.setAttribute("fill-opacity", "0.6");
            r.setAttribute("stroke", "#111827");
            r.setAttribute("stroke-width", "0.5");
            layoutSvg.appendChild(r);
          });
        });

        // Label
        const text = document.createElementNS(
          "http://www.w3.org/2000/svg",
          "text"
        );
        text.setAttribute("x", offsetX + 8);
        text.setAttribute("y", offsetY + 18);
        text.setAttribute("fill", "#111827");
        text.setAttribute("font-size", "12");
        text.textContent = `Layer ${currentLayerIndex + 1} – ${
          layoutLayers[currentLayerIndex].itemDescription
        }`;
        layoutSvg.appendChild(text);
      }

      toggleLayerViewBtn.addEventListener("click", () => {
        if (!layoutLayers.length) return;
        currentLayerIndex = (currentLayerIndex + 1) % layoutLayers.length;
        const Lc = parseFloat(containerLengthInput.value);
        const Wc = parseFloat(containerWidthInput.value);
        if (Lc && Wc) {
          drawLayout(Lc, Wc);
        }
      });

      calculateBtn.addEventListener("click", calculatePlan);

      resetBtn.addEventListener("click", () => {
        cargoTbody.innerHTML = "";
        itemCounter = 0;
        addCargoRow({
          description: "Carton A",
          L: 400,
          W: 300,
          H: 250,
          qty: 500,
          weight: 10,
          stack: 4,
        });
        setContainerFromPreset();
        maxGrossWeightInput.value = "";
        packingStrategySelect.value = "volume";
        resetResults();
      });

      function escapeHtml(str) {
        return String(str).replace(
          /[&<>"']/g,
          (s) =>
            ({
              "&": "&amp;",
              "<": "&lt;",
              ">": "&gt;",
              '"': "&quot;",
              "'": "&#39;",
            }[s])
        );
      }

      // Mobile menu script (simple)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", () => {
          const menu = document.getElementById("mobile-menu");
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
