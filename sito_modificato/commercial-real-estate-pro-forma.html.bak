<script="">
        function calculateProForma() {
            const propertyValue = parseFloat(document.getElementById('propertyValue').value) || 0;
            const operatingIncome = parseFloat(document.getElementById('operatingIncome').value) || 0;
            const operatingExpenses = parseFloat(document.getElementById('operatingExpenses').value) || 0;
            
            const noi = operatingIncome - operatingExpenses;
            const capRate = ((noi / propertyValue) * 100).toFixed(2);

            document.getElementById('noiValue').textContent = `$${noi.toFixed(2)}`;
            document.getElementById('capRateValue').textContent = `${capRate}%`;
        }
    </script>


