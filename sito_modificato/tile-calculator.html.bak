    <script>
      // Utility: create area row
      function createAreaRow(index, units) {
        const isImperial = units === "imperial";
        const lengthLabel = isImperial ? "Length" : "Length";
        const widthLabel = isImperial ? "Width" : "Width";
        const lengthUnit = isImperial ? "ft" : "m";
        const widthUnit = isImperial ? "ft" : "m";

        const div = document.createElement("div");
        div.className = "border border-gray-200 rounded-lg p-3";
        div.dataset.areaIndex = index;

        div.innerHTML = `
      <div class="flex justify-between items-center mb-2">
        <h3 class="text-sm font-semibold text-gray-800">Area ${index + 1}</h3>
        <button type="button" class="text-xs text-red-600 hover:text-red-700 remove-area-btn">Remove</button>
      </div>
      <div class="grid grid-cols-2 gap-3">
        <div>
          <label class="block text-xs font-medium text-gray-700">${lengthLabel}</label>
          <div class="mt-1 flex">
            <input type="number" class="area-length flex-1 border border-gray-300 rounded-l-md px-2 py-1.5 text-xs" min="0" step="0.01" value="10">
            <span class="inline-flex items-center px-2 border border-l-0 border-gray-300 rounded-r-md text-xs bg-gray-50">${lengthUnit}</span>
          </div>
        </div>
        <div>
          <label class="block text-xs font-medium text-gray-700">${widthLabel}</label>
          <div class="mt-1 flex">
            <input type="number" class="area-width flex-1 border border-gray-300 rounded-l-md px-2 py-1.5 text-xs" min="0" step="0.01" value="10">
            <span class="inline-flex items-center px-2 border border-l-0 border-gray-300 rounded-r-md text-xs bg-gray-50">${widthUnit}</span>
          </div>
        </div>
      </div>
    `;
        return div;
      }

      (function () {
        const areasContainer = document.getElementById("areas-container");
        const addAreaBtn = document.getElementById("add-area-btn");
        const calculateBtn = document.getElementById("calculate-btn");
        const resetBtn = document.getElementById("reset-btn");
        const errorMsg = document.getElementById("error-msg");
        const resultsSection = document.getElementById("results-section");

        const tabImperial = document.getElementById("tab-imperial");
        const tabMetric = document.getElementById("tab-metric");
        const tabBasic = document.getElementById("tab-basic");
        const tabAdvanced = document.getElementById("tab-advanced");
        const advancedOptions = document.getElementById("advanced-options");

        let currentUnits = "imperial"; // 'imperial' or 'metric'
        let currentMode = "basic"; // 'basic' or 'advanced'

        function setUnits(units) {
          currentUnits = units;
          const isImperial = units === "imperial";

          // Update tile units
          document.getElementById("tile-length-unit").textContent = isImperial
            ? "in"
            : "mm";
          document.getElementById("tile-width-unit").textContent = isImperial
            ? "in"
            : "mm";

          // Update grout-related units
          document.getElementById("grout-width-unit").textContent = isImperial
            ? "mm"
            : "mm";
          document.getElementById("tile-thickness-unit").textContent =
            isImperial ? "mm" : "mm";
          document.getElementById("grout-density-unit").textContent = "g/cm³";
          document.getElementById("grout-bag-unit").textContent = isImperial
            ? "lb"
            : "kg";

          // Update area row units
          Array.from(areasContainer.children).forEach((row) => {
            const spans = row.querySelectorAll("span.inline-flex");
            if (spans[0]) spans[0].textContent = isImperial ? "ft" : "m";
            if (spans[1]) spans[1].textContent = isImperial ? "ft" : "m";
          });
        }

        function setMode(mode) {
          currentMode = mode;
          if (mode === "advanced") {
            advancedOptions.classList.remove("hidden");
          } else {
            advancedOptions.classList.add("hidden");
          }
        }

        function clearAreas() {
          areasContainer.innerHTML = "";
        }

        function addArea() {
          const index = areasContainer.children.length;
          const row = createAreaRow(index, currentUnits);
          areasContainer.appendChild(row);
        }

        // Initialize with one area
        addArea();

        // Event: add area
        addAreaBtn.addEventListener("click", () => {
          addArea();
        });

        // Event: remove area (event delegation)
        areasContainer.addEventListener("click", (e) => {
          if (e.target.classList.contains("remove-area-btn")) {
            const row = e.target.closest("[data-area-index]");
            if (row && areasContainer.children.length > 1) {
              areasContainer.removeChild(row);
            }
          }
        });

        // Tabs: units
        function activateUnitTab(selected) {
          [tabImperial, tabMetric].forEach((btn) => {
            if (btn === selected) {
              btn.classList.add("tab-active");
              btn.classList.remove("text-gray-600");
              btn.setAttribute("aria-selected", "true");
            } else {
              btn.classList.remove("tab-active");
              btn.classList.add("text-gray-600");
              btn.setAttribute("aria-selected", "false");
            }
          });
        }

        tabImperial.addEventListener("click", () => {
          activateUnitTab(tabImperial);
          setUnits("imperial");
        });

        tabMetric.addEventListener("click", () => {
          activateUnitTab(tabMetric);
          setUnits("metric");
        });

        // Tabs: mode
        function activateModeTab(selected) {
          [tabBasic, tabAdvanced].forEach((btn) => {
            if (btn === selected) {
              btn.classList.add("tab-active");
              btn.classList.remove("text-gray-600");
              btn.setAttribute("aria-selected", "true");
            } else {
              btn.classList.remove("tab-active");
              btn.classList.add("text-gray-600");
              btn.setAttribute("aria-selected", "false");
            }
          });
        }

        tabBasic.addEventListener("click", () => {
          activateModeTab(tabBasic);
          setMode("basic");
        });

        tabAdvanced.addEventListener("click", () => {
          activateModeTab(tabAdvanced);
          setMode("advanced");
        });

        // Calculation
        function showError(msg) {
          errorMsg.textContent = msg;
          errorMsg.classList.remove("hidden");
          resultsSection.classList.add("hidden");
        }

        function clearError() {
          errorMsg.textContent = "";
          errorMsg.classList.add("hidden");
        }

        function calculate() {
          clearError();

          // Collect areas
          let totalArea = 0;
          const isImperial = currentUnits === "imperial";

          const rows = Array.from(areasContainer.children);
          if (rows.length === 0) {
            showError("Please add at least one area.");
            return;
          }

          for (const row of rows) {
            const lenInput = row.querySelector(".area-length");
            const widInput = row.querySelector(".area-width");
            const L = parseFloat(lenInput.value);
            const W = parseFloat(widInput.value);
            if (!(L > 0) || !(W > 0)) {
              showError(
                "All area lengths and widths must be positive numbers."
              );
              return;
            }
            totalArea += L * W; // ft² or m²
          }

          // Tile size
          const tileL = parseFloat(
            document.getElementById("tile-length").value
          );
          const tileW = parseFloat(document.getElementById("tile-width").value);
          if (!(tileL > 0) || !(tileW > 0)) {
            showError("Tile length and width must be positive numbers.");
            return;
          }

          // Convert tile area to same base area units
          let tileArea; // in ft² or m²
          if (isImperial) {
            // tileL, tileW in inches -> ft²
            tileArea = (tileL / 12) * (tileW / 12);
          } else {
            // tileL, tileW in mm -> m²
            tileArea = (tileL / 1000) * (tileW / 1000);
          }

          if (!(tileArea > 0)) {
            showError("Tile area must be greater than zero.");
            return;
          }

          const netTiles = totalArea / tileArea;

          // Waste
          let wastePct = parseFloat(document.getElementById("waste").value);
          if (!(wastePct >= 0)) wastePct = 0;
          const totalTiles = Math.ceil(netTiles * (1 + wastePct / 100));

          // Boxes & cost
          const tilesPerBox = parseFloat(
            document.getElementById("tiles-per-box").value
          );
          let boxes = null;
          if (tilesPerBox > 0) {
            boxes = Math.ceil(totalTiles / tilesPerBox);
          }

          const pricePerBox = parseFloat(
            document.getElementById("price-per-box").value
          );
          let cost = null;
          if (boxes !== null && pricePerBox > 0) {
            cost = boxes * pricePerBox;
          }

          // Grout (advanced)
          let groutVolume = null;
          let groutWeight = null;
          let groutBags = null;

          if (currentMode === "advanced") {
            const gWidth = parseFloat(
              document.getElementById("grout-width").value
            ); // mm
            const tThick = parseFloat(
              document.getElementById("tile-thickness").value
            ); // mm
            const density = parseFloat(
              document.getElementById("grout-density").value
            ); // g/cm³
            const bagWeight = parseFloat(
              document.getElementById("grout-bag-weight").value
            ); // kg or lb

            if (gWidth > 0 && tThick > 0 && density > 0 && bagWeight > 0) {
              // Convert everything to meters
              const g_m = gWidth / 1000;
              const t_m = tThick / 1000;
              let tileL_m, tileW_m;
              if (isImperial) {
                tileL_m = tileL * 0.0254;
                tileW_m = tileW * 0.0254;
              } else {
                tileL_m = tileL / 1000;
                tileW_m = tileW / 1000;
              }

              // Approximate grout volume (m³)
              const groutFactor = (2 * g_m) / (tileL_m + tileW_m);
              const V_m3 = totalArea * t_m * groutFactor;

              // Convert to liters (1 m³ = 1000 L)
              const V_L = V_m3 * 1000;
              groutVolume = V_L;

              // Convert density g/cm³ to kg/L (1 g/cm³ = 1 kg/L)
              const density_kg_per_L = density;
              const weight_kg = V_L * density_kg_per_L;

              if (isImperial) {
                // Convert kg to lb
                const weight_lb = weight_kg * 2.20462;
                groutWeight = weight_lb;
                groutBags = Math.ceil(weight_lb / bagWeight);
              } else {
                groutWeight = weight_kg;
                groutBags = Math.ceil(weight_kg / bagWeight);
              }
            }
          }

          // Populate results
          const areaUnit = isImperial ? "ft²" : "m²";

          document.getElementById("res-total-area").textContent =
            totalArea.toFixed(2) + " " + areaUnit;
          document.getElementById("res-net-tiles").textContent =
            Math.ceil(netTiles).toLocaleString();
          document.getElementById("res-waste").textContent =
            wastePct.toFixed(0) + " %";
          document.getElementById("res-total-tiles").textContent =
            totalTiles.toLocaleString();

          if (boxes !== null) {
            document.getElementById("res-boxes").textContent =
              boxes.toLocaleString();
          } else {
            document.getElementById("res-boxes").textContent = "–";
          }

          if (cost !== null) {
            document.getElementById("res-cost").textContent =
              "$" + cost.toFixed(2);
          } else {
            document.getElementById("res-cost").textContent = "–";
          }

          if (
            groutVolume !== null &&
            groutWeight !== null &&
            groutBags !== null
          ) {
            const volStr = groutVolume.toFixed(1) + " L";
            const weightStr = isImperial
              ? groutWeight.toFixed(1) + " lb"
              : groutWeight.toFixed(1) + " kg";
            document.getElementById("res-grout-volume").textContent = volStr;
            document.getElementById("res-grout-weight").textContent = weightStr;
            document.getElementById("res-grout-bags").textContent =
              groutBags.toLocaleString();
          } else {
            document.getElementById("res-grout-volume").textContent = "–";
            document.getElementById("res-grout-weight").textContent = "–";
            document.getElementById("res-grout-bags").textContent = "–";
          }

          resultsSection.classList.remove("hidden");
        }

        calculateBtn.addEventListener("click", calculate);

        // Reset
        resetBtn.addEventListener("click", () => {
          document.getElementById("tile-form").reset();
          clearAreas();
          addArea();
          setUnits("imperial");
          activateUnitTab(tabImperial);
          setMode("basic");
          activateModeTab(tabBasic);
          clearError();
          resultsSection.classList.add("hidden");
        });
      })();
    </script>

