    <script>
      (function () {
        const modeButtons = {
          fromCostRevenue: document.getElementById("mode-from-cost-revenue"),
          fromMargin: document.getElementById("mode-from-margin"),
          fromMarkup: document.getElementById("mode-from-markup"),
        };

        const groupSellingPrice = document.getElementById(
          "group-selling-price"
        );
        const groupMargin = document.getElementById("group-margin");
        const groupMarkup = document.getElementById("group-markup");

        const currencySelect = document.getElementById("currency-symbol");
        const costPrefix = document.getElementById("cost-currency-prefix");
        const pricePrefix = document.getElementById("price-currency-prefix");

        const costInput = document.getElementById("cost-input");
        const priceInput = document.getElementById("price-input");
        const marginInput = document.getElementById("margin-input");
        const markupInput = document.getElementById("markup-input");
        const discountInput = document.getElementById("discount-input");
        const taxInput = document.getElementById("tax-input");
        const quantityInput = document.getElementById("quantity-input");

        const calculateBtn = document.getElementById("calculate-btn");
        const resetBtn = document.getElementById("reset-btn");

        const resultCostPerUnit = document.getElementById(
          "result-cost-per-unit"
        );
        const resultPricePerUnit = document.getElementById(
          "result-price-per-unit"
        );
        const resultProfitPerUnit = document.getElementById(
          "result-profit-per-unit"
        );
        const resultMargin = document.getElementById("result-margin");
        const resultMarkup = document.getElementById("result-markup");
        const resultBreakeven = document.getElementById("result-breakeven");
        const resultTotalRevenue = document.getElementById(
          "result-total-revenue"
        );
        const resultTotalCost = document.getElementById("result-total-cost");
        const resultTotalProfit = document.getElementById(
          "result-total-profit"
        );
        const resultTotalMargin = document.getElementById(
          "result-total-margin"
        );
        const warningContainer = document.getElementById("warning-container");

        let currentMode = "fromCostRevenue";

        function setMode(mode) {
          currentMode = mode;
          Object.values(modeButtons).forEach((btn) => {
            btn.classList.remove("toggle-btn-active");
            btn.classList.add("toggle-btn-inactive");
          });
          if (mode === "fromCostRevenue") {
            modeButtons.fromCostRevenue.classList.add("toggle-btn-active");
            modeButtons.fromCostRevenue.classList.remove("toggle-btn-inactive");
            groupSellingPrice.classList.remove("hidden");
            groupMargin.classList.add("hidden");
            groupMarkup.classList.add("hidden");
          } else if (mode === "fromMargin") {
            modeButtons.fromMargin.classList.add("toggle-btn-active");
            modeButtons.fromMargin.classList.remove("toggle-btn-inactive");
            groupSellingPrice.classList.add("hidden");
            groupMargin.classList.remove("hidden");
            groupMarkup.classList.add("hidden");
          } else if (mode === "fromMarkup") {
            modeButtons.fromMarkup.classList.add("toggle-btn-active");
            modeButtons.fromMarkup.classList.remove("toggle-btn-inactive");
            groupSellingPrice.classList.add("hidden");
            groupMargin.classList.add("hidden");
            groupMarkup.classList.remove("hidden");
          }
          clearResults();
          hideWarning();
        }

        modeButtons.fromCostRevenue.addEventListener("click", () =>
          setMode("fromCostRevenue")
        );
        modeButtons.fromMargin.addEventListener("click", () =>
          setMode("fromMargin")
        );
        modeButtons.fromMarkup.addEventListener("click", () =>
          setMode("fromMarkup")
        );

        currencySelect.addEventListener("change", () => {
          const symbol = currencySelect.value || "";
          costPrefix.textContent = symbol;
          pricePrefix.textContent = symbol;
          // Recalculate to update formatted outputs
          if (
            costInput.value ||
            priceInput.value ||
            marginInput.value ||
            markupInput.value
          ) {
            calculate();
          }
        });

        function formatMoney(value) {
          if (isNaN(value)) return "–";
          const symbol = currencySelect.value || "";
          const absVal = Math.abs(value);
          const decimals = absVal < 1 ? 4 : absVal < 100 ? 2 : 2;
          const formatted = value.toLocaleString(undefined, {
            minimumFractionDigits: decimals,
            maximumFractionDigits: decimals,
          });
          return symbol ? symbol + formatted : formatted;
        }

        function formatPercent(value) {
          if (isNaN(value)) return "–";
          return value.toFixed(2).replace(/\.00$/, "") + "%";
        }

        function showWarning(message) {
          warningContainer.textContent = message;
          warningContainer.classList.remove("hidden");
        }

        function hideWarning() {
          warningContainer.classList.add("hidden");
          warningContainer.textContent = "";
        }

        function clearResults() {
          const dash = "–";
          resultCostPerUnit.textContent = dash;
          resultPricePerUnit.textContent = dash;
          resultProfitPerUnit.textContent = dash;
          resultMargin.textContent = dash;
          resultMarkup.textContent = dash;
          resultBreakeven.textContent = dash;
          resultTotalRevenue.textContent = dash;
          resultTotalCost.textContent = dash;
          resultTotalProfit.textContent = dash;
          resultTotalMargin.textContent = dash;
        }

        function calculate() {
          hideWarning();
          clearResults();

          const cost = parseFloat(costInput.value);
          const price = parseFloat(priceInput.value);
          const margin = parseFloat(marginInput.value);
          const markup = parseFloat(markupInput.value);
          const discount = parseFloat(discountInput.value);
          const tax = parseFloat(taxInput.value);
          const qtyRaw = parseFloat(quantityInput.value);
          const quantity =
            isNaN(qtyRaw) || qtyRaw <= 0 ? 1 : Math.floor(qtyRaw);

          if (isNaN(cost) || cost < 0) {
            showWarning("Please enter a valid non-negative cost.");
            return;
          }

          let effectivePrice;

          if (currentMode === "fromCostRevenue") {
            if (isNaN(price) || price < 0) {
              showWarning("Please enter a valid non-negative selling price.");
              return;
            }
            effectivePrice = price;
          } else if (currentMode === "fromMargin") {
            if (isNaN(margin)) {
              showWarning("Please enter a target margin percentage.");
              return;
            }
            const marginDec = margin / 100;
            if (marginDec >= 1) {
              showWarning("Target margin must be less than 100%.");
              return;
            }
            effectivePrice = cost / (1 - marginDec);
          } else if (currentMode === "fromMarkup") {
            if (isNaN(markup)) {
              showWarning("Please enter a markup percentage.");
              return;
            }
            const markupDec = markup / 100;
            effectivePrice = cost * (1 + markupDec);
          }

          // Apply discount on price (percentage off)
          let discountedPrice = effectivePrice;
          if (!isNaN(discount) && discount > 0) {
            const discDec = discount / 100;
            discountedPrice = effectivePrice * (1 - discDec);
          }

          // Per-unit profit and margin
          const profitPerUnit = discountedPrice - cost;
          const marginPerUnit =
            discountedPrice > 0 ? (profitPerUnit / discountedPrice) * 100 : NaN;
          const markupPerUnit = cost > 0 ? (profitPerUnit / cost) * 100 : NaN;

          // Tax is added on top of discounted price
          let priceWithTax = discountedPrice;
          if (!isNaN(tax) && tax > 0) {
            const taxDec = tax / 100;
            priceWithTax = discountedPrice * (1 + taxDec);
          }

          const totalRevenue = priceWithTax * quantity;
          const totalCost = cost * quantity;
          const totalProfit = totalRevenue - totalCost;
          const totalMargin =
            totalRevenue > 0 ? (totalProfit / totalRevenue) * 100 : NaN;

          // Populate results
          resultCostPerUnit.textContent = formatMoney(cost);
          resultPricePerUnit.textContent = formatMoney(discountedPrice);
          resultProfitPerUnit.textContent = formatMoney(profitPerUnit);
          resultMargin.textContent = isNaN(marginPerUnit)
            ? "–"
            : formatPercent(marginPerUnit);
          resultMarkup.textContent = isNaN(markupPerUnit)
            ? "–"
            : formatPercent(markupPerUnit);
          resultBreakeven.textContent = formatMoney(cost);

          resultTotalRevenue.textContent = formatMoney(totalRevenue);
          resultTotalCost.textContent = formatMoney(totalCost);
          resultTotalProfit.textContent = formatMoney(totalProfit);
          resultTotalMargin.textContent = isNaN(totalMargin)
            ? "–"
            : formatPercent(totalMargin);

          if (profitPerUnit < 0) {
            showWarning(
              "Warning: Your cost is higher than your effective selling price. You are losing money on each unit."
            );
          } else if (profitPerUnit === 0) {
            showWarning(
              "Note: Your selling price equals your cost. You are breaking even with 0% margin."
            );
          }
        }

        calculateBtn.addEventListener("click", calculate);

        [
          costInput,
          priceInput,
          marginInput,
          markupInput,
          discountInput,
          taxInput,
          quantityInput,
        ].forEach((input) => {
          input.addEventListener("keydown", function (e) {
            if (e.key === "Enter") {
              e.preventDefault();
              calculate();
            }
          });
        });

        resetBtn.addEventListener("click", () => {
          costInput.value = "";
          priceInput.value = "";
          marginInput.value = "";
          markupInput.value = "";
          discountInput.value = "";
          taxInput.value = "";
          quantityInput.value = "";
          hideWarning();
          clearResults();
        });

        document
          .querySelectorAll("#faq-container .faq-question")
          .forEach((btn) => {
            btn.addEventListener("click", () => {
              const answer = btn.nextElementSibling;
              const symbol = btn.querySelector("span:last-child");
              const isHidden = answer.classList.contains("hidden");
              document
                .querySelectorAll("#faq-container .faq-answer")
                .forEach((a) => a.classList.add("hidden"));
              document
                .querySelectorAll(
                  "#faq-container .faq-question span:last-child"
                )
                .forEach((s) => (s.textContent = "+"));
              if (isHidden) {
                answer.classList.remove("hidden");
                symbol.textContent = "−";
              }
            });
          });
      })();
    </script>
  
