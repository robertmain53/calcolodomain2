    <script>
      // Unit conversion helpers (to SI and back)
      const KToSI = (value, unit) => {
        if (isNaN(value)) return NaN;
        switch (unit) {
          case "m_s":
            return value;
          case "cm_s":
            return value / 100;
          case "m_day":
            return value / 86400;
          case "ft_day":
            return (value * 0.3048) / 86400;
          default:
            return NaN;
        }
      };

      const KFromSI = (valueSI, unit) => {
        switch (unit) {
          case "m_s":
            return valueSI;
          case "cm_s":
            return valueSI * 100;
          case "m_day":
            return valueSI * 86400;
          case "ft_day":
            return (valueSI * 86400) / 0.3048;
          default:
            return NaN;
        }
      };

      const AToSI = (value, unit) => {
        if (isNaN(value)) return NaN;
        switch (unit) {
          case "m2":
            return value;
          case "cm2":
            return value / 10000;
          case "ft2":
            return value * 0.09290304;
          default:
            return NaN;
        }
      };

      const dHToSI = (value, unit) => {
        if (isNaN(value)) return NaN;
        switch (unit) {
          case "m":
            return value;
          case "cm":
            return value / 100;
          case "ft":
            return value * 0.3048;
          default:
            return NaN;
        }
      };

      const LToSI = dHToSI;

      const QToSI = (value, unit) => {
        if (isNaN(value)) return NaN;
        switch (unit) {
          case "m3_s":
            return value;
          case "L_s":
            return value / 1000;
          case "m3_day":
            return value / 86400;
          case "ft3_s":
            return value * 0.028316846592;
          case "gpm":
            return value * 0.0000630901964; // US gallon per minute to m3/s
          default:
            return NaN;
        }
      };

      const QFromSI = (valueSI, unit) => {
        switch (unit) {
          case "m3_s":
            return valueSI;
          case "L_s":
            return valueSI * 1000;
          case "m3_day":
            return valueSI * 86400;
          case "ft3_s":
            return valueSI / 0.028316846592;
          case "gpm":
            return valueSI / 0.0000630901964;
          default:
            return NaN;
        }
      };

      function formatNumber(x, digits = 6) {
        if (!isFinite(x)) return "—";
        if (x === 0) return "0";
        const abs = Math.abs(x);
        if (abs >= 1e4 || abs <= 1e-3) {
          return x.toExponential(3);
        }
        return parseFloat(x.toFixed(digits)).toString();
      }

      document.addEventListener("DOMContentLoaded", () => {
        const solveForEl = document.getElementById("solveFor");
        const calcBtn = document.getElementById("calcBtn");
        const resetBtn = document.getElementById("resetBtn");
        const errorMsg = document.getElementById("errorMsg");
        const resultsCard = document.getElementById("resultsCard");
        const resultsContent = document.getElementById("resultsContent");

        function clearResults() {
          errorMsg.textContent = "";
          resultsCard.classList.add("hidden");
          resultsContent.innerHTML = "";
        }

        function handleCalculate() {
          clearResults();

          const solveFor = solveForEl.value;

          const KVal = parseFloat(document.getElementById("KValue").value);
          const KUnit = document.getElementById("KUnit").value;
          const AVal = parseFloat(document.getElementById("AValue").value);
          const AUnit = document.getElementById("AUnit").value;
          const dHVal = parseFloat(document.getElementById("dHValue").value);
          const dHUnit = document.getElementById("dHUnit").value;
          const LVal = parseFloat(document.getElementById("LValue").value);
          const LUnit = document.getElementById("LUnit").value;
          const QVal = parseFloat(document.getElementById("QValue").value);
          const QUnit = document.getElementById("QUnit").value;
          const nVal = parseFloat(document.getElementById("nValue").value);

          // Convert to SI
          const K_SI = !isNaN(KVal) ? KToSI(KVal, KUnit) : NaN;
          const A_SI = !isNaN(AVal) ? AToSI(AVal, AUnit) : NaN;
          const dH_SI = !isNaN(dHVal) ? dHToSI(dHVal, dHUnit) : NaN;
          const L_SI = !isNaN(LVal) ? LToSI(LVal, LUnit) : NaN;
          const Q_SI = !isNaN(QVal) ? QToSI(QVal, QUnit) : NaN;

          let i, q, v, K, Q;

          // Compute gradient if possible
          if (!isNaN(dH_SI) && !isNaN(L_SI) && L_SI !== 0) {
            i = dH_SI / L_SI;
          }

          try {
            switch (solveFor) {
              case "Q":
                if (
                  isNaN(K_SI) ||
                  isNaN(A_SI) ||
                  typeof i === "undefined" ||
                  isNaN(i)
                ) {
                  throw new Error(
                    "To solve for Q, provide K, A, Δh, and L (or i)."
                  );
                }
                K = K_SI;
                Q = K_SI * A_SI * i;
                q = Q / A_SI;
                if (!isNaN(nVal) && nVal > 0) {
                  v = q / nVal;
                }
                break;

              case "q":
                if (isNaN(K_SI) || typeof i === "undefined" || isNaN(i)) {
                  throw new Error(
                    "To solve for q, provide K, Δh, and L (or i)."
                  );
                }
                K = K_SI;
                q = K_SI * i;
                if (!isNaN(A_SI)) {
                  Q = q * A_SI;
                }
                if (!isNaN(nVal) && nVal > 0) {
                  v = q / nVal;
                }
                break;

              case "v":
                if (
                  isNaN(K_SI) ||
                  typeof i === "undefined" ||
                  isNaN(i) ||
                  isNaN(nVal) ||
                  nVal <= 0
                ) {
                  throw new Error(
                    "To solve for v, provide K, Δh, L (or i), and effective porosity nₑ > 0."
                  );
                }
                K = K_SI;
                q = K_SI * i;
                v = q / nVal;
                if (!isNaN(A_SI)) {
                  Q = q * A_SI;
                }
                break;

              case "K":
                if (
                  isNaN(Q_SI) ||
                  isNaN(A_SI) ||
                  typeof i === "undefined" ||
                  isNaN(i) ||
                  i === 0
                ) {
                  throw new Error(
                    "To solve for K, provide Q, A, Δh, and L (or i)."
                  );
                }
                K = Q_SI / (A_SI * i);
                Q = Q_SI;
                q = Q / A_SI;
                if (!isNaN(nVal) && nVal > 0) {
                  v = q / nVal;
                }
                break;

              case "i":
                if (
                  isNaN(Q_SI) ||
                  isNaN(K_SI) ||
                  isNaN(A_SI) ||
                  K_SI === 0 ||
                  A_SI === 0
                ) {
                  throw new Error("To solve for i, provide Q, K, and A.");
                }
                K = K_SI;
                Q = Q_SI;
                i = Q_SI / (K_SI * A_SI);
                q = Q / A_SI;
                if (!isNaN(nVal) && nVal > 0) {
                  v = q / nVal;
                }
                break;
            }
          } catch (e) {
            errorMsg.textContent = e.message;
            return;
          }

          // Build results
          const lines = [];

          if (typeof i !== "undefined" && !isNaN(i)) {
            lines.push(
              `<div><strong>Hydraulic gradient i</strong> = ${formatNumber(
                i,
                6
              )} (dimensionless)</div>`
            );
          }

          if (!isNaN(K)) {
            const KOut = KFromSI(K, KUnit);
            lines.push(
              `<div><strong>Hydraulic conductivity K</strong> = ${formatNumber(
                KOut,
                6
              )} ${
                document.getElementById("KUnit").selectedOptions[0].textContent
              }</div>`
            );
          }

          if (!isNaN(Q)) {
            const QOut = QFromSI(Q, QUnit);
            lines.push(
              `<div><strong>Discharge Q</strong> = ${formatNumber(QOut, 6)} ${
                document.getElementById("QUnit").selectedOptions[0].textContent
              }</div>`
            );
          }

          if (!isNaN(q)) {
            lines.push(
              `<div><strong>Darcy velocity q</strong> = ${formatNumber(
                q,
                6
              )} m/s</div>`
            );
          }

          if (!isNaN(v)) {
            lines.push(
              `<div><strong>Seepage velocity v</strong> = ${formatNumber(
                v,
                6
              )} m/s</div>`
            );
          }

          if (!lines.length) {
            errorMsg.textContent =
              "Not enough valid inputs to perform the calculation.";
            return;
          }

          resultsContent.innerHTML = lines.join("");
          resultsCard.classList.remove("hidden");
        }

        calcBtn.addEventListener("click", handleCalculate);

        resetBtn.addEventListener("click", () => {
          document.getElementById("KValue").value = "";
          document.getElementById("AValue").value = "";
          document.getElementById("dHValue").value = "";
          document.getElementById("LValue").value = "";
          document.getElementById("QValue").value = "";
          document.getElementById("nValue").value = "";
          clearResults();
        });
      });

      // Mobile menu script (from template)
      document.addEventListener("DOMContentLoaded", function () {
        const toggle = document.getElementById("mobile-menu-toggle");
        const menu = document.getElementById("mobile-menu");
        if (toggle && menu) {
          toggle.addEventListener("click", () => {
            const isHidden = menu.classList.contains("hidden");
            menu.classList.toggle("hidden", !isHidden);
            toggle.setAttribute("aria-expanded", isHidden ? "true" : "false");
          });
        }
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
