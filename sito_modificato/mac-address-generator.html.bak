    <script>
      (function () {
        function getRandomBytes(len) {
          if (window.crypto && window.crypto.getRandomValues) {
            const arr = new Uint8Array(len);
            window.crypto.getRandomValues(arr);
            return Array.from(arr);
          } else {
            const arr = [];
            for (let i = 0; i < len; i++) {
              arr.push(Math.floor(Math.random() * 256));
            }
            return arr;
          }
        }

        function parseOUI(input) {
          if (!input) return null;
          let cleaned = input.replace(/[^0-9a-fA-F]/g, "").toUpperCase();
          if (cleaned.length !== 6) return null;
          const bytes = [];
          for (let i = 0; i < 6; i += 2) {
            bytes.push(parseInt(cleaned.substr(i, 2), 16));
          }
          if (bytes.some((b) => isNaN(b))) return null;
          return bytes;
        }

        function formatMac(bytes, format) {
          const hex = bytes.map((b) => b.toString(16).padStart(2, "0"));
          switch (format) {
            case "colon":
              return hex.join(":").toUpperCase();
            case "dash":
              return hex.join("-").toUpperCase();
            case "dot":
              return (
                hex[0] +
                hex[1] +
                "." +
                hex[2] +
                hex[3] +
                "." +
                hex[4] +
                hex[5]
              ).toUpperCase();
            case "plain":
              return hex.join("").toUpperCase();
            case "lower":
              return hex.join(":").toLowerCase();
            default:
              return hex.join(":").toUpperCase();
          }
        }

        function generateMac(options) {
          const bytes = getRandomBytes(6);
          if (options.oui && options.oui.length === 3) {
            bytes[0] = options.oui[0];
            bytes[1] = options.oui[1];
            bytes[2] = options.oui[2];
          }
          // Clear the two LSBs of first octet
          bytes[0] = bytes[0] & 0b11111100;
          // Set U/L bit
          if (options.scope === "local") {
            bytes[0] |= 0b00000010;
          }
          // Set I/G bit
          if (options.cast === "multicast") {
            bytes[0] |= 0b00000001;
          }
          return bytes;
        }

        function validateMacString(str) {
          if (!str) return { valid: false, reason: "Empty input" };
          const cleaned = str.trim();
          const hexOnly = cleaned.replace(/[^0-9a-fA-F]/g, "");
          if (hexOnly.length !== 12) {
            return {
              valid: false,
              reason: "MAC must contain 12 hexadecimal digits",
            };
          }
          if (!/^[0-9a-fA-F]{12}$/.test(hexOnly)) {
            return { valid: false, reason: "Invalid hexadecimal characters" };
          }
          const bytes = [];
          for (let i = 0; i < 12; i += 2) {
            bytes.push(parseInt(hexOnly.substr(i, 2), 16));
          }
          const first = bytes[0];
          const isMulticast = (first & 1) === 1;
          const isLocal = (first & 2) === 2;
          const isBroadcast = bytes.every((b) => b === 0xff);
          return {
            valid: true,
            normalized: formatMac(bytes, "colon"),
            bytes,
            isMulticast,
            isLocal,
            isBroadcast,
          };
        }

        document.addEventListener("DOMContentLoaded", function () {
          const countInput = document.getElementById("mac-count");
          const formatSelect = document.getElementById("mac-format");
          const ouiInput = document.getElementById("oui-prefix");
          const outputArea = document.getElementById("mac-output");
          const generateBtn = document.getElementById("generate-btn");
          const copyAllBtn = document.getElementById("copy-all-btn");
          const clearBtn = document.getElementById("clear-btn");
          const copyStatus = document.getElementById("copy-status");
          const validateInput = document.getElementById("mac-validate-input");
          const validateBtn = document.getElementById("validate-btn");
          const validateResult = document.getElementById("validate-result");
          const validateDetails = document.getElementById("validate-details");

          function getScope() {
            const el = document.querySelector(
              'input[name="addr-scope"]:checked'
            );
            return el ? el.value : "universal";
          }
          function getCast() {
            const el = document.querySelector(
              'input[name="addr-cast"]:checked'
            );
            return el ? el.value : "unicast";
          }

          generateBtn.addEventListener("click", function () {
            let count = parseInt(countInput.value, 10);
            if (isNaN(count) || count < 1) count = 1;
            if (count > 500) count = 500;
            countInput.value = count;

            const format = formatSelect.value;
            const oui = parseOUI(ouiInput.value);
            if (ouiInput.value && !oui) {
              alert(
                "Invalid OUI. Please enter 6 hex digits (e.g. 00:1A:2B or 001A2B)."
              );
              return;
            }

            const scope = getScope();
            const cast = getCast();
            const lines = [];
            for (let i = 0; i < count; i++) {
              const macBytes = generateMac({ oui, scope, cast });
              lines.push(formatMac(macBytes, format));
            }
            outputArea.value = lines.join("\n");
          });

          copyAllBtn.addEventListener("click", function () {
            if (!outputArea.value.trim()) return;
            navigator.clipboard
              .writeText(outputArea.value)
              .then(function () {
                copyStatus.classList.remove("hidden");
                setTimeout(() => copyStatus.classList.add("hidden"), 2000);
              })
              .catch(function () {
                copyStatus.textContent = "Copy failed";
                copyStatus.classList.remove("hidden");
                setTimeout(() => {
                  copyStatus.textContent = "Copied to clipboard";
                  copyStatus.classList.add("hidden");
                }, 2000);
              });
          });

          clearBtn.addEventListener("click", function () {
            outputArea.value = "";
          });

          validateBtn.addEventListener("click", function () {
            const value = validateInput.value;
            const res = validateMacString(value);
            validateDetails.innerHTML = "";
            if (!res.valid) {
              validateResult.textContent = "Invalid MAC address";
              validateResult.className = "mb-1 font-medium text-red-600";
              validateDetails.innerHTML = "<p>" + res.reason + "</p>";
              return;
            }
            validateResult.textContent = "Valid MAC address";
            validateResult.className = "mb-1 font-medium text-green-700";
            const items = [];
            items.push(
              "<p><strong>Normalized:</strong> " + res.normalized + "</p>"
            );
            items.push(
              "<p><strong>Type:</strong> " +
                (res.isBroadcast
                  ? "Broadcast"
                  : res.isMulticast
                  ? "Multicast"
                  : "Unicast") +
                "</p>"
            );
            items.push(
              "<p><strong>Scope:</strong> " +
                (res.isLocal
                  ? "Locally administered"
                  : "Universally administered") +
                "</p>"
            );
            validateDetails.innerHTML = items.join("");
          });
        });
      })();
    </script>

