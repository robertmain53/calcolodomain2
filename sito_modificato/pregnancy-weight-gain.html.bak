    <script>
      // Unit toggle
      const unitMetricBtn = document.getElementById("unit-metric");
      const unitUsBtn = document.getElementById("unit-us");
      const heightMetricGroup = document.getElementById("height-metric-group");
      const heightUsGroup = document.getElementById("height-us-group");
      const weightMetricGroup = document.getElementById("weight-metric-group");
      const weightUsGroup = document.getElementById("weight-us-group");
      const currentWeightUnitSpan = document.getElementById(
        "current-weight-unit"
      );

      let currentUnit = "metric";

      function setUnit(unit) {
        currentUnit = unit;
        if (unit === "metric") {
          unitMetricBtn.classList.add("tab-active");
          unitUsBtn.classList.remove("tab-active");
          heightMetricGroup.classList.remove("hidden");
          weightMetricGroup.classList.remove("hidden");
          heightUsGroup.classList.add("hidden");
          weightUsGroup.classList.add("hidden");
          currentWeightUnitSpan.textContent = "kg";
        } else {
          unitUsBtn.classList.add("tab-active");
          unitMetricBtn.classList.remove("tab-active");
          heightMetricGroup.classList.add("hidden");
          weightMetricGroup.classList.add("hidden");
          heightUsGroup.classList.remove("hidden");
          weightUsGroup.classList.remove("hidden");
          currentWeightUnitSpan.textContent = "lb";
        }
      }

      unitMetricBtn.addEventListener("click", () => setUnit("metric"));
      unitUsBtn.addEventListener("click", () => setUnit("us"));

      // Core calculation
      const calculateBtn = document.getElementById("calculate-btn");
      const resultsSection = document.getElementById("results-section");

      const bmiValueEl = document.getElementById("bmi-value");
      const bmiCategoryEl = document.getElementById("bmi-category");
      const totalGainRangeEl = document.getElementById("total-gain-range");
      const totalGainNoteEl = document.getElementById("total-gain-note");
      const weekLabelEl = document.getElementById("week-label");
      const weekLabel2El = document.getElementById("week-label-2");
      const targetByWeekEl = document.getElementById("target-by-week");
      const weeklyRateEl = document.getElementById("weekly-rate");
      const trimesterTableBody = document.getElementById(
        "trimester-table-body"
      );

      const actualCompareCard = document.getElementById("actual-compare-card");
      const actualGainEl = document.getElementById("actual-gain");
      const recommendedByWeekEl = document.getElementById(
        "recommended-by-week"
      );
      const comparisonMessageEl = document.getElementById("comparison-message");

      const rangeFillEl = document.getElementById("range-fill");
      const currentMarkerEl = document.getElementById("current-marker");
      const minLabelEl = document.getElementById("min-label");
      const midLabelEl = document.getElementById("mid-label");
      const maxLabelEl = document.getElementById("max-label");

      function round1(x) {
        return Math.round(x * 10) / 10;
      }

      function getInputs() {
        let heightM,
          preWeightKg,
          currentWeightKg = null;

        if (currentUnit === "metric") {
          const hCm = parseFloat(document.getElementById("height-cm").value);
          const wKg = parseFloat(document.getElementById("weight-kg").value);
          if (!hCm || !wKg) return null;
          heightM = hCm / 100;
          preWeightKg = wKg;
          const cw = parseFloat(
            document.getElementById("current-weight").value
          );
          if (!isNaN(cw)) currentWeightKg = cw;
        } else {
          const hFt = parseFloat(document.getElementById("height-ft").value);
          const hIn = parseFloat(document.getElementById("height-in").value);
          const wLb = parseFloat(document.getElementById("weight-lb").value);
          if (!hFt || isNaN(hIn) || !wLb) return null;
          const totalIn = hFt * 12 + hIn;
          heightM = totalIn * 0.0254;
          preWeightKg = wLb * 0.45359237;
          const cw = parseFloat(
            document.getElementById("current-weight").value
          );
          if (!isNaN(cw)) currentWeightKg = cw * 0.45359237;
        }

        const week = parseInt(document.getElementById("week").value, 10);
        const babies = parseInt(document.getElementById("babies").value, 10);
        if (!week || week < 4 || week > 42) return null;

        return { heightM, preWeightKg, currentWeightKg, week, babies };
      }

      function classifyBMI(bmi) {
        if (bmi < 18.5) return "underweight";
        if (bmi < 25) return "normal";
        if (bmi < 30) return "overweight";
        return "obese";
      }

      function getGainGuidelines(bmiCat, babies) {
        // values in kg
        if (babies === 1) {
          switch (bmiCat) {
            case "underweight":
              return { min: 12.5, max: 18, firstTrimester: 2 };
            case "normal":
              return { min: 11.5, max: 16, firstTrimester: 1.5 };
            case "overweight":
              return { min: 7, max: 11.5, firstTrimester: 1 };
            case "obese":
              return { min: 5, max: 9, firstTrimester: 1 };
          }
        } else if (babies === 2) {
          // twins (approximate ranges)
          switch (bmiCat) {
            case "underweight":
              return { min: 17, max: 25, firstTrimester: 2.5 };
            case "normal":
              return { min: 16.8, max: 24.5, firstTrimester: 2.5 };
            case "overweight":
              return { min: 14.1, max: 22.7, firstTrimester: 2 };
            case "obese":
              return { min: 11.3, max: 19.1, firstTrimester: 2 };
          }
        }
        return null;
      }

      function formatRangeKg(minKg, maxKg) {
        if (currentUnit === "metric") {
          return `${round1(minKg)}–${round1(maxKg)} kg`;
        } else {
          const minLb = minKg / 0.45359237;
          const maxLb = maxKg / 0.45359237;
          return `${round1(minLb)}–${round1(maxLb)} lb`;
        }
      }

      function formatSingleKg(kg) {
        if (currentUnit === "metric") {
          return `${round1(kg)} kg`;
        } else {
          const lb = kg / 0.45359237;
          return `${round1(lb)} lb`;
        }
      }

      function buildTrimesterTable(guidelines) {
        const { min, max, firstTrimester } = guidelines;
        const totalSpan = max - min;
        const weeks2to3 = 40 - 13;
        const minWeekly = (min - firstTrimester) / weeks2to3;
        const maxWeekly = (max - firstTrimester) / weeks2to3;

        const rows = [
          {
            name: "1st trimester",
            weeks: "1–13",
            gainMin: 0,
            gainMax: firstTrimester,
            notes: "Many people gain little; nausea may limit intake.",
          },
          {
            name: "2nd trimester",
            weeks: "14–27",
            gainMin: firstTrimester + minWeekly * (27 - 13),
            gainMax: firstTrimester + maxWeekly * (27 - 13),
            notes: "Most weight gain begins; appetite often improves.",
          },
          {
            name: "3rd trimester",
            weeks: "28–40",
            gainMin: min,
            gainMax: max,
            notes: "Steady gain as baby grows; rate may slow near term.",
          },
        ];

        trimesterTableBody.innerHTML = rows
          .map(
            (r) => `
      <tr>
        <td class="border px-3 py-2">${r.name}</td>
        <td class="border px-3 py-2">${r.weeks}</td>
        <td class="border px-3 py-2">${formatRangeKg(r.gainMin, r.gainMax)}</td>
        <td class="border px-3 py-2">${r.notes}</td>
      </tr>
    `
          )
          .join("");
      }

      function updateBar(minKg, maxKg, actualGainKg) {
        minLabelEl.textContent = formatSingleKg(minKg);
        maxLabelEl.textContent = formatSingleKg(maxKg);
        midLabelEl.textContent = formatSingleKg((minKg + maxKg) / 2);

        rangeFillEl.style.width = "100%";

        if (typeof actualGainKg === "number") {
          const span = maxKg - minKg;
          let pos;
          if (span <= 0) {
            pos = 0;
          } else {
            pos = ((actualGainKg - minKg) / span) * 100;
          }
          pos = Math.max(0, Math.min(100, pos));
          currentMarkerEl.style.left = pos + "%";
          currentMarkerEl.classList.remove("hidden");
        } else {
          currentMarkerEl.classList.add("hidden");
        }
      }

      calculateBtn.addEventListener("click", () => {
        const data = getInputs();
        if (!data) {
          alert(
            "Please enter your height, pre‑pregnancy weight, and a valid week of pregnancy."
          );
          return;
        }
        const { heightM, preWeightKg, currentWeightKg, week, babies } = data;

        const bmi = preWeightKg / (heightM * heightM);
        const bmiCat = classifyBMI(bmi);
        const guidelines = getGainGuidelines(bmiCat, babies);
        if (!guidelines) {
          alert(
            "Unable to determine guidelines for these inputs. Please check your values."
          );
          return;
        }

        const { min, max, firstTrimester } = guidelines;
        const weeks2to3 = 40 - 13;
        const minWeekly = (min - firstTrimester) / weeks2to3;
        const maxWeekly = (max - firstTrimester) / weeks2to3;

        let minByWeek, maxByWeek;
        if (week <= 13) {
          const frac = (week - 1) / (13 - 1);
          minByWeek = 0 + frac * firstTrimester;
          maxByWeek = 0 + frac * firstTrimester;
        } else if (week >= 40) {
          minByWeek = min;
          maxByWeek = max;
        } else {
          const weeksInto = week - 13;
          minByWeek = firstTrimester + minWeekly * weeksInto;
          maxByWeek = firstTrimester + maxWeekly * weeksInto;
        }

        // Update BMI
        bmiValueEl.textContent = round1(bmi);
        let catLabel = "";
        switch (bmiCat) {
          case "underweight":
            catLabel = "Underweight (BMI < 18.5)";
            break;
          case "normal":
            catLabel = "Normal weight (BMI 18.5–24.9)";
            break;
          case "overweight":
            catLabel = "Overweight (BMI 25.0–29.9)";
            break;
          case "obese":
            catLabel = "Obesity (BMI ≥ 30.0)";
            break;
        }
        bmiCategoryEl.textContent = catLabel;

        // Total gain
        totalGainRangeEl.textContent = formatRangeKg(min, max);
        totalGainNoteEl.textContent =
          babies === 1
            ? "Singleton pregnancy guideline range for your BMI category."
            : "Twin pregnancy guideline range for your BMI category (approximate).";

        // Week-specific
        weekLabelEl.textContent = week;
        weekLabel2El.textContent = week;
        targetByWeekEl.textContent = formatRangeKg(minByWeek, maxByWeek);
        weeklyRateEl.textContent = `${formatSingleKg(
          minWeekly
        )}–${formatSingleKg(maxWeekly)} per week`;

        // Trimester table
        buildTrimesterTable(guidelines);

        // Actual comparison
        if (typeof currentWeightKg === "number") {
          const gainKg = currentWeightKg - preWeightKg;
          actualGainEl.textContent = formatSingleKg(gainKg);
          recommendedByWeekEl.textContent = formatRangeKg(minByWeek, maxByWeek);

          let msg;
          if (gainKg < minByWeek - 0.5) {
            msg =
              "Your gain is below the typical range for this week. This can be normal for some people, but discuss it with your prenatal care provider, especially if you are having trouble eating or losing weight unintentionally.";
          } else if (gainKg > maxByWeek + 0.5) {
            msg =
              "Your gain is above the typical range for this week. Consider reviewing your diet and activity with your care team, especially if your provider has expressed concerns about rapid weight gain.";
          } else {
            msg =
              "Your gain is within the typical guideline range for this week. Keep following the plan you have agreed on with your prenatal care provider.";
          }
          comparisonMessageEl.textContent = msg;
          actualCompareCard.classList.remove("hidden");
          updateBar(min, max, gainKg);
        } else {
          actualCompareCard.classList.add("hidden");
          updateBar(min, max, null);
        }

        resultsSection.classList.remove("hidden");
        resultsSection.scrollIntoView({ behavior: "smooth", block: "start" });
      });

      // Mobile menu script (simple)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", () => {
          const menu = document.getElementById("mobile-menu");
          menu.classList.toggle("hidden");
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
