    <script>
      // --- Wisconsin and Federal Tax Logic (simplified, for educational use only) ---

      // Approximate 2024 WI brackets (single-like baseline).
      // For simplicity, we'll use the same thresholds for all statuses but adjust standard deduction.
      const WI_BRACKETS = [
        { upTo: 15000, rate: 0.035 }, // 3.5%
        { upTo: 30000, rate: 0.0465 }, // 4.65%
        { upTo: 250000, rate: 0.053 }, // 5.3%
        { upTo: Infinity, rate: 0.0765 }, // 7.65%
      ];

      // Approximate WI standard deduction by filing status and income.
      function estimateWiStandardDeduction(status, agi) {
        // Very rough approximation to keep UX simple.
        let base, phaseOutStart, phaseOutEnd;
        switch (status) {
          case "mfj":
            base = 24000;
            phaseOutStart = 80000;
            phaseOutEnd = 200000;
            break;
          case "hoh":
            base = 20000;
            phaseOutStart = 60000;
            phaseOutEnd = 160000;
            break;
          case "mfs":
            base = 12000;
            phaseOutStart = 40000;
            phaseOutEnd = 100000;
            break;
          case "single":
          default:
            base = 14000;
            phaseOutStart = 50000;
            phaseOutEnd = 130000;
            break;
        }
        if (agi <= 0) return 0;
        if (agi <= phaseOutStart) return base;
        if (agi >= phaseOutEnd) return 0;
        const fraction = (agi - phaseOutStart) / (phaseOutEnd - phaseOutStart);
        return Math.max(0, base * (1 - fraction));
      }

      // Simple dependent credit approximation for WI
      function estimateWiDependentCredit(dependents) {
        const perDependent = 100; // conservative placeholder
        return dependents * perDependent;
      }

      // Compute WI tax by brackets and return breakdown
      function computeWiTax(taxableIncome) {
        let remaining = Math.max(0, taxableIncome);
        let lastLimit = 0;
        let totalTax = 0;
        const breakdown = [];

        for (const bracket of WI_BRACKETS) {
          if (remaining <= 0) break;
          const upper = bracket.upTo;
          const amountInBracket = Math.max(
            0,
            Math.min(remaining, upper - lastLimit)
          );
          const taxInBracket = amountInBracket * bracket.rate;
          breakdown.push({
            range: [lastLimit, upper],
            rate: bracket.rate,
            amount: amountInBracket,
            tax: taxInBracket,
          });
          totalTax += taxInBracket;
          remaining -= amountInBracket;
          lastLimit = upper;
        }

        const marginalRate =
          taxableIncome <= 0 ? 0 : breakdown[breakdown.length - 1].rate;
        return { totalTax, breakdown, marginalRate };
      }

      // Very simplified federal tax model (single baseline, adjusted standard deduction)
      const FEDERAL_BRACKETS_SINGLE = [
        { upTo: 11600, rate: 0.1 },
        { upTo: 47150, rate: 0.12 },
        { upTo: 100525, rate: 0.22 },
        { upTo: 191950, rate: 0.24 },
        { upTo: 243725, rate: 0.32 },
        { upTo: 609350, rate: 0.35 },
        { upTo: Infinity, rate: 0.37 },
      ];

      function federalStandardDeduction(status) {
        // 2024 approximate standard deduction
        switch (status) {
          case "mfj":
            return 29200;
          case "hoh":
            return 21900;
          case "mfs":
            return 14600;
          case "single":
          default:
            return 14600;
        }
      }

      function computeFederalTax(status, agi) {
        const stdDed = federalStandardDeduction(status);
        const taxable = Math.max(0, agi - stdDed);
        let remaining = taxable;
        let lastLimit = 0;
        let totalTax = 0;

        for (const bracket of FEDERAL_BRACKETS_SINGLE) {
          if (remaining <= 0) break;
          const upper = bracket.upTo;
          const amountInBracket = Math.max(
            0,
            Math.min(remaining, upper - lastLimit)
          );
          totalTax += amountInBracket * bracket.rate;
          remaining -= amountInBracket;
          lastLimit = upper;
        }
        return { taxable, totalTax };
      }

      // Format helpers
      function fmtMoney(value) {
        if (!isFinite(value)) return "$0";
        return "$" + value.toFixed(0).replace(/\B(?=(\d{3})+(?!\d))/g, ",");
      }

      function fmtPercent(value) {
        if (!isFinite(value)) return "0%";
        return (value * 100).toFixed(1).replace(/\.0$/, "") + "%";
      }

      function updateCalculator(fromSlider = false) {
        const status = document.getElementById("filing-status").value;
        const grossIncomeInput = document.getElementById("gross-income");
        const pretax =
          parseFloat(document.getElementById("pretax-deductions").value) || 0;
        const wiAdj =
          parseFloat(document.getElementById("wi-adjustments").value) || 0;
        const dependents =
          parseInt(document.getElementById("dependents").value, 10) || 0;
        const otherCredits =
          parseFloat(document.getElementById("other-credits").value) || 0;
        const includeFederal =
          document.getElementById("include-federal").value === "yes";

        let grossIncome = parseFloat(grossIncomeInput.value) || 0;
        if (grossIncome < 0) grossIncome = 0;

        // Sync slider and input if needed
        const slider = document.getElementById("income-slider");
        const sliderLabel = document.getElementById("income-slider-label");
        if (fromSlider) {
          grossIncome = parseFloat(slider.value) || 0;
          grossIncomeInput.value = grossIncome;
        } else {
          slider.value = Math.max(
            slider.min,
            Math.min(slider.max, grossIncome)
          );
        }
        sliderLabel.textContent = fmtMoney(grossIncome).replace("$", "$");

        const effectivePretax = Math.min(pretax, grossIncome);
        const federalAGI = Math.max(0, grossIncome - effectivePretax);

        // Wisconsin taxable income
        const wiStdDed = estimateWiStandardDeduction(status, federalAGI);
        const wiTaxable = Math.max(0, federalAGI - wiStdDed + wiAdj);

        const wiResult = computeWiTax(wiTaxable);
        const wiGrossTax = wiResult.totalTax;
        const wiDepCredit = estimateWiDependentCredit(dependents);
        const wiTotalCredits = wiDepCredit + Math.max(0, otherCredits);
        const wiNetTax = Math.max(0, wiGrossTax - wiTotalCredits);

        // Federal tax
        let federalTax = 0;
        if (includeFederal) {
          const fed = computeFederalTax(status, federalAGI);
          federalTax = fed.totalTax;
        }

        const totalTax = wiNetTax + federalTax;
        const takeHome = Math.max(0, grossIncome - totalTax);

        // Update summary UI
        document.getElementById("wi-tax-output").textContent =
          fmtMoney(wiNetTax);
        const effRate = grossIncome > 0 ? wiNetTax / grossIncome : 0;
        document.getElementById("wi-effective-rate-output").textContent =
          fmtPercent(effRate);
        document.getElementById("wi-marginal-rate-output").textContent =
          fmtPercent(wiResult.marginalRate);
        document.getElementById("wi-taxable-output").textContent =
          fmtMoney(wiTaxable);
        document.getElementById("gross-income-summary").textContent =
          fmtMoney(grossIncome);
        document.getElementById("federal-tax-output").textContent =
          includeFederal ? fmtMoney(federalTax) : "$0";
        document.getElementById("total-tax-output").textContent =
          fmtMoney(totalTax);
        document.getElementById("takehome-output").textContent =
          fmtMoney(takeHome);

        // Update bracket table
        const tbody = document.getElementById("wi-bracket-tbody");
        tbody.innerHTML = "";
        wiResult.breakdown.forEach((row, idx) => {
          const tr = document.createElement("tr");
          tr.className = idx % 2 === 0 ? "bg-white" : "bg-gray-50";
          const label = `Bracket ${idx + 1}`;
          const rangeText = `${fmtMoney(row.range[0])} – ${
            row.range[1] === Infinity ? "∞" : fmtMoney(row.range[1])
          }`;
          tr.innerHTML = `
        <td class="px-3 py-2 text-left text-gray-700">${label}</td>
        <td class="px-3 py-2 text-gray-700">${rangeText}</td>
        <td class="px-3 py-2 text-gray-700">${fmtPercent(row.rate)}</td>
        <td class="px-3 py-2 text-gray-700">${fmtMoney(row.amount)}</td>
        <td class="px-3 py-2 text-gray-700">${fmtMoney(row.tax)}</td>
      `;
          tbody.appendChild(tr);
        });

        if (wiResult.breakdown.length === 0) {
          const tr = document.createElement("tr");
          tr.innerHTML = `<td colspan="5" class="px-3 py-3 text-center text-gray-500">No Wisconsin taxable income at the current inputs.</td>`;
          tbody.appendChild(tr);
        }
      }

      document.addEventListener("DOMContentLoaded", () => {
        const inputs = [
          "filing-status",
          "gross-income",
          "pretax-deductions",
          "wi-adjustments",
          "dependents",
          "other-credits",
          "include-federal",
        ];
        inputs.forEach((id) => {
          const el = document.getElementById(id);
          if (!el) return;
          el.addEventListener("input", () => updateCalculator(false));
          el.addEventListener("change", () => updateCalculator(false));
        });

        const slider = document.getElementById("income-slider");
        slider.addEventListener("input", () => updateCalculator(true));

        updateCalculator(false);
      });

      // MathJax config
      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
