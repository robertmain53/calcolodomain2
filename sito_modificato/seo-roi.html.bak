    <script>
      // Helper: format currency
      function formatCurrency(value) {
        if (!isFinite(value)) return "–";
        const abs = Math.abs(value);
        const sign = value < 0 ? "-" : "";
        let formatted;
        if (abs >= 1_000_000_000) {
          formatted = (abs / 1_000_000_000).toFixed(2) + "B";
        } else if (abs >= 1_000_000) {
          formatted = (abs / 1_000_000).toFixed(2) + "M";
        } else if (abs >= 1_000) {
          formatted = (abs / 1_000).toFixed(1) + "k";
        } else {
          formatted = abs.toFixed(0);
        }
        return sign + "$" + formatted;
      }

      function formatPercent(value) {
        if (!isFinite(value)) return "–";
        return value.toFixed(1) + "%";
      }

      function calculateSeoRoi() {
        const sessions =
          parseFloat(document.getElementById("monthlyOrganicSessions").value) ||
          0;
        const cr =
          (parseFloat(document.getElementById("conversionRate").value) || 0) /
          100;
        const aov =
          parseFloat(document.getElementById("avgOrderValue").value) || 0;
        const margin =
          (parseFloat(document.getElementById("profitMargin").value) || 0) /
          100;
        const monthlyCost =
          parseFloat(document.getElementById("monthlySeoCost").value) || 0;
        const attribution =
          (parseFloat(document.getElementById("attributionPercent").value) ||
            0) / 100;
        const horizon = Math.max(
          1,
          parseInt(document.getElementById("timeHorizonMonths").value, 10) || 1
        );

        const revenueType =
          document.querySelector('input[name="revenueType"]:checked')?.value ||
          "oneTime";
        const monthsRetention = Math.max(
          1,
          parseInt(document.getElementById("monthsRetention").value, 10) || 1
        );
        const discountRate =
          (parseFloat(document.getElementById("discountRate").value) || 0) /
          100;

        // Base monthly conversions and revenue
        const monthlyConversions = sessions * cr;
        const baseMonthlyRevenue = monthlyConversions * aov;

        let totalRevenue = 0;
        let totalProfit = 0;

        if (revenueType === "oneTime") {
          // Same revenue every month over horizon
          const attributedMonthlyRevenue = baseMonthlyRevenue * attribution;
          const monthlyProfit = attributedMonthlyRevenue * margin;
          totalRevenue = attributedMonthlyRevenue * horizon;
          totalProfit = monthlyProfit * horizon;
        } else {
          // Recurring: approximate LTV over horizon with optional discounting and retention cap
          for (let m = 1; m <= horizon; m++) {
            // Assume customers churn after monthsRetention
            const activeMonths = Math.min(m, monthsRetention);
            // Revenue in month m from all cohorts up to that month
            const cohortRevenue = baseMonthlyRevenue * activeMonths;
            const attributedRevenue = cohortRevenue * attribution;
            const discountFactor = Math.pow(1 - discountRate, m - 1);
            const discountedRevenue = attributedRevenue * discountFactor;
            totalRevenue += discountedRevenue;
          }
          totalProfit = totalRevenue * margin;
        }

        const totalCost = monthlyCost * horizon;
        const monthlyAttributedRevenue =
          revenueType === "oneTime"
            ? baseMonthlyRevenue * attribution
            : totalRevenue / horizon;
        const monthlyProfit =
          revenueType === "oneTime"
            ? monthlyAttributedRevenue * margin
            : totalProfit / horizon;
        const monthlyNetProfit = monthlyProfit - monthlyCost;

        const roi =
          totalCost > 0 ? ((totalProfit - totalCost) / totalCost) * 100 : NaN;
        const revenueMultiple = totalCost > 0 ? totalRevenue / totalCost : NaN;
        const profitMultiple = totalCost > 0 ? totalProfit / totalCost : NaN;
        const paybackMonths =
          monthlyNetProfit > 0 ? monthlyCost / monthlyNetProfit : Infinity;

        // Update outputs
        document.getElementById("outMonthlyConversions").textContent = isFinite(
          monthlyConversions
        )
          ? monthlyConversions.toFixed(1)
          : "–";
        document.getElementById("outMonthlyRevenue").textContent =
          formatCurrency(monthlyAttributedRevenue);
        document.getElementById("outMonthlyProfit").textContent =
          formatCurrency(monthlyProfit);
        document.getElementById("outMonthlyNetProfit").textContent =
          formatCurrency(monthlyNetProfit);
        document.getElementById("outTotalRevenue").textContent =
          formatCurrency(totalRevenue);
        document.getElementById("outTotalProfit").textContent =
          formatCurrency(totalProfit);
        document.getElementById("outTotalCost").textContent =
          formatCurrency(totalCost);
        document.getElementById("outRoiPercent").textContent =
          formatPercent(roi);
        document.getElementById("outRevenueMultiple").textContent = isFinite(
          revenueMultiple
        )
          ? revenueMultiple.toFixed(2) + "×"
          : "–";
        document.getElementById("outProfitMultiple").textContent = isFinite(
          profitMultiple
        )
          ? profitMultiple.toFixed(2) + "×"
          : "–";
        document.getElementById("outPayback").textContent =
          paybackMonths === Infinity || !isFinite(paybackMonths)
            ? "No payback within horizon"
            : paybackMonths.toFixed(1) + " months";

        // Narrative
        const narrativeEl = document.getElementById("outNarrative");
        if (!isFinite(roi)) {
          narrativeEl.textContent =
            "Enter realistic traffic, conversion, and cost assumptions to see whether your SEO investment is likely to be profitable.";
          return;
        }
        if (roi < 0) {
          narrativeEl.textContent =
            "Under these assumptions, SEO is not yet profitable over your selected time horizon. Consider lowering costs, improving conversion rate, or extending the time horizon to capture compounding gains.";
        } else if (roi < 100) {
          narrativeEl.textContent =
            "Your SEO plan is profitable but modest. You earn more than you spend, but ROI is below 100%. Small improvements in conversion rate or average order value can significantly boost returns.";
        } else {
          narrativeEl.textContent =
            "Your SEO investment looks strong: projected profit meaningfully exceeds cost over the selected horizon. Monitor actual performance monthly and refine assumptions as real data comes in.";
        }
      }

      document.addEventListener("DOMContentLoaded", function () {
        const calcBtn = document.getElementById("calculateBtn");
        calcBtn.addEventListener("click", calculateSeoRoi);

        // Toggle recurring fields
        const revenueRadios = document.querySelectorAll(
          'input[name="revenueType"]'
        );
        const recurringFields = document.getElementById("recurringFields");
        revenueRadios.forEach((r) => {
          r.addEventListener("change", () => {
            if (
              document.querySelector('input[name="revenueType"]:checked')
                .value === "recurring"
            ) {
              recurringFields.classList.remove("hidden");
            } else {
              recurringFields.classList.add("hidden");
            }
          });
        });

        // Auto-calc on load with defaults
        calculateSeoRoi();
      });

      // Mobile menu script (simple)
      document.addEventListener("DOMContentLoaded", function () {
        const toggle = document.getElementById("mobile-menu-toggle");
        const menu = document.getElementById("mobile-menu");
        if (toggle && menu) {
          toggle.addEventListener("click", () => {
            const isHidden = menu.classList.contains("hidden");
            menu.classList.toggle("hidden", !isHidden);
            toggle.setAttribute("aria-expanded", isHidden ? "true" : "false");
          });
        }
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
