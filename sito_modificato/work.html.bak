    <script>
      // Tab handling
      const tabButtons = document.querySelectorAll(".tab-btn");
      const tabs = document.querySelectorAll(".calc-tab");
      function activateTab(id) {
        tabs.forEach((t) => t.classList.toggle("hidden", t.id !== id));
        tabButtons.forEach((btn) => {
          if (btn.dataset.tab === id) {
            btn.classList.add("tab-active");
          } else {
            btn.classList.remove("tab-active");
          }
        });
      }
      tabButtons.forEach((btn) => {
        btn.addEventListener("click", () => activateTab(btn.dataset.tab));
      });
      // default tab
      activateTab("tab-work");

      function toRadians(value, unit) {
        return unit === "deg" ? (value * Math.PI) / 180 : value;
      }
      function roundSmart(x) {
        if (!isFinite(x)) return "—";
        if (Math.abs(x) === 0) return "0";
        const abs = Math.abs(x);
        if (abs >= 1e4 || abs < 1e-3) return x.toExponential(4);
        return x.toFixed(4).replace(/\.?0+$/, "");
      }

      // Work calculator
      document.getElementById("btnCalcWork").addEventListener("click", () => {
        const F = parseFloat(document.getElementById("force").value);
        const Funit = document.getElementById("forceUnit").value;
        const s = parseFloat(document.getElementById("distance").value);
        const sUnit = document.getElementById("distanceUnit").value;
        const theta = parseFloat(document.getElementById("angle").value || "0");
        const thetaUnit = document.getElementById("angleUnit").value;

        if (isNaN(F) || isNaN(s)) {
          alert("Please enter both force and displacement.");
          return;
        }

        // Force to N
        let F_N = F;
        if (Funit === "kN") F_N = F * 1000;
        if (Funit === "lbf") F_N = F * 4.4482216153;

        // Distance to m
        let s_m = s;
        if (sUnit === "cm") s_m = s / 100;
        if (sUnit === "km") s_m = s * 1000;
        if (sUnit === "ft") s_m = s * 0.3048;

        const thetaRad = toRadians(theta, thetaUnit);
        const W = F_N * s_m * Math.cos(thetaRad); // in joules

        const W_J = W;
        const W_kJ = W / 1000;

        document.getElementById("workResultValue").textContent = `${roundSmart(
          W_J
        )} J  (${roundSmart(W_kJ)} kJ)`;
        document.getElementById(
          "workResultDetail"
        ).textContent = `Using W = F · s · cos(θ) with F = ${roundSmart(
          F_N
        )} N, s = ${roundSmart(s_m)} m and θ = ${roundSmart(thetaRad)} rad.`;
        document.getElementById("workResultCard").classList.remove("hidden");
      });

      document.getElementById("btnResetWork").addEventListener("click", () => {
        ["force", "distance", "angle"].forEach(
          (id) => (document.getElementById(id).value = "")
        );
        document.getElementById("forceUnit").value = "N";
        document.getElementById("distanceUnit").value = "m";
        document.getElementById("angleUnit").value = "deg";
        document.getElementById("workResultCard").classList.add("hidden");
      });

      // Power calculator
      document.getElementById("btnCalcPower").addEventListener("click", () => {
        const W = parseFloat(document.getElementById("powerWork").value);
        const Wunit = document.getElementById("powerWorkUnit").value;
        const t = parseFloat(document.getElementById("powerTime").value);
        const tUnit = document.getElementById("powerTimeUnit").value;

        if (isNaN(W) || isNaN(t) || t <= 0) {
          alert("Please enter valid work and a positive time.");
          return;
        }

        // Work to joules
        let W_J = W;
        if (Wunit === "kJ") W_J = W * 1000;
        if (Wunit === "Wh") W_J = W * 3600;
        if (Wunit === "kWh") W_J = W * 3_600_000;

        // Time to seconds
        let t_s = t;
        if (tUnit === "min") t_s = t * 60;
        if (tUnit === "h") t_s = t * 3600;

        const P_W = W_J / t_s;
        const P_kW = P_W / 1000;
        const P_hp = P_W / 745.699872;

        document.getElementById("powerResultValue").textContent = `${roundSmart(
          P_W
        )} W  (${roundSmart(P_kW)} kW, ${roundSmart(P_hp)} hp)`;
        document.getElementById(
          "powerResultDetail"
        ).textContent = `Using P = W / t with W = ${roundSmart(
          W_J
        )} J and t = ${roundSmart(t_s)} s.`;
        document.getElementById("powerResultCard").classList.remove("hidden");
      });

      document.getElementById("btnResetPower").addEventListener("click", () => {
        ["powerWork", "powerTime"].forEach(
          (id) => (document.getElementById(id).value = "")
        );
        document.getElementById("powerWorkUnit").value = "J";
        document.getElementById("powerTimeUnit").value = "s";
        document.getElementById("powerResultCard").classList.add("hidden");
      });

      // GPE calculator
      document.getElementById("btnCalcGpe").addEventListener("click", () => {
        const m = parseFloat(document.getElementById("mass").value);
        const mUnit = document.getElementById("massUnit").value;
        const h = parseFloat(document.getElementById("height").value);
        const hUnit = document.getElementById("heightUnit").value;
        const g = parseFloat(document.getElementById("gravity").value);
        const gUnit = document.getElementById("gravityUnit").value;

        if (isNaN(m) || isNaN(h) || isNaN(g)) {
          alert("Please enter mass, height, and gravitational acceleration.");
          return;
        }

        // Mass to kg
        let m_kg = m;
        if (mUnit === "g") m_kg = m / 1000;
        if (mUnit === "lb") m_kg = m * 0.45359237;

        // Height to m
        let h_m = h;
        if (hUnit === "cm") h_m = h / 100;
        if (hUnit === "ft") h_m = h * 0.3048;

        // g to m/s^2
        let g_ms2 = g;
        if (gUnit === "fts2") g_ms2 = g * 0.3048;

        const E = m_kg * g_ms2 * h_m;
        const E_J = E;
        const E_kJ = E / 1000;

        document.getElementById("gpeResultValue").textContent = `${roundSmart(
          E_J
        )} J  (${roundSmart(E_kJ)} kJ)`;
        document.getElementById(
          "gpeResultDetail"
        ).textContent = `Using Eₚ = m g h with m = ${roundSmart(
          m_kg
        )} kg, g = ${roundSmart(g_ms2)} m/s² and h = ${roundSmart(h_m)} m.`;
        document.getElementById("gpeResultCard").classList.remove("hidden");
      });

      document.getElementById("btnResetGpe").addEventListener("click", () => {
        ["mass", "height", "gravity"].forEach(
          (id) =>
            (document.getElementById(id).value = id === "gravity" ? "9.81" : "")
        );
        document.getElementById("massUnit").value = "kg";
        document.getElementById("heightUnit").value = "m";
        document.getElementById("gravityUnit").value = "ms2";
        document.getElementById("gpeResultCard").classList.add("hidden");
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
