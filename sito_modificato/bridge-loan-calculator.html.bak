    <script>
      // Bridge loan calculator logic
      (function () {
        const modeSimpleBtn = document.getElementById("mode-simple");
        const modeAdvancedBtn = document.getElementById("mode-advanced");
        const advancedSection = document.getElementById("advanced-section");
        const advancedSummary = document.getElementById("advancedSummary");

        const loanAmountEl = document.getElementById("loanAmount");
        const interestRateEl = document.getElementById("interestRate");
        const termMonthsEl = document.getElementById("termMonths");
        const feePercentEl = document.getElementById("feePercent");
        const feeFlatEl = document.getElementById("feeFlat");
        const feeFinancedEl = document.getElementById("feeFinanced");
        const interestCalcEl = document.getElementById("interestCalc");
        const interestPaymentEl = document.getElementById("interestPayment");

        const currentValueEl = document.getElementById("currentValue");
        const currentMortgageEl = document.getElementById("currentMortgage");
        const maxLtvEl = document.getElementById("maxLtv");
        const newPriceEl = document.getElementById("newPrice");
        const newDownPaymentEl = document.getElementById("newDownPayment");
        const otherCashEl = document.getElementById("otherCash");

        const resEffectiveLoan = document.getElementById("resEffectiveLoan");
        const resMonthlyPayment = document.getElementById("resMonthlyPayment");
        const resTotalInterest = document.getElementById("resTotalInterest");
        const resTotalFees = document.getElementById("resTotalFees");
        const resCashUpfront = document.getElementById("resCashUpfront");
        const resNetCash = document.getElementById("resNetCash");
        const resPayoff = document.getElementById("resPayoff");
        const summaryTableBody = document.getElementById("summaryTableBody");
        const errorMsg = document.getElementById("errorMsg");

        const calcBtn = document.getElementById("calcBtn");
        const resetBtn = document.getElementById("resetBtn");

        let mode = "simple";

        function setMode(newMode) {
          mode = newMode;
          if (mode === "advanced") {
            advancedSection.classList.remove("hidden");
            advancedSummary.classList.remove("hidden");
            modeAdvancedBtn.classList.add("toggle-btn-active");
            modeAdvancedBtn.classList.remove("toggle-btn-inactive");
            modeSimpleBtn.classList.remove("toggle-btn-active");
            modeSimpleBtn.classList.add("toggle-btn-inactive");
          } else {
            advancedSection.classList.add("hidden");
            advancedSummary.classList.add("hidden");
            modeSimpleBtn.classList.add("toggle-btn-active");
            modeSimpleBtn.classList.remove("toggle-btn-inactive");
            modeAdvancedBtn.classList.remove("toggle-btn-active");
            modeAdvancedBtn.classList.add("toggle-btn-inactive");
          }
        }

        modeSimpleBtn.addEventListener("click", () => setMode("simple"));
        modeAdvancedBtn.addEventListener("click", () => setMode("advanced"));

        function parseNum(el) {
          const v = parseFloat(el.value.replace(/,/g, ""));
          return isNaN(v) ? 0 : v;
        }

        function formatMoney(v) {
          if (!isFinite(v)) return "$0";
          const sign = v < 0 ? "-" : "";
          const abs = Math.abs(v);
          return (
            sign +
            "$" +
            abs.toLocaleString(undefined, {
              maximumFractionDigits: 2,
              minimumFractionDigits: 2,
            })
          );
        }

        function calculateLoanAmountFromAdvanced() {
          const currentValue = parseNum(currentValueEl);
          const currentMortgage = parseNum(currentMortgageEl);
          const maxLtv = parseNum(maxLtvEl) / 100;
          const newPrice = parseNum(newPriceEl);
          const newDown = parseNum(newDownPaymentEl);
          const otherCash = parseNum(otherCashEl);

          const maxBridgeOnCurrent = Math.max(
            0,
            currentValue * maxLtv - currentMortgage
          );
          const cashNeeded = Math.max(0, newPrice - newDown - otherCash);

          return Math.max(0, Math.min(maxBridgeOnCurrent, cashNeeded));
        }

        function buildSummaryRows(
          effectiveLoan,
          annualRate,
          termMonths,
          interestCalc,
          interestPayment
        ) {
          const terms = [3, 6, 9, termMonths]
            .filter((v, idx, arr) => v > 0 && arr.indexOf(v) === idx)
            .sort((a, b) => a - b);
          const rows = [];
          terms.forEach((m) => {
            const months = m;
            let totalInterest = 0;
            const principal = effectiveLoan;
            if (interestCalc === "monthly") {
              const monthlyRate = annualRate / 12;
              totalInterest = principal * monthlyRate * months;
            } else {
              const daysPerYear = interestCalc === "daily365" ? 365 : 360;
              const days = months * 30;
              const dailyRate = annualRate / daysPerYear;
              totalInterest = principal * dailyRate * days;
            }
            const monthlyPayment =
              interestPayment === "monthly"
                ? interestCalc === "monthly"
                  ? principal * (annualRate / 12)
                  : totalInterest / months
                : 0;
            const payoff =
              interestPayment === "rolled"
                ? principal + totalInterest
                : principal;

            rows.push({
              months,
              monthlyPayment,
              totalInterest,
              payoff,
            });
          });
          return rows;
        }

        function renderSummaryTable(rows) {
          if (!rows || rows.length === 0) {
            summaryTableBody.innerHTML =
              '<tr><td class="px-3 py-2 text-gray-500 text-center" colspan="4">Run a calculation to see details.</td></tr>';
            return;
          }
          summaryTableBody.innerHTML = rows
            .map(
              (r) => `
        <tr class="border-t border-gray-100">
          <td class="px-3 py-2 text-gray-800">${r.months} month${
                r.months === 1 ? "" : "s"
              }</td>
          <td class="px-3 py-2 text-right">${formatMoney(r.monthlyPayment)}</td>
          <td class="px-3 py-2 text-right">${formatMoney(r.totalInterest)}</td>
          <td class="px-3 py-2 text-right">${formatMoney(r.payoff)}</td>
        </tr>
      `
            )
            .join("");
        }

        function calculate() {
          errorMsg.textContent = "";

          let loanAmount = parseNum(loanAmountEl);
          if (mode === "advanced") {
            loanAmount = calculateLoanAmountFromAdvanced();
            loanAmountEl.value = loanAmount.toFixed(0);
          }

          const rate = parseNum(interestRateEl) / 100;
          const termMonths = parseInt(termMonthsEl.value, 10) || 0;
          const feePercent = parseNum(feePercentEl) / 100;
          const feeFlat = parseNum(feeFlatEl);
          const feeFinanced = feeFinancedEl.value;
          const interestCalc = interestCalcEl.value;
          const interestPayment = interestPaymentEl.value;

          if (loanAmount <= 0) {
            errorMsg.textContent =
              "Please enter a positive bridge loan amount or adjust your property values.";
            return;
          }
          if (rate < 0) {
            errorMsg.textContent = "Interest rate cannot be negative.";
            return;
          }
          if (termMonths <= 0) {
            errorMsg.textContent = "Term length must be at least 1 month.";
            return;
          }

          const feeFromPercent = loanAmount * feePercent;
          const totalFees = feeFromPercent + feeFlat;

          const financedFees = feeFinanced === "financed" ? totalFees : 0;
          const cashFees = feeFinanced === "cash" ? totalFees : 0;

          const effectiveLoan = loanAmount + financedFees;

          let totalInterest = 0;
          let monthlyPayment = 0;

          if (interestCalc === "monthly") {
            const monthlyRate = rate / 12;
            totalInterest = effectiveLoan * monthlyRate * termMonths;
            if (interestPayment === "monthly") {
              monthlyPayment = effectiveLoan * monthlyRate;
            } else {
              monthlyPayment = 0;
            }
          } else {
            const daysPerYear = interestCalc === "daily365" ? 365 : 360;
            const days = termMonths * 30;
            const dailyRate = rate / daysPerYear;
            totalInterest = effectiveLoan * dailyRate * days;
            if (interestPayment === "monthly") {
              monthlyPayment = totalInterest / termMonths;
            } else {
              monthlyPayment = 0;
            }
          }

          const payoff =
            interestPayment === "rolled"
              ? effectiveLoan + totalInterest
              : effectiveLoan;

          const cashUpfront = cashFees + (interestPayment === "rolled" ? 0 : 0);
          const netCash = loanAmount - cashFees;

          resEffectiveLoan.textContent = formatMoney(effectiveLoan);
          resMonthlyPayment.textContent = formatMoney(monthlyPayment);
          resTotalInterest.textContent = formatMoney(totalInterest);
          resTotalFees.textContent = formatMoney(totalFees);
          resCashUpfront.textContent = formatMoney(cashUpfront);
          resNetCash.textContent = formatMoney(netCash);
          resPayoff.textContent = formatMoney(payoff);

          const summaryRows = buildSummaryRows(
            effectiveLoan,
            rate,
            termMonths,
            interestCalc,
            interestPayment
          );
          renderSummaryTable(summaryRows);
        }

        function reset() {
          setMode("simple");
          loanAmountEl.value = 200000;
          interestRateEl.value = 8.5;
          termMonthsEl.value = 12;
          feePercentEl.value = 2;
          feeFlatEl.value = 1500;
          feeFinancedEl.value = "financed";
          interestCalcEl.value = "monthly";
          interestPaymentEl.value = "monthly";

          currentValueEl.value = 500000;
          currentMortgageEl.value = 300000;
          maxLtvEl.value = 75;
          newPriceEl.value = 600000;
          newDownPaymentEl.value = 60000;
          otherCashEl.value = 0;

          resEffectiveLoan.textContent = "$0";
          resMonthlyPayment.textContent = "$0";
          resTotalInterest.textContent = "$0";
          resTotalFees.textContent = "$0";
          resCashUpfront.textContent = "$0";
          resNetCash.textContent = "$0";
          resPayoff.textContent = "$0";
          renderSummaryTable([]);
          errorMsg.textContent = "";
        }

        calcBtn.addEventListener("click", calculate);
        resetBtn.addEventListener("click", reset);

        // FAQ toggle (simple)
        document
          .querySelectorAll("#faq-container .faq-question")
          .forEach((btn) => {
            btn.addEventListener("click", () => {
              const content = btn.nextElementSibling;
              const isHidden = content.classList.contains("hidden");
              document
                .querySelectorAll("#faq-container .faq-question + div")
                .forEach((div) => div.classList.add("hidden"));
              document
                .querySelectorAll(
                  "#faq-container .faq-question span:last-child"
                )
                .forEach((icon) => (icon.textContent = "+"));
              if (isHidden) {
                content.classList.remove("hidden");
                btn.querySelector("span:last-child").textContent = "−";
              }
            });
          });

        // Initial render
        renderSummaryTable([]);
      })();
    </script>

