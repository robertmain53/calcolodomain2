    <script>
      // Tabs
      const tabButtons = document.querySelectorAll('.tab-button');
      const tabPanels = document.querySelectorAll('.tab-panel');

      tabButtons.forEach(btn => {
        btn.addEventListener('click', () => {
          const target = btn.getAttribute('data-tab');
          tabButtons.forEach(b => b.classList.remove('tab-button-active', 'bg-blue-50', 'text-blue-900', 'border-blue-100'));
          btn.classList.add('tab-button-active', 'bg-blue-50', 'text-blue-900', 'border-blue-100');
          tabPanels.forEach(panel => {
            panel.classList.toggle('hidden', panel.id !== 'tab-' + target);
          });
        });
      });

      // Dynamic viscosity factors - base Pa·s
      const DYN_FACTORS = {
        pa_s: 1,
        mpa_s: 1e-3,     // 1 mPa·s = 0.001 Pa·s
        cp: 1e-3,        // 1 cP = 0.001 Pa·s
        p: 0.1,          // 1 Poise = 0.1 Pa·s
        lbf_s_ft2: 47.88025898 // 1 lbf·s/ft² = 47.88025898 Pa·s
      };

      const dynFromValue = document.getElementById('dyn-from-value');
      const dynFromUnit = document.getElementById('dyn-from-unit');
      const dynToValue = document.getElementById('dyn-to-value');
      const dynToUnit = document.getElementById('dyn-to-unit');

      function updateDynamicFromSource() {
        const val = parseFloat(dynFromValue.value || 0);
        const basePaS = val * DYN_FACTORS[dynFromUnit.value];
        const result = basePaS / DYN_FACTORS[dynToUnit.value];
        dynToValue.value = +result.toFixed(6);
      }

      function updateDynamicFromTarget() {
        const val = parseFloat(dynToValue.value || 0);
        const basePaS = val * DYN_FACTORS[dynToUnit.value];
        const result = basePaS / DYN_FACTORS[dynFromUnit.value];
        dynFromValue.value = +result.toFixed(6);
      }

      [dynFromValue, dynFromUnit, dynToUnit].forEach(el => el.addEventListener('input', updateDynamicFromSource));
      dynToValue.addEventListener('input', updateDynamicFromTarget);
      updateDynamicFromSource();

      // Kinematic viscosity factors - base m²/s
      const KIN_FACTORS = {
        m2_s: 1,
        st: 1e-4,    // 1 St = 1e-4 m²/s
        cst: 1e-6,   // 1 cSt = 1e-6 m²/s
        ft2_s: 0.09290304 // 1 ft²/s = 0.09290304 m²/s
      };

      const kinFromValue = document.getElementById('kin-from-value');
      const kinFromUnit = document.getElementById('kin-from-unit');
      const kinToValue = document.getElementById('kin-to-value');
      const kinToUnit = document.getElementById('kin-to-unit');

      function updateKinematicFromSource() {
        const val = parseFloat(kinFromValue.value || 0);
        const base = val * KIN_FACTORS[kinFromUnit.value];
        const res = base / KIN_FACTORS[kinToUnit.value];
        kinToValue.value = +res.toFixed(6);
      }

      function updateKinematicFromTarget() {
        const val = parseFloat(kinToValue.value || 0);
        const base = val * KIN_FACTORS[kinToUnit.value];
        const res = base / KIN_FACTORS[kinFromUnit.value];
        kinFromValue.value = +res.toFixed(6);
      }

      [kinFromValue, kinFromUnit, kinToUnit].forEach(el => el.addEventListener('input', updateKinematicFromSource));
      kinToValue.addEventListener('input', updateKinematicFromTarget);
      updateKinematicFromSource();

      // Interconversion μ ↔ ν
      const interDyn = document.getElementById('inter-dyn');
      const interDynUnit = document.getElementById('inter-dyn-unit');
      const interDensity = document.getElementById('inter-density');
      const interDensityUnit = document.getElementById('inter-density-unit');
      const interKin = document.getElementById('inter-kin');
      const interKinUnit = document.getElementById('inter-kin-unit');

      function dynToPaS(val, unit) {
        if (unit === 'pa_s') return val;
        if (unit === 'mpa_s' || unit === 'cp') return val * 1e-3;
        return val;
      }

      function densityToKgM3(val, unit) {
        if (unit === 'kg_m3') return val;
        if (unit === 'g_cm3') return val * 1000;
        return val;
      }

      function kinFromM2S(val, unit) {
        return val / KIN_FACTORS[unit];
      }

      function updateInter() {
        const muPaS = dynToPaS(parseFloat(interDyn.value || 0), interDynUnit.value);
        const rho = densityToKgM3(parseFloat(interDensity.value || 0), interDensityUnit.value);
        if (rho === 0) {
          interKin.value = '';
          return;
        }
        const nu = muPaS / rho; // m²/s
        const res = kinFromM2S(nu, interKinUnit.value);
        interKin.value = +res.toFixed(6);
      }

      [interDyn, interDynUnit, interDensity, interDensityUnit, interKinUnit].forEach(el => el.addEventListener('input', updateInter));
      updateInter();
    </script>

