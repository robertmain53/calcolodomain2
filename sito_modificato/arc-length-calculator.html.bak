            <script>
              document.addEventListener('DOMContentLoaded', function() {
                const inputRadius = document.getElementById('input-radius');
                const inputAngle = document.getElementById('input-angle');
                const angleUnit = document.getElementById('angle-unit');
                const calculateBtn = document.getElementById('calculate-btn');
                const resultsContainer = document.getElementById('results-container');
                const errorMessage = document.getElementById('error-message');
                
                const resultArcLength = document.getElementById('result-arc-length');
                const resultCircumference = document.getElementById('result-circumference');
                const resultSectorArea = document.getElementById('result-sector-area');
                const stepByStep = document.getElementById('step-by-step');

                const R_TO_D = 180 / Math.PI;
                const D_TO_R = Math.PI / 180;

                // --- Helper Functions ---
                function parseInput(val) {
                    const num = parseFloat(val);
                    return (num >= 0 && !isNaN(num)) ? num : NaN;
                }
                
                function formatNumber(num) {
                    if (Math.abs(num) < 1e-10) return '0';
                    return parseFloat(num.toFixed(6)).toString();
                }

                function showError(message) {
                    errorMessage.textContent = `Error: ${message}`;
                    resultsContainer.classList.add('hidden');
                }

                // --- Main Calculation Logic ---
                calculateBtn.addEventListener('click', () => {
                    const r = parseInput(inputRadius.value);
                    const angleRaw = parseInput(inputAngle.value);
                    const unit = angleUnit.value;
                    
                    try {
                        if (isNaN(r) || isNaN(angleRaw)) {
                            throw new Error('Please enter valid, non-negative numbers for Radius and Angle.');
                        }
                        if (r <= 0) {
                            throw new Error('Radius must be greater than zero.');
                        }
                        if (unit === 'degrees' && (angleRaw > 360)) {
                             // Allow >360 but note that it represents multiple full circles.
                        }

                        errorMessage.textContent = '';
                        
                        let angleRad;
                        let angleDeg;
                        let unitStep = '';

                        // 1. Convert to Radians (for universal calculation)
                        if (unit === 'degrees') {
                            angleDeg = angleRaw;
                            angleRad = angleRaw * D_TO_R;
                            unitStep = `<p>1. <strong>Angle Conversion:</strong> Convert degrees to radians: $\\theta_{\\text{rad}} = ${formatNumber(angleDeg)}^\\circ \\times \\frac{\\pi}{180^\\circ} \\approx ${formatNumber(angleRad)}$ rad</p>`;
                        } else {
                            angleRad = angleRaw;
                            angleDeg = angleRaw * R_TO_D;
                            unitStep = `<p>1. <strong>Angle in Radians:</strong> $\\theta = ${formatNumber(angleRad)}$ rad</p>`;
                        }

                        // 2. Calculate Properties
                        const circumference = 2 * Math.PI * r;
                        const arcLength = r * angleRad;
                        const sectorArea = 0.5 * r * r * angleRad;
                        
                        // --- Step-by-Step Construction ---
                        let steps = unitStep;
                        const fR = formatNumber(r);
                        const fThetaRad = formatNumber(angleRad);
                        const fThetaDeg = formatNumber(angleDeg);

                        // 2. Arc Length Step
                        steps += `<h4 class="font-semibold text-lg mt-4">2. Calculate Arc Length ($L$):</h4>`;
                        if (unit === 'degrees') {
                            steps += `<p>Formula (Degrees): $L = 2\\pi r \\left( \\frac{\\theta}{360^\\circ} \\right)$</p>`;
                            steps += `<p class="formula-box text-center">$L = 2\\pi \\times ${fR} \\left( \\frac{${fThetaDeg}}{360^\\circ} \\right) \\approx ${formatNumber(arcLength)}$</p>`;
                        } else {
                            steps += `<p>Formula (Radians): $L = r \\theta$</p>`;
                            steps += `<p class="formula-box text-center">$L = ${fR} \\times ${fThetaRad} \\approx ${formatNumber(arcLength)}$</p>`;
                        }

                        // 3. Circumference Step
                        steps += `<h4 class="font-semibold text-lg mt-4">3. Calculate Full Circumference ($C$):</h4>`;
                        steps += `<p>Formula: $C = 2\\pi r$</p>`;
                        steps += `<p class="formula-box text-center">$C = 2\\pi \\times ${fR} \\approx ${formatNumber(circumference)}$</p>`;
                        
                        // 4. Sector Area Step
                        steps += `<h4 class="font-semibold text-lg mt-4">4. Calculate Sector Area ($\mathcal{A}$):</h4>`;
                        steps += `<p>Formula: $\\mathcal{A} = \\frac{1}{2} r L$ (using arc length)</p>`;
                        steps += `<p class="formula-box text-center">$\\mathcal{A} = \\frac{1}{2} \\times ${fR} \\times ${formatNumber(arcLength)} \\approx ${formatNumber(sectorArea)}$</p>`;
                        
                        // 5. Update DOM
                        resultArcLength.textContent = formatNumber(arcLength);
                        resultCircumference.textContent = formatNumber(circumference);
                        resultSectorArea.textContent = formatNumber(sectorArea);
                        stepByStep.innerHTML = steps;
                        resultsContainer.classList.remove('hidden');

                        // Re-render MathJax
                        if (window.MathJax) {
                            window.MathJax.typesetPromise([stepByStep, resultArcLength, resultCircumference, resultSectorArea]);
                        }

                    } catch (e) {
                        showError(e.message);
                    }
                });

                // Initial calculation (for example values)
                calculateBtn.click();
            </script>

