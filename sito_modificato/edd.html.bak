    <script>
      // Tab switching
      const tabs = document.querySelectorAll(".edd-tab");
      const panels = {
        lmp: document.getElementById("panel-lmp"),
        conception: document.getElementById("panel-conception"),
        ivf: document.getElementById("panel-ivf"),
      };
      let currentMethod = "lmp";

      tabs.forEach((tab) => {
        tab.addEventListener("click", () => {
          const target = tab.getAttribute("data-tab");
          currentMethod = target;
          tabs.forEach((t) => {
            if (t === tab) {
              t.classList.remove("bg-gray-100", "text-gray-700");
              t.classList.add("bg-white", "text-blue-600");
            } else {
              t.classList.remove("bg-white", "text-blue-600");
              t.classList.add("bg-gray-100", "text-gray-700");
            }
          });
          Object.keys(panels).forEach((key) => {
            panels[key].classList.toggle("hidden", key !== target);
          });
        });
      });

      function formatDate(date) {
        if (!date) return "—";
        return date.toLocaleDateString(undefined, {
          year: "numeric",
          month: "short",
          day: "numeric",
        });
      }

      function addDays(date, days) {
        const d = new Date(date.getTime());
        d.setDate(d.getDate() + days);
        return d;
      }

      function diffInDays(a, b) {
        const ms = b.getTime() - a.getTime();
        return Math.floor(ms / (1000 * 60 * 60 * 24));
      }

      function calculateEDD() {
        const today = new Date();
        let edd = null;
        let lmpEquivalent = null; // gestational age reference

        if (currentMethod === "lmp") {
          const lmpInput = document.getElementById("lmp-date").value;
          const cycleLength = parseInt(
            document.getElementById("cycle-length").value,
            10
          );
          if (!lmpInput || isNaN(cycleLength)) {
            alert("Please enter your LMP date and cycle length.");
            return;
          }
          const lmpDate = new Date(lmpInput + "T12:00:00");
          const adjustment = cycleLength - 28;
          edd = addDays(lmpDate, 280 + adjustment);
          lmpEquivalent = addDays(lmpDate, adjustment); // adjust LMP so ovulation aligns
        } else if (currentMethod === "conception") {
          const concInput = document.getElementById("conception-date").value;
          if (!concInput) {
            alert("Please enter your conception date.");
            return;
          }
          const concDate = new Date(concInput + "T12:00:00");
          // Gestational age is conception + 14 days
          lmpEquivalent = addDays(concDate, -14);
          edd = addDays(lmpEquivalent, 280);
        } else if (currentMethod === "ivf") {
          const transferInput =
            document.getElementById("ivf-transfer-date").value;
          const embryoAge = parseInt(
            document.getElementById("ivf-embryo-age").value,
            10
          );
          if (!transferInput || isNaN(embryoAge)) {
            alert("Please enter the embryo transfer date and embryo age.");
            return;
          }
          const transferDate = new Date(transferInput + "T12:00:00");
          // Approximate conception equivalent:
          // Day 3: transfer ≈ conception + 3 days -> conception ≈ transfer - 3
          // Day 5: transfer ≈ conception + 5 days -> conception ≈ transfer - 5
          const conceptionEquivalent = addDays(transferDate, -embryoAge);
          lmpEquivalent = addDays(conceptionEquivalent, -14);
          edd = addDays(lmpEquivalent, 280);
        }

        if (!edd || !lmpEquivalent) return;

        // Gestational age today
        const daysPregnant = diffInDays(lmpEquivalent, today);
        const weeks = Math.floor(daysPregnant / 7);
        const days = daysPregnant % 7;

        let gaText = "—";
        if (daysPregnant < 0) {
          gaText = "Pregnancy not started yet (based on entered dates)";
        } else {
          gaText = `${weeks} weeks ${days} day${
            days !== 1 ? "s" : ""
          } pregnant`;
        }

        // Trimester
        let trimester = "—";
        if (weeks < 14) trimester = "First trimester";
        else if (weeks < 28) trimester = "Second trimester";
        else trimester = "Third trimester";

        // Trimester date ranges
        const tri1Start = lmpEquivalent;
        const tri1End = addDays(lmpEquivalent, 13 * 7 + 6);
        const tri2Start = addDays(tri1End, 1);
        const tri2End = addDays(lmpEquivalent, 27 * 7 + 6);
        const tri3Start = addDays(tri2End, 1);

        // Typical delivery window 37–42 weeks
        const windowStart = addDays(lmpEquivalent, 37 * 7);
        const windowEnd = addDays(lmpEquivalent, 42 * 7);

        document.getElementById("edd-date").textContent = formatDate(edd);
        document.getElementById(
          "edd-week-range"
        ).textContent = `Typical delivery window: ${formatDate(
          windowStart
        )} – ${formatDate(windowEnd)}`;
        document.getElementById("edd-gestation").textContent = gaText;
        document.getElementById("edd-trimester").textContent = trimester;
        document.getElementById("tri1-range").textContent = `${formatDate(
          tri1Start
        )} – ${formatDate(tri1End)}`;
        document.getElementById("tri2-range").textContent = `${formatDate(
          tri2Start
        )} – ${formatDate(tri2End)}`;
        document.getElementById("tri3-range").textContent = `${formatDate(
          tri3Start
        )} – ${formatDate(edd)}`;

        document.getElementById("edd-results").classList.remove("hidden");
      }

      document
        .getElementById("edd-calc-btn")
        .addEventListener("click", calculateEDD);

      document.getElementById("edd-reset-btn").addEventListener("click", () => {
        document.getElementById("lmp-date").value = "";
        document.getElementById("cycle-length").value = 28;
        document.getElementById("conception-date").value = "";
        document.getElementById("ivf-transfer-date").value = "";
        document.getElementById("ivf-embryo-age").value = 5;
        document.getElementById("edd-results").classList.add("hidden");
      });

      // Set default LMP to 2 weeks ago for convenience
      (function presetLMP() {
        const lmpInput = document.getElementById("lmp-date");
        if (!lmpInput) return;
        const d = new Date();
        d.setDate(d.getDate() - 14);
        const iso = d.toISOString().slice(0, 10);
        lmpInput.value = iso;
      })();

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
