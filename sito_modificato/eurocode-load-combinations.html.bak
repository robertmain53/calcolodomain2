    <script>
      // Toggle mobile menu
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          this.setAttribute("aria-expanded", isHidden ? "true" : "false");
        });

      const combTypeButtons = document.querySelectorAll(".comb-type-btn");
      let currentCombType = "uls-str";

      combTypeButtons.forEach((btn) => {
        btn.addEventListener("click", () => {
          combTypeButtons.forEach((b) => {
            b.classList.remove("border-blue-600", "text-blue-600");
          });
          btn.classList.add("border-blue-600", "text-blue-600");
          currentCombType = btn.getAttribute("data-comb-type");
          const label = document.getElementById("combTypeLabel");
          const map = {
            "uls-str": "ULS STR/GEO (fundamental)",
            "uls-equ": "ULS EQU (equilibrium)",
            "uls-acc": "ULS Accidental",
            "sls-char": "SLS Characteristic",
            "sls-freq": "SLS Frequent",
            "sls-quasi": "SLS Quasi-permanent",
          };
          label.textContent = "Current: " + (map[currentCombType] || "");
        });
      });

      function val(id) {
        const v = parseFloat(document.getElementById(id).value);
        return isNaN(v) ? 0 : v;
      }

      function formatNumber(x) {
        return Number.isFinite(x) ? x.toFixed(3).replace(/\.?0+$/, "") : "";
      }

      function generateULS_STR(
        Gk,
        Pk,
        Q1,
        Q2,
        Q3,
        gammaG,
        gammaP,
        gammaQ,
        psi0_2,
        psi0_3
      ) {
        const combos = [];
        const vars = [
          { name: "Qk,1", value: Q1, psi0: 1.0 },
          { name: "Qk,2", value: Q2, psi0: psi0_2 },
          { name: "Qk,3", value: Q3, psi0: psi0_3 },
        ];
        vars.forEach((lead, idx) => {
          if (!lead.value) return;
          let expr = `γG·Gk + γP·Pk + γQ·${lead.name}`;
          let Ed = gammaG * Gk + gammaP * Pk + gammaQ * lead.value;
          vars.forEach((acc, j) => {
            if (j === idx || !acc.value) return;
            expr += ` + γQ·ψ0·${acc.name}`;
            Ed += gammaQ * acc.psi0 * acc.value;
          });
          combos.push({
            type: "ULS STR/GEO",
            expression: expr,
            value: Ed,
          });
        });
        return combos;
      }

      function generateULS_EQU(
        Gk,
        Q1,
        Q2,
        Q3,
        gammaGstb,
        gammaGdst,
        gammaQequ,
        psi0_2,
        psi0_3
      ) {
        const combos = [];
        const vars = [
          { name: "Qk,1", value: Q1, psi0: 1.0 },
          { name: "Qk,2", value: Q2, psi0: psi0_2 },
          { name: "Qk,3", value: Q3, psi0: psi0_3 },
        ];
        vars.forEach((lead, idx) => {
          if (!lead.value) return;
          let expr = `γG,stb·Gk,stb + γG,dst·Gk,dst + γQ·${lead.name}`;
          let Ed = gammaGstb * Gk + gammaGdst * 0 + gammaQequ * lead.value; // user must split Gk externally; here we treat all as stabilising for simplicity
          vars.forEach((acc, j) => {
            if (j === idx || !acc.value) return;
            expr += ` + γQ·ψ0·${acc.name}`;
            Ed += gammaQequ * acc.psi0 * acc.value;
          });
          combos.push({
            type: "ULS EQU",
            expression: expr,
            value: Ed,
          });
        });
        return combos;
      }

      function generateULS_Acc(
        Gk,
        Pk,
        Q1,
        Q2,
        Q3,
        Ak,
        gammaG,
        gammaP,
        gammaA,
        psi2_1,
        psi2_2,
        psi2_3
      ) {
        const combos = [];
        if (!Ak) return combos;
        const expr = `γG·Gk + γP·Pk + γA·Ad + ψ2,1·Qk,1 + ψ2,2·Qk,2 + ψ2,3·Qk,3`;
        const Ed =
          gammaG * Gk +
          gammaP * Pk +
          gammaA * Ak +
          psi2_1 * Q1 +
          psi2_2 * Q2 +
          psi2_3 * Q3;
        combos.push({
          type: "ULS Accidental",
          expression: expr,
          value: Ed,
        });
        return combos;
      }

      function generateSLS(
        Gk,
        Pk,
        Q1,
        Q2,
        Q3,
        psi0_2,
        psi0_3,
        psi1_1,
        psi2_1,
        psi2_2,
        psi2_3,
        type
      ) {
        const combos = [];
        if (type === "sls-char") {
          const vars = [
            { name: "Qk,1", value: Q1, psi0_2: psi0_2, psi0_3: psi0_3 },
            { name: "Qk,2", value: Q2, psi0_2: 1.0, psi0_3: psi0_3 },
            { name: "Qk,3", value: Q3, psi0_2: psi0_2, psi0_3: 1.0 },
          ];
          vars.forEach((lead, idx) => {
            if (!lead.value) return;
            let expr = `Gk + Pk + ${lead.name}`;
            let Ed = Gk + Pk + lead.value;
            if (idx !== 0 && Q1) {
              expr += ` + ψ0,1·Qk,1`;
              Ed += 0.7 * Q1; // illustrative
            }
            if (idx !== 1 && Q2) {
              expr += ` + ψ0,2·Qk,2`;
              Ed += psi0_2 * Q2;
            }
            if (idx !== 2 && Q3) {
              expr += ` + ψ0,3·Qk,3`;
              Ed += psi0_3 * Q3;
            }
            combos.push({
              type: "SLS characteristic",
              expression: expr,
              value: Ed,
            });
          });
        } else if (type === "sls-freq") {
          const expr = `Gk + Pk + ψ1,1·Qk,1 + ψ2,2·Qk,2 + ψ2,3·Qk,3`;
          const Ed = Gk + Pk + psi1_1 * Q1 + psi2_2 * Q2 + psi2_3 * Q3;
          combos.push({
            type: "SLS frequent",
            expression: expr,
            value: Ed,
          });
        } else if (type === "sls-quasi") {
          const expr = `Gk + Pk + ψ2,1·Qk,1 + ψ2,2·Qk,2 + ψ2,3·Qk,3`;
          const Ed = Gk + Pk + psi2_1 * Q1 + psi2_2 * Q2 + psi2_3 * Q3;
          combos.push({
            type: "SLS quasi-permanent",
            expression: expr,
            value: Ed,
          });
        }
        return combos;
      }

      document.getElementById("generateBtn").addEventListener("click", () => {
        const Gk = val("gk");
        const Pk = val("pk");
        const Q1 = val("qk1");
        const Q2 = val("qk2");
        const Q3 = val("qk3");
        const Ak = val("ak");

        const gammaG = val("gammaG") || 1.35;
        const gammaQ = val("gammaQ") || 1.5;
        const gammaP = val("gammaP") || 1.0;
        const gammaA = val("gammaA") || 1.0;
        const gammaGstb = val("gammaGstb") || 0.9;
        const gammaGdst = val("gammaGdst") || 1.1;
        const gammaQequ = val("gammaQequ") || 1.5;

        const psi0_2 = val("psi0_2") || 0.6;
        const psi0_3 = val("psi0_3") || 0.7;
        const psi1_1 = val("psi1_1") || 0.5;
        const psi2_1 = val("psi2_1") || 0.3;
        const psi2_2 = 0.0; // typical for wind
        const psi2_3 = 0.2; // typical for snow

        let combos = [];
        if (currentCombType === "uls-str") {
          combos = generateULS_STR(
            Gk,
            Pk,
            Q1,
            Q2,
            Q3,
            gammaG,
            gammaP,
            gammaQ,
            psi0_2,
            psi0_3
          );
        } else if (currentCombType === "uls-equ") {
          combos = generateULS_EQU(
            Gk,
            Q1,
            Q2,
            Q3,
            gammaGstb,
            gammaGdst,
            gammaQequ,
            psi0_2,
            psi0_3
          );
        } else if (currentCombType === "uls-acc") {
          combos = generateULS_Acc(
            Gk,
            Pk,
            Q1,
            Q2,
            Q3,
            Ak,
            gammaG,
            gammaP,
            gammaA,
            psi2_1,
            psi2_2,
            psi2_3
          );
        } else {
          combos = generateSLS(
            Gk,
            Pk,
            Q1,
            Q2,
            Q3,
            psi0_2,
            psi0_3,
            psi1_1,
            psi2_1,
            psi2_2,
            psi2_3,
            currentCombType
          );
        }

        const tbody = document.getElementById("resultsBody");
        tbody.innerHTML = "";
        if (!combos.length) {
          const tr = document.createElement("tr");
          tr.innerHTML =
            '<td class="px-3 py-2 text-gray-500" colspan="4">No valid combinations generated. Check that you entered at least one non-zero action.</td>';
          tbody.appendChild(tr);
          return;
        }

        combos.forEach((c, i) => {
          const tr = document.createElement("tr");
          tr.className = i % 2 ? "bg-gray-50" : "";
          tr.innerHTML = `
        <td class="px-3 py-2">${i + 1}</td>
        <td class="px-3 py-2">
          <span class="badge bg-blue-100 text-blue-800">${c.type}</span>
        </td>
        <td class="px-3 py-2 font-mono text-xs md:text-sm">${c.expression}</td>
        <td class="px-3 py-2 text-right font-semibold">${formatNumber(
          c.value
        )}</td>
      `;
          tbody.appendChild(tr);
        });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
