    <script>
      // Core calculation logic
      function formatCurrency(value) {
        if (!isFinite(value)) return "–";
        const abs = Math.abs(value);
        const opts = { minimumFractionDigits: 2, maximumFractionDigits: 2 };
        const formatted = abs.toLocaleString(undefined, opts);
        return (value < 0 ? "-" : "") + "$" + formatted;
      }

      function formatPercent(value) {
        if (!isFinite(value)) return "–";
        const opts = { minimumFractionDigits: 2, maximumFractionDigits: 2 };
        return value.toLocaleString(undefined, opts) + "%";
      }

      function formatNumber(value) {
        if (!isFinite(value)) return "–";
        return Math.round(value).toLocaleString();
      }

      function computeTargets(
        positionType,
        entry,
        shares,
        maxRisk,
        riskPerShare,
        totalFees,
        targets
      ) {
        const rows = [];
        const isLong = positionType === "long";

        targets.forEach((t, idx) => {
          if (!t || t <= 0) return;
          const priceMovePerShare = isLong ? t - entry : entry - t;
          const grossProfit = shares * priceMovePerShare;
          const netProfit = grossProfit - totalFees;
          const netLoss = maxRisk + totalFees; // worst case including fees
          const rr = netLoss > 0 ? netProfit / netLoss : NaN;
          const returnPct =
            shares * entry > 0 ? (netProfit / (shares * entry)) * 100 : NaN;

          rows.push({
            label: "Target " + (idx + 1),
            price: t,
            netProfit,
            returnPct,
            rr,
          });
        });

        return rows;
      }

      function updateUI() {
        const accountSize = parseFloat(
          document.getElementById("accountSize").value
        );
        const riskPercent = parseFloat(
          document.getElementById("riskPercent").value
        );
        const positionType = document.getElementById("positionType").value;
        const entryPrice = parseFloat(
          document.getElementById("entryPrice").value
        );
        const stopPrice = parseFloat(
          document.getElementById("stopPrice").value
        );
        const targetPrice = parseFloat(
          document.getElementById("targetPrice").value
        );
        const targetPrice2 = parseFloat(
          document.getElementById("targetPrice2").value
        );
        const targetPrice3 = parseFloat(
          document.getElementById("targetPrice3").value
        );
        const commissionPerTrade =
          parseFloat(document.getElementById("commissionPerTrade").value) || 0;
        const feePerShare =
          parseFloat(document.getElementById("feePerShare").value) || 0;

        const errorEl = document.getElementById("errorMsg");
        errorEl.classList.add("hidden");
        errorEl.textContent = "";

        if (
          !isFinite(entryPrice) ||
          !isFinite(stopPrice) ||
          entryPrice <= 0 ||
          stopPrice <= 0
        ) {
          errorEl.textContent =
            "Please enter valid entry and stop‑loss prices.";
          errorEl.classList.remove("hidden");
          return;
        }

        const isLong = positionType === "long";
        let riskPerShare = isLong
          ? entryPrice - stopPrice
          : stopPrice - entryPrice;

        if (riskPerShare <= 0) {
          errorEl.textContent =
            "For a " +
            (isLong ? "long" : "short") +
            " trade, the stop‑loss must be on the risk side of the entry (below for long, above for short).";
          errorEl.classList.remove("hidden");
          return;
        }

        let maxRisk = NaN;
        if (
          isFinite(accountSize) &&
          isFinite(riskPercent) &&
          accountSize > 0 &&
          riskPercent > 0
        ) {
          maxRisk = accountSize * (riskPercent / 100);
        } else {
          errorEl.textContent =
            "Enter a valid account size and risk percentage to calculate position size.";
          errorEl.classList.remove("hidden");
          return;
        }

        let shares = Math.floor(maxRisk / riskPerShare);
        if (!isFinite(shares) || shares <= 0) {
          errorEl.textContent =
            "Risk per share is too high for the selected risk percentage. Adjust your stop‑loss or risk settings.";
          errorEl.classList.remove("hidden");
          return;
        }

        const positionValue = shares * entryPrice;
        const portfolioRiskPct =
          accountSize > 0 ? (maxRisk / accountSize) * 100 : NaN;
        const totalFees = 2 * commissionPerTrade + shares * feePerShare * 2; // entry + exit

        // Update summary
        document.getElementById("maxRisk").textContent =
          formatCurrency(maxRisk);
        document.getElementById("riskPerShare").textContent =
          formatCurrency(riskPerShare);
        document.getElementById("shares").textContent = formatNumber(shares);
        document.getElementById("positionValue").textContent =
          formatCurrency(positionValue);
        document.getElementById("portfolioRisk").textContent =
          formatPercent(portfolioRiskPct);
        document.getElementById("totalFees").textContent =
          formatCurrency(totalFees);

        const badge = document.getElementById("positionBadge");
        if (isLong) {
          badge.textContent = "Long";
          badge.className = "badge bg-green-100 text-green-700";
        } else {
          badge.textContent = "Short";
          badge.className = "badge bg-red-100 text-red-700";
        }

        // Update targets table
        const tbody = document.getElementById("targetsTableBody");
        tbody.innerHTML = "";

        const targets = computeTargets(
          positionType,
          entryPrice,
          shares,
          maxRisk,
          riskPerShare,
          totalFees,
          [targetPrice, targetPrice2, targetPrice3]
        );

        if (!targets.length) {
          const row = document.createElement("tr");
          row.className = "border-b";
          row.innerHTML =
            '<td class="py-1 pr-4 text-gray-400" colspan="4">Enter at least one target price and click “Calculate trade”.</td>';
          tbody.appendChild(row);
          return;
        }

        targets.forEach((t) => {
          const tr = document.createElement("tr");
          tr.className = "border-b";
          const rrDisplay = isFinite(t.rr) ? t.rr.toFixed(2) + " : 1" : "–";
          const returnDisplay = formatPercent(t.returnPct);
          const priceDisplay = "$" + t.price.toFixed(2);

          tr.innerHTML = `
        <td class="py-1 pr-4">
          <div class="flex flex-col">
            <span class="font-medium text-gray-800">${t.label}</span>
            <span class="text-gray-500">${priceDisplay}</span>
          </div>
        </td>
        <td class="py-1 pr-4">${formatCurrency(t.netProfit)}</td>
        <td class="py-1 pr-4">${returnDisplay}</td>
        <td class="py-1 pr-4">${rrDisplay}</td>
      `;
          tbody.appendChild(tr);
        });
      }

      document
        .getElementById("calculateBtn")
        .addEventListener("click", function (e) {
          e.preventDefault();
          updateUI();
        });

      document
        .getElementById("resetBtn")
        .addEventListener("click", function () {
          [
            "accountSize",
            "riskPercent",
            "entryPrice",
            "stopPrice",
            "targetPrice",
            "targetPrice2",
            "targetPrice3",
            "commissionPerTrade",
            "feePerShare",
          ].forEach((id) => {
            document.getElementById(id).value = "";
          });
          document.getElementById("errorMsg").classList.add("hidden");
          document.getElementById("errorMsg").textContent = "";
          document.getElementById("maxRisk").textContent = "–";
          document.getElementById("riskPerShare").textContent = "–";
          document.getElementById("shares").textContent = "–";
          document.getElementById("positionValue").textContent = "–";
          document.getElementById("portfolioRisk").textContent = "–";
          document.getElementById("totalFees").textContent = "–";
          document.getElementById("positionBadge").textContent = "Long";
          document.getElementById("positionBadge").className =
            "badge bg-green-100 text-green-700";
          document.getElementById("targetsTableBody").innerHTML =
            '<tr class="border-b"><td class="py-1 pr-4 text-gray-400" colspan="4">Enter at least one target price and click “Calculate trade”.</td></tr>';
        });

      // Optional: basic auto-calc when pressing Enter in any input
      Array.from(
        document.querySelectorAll(
          "#accountSize,#riskPercent,#entryPrice,#stopPrice,#targetPrice,#targetPrice2,#targetPrice3,#commissionPerTrade,#feePerShare"
        )
      ).forEach((el) => {
        el.addEventListener("keydown", function (e) {
          if (e.key === "Enter") {
            e.preventDefault();
            updateUI();
          }
        });
      });
    </script>

