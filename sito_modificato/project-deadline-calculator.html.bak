    <script>
      // Utility functions
      function parseDate(value) {
        if (!value) return null;
        const parts = value.split("-").map(Number);
        if (parts.length !== 3) return null;
        const d = new Date(parts[0], parts[1] - 1, parts[2]);
        return isNaN(d.getTime()) ? null : d;
      }

      function formatDateISO(date) {
        const y = date.getFullYear();
        const m = String(date.getMonth() + 1).padStart(2, "0");
        const d = String(date.getDate()).padStart(2, "0");
        return y + "-" + m + "-" + d;
      }

      function formatDateHuman(date) {
        return date.toLocaleDateString(undefined, {
          year: "numeric",
          month: "short",
          day: "numeric",
        });
      }

      function weekdayName(date) {
        return date.toLocaleDateString(undefined, { weekday: "long" });
      }

      function getSelectedWorkdays() {
        const boxes = document.querySelectorAll(".weekday-checkbox");
        const set = new Set();
        boxes.forEach((b) => {
          if (b.checked) set.add(Number(b.dataset.day));
        });
        return set;
      }

      function parseHolidaySet() {
        const txt = document.getElementById("holidays").value.trim();
        const set = new Set();
        if (!txt) return set;
        txt.split(/\r?\n/).forEach((line) => {
          const v = line.trim();
          if (!v) return;
          const d = parseDate(v);
          if (d) set.add(formatDateISO(d));
        });
        return set;
      }

      function isWorkday(date, workdaysSet, holidaySet) {
        const day = date.getDay(); // 0-6
        if (!workdaysSet.has(day)) return false;
        if (holidaySet.has(formatDateISO(date))) return false;
        return true;
      }

      function diffInDays(start, end) {
        const msPerDay = 24 * 60 * 60 * 1000;
        const utc1 = Date.UTC(
          start.getFullYear(),
          start.getMonth(),
          start.getDate()
        );
        const utc2 = Date.UTC(end.getFullYear(), end.getMonth(), end.getDate());
        return Math.round((utc2 - utc1) / msPerDay);
      }

      document.addEventListener("DOMContentLoaded", function () {
        const modeButtons = {
          forward: document.getElementById("mode-forward"),
          backward: document.getElementById("mode-backward"),
          between: document.getElementById("mode-between"),
        };
        let mode = "forward";

        const startInput = document.getElementById("start-date");
        const endInput = document.getElementById("end-date");
        const endWrapper = document.getElementById("end-date-wrapper");
        const durationRow = document.getElementById("duration-row");
        const durationInput = document.getElementById("duration");
        const includeStart = document.getElementById("include-start");
        const includeEnd = document.getElementById("include-end");
        const calcBtn = document.getElementById("calculate-btn");
        const resetBtn = document.getElementById("reset-btn");
        const errorEl = document.getElementById("calc-error");

        const resultDeadline = document.getElementById("result-deadline");
        const resultDeadlineWeekday = document.getElementById(
          "result-deadline-weekday"
        );
        const resultTotalDays = document.getElementById("result-total-days");
        const resultWorkdays = document.getElementById("result-workdays");
        const resultWorkdaysNote = document.getElementById(
          "result-workdays-note"
        );
        const timelineCard = document.getElementById("timeline-card");
        const timelineList = document.getElementById("timeline-list");
        const timelineMode = document.getElementById("timeline-mode");

        // Initialize default dates
        const today = new Date();
        startInput.value = formatDateISO(today);
        const defaultEnd = new Date(today);
        defaultEnd.setDate(defaultEnd.getDate() + 30);
        endInput.value = formatDateISO(defaultEnd);

        function setMode(newMode) {
          mode = newMode;
          Object.keys(modeButtons).forEach((key) => {
            const btn = modeButtons[key];
            if (key === newMode) {
              btn.classList.remove("bg-gray-100", "text-gray-700");
              btn.classList.add("bg-blue-600", "text-white");
            } else {
              btn.classList.remove("bg-blue-600", "text-white");
              btn.classList.add("bg-gray-100", "text-gray-700");
            }
          });

          if (newMode === "forward") {
            endWrapper.classList.add("hidden");
            durationRow.classList.remove("hidden");
          } else if (newMode === "backward") {
            endWrapper.classList.remove("hidden");
            durationRow.classList.remove("hidden");
          } else {
            // between
            endWrapper.classList.remove("hidden");
            durationRow.classList.add("hidden");
          }

          // Clear results on mode change
          errorEl.classList.add("hidden");
          resultDeadline.textContent = "—";
          resultDeadlineWeekday.textContent = "—";
          resultTotalDays.textContent = "—";
          resultWorkdays.textContent = "—";
          timelineCard.classList.add("hidden");
        }

        modeButtons.forward.addEventListener("click", () => setMode("forward"));
        modeButtons.backward.addEventListener("click", () =>
          setMode("backward")
        );
        modeButtons.between.addEventListener("click", () => setMode("between"));

        function showError(msg) {
          errorEl.textContent = msg;
          errorEl.classList.remove("hidden");
        }

        function clearError() {
          errorEl.classList.add("hidden");
          errorEl.textContent = "";
        }

        function calculate() {
          clearError();
          const startDate = parseDate(startInput.value);
          if (!startDate) {
            showError("Please enter a valid start date.");
            return;
          }

          const workdaysSet = getSelectedWorkdays();
          const holidaySet = parseHolidaySet();
          const durationType =
            document.querySelector('input[name="duration-type"]:checked')
              ?.value || "calendar";

          if (mode === "forward") {
            const n = Number(durationInput.value);
            if (!Number.isFinite(n) || n < 0) {
              showError("Please enter a non-negative duration.");
              return;
            }
            if (n === 0) {
              resultDeadline.textContent = formatDateHuman(startDate);
              resultDeadlineWeekday.textContent = weekdayName(startDate);
              resultTotalDays.textContent = "0";
              resultWorkdays.textContent = "0";
              timelineCard.classList.add("hidden");
              return;
            }

            let current = new Date(startDate);
            let count = 0;
            const maxIterations = 3660; // ~10 years safety
            let iterations = 0;

            if (durationType === "calendar") {
              // Simple: add n-1 days if including start, else n days
              const daysToAdd = includeStart.checked ? n - 1 : n;
              current.setDate(current.getDate() + daysToAdd);
            } else {
              // workdays
              while (count < n && iterations < maxIterations) {
                if (includeStart.checked || iterations > 0) {
                  if (isWorkday(current, workdaysSet, holidaySet)) {
                    count++;
                    if (count === n) break;
                  }
                }
                current.setDate(current.getDate() + 1);
                iterations++;
              }
              if (iterations >= maxIterations) {
                showError(
                  "Duration is too large to calculate safely. Try a smaller number of workdays."
                );
                return;
              }
            }

            const deadline = current;
            const totalDays = diffInDays(startDate, deadline);
            let workdayCount = 0;
            let cursor = new Date(startDate);
            while (cursor <= deadline) {
              if (isWorkday(cursor, workdaysSet, holidaySet)) workdayCount++;
              cursor.setDate(cursor.getDate() + 1);
            }

            resultDeadline.textContent = formatDateHuman(deadline);
            resultDeadlineWeekday.textContent = weekdayName(deadline);
            resultTotalDays.textContent = String(totalDays);
            resultWorkdays.textContent = String(workdayCount);
            resultWorkdaysNote.textContent =
              "Working days between start and deadline (excluding weekends & holidays).";

            // Timeline
            timelineMode.textContent = "Forward schedule";
            timelineList.innerHTML = "";
            const li1 = document.createElement("li");
            li1.textContent =
              "Start: " +
              formatDateHuman(startDate) +
              " (" +
              weekdayName(startDate) +
              ")";
            const li2 = document.createElement("li");
            li2.textContent =
              "Deadline: " +
              formatDateHuman(deadline) +
              " (" +
              weekdayName(deadline) +
              ")";
            const li3 = document.createElement("li");
            li3.textContent =
              "Total: " +
              totalDays +
              " calendar days, " +
              workdayCount +
              " working days.";
            timelineList.appendChild(li1);
            timelineList.appendChild(li2);
            timelineList.appendChild(li3);
            timelineCard.classList.remove("hidden");
          } else if (mode === "backward") {
            const endDate = parseDate(endInput.value);
            if (!endDate) {
              showError("Please enter a valid end date / deadline.");
              return;
            }
            const n = Number(durationInput.value);
            if (!Number.isFinite(n) || n < 0) {
              showError("Please enter a non-negative duration.");
              return;
            }
            if (n === 0) {
              resultDeadline.textContent = formatDateHuman(endDate);
              resultDeadlineWeekday.textContent = weekdayName(endDate);
              resultTotalDays.textContent = "0";
              resultWorkdays.textContent = "0";
              timelineCard.classList.add("hidden");
              return;
            }

            let current = new Date(endDate);
            let count = 0;
            const maxIterations = 3660;
            let iterations = 0;

            if (durationType === "calendar") {
              // Backward calendar: subtract n-1 days if include-end, else n days
              const daysToSub = includeEnd.checked ? n - 1 : n;
              current.setDate(current.getDate() - daysToSub);
            } else {
              // workdays backward
              while (count < n && iterations < maxIterations) {
                if (includeEnd.checked || iterations > 0) {
                  if (isWorkday(current, workdaysSet, holidaySet)) {
                    count++;
                    if (count === n) break;
                  }
                }
                current.setDate(current.getDate() - 1);
                iterations++;
              }
              if (iterations >= maxIterations) {
                showError(
                  "Duration is too large to calculate safely. Try a smaller number of workdays."
                );
                return;
              }
            }

            const latestStart = current;
            const totalDays = diffInDays(latestStart, endDate);
            let workdayCount = 0;
            let cursor = new Date(latestStart);
            while (cursor <= endDate) {
              if (isWorkday(cursor, workdaysSet, holidaySet)) workdayCount++;
              cursor.setDate(cursor.getDate() + 1);
            }

            resultDeadline.textContent = formatDateHuman(latestStart);
            resultDeadlineWeekday.textContent = weekdayName(latestStart);
            resultTotalDays.textContent = String(totalDays);
            resultWorkdays.textContent = String(workdayCount);
            resultWorkdaysNote.textContent =
              "Working days between latest start and deadline (excluding weekends & holidays).";

            timelineMode.textContent = "Backward schedule";
            timelineList.innerHTML = "";
            const li1 = document.createElement("li");
            li1.textContent =
              "Latest start: " +
              formatDateHuman(latestStart) +
              " (" +
              weekdayName(latestStart) +
              ")";
            const li2 = document.createElement("li");
            li2.textContent =
              "Deadline: " +
              formatDateHuman(endDate) +
              " (" +
              weekdayName(endDate) +
              ")";
            const li3 = document.createElement("li");
            li3.textContent =
              "Total: " +
              totalDays +
              " calendar days, " +
              workdayCount +
              " working days.";
            timelineList.appendChild(li1);
            timelineList.appendChild(li2);
            timelineList.appendChild(li3);
            timelineCard.classList.remove("hidden");
          } else {
            // between
            const endDate = parseDate(endInput.value);
            if (!endDate) {
              showError("Please enter a valid end date.");
              return;
            }
            if (endDate < startDate) {
              showError("End date must be on or after the start date.");
              return;
            }
            let total = diffInDays(startDate, endDate);
            if (includeEnd.checked) total += 1;

            let workdayCount = 0;
            let cursor = new Date(startDate);
            while (cursor <= endDate) {
              if (isWorkday(cursor, workdaysSet, holidaySet)) workdayCount++;
              cursor.setDate(cursor.getDate() + 1);
            }

            resultDeadline.textContent = formatDateHuman(endDate);
            resultDeadlineWeekday.textContent = weekdayName(endDate);
            resultTotalDays.textContent = String(total);
            resultWorkdays.textContent = String(workdayCount);
            resultWorkdaysNote.textContent =
              "Working days in the selected range (excluding weekends & holidays).";

            timelineMode.textContent = "Between dates";
            timelineList.innerHTML = "";
            const li1 = document.createElement("li");
            li1.textContent =
              "Start: " +
              formatDateHuman(startDate) +
              " (" +
              weekdayName(startDate) +
              ")";
            const li2 = document.createElement("li");
            li2.textContent =
              "End: " +
              formatDateHuman(endDate) +
              " (" +
              weekdayName(endDate) +
              ")";
            const li3 = document.createElement("li");
            li3.textContent =
              "Total: " +
              total +
              " calendar days, " +
              workdayCount +
              " working days.";
            timelineList.appendChild(li1);
            timelineList.appendChild(li2);
            timelineList.appendChild(li3);
            timelineCard.classList.remove("hidden");
          }
        }

        calcBtn.addEventListener("click", calculate);

        resetBtn.addEventListener("click", function () {
          startInput.value = formatDateISO(today);
          const newEnd = new Date(today);
          newEnd.setDate(newEnd.getDate() + 30);
          endInput.value = formatDateISO(newEnd);
          durationInput.value = 10;
          document.querySelector(
            'input[name="duration-type"][value="calendar"]'
          ).checked = true;
          includeStart.checked = true;
          includeEnd.checked = true;
          document.getElementById("holidays").value = "";
          document.querySelectorAll(".weekday-checkbox").forEach((cb) => {
            const d = Number(cb.dataset.day);
            cb.checked = d >= 1 && d <= 5; // Mon-Fri
          });
          setMode("forward");
        });
      });

