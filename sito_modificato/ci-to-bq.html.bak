  <script>
    (function() {
      const CI_TO_BQ = 3.7e10;

      const ciValue = document.getElementById('ciValue');
      const ciUnit = document.getElementById('ciUnit');
      const bqValue = document.getElementById('bqValue');
      const bqUnit = document.getElementById('bqUnit');
      const ciTableBody = document.getElementById('ciTableBody');

      let updating = false;

      function round(val, decimals) {
        if (!isFinite(val)) return '';
        const f = Math.pow(10, decimals);
        return Math.round(val * f) / f;
      }

      function ciToBaseCi(val, unit) {
        if (unit === 'Ci') return val;
        if (unit === 'mCi') return val / 1e3;
        if (unit === 'uCi') return val / 1e6;
        if (unit === 'nCi') return val / 1e9;
        return val;
      }

      function bqToBaseBq(val, unit) {
        if (unit === 'Bq') return val;
        if (unit === 'kBq') return val * 1e3;
        if (unit === 'MBq') return val * 1e6;
        if (unit === 'GBq') return val * 1e9;
        return val;
      }

      function baseBqToDisplay(val, unit) {
        if (unit === 'Bq') return val;
        if (unit === 'kBq') return val / 1e3;
        if (unit === 'MBq') return val / 1e6;
        if (unit === 'GBq') return val / 1e9;
        return val;
      }

      function baseCiToDisplay(val, unit) {
        if (unit === 'Ci') return val;
        if (unit === 'mCi') return val * 1e3;
        if (unit === 'uCi') return val * 1e6;
        if (unit === 'nCi') return val * 1e9;
        return val;
      }

      function updateFromCi() {
        if (updating) return;
        updating = true;
        const val = parseFloat(ciValue.value);
        if (isNaN(val)) {
          bqValue.value = '';
          updating = false;
          return;
        }
        const baseCi = ciToBaseCi(val, ciUnit.value);
        const baseBq = baseCi * CI_TO_BQ;
        const out = baseBqToDisplay(baseBq, bqUnit.value);
        // use scientific notation for huge values
        bqValue.value = (out > 1e6) ? out.toExponential(4) : round(out, 4);
        updating = false;
      }

      function updateFromBq() {
        if (updating) return;
        updating = true;
        let val = parseFloat(bqValue.value);
        if (isNaN(val)) {
          ciValue.value = '';
          updating = false;
          return;
        }
        const baseBq = bqToBaseBq(val, bqUnit.value);
        const baseCi = baseBq / CI_TO_BQ;
        const out = baseCiToDisplay(baseCi, ciUnit.value);
        ciValue.value = out < 1e-4 ? out.toExponential(4) : round(out, 8);
        updating = false;
      }

      if (ciValue) ciValue.addEventListener('input', updateFromCi);
      if (ciUnit) ciUnit.addEventListener('change', updateFromCi);
      if (bqValue) bqValue.addEventListener('input', updateFromBq);
      if (bqUnit) bqUnit.addEventListener('change', updateFromBq);

      // populate table with some common values
      const samples = [
        { ci: 1e-6 }, // 1 µCi
        { ci: 1e-3 }, // 1 mCi
        { ci: 0.01 },
        { ci: 0.1 },
        { ci: 1 },
        { ci: 5 },
        { ci: 10 }
      ];

      samples.forEach(item => {
        const baseBq = item.ci * CI_TO_BQ;
        const tr = document.createElement('tr');
        tr.innerHTML = `
          <td class="text-right">${item.ci}</td>
          <td class="text-right">${baseBq.toExponential(3)}</td>
          <td class="text-right">${round(baseBq / 1e6, 3)}</td>
        `;
        ciTableBody.appendChild(tr);
      });

    })();
  </script>

