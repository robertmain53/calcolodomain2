    <script>
      // Toggle logic
      document.getElementById("mobile-menu-toggle")?.addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          if (menu) menu.classList.toggle("hidden");
      });

      // Ziegler–Nichols Logic
      const kuInput = document.getElementById("ku");
      const puInput = document.getElementById("pu");
      const computeBtn = document.getElementById("compute-btn");
      const resetBtn = document.getElementById("reset-btn");
      const errorMsg = document.getElementById("error-msg");
      const resultsBody = document.getElementById("results-body");
      const tabs = document.querySelectorAll(".zn-tab");

      const rules = {
        classic: {
          name: "Classic",
          rows: [
            { type: "P", kpFactor: 0.5, tiFactor: null, tdFactor: null },
            { type: "PI", kpFactor: 0.45, tiFactor: 1 / 1.2, tdFactor: null },
            { type: "PID", kpFactor: 0.6, tiFactor: 0.5, tdFactor: 0.125 },
          ],
        },
        modified: {
          name: "Modified",
          rows: [
            { type: "P", kpFactor: 0.33, tiFactor: null, tdFactor: null },
            { type: "PI", kpFactor: 0.35, tiFactor: 1 / 1.2, tdFactor: null },
            { type: "PID", kpFactor: 0.5, tiFactor: 0.5, tdFactor: 0.125 }, // Note: Td/Ti factors are usually kept same, Kp drops
          ],
        },
      };

      let currentRule = "classic";
      let lastParams = null; 

      function formatNumber(val) {
        if (!isFinite(val)) return "";
        const abs = Math.abs(val);
        if (abs === 0) return "0";
        if (abs >= 0.001 && abs < 1e5) {
          return Number(val.toFixed(4)).toString();
        }
        return val.toExponential(3);
      }

      function computeGains(ku, pu, ruleKey) {
        const rule = rules[ruleKey];
        const rowsHtml = rule.rows
          .map((row) => {
            const kp = row.kpFactor * ku;
            const ti = row.tiFactor != null ? pu * row.tiFactor : null;
            const td = row.tdFactor != null ? pu * row.tdFactor : null;
            
            // Ki = Kp / Ti
            const ki = ti != null && ti !== 0 ? kp / ti : null;
            // Kd = Kp * Td
            const kd = td != null ? kp * td : null;

            return `
              <tr class="border-t border-gray-200">
                <td class="px-3 py-2 text-gray-800 font-medium">
                  ${row.type}
                  ${ruleKey === "classic" 
                    ? '<span class="badge bg-red-100 text-red-700 ml-1">Fast</span>' 
                    : '<span class="badge bg-green-100 text-green-700 ml-1">Robust</span>'}
                </td>
                <td class="px-3 py-2 text-right font-mono">${formatNumber(kp)}</td>
                <td class="px-3 py-2 text-right font-mono">${ti != null ? formatNumber(ti) : "—"}</td>
                <td class="px-3 py-2 text-right font-mono">${td != null ? formatNumber(td) : "—"}</td>
                <td class="px-3 py-2 text-right font-mono">${ki != null ? formatNumber(ki) : "—"}</td>
                <td class="px-3 py-2 text-right font-mono">${kd != null ? formatNumber(kd) : "—"}</td>
              </tr>
            `;
          })
          .join("");
        resultsBody.innerHTML = rowsHtml;
      }

      function validateInputs() {
        const ku = parseFloat(kuInput.value);
        const pu = parseFloat(puInput.value);
        
        if (isNaN(ku) || ku <= 0) {
          errorMsg.textContent = "Please enter a positive Ultimate Gain.";
          resultsBody.innerHTML = "";
          return null;
        }
        if (isNaN(pu) || pu <= 0) {
          errorMsg.textContent = "Please enter a positive Period.";
          resultsBody.innerHTML = "";
          return null;
        }
        errorMsg.textContent = "";
        return { ku, pu };
      }

      computeBtn?.addEventListener("click", () => {
        const vals = validateInputs();
        if (!vals) return;
        lastParams = vals;
        computeGains(vals.ku, vals.pu, currentRule);
      });

      resetBtn?.addEventListener("click", () => {
        kuInput.value = "";
        puInput.value = "";
        errorMsg.textContent = "";
        resultsBody.innerHTML = '<tr><td colspan="6" class="px-3 py-4 text-center text-gray-500">Enter $K_u$ and $P_u$ to see results.</td></tr>';
        lastParams = null;
      });

      tabs.forEach((tab) => {
        tab.addEventListener("click", () => {
          const ruleKey = tab.getAttribute("data-rule");
          if (!ruleKey || !rules[ruleKey]) return;
          currentRule = ruleKey;

          tabs.forEach((t) => {
            if (t === tab) {
              t.classList.remove("bg-gray-100", "text-gray-700");
              t.classList.add("bg-blue-600", "text-white", "shadow-sm");
            } else {
              t.classList.remove("bg-blue-600", "text-white", "shadow-sm");
              t.classList.add("bg-gray-100", "text-gray-700");
            }
          });

          if (lastParams) {
            computeGains(lastParams.ku, lastParams.pu, currentRule);
          }
        });
      });
    </script>
    
