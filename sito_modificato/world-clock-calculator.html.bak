    <script>
      // World Clock Calculator Logic
      (function () {
        const baseTzSelect = document.getElementById("base-timezone");
        const baseDateInput = document.getElementById("base-date");
        const baseTimeInput = document.getElementById("base-time");
        const citiesTbody = document.getElementById("cities-tbody");
        const nowBtn = document.getElementById("now-btn");
        const timeShiftBtns = document.querySelectorAll(".time-shift-btn");
        const format12Btn = document.getElementById("format-12");
        const format24Btn = document.getElementById("format-24");
        const addCityBtn = document.getElementById("add-city-btn");
        const customCityInput = document.getElementById("custom-city");
        const tzDatalist = document.getElementById("timezone-datalist");
        const clearCitiesBtn = document.getElementById("clear-cities-btn");
        const summaryBox = document.getElementById("summary-box");
        const quickPills = document.querySelectorAll(".tz-pill");

        let use24Hour = true;
        let cities = []; // { id, tz, label }
        let cityIdCounter = 1;

        function getAllTimeZones() {
          if (Intl.supportedValuesOf) {
            try {
              return Intl.supportedValuesOf("timeZone");
            } catch (e) {}
          }
          // Fallback minimal list
          return [
            "UTC",
            "Europe/London",
            "Europe/Paris",
            "Europe/Berlin",
            "Europe/Madrid",
            "America/New_York",
            "America/Chicago",
            "America/Denver",
            "America/Los_Angeles",
            "America/Sao_Paulo",
            "Asia/Tokyo",
            "Asia/Shanghai",
            "Asia/Hong_Kong",
            "Asia/Singapore",
            "Asia/Dubai",
            "Asia/Kolkata",
            "Australia/Sydney",
          ];
        }

        const allTimeZones = getAllTimeZones();

        function populateBaseTzSelect() {
          baseTzSelect.innerHTML = "";
          const userTz =
            Intl.DateTimeFormat().resolvedOptions().timeZone || "UTC";

          // Group by region
          const regions = {};
          allTimeZones.forEach((tz) => {
            const parts = tz.split("/");
            const region = parts[0] || "Other";
            if (!regions[region]) regions[region] = [];
            regions[region].push(tz);
          });

          Object.keys(regions)
            .sort()
            .forEach((region) => {
              const optgroup = document.createElement("optgroup");
              optgroup.label = region;
              regions[region].sort().forEach((tz) => {
                const option = document.createElement("option");
                option.value = tz;
                option.textContent = tz.replace("_", " ");
                if (tz === userTz) option.selected = true;
                optgroup.appendChild(option);
              });
              baseTzSelect.appendChild(optgroup);
            });
        }

        function populateDatalist() {
          tzDatalist.innerHTML = "";
          allTimeZones.forEach((tz) => {
            const opt = document.createElement("option");
            opt.value = tz;
            tzDatalist.appendChild(opt);
          });
        }

        function setNow() {
          const now = new Date();
          const localISO = new Date(
            now.getTime() - now.getTimezoneOffset() * 60000
          ).toISOString();
          baseDateInput.value = localISO.slice(0, 10);
          baseTimeInput.value = localISO.slice(11, 16);
          updateTable();
        }

        function getBaseDateTimeUTC() {
          const dateStr = baseDateInput.value;
          const timeStr = baseTimeInput.value || "00:00";
          if (!dateStr) return null;
          const [year, month, day] = dateStr.split("-").map(Number);
          const [hour, minute] = timeStr.split(":").map(Number);
          const baseTz = baseTzSelect.value || "UTC";

          // Interpret as time in baseTz, then convert to UTC by formatting with timeZone and parsing
          const local = new Date(
            Date.UTC(year, month - 1, day, hour, minute, 0)
          );
          // We approximate by using the offset at that instant
          const fmt = new Intl.DateTimeFormat("en-US", {
            timeZone: baseTz,
            hour12: false,
            year: "numeric",
            month: "2-digit",
            day: "2-digit",
            hour: "2-digit",
            minute: "2-digit",
          });
          const parts = fmt.formatToParts(local);
          const y = Number(parts.find((p) => p.type === "year").value);
          const m = Number(parts.find((p) => p.type === "month").value);
          const d = Number(parts.find((p) => p.type === "day").value);
          const h = Number(parts.find((p) => p.type === "hour").value);
          const min = Number(parts.find((p) => p.type === "minute").value);
          // This is the same instant, but expressed in baseTz. To get UTC, we can use Date.UTC with those parts and then subtract the actual offset.
          const zoned = new Date(Date.UTC(y, m - 1, d, h, min, 0));
          return zoned;
        }

        function shiftBaseMinutes(deltaMinutes) {
          const baseUTC = getBaseDateTimeUTC();
          if (!baseUTC) return;
          const shifted = new Date(baseUTC.getTime() + deltaMinutes * 60000);
          const baseTz = baseTzSelect.value || "UTC";
          const fmt = new Intl.DateTimeFormat("en-CA", {
            timeZone: baseTz,
            hour12: false,
            year: "numeric",
            month: "2-digit",
            day: "2-digit",
            hour: "2-digit",
            minute: "2-digit",
          });
          const parts = fmt.formatToParts(shifted);
          const y = parts.find((p) => p.type === "year").value;
          const m = parts.find((p) => p.type === "month").value;
          const d = parts.find((p) => p.type === "day").value;
          const h = parts.find((p) => p.type === "hour").value;
          const min = parts.find((p) => p.type === "minute").value;
          baseDateInput.value = `${y}-${m}-${d}`;
          baseTimeInput.value = `${h}:${min}`;
          updateTable();
        }

        function formatTime(date, tz) {
          const opts = {
            timeZone: tz,
            hour: "2-digit",
            minute: "2-digit",
            hour12: !use24Hour,
          };
          return new Intl.DateTimeFormat("en-US", opts).format(date);
        }

        function getLocalDateParts(date, tz) {
          const fmt = new Intl.DateTimeFormat("en-CA", {
            timeZone: tz,
            year: "numeric",
            month: "2-digit",
            day: "2-digit",
          });
          const [y, m, d] = fmt.format(date).split("-").map(Number);
          return { y, m, d };
        }

        function getUtcOffsetMinutes(date, tz) {
          const fmt = new Intl.DateTimeFormat("en-US", {
            timeZone: tz,
            hour12: false,
            year: "numeric",
            month: "2-digit",
            day: "2-digit",
            hour: "2-digit",
            minute: "2-digit",
          });
          const parts = fmt.formatToParts(date);
          const y = Number(parts.find((p) => p.type === "year").value);
          const m = Number(parts.find((p) => p.type === "month").value);
          const d = Number(parts.find((p) => p.type === "day").value);
          const h = Number(parts.find((p) => p.type === "hour").value);
          const min = Number(parts.find((p) => p.type === "minute").value);
          const asUTC = Date.UTC(y, m - 1, d, h, min, 0);
          return (asUTC - date.getTime()) / 60000;
        }

        function formatOffset(minutes) {
          const sign = minutes >= 0 ? "+" : "-";
          const abs = Math.abs(minutes);
          const h = Math.floor(abs / 60);
          const m = abs % 60;
          return `UTC${sign}${String(h).padStart(2, "0")}:${String(m).padStart(
            2,
            "0"
          )}`;
        }

        function describeDayRelation(baseDate, localDate) {
          const baseOnly = new Date(
            Date.UTC(baseDate.y, baseDate.m - 1, baseDate.d)
          );
          const localOnly = new Date(
            Date.UTC(localDate.y, localDate.m - 1, localDate.d)
          );
          const diffDays = Math.round((localOnly - baseOnly) / 86400000);
          if (diffDays === 0) return "Today";
          if (diffDays === -1) return "Yesterday";
          if (diffDays === 1) return "Tomorrow";
          if (diffDays > 1) return `${diffDays} days ahead`;
          return `${Math.abs(diffDays)} days behind`;
        }

        function workingHourBadge(hour) {
          // simple heuristic: 9–17 working, 7–9 & 17–21 shoulder, else night
          if (hour >= 9 && hour < 17)
            return {
              label: "Working hours",
              color: "bg-green-100 text-green-800",
            };
          if ((hour >= 7 && hour < 9) || (hour >= 17 && hour < 21))
            return {
              label: "Early / late",
              color: "bg-yellow-100 text-yellow-800",
            };
          return { label: "Night time", color: "bg-gray-100 text-gray-700" };
        }

        function updateTable() {
          const baseUTC = getBaseDateTimeUTC();
          if (!baseUTC) return;
          const baseTz = baseTzSelect.value || "UTC";
          const baseDateParts = getLocalDateParts(baseUTC, baseTz);
          const baseOffset = getUtcOffsetMinutes(baseUTC, baseTz);

          citiesTbody.innerHTML = "";
          if (cities.length === 0) {
            const tr = document.createElement("tr");
            tr.innerHTML =
              '<td colspan="6" class="px-3 py-4 text-center text-xs text-gray-500">Add cities using the quick list or the input above to compare time zones.</td>';
            citiesTbody.appendChild(tr);
            summaryBox.classList.add("hidden");
            return;
          }

          let minOffset = Infinity,
            maxOffset = -Infinity;

          cities.forEach((city) => {
            const localDateParts = getLocalDateParts(baseUTC, city.tz);
            const localOffset = getUtcOffsetMinutes(baseUTC, city.tz);
            minOffset = Math.min(minOffset, localOffset);
            maxOffset = Math.max(maxOffset, localOffset);

            const localTimeStr = formatTime(baseUTC, city.tz);
            const dayRelation = describeDayRelation(
              baseDateParts,
              localDateParts
            );
            const diffMinutes = localOffset - baseOffset;
            const diffHours = diffMinutes / 60;
            const diffLabel =
              diffHours === 0
                ? "Same as base"
                : diffHours > 0
                ? `+${diffHours.toFixed(1)} h`
                : `${diffHours.toFixed(1)} h`;

            const localHourFmt = new Intl.DateTimeFormat("en-US", {
              timeZone: city.tz,
              hour: "2-digit",
              hour12: false,
            }).format(baseUTC);
            const hourNum = parseInt(localHourFmt, 10);
            const badge = workingHourBadge(hourNum);

            const tr = document.createElement("tr");
            tr.innerHTML = `
          <td class="px-3 py-2 align-top">
            <div class="font-medium text-gray-800">${city.label}</div>
            <div class="text-xs text-gray-500">${city.tz}</div>
          </td>
          <td class="px-3 py-2 align-top">
            <div class="flex items-center space-x-2">
              <span class="font-mono">${localTimeStr}</span>
              <span class="inline-flex items-center px-2 py-0.5 rounded-full text-[11px] ${
                badge.color
              }">${badge.label}</span>
            </div>
          </td>
          <td class="px-3 py-2 align-top text-xs text-gray-700">${dayRelation}</td>
          <td class="px-3 py-2 align-top text-xs text-gray-700">${formatOffset(
            localOffset
          )}</td>
          <td class="px-3 py-2 align-top text-xs text-gray-700">${diffLabel}</td>
          <td class="px-3 py-2 align-top text-right">
            <button type="button" data-id="${
              city.id
            }" class="remove-city-btn text-xs text-red-600 hover:underline">Remove</button>
          </td>
        `;
            citiesTbody.appendChild(tr);
          });

          const spanHours = (maxOffset - minOffset) / 60;
          summaryBox.textContent = `Current spread between earliest and latest time zone: approximately ${spanHours.toFixed(
            1
          )} hours.`;
          summaryBox.classList.remove("hidden");

          // Attach remove handlers
          document.querySelectorAll(".remove-city-btn").forEach((btn) => {
            btn.addEventListener("click", () => {
              const id = Number(btn.getAttribute("data-id"));
              cities = cities.filter((c) => c.id !== id);
              updateQuickPills();
              updateTable();
            });
          });
        }

        function addCity(tz, label) {
          if (!tz) return;
          if (!allTimeZones.includes(tz) && tz !== "UTC") {
            alert(
              "Unknown or unsupported time zone. Please choose one from the suggestions."
            );
            return;
          }
          if (cities.some((c) => c.tz === tz)) return;
          cities.push({ id: cityIdCounter++, tz, label: label || tz });
          updateQuickPills();
          updateTable();
        }

        function updateQuickPills() {
          quickPills.forEach((pill) => {
            const tz = pill.getAttribute("data-tz");
            const active = cities.some((c) => c.tz === tz);
            pill.classList.toggle("tz-pill-active", active);
            pill.classList.toggle("tz-pill-inactive", !active);
          });
        }

        // Event bindings
        nowBtn.addEventListener("click", setNow);
        timeShiftBtns.forEach((btn) => {
          btn.addEventListener("click", () => {
            const shift = Number(btn.getAttribute("data-shift"));
            shiftBaseMinutes(shift);
          });
        });
        baseTzSelect.addEventListener("change", updateTable);
        baseDateInput.addEventListener("change", updateTable);
        baseTimeInput.addEventListener("change", updateTable);

        format12Btn.addEventListener("click", () => {
          use24Hour = false;
          format12Btn.classList.add(
            "border-blue-500",
            "text-blue-600",
            "bg-blue-50"
          );
          format24Btn.classList.remove(
            "border-blue-500",
            "text-blue-600",
            "bg-blue-50"
          );
          format24Btn.classList.add("border-gray-300");
          updateTable();
        });
        format24Btn.addEventListener("click", () => {
          use24Hour = true;
          format24Btn.classList.add(
            "border-blue-500",
            "text-blue-600",
            "bg-blue-50"
          );
          format12Btn.classList.remove(
            "border-blue-500",
            "text-blue-600",
            "bg-blue-50"
          );
          format12Btn.classList.add("border-gray-300");
          updateTable();
        });

        addCityBtn.addEventListener("click", () => {
          const input = customCityInput.value.trim();
          if (!input) return;
          let tz = input;
          // If user typed a city name that matches a known tz suffix, try to map
          const match = allTimeZones.find((z) =>
            z
              .toLowerCase()
              .endsWith("/" + input.replace(/\s+/g, "_").toLowerCase())
          );
          if (match) tz = match;
          addCity(tz, input);
          customCityInput.value = "";
        });

        customCityInput.addEventListener("keydown", (e) => {
          if (e.key === "Enter") {
            e.preventDefault();
            addCityBtn.click();
          }
        });

        quickPills.forEach((pill) => {
          pill.addEventListener("click", () => {
            const tz = pill.getAttribute("data-tz");
            const label = pill.getAttribute("data-label");
            if (cities.some((c) => c.tz === tz)) {
              cities = cities.filter((c) => c.tz !== tz);
            } else {
              addCity(tz, label);
            }
            updateQuickPills();
            updateTable();
          });
        });

        clearCitiesBtn.addEventListener("click", () => {
          cities = [];
          updateQuickPills();
          updateTable();
        });

        // Initialize
        populateBaseTzSelect();
        populateDatalist();
        setNow();
        // Add user local zone and a couple of common cities by default
        const userTz =
          Intl.DateTimeFormat().resolvedOptions().timeZone || "UTC";
        addCity(userTz, "My time zone");
        if (userTz !== "Europe/London") addCity("Europe/London", "London");
        if (userTz !== "America/New_York")
          addCity("America/New_York", "New York");
      })();
    </script>

