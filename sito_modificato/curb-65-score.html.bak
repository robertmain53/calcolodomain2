    <script>
      // CURB-65 calculator logic
      (function () {
        const confusionButtons = document.querySelectorAll(".confusion-btn");
        let confusion = false;

        const bunMmolInput = document.getElementById("bun-mmol");
        const bunMgInput = document.getElementById("bun-mg");
        const bunFlag = document.getElementById("bun-flag");

        const rrInput = document.getElementById("rr-input");
        const rrFlag = document.getElementById("rr-flag");

        const sbpInput = document.getElementById("sbp-input");
        const dbpInput = document.getElementById("dbp-input");
        const bpFlag = document.getElementById("bp-flag");

        const ageInput = document.getElementById("age-input");
        const ageFlag = document.getElementById("age-flag");

        const curbScoreBadge = document.getElementById("curb65-score-badge");
        const curbRiskText = document.getElementById("curb65-risk-text");
        const curbMortalityText = document.getElementById(
          "curb65-mortality-text"
        );
        const curbRecommendation = document.getElementById(
          "curb65-recommendation"
        );

        const crbScoreBadge = document.getElementById("crb65-score-badge");
        const crbRiskText = document.getElementById("crb65-risk-text");
        const crbMortalityText = document.getElementById(
          "crb65-mortality-text"
        );
        const crbRecommendation = document.getElementById(
          "crb65-recommendation"
        );

        const resetBtn = document.getElementById("reset-btn");

        function parseNumber(el) {
          const v = parseFloat(el.value);
          return isNaN(v) ? null : v;
        }

        function syncBun(from, to, factor) {
          const v = parseNumber(from);
          if (v === null) {
            if (!to.matches(":focus")) to.value = "";
            return;
          }
          const converted = factor(v);
          if (!to.matches(":focus")) {
            to.value = converted.toFixed(1);
          }
        }

        confusionButtons.forEach((btn) => {
          btn.addEventListener("click", () => {
            const val = btn.getAttribute("data-value");
            confusion = val === "yes";
            confusionButtons.forEach((b) => {
              if (b === btn) {
                b.classList.add(
                  "bg-blue-50",
                  "border-blue-500",
                  "text-blue-700"
                );
                b.classList.remove("bg-white", "text-gray-700");
              } else {
                b.classList.remove(
                  "bg-blue-50",
                  "border-blue-500",
                  "text-blue-700"
                );
                b.classList.add("bg-white", "text-gray-700");
              }
            });
            updateScores();
          });
        });

        bunMmolInput.addEventListener("input", () => {
          syncBun(bunMmolInput, bunMgInput, (mmol) => mmol * 2.8); // approx
          updateScores();
        });
        bunMgInput.addEventListener("input", () => {
          syncBun(bunMgInput, bunMmolInput, (mg) => mg / 2.8);
          updateScores();
        });

        [rrInput, sbpInput, dbpInput, ageInput].forEach((el) => {
          el.addEventListener("input", updateScores);
        });

        resetBtn.addEventListener("click", () => {
          confusion = false;
          confusionButtons.forEach((b) => {
            if (b.getAttribute("data-value") === "no") {
              b.classList.add("bg-blue-50", "border-blue-500", "text-blue-700");
              b.classList.remove("bg-white", "text-gray-700");
            } else {
              b.classList.remove(
                "bg-blue-50",
                "border-blue-500",
                "text-blue-700"
              );
              b.classList.add("bg-white", "text-gray-700");
            }
          });
          [
            bunMmolInput,
            bunMgInput,
            rrInput,
            sbpInput,
            dbpInput,
            ageInput,
          ].forEach((el) => (el.value = ""));
          bunFlag.textContent = "";
          rrFlag.textContent = "";
          bpFlag.textContent = "";
          ageFlag.textContent = "";
          curbScoreBadge.textContent = "0 / 5";
          crbScoreBadge.textContent = "0 / 4";
          curbRiskText.textContent = "Insufficient data.";
          curbMortalityText.textContent = "";
          curbRecommendation.textContent = "";
          crbRiskText.textContent = "Insufficient data.";
          crbMortalityText.textContent = "";
          crbRecommendation.textContent = "";
        });

        function interpretCURB(score) {
          if (score === null) {
            return {
              risk: "Insufficient data.",
              mortality: "",
              recommendation: "",
            };
          }
          if (score <= 1) {
            return {
              risk: "Low risk (CURB‑65 " + score + ")",
              mortality: "Estimated 30‑day mortality usually < 3%.",
              recommendation:
                "Outpatient treatment is often appropriate if there are no other concerning clinical or social factors.",
            };
          } else if (score === 2) {
            return {
              risk: "Moderate risk (CURB‑65 2)",
              mortality: "Estimated 30‑day mortality around 9–10%.",
              recommendation:
                "Hospital admission or very close observation is usually recommended, depending on overall clinical status.",
            };
          } else {
            return {
              risk: "High risk (CURB‑65 " + score + ")",
              mortality: "Estimated 30‑day mortality often > 15–30%.",
              recommendation:
                "Hospital admission is recommended with consideration of ICU or high‑dependency care where appropriate.",
            };
          }
        }

        function interpretCRB(score) {
          if (score === null) {
            return {
              risk: "Insufficient data.",
              mortality: "",
              recommendation: "",
            };
          }
          if (score <= 1) {
            return {
              risk: "Low risk (CRB‑65 " + score + ")",
              mortality:
                "Estimated 30‑day mortality generally low in validation studies.",
              recommendation:
                "Outpatient treatment may be appropriate if there are no other concerning features and good follow‑up is available.",
            };
          } else if (score === 2) {
            return {
              risk: "Moderate risk (CRB‑65 2)",
              mortality: "Intermediate 30‑day mortality risk.",
              recommendation:
                "Hospital assessment and likely admission are usually recommended.",
            };
          } else {
            return {
              risk: "High risk (CRB‑65 " + score + ")",
              mortality: "High 30‑day mortality risk.",
              recommendation:
                "Hospital admission is recommended; consider higher level of care depending on overall status.",
            };
          }
        }

        function updateScores() {
          // Urea/BUN
          const bunMmol = parseNumber(bunMmolInput);
          const bunMg = parseNumber(bunMgInput);
          let ureaPoint = 0;
          let hasUrea = false;

          if (bunMmol !== null) {
            hasUrea = true;
            if (bunMmol > 7) ureaPoint = 1;
          } else if (bunMg !== null) {
            hasUrea = true;
            if (bunMg > 20) ureaPoint = 1;
          }

          if (hasUrea) {
            bunFlag.textContent =
              ureaPoint === 1
                ? "Urea above threshold: +1 point."
                : "Urea not above threshold: 0 points.";
            bunFlag.className =
              "mt-2 text-xs font-semibold " +
              (ureaPoint ? "text-red-700" : "text-green-700");
          } else {
            bunFlag.textContent = "";
          }

          // Respiratory rate
          const rr = parseNumber(rrInput);
          let rrPoint = 0;
          if (rr !== null) {
            if (rr >= 30) {
              rrPoint = 1;
              rrFlag.textContent = "Respiratory rate ≥ 30: +1 point.";
              rrFlag.className = "mt-2 text-xs font-semibold text-red-700";
            } else {
              rrFlag.textContent = "Respiratory rate below 30: 0 points.";
              rrFlag.className = "mt-2 text-xs font-semibold text-green-700";
            }
          } else {
            rrFlag.textContent = "";
          }

          // Blood pressure
          const sbp = parseNumber(sbpInput);
          const dbp = parseNumber(dbpInput);
          let bpPoint = 0;
          if (sbp !== null || dbp !== null) {
            const lowSBP = sbp !== null && sbp < 90;
            const lowDBP = dbp !== null && dbp <= 60;
            if (lowSBP || lowDBP) {
              bpPoint = 1;
              bpFlag.textContent = "SBP < 90 or DBP ≤ 60: +1 point.";
              bpFlag.className = "mt-2 text-xs font-semibold text-red-700";
            } else {
              bpFlag.textContent = "Blood pressure not in low range: 0 points.";
              bpFlag.className = "mt-2 text-xs font-semibold text-green-700";
            }
          } else {
            bpFlag.textContent = "";
          }

          // Age
          const age = parseNumber(ageInput);
          let agePoint = 0;
          if (age !== null) {
            if (age >= 65) {
              agePoint = 1;
              ageFlag.textContent = "Age ≥ 65: +1 point.";
              ageFlag.className = "mt-2 text-xs font-semibold text-red-700";
            } else {
              ageFlag.textContent = "Age < 65: 0 points.";
              ageFlag.className = "mt-2 text-xs font-semibold text-green-700";
            }
          } else {
            ageFlag.textContent = "";
          }

          // CRB-65 (no urea)
          const crbComponentsKnown = [
            confusion !== null,
            rr !== null,
            sbp !== null || dbp !== null,
            age !== null,
          ].some(Boolean);

          let crbScore = null;
          if (crbComponentsKnown) {
            crbScore = (confusion ? 1 : 0) + rrPoint + bpPoint + agePoint;
          }

          const crbInterp = interpretCRB(crbScore);
          crbScoreBadge.textContent =
            (crbScore !== null ? crbScore : 0) + " / 4";
          crbRiskText.textContent = crbInterp.risk;
          crbMortalityText.textContent = crbInterp.mortality;
          crbRecommendation.textContent = crbInterp.recommendation;

          // CURB-65 (requires at least some data; urea may be unknown but score still computed as 0 for U)
          let curbScore = null;
          const anyComponentKnown = crbComponentsKnown || hasUrea;
          if (anyComponentKnown) {
            curbScore =
              (confusion ? 1 : 0) + ureaPoint + rrPoint + bpPoint + agePoint;
          }

          const curbInterp = interpretCURB(curbScore);
          curbScoreBadge.textContent =
            (curbScore !== null ? curbScore : 0) + " / 5";
          curbRiskText.textContent = curbInterp.risk;
          curbMortalityText.textContent = curbInterp.mortality;
