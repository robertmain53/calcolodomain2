    <script>
      // Mobile menu toggle
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const expanded = this.getAttribute("aria-expanded") === "true";
          this.setAttribute("aria-expanded", String(!expanded));
          menu.classList.toggle("hidden");
        });

      function formatPercent(value) {
        if (!isFinite(value)) return "–";
        return (value * 100).toFixed(2) + "%";
      }

      function formatRatio(value, decimals = 2) {
        if (!isFinite(value)) return "–";
        return value.toFixed(decimals) + "×";
      }

      function clampBarWidth(value, benchmark) {
        if (!isFinite(value) || benchmark <= 0) return 0;
        const ratio = value / benchmark;
        return Math.max(5, Math.min(100, ratio * 100));
      }

      function calculateDupont() {
        const netIncome = parseFloat(
          document.getElementById("netIncome").value
        );
        const sales = parseFloat(document.getElementById("sales").value);
        const assets = parseFloat(document.getElementById("assets").value);
        const equity = parseFloat(document.getElementById("equity").value);
        const ebit = parseFloat(document.getElementById("ebit").value);
        const ebt = parseFloat(document.getElementById("ebt").value);

        const errorEl = document.getElementById("calcError");
        errorEl.classList.add("hidden");

        if (
          !(sales > 0) ||
          !(assets > 0) ||
          !(equity > 0) ||
          !(netIncome === 0 || netIncome)
        ) {
          errorEl.textContent =
            "Please enter positive values for net income, sales, assets, and equity.";
          errorEl.classList.remove("hidden");
          return;
        }

        const margin = netIncome / sales;
        const turnover = sales / assets;
        const equityMult = assets / equity;
        const roe = netIncome / equity;

        document.getElementById("marginDisplay").textContent =
          formatPercent(margin);
        document.getElementById("turnoverDisplay").textContent =
          formatRatio(turnover);
        document.getElementById("equityMultDisplay").textContent =
          formatRatio(equityMult);
        document.getElementById("roeDisplay").textContent = formatPercent(roe);

        // 5-step components
        const taxBurdenCard = document.getElementById("taxBurdenCard");
        const interestBurdenCard =
          document.getElementById("interestBurdenCard");
        const ebitMarginCard = document.getElementById("ebitMarginCard");

        let taxBurden, interestBurden, ebitMargin;

        if (ebt > 0 && ebit > 0 && sales > 0) {
          taxBurden = netIncome / ebt;
          interestBurden = ebt / ebit;
          ebitMargin = ebit / sales;

          document.getElementById("taxBurdenDisplay").textContent = formatRatio(
            taxBurden,
            2
          );
          document.getElementById("interestBurdenDisplay").textContent =
            formatRatio(interestBurden, 2);
          document.getElementById("ebitMarginDisplay").textContent =
            formatPercent(ebitMargin);

          taxBurdenCard.classList.remove("hidden");
          interestBurdenCard.classList.remove("hidden");
          ebitMarginCard.classList.remove("hidden");
        } else {
          taxBurdenCard.classList.add("hidden");
          interestBurdenCard.classList.add("hidden");
          ebitMarginCard.classList.add("hidden");
        }

        // Visual bars
        const marginBar = document.getElementById("marginBar");
        const turnoverBar = document.getElementById("turnoverBar");
        const leverageBar = document.getElementById("leverageBar");

        const marginWidth = clampBarWidth(margin, 0.1); // benchmark 10% margin
        const turnoverWidth = clampBarWidth(turnover, 1.0); // benchmark 1x turnover
        const leverageWidth = clampBarWidth(equityMult, 3.0); // benchmark 3x leverage

        marginBar.style.width = marginWidth + "%";
        turnoverBar.style.width = turnoverWidth + "%";
        leverageBar.style.width = leverageWidth + "%";

        document.getElementById("marginBarLabel").textContent =
          formatPercent(margin);
        document.getElementById("turnoverBarLabel").textContent =
          formatRatio(turnover);
        document.getElementById("leverageBarLabel").textContent =
          formatRatio(equityMult);
      }

      document
        .getElementById("calculateBtn")
        .addEventListener("click", calculateDupont);

      document
        .getElementById("resetBtn")
        .addEventListener("click", function () {
          ["netIncome", "sales", "assets", "equity", "ebit", "ebt"].forEach(
            (id) => {
              const el = document.getElementById(id);
              if (el) el.value = "";
            }
          );
          [
            "roeDisplay",
            "marginDisplay",
            "turnoverDisplay",
            "equityMultDisplay",
          ].forEach((id) => {
            document.getElementById(id).textContent = "–";
          });
          ["taxBurdenCard", "interestBurdenCard", "ebitMarginCard"].forEach(
            (id) => {
              document.getElementById(id).classList.add("hidden");
            }
          );
          ["marginBar", "turnoverBar", "leverageBar"].forEach((id) => {
            document.getElementById(id).style.width = "0%";
          });
          document.getElementById("marginBarLabel").textContent = "–";
          document.getElementById("turnoverBarLabel").textContent = "–";
          document.getElementById("leverageBarLabel").textContent = "–";
          document.getElementById("calcError").classList.add("hidden");
        });

      // Mode toggle
      const mode3 = document.getElementById("mode-3step");
      const mode5 = document.getElementById("mode-5step");
      const advInputs = document.getElementById("advanced-inputs");

      mode3.addEventListener("click", () => {
        mode3.classList.add("bg-blue-600", "text-white", "border-blue-600");
        mode3.classList.remove(
          "border-gray-300",
          "text-gray-700",
          "hover:bg-gray-50"
        );
        mode5.classList.remove("bg-blue-600", "text-white", "border-blue-600");
        mode5.classList.add(
          "border-gray-300",
          "text-gray-700",
          "hover:bg-gray-50"
        );
        advInputs.classList.add("hidden");
      });

      mode5.addEventListener("click", () => {
        mode5.classList.add("bg-blue-600", "text-white", "border-blue-600");
        mode5.classList.remove(
          "border-gray-300",
          "text-gray-700",
          "hover:bg-gray-50"
        );
        mode3.classList.remove("bg-blue-600", "text-white", "border-blue-600");
        mode3.classList.add(
          "border-gray-300",
          "text-gray-700",
          "hover:bg-gray-50"
        );
        advInputs.classList.remove("hidden");
      });

      // Simple FAQ toggle (in addition to global script_faq if any)
      document
        .querySelectorAll("#faq-container .faq-question")
        .forEach((btn) => {
          btn.addEventListener("click", () => {
            const answer = btn.parentElement.querySelector(".faq-answer");
            const icon = btn.querySelector("span:last-child");
            const isHidden = answer.classList.contains("hidden");
            document
              .querySelectorAll("#faq-container .faq-answer")
              .forEach((a) => a.classList.add("hidden"));
            document
              .querySelectorAll("#faq-container .faq-question span:last-child")
              .forEach((i) => (i.textContent = "+"));
            if (isHidden) {
              answer.classList.remove("hidden");
              icon.textContent = "−";
            }
          });
        });
    </script>

