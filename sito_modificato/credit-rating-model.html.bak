    <script>
      function formatNumber(value, decimals = 2) {
        if (!isFinite(value)) return "–";
        return value.toLocaleString(undefined, {
          minimumFractionDigits: decimals,
          maximumFractionDigits: decimals,
        });
      }

      function getRiskMapping(z) {
        if (!isFinite(z)) {
          return {
            zone: "No result",
            badgeColor: "bg-gray-200 text-gray-800",
            pdText: "–",
            pdComment: "Enter inputs and calculate to see PD band.",
          };
        }

        if (z > 3.5) {
          return {
            zone: "Very strong (Safe zone)",
            badgeColor: "bg-green-100 text-green-800",
            pdText: "< 0.5%",
            pdComment: "Very low indicative 1-year default probability.",
          };
        } else if (z > 2.99) {
          return {
            zone: "Safe zone",
            badgeColor: "bg-green-100 text-green-800",
            pdText: "0.5% – 1%",
            pdComment: "Low indicative 1-year default probability.",
          };
        } else if (z > 2.3) {
          return {
            zone: "Upper grey zone",
            badgeColor: "bg-yellow-100 text-yellow-800",
            pdText: "1% – 3%",
            pdComment: "Moderate indicative 1-year default probability.",
          };
        } else if (z >= 1.81) {
          return {
            zone: "Lower grey zone",
            badgeColor: "bg-yellow-100 text-yellow-800",
            pdText: "3% – 7%",
            pdComment: "Elevated indicative 1-year default probability.",
          };
        } else {
          return {
            zone: "Distress zone",
            badgeColor: "bg-red-100 text-red-800",
            pdText: "> 7%",
            pdComment: "High indicative 1-year default probability.",
          };
        }
      }

      document
        .getElementById("calculateBtn")
        .addEventListener("click", function () {
          const wc = parseFloat(
            document.getElementById("workingCapital").value
          );
          const re = parseFloat(
            document.getElementById("retainedEarnings").value
          );
          const ebit = parseFloat(document.getElementById("ebit").value);
          const ta = parseFloat(document.getElementById("totalAssets").value);
          const mve = parseFloat(
            document.getElementById("marketValueEquity").value
          );
          const tl = parseFloat(
            document.getElementById("totalLiabilities").value
          );
          const sales = parseFloat(document.getElementById("sales").value);

          const errorEl = document.getElementById("inputError");

          if (!ta || ta <= 0 || !tl || tl <= 0) {
            errorEl.classList.remove("hidden");
            return;
          } else {
            errorEl.classList.add("hidden");
          }

          const x1 = wc / ta;
          const x2 = re / ta;
          const x3 = ebit / ta;
          const x4 = mve / tl;
          const x5 = sales / ta;

          const w1 = 1.2 * x1;
          const w2 = 1.4 * x2;
          const w3 = 3.3 * x3;
          const w4 = 0.6 * x4;
          const w5 = 1.0 * x5;

          const z = w1 + w2 + w3 + w4 + w5;

          document.getElementById("x1Value").textContent = isFinite(x1)
            ? formatNumber(x1, 3)
            : "–";
          document.getElementById("x2Value").textContent = isFinite(x2)
            ? formatNumber(x2, 3)
            : "–";
          document.getElementById("x3Value").textContent = isFinite(x3)
            ? formatNumber(x3, 3)
            : "–";
          document.getElementById("x4Value").textContent = isFinite(x4)
            ? formatNumber(x4, 3)
            : "–";
          document.getElementById("x5Value").textContent = isFinite(x5)
            ? formatNumber(x5, 3)
            : "–";

          document.getElementById("x1Weighted").textContent = isFinite(w1)
            ? formatNumber(w1, 3)
            : "–";
          document.getElementById("x2Weighted").textContent = isFinite(w2)
            ? formatNumber(w2, 3)
            : "–";
          document.getElementById("x3Weighted").textContent = isFinite(w3)
            ? formatNumber(w3, 3)
            : "–";
          document.getElementById("x4Weighted").textContent = isFinite(w4)
            ? formatNumber(w4, 3)
            : "–";
          document.getElementById("x5Weighted").textContent = isFinite(w5)
            ? formatNumber(w5, 3)
            : "–";

          document.getElementById("zScoreOutput").textContent = formatNumber(
            z,
            2
          );
          document.getElementById("zScoreTable").textContent = formatNumber(
            z,
            2
          );
          document.getElementById("zScoreComment").textContent =
            "Higher is better; Z > 2.99 is typically considered safe.";

          const mapping = getRiskMapping(z);
          document.getElementById("riskZoneOutput").textContent = mapping.zone;

          const badgeEl = document.getElementById("riskZoneBadge");
          badgeEl.textContent = mapping.zone;
          badgeEl.className = "inline-block mt-2 badge " + mapping.badgeColor;

          document.getElementById("pdOutput").textContent = mapping.pdText;
          document.getElementById("pdComment").textContent = mapping.pdComment;
        });

      document
        .getElementById("resetBtn")
        .addEventListener("click", function () {
          [
            "workingCapital",
            "retainedEarnings",
            "ebit",
            "totalAssets",
            "marketValueEquity",
            "totalLiabilities",
            "sales",
          ].forEach((id) => {
            document.getElementById(id).value = "";
          });

          document.getElementById("zScoreOutput").textContent = "–";
          document.getElementById("zScoreTable").textContent = "–";
          document.getElementById("zScoreComment").textContent =
            "Calculated from five financial ratios.";

          document.getElementById("riskZoneOutput").textContent = "–";
          const badgeEl = document.getElementById("riskZoneBadge");
          badgeEl.textContent = "No result";
          badgeEl.className =
            "inline-block mt-2 badge bg-gray-200 text-gray-800";

          document.getElementById("pdOutput").textContent = "–";
          document.getElementById("pdComment").textContent =
            "Approximate probability of default band.";

          [
            "x1Value",
            "x2Value",
            "x3Value",
            "x4Value",
            "x5Value",
            "x1Weighted",
            "x2Weighted",
            "x3Weighted",
            "x4Weighted",
            "x5Weighted",
          ].forEach((id) => {
            document.getElementById(id).textContent = "–";
          });

          document.getElementById("inputError").classList.add("hidden");
        });

      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          this.setAttribute("aria-expanded", String(isHidden));
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
