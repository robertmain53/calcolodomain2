    <script>
      // Simple state
      const cardioSessions = [];
      const liftSessions = [];

      function getProfile() {
        const age = parseFloat(document.getElementById("age").value) || 30;
        const sex = document.getElementById("sex").value;
        const height =
          parseFloat(document.getElementById("height").value) || 175;
        const weight =
          parseFloat(document.getElementById("weight").value) || 70;
        const level = document.getElementById("fitness-level").value;
        return { age, sex, height, weight, level };
      }

      function intensityToMET(activity, intensity) {
        // Rough MET mapping based on compendia of physical activities
        const base = {
          run: { easy: 7, moderate: 9, hard: 11, "very-hard": 13 },
          cycle: { easy: 4, moderate: 6, hard: 8, "very-hard": 10 },
          walk: { easy: 2.5, moderate: 3.5, hard: 4.5, "very-hard": 5.5 },
          other: { easy: 4, moderate: 6, hard: 8, "very-hard": 10 },
        };
        const map = base[activity] || base.other;
        return map[intensity] || 6;
      }

      function metToCalories(met, minutes, weightKg) {
        // kcal/min ≈ MET × 3.5 × kg / 200
        const perMin = (met * 3.5 * weightKg) / 200;
        return perMin * minutes;
      }

      function renderCardioList() {
        const list = document.getElementById("cardio-list");
        const empty = document.getElementById("cardio-empty");
        list.innerHTML = "";
        if (cardioSessions.length === 0) {
          empty.classList.remove("hidden");
          empty.textContent = "No sessions added yet.";
          list.appendChild(empty);
          return;
        }
        cardioSessions.forEach((s, idx) => {
          const row = document.createElement("div");
          row.className =
            "flex justify-between items-center py-1 border-b border-gray-100 last:border-b-0";
          row.innerHTML = `
        <div>
          <span class="font-medium capitalize">${s.type}</span>
          <span class="text-gray-500 text-xs ml-1">(${s.intensity}, ${s.duration} min)</span>
        </div>
        <button type="button" data-idx="${idx}" class="text-xs text-red-500 hover:underline">Remove</button>
      `;
          list.appendChild(row);
        });
        list.querySelectorAll("button[data-idx]").forEach((btn) => {
          btn.addEventListener("click", () => {
            const i = parseInt(btn.getAttribute("data-idx"), 10);
            cardioSessions.splice(i, 1);
            renderCardioList();
          });
        });
      }

      function renderLiftList() {
        const list = document.getElementById("lift-list");
        const empty = document.getElementById("lift-empty");
        list.innerHTML = "";
        if (liftSessions.length === 0) {
          empty.classList.remove("hidden");
          empty.textContent = "No lifts added yet.";
          list.appendChild(empty);
          return;
        }
        liftSessions.forEach((s, idx) => {
          const row = document.createElement("div");
          row.className =
            "flex justify-between items-center py-1 border-b border-gray-100 last:border-b-0";
          row.innerHTML = `
        <div>
          <span class="font-medium">${s.name}</span>
          <span class="text-gray-500 text-xs ml-1">${s.sets}×${s.reps} @ ${s.load} kg, ${s.freq}×/week</span>
        </div>
        <button type="button" data-idx="${idx}" class="text-xs text-red-500 hover:underline">Remove</button>
      `;
          list.appendChild(row);
        });
        list.querySelectorAll("button[data-idx]").forEach((btn) => {
          btn.addEventListener("click", () => {
            const i = parseInt(btn.getAttribute("data-idx"), 10);
            liftSessions.splice(i, 1);
            renderLiftList();
          });
        });
      }

      document.getElementById("add-cardio").addEventListener("click", () => {
        const type = document.getElementById("cardio-type").value;
        const duration = Math.max(
          1,
          parseFloat(document.getElementById("cardio-duration").value) || 0
        );
        const intensity = document.getElementById("cardio-intensity").value;
        cardioSessions.push({ type, duration, intensity });
        renderCardioList();
      });

      document.getElementById("add-lift").addEventListener("click", () => {
        const name = (
          document.getElementById("lift-name").value || "Lift"
        ).trim();
        const sets = Math.max(
          1,
          parseInt(document.getElementById("lift-sets").value, 10) || 1
        );
        const reps = Math.max(
          1,
          parseInt(document.getElementById("lift-reps").value, 10) || 1
        );
        const load = Math.max(
          1,
          parseFloat(document.getElementById("lift-load").value) || 1
        );
        const freq = Math.max(
          1,
          parseInt(document.getElementById("lift-frequency").value, 10) || 1
        );
        const rpe = parseFloat(document.getElementById("lift-rpe").value) || 8;
        liftSessions.push({ name, sets, reps, load, freq, rpe });
        document.getElementById("lift-name").value = "";
        renderLiftList();
      });

      document
        .getElementById("update-summary")
        .addEventListener("click", () => {
          const profile = getProfile();
          let totalMetMin = 0;
          let totalKcal = 0;
          let cardioMinutes = 0;

          cardioSessions.forEach((s) => {
            const met = intensityToMET(s.type, s.intensity);
            const kcal = metToCalories(met, s.duration, profile.weight);
            totalMetMin += met * s.duration;
            totalKcal += kcal;
            cardioMinutes += s.duration;
          });

          // Approximate lifting as 3–6 MET depending on RPE
          let strengthMinutes = 0;
          liftSessions.forEach((s) => {
            const perSetMin = 2; // average time per set including rest
            const sessionMinutes = s.sets * perSetMin;
            const weeklyMinutes = sessionMinutes * s.freq;
            strengthMinutes += weeklyMinutes;
            const met = 3 + (s.rpe - 6) * 1; // RPE 6 -> 3 MET, RPE 9 -> 6 MET
            totalMetMin += met * weeklyMinutes;
            totalKcal += metToCalories(met, weeklyMinutes, profile.weight);
          });

          const summaryMet = document.getElementById("summary-met");
          const summaryMetNote = document.getElementById("summary-met-note");
          const summaryKcal = document.getElementById("summary-kcal");
          const summaryBalance = document.getElementById("summary-balance");
          const summaryBalanceNote = document.getElementById(
            "summary-balance-note"
          );

          summaryMet.textContent = Math.round(totalMetMin);
          const pct = totalMetMin === 0 ? 0 : (totalMetMin / 600) * 100;
          summaryMetNote.textContent = `${Math.round(
            pct
          )}% of basic guideline (600 MET·min/week).`;

          summaryKcal.textContent = `${Math.round(totalKcal)} kcal`;
          summaryBalance.textContent = `${Math.round(
            cardioMinutes
          )} / ${Math.round(strengthMinutes)}`;
          summaryBalanceNote.textContent = `Minutes of cardio vs estimated minutes of lifting.`;
        });

      // Initialize empty lists
      renderCardioList();
      renderLiftList();

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
