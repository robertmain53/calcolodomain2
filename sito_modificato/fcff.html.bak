    <script>
      (function () {
        const methodButtons = document.querySelectorAll(".method-btn");
        const methodGroups = document.querySelectorAll(".method-group");
        const methodHelp = document.getElementById("method-help");
        const calculateBtn = document.getElementById("calculateBtn");
        const resetBtn = document.getElementById("resetBtn");
        const errorMsg = document.getElementById("errorMsg");
        const resultsBox = document.getElementById("results");
        const valuationBox = document.getElementById("valuationResults");
        const fcffValueEl = document.getElementById("fcffValue");
        const evValueEl = document.getElementById("evValue");
        const equityValueEl = document.getElementById("equityValue");
        const perShareValueEl = document.getElementById("perShareValue");
        const formulaUsedEl = document.getElementById("formulaUsed");

        let currentMethod = "ebit";

        const methodDescriptions = {
          ebit: "FCFF = EBIT × (1 − Tax Rate) + Depreciation & Amortization − Capex − ΔWorking Capital",
          ni: "FCFF = Net Income + Depreciation & Amortization + Interest × (1 − Tax Rate) − Capex − ΔWorking Capital",
          cfo: "FCFF = CFO + Interest × (1 − Tax Rate) − Capex",
        };

        function formatCurrency(value) {
          if (!isFinite(value)) return "—";
          const abs = Math.abs(value);
          let formatted = value.toFixed(2);
          if (abs >= 1e9) {
            formatted = (value / 1e9).toFixed(2) + " B";
          } else if (abs >= 1e6) {
            formatted = (value / 1e6).toFixed(2) + " M";
          } else if (abs >= 1e3) {
            formatted = (value / 1e3).toFixed(2) + " K";
          }
          return formatted;
        }

        function formatNumber(value) {
          if (!isFinite(value)) return "—";
          return value.toFixed(2);
        }

        methodButtons.forEach((btn) => {
          btn.addEventListener("click", () => {
            const method = btn.getAttribute("data-method");
            if (method === currentMethod) return;
            currentMethod = method;

            methodButtons.forEach((b) => {
              if (b === btn) {
                b.classList.remove(
                  "border-gray-300",
                  "text-gray-700",
                  "bg-white"
                );
                b.classList.add(
                  "border-blue-500",
                  "bg-blue-50",
                  "text-blue-700",
                  "font-medium"
                );
              } else {
                b.classList.remove(
                  "border-blue-500",
                  "bg-blue-50",
                  "text-blue-700",
                  "font-medium"
                );
                b.classList.add("border-gray-300", "text-gray-700", "bg-white");
              }
            });

            methodGroups.forEach((group) => {
              if (group.getAttribute("data-group") === method) {
                group.classList.remove("hidden");
              } else {
                group.classList.add("hidden");
              }
            });

            methodHelp.textContent = methodDescriptions[method];
            resultsBox.classList.add("hidden");
            errorMsg.textContent = "";
          });
        });

        calculateBtn.addEventListener("click", () => {
          errorMsg.textContent = "";
          resultsBox.classList.add("hidden");
          valuationBox.classList.add("hidden");

          const taxRate =
            parseFloat(document.getElementById("taxRate").value) / 100;
          const da = parseFloat(document.getElementById("da").value) || 0;
          const capex = parseFloat(document.getElementById("capex").value) || 0;
          const deltaWc =
            parseFloat(document.getElementById("deltaWc").value) || 0;

          if (isNaN(taxRate) || taxRate < 0 || taxRate > 1) {
            errorMsg.textContent =
              "Please enter a valid tax rate between 0 and 100%.";
            return;
          }

          let fcff = NaN;
          let formulaText = "";

          if (currentMethod === "ebit") {
            const ebit = parseFloat(document.getElementById("ebit").value);
            if (isNaN(ebit)) {
              errorMsg.textContent = "Please enter EBIT.";
              return;
            }
            fcff = ebit * (1 - taxRate) + da - capex - deltaWc;
            formulaText = "FCFF = EBIT × (1 − T) + D&A − Capex − ΔWC";
          } else if (currentMethod === "ni") {
            const ni = parseFloat(document.getElementById("netIncome").value);
            const interest =
              parseFloat(document.getElementById("interestExpense").value) || 0;
            if (isNaN(ni)) {
              errorMsg.textContent = "Please enter Net Income.";
              return;
            }
            fcff = ni + da + interest * (1 - taxRate) - capex - deltaWc;
            formulaText =
              "FCFF = Net Income + D&A + Interest × (1 − T) − Capex − ΔWC";
          } else if (currentMethod === "cfo") {
            const cfo = parseFloat(document.getElementById("cfo").value);
            const interest =
              parseFloat(document.getElementById("interestExpenseCfo").value) ||
              0;
            if (isNaN(cfo)) {
              errorMsg.textContent = "Please enter CFO.";
              return;
            }
            fcff = cfo + interest * (1 - taxRate) - capex;
            formulaText = "FCFF = CFO + Interest × (1 − T) − Capex";
          }

          if (!isFinite(fcff)) {
            errorMsg.textContent =
              "Unable to compute FCFF. Please check your inputs.";
            return;
          }

          fcffValueEl.textContent = formatCurrency(fcff);
          formulaUsedEl.textContent = "Formula used: " + formulaText;
          resultsBox.classList.remove("hidden");

          // Optional valuation
          const waccInput = document.getElementById("wacc").value;
          const growthInput = document.getElementById("growth").value;
          const netDebtInput = document.getElementById("netDebt").value;
          const sharesInput = document.getElementById("shares").value;

          if (waccInput !== "" && growthInput !== "") {
            const wacc = parseFloat(waccInput) / 100;
            const g = parseFloat(growthInput) / 100;

            if (wacc > g && wacc > 0) {
              const fcff1 = fcff * (1 + g);
              const ev = fcff1 / (wacc - g);
              evValueEl.textContent = formatCurrency(ev);

              let equity = NaN;
              if (netDebtInput !== "") {
                const netDebt = parseFloat(netDebtInput) || 0;
                equity = ev - netDebt;
                equityValueEl.textContent = formatCurrency(equity);
              } else {
                equityValueEl.textContent = "—";
              }

              if (sharesInput !== "" && isFinite(equity)) {
                const shares = parseFloat(sharesInput);
                if (shares > 0) {
                  const perShare = equity / shares;
                  perShareValueEl.textContent = formatNumber(perShare);
                } else {
                  perShareValueEl.textContent = "—";
                }
              } else {
                perShareValueEl.textContent = "—";
              }

              valuationBox.classList.remove("hidden");
            }
          }
        });

        resetBtn.addEventListener("click", () => {
          errorMsg.textContent = "";
          resultsBox.classList.add("hidden");
          valuationBox.classList.add("hidden");
          formulaUsedEl.textContent = "";
        });
      })();
    </script>

