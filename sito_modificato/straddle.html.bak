    <script src="https://cdn.jsdelivr.net/npm/chart.js@4.4.0/dist/chart.umd.min.js"></script>
    <script>
      // UI state: show/hide strike inputs based on strategy
      const strategyTypeEl = document.getElementById("strategyType");
      const straddleStrikesEl = document.getElementById("straddleStrikes");
      const strangleStrikesEl = document.getElementById("strangleStrikes");

      function updateStrikeInputs() {
        const val = strategyTypeEl.value;
        const isStrangle = val.includes("strangle");
        if (isStrangle) {
          straddleStrikesEl.classList.add("hidden");
          strangleStrikesEl.classList.remove("hidden");
        } else {
          straddleStrikesEl.classList.remove("hidden");
          strangleStrikesEl.classList.add("hidden");
        }
      }
      strategyTypeEl.addEventListener("change", () => {
        updateStrikeInputs();
        calculateAndRender();
      });

      // Core calculation
      const underlyingPriceEl = document.getElementById("underlyingPrice");
      const strikeEl = document.getElementById("strike");
      const putStrikeEl = document.getElementById("putStrike");
      const callStrikeEl = document.getElementById("callStrike");
      const callPremiumEl = document.getElementById("callPremium");
      const putPremiumEl = document.getElementById("putPremium");
      const contractsEl = document.getElementById("contracts");
      const priceStepEl = document.getElementById("priceStep");
      const spotForPLEl = document.getElementById("spotForPL");

      const totalPremiumDisplay = document.getElementById(
        "totalPremiumDisplay"
      );
      const netCreditDebitLabel = document.getElementById(
        "netCreditDebitLabel"
      );
      const breakevenDisplay = document.getElementById("breakevenDisplay");
      const maxProfitLossDisplay = document.getElementById(
        "maxProfitLossDisplay"
      );
      const plAtSpotDisplay = document.getElementById("plAtSpotDisplay");

      const calculateBtn = document.getElementById("calculateBtn");
      const resetBtn = document.getElementById("resetBtn");

      const CONTRACT_SIZE = 100;

      function toNumber(el, fallback = 0) {
        const v = parseFloat(el.value);
        return isNaN(v) ? fallback : v;
      }

      function payoffAtPrice(strategy, S, params) {
        const { K, Kp, Kc, C, P, contracts } = params;
        const qty = contracts * CONTRACT_SIZE;

        let callPayoff = 0;
        let putPayoff = 0;

        if (strategy === "long-straddle" || strategy === "short-straddle") {
          callPayoff = Math.max(S - K, 0);
          putPayoff = Math.max(K - S, 0);
        } else {
          // strangle
          callPayoff = Math.max(S - Kc, 0);
          putPayoff = Math.max(Kp - S, 0);
        }

        const totalPremiumPerShare = C + P;
        const longPLPerShare = callPayoff + putPayoff - totalPremiumPerShare;

        if (strategy.startsWith("long")) {
          return longPLPerShare * qty;
        } else {
          // short = opposite of long
          return -longPLPerShare * qty;
        }
      }

      function computeBreakevens(strategy, params) {
        const { K, Kp, Kc, C, P } = params;
        const totalPrem = C + P;
        if (totalPrem <= 0) return [];

        if (strategy.includes("straddle")) {
          return [K - totalPrem, K + totalPrem];
        } else {
          return [Kp - totalPrem, Kc + totalPrem];
        }
      }

      function computeExtremes(strategy, params) {
        const { K, Kp, Kc, C, P, contracts } = params;
        const totalPrem = (C + P) * CONTRACT_SIZE * contracts;

        if (strategy.startsWith("long")) {
          const maxLoss = totalPrem;
          let maxProfitDesc;
          if (strategy.includes("straddle")) {
            maxProfitDesc = "Unlimited upside; large but finite downside";
          } else {
            maxProfitDesc = "Unlimited upside; large but finite downside";
          }
          return {
            maxProfitText: maxProfitDesc,
            maxLossText: `Max loss ≈ $${maxLoss.toFixed(
              2
            )} (total premium paid)`,
          };
        } else {
          // short
          const maxProfit = totalPrem;
          return {
            maxProfitText: `Max profit ≈ $${maxProfit.toFixed(
              2
            )} (premium received)`,
            maxLossText:
              "Potential loss is very large (especially on the upside)",
          };
        }
      }

      // Chart.js setup
      let payoffChart;
      const ctx = document.getElementById("payoffChart").getContext("2d");

      function buildChart(labels, data) {
        if (payoffChart) {
          payoffChart.destroy();
        }
        payoffChart = new Chart(ctx, {
          type: "line",
          data: {
            labels,
            datasets: [
              {
                label: "P/L at expiry",
                data,
                borderColor: "#2563eb",
                backgroundColor: "rgba(37, 99, 235, 0.1)",
                tension: 0.1,
                fill: true,
                pointRadius: 0,
              },
            ],
          },
          options: {
            responsive: true,
            maintainAspectRatio: false,
            interaction: {
              mode: "index",
              intersect: false,
            },
            plugins: {
              legend: { display: false },
              tooltip: {
                callbacks: {
                  label: function (ctx) {
                    return `P/L: $${ctx.parsed.y.toFixed(2)}`;
                  },
                },
              },
            },
            scales: {
              x: {
                title: { display: true, text: "Underlying price at expiry" },
              },
              y: {
                title: { display: true, text: "Profit / Loss ($)" },
                ticks: {
                  callback: function (value) {
                    return "$" + value;
                  },
                },
              },
            },
          },
        });
      }

      function calculateAndRender() {
        const strategy = strategyTypeEl.value;
        const S0 = toNumber(underlyingPriceEl, 100);
        const K = toNumber(strikeEl, S0);
        const Kp = toNumber(putStrikeEl, S0 * 0.95);
        const Kc = toNumber(callStrikeEl, S0 * 1.05);
        const C = toNumber(callPremiumEl, 0);
        const P = toNumber(putPremiumEl, 0);
        const contracts = Math.max(1, Math.round(toNumber(contractsEl, 1)));
        const step = Math.max(0.1, toNumber(priceStepEl, 1));

        const params = { K, Kp, Kc, C, P, contracts };

        // Total premium
        const totalPremPerShare = C + P;
        const totalPrem = totalPremPerShare * CONTRACT_SIZE * contracts;
        const isLong = strategy.startsWith("long");
        totalPremiumDisplay.textContent = "$" + totalPrem.toFixed(2);
        netCreditDebitLabel.textContent = isLong
          ? "Net debit (premium paid)"
          : "Net credit (premium received)";

        // Breakevens
        const bes = computeBreakevens(strategy, params);
        if (bes.length === 2) {
          breakevenDisplay.textContent = `$${bes[0].toFixed(
            2
          )} and $${bes[1].toFixed(2)}`;
        } else {
          breakevenDisplay.textContent = "–";
        }

        // Extremes
        const extremes = computeExtremes(strategy, params);
        maxProfitLossDisplay.textContent = `${extremes.maxProfitText}; ${extremes.maxLossText}`;

        // Chart range
        let minPrice, maxPrice;
        if (strategy.includes("straddle")) {
          minPrice = Math.max(0, K - 4 * totalPremPerShare);
          maxPrice = K + 4 * totalPremPerShare;
        } else {
          const mid = (Kp + Kc) / 2;
          minPrice = Math.max(0, mid - 4 * totalPremPerShare);
          maxPrice = mid + 4 * totalPremPerShare;
        }
        if (minPrice === maxPrice) {
          minPrice = Math.max(0, S0 * 0.5);
          maxPrice = S0 * 1.5;
        }

        const labels = [];
        const data = [];
        for (let S = minPrice; S <= maxPrice + 1e-8; S += step) {
          labels.push(S.toFixed(2));
          data.push(payoffAtPrice(strategy, S, params));
        }
        buildChart(labels, data);

        // P/L at specific price
        const Sspot = toNumber(spotForPLEl, S0);
        const plSpot = payoffAtPrice(strategy, Sspot, params);
        plAtSpotDisplay.textContent =
          (plSpot >= 0 ? "+" : "") + "$" + plSpot.toFixed(2);
        plAtSpotDisplay.classList.toggle("text-green-600", plSpot > 0);
        plAtSpotDisplay.classList.toggle("text-red-600", plSpot < 0);
      }

      calculateBtn.addEventListener("click", calculateAndRender);

      resetBtn.addEventListener("click", () => {
        strategyTypeEl.value = "long-straddle";
        underlyingPriceEl.value = 100;
        strikeEl.value = 100;
        putStrikeEl.value = 95;
        callStrikeEl.value = 105;
        callPremiumEl.value = 3.5;
        putPremiumEl.value = 3.2;
        contractsEl.value = 1;
        priceStepEl.value = 1;
        spotForPLEl.value = 100;
        updateStrikeInputs();
        calculateAndRender();
      });

      // Recalculate on key input blur for responsiveness
      [
        underlyingPriceEl,
        strikeEl,
        putStrikeEl,
        callStrikeEl,
        callPremiumEl,
        putPremiumEl,
        contractsEl,
        priceStepEl,
        spotForPLEl,
      ].forEach((el) => {
        el.addEventListener("change", calculateAndRender);
        el.addEventListener("blur", calculateAndRender);
      });

      // Initialize
      updateStrikeInputs();
      calculateAndRender();
    </script>

