    <script>
      // Simple helper to get trimmed value
      function val(id) {
        return document.getElementById(id).value.trim();
      }

      // Presets
      const presetButtons = document.querySelectorAll("button[data-preset]");
      presetButtons.forEach((btn) => {
        btn.addEventListener("click", () => {
          presetButtons.forEach((b) => b.classList.remove("active"));
          btn.classList.add("active");
          const preset = btn.getAttribute("data-preset");
          applyPreset(preset);
        });
      });

      function applyPreset(preset) {
        const context = document.getElementById("context");
        const industry = document.getElementById("industry");
        const country = document.getElementById("country");
        const horizon = document.getElementById("horizon");

        if (preset === "saas") {
          context.value =
            "Launching a B2B SaaS productivity and collaboration platform for small and medium businesses, focused on the US and EU markets.";
          industry.value = "SaaS / B2B software";
          country.value = "US & EU";
          horizon.value = "Next 2–3 years";
        } else if (preset === "retail") {
          context.value =
            "Expanding a mid-sized fashion retail brand with both e‑commerce and physical stores into two new European countries.";
          industry.value = "Retail / Fashion";
          country.value = "Europe";
          horizon.value = "Next 3–5 years";
        } else if (preset === "healthcare") {
          context.value =
            "Introducing a digital health monitoring app for chronic disease patients, working with hospitals and insurers.";
          industry.value = "Healthcare / Digital health";
          country.value = "US";
          horizon.value = "Next 3 years";
        } else if (preset === "manufacturing") {
          context.value =
            "Modernizing a mid-sized manufacturing plant with automation and IoT sensors to increase efficiency and reduce downtime.";
          industry.value = "Manufacturing / Industrial";
          country.value = "Global supply chain";
          horizon.value = "Next 5 years";
        }
      }

      // Insert example bullets for each section
      const exampleButtons = document.querySelectorAll("button[data-section]");
      const examples = {
        political: `- Stable government with pro-business policies\n- Data privacy and cybersecurity regulations tightening\n- Public funding available for digital transformation`,
        economic: `- Slowing GDP growth but resilient digital spending\n- Higher interest rates increasing cost of capital\n- Currency fluctuations affecting international pricing`,
        social: `- Younger workforce expects flexible, remote-friendly tools\n- Increasing focus on work–life balance and wellbeing\n- Growing acceptance of subscription-based services`,
        technological: `- Rapid adoption of cloud and mobile technologies\n- AI and automation reshaping workflows and expectations\n- Cybersecurity threats becoming more sophisticated`,
        legal: `- Stricter data protection and consumer rights laws\n- Industry-specific compliance requirements (e.g., HIPAA, PCI-DSS)\n- IP and licensing considerations for software components`,
        environmental: `- Pressure to reduce carbon footprint and energy use\n- Customers favor vendors with strong ESG credentials\n- Potential physical climate risks to data centers and logistics`,
      };

      exampleButtons.forEach((btn) => {
        btn.addEventListener("click", () => {
          const section = btn.getAttribute("data-section");
          const textarea = document.getElementById(section);
          if (textarea && examples[section]) {
            textarea.value = examples[section];
          }
        });
      });

      // Autofill prompts (lightweight: just ensure placeholders are present)
      document.getElementById("btn-autofill").addEventListener("click", () => {
        [
          "political",
          "economic",
          "social",
          "technological",
          "legal",
          "environmental",
        ].forEach((id) => {
          const el = document.getElementById(id);
          if (el && !el.value.trim()) {
            el.placeholder =
              (el.placeholder || "") +
              "\n\nTip: List 3–5 bullet points. For each, add why it matters for your strategy.";
          }
        });
      });

      // Clear all
      document.getElementById("btn-clear-all").addEventListener("click", () => {
        [
          "context",
          "industry",
          "country",
          "horizon",
          "political",
          "economic",
          "social",
          "technological",
          "legal",
          "environmental",
          "summary-output",
        ].forEach((id) => {
          const el = document.getElementById(id);
          if (el) el.value = "";
        });
        document.getElementById("copy-status").classList.add("hidden");
      });

      // Generate summary
      document
        .getElementById("btn-generate-summary")
        .addEventListener("click", () => {
          const ctx =
            val("context") ||
            "[Describe your business, product, or project here]";
          const ind = val("industry");
          const ctry = val("country");
          const hor = val("horizon");

          const headerLines = [];
          headerLines.push("PESTLE ANALYSIS");
          headerLines.push("================");
          headerLines.push("");
          headerLines.push("Scope");
          headerLines.push("-----");
          headerLines.push("Context: " + ctx);
          if (ind) headerLines.push("Industry: " + ind);
          if (ctry) headerLines.push("Country / Region: " + ctry);
          if (hor) headerLines.push("Time horizon: " + hor);
          headerLines.push("");

          function sectionBlock(title, content) {
            const safe =
              content || "[No factors listed yet – add 3–5 bullet points.]";
            return title + "\n" + "-".repeat(title.length) + "\n" + safe + "\n";
          }

          const summaryParts = [
            ...headerLines,
            sectionBlock("Political factors", val("political")),
            sectionBlock("Economic factors", val("economic")),
            sectionBlock("Social factors", val("social")),
            sectionBlock("Technological factors", val("technological")),
            sectionBlock("Legal factors", val("legal")),
            sectionBlock("Environmental factors", val("environmental")),
            "Notes",
            "-----",
            "• Classify each factor as an opportunity or threat.",
            "• Prioritize high-impact, likely factors for your strategy and risk register.",
          ];

          document.getElementById("summary-output").value =
            summaryParts.join("\n");
          document.getElementById("copy-status").classList.add("hidden");
        });

      // Copy to clipboard
      document
        .getElementById("btn-copy")
        .addEventListener("click", async () => {
          const txt = document.getElementById("summary-output").value;
          if (!txt.trim()) return;
          try {
            await navigator.clipboard.writeText(txt);
            const status = document.getElementById("copy-status");
            status.textContent = "Summary copied to clipboard.";
            status.classList.remove("hidden");
            setTimeout(() => status.classList.add("hidden"), 3000);
          } catch (e) {
            alert("Unable to copy. Please copy manually.");
          }
        });

      // Download as .txt
      document.getElementById("btn-download").addEventListener("click", () => {
        const txt = document.getElementById("summary-output").value;
        if (!txt.trim()) return;
        const blob = new Blob([txt], { type: "text/plain" });
        const url = URL.createObjectURL(blob);
        const a = document.createElement("a");
        a.href = url;
        a.download = "pestle-analysis.txt";
        document.body.appendChild(a);
        a.click();
        document.body.removeChild(a);
        URL.revokeObjectURL(url);
      });

      // Print
      document.getElementById("btn-print").addEventListener("click", () => {
        const txt = document.getElementById("summary-output").value;
        if (!txt.trim()) return;
        const w = window.open("", "_blank");
        if (!w) return;
        w.document.write(
          "<pre style=\"font-family: system-ui, -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif; white-space: pre-wrap; word-wrap: break-word; padding: 16px;\">" +
            txt.replace(/</g, "&lt;").replace(/>/g, "&gt;") +
            "</pre>"
        );
        w.document.close();
        w.focus();
        w.print();
      });
    </script>

