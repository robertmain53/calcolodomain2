    <script>
      // Unit toggle and labels
      const unitButtons = document.querySelectorAll(".unit-toggle");
      const weightInput = document.getElementById("weight");
      const weightUnitLabel = document.getElementById("weight-unit-label");
      let currentUnits = "metric"; // default

      unitButtons.forEach((btn) => {
        btn.addEventListener("click", () => {
          const selected = btn.getAttribute("data-units");
          if (selected === currentUnits) return;
          currentUnits = selected;

          unitButtons.forEach((b) => {
            if (b.getAttribute("data-units") === currentUnits) {
              b.classList.remove(
                "border-gray-300",
                "text-gray-700",
                "bg-white"
              );
              b.classList.add("border-blue-600", "text-blue-600", "bg-blue-50");
            } else {
              b.classList.remove(
                "border-blue-600",
                "text-blue-600",
                "bg-blue-50"
              );
              b.classList.add("border-gray-300", "text-gray-700", "bg-white");
            }
          });

          // Update label and convert weight value approximately
          if (currentUnits === "metric") {
            weightUnitLabel.textContent = "(kg)";
            const lb = parseFloat(weightInput.value);
            if (!isNaN(lb)) {
              weightInput.value = (lb / 2.20462).toFixed(1);
            }
          } else {
            weightUnitLabel.textContent = "(lb)";
            const kg = parseFloat(weightInput.value);
            if (!isNaN(kg)) {
              weightInput.value = (kg * 2.20462).toFixed(1);
            }
          }
        });
      });

      function getVO2Category(vo2, age, sex) {
        // Simple heuristic categories based on broad adult norms
        const isMale = sex === "male";
        let category = "Fair";
        let percentile = "around the average range";

        if (isMale) {
          if (vo2 < 25) {
            category = "Very poor";
            percentile = "well below average";
          } else if (vo2 < 34) {
            category = "Poor";
            percentile = "below average";
          } else if (vo2 < 43) {
            category = "Fair";
            percentile = "around average";
          } else if (vo2 < 51) {
            category = "Good";
            percentile = "above average";
          } else {
            category = "Excellent";
            percentile = "well above average";
          }
        } else {
          if (vo2 < 22) {
            category = "Very poor";
            percentile = "well below average";
          } else if (vo2 < 29) {
            category = "Poor";
            percentile = "below average";
          } else if (vo2 < 36) {
            category = "Fair";
            percentile = "around average";
          } else if (vo2 < 43) {
            category = "Good";
            percentile = "above average";
          } else {
            category = "Excellent";
            percentile = "well above average";
          }
        }

        // Light age adjustment in description only
        let ageNote = "";
        if (age < 30) ageNote = " for your age";
        else if (age > 55) ageNote = " for your age group";

        return { category, percentile: percentile + ageNote };
      }

      function calculateRockport() {
        const age = parseInt(document.getElementById("age").value, 10);
        const sex = document.getElementById("sex").value;
        const weightVal = parseFloat(weightInput.value);
        const timeMin = parseInt(document.getElementById("time-min").value, 10);
        const timeSec = parseInt(document.getElementById("time-sec").value, 10);
        const hr = parseInt(document.getElementById("hr").value, 10);

        if (isNaN(age) || age < 10 || age > 90) {
          alert("Please enter a valid age between 10 and 90 years.");
          return;
        }
        if (isNaN(weightVal) || weightVal <= 0) {
          alert("Please enter a valid body weight.");
          return;
        }
        if (
          isNaN(timeMin) ||
          isNaN(timeSec) ||
          timeMin < 8 ||
          timeMin > 40 ||
          timeSec < 0 ||
          timeSec > 59
        ) {
          alert(
            "Please enter a realistic 1‑mile time (8–40 minutes, seconds 0–59)."
          );
          return;
        }
        if (isNaN(hr) || hr < 60 || hr > 220) {
          alert("Please enter a realistic heart rate between 60 and 220 bpm.");
          return;
        }

        // Convert weight to pounds for the Rockport equation
        const weightLb =
          currentUnits === "metric" ? weightVal * 2.20462 : weightVal;
        const timeDecimal = timeMin + timeSec / 60;

        const sexCode = sex === "male" ? 1 : 0;

        // Rockport equation
        const vo2 =
          132.853 -
          0.0769 * weightLb -
          0.3877 * age +
          6.315 * sexCode -
          3.2649 * timeDecimal -
          0.1565 * hr;

        const vo2Rounded = Math.max(5, vo2); // avoid negative values in edge cases

        const { category, percentile } = getVO2Category(vo2Rounded, age, sex);

        // Update UI
        document.getElementById("vo2-value").textContent =
          vo2Rounded.toFixed(1) + " ml·kg⁻¹·min⁻¹";
        document.getElementById("fitness-category").textContent = category;
        document.getElementById("fitness-percentile").textContent =
          percentile.charAt(0).toUpperCase() + percentile.slice(1);

        const summaryList = document.getElementById("summary-list");
        summaryList.innerHTML = "";
        const li1 = document.createElement("li");
        li1.textContent = `Age: ${age} years, Sex: ${
          sex === "male" ? "Male" : "Female"
        }`;
        const li2 = document.createElement("li");
        li2.textContent = `Weight: ${weightVal.toFixed(1)} ${
          currentUnits === "metric" ? "kg" : "lb"
        } (${weightLb.toFixed(1)} lb used in formula)`;
        const li3 = document.createElement("li");
        li3.textContent = `1‑mile time: ${timeMin} min ${timeSec} s (${timeDecimal.toFixed(
          2
        )} min)`;
        const li4 = document.createElement("li");
        li4.textContent = `Heart rate at finish: ${hr} bpm`;
        [li1, li2, li3, li4].forEach((li) => summaryList.appendChild(li));

        const interp = document.getElementById("interpretation-text");
        interp.textContent =
          `Your estimated VO₂ max is ${vo2Rounded.toFixed(
            1
          )} ml·kg⁻¹·min⁻¹, which falls in the “${category}” range ${percentile}. ` +
          `Repeat the Rockport Walk Test every 6–8 weeks under similar conditions to monitor changes in your aerobic fitness.`;

        document.getElementById("results-card").classList.remove("hidden");
        document.getElementById("results-placeholder").classList.add("hidden");
      }

      document
        .getElementById("calculate-btn")
        .addEventListener("click", calculateRockport);

      document.getElementById("reset-btn").addEventListener("click", () => {
        document.getElementById("age").value = 35;
        document.getElementById("sex").value = "female";
        currentUnits = "metric";
        unitButtons.forEach((b) => {
          if (b.getAttribute("data-units") === "metric") {
            b.classList.remove("border-gray-300", "text-gray-700", "bg-white");
            b.classList.add("border-blue-600", "text-blue-600", "bg-blue-50");
          } else {
            b.classList.remove(
              "border-blue-600",
              "text-blue-600",
              "bg-blue-50"
            );
            b.classList.add("border-gray-300", "text-gray-700", "bg-white");
          }
        });
        weightUnitLabel.textContent = "(kg)";
        weightInput.value = 70;
        document.getElementById("time-min").value = 15;
        document.getElementById("time-sec").value = 0;
        document.getElementById("hr").value = 140;
        document.getElementById("results-card").classList.add("hidden");
        document
          .getElementById("results-placeholder")
          .classList.remove("hidden");
      });

      // Mobile menu script safeguard if not in external file
      document.addEventListener("DOMContentLoaded", () => {
        const toggle = document.getElementById("mobile-menu-toggle");
        const menu = document.getElementById("mobile-menu");
        if (toggle && menu) {
          toggle.addEventListener("click", () => {
            const isHidden = menu.classList.contains("hidden");
            menu.classList.toggle("hidden", !isHidden);
            toggle.setAttribute("aria-expanded", isHidden ? "true" : "false");
          });
        }
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
