    <script>
      // Utility helpers
      function roundTo(value, decimals) {
        if (!isFinite(value)) return NaN;
        const factor = Math.pow(10, decimals);
        return Math.round(value * factor) / factor;
      }

      function showElement(el) {
        el.classList.remove("hidden");
      }

      function hideElement(el) {
        el.classList.add("hidden");
      }

      document.addEventListener("DOMContentLoaded", function () {
        const a260Input = document.getElementById("a260");
        const a280Input = document.getElementById("a280");
        const dilutionInput = document.getElementById("dilution-factor");
        const volumeInput = document.getElementById("volume");
        const volumeUnit = document.getElementById("volume-unit");
        const pathlengthInput = document.getElementById("pathlength");
        const pathlengthUnit = document.getElementById("pathlength-unit");
        const naTypeRadios = document.querySelectorAll('input[name="na-type"]');
        const customExtWrapper = document.getElementById("custom-ext-wrapper");
        const customExtInput = document.getElementById("custom-ext");
        const customExtUnit = document.getElementById("custom-ext-unit");
        const mwInput = document.getElementById("mw");

        const calcBtn = document.getElementById("calc-btn");
        const resetBtn = document.getElementById("reset-btn");

        const resultsSection = document.getElementById("results-section");
        const concNgUlSpan = document.getElementById("conc-ngul");
        const concUgMlSpan = document.getElementById("conc-ugml");
        const concUMSpan = document.getElementById("conc-uM");
        const concUMNote = document.getElementById("conc-uM-note");
        const totalUgSpan = document.getElementById("total-ug");
        const ratioSpan = document.getElementById("ratio-260-280");
        const ratioInterpretation = document.getElementById(
          "ratio-interpretation"
        );
        const warningText = document.getElementById("warning-text");

        // Toggle custom extinction coefficient panel
        naTypeRadios.forEach((radio) => {
          radio.addEventListener("change", () => {
            if (radio.value === "custom" && radio.checked) {
              showElement(customExtWrapper);
            } else if (radio.checked) {
              hideElement(customExtWrapper);
            }
          });
        });

        function getSelectedNaType() {
          for (const r of naTypeRadios) {
            if (r.checked) return r.value;
          }
          return "dsdna";
        }

        function calculate() {
          hideElement(warningText);
          warningText.textContent = "";

          const A260raw = parseFloat(a260Input.value);
          const A280raw = parseFloat(a280Input.value);
          const dilution = parseFloat(dilutionInput.value) || 1;
          const volumeVal = parseFloat(volumeInput.value);
          const pathVal = parseFloat(pathlengthInput.value);

          if (!isFinite(A260raw)) {
            alert("Please enter a valid A260 absorbance value.");
            return;
          }
          if (!isFinite(pathVal) || pathVal <= 0) {
            alert("Please enter a valid pathlength greater than 0.");
            return;
          }

          // Normalize pathlength to cm
          let pathCm = pathVal;
          if (pathlengthUnit.value === "mm") {
            pathCm = pathVal / 10.0;
          }

          // Normalize A260 to 1 cm pathlength
          const A260 = A260raw * (1.0 / pathCm);

          // Determine extinction coefficient in µg/mL per A260
          const naType = getSelectedNaType();
          let epsilonUgPerMl;
          let canComputeUM = false;
          let mw = parseFloat(mwInput.value);

          if (naType === "dsdna") {
            epsilonUgPerMl = 50;
          } else if (naType === "ssdna") {
            epsilonUgPerMl = 33;
          } else if (naType === "rna") {
            epsilonUgPerMl = 40;
          } else {
            // custom
            const extVal = parseFloat(customExtInput.value);
            if (!isFinite(extVal) || extVal <= 0) {
              alert("Please enter a valid custom extinction coefficient.");
              return;
            }
            if (customExtUnit.value === "ugml") {
              epsilonUgPerMl = extVal;
            } else {
              // L·mol⁻¹·cm⁻¹, need MW to convert
              if (!isFinite(mw) || mw <= 0) {
                alert(
                  "To use L·mol⁻¹·cm⁻¹, please also enter a valid molecular weight."
                );
                return;
              }
              // 1 A260 = ε (L/mol·cm) * c (mol/L) * 1 cm
              // c (mol/L) = 1 / ε
              // mass conc (g/L) = c * MW = MW / ε
              // Convert g/L to µg/mL: 1 g/L = 1000 µg/mL
              epsilonUgPerMl = (mw / extVal) * 1000;
              canComputeUM = true;
            }
          }

          // Concentration in µg/mL
          const concUgMl = A260 * epsilonUgPerMl * dilution;
          const concNgUl = concUgMl; // numerically same

          // Total yield
          let volumeUl;
          if (isFinite(volumeVal) && volumeVal > 0) {
            volumeUl = volumeUnit.value === "mL" ? volumeVal * 1000 : volumeVal;
          }

          let totalUg = NaN;
          if (volumeUl) {
            totalUg = (concNgUl * volumeUl) / 1000.0;
          }

          // Molar concentration (µM) if MW known or custom L/mol·cm with MW
          let concUM = NaN;
          if (!isFinite(mw) || mw <= 0) {
            // approximate MW for dsDNA/ssDNA/RNA if user didn't provide, only for rough estimate
            if (naType === "dsdna") {
              // approximate 660 g/mol per bp, but we don't know length -> cannot compute
              canComputeUM = false;
            } else if (naType === "ssdna") {
              canComputeUM = false;
            } else if (naType === "rna") {
              canComputeUM = false;
            }
          } else {
            canComputeUM = true;
          }

          if (canComputeUM && isFinite(mw) && mw > 0) {
            // conc (g/L) = concUgMl * 1e-3
            const concGL = concUgMl * 1e-3;
            const concMolL = concGL / mw;
            concUM = concMolL * 1e6;
          }

          // A260/A280 ratio
          let ratio = NaN;
          if (isFinite(A280raw) && A280raw > 0) {
            const A280 = A280raw * (1.0 / pathCm);
            ratio = A260 / A280;
          }

          // Populate UI
          showElement(resultsSection);
          concUgMlSpan.textContent = isFinite(concUgMl)
            ? roundTo(concUgMl, 2)
            : "–";
          concNgUlSpan.textContent = isFinite(concNgUl)
            ? roundTo(concNgUl, 2)
            : "–";

          if (isFinite(concUM)) {
            concUMSpan.textContent = roundTo(concUM, 3);
            concUMNote.textContent = mw ? " (using MW " + mw + " g/mol)" : "";
          } else {
            concUMSpan.textContent = "–";
            concUMNote.textContent = "";
          }

          totalUgSpan.textContent = isFinite(totalUg)
            ? roundTo(totalUg, 3)
            : "–";

          if (isFinite(ratio)) {
            ratioSpan.textContent = roundTo(ratio, 2);
            let interp = "";
            if (naType === "rna") {
              if (ratio >= 1.9 && ratio <= 2.1) {
                interp = "Typical for relatively pure RNA.";
              } else if (ratio < 1.8) {
                interp =
                  "Low ratio – possible protein or phenol contamination.";
              } else if (ratio > 2.2) {
                interp =
                  "High ratio – possible baseline issue or sample contamination.";
              }
            } else {
              if (ratio >= 1.7 && ratio <= 1.9) {
                interp = "Typical for relatively pure dsDNA.";
              } else if (ratio < 1.7) {
                interp =
                  "Low ratio – possible protein or phenol contamination.";
              } else if (ratio > 2.1) {
                interp =
                  "High ratio – possible RNA contamination or baseline issue.";
              }
            }
            ratioInterpretation.textContent = interp;
          } else {
            ratioSpan.textContent = "–";
            ratioInterpretation.textContent =
              "Enter A280 to assess purity (A260/A280).";
          }

          // Warnings for extreme values
          const warnings = [];
          if (A260raw > 2 || A260raw < 0) {
            warnings.push(
              "A260 is outside the typical linear range for many spectrophotometers. Consider diluting your sample or checking the blank."
            );
          }
          if (isFinite(concNgUl) && concNgUl > 5000) {
            warnings.push(
              "Calculated concentration is extremely high (> 5000 ng/µL). Ensure the dilution factor and pathlength are correct."
            );
          }
          if (warnings.length) {
            warningText.textContent = warnings.join(" ");
            showElement(warningText);
          }
        }

        function resetForm() {
          a260Input.value = "";
          a280Input.value = "";
          dilutionInput.value = "1";
          volumeInput.value = "";
          volumeUnit.value = "uL";
          pathlengthInput.value = "1";
          pathlengthUnit.value = "cm";
          document.querySelector(
            'input[name="na-type"][value="dsdna"]'
          ).checked = true;
          customExtInput.value = "";
          customExtUnit.value = "Lmol";
          mwInput.value = "";
          hideElement(customExtWrapper);
          hideElement(resultsSection);
          hideElement(warningText);
          warningText.textContent = "";
        }

        calcBtn.addEventListener("click", function (e) {
          e.preventDefault();
          calculate();
        });

        resetBtn.addEventListener("click", function (e) {
          e.preventDefault();
          resetForm();
        });
      });
    </script>

