    <script>
      // Helper: format number
      function fmt(val, decimals = 2) {
        if (!isFinite(val)) return "–";
        return Number(val).toFixed(decimals);
      }

      function setBadge(el, ok) {
        if (!el) return;
        el.className =
          "badge " +
          (ok ? "bg-green-100 text-green-800" : "bg-red-100 text-red-800");
        el.textContent = ok ? "OK" : "NG";
      }

      document.getElementById("reset-btn").addEventListener("click", () => {
        document.getElementById("calc-status").textContent = "";
        document
          .getElementById("results-container")
          .querySelectorAll("span[id]")
          .forEach((s) => (s.textContent = "–"));
        document.getElementById("detailed-results").classList.add("hidden");
        ["res-bending-status", "res-shear-status", "res-global-status"].forEach(
          (id) => {
            const el = document.getElementById(id);
            if (el) {
              el.className = "badge bg-gray-200 text-gray-700";
              el.textContent = "–";
            }
          }
        );
      });

      document.getElementById("calculate-btn").addEventListener("click", () => {
        const statusEl = document.getElementById("calc-status");
        statusEl.textContent = "Calculating…";

        const fy =
          parseFloat(document.getElementById("steel-grade").value) * 1e6; // MPa -> N/m², but we work in N/mm² so keep MPa
        const gammaM0 =
          parseFloat(document.getElementById("gamma-m0").value) || 1.0;
        const gammaM1 =
          parseFloat(document.getElementById("gamma-m1").value) || 1.0;

        const A = parseFloat(document.getElementById("a").value); // mm²
        const Iy = parseFloat(document.getElementById("iy").value); // mm⁴
        const Wpl = parseFloat(document.getElementById("wpl-y").value); // mm³
        const Wel = parseFloat(document.getElementById("wel-y").value); // mm³
        const It = parseFloat(document.getElementById("it").value); // mm⁴
        const Iw = parseFloat(document.getElementById("iw").value); // mm⁶
        const secClass = parseInt(
          document.getElementById("section-class").value,
          10
        );

        const L = parseFloat(document.getElementById("span").value); // m
        const Lb = parseFloat(document.getElementById("ltb-length").value); // m
        const C1 = parseFloat(document.getElementById("restraint").value);
        const alpha = parseFloat(
          document.getElementById("alpha-buckling").value
        );

        const MyEd = parseFloat(document.getElementById("my-ed").value); // kNm
        const VzEd = parseFloat(document.getElementById("vz-ed").value); // kN

        if (!A || !Iy || !Wpl || !Wel || !L || !Lb) {
          statusEl.textContent =
            "Please fill in all key section and geometry fields.";
          return;
        }

        // Convert to consistent units: N, mm
        const fyNmm2 = fy / 1e6; // MPa -> N/mm²
        const Lmm = L * 1000;
        const Lbmm = Lb * 1000;

        // 1) Bending resistance without LTB
        let MRk; // kNm
        if (secClass === 1 || secClass === 2) {
          MRk = (Wpl * fyNmm2) / 1e6; // N*mm -> kNm
        } else {
          MRk = (Wel * fyNmm2) / 1e6;
        }
        const MRd_noLTB = MRk / (gammaM0 * 1.0); // kNm

        // 2) Shear resistance
        const Av = A; // approximation
        const VRd = (Av * fyNmm2) / (Math.sqrt(3) * gammaM0 * 1e3); // N -> kN

        // 3) LTB: approximate Mcr (very simplified)
        let Mcr = NaN;
        if (It && Iw && Iy && Lbmm > 0) {
          const E = 210000; // N/mm²
          const G = 81000; // N/mm²
          const kz = C1 || 1.0;
          // Simplified elastic critical moment (not exact EC3 Annex F):
          // Mcr ≈ (pi^2 * E * Iw / (Lb^2)) * sqrt(G*It / (E*Iy))
          const pi2 = Math.PI * Math.PI;
          Mcr =
            ((pi2 * E * Iw) / (Lbmm * Lbmm)) * Math.sqrt((G * It) / (E * Iy)); // N*mm
          Mcr = Mcr / 1e6; // kNm
        }

        let chiLT = 1.0;
        let lambdaLT = 0;
        if (isFinite(Mcr) && Mcr > 0) {
          const MRk_used = MRk; // kNm
          lambdaLT = Math.sqrt(MRk_used / Mcr);
          const phi =
            0.5 * (1 + alpha * (lambdaLT - 0.2) + lambdaLT * lambdaLT);
          const chi = 1 / (phi + Math.sqrt(phi * phi - lambdaLT * lambdaLT));
          chiLT = Math.min(1, Math.max(0, chi));
        }

        const MRd_LTB = (MRk * chiLT) / gammaM1; // kNm
        let MRd = Math.min(MRd_noLTB, MRd_LTB || MRd_noLTB);

        // 4) Shear–bending interaction
        let rho = 0;
        if (VzEd >= 0.5 * VRd) {
          rho = 2 * (VzEd / VRd - 0.5);
          rho = Math.min(1, Math.max(0, rho));
          MRd = MRd * (1 - rho);
        }

        // Utilisations
        const etaM = MyEd / MRd;
        const etaV = VzEd / VRd;
        const etaInt = Math.max(etaM, etaV);

        // Update UI
        document.getElementById("mrd-val").textContent = fmt(MRd, 1);
        document.getElementById("vrd-val").textContent = fmt(VRd, 1);
        document.getElementById("eta-m").textContent = fmt(etaM, 2);
        document.getElementById("eta-v").textContent = fmt(etaV, 2);
        document.getElementById("eta-int").textContent = fmt(etaInt, 2);
        document.getElementById("chi-lt").textContent = isFinite(chiLT)
          ? fmt(chiLT, 3)
          : "–";

        setBadge(document.getElementById("res-bending-status"), etaM <= 1.0);
        setBadge(document.getElementById("res-shear-status"), etaV <= 1.0);
        setBadge(document.getElementById("res-global-status"), etaInt <= 1.0);

        // Detailed text
        const details = [];
        details.push(
          `Section: ${
            document.getElementById("section-name").value || "User-defined"
          }, Class ${secClass}`
        );
        details.push(
          `Steel grade: S${fyNmm2.toFixed(0)} (f_y = ${fyNmm2.toFixed(
            0
          )} N/mm²), γ_M0 = ${gammaM0}, γ_M1 = ${gammaM1}`
        );
        details.push("");
        details.push("Bending resistance:");
        details.push(
          `  M_Rk = ${fmt(MRk, 1)} kNm (${
            secClass <= 2 ? "plastic" : "elastic"
          })`
        );
        details.push(`  M_Rd (no LTB) = ${fmt(MRd_noLTB, 1)} kNm`);
        if (isFinite(Mcr) && Mcr > 0) {
          details.push("");
          details.push("Lateral torsional buckling:");
          details.push(`  M_cr ≈ ${fmt(Mcr, 1)} kNm (simplified)`);
          details.push(`  λ_LT = ${fmt(lambdaLT, 3)}, χ_LT = ${fmt(chiLT, 3)}`);
          details.push(`  M_Rd,LTB = ${fmt(MRd_LTB, 1)} kNm`);
        } else {
          details.push("");
          details.push("Lateral torsional buckling:");
          details.push(
            "  Not evaluated (missing I_T or I_w). χ_LT assumed = 1.0."
          );
        }
        if (rho > 0) {
          details.push("");
          details.push("Shear–bending interaction:");
          details.push(`  V_Rd = ${fmt(VRd, 1)} kN, V_Ed = ${fmt(VzEd, 1)} kN`);
          details.push(
            `  ρ = ${fmt(rho, 3)}, reduced M_Rd = ${fmt(MRd, 1)} kNm`
          );
        } else {
          details.push("");
          details.push("Shear–bending interaction:");
          details.push(`  V_Rd = ${fmt(VRd, 1)} kN, V_Ed = ${fmt(VzEd, 1)} kN`);
          details.push("  V_Ed < 0.5·V_Rd → no reduction applied.");
        }
        details.push("");
        details.push("Utilisation ratios:");
        details.push(`  η_M = M_Ed / M_Rd = ${fmt(etaM, 2)}`);
        details.push(`  η_V = V_Ed / V_Rd = ${fmt(etaV, 2)}`);
        details.push(`  Governing η_int = ${fmt(etaInt, 2)}`);

        document.getElementById("details-body").textContent =
          details.join("\n");
        document.getElementById("detailed-results").classList.remove("hidden");

        statusEl.textContent =
          "Calculation completed. Check utilisation ratios and detailed steps below.";
      });
    </script>

