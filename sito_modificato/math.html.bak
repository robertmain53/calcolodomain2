    <script>
      // Simple tab handling
      const tabs = {
        workspace: {
          btn: document.getElementById("tab-workspace"),
          panel: document.getElementById("panel-workspace"),
        },
        practice: {
          btn: document.getElementById("tab-practice"),
          panel: document.getElementById("panel-practice"),
        },
        reference: {
          btn: document.getElementById("tab-reference"),
          panel: document.getElementById("panel-reference"),
        },
      };
      function activateTab(name) {
        Object.keys(tabs).forEach((key) => {
          const { btn, panel } = tabs[key];
          if (key === name) {
            btn.classList.remove("tab-inactive");
            btn.classList.add("tab-active");
            panel.classList.remove("hidden");
          } else {
            btn.classList.remove("tab-active");
            btn.classList.add("tab-inactive");
            panel.classList.add("hidden");
          }
        });
      }
      tabs.workspace.btn.addEventListener("click", () =>
        activateTab("workspace")
      );
      tabs.practice.btn.addEventListener("click", () =>
        activateTab("practice")
      );
      tabs.reference.btn.addEventListener("click", () =>
        activateTab("reference")
      );

      // Keypad insertion
      const mathInput = document.getElementById("math-input");
      document.querySelectorAll("[data-insert]").forEach((btn) => {
        btn.addEventListener("click", () => {
          const insert = btn.getAttribute("data-insert");
          const start = mathInput.selectionStart || 0;
          const end = mathInput.selectionEnd || 0;
          const value = mathInput.value;
          mathInput.value = value.slice(0, start) + insert + value.slice(end);
          const pos = start + insert.length;
          mathInput.focus();
          mathInput.setSelectionRange(pos, pos);
        });
      });

      // Basic math engine using math.js (loaded via CDN)
    </script>
    <script src="https://cdn.jsdelivr.net/npm/mathjs@11.11.0/lib/browser/math.js"></script>
    <script>
      const resultOutput = document.getElementById("result-output");
      const stepsContainer = document.getElementById("steps-container");
      const stepsList = document.getElementById("steps-list");
      const btnCalc = document.getElementById("btn-calc");
      const btnClear = document.getElementById("btn-clear");
      const btnExample = document.getElementById("btn-example");
      const btnGraph = document.getElementById("btn-graph");
      const graphCanvas = document.getElementById("graph-canvas");
      const ctx = graphCanvas.getContext("2d");

      function setResult(html) {
        resultOutput.innerHTML = html;
      }

      function showSteps(steps) {
        stepsList.innerHTML = "";
        if (!steps || !steps.length) {
          stepsContainer.classList.add("hidden");
          return;
        }
        steps.forEach((s) => {
          const li = document.createElement("li");
          li.textContent = s;
          stepsList.appendChild(li);
        });
        stepsContainer.classList.remove("hidden");
      }

      function parseCommand(input) {
        const trimmed = input.trim();
        const lower = trimmed.toLowerCase();

        if (lower.startsWith("solve:")) {
          return { type: "solve", expr: trimmed.slice(6).trim() };
        }
        if (lower.startsWith("factor:")) {
          return { type: "factor", expr: trimmed.slice(7).trim() };
        }
        if (lower.startsWith("derivative:")) {
          return { type: "derivative", expr: trimmed.slice(11).trim() };
        }
        if (lower.startsWith("integral:")) {
          return { type: "integral", expr: trimmed.slice(9).trim() };
        }
        // function evaluation: "expr at x=2"
        const atIndex = lower.indexOf(" at ");
        if (atIndex !== -1) {
          return {
            type: "evaluate",
            expr: trimmed.slice(0, atIndex).trim(),
            at: trimmed.slice(atIndex + 4).trim(),
          };
        }
        return { type: "evaluateRaw", expr: trimmed };
      }

      function compute() {
        const input = mathInput.value.trim();
        if (!input) {
          setResult("Please enter an expression or equation to compute.");
          showSteps([]);
          return;
        }

        try {
          const cmd = parseCommand(input);
          let res,
            steps = [];

          if (cmd.type === "solve") {
            const node = math.parse(cmd.expr);
            const eq =
              node.isAssignmentNode || node.isRelationalNode
                ? node
                : math.parse(cmd.expr + "=0");
            const solutions = math.solve(eq, "x");
            res = solutions;
            steps.push("Rewrote the equation in standard form if needed.");
            steps.push("Applied algebraic solving routines for variable x.");
          } else if (cmd.type === "factor") {
            res = math
              .simplify(cmd.expr, {}, { exactFractions: true })
              .toString();
            steps.push("Parsed the polynomial expression.");
            steps.push(
              "Applied symbolic simplification and factorization rules."
            );
          } else if (cmd.type === "derivative") {
            const d = math.derivative(cmd.expr, "x");
            res = d.toString();
            steps.push("Interpreted the input as a function of x.");
            steps.push("Applied standard derivative rules term-by-term.");
          } else if (cmd.type === "integral") {
            // math.js has limited symbolic integration; we attempt and catch errors
            try {
              const integral = math.integral(cmd.expr, "x");
              res = integral.toString() + " + C";
              steps.push("Attempted symbolic integration with respect to x.");
              steps.push("Added the constant of integration C.");
            } catch (e) {
              res =
                "This integral is too complex for the built-in engine. Try a simpler function or a dedicated CAS.";
              steps = [];
            }
          } else if (cmd.type === "evaluate") {
            const scope = {};
            const atParts = cmd.at.split(",");
            atParts.forEach((part) => {
              const [name, value] = part.split("=");
              if (name && value) {
                scope[name.trim()] = math.evaluate(value.trim());
              }
            });
            const value = math.evaluate(cmd.expr, scope);
            res = value;
            steps.push(
              "Parsed the expression as a function of one or more variables."
            );
            steps.push("Substituted the given values into the expression.");
            steps.push(
              "Evaluated numerically using high-precision arithmetic."
            );
          } else {
            // evaluateRaw
            res = math.evaluate(cmd.expr);
            steps.push(
              "Parsed the expression using standard operator precedence."
            );
            steps.push(
              "Simplified and evaluated the result numerically or symbolically."
            );
          }

          if (typeof res === "number") {
            setResult(
              '<span class="font-mono">' +
                math.format(res, { precision: 14 }) +
                "</span>"
            );
          } else if (Array.isArray(res)) {
            setResult(
              '<span class="font-mono">' + JSON.stringify(res) + "</span>"
            );
          } else if (typeof res === "object" && res !== null && res.entries) {
            setResult(
              '<pre class="font-mono text-xs overflow-x-auto">' +
                JSON.stringify(res.entries, null, 2) +
                "</pre>"
            );
          } else {
            setResult(
              '<span class="font-mono break-all">' + String(res) + "</span>"
            );
          }
          showSteps(steps);
        } catch (err) {
          setResult(
            '<span class="text-red-600 text-sm">Error: ' +
              err.message +
              "</span>"
          );
          showSteps([]);
        }
      }

      btnCalc.addEventListener("click", compute);
      mathInput.addEventListener("keydown", (e) => {
        if (e.key === "Enter" && (e.ctrlKey || e.metaKey || e.shiftKey)) {
          e.preventDefault();
          compute();
        }
      });

      btnClear.addEventListener("click", () => {
        mathInput.value = "";
        setResult(
          'Enter an expression and click <span class="font-semibold">Compute</span> to see the result and steps.'
        );
        showSteps([]);
      });

      btnExample.addEventListener("click", () => {
        mathInput.value = "solve: 2x^2 - 5x - 3 = 0";
        mathInput.focus();
      });

      // Simple graphing: plot y = f(x) for x in [-10, 10]
      function drawAxes(ctx, w, h) {
        ctx.clearRect(0, 0, w, h);
        ctx.fillStyle = "#ffffff";
        ctx.fillRect(0, 0, w, h);
        ctx.strokeStyle = "#e5e7eb";
        ctx.lineWidth = 1;

        // grid
        for (let i = 0; i <= 10; i++) {
          const x = (w / 10) * i;
          const y = (h / 10) * i;
          ctx.beginPath();
          ctx.moveTo(x, 0);
          ctx.lineTo(x, h);
          ctx.stroke();
          ctx.beginPath();
          ctx.moveTo(0, y);
          ctx.lineTo(w, y);
          ctx.stroke();
        }

        // axes
        ctx.strokeStyle = "#9ca3af";
        ctx.lineWidth = 1.2;
        const midX = w / 2;
        const midY = h / 2;
        ctx.beginPath();
        ctx.moveTo(0, midY);
        ctx.lineTo(w, midY);
        ctx.stroke();
        ctx.beginPath();
        ctx.moveTo(midX, 0);
        ctx.lineTo(midX, h);
        ctx.stroke();
      }

      function plotExpression() {
        const expr = mathInput.value.trim();
        if (!expr) return;
        const cmd = parseCommand(expr);
        const rawExpr =
          cmd.expr || cmd.type === "evaluateRaw" ? cmd.expr : expr;

        let compiled;
        try {
          compiled = math.compile(rawExpr);
        } catch (e) {
          setResult(
            '<span class="text-red-600 text-sm">Cannot plot: ' +
              e.message +
              "</span>"
          );
          return;
        }

        const w = graphCanvas.width;
        const h = graphCanvas.height;
        drawAxes(ctx, w, h);

        const xMin = -10,
          xMax = 10;
        const yMin = -10,
          yMax = 10;
        const toCanvasX = (x) => ((x - xMin) / (xMax - xMin)) * w;
        const toCanvasY = (y) => h - ((y - yMin) / (yMax - yMin)) * h;

        ctx.strokeStyle = "#2563eb";
        ctx.lineWidth = 1.5;
        ctx.beginPath();
        let started = false;

        for (let i = 0; i <= w; i++) {
          const x = xMin + (xMax - xMin) * (i / w);
          let y;
          try {
            y = compiled.evaluate({ x });
            if (typeof y !== "number" || !isFinite(y)) {
              started = false;
              continue;
            }
          } catch {
            started = false;
            continue;
          }
          const cx = toCanvasX(x);
          const cy = toCanvasY(y);
          if (!started) {
            ctx.moveTo(cx, cy);
            started = true;
          } else {
            ctx.lineTo(cx, cy);
          }
        }
        ctx.stroke();
      }

      drawAxes(ctx, graphCanvas.width, graphCanvas.height);
      btnGraph.addEventListener("click", plotExpression);

      // PRACTICE ENGINE
      const practiceTopic = document.getElementById("practice-topic");
      const practiceQuestion = document.getElementById("practice-question");
      const practiceAnswer = document.getElementById("practice-answer");
      const practiceCheck = document.getElementById("practice-check");
      const practiceNew = document.getElementById("practice-new");
      const practiceFeedback = document.getElementById("practice-feedback");
      const practiceCount = document.getElementById("practice-count");

      let currentQuestion = null;
      let questionCounter = 1;

      function randInt(min, max) {
        return Math.floor(Math.random() * (max - min + 1)) + min;
      }

      function generateQuestion(topic) {
        if (topic === "arithmetic") {
          const a = randInt(1, 20);
          const b = randInt(1, 20);
          const type = Math.random() < 0.5 ? "percent-of" : "percent-change";
          if (type === "percent-of") {
            const q = `What is ${a}% of ${b}?`;
            const ans = (a / 100) * b;
            return { text: q, answer: ans, tolerance: 1e-6 };
          } else {
            const oldVal = randInt(10, 80);
            const change = randInt(-10, 20);
            const newVal = oldVal + change;
            const q = `What is the percentage change from ${oldVal} to ${newVal}? (Answer in %, can be negative)`;
            const ans = ((newVal - oldVal) / oldVal) * 100;
            return { text: q, answer: ans, tolerance: 0.1 };
          }
        }

        if (topic === "algebra") {
          const a = randInt(1, 9);
          const b = randInt(-10, 10);
          const c = randInt(-10, 10);
          const q = `Solve for x: ${a}x ${
            b >= 0 ? "+ " + b : "- " + Math.abs(b)
          } = ${c}`;
          const ans = (c - b) / a;
          return { text: q, answer: ans, tolerance: 1e-6 };
        }

        if (topic === "quadratic") {
          const r1 = randInt(-5, 5);
          const r2 = randInt(-5, 5);
          const a = 1;
          const b = -(r1 + r2);
          const c = r1 * r2;
          const q = `One root of the quadratic equation x² ${
            b >= 0 ? "+ " + b : "- " + Math.abs(b)
          }x ${c >= 0 ? "+ " + c : "- " + Math.abs(c)} = 0 is:`;
          // Accept either root
          return { text: q, answer: [r1, r2], tolerance: 1e-6 };
        }

        if (topic === "derivative") {
          const n = randInt(2, 5);
          const q = `Find the derivative of f(x) = x^${n}. Give your answer as a simplified expression in terms of x.`;
          const ans = `${n}x^${n - 1}`;
          return { text: q, answer: ans, tolerance: null };
        }

        return {
          text: "Error generating question.",
          answer: null,
          tolerance: null,
        };
      }

      function loadQuestion() {
        const topic = practiceTopic.value;
        currentQuestion = generateQuestion(topic);
        practiceQuestion.textContent = currentQuestion.text;
        practiceAnswer.value = "";
        practiceFeedback.textContent = "";
        practiceFeedback.className = "text-sm";
        practiceCount.textContent = questionCounter;
      }

      function checkAnswer() {
        if (!currentQuestion) return;
        const user = practiceAnswer.value.trim();
        if (!user) {
          practiceFeedback.textContent = "Enter an answer first.";
          practiceFeedback.className = "text-sm text-gray-600";
          return;
        }

        if (Array.isArray(currentQuestion.answer)) {
          const num = Number(user);
          if (!isFinite(num)) {
            practiceFeedback.textContent = "Please enter a numeric answer.";
            practiceFeedback.className = "text-sm text-red-600";
            return;
          }
          const ok = currentQuestion.answer.some(
            (root) =>
              Math.abs(num - root) <= (currentQuestion.tolerance || 1e-6)
          );
          if (ok) {
            practiceFeedback.textContent =
              "Correct! Either root is acceptable.";
            practiceFeedback.className = "text-sm text-emerald-700";
          } else {
            practiceFeedback.textContent = `Not quite. The exact roots are ${currentQuestion.answer.join(
              " and "
            )}.`;
            practiceFeedback.className = "text-sm text-orange-600";
          }
          return;
        }

        if (typeof currentQuestion.answer === "number") {
          const num = Number(user);
          if (!isFinite(num)) {
            practiceFeedback.textContent = "Please enter a numeric answer.";
            practiceFeedback.className = "text-sm text-red-600";
            return;
          }
          const tol = currentQuestion.tolerance || 1e-6;
          if (Math.abs(num - currentQuestion.answer) <= tol) {
            practiceFeedback.textContent = "Correct!";
            practiceFeedback.className = "text-sm text-emerald-700";
          } else {
            practiceFeedback.textContent = `Not quite. The correct answer is approximately ${currentQuestion.answer.toFixed(
              3
            )}.`;
            practiceFeedback.className = "text-sm text-orange-600";
          }
          return;
        }

        // symbolic string comparison (very light)
        const normalizedUser = user.replace(/\s+/g, "").toLowerCase();
        const normalizedAns = String(currentQuestion.answer)
          .replace(/\s+/g, "")
          .toLowerCase();
        if (normalizedUser === normalizedAns) {
          practiceFeedback.textContent = "Correct!";
          practiceFeedback.className = "text-sm text-emerald-700";
        } else {
          practiceFeedback.textContent = `Check your work. One correct answer is: ${currentQuestion.answer}`;
          practiceFeedback.className = "text-sm text-orange-600";
        }
      }

      practiceCheck.addEventListener("click", checkAnswer);
      practiceNew.addEventListener("click", () => {
        questionCounter += 1;
        loadQuestion();
      });
      practiceTopic.addEventListener("change", () => {
        questionCounter = 1;
        loadQuestion();
      });

      // Initialize first practice question
      loadQuestion();

      // Mobile menu script (simple)
      const mobileToggle = document.getElementById("mobile-menu-toggle");
      const mobileMenu = document.getElementById("mobile-menu");
      if (mobileToggle && mobileMenu) {
        mobileToggle.addEventListener("click", () => {
          const isHidden = mobileMenu.classList.contains("hidden");
          mobileMenu.classList.toggle("hidden", !isHidden);
          mobileToggle.setAttribute("aria-expanded", String(isHidden));
        });
      }

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
