    <script>
      (function () {
        const lineTypeEl = document.getElementById("lineType");
        const lossyParamsEl = document.getElementById("lossyParams");
        const z0El = document.getElementById("z0");
        const freqMHzEl = document.getElementById("freqMHz");
        const lengthEl = document.getElementById("length");
        const velFactorEl = document.getElementById("velFactor");
        const alphaDbPerMEl = document.getElementById("alphaDbPerM");
        const betaOverrideEl = document.getElementById("betaOverride");
        const rlEl = document.getElementById("rl");
        const xlEl = document.getElementById("xl");
        const vsEl = document.getElementById("vs");
        const rsEl = document.getElementById("rs");
        const xsEl = document.getElementById("xs");
        const calcBtn = document.getElementById("calcBtn");
        const resetBtn = document.getElementById("resetBtn");
        const errorMsgEl = document.getElementById("errorMsg");

        const zinDisplay = document.getElementById("zinDisplay");
        const gammaLDisplay = document.getElementById("gammaLDisplay");
        const vswrDisplay = document.getElementById("vswrDisplay");
        const rlossDisplay = document.getElementById("rlossDisplay");
        const ploadDisplay = document.getElementById("ploadDisplay");
        const gammaInDisplay = document.getElementById("gammaInDisplay");

        const posSlider = document.getElementById("posSlider");
        const posValue = document.getElementById("posValue");
        const zAtPosDisplay = document.getElementById("zAtPosDisplay");
        const gammaAtPosDisplay = document.getElementById("gammaAtPosDisplay");
        const vswrAtPosDisplay = document.getElementById("vswrAtPosDisplay");

        let lastState = null; // store computed parameters for along-line updates

        function showLossyParams() {
          lossyParamsEl.classList.toggle(
            "hidden",
            lineTypeEl.value !== "lossy"
          );
        }

        lineTypeEl.addEventListener("change", showLossyParams);
        showLossyParams();

        function parseNumber(el) {
          const v = parseFloat(el.value);
          return isFinite(v) ? v : NaN;
        }

        function formatComplex(re, im, digits = 2) {
          if (!isFinite(re) || !isFinite(im)) return "–";
          const r = re.toFixed(digits);
          const i = Math.abs(im).toFixed(digits);
          if (Math.abs(im) < 1e-9) return `${r} Ω`;
          const sign = im >= 0 ? "+" : "−";
          return `${r} ${sign} j${i} Ω`;
        }

        function formatMag(val, digits = 3) {
          if (!isFinite(val)) return "–";
          return val.toFixed(digits);
        }

        function formatDb(val, digits = 2) {
          if (!isFinite(val)) return "–";
          return val.toFixed(digits);
        }

        function calculate() {
          errorMsgEl.textContent = "";
          lastState = null;

          const z0 = parseNumber(z0El);
          const fMHz = parseNumber(freqMHzEl);
          const L = parseNumber(lengthEl);
          const vf = parseNumber(velFactorEl);
          const lineType = lineTypeEl.value;

          const rl = parseNumber(rlEl);
          const xl = parseNumber(xlEl);
          const vs = parseNumber(vsEl);
          const rs = parseNumber(rsEl);
          const xs = parseNumber(xsEl);

          if ([z0, fMHz, L, vf, rl, xl, vs, rs, xs].some((v) => !isFinite(v))) {
            errorMsgEl.textContent =
              "Please enter valid numeric values for all required fields.";
            return;
          }
          if (z0 <= 0 || vf <= 0 || fMHz <= 0 || L < 0) {
            errorMsgEl.textContent =
              "Z0, frequency, velocity factor must be positive; length must be non‑negative.";
            return;
          }

          const f = fMHz * 1e6;
          const c = 299792458;
          const v = vf * c;

          let beta;
          const betaOverride = parseNumber(betaOverrideEl);
          if (lineType === "lossy" && isFinite(betaOverride)) {
            beta = betaOverride;
          } else {
            const lambda = v / f;
            beta = (2 * Math.PI) / lambda;
          }

          let alphaNp = 0;
          if (lineType === "lossy") {
            const alphaDb = parseNumber(alphaDbPerMEl);
            if (!isFinite(alphaDb) || alphaDb < 0) {
              errorMsgEl.textContent =
                "Attenuation α must be a non‑negative number.";
              return;
            }
            alphaNp = (alphaDb * Math.log(10)) / 20;
          }

          const gamma = { re: alphaNp, im: beta };

          const Z0 = { re: z0, im: 0 };
          const ZL = { re: rl, im: xl };
          const Zs = { re: rs, im: xs };

          // Complex helpers
          function cAdd(a, b) {
            return { re: a.re + b.re, im: a.im + b.im };
          }
          function cSub(a, b) {
            return { re: a.re - b.re, im: a.im - b.im };
          }
          function cMul(a, b) {
            return {
              re: a.re * b.re - a.im * b.im,
              im: a.re * b.im + a.im * b.re,
            };
          }
          function cDiv(a, b) {
            const d = b.re * b.re + b.im * b.im;
            return {
              re: (a.re * b.re + a.im * b.im) / d,
              im: (a.im * b.re - a.re * b.im) / d,
            };
          }
          function cMag(a) {
            return Math.sqrt(a.re * a.re + a.im * a.im);
          }
          function cTan(a) {
            // tan(x+jy) = (sin(2x) + j sinh(2y)) / (cos(2x) + cosh(2y))
            const x = a.re,
              y = a.im;
            const sin2x = Math.sin(2 * x);
            const cos2x = Math.cos(2 * x);
            const sinh2y = Math.sinh(2 * y);
            const cosh2y = Math.cosh(2 * y);
            const denom = cos2x + cosh2y;
            return { re: sin2x / denom, im: sinh2y / denom };
          }
          function cTanh(a) {
            // tanh(x+jy) = (sinh(2x) + j sin(2y)) / (cosh(2x) + cos(2y))
            const x = a.re,
              y = a.im;
            const sinh2x = Math.sinh(2 * x);
            const cosh2x = Math.cosh(2 * x);
            const sin2y = Math.sin(2 * y);
            const cos2y = Math.cos(2 * y);
            const denom = cosh2x + cos2y;
            return { re: sinh2x / denom, im: sin2y / denom };
          }
          function cExp(a) {
            const er = Math.exp(a.re);
            return { re: er * Math.cos(a.im), im: er * Math.sin(a.im) };
          }

          // Reflection at load
          const gammaL = cDiv(cSub(ZL, Z0), cAdd(ZL, Z0));
          const magGammaL = cMag(gammaL);
          const vswr = (1 + magGammaL) / (1 - magGammaL);
          const rloss = -20 * Math.log10(magGammaL || 1e-12);

          // Input impedance
          let Zin;
          if (alphaNp === 0 && lineType === "lossless") {
            const jBetaL = { re: 0, im: beta * L };
            const jTan = cTan(jBetaL); // j*tan(beta L) = tan(jBetaL)
            const num = cAdd(ZL, cMul({ re: 0, im: 1 }, cMul(Z0, jTan)));
            const den = cAdd(Z0, cMul({ re: 0, im: 1 }, cMul(ZL, jTan)));
            Zin = cMul(Z0, cDiv(num, den));
          } else {
            const gammaLlen = { re: gamma.re * L, im: gamma.im * L };
            const tgh = cTanh(gammaLlen);
            const num = cAdd(ZL, cMul(Z0, tgh));
            const den = cAdd(Z0, cMul(ZL, tgh));
            Zin = cMul(Z0, cDiv(num, den));
          }

          // Reflection at input
          const gammaIn = cDiv(cSub(Zin, Z0), cAdd(Zin, Z0));
          const magGammaIn = cMag(gammaIn);

          // Power delivered to load
          const ZinPlusZs = cAdd(Zin, Zs);
          const Iin = cDiv({ re: vs, im: 0 }, ZinPlusZs);
          const VL = cMul(Iin, ZL);
          const magVL = cMag(VL);
          const PL = (magVL * magVL) / (2 * Math.max(ZL.re, 1e-9));

          // Update main displays
          zinDisplay.textContent = formatComplex(Zin.re, Zin.im, 2);
          gammaLDisplay.textContent = formatMag(magGammaL, 4);
          vswrDisplay.textContent =
            magGammaL >= 0.9999 ? "∞" : formatMag(vswr, 3);
          rlossDisplay.textContent = formatDb(rloss, 2);
          ploadDisplay.textContent = formatMag(PL, 4);
          gammaInDisplay.textContent = formatMag(magGammaIn, 4);

          // Store state for along-line calculations
          lastState = {
            Z0,
            ZL,
            gamma,
            L,
            gammaL,
          };
          updateAlongLine();
        }

        function updateAlongLine() {
          if (!lastState) return;
          const { Z0, ZL, gamma, L, gammaL } = lastState;
          const posFrac = parseFloat(posSlider.value);
          const z = posFrac * L;
          posValue.textContent = z.toFixed(2);

          function cAdd(a, b) {
            return { re: a.re + b.re, im: a.im + b.im };
          }
          function cSub(a, b) {
            return { re: a.re - b.re, im: a.im - b.im };
          }
          function cMul(a, b) {
            return {
              re: a.re * b.re - a.im * b.im,
              im: a.re * b.im + a.im * b.re,
            };
          }
          function cDiv(a, b) {
            const d = b.re * b.re + b.im * b.im;
            return {
              re: (a.re * b.re + a.im * b.im) / d,
              im: (a.im * b.re - a.re * b.im) / d,
            };
          }
          function cMag(a) {
            return Math.sqrt(a.re * a.re + a.im * a.im);
          }
          function cExp(a) {
            const er = Math.exp(a.re);
            return { re: er * Math.cos(a.im), im: er * Math.sin(a.im) };
          }

          const minus2gammaZ = { re: -2 * gamma.re * z, im: -2 * gamma.im * z };
          const expTerm = cExp(minus2gammaZ);
          const gammaZ = cMul(gammaL, expTerm);
          const magGammaZ = cMag(gammaZ);
          const vswrZ = (1 + magGammaZ) / (1 - magGammaZ);

          const one = { re: 1, im: 0 };
          const num = cAdd(one, gammaZ);
          const den = cSub(one, gammaZ);
          const Zz = cMul(Z0, cDiv(num, den));

          zAtPosDisplay.textContent = formatComplex(Zz.re, Zz.im, 2);
          gammaAtPosDisplay.textContent = formatMag(magGammaZ, 4);
          vswrAtPosDisplay.textContent =
            magGammaZ >= 0.9999 ? "∞" : formatMag(vswrZ, 3);
        }

        posSlider.addEventListener("input", updateAlongLine);

        function reset() {
          z0El.value = 50;
          freqMHzEl.value = 100;
          lengthEl.value = 1;
          velFactorEl.value = 0.66;
          lineTypeEl.value = "lossless";
          alphaDbPerMEl.value = 0.01;
          betaOverrideEl.value = "";
          rlEl.value = 75;
          xlEl.value = 0;
          vsEl.value = 1;
          rsEl.value = 50;
          xsEl.value = 0;
          posSlider.value = 0;
          posValue.textContent = "0.00";
          showLossyParams();
          zinDisplay.textContent = "–";
          gammaLDisplay.textContent = "–";
          vswrDisplay.textContent = "–";
          rlossDisplay.textContent = "–";
