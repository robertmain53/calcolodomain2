  <script>
    // mobile menu toggle
    const mobileToggle = document.getElementById('mobile-menu-toggle');
    const mobileMenu = document.getElementById('mobile-menu');
    if (mobileToggle && mobileMenu) {
      mobileToggle.addEventListener('click', () => {
        const isHidden = mobileMenu.classList.contains('hidden');
        mobileMenu.classList.toggle('hidden', !isHidden);
        mobileToggle.setAttribute('aria-expanded', String(isHidden));
      });
    }

    function parseNumbers(str) {
      return str
        .split(/[\s,;]+/)
        .map(v => v.trim())
        .filter(v => v.length > 0)
        .map(Number)
        .filter(v => !isNaN(v));
    }

    function mean(arr) {
      return arr.reduce((a, b) => a + b, 0) / arr.length;
    }

    document.getElementById('calc-btn').addEventListener('click', () => {
      const xArr = parseNumbers(document.getElementById('x-data').value);
      const yArr = parseNumbers(document.getElementById('y-data').value);
      const decimals = parseInt(document.getElementById('decimals').value, 10);

      if (xArr.length === 0 || yArr.length === 0) {
        alert('Please enter values for both X and Y.');
        return;
      }
      if (xArr.length !== yArr.length) {
        alert('X and Y must have the same number of values.');
        return;
      }
      if (xArr.length < 2) {
        alert('You need at least 2 pairs of values.');
        return;
      }

      const n = xArr.length;
      const xBar = mean(xArr);
      const yBar = mean(yArr);

      let Sxy = 0, Sxx = 0, Syy = 0;
      for (let i = 0; i < n; i++) {
        const dx = xArr[i] - xBar;
        const dy = yArr[i] - yBar;
        Sxy += dx * dy;
        Sxx += dx * dx;
        Syy += dy * dy;
      }

      if (Sxx === 0 || Syy === 0) {
        alert('One of the variables has zero variance. Correlation is undefined.');
        return;
      }

      const r = Sxy / Math.sqrt(Sxx * Syy);
      const r2 = r * r;

      // regression line: y = a + b x
      const b = Sxy / Sxx;
      const a = yBar - b * xBar;

      let dir = 'no linear trend';
      if (r > 0) dir = 'positive';
      if (r < 0) dir = 'negative';

      // interpretation
      const absr = Math.abs(r);
      let strength;
      if (absr < 0.3) strength = 'weak/negligible';
      else if (absr < 0.5) strength = 'moderate';
      else strength = 'strong';

      document.getElementById('res-r').textContent = r.toFixed(decimals);
      document.getElementById('res-r2').textContent = r2.toFixed(decimals);
      document.getElementById('res-n').textContent = n;
      document.getElementById('res-dir').textContent = dir;

      document.getElementById('res-reg').textContent = `Y = ${a.toFixed(decimals)} + ${b.toFixed(decimals)} · X`;

      document.getElementById('res-interpret').textContent =
        `The correlation coefficient is r = ${r.toFixed(decimals)}, indicating a ${strength} ${dir} linear relationship. r² = ${(r2*100).toFixed(1)}% of the variation in Y can be explained by X.`;
    });

    // initial calculation with sample values
    document.getElementById('calc-btn').click();
  </script>

