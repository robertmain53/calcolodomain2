    <script>
      // Basic joist property table (approximate, for preliminary checks only)
      // Values are intentionally simplified and conservative.
      const joistDatabase = {
        K: [
          { id: "K10", label: "K10 (light)", Mn: 60, Vn: 20, EI: 1.0e9 },
          { id: "K12", label: "K12 (medium)", Mn: 90, Vn: 25, EI: 1.6e9 },
          { id: "K16", label: "K16 (heavy)", Mn: 130, Vn: 35, EI: 2.4e9 },
        ],
        LH: [
          { id: "LH18", label: "LH18 (light)", Mn: 200, Vn: 50, EI: 4.0e9 },
          { id: "LH22", label: "LH22 (medium)", Mn: 260, Vn: 65, EI: 5.5e9 },
          { id: "LH26", label: "LH26 (heavy)", Mn: 320, Vn: 80, EI: 7.0e9 },
        ],
        DLH: [
          { id: "DLH24", label: "DLH24 (light)", Mn: 350, Vn: 90, EI: 8.0e9 },
          {
            id: "DLH30",
            label: "DLH30 (medium)",
            Mn: 450,
            Vn: 110,
            EI: 1.0e10,
          },
          { id: "DLH36", label: "DLH36 (heavy)", Mn: 550, Vn: 130, EI: 1.2e10 },
        ],
      };

      // Units: for US, Mn/Vn in kip-ft and kips; EI in kip-ft^2 (scaled); loads in plf
      // For metric, we will convert internally to consistent units (kN, m).

      const unitSystemEl = document.getElementById("unit-system");
      const joistSeriesEl = document.getElementById("joist-series");
      const joistDesignationEl = document.getElementById("joist-designation");
      const spanEl = document.getElementById("span");
      const spacingEl = document.getElementById("spacing");
      const deadLoadEl = document.getElementById("dead-load");
      const liveLoadEl = document.getElementById("live-load");
      const loadComboEl = document.getElementById("load-combo");
      const deflectionLimitTypeEl = document.getElementById(
        "deflection-limit-type"
      );
      const customDeflectionWrapper = document.getElementById(
        "custom-deflection-wrapper"
      );
      const customDeflectionEl = document.getElementById("custom-deflection");
      const modulusEEl = document.getElementById("modulus-E");

      const spanUnitEl = document.getElementById("span-unit");
      const spacingUnitEl = document.getElementById("spacing-unit");
      const areaLoadUnitEl = document.getElementById("area-load-unit");
      const areaLoadUnitEl2 = document.getElementById("area-load-unit-2");
      const deflectionUnitEl = document.getElementById("deflection-unit");
      const EUnitEl = document.getElementById("E-unit");

      const lineLoadDisplay = document.getElementById("line-load-display");
      const serviceLineLoadDisplay = document.getElementById(
        "service-line-load-display"
      );
      const MuDisplay = document.getElementById("Mu-display");
      const MnDisplay = document.getElementById("Mn-display");
      const VuDisplay = document.getElementById("Vu-display");
      const VnDisplay = document.getElementById("Vn-display");
      const momentUtilizationDisplay =
        document.getElementById("moment-utilization");
      const shearUtilizationDisplay =
        document.getElementById("shear-utilization");
      const deflectionDisplay = document.getElementById("deflection-display");
      const deflectionAllowableDisplay = document.getElementById(
        "deflection-allowable-display"
      );
      const deflectionRatioDisplay = document.getElementById(
        "deflection-ratio-display"
      );
      const strengthStatus = document.getElementById("strength-status");
      const deflectionStatus = document.getElementById("deflection-status");
      const overallStatus = document.getElementById("overall-status");
      const resultsSummary = document.getElementById("results-summary");

      function populateJoistOptions() {
        const series = joistSeriesEl.value;
        const list = joistDatabase[series] || [];
        joistDesignationEl.innerHTML = "";
        list.forEach((j) => {
          const opt = document.createElement("option");
          opt.value = j.id;
          opt.textContent = j.label;
          joistDesignationEl.appendChild(opt);
        });
      }

      function updateUnits() {
        const system = unitSystemEl.value;
        if (system === "us") {
          spanUnitEl.textContent = "ft";
          spacingUnitEl.textContent = "ft";
          areaLoadUnitEl.textContent = "psf";
          areaLoadUnitEl2.textContent = "psf";
          deflectionUnitEl.textContent = "in";
          EUnitEl.textContent = "psi";
          modulusEEl.value = 29000000;
        } else {
          spanUnitEl.textContent = "m";
          spacingUnitEl.textContent = "m";
          areaLoadUnitEl.textContent = "kN/m²";
          areaLoadUnitEl2.textContent = "kN/m²";
          deflectionUnitEl.textContent = "mm";
          EUnitEl.textContent = "MPa";
          modulusEEl.value = 200000; // ~200 GPa
        }
      }

      function formatNumber(value, decimals = 2) {
        if (!isFinite(value)) return "–";
        return value.toFixed(decimals);
      }

      function getSelectedJoist() {
        const series = joistSeriesEl.value;
        const id = joistDesignationEl.value;
        const list = joistDatabase[series] || [];
        return list.find((j) => j.id === id) || null;
      }

      function runCheck() {
        const system = unitSystemEl.value;
        const span = parseFloat(spanEl.value);
        const spacing = parseFloat(spacingEl.value);
        const D = parseFloat(deadLoadEl.value);
        const L = parseFloat(liveLoadEl.value);
        const combo = loadComboEl.value;
        const Einput = parseFloat(modulusEEl.value);
        const joist = getSelectedJoist();

        if (
          !span ||
          !spacing ||
          !joist ||
          isNaN(D) ||
          isNaN(L) ||
          isNaN(Einput)
        ) {
          overallStatus.textContent =
            "Please enter valid span, spacing, loads, and select a joist.";
          overallStatus.className = "mt-4 text-sm font-semibold text-red-700";
          return;
        }

        // Convert to consistent internal units
        let L, s, Darea, Larea, E, unitLine, unitMoment, unitShear, unitDefl;
        if (system === "us") {
          // ft, psf, psi
          L = span;
          s = spacing;
          Darea = D; // psf
          Larea = L; // psf
          E = Einput; // psi
          unitLine = "plf";
          unitMoment = "kip-ft";
          unitShear = "kips";
          unitDefl = "in";

          // Convert psf to plf: w = q * s
          const wService = (Darea + Larea) * s; // plf
          let wFactored;
          if (combo === "lrfd") {
            wFactored = (1.2 * Darea + 1.6 * Larea) * s;
          } else {
            wFactored = (Darea + Larea) * s;
          }

          // Strength: use wFactored
          const Mu = (wFactored * L * L) / 8.0 / 1000.0; // kip-ft (plf*ft^2/8 /1000)
          const Vu = (wFactored * L) / 2.0 / 1000.0; // kips

          // Deflection: use service load
          const wServiceKipPerFt = wService / 1000.0; // kip/ft
          // Convert to kip/in for EI in kip-in^2
          const wServiceKipPerIn = wServiceKipPerFt / 12.0;
          const L_in = L * 12.0;
          // Convert EI from table (approx) to kip-in^2
          const EI = joist.EI; // already in kip-ft^2 approx
          const EI_kip_in2 = EI * 12.0 * 12.0; // kip-ft^2 -> kip-in^2

          const delta_in =
            (5 * wServiceKipPerIn * Math.pow(L_in, 4)) / (384 * EI_kip_in2);

          // Allowable deflection
          let deltaAllow_in;
          if (deflectionLimitTypeEl.value === "custom") {
            deltaAllow_in = parseFloat(customDeflectionEl.value) || 0;
          } else {
            const ratio = parseInt(
              deflectionLimitTypeEl.value.replace("L", ""),
              10
            );
            deltaAllow_in = L_in / ratio;
          }

          // Utilization
          const momentUtil = Mu / joist.Mn;
          const shearUtil = Vu / joist.Vn;
          const deflUtil = deltaAllow_in > 0 ? delta_in / deltaAllow_in : NaN;

          // Update displays
          lineLoadDisplay.textContent =
            formatNumber(wFactored, 1) + " " + unitLine;
          serviceLineLoadDisplay.textContent =
            formatNumber(wService, 1) + " " + unitLine;
          MuDisplay.textContent = formatNumber(Mu, 2) + " " + unitMoment;
          MnDisplay.textContent = formatNumber(joist.Mn, 2) + " " + unitMoment;
          VuDisplay.textContent = formatNumber(Vu, 2) + " " + unitShear;
          VnDisplay.textContent = formatNumber(joist.Vn, 2) + " " + unitShear;
          momentUtilizationDisplay.textContent =
            formatNumber(momentUtil * 100, 1) + " %";
          shearUtilizationDisplay.textContent =
            formatNumber(shearUtil * 100, 1) + " %";
          deflectionDisplay.textContent =
            formatNumber(delta_in, 3) + " " + unitDefl;
          deflectionAllowableDisplay.textContent =
            formatNumber(deltaAllow_in, 3) + " " + unitDefl;
          deflectionRatioDisplay.textContent =
            "L/" + formatNumber(L_in / delta_in, 0);

          // Status badges
          const strengthOK = momentUtil <= 1.0 && shearUtil <= 1.0;
          const deflectionOK = deflUtil <= 1.0;

          strengthStatus.textContent = strengthOK ? "OK" : "NG";
          strengthStatus.className =
            "badge px-2 " +
            (strengthOK
              ? "bg-green-100 text-green-700 border border-green-300"
              : "bg-red-100 text-red-700 border border-red-300");

          deflectionStatus.textContent = deflectionOK ? "OK" : "NG";
          deflectionStatus.className =
            "badge px-2 " +
            (deflectionOK
              ? "bg-green-100 text-green-700 border border-green-300"
              : "bg-red-100 text-red-700 border border-red-300");

          if (strengthOK && deflectionOK) {
            overallStatus.textContent =
              "This joist passes both strength and deflection checks for the specified loads and span (preliminary only).";
            overallStatus.className =
              "mt-4 text-sm font-semibold text-green-700";
          } else if (!strengthOK && !deflectionOK) {
            overallStatus.textContent =
              "This joist fails both strength and deflection checks. Increase joist size, reduce spacing, or reduce loads.";
            overallStatus.className = "mt-4 text-sm font-semibold text-red-700";
          } else if (!strengthOK) {
            overallStatus.textContent =
              "Strength check fails (moment and/or shear). Try a heavier joist or closer spacing.";
            overallStatus.className = "mt-4 text-sm font-semibold text-red-700";
          } else {
            overallStatus.textContent =
              "Deflection check fails. Consider a stiffer joist, shorter span, or tighter spacing.";
            overallStatus.className =
              "mt-4 text-sm font-semibold text-amber-700";
          }

          resultsSummary.textContent =
            "Results are based on a simply supported joist with uniform load and approximate SJI-style properties.";
        } else {
          // Metric system: m, kN/m², MPa
          L = span;
          s = spacing;
          Darea = D; // kN/m²
          Larea = L; // kN/m²
          E = Einput; // MPa = N/mm²

          unitLine = "kN/m";
          unitMoment = "kN·m";
          unitShear = "kN";
          unitDefl = "mm";

          const wService = (Darea + Larea) * s; // kN/m
          let wFactored;
          if (combo === "lrfd") {
            wFactored = (1.2 * Darea + 1.6 * Larea) * s;
          } else {
            wFactored = (Darea + Larea) * s;
          }

          const Mu = (wFactored * L * L) / 8.0; // kN·m
          const Vu = (wFactored * L) / 2.0; // kN

          // Deflection: convert to N/mm
          const wServiceNPerM = wService * 1000.0; // kN/m -> N/m
          const wServiceNPerMm = wServiceNPerM / 1000.0; // N/mm
          const L_mm = L * 1000.0;
          // Approximate EI: joist.EI is in kip-ft^2; convert to N·mm² roughly
          const EI_kip_ft2 = joist.EI;
          const kipToN = 4448.22;
          const ftToMm = 304.8;
          const EI_N_mm2 = EI_kip_ft2 * kipToN * Math.pow(ftToMm, 2);

          const delta_mm =
            (5 * wServiceNPerMm * Math.pow(L_mm, 4)) / (384 * EI_N_mm2);

          let deltaAllow_mm;
          if (deflectionLimitTypeEl.value === "custom") {
            deltaAllow_mm = parseFloat(customDeflectionEl.value) || 0;
          } else {
            const ratio = parseInt(
              deflectionLimitTypeEl.value.replace("L", ""),
              10
            );
            deltaAllow_mm = L_mm / ratio;
          }

          const momentUtil = Mu / (joist.Mn * 1.356); // rough kip-ft -> kN·m
          const shearUtil = Vu / (joist.Vn * 4.448); // rough kips -> kN
          const deflUtil = deltaAllow_mm > 0 ? delta_mm / deltaAllow_mm : NaN;

          lineLoadDisplay.textContent =
            formatNumber(wFactored, 2) + " " + unitLine;
          serviceLineLoadDisplay.textContent =
            formatNumber(wService, 2) + " " + unitLine;
          MuDisplay.textContent = formatNumber(Mu, 2) + " " + unitMoment;
          MnDisplay.textContent =
            formatNumber(joist.Mn * 1.356, 2) + " " + unitMoment;
          VuDisplay.textContent = formatNumber(Vu, 2) + " " + unitShear;
          VnDisplay.textContent =
            formatNumber(joist.Vn * 4.448, 2) + " " + unitShear;
          momentUtilizationDisplay.textContent =
            formatNumber(momentUtil * 100, 1) + " %";
          shearUtilizationDisplay.textContent =
            formatNumber(shearUtil * 100, 1) + " %";
          deflectionDisplay.textContent =
            formatNumber(delta_mm, 2) + " " + unitDefl;
          deflectionAllowableDisplay.textContent =
            formatNumber(deltaAllow_mm, 2) + " " + unitDefl;
          deflectionRatioDisplay.textContent =
            "L/" + formatNumber(L_mm / delta_mm, 0);

          const strengthOK = momentUtil <= 1.0 && shearUtil <= 1.0;
          const deflectionOK = deflUtil <= 1.0;

          strengthStatus.textContent = strengthOK ? "OK" : "NG";
          strengthStatus.className =
            "badge px-2 " +
            (strengthOK
              ? "bg-green-100 text-green-700 border border-green-300"
              : "bg-red-100 text-red-700 border border-red-300");

          deflectionStatus.textContent = deflectionOK ? "OK" : "NG";
          deflectionStatus.className =
            "badge px-2 " +
            (deflectionOK
              ? "bg-green-100 text-green-700 border border-green-300"
              : "bg-red-100 text-red-700 border border-red-300");

          if (strengthOK && deflectionOK) {
            overallStatus.textContent =
              "This joist passes both strength and deflection checks for the specified loads and span (preliminary only).";
            overallStatus.className =
              "mt-4 text-sm font-semibold text-green-700";
          } else if (!strengthOK && !deflectionOK) {
            overallStatus.textContent =
              "This joist fails both strength and deflection checks. Increase joist size, reduce spacing, or reduce loads.";
            overallStatus.className = "mt-4 text-sm font-semibold text-red-700";
          } else if (!strengthOK) {
            overallStatus.textContent =
              "Strength check fails (moment and/or shear). Try a heavier joist or closer spacing.";
            overallStatus.className = "mt-4 text-sm font-semibold text-red-700";
          } else {
            overallStatus.textContent =
              "Deflection check fails. Consider a stiffer joist, shorter span, or tighter spacing.";
            overallStatus.className =
              "mt-4 text-sm font-semibold text-amber-700";
          }

          resultsSummary.textContent =
            "Results are based on a simply supported joist with uniform load and approximate SJI-style properties.";
        }
      }

      function resetForm() {
        unitSystemEl.value = "us";
        updateUnits();
        joistSeriesEl.value = "K";
        populateJoistOptions();
        spanEl.value = 40;
        spacingEl.value = 5;
        deadLoadEl.value = 20;
        liveLoadEl.value = 40;
        loadComboEl.value = "service";
        deflectionLimitTypeEl.value = "L360";
        customDeflectionWrapper.classList.add("hidden");
        customDeflectionEl.value = 1.0;
        modulusEEl.value = 29000000;

        lineLoadDisplay.textContent = "–";
        serviceLineLoadDisplay.textContent = "–";
        MuDisplay.textContent = "–";
        MnDisplay.textContent = "–";
        VuDisplay.textContent = "–";
        VnDisplay.textContent = "–";
        momentUtilizationDisplay.textContent = "–";
        shearUtilizationDisplay.textContent = "–";
        deflectionDisplay.textContent = "–";
        deflectionAllowableDisplay.textContent = "–";
        deflectionRatioDisplay.textContent = "–";
        strengthStatus.textContent = "–";
        strengthStatus.className = "badge border text-gray-600";
        deflectionStatus.textContent = "–";
        deflectionStatus.className = "badge border text-gray-600";
        overallStatus.textContent = "";
        resultsSummary.textContent =
          "Enter span, spacing, loads, and select a joist designation, then click “Check Joist Design”.";
      }

      unitSystemEl.addEventListener("change", updateUnits);
      joistSeriesEl.addEventListener("change", populateJoistOptions);
      deflectionLimitTypeEl.addEventListener("change", () => {
        if (deflectionLimitTypeEl.value === "custom") {
          customDeflectionWrapper.classList.remove("hidden");
        } else {
          customDeflectionWrapper.classList.add("hidden");
        }
      });

      document.getElementById("run-check").addEventListener("click", runCheck);
      document
        .getElementById("reset-form")
        .addEventListener("click", resetForm);

      // Initialize
      updateUnits();
      populateJoistOptions();
      resetForm();
    </script>

