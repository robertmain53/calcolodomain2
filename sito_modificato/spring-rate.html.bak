    <script>
      // Tab handling
      const tabButtons = document.querySelectorAll("[data-tab]");
      const tabPanels = document.querySelectorAll(".tab-panel");
      tabButtons.forEach((btn) => {
        btn.addEventListener("click", () => {
          const target = btn.getAttribute("data-tab");
          tabButtons.forEach((b) => b.classList.remove("tab-active"));
          btn.classList.add("tab-active");
          tabPanels.forEach((p) => {
            p.id === target
              ? p.classList.remove("hidden")
              : p.classList.add("hidden");
          });
        });
      });

      // Helper functions
      function toNumber(val) {
        const n = parseFloat(val);
        return isNaN(n) ? null : n;
      }
      function round(value, decimals = 3) {
        if (!isFinite(value)) return "–";
        const factor = Math.pow(10, decimals);
        return (Math.round(value * factor) / factor).toString();
      }

      // Shear modulus custom input toggle
      const matSelect = document.getElementById("dim-material");
      const customGWrapper = document.getElementById("dim-customG-wrapper");
      matSelect.addEventListener("change", () => {
        if (matSelect.value === "custom") {
          customGWrapper.classList.remove("hidden");
        } else {
          customGWrapper.classList.add("hidden");
        }
      });

      // Dimension-based spring rate
      document.getElementById("dim-calc").addEventListener("click", () => {
        const d = toNumber(document.getElementById("dim-wire").value);
        const D = toNumber(document.getElementById("dim-mean").value);
        const n = toNumber(document.getElementById("dim-coils").value);
        const dUnit = document.getElementById("dim-wire-unit").value;
        const DUnit = document.getElementById("dim-mean-unit").value;

        if (d == null || D == null || n == null || d <= 0 || D <= 0 || n <= 0) {
          alert(
            "Please enter positive values for wire diameter, mean diameter, and active coils."
          );
          return;
        }

        // Convert diameters to meters
        let d_m = d;
        let D_m = D;
        if (dUnit === "mm") d_m = d / 1000;
        if (dUnit === "in") d_m = d * 0.0254;
        if (DUnit === "mm") D_m = D / 1000;
        if (DUnit === "in") D_m = D * 0.0254;

        // Shear modulus G in Pa
        let G;
        if (matSelect.value === "custom") {
          const Gval = toNumber(document.getElementById("dim-customG").value);
          const Gunit = document.getElementById("dim-customG-unit").value;
          if (Gval == null || Gval <= 0) {
            alert("Please enter a positive custom shear modulus G.");
            return;
          }
          G = Gunit === "gpa" ? Gval * 1e9 : Gval * 6894.757; // psi to Pa
        } else {
          const Ggpa = parseFloat(matSelect.value); // in GPa
          G = Ggpa * 1e9;
        }

        // k in N/m
        const k_N_per_m = (G * Math.pow(d_m, 4)) / (8 * n * Math.pow(D_m, 3));
        const k_N_per_mm = k_N_per_m / 1000;
        const k_lb_per_in = k_N_per_m * 0.0057101471627692; // 1 N/m = 0.00571 lb/in
        const k_kg_per_mm = k_N_per_m / (9.80665 * 1000); // N/m to kg/mm

        document.getElementById("dim-k-main").textContent = round(
          k_N_per_mm,
          3
        );
        document.getElementById("dim-k-lbin").textContent = round(
          k_lb_per_in,
          3
        );
        document.getElementById("dim-k-kgmm").textContent = round(
          k_kg_per_mm,
          4
        );
        document.getElementById("dim-results").classList.remove("hidden");
      });

      document.getElementById("dim-reset").addEventListener("click", () => {
        ["dim-wire", "dim-mean", "dim-coils", "dim-customG"].forEach((id) => {
          const el = document.getElementById(id);
          if (el) el.value = "";
        });
        document.getElementById("dim-wire-unit").value = "mm";
        document.getElementById("dim-mean-unit").value = "mm";
        matSelect.value = "79";
        customGWrapper.classList.add("hidden");
        document.getElementById("dim-results").classList.add("hidden");
      });

      // Load/deflection spring rate
      document.getElementById("def-calc").addEventListener("click", () => {
        const F = toNumber(document.getElementById("def-force").value);
        const def = toNumber(document.getElementById("def-defl").value);
        const Funit = document.getElementById("def-force-unit").value;
        const defUnit = document.getElementById("def-defl-unit").value;

        if (F == null || def == null || F <= 0 || def <= 0) {
          alert("Please enter positive values for force and deflection.");
          return;
        }

        // Convert to N and m
        let F_N = F;
        if (Funit === "kgf") F_N = F * 9.80665;
        if (Funit === "lbf") F_N = F * 4.4482216153;

        let def_m = def;
        if (defUnit === "mm") def_m = def / 1000;
        if (defUnit === "in") def_m = def * 0.0254;

        const k_N_per_m = F_N / def_m;
        const k_N_per_mm = k_N_per_m / 1000;
        const k_lb_per_in = k_N_per_m * 0.0057101471627692;
        const k_kg_per_mm = k_N_per_m / (9.80665 * 1000);

        document.getElementById("def-k-main").textContent = round(
          k_N_per_mm,
          3
        );
        document.getElementById("def-k-lbin").textContent = round(
          k_lb_per_in,
          3
        );
        document.getElementById("def-k-kgmm").textContent = round(
          k_kg_per_mm,
          4
        );
        document.getElementById("def-results").classList.remove("hidden");
      });

      document.getElementById("def-reset").addEventListener("click", () => {
        document.getElementById("def-force").value = "";
        document.getElementById("def-defl").value = "";
        document.getElementById("def-force-unit").value = "N";
        document.getElementById("def-defl-unit").value = "mm";
        document.getElementById("def-results").classList.add("hidden");
      });

      // Wheel rate
      document.getElementById("wheel-calc").addEventListener("click", () => {
        const ks = toNumber(document.getElementById("wheel-ks").value);
        const mr = toNumber(document.getElementById("wheel-mr").value);
        const angle =
          toNumber(document.getElementById("wheel-angle").value) || 0;
        const ksUnit = document.getElementById("wheel-ks-unit").value;
        const angleUnit = document.getElementById("wheel-angle-unit").value;

        if (ks == null || ks <= 0 || mr == null || mr <= 0) {
          alert(
            "Please enter positive values for spring rate and motion ratio."
          );
          return;
        }

        // Convert ks to N/mm
        let ks_N_per_mm;
        if (ksUnit === "N/mm") {
          ks_N_per_mm = ks;
        } else if (ksUnit === "lb/in") {
          ks_N_per_mm = ks * 0.175126835; // 1 lb/in ≈ 0.17513 N/mm
        } else if (ksUnit === "kg/mm") {
          ks_N_per_mm = ks * 9.80665; // 1 kg/mm ≈ 9.80665 N/mm
        }

        const theta_rad = angleUnit === "deg" ? (angle * Math.PI) / 180 : angle;
        const factor = mr * mr * Math.pow(Math.cos(theta_rad), 2);
        const kw_N_per_mm = ks_N_per_mm * factor;
        const kw_N_per_m = kw_N_per_mm * 1000;
        const kw_lb_per_in = kw_N_per_m * 0.0057101471627692;
        const kw_kg_per_mm = kw_N_per_m / (9.80665 * 1000);

        document.getElementById("wheel-k-main").textContent = round(
          kw_N_per_mm,
          3
        );
        document.getElementById("wheel-k-lbin").textContent = round(
          kw_lb_per_in,
          3
        );
        document.getElementById("wheel-k-kgmm").textContent = round(
          kw_kg_per_mm,
          4
        );
        document.getElementById("wheel-results").classList.remove("hidden");
      });

      document.getElementById("wheel-reset").addEventListener("click", () => {
        document.getElementById("wheel-ks").value = "";
        document.getElementById("wheel-mr").value = "";
        document.getElementById("wheel-angle").value = "";
        document.getElementById("wheel-ks-unit").value = "N/mm";
        document.getElementById("wheel-angle-unit").value = "deg";
        document.getElementById("wheel-results").classList.add("hidden");
      });

      // Unit converter
      document.getElementById("conv-calc").addEventListener("click", () => {
        const val = toNumber(document.getElementById("conv-value").value);
        const unit = document.getElementById("conv-unit").value;
        if (val == null) {
          alert("Please enter a numeric value to convert.");
          return;
        }

        // Convert input to N/mm
        let k_N_per_mm;
        if (unit === "N/mm") {
          k_N_per_mm = val;
        } else if (unit === "lb/in") {
          k_N_per_mm = val * 0.175126835;
        } else if (unit === "kg/mm") {
          k_N_per_mm = val * 9.80665;
        }

        const k_N_per_m = k_N_per_mm * 1000;
        const k_lb_per_in = k_N_per_m * 0.0057101471627692;
        const k_kg_per_mm = k_N_per_m / (9.80665 * 1000);

        document.getElementById("conv-nmm").textContent = round(k_N_per_mm, 4);
        document.getElementById("conv-lbin").textContent = round(
          k_lb_per_in,
          4
        );
        document.getElementById("conv-kgmm").textContent = round(
          k_kg_per_mm,
          5
        );
        document.getElementById("conv-results").classList.remove("hidden");
      });

      document.getElementById("conv-reset").addEventListener("click", () => {
        document.getElementById("conv-value").value = "";
        document.getElementById("conv-unit").value = "N/mm";
        document.getElementById("conv-results").classList.add("hidden");
      });

      document
        .querySelectorAll("#faq-container .faq-question")
        .forEach((btn) => {
          btn.addEventListener("click", () => {
            const answer = btn.parentElement.querySelector(".faq-answer");
            const icon = btn.querySelector("span:last-child");
            const isHidden = answer.classList.contains("hidden");
            document
              .querySelectorAll("#faq-container .faq-answer")
              .forEach((a) => a.classList.add("hidden"));
            document
              .querySelectorAll("#faq-container .faq-question span:last-child")
              .forEach((i) => (i.textContent = "+"));
            if (isHidden) {
              answer.classList.remove("hidden");
              icon.textContent = "−";
            }
          });
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
