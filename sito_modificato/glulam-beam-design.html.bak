    <script>
      // Utility helpers
      function roundTo(val, decimals) {
        const p = Math.pow(10, decimals);
        return Math.round(val * p) / p;
      }

      function setBadge(el, ratio) {
        el.className = "text-xs px-2 py-1 rounded-full";
        if (ratio <= 0.95) {
          el.classList.add("result-badge-ok");
          el.textContent = "OK (" + (ratio * 100).toFixed(0) + "%)";
        } else if (ratio <= 1.05) {
          el.classList.add("result-badge-warn");
          el.textContent = "Near limit (" + (ratio * 100).toFixed(0) + "%)";
        } else {
          el.classList.add("result-badge-ng");
          el.textContent = "Overstressed (" + (ratio * 100).toFixed(0) + "%)";
        }
      }

      // Grade presets (US units)
      const gradePresets = {
        "24F-V4": { Fb: 2400, Fv: 165, E: 1.8 },
        "24F-V8": { Fb: 2400, Fv: 190, E: 2.0 },
        "20F-V4": { Fb: 2000, Fv: 165, E: 1.6 },
      };

      document.addEventListener("DOMContentLoaded", () => {
        const unitSystemEl = document.getElementById("unitSystem");
        const presetSizeEl = document.getElementById("presetSize");
        const gradeEl = document.getElementById("grade");
        const loadTypeEl = document.getElementById("loadType");
        const pointLoadGroup = document.getElementById("pointLoadGroup");
        const calcBtn = document.getElementById("calcBtn");
        const resetBtn = document.getElementById("resetBtn");
        const errorMsg = document.getElementById("errorMsg");
        const resultsContainer = document.getElementById("resultsContainer");
        const noResultsHint = document.getElementById("noResultsHint");

        function updateUnitLabels() {
          const us = unitSystemEl.value === "us";
          document.getElementById("spanUnitLabel").textContent = us
            ? "(ft)"
            : "(m)";
          document.getElementById("bUnitLabel").textContent = us
            ? "(in)"
            : "(mm)";
          document.getElementById("dUnitLabel").textContent = us
            ? "(in)"
            : "(mm)";
          document.getElementById("fbUnitLabel").textContent = us
            ? "(psi)"
            : "(MPa)";
          document.getElementById("fvUnitLabel").textContent = us
            ? "(psi)"
            : "(MPa)";
          document.getElementById("eUnitLabel").innerHTML = us
            ? "(10<sup>6</sup> psi)"
            : "(GPa)";
          document.getElementById("wDeadUnitLabel").textContent = us
            ? "(plf)"
            : "(kN/m)";
          document.getElementById("wLiveUnitLabel").textContent = us
            ? "(plf)"
            : "(kN/m)";
          document.getElementById("pUnitLabel").textContent = us
            ? "(lb)"
            : "(kN)";
          document.getElementById("aUnitLabel").textContent = us
            ? "(ft)"
            : "(m)";
          document.getElementById("bearingUnitLabel").textContent = us
            ? "(in)"
            : "(mm)";
          document.getElementById("fcPerpUnitLabel").textContent = us
            ? "(psi)"
            : "(MPa)";
        }

        function applyPresetSize() {
          const val = presetSizeEl.value;
          if (val === "custom") return;
          const parts = val.split("x");
          const b = parseFloat(parts[0]);
          const d = parseFloat(parts[1]);
          const us = unitSystemEl.value === "us";
          if (us) {
            document.getElementById("b").value = b;
            document.getElementById("d").value = d;
          } else {
            // convert inches to mm
            document.getElementById("b").value = roundTo(b * 25.4, 1);
            document.getElementById("d").value = roundTo(d * 25.4, 1);
          }
        }

        function applyGradePreset() {
          const grade = gradeEl.value;
          const us = unitSystemEl.value === "us";
          if (gradePresets[grade]) {
            const g = gradePresets[grade];
            if (us) {
              document.getElementById("Fb").value = g.Fb;
              document.getElementById("Fv").value = g.Fv;
              document.getElementById("E").value = g.E;
            } else {
              // convert psi to MPa (1 psi ≈ 0.00689476 MPa), E in 10^6 psi to GPa
              document.getElementById("Fb").value = roundTo(
                g.Fb * 0.00689476,
                1
              );
              document.getElementById("Fv").value = roundTo(
                g.Fv * 0.00689476,
                2
              );
              document.getElementById("E").value = roundTo(g.E * 6.89476, 2);
            }
          }
        }

        function updateLoadType() {
          pointLoadGroup.classList.toggle("hidden", loadTypeEl.value === "udl");
        }

        unitSystemEl.addEventListener("change", () => {
          updateUnitLabels();
          applyPresetSize();
          applyGradePreset();
          resultsContainer.classList.add("hidden");
          noResultsHint.classList.remove("hidden");
          errorMsg.textContent = "";
        });

        presetSizeEl.addEventListener("change", () => {
          applyPresetSize();
          resultsContainer.classList.add("hidden");
          noResultsHint.classList.remove("hidden");
        });

        gradeEl.addEventListener("change", () => {
          if (gradeEl.value !== "custom") {
            applyGradePreset();
          }
          resultsContainer.classList.add("hidden");
          noResultsHint.classList.remove("hidden");
        });

        loadTypeEl.addEventListener("change", () => {
          updateLoadType();
          resultsContainer.classList.add("hidden");
          noResultsHint.classList.remove("hidden");
        });

        resetBtn.addEventListener("click", () => {
          unitSystemEl.value = "us";
          document.getElementById("span").value = 20;
          presetSizeEl.value = "5.125x18.0";
          applyPresetSize();
          gradeEl.value = "24F-V4";
          applyGradePreset();
          document.getElementById("deflLimitLive").value = "360";
          document.getElementById("deflLimitTotal").value = "240";
          document.getElementById("loadType").value = "udl";
          document.getElementById("wDead").value = 150;
          document.getElementById("wLive").value = 300;
          document.getElementById("P").value = 2000;
          document.getElementById("a").value = 10;
          document.getElementById("bearingLength").value = 3.5;
          document.getElementById("FcPerp").value = 625;
          updateUnitLabels();
          updateLoadType();
          resultsContainer.classList.add("hidden");
          noResultsHint.classList.remove("hidden");
          errorMsg.textContent = "";
        });

        calcBtn.addEventListener("click", () => {
          errorMsg.textContent = "";
          const us = unitSystemEl.value === "us";

          let L = parseFloat(document.getElementById("span").value);
          let b = parseFloat(document.getElementById("b").value);
          let d = parseFloat(document.getElementById("d").value);
          let Fb = parseFloat(document.getElementById("Fb").value);
          let Fv = parseFloat(document.getElementById("Fv").value);
          let E = parseFloat(document.getElementById("E").value);
          const deflLimitLive = parseFloat(
            document.getElementById("deflLimitLive").value
          );
          const deflLimitTotal = parseFloat(
            document.getElementById("deflLimitTotal").value
          );
          let wD = parseFloat(document.getElementById("wDead").value) || 0;
          let wL = parseFloat(document.getElementById("wLive").value) || 0;
          let P = parseFloat(document.getElementById("P").value) || 0;
          let a = parseFloat(document.getElementById("a").value) || 0;
          let bearingLength = parseFloat(
            document.getElementById("bearingLength").value
          );
          let FcPerp = parseFloat(document.getElementById("FcPerp").value);

          if (
            [L, b, d, Fb, Fv, E, bearingLength, FcPerp].some(
              (v) => !isFinite(v) || v <= 0
            )
          ) {
            errorMsg.textContent =
              "Please enter positive numeric values for span, section, material, and bearing.";
            resultsContainer.classList.add("hidden");
            noResultsHint.classList.remove("hidden");
            return;
          }

          const loadType = loadTypeEl.value;
          if (
            (loadType === "udl+point" || loadType === "point") &&
            (!isFinite(P) || P <= 0)
          ) {
            errorMsg.textContent = "Please enter a positive point load.";
            resultsContainer.classList.add("hidden");
            noResultsHint.classList.remove("hidden");
            return;
          }

          if (!us) {
            // Convert metric inputs to US internal units for formulas
            // L: m -> ft, b,d,bearing: mm -> in, Fb,Fv,FcPerp: MPa -> psi, E: GPa -> 10^6 psi
            L = L * 3.28084;
            b = b * 0.0393701;
            d = d * 0.0393701;
            bearingLength = bearingLength * 0.0393701;
            Fb = Fb / 0.00689476;
            Fv = Fv / 0.00689476;
            FcPerp = FcPerp / 0.00689476;
            E = E / 6.89476; // GPa to 10^6 psi
            wD = wD * 68.5218; // kN/m -> plf (approx)
            wL = wL * 68.5218;
            P = P * 224.809; // kN -> lb
            a = a * 3.28084;
          }

          // Section properties (in, in^3, in^4)
          const S = (b * d * d) / 6.0;
          const I = (b * Math.pow(d, 3)) / 12.0;
          const Epsi = E * 1e6; // convert 10^6 psi to psi

          // Loads in plf and lb
          const wTotal = wD + wL;

          // Moments and shears from UDL
          const M_udl = (wTotal * Math.pow(L, 2)) / 8.0; // lb-ft
          const V_udl = (wTotal * L) / 2.0; // lb

          // Point load effects
          let M_point = 0;
          let V_point = 0;
          let R_point = 0;
          if (loadType === "udl+point" || loadType === "point") {
            if (a <= 0 || a >= L) {
              errorMsg.textContent =
                "Point load position must be between 0 and span.";
              resultsContainer.classList.add("hidden");
              noResultsHint.classList.remove("hidden");
              return;
            }
            const RA = (P * (L - a)) / L;
            const RB = (P * a) / L;
            M_point = RA * a; // lb-ft
            V_point = Math.max(Math.abs(RA), Math.abs(RB));
            R_point = Math.max(Math.abs(RA), Math.abs(RB));
          }

          const Mmax = M_udl + M_point; // lb-ft
          const Vmax = V_udl + V_point; // lb

          // Reactions (use worst of UDL + point)
          const R_udl = (wTotal * L) / 2.0;
          const R = R_udl + R_point;

          // Convert Mmax to lb-in for stress
          const Mmax_in = Mmax * 12.0;

          // Stresses
          const fb = Mmax_in / S; // psi
          const fv = (1.5 * Vmax) / (b * d); // psi

          // Bearing stress
          const Ab = b * bearingLength; // in^2
          const fc_perp = R / Ab; // psi

          // Deflection (use wD and wL separately)
          const wD_in = wD / 12.0; // plf -> pli
          const wL_in = wL / 12.0;
          const L_in = L * 12.0; // ft -> in

          const deltaD = (5 * wD_in * Math.pow(L_in, 4)) / (384 * Epsi * I);
          const deltaL = (5 * wL_in * Math.pow(L_in, 4)) / (384 * Epsi * I);

          // Approximate point-load deflection at midspan if present (assume at midspan)
          let deltaPoint = 0;
          if (loadType === "udl+point" || loadType === "point") {
            const P_in = P; // lb
            deltaPoint = (P_in * Math.pow(L_in, 3)) / (48 * Epsi * I);
            // Assume point load is live load
            deltaL += deltaPoint;
          }

          const deltaTotal = deltaD + deltaL;

          // Deflection limits
          const deltaLimitLive = L_in / deflLimitLive;
          const deltaLimitTotal = L_in / deflLimitTotal;

          // Utilization ratios
          const utilBending = fb / Fb;
          const utilShear = fv / Fv;
          const utilBearing = fc_perp / FcPerp;
          const utilDeflLive = deltaL / deltaLimitLive;
          const utilDeflTotal = deltaTotal / deltaLimitTotal;

          // Determine governing condition
          const utilMap = {
            Bending: utilBending,
            Shear: utilShear,
            Bearing: utilBearing,
            "Live deflection": utilDeflLive,
            "Total deflection": utilDeflTotal,
          };
          let governing = "";
          let maxUtil = -Infinity;
          Object.keys(utilMap).forEach((k) => {
            if (utilMap[k] > maxUtil) {
              maxUtil = utilMap[k];
              governing = k;
            }
          });

          // Populate results
          const spanDisplay = us
            ? `${roundTo(L, 2)} ft`
            : `${roundTo(L * 0.3048, 3)} m`;
          const bDisplay = us
            ? `${roundTo(b, 2)} in`
            : `${roundTo(b * 25.4, 1)} mm`;
          const dDisplay = us
            ? `${roundTo(d, 2)} in`
            : `${roundTo(d * 25.4, 1)} mm`;
          const SDisplay = us
            ? `${roundTo(S, 1)} in³`
            : `${roundTo(S * Math.pow(25.4, 3), 0)} mm³`;
          const IDisplay = us
            ? `${roundTo(I, 1)} in⁴`
            : `${roundTo(I * Math.pow(25.4, 4), 0)} mm⁴`;

          document.getElementById("resSpan").textContent = spanDisplay;
          document.getElementById("resB").textContent = bDisplay;
          document.getElementById("resD").textContent = dDisplay;
          document.getElementById("resS").textContent = SDisplay;
          document.getElementById("resI").textContent = IDisplay;

          const MDisplay = us
            ? `${roundTo(Mmax, 1)} ft·lb (${roundTo(Mmax / 12.0, 1)} kip·in)`
            : `${roundTo(Mmax * 1.35582, 1)} kN·m`;
          const VDisplay = us
            ? `${roundTo(Vmax, 0)} lb (${roundTo(Vmax / 1000.0, 2)} kip)`
            : `${roundTo(Vmax * 0.00444822, 2)} kN`;
          const RDisplay = us
            ? `${roundTo(R, 0)} lb (${roundTo(R / 1000.0, 2)} kip)`
            : `${roundTo(R * 0.00444822, 2)} kN`;

          document.getElementById("resMmax").textContent = MDisplay;
          document.getElementById("resVmax").textContent = VDisplay;
          document.getElementById("resR").textContent = RDisplay;

          const deflLiveDisplay = us
            ? `${roundTo(deltaL, 2)} in`
            : `${roundTo(deltaL * 25.4, 1)} mm`;
          const deflTotalDisplay = us
            ? `${roundTo(deltaTotal, 2)} in`
            : `${roundTo(deltaTotal * 25.4, 1)} mm`;

          document.getElementById("resDeflLive").textContent = deflLiveDisplay;
          document.getElementById("resDeflTotal").textContent =
            deflTotalDisplay;

          document.getElementById("resLimitLive").textContent =
            deflLimitLive.toString();
          document.getElementById("resLimitTotal").textContent =
            deflLimitTotal.toString();

          document.getElementById("utilBending").textContent =
            (utilBending * 100).toFixed(1) + " %";
          document.getElementById("utilShear").textContent =
            (utilShear * 100).toFixed(1) + " %";
          document.getElementById("utilBearing").textContent =
            (utilBearing * 100).toFixed(1) + " %";
          document.getElementById("utilDeflLive").textContent =
            (utilDeflLive * 100).toFixed(1) + " %";
          document.getElementById("utilDeflTotal").textContent =
            (utilDeflTotal * 100).toFixed(1) + " %";

          setBadge(document.getElementById("badgeBending"), utilBending);
          setBadge(document.getElementById("badgeShear"), utilShear);
          setBadge(document.getElementById("badgeBearing"), utilBearing);
          setBadge(document.getElementById("badgeDeflLive"), utilDeflLive);
          setBadge(document.getElementById("badgeDeflTotal"), utilDeflTotal);

          document.getElementById("governing").textContent = `${governing} (${(
            maxUtil * 100
          ).toFixed(1)} % utilization)`;

          resultsContainer.classList.remove("hidden");
          noResultsHint.classList.add("hidden");
        });

        // Initial setup
        updateUnitLabels();
        applyPresetSize();
        applyGradePreset();
        updateLoadType();
      });
    </script>

