    <script>
      (function () {
        const modeButtons = document.querySelectorAll(".toggle-mode-btn");
        const inputTypeButtons = document.querySelectorAll(
          ".toggle-input-type-btn"
        );
        const lineTypeButtons = document.querySelectorAll(
          ".toggle-line-type-btn"
        );

        const groupAmps = document.getElementById("group-amps");
        const groupOhms = document.getElementById("group-ohms");
        const groupPf = document.getElementById("group-pf");
        const groupLineType = document.getElementById("group-line-type");

        const voltageEl = document.getElementById("voltage");
        const voltageUnitEl = document.getElementById("voltage-unit");
        const currentEl = document.getElementById("current");
        const currentUnitEl = document.getElementById("current-unit");
        const resistanceEl = document.getElementById("resistance");
        const resistanceUnitEl = document.getElementById("resistance-unit");
        const pfEl = document.getElementById("power-factor");

        const btnCalc = document.getElementById("btn-calc");
        const btnClear = document.getElementById("btn-clear");

        const resultCard = document.getElementById("result-card");
        const resultMain = document.getElementById("result-main");
        const resultDetail = document.getElementById("result-detail");
        const errorMsg = document.getElementById("error-msg");

        let currentMode = "dc"; // 'dc', 'ac1', 'ac3'
        let inputType = "amps"; // 'amps', 'ohms'
        let lineType = "ll"; // 'll', 'ln' for 3-phase

        function setMode(mode) {
          currentMode = mode;
          modeButtons.forEach((btn) => {
            btn.classList.toggle(
              "toggle-btn-active",
              btn.dataset.mode === mode
            );
          });

          const isAC = mode === "ac1" || mode === "ac3";
          groupPf.classList.toggle("hidden", !isAC);
          groupLineType.classList.toggle("hidden", mode !== "ac3");

          if (!isAC) {
            pfEl.value = "";
          }
          clearResult();
        }

        function setInputType(type) {
          inputType = type;
          inputTypeButtons.forEach((btn) => {
            const isAmps = btn.id === "btn-use-amps";
            btn.classList.toggle(
              "toggle-btn-active",
              (type === "amps" && isAmps) || (type === "ohms" && !isAmps)
            );
          });
          groupAmps.classList.toggle("hidden", type !== "amps");
          groupOhms.classList.toggle("hidden", type !== "ohms");
          clearResult();
        }

        function setLineType(type) {
          lineType = type;
          lineTypeButtons.forEach((btn) => {
            btn.classList.toggle(
              "toggle-btn-active",
              btn.dataset.linetype === type
            );
          });
          clearResult();
        }

        function clearResult() {
          resultCard.classList.add("hidden");
          errorMsg.classList.add("hidden");
          errorMsg.textContent = "";
        }

        function convertVoltageToVolts(value, unit) {
          const v = parseFloat(value);
          if (isNaN(v)) return NaN;
          if (unit === "kV") return v * 1000;
          return v;
        }

        function convertCurrentToAmps(value, unit) {
          const i = parseFloat(value);
          if (isNaN(i)) return NaN;
          if (unit === "mA") return i / 1000;
          return i;
        }

        function convertResistanceToOhms(value, unit) {
          const r = parseFloat(value);
          if (isNaN(r)) return NaN;
          if (unit === "kOhm") return r * 1000;
          return r;
        }

        function formatNumber(num, decimals = 2) {
          if (!isFinite(num)) return "";
          if (num === 0) return "0";
          if (Math.abs(num) >= 10000 || Math.abs(num) < 0.01) {
            return num.toExponential(decimals);
          }
          return num.toLocaleString(undefined, {
            maximumFractionDigits: decimals,
          });
        }

        function calculate() {
          clearResult();

          const V = convertVoltageToVolts(voltageEl.value, voltageUnitEl.value);
          if (!isFinite(V) || V <= 0) {
            errorMsg.textContent = "Please enter a valid positive voltage.";
            errorMsg.classList.remove("hidden");
            return;
          }

          let P = NaN;
          let detail = "";

          if (inputType === "amps") {
            const I = convertCurrentToAmps(
              currentEl.value,
              currentUnitEl.value
            );
            if (!isFinite(I) || I <= 0) {
              errorMsg.textContent = "Please enter a valid positive current.";
              errorMsg.classList.remove("hidden");
              return;
            }

            if (currentMode === "dc") {
              P = V * I;
              detail = `DC: P = V × I = ${formatNumber(V)} V × ${formatNumber(
                I
              )} A`;
            } else {
              let PF = parseFloat(pfEl.value);
              if (!isFinite(PF) || PF <= 0 || PF > 1) {
                errorMsg.textContent =
                  "Please enter a valid power factor between 0 and 1.";
                errorMsg.classList.remove("hidden");
                return;
              }

              if (currentMode === "ac1") {
                P = V * I * PF;
                detail = `AC single‑phase: P = V × I × PF = ${formatNumber(
                  V
                )} V × ${formatNumber(I)} A × ${formatNumber(PF, 3)}`;
              } else if (currentMode === "ac3") {
                if (lineType === "ll") {
                  P = Math.sqrt(3) * V * I * PF;
                  detail = `AC three‑phase (line‑to‑line): P = √3 × V × I × PF = 1.732 × ${formatNumber(
                    V
                  )} V × ${formatNumber(I)} A × ${formatNumber(PF, 3)}`;
                } else {
                  P = 3 * V * I * PF;
                  detail = `AC three‑phase (line‑to‑neutral): P = 3 × V × I × PF = 3 × ${formatNumber(
                    V
                  )} V × ${formatNumber(I)} A × ${formatNumber(PF, 3)}`;
                }
              }
            }
          } else {
            const R = convertResistanceToOhms(
              resistanceEl.value,
              resistanceUnitEl.value
            );
            if (!isFinite(R) || R <= 0) {
              errorMsg.textContent =
                "Please enter a valid positive resistance.";
              errorMsg.classList.remove("hidden");
              return;
            }
            P = (V * V) / R;
            detail = `Using Ohm’s law: P = V² ÷ R = (${formatNumber(
              V
            )} V)² ÷ ${formatNumber(R)} Ω`;
          }

          if (!isFinite(P) || P <= 0) {
            errorMsg.textContent =
              "Unable to compute power with the given values.";
            errorMsg.classList.remove("hidden");
            return;
          }

          resultMain.textContent = `${formatNumber(P)} W`;
          resultDetail.textContent = detail;
          resultCard.classList.remove("hidden");
        }

        function clearAll() {
          voltageEl.value = "";
          currentEl.value = "";
          resistanceEl.value = "";
          pfEl.value = "";
          clearResult();
        }

        modeButtons.forEach((btn) => {
          btn.addEventListener("click", () => setMode(btn.dataset.mode));
        });

        inputTypeButtons.forEach((btn) => {
          btn.addEventListener("click", () => {
            const type = btn.id === "btn-use-amps" ? "amps" : "ohms";
            setInputType(type);
          });
        });

        lineTypeButtons.forEach((btn) => {
          btn.addEventListener("click", () =>
            setLineType(btn.dataset.linetype)
          );
        });

        btnCalc.addEventListener("click", calculate);
        btnClear.addEventListener("click", clearAll);

        [voltageEl, currentEl, resistanceEl, pfEl].forEach((el) => {
          el.addEventListener("keydown", (e) => {
            if (e.key === "Enter") {
              e.preventDefault();
              calculate();
            }
          });
        });

        setMode("dc");
        setInputType("amps");
        setLineType("ll");
      })();
    </script>

