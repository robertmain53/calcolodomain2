    <script>
      // Tab switching
      const tabEt = document.getElementById("tab-et");
      const tabHp = document.getElementById("tab-hp");
      const panelEt = document.getElementById("panel-et");
      const panelHp = document.getElementById("panel-hp");

      function activateTab(tab) {
        if (tab === "et") {
          tabEt.classList.add("tab-active");
          tabHp.classList.remove("tab-active");
          panelEt.classList.remove("hidden");
          panelHp.classList.add("hidden");
        } else {
          tabHp.classList.add("tab-active");
          tabEt.classList.remove("tab-active");
          panelHp.classList.remove("hidden");
          panelEt.classList.add("hidden");
        }
      }

      tabEt.addEventListener("click", () => activateTab("et"));
      tabHp.addEventListener("click", () => activateTab("hp"));

      // Helper conversions
      function kgToLb(kg) {
        return kg * 2.2046226218;
      }
      function lbToKg(lb) {
        return lb / 2.2046226218;
      }
      function kwToHp(kw) {
        return kw * 1.34102209;
      }
      function hpToKw(hp) {
        return hp / 1.34102209;
      }
      function mphToKph(mph) {
        return mph * 1.609344;
      }

      // ET & trap speed calculation
      const calcEtBtn = document.getElementById("calcEtBtn");
      const resetEtBtn = document.getElementById("resetEtBtn");
      const etResults = document.getElementById("etResults");
      const etTimeSpan = document.getElementById("etTime");
      const trapMphSpan = document.getElementById("trapSpeedMph");
      const trapKphSpan = document.getElementById("trapSpeedKph");

      calcEtBtn.addEventListener("click", () => {
        const weightVal = parseFloat(document.getElementById("weight").value);
        const weightUnit = document.getElementById("weightUnit").value;
        const powerVal = parseFloat(document.getElementById("power").value);
        const powerUnit = document.getElementById("powerUnit").value;
        const drivetrainLoss =
          parseFloat(document.getElementById("drivetrainLoss").value) || 0;
        const tractionFactor = parseFloat(
          document.getElementById("tractionFactor").value
        );

        if (!weightVal || weightVal <= 0 || !powerVal || powerVal <= 0) {
          alert("Please enter valid weight and power values.");
          return;
        }

        // Convert to lb and hp
        let weightLb = weightUnit === "kg" ? kgToLb(weightVal) : weightVal;
        let hp = powerUnit === "kw" ? kwToHp(powerVal) : powerVal;

        // If drivetrain loss provided, convert crank hp to wheel hp
        if (drivetrainLoss > 0 && drivetrainLoss < 60) {
          hp = hp * (1 - drivetrainLoss / 100);
        }

        // ET formula
        const baseEt = 5.825 * Math.cbrt(weightLb / hp);
        const adjEt = baseEt * tractionFactor;

        // Trap speed formula
        const trapMph = 234 * Math.cbrt(hp / weightLb);
        const trapKph = mphToKph(trapMph);

        etTimeSpan.textContent = adjEt.toFixed(2);
        trapMphSpan.textContent = trapMph.toFixed(1);
        trapKphSpan.textContent = trapKph.toFixed(1);
        etResults.classList.remove("hidden");

        lastEtResult = {
          weightLb: weightLb,
          hp: hp,
          et: adjEt,
          trapMph: trapMph,
        };
      });

      resetEtBtn.addEventListener("click", () => {
        document.getElementById("weight").value = "";
        document.getElementById("power").value = "";
        document.getElementById("drivetrainLoss").value = "";
        document.getElementById("tractionFactor").value = "1.05";
        etResults.classList.add("hidden");
      });

      // HP from ET calculation
      const calcHpBtn = document.getElementById("calcHpBtn");
      const resetHpBtn = document.getElementById("resetHpBtn");
      const hpResults = document.getElementById("hpResults");
      const estHpSpan = document.getElementById("estHp");
      const estKwSpan = document.getElementById("estKw");
      const pwRatioSpan = document.getElementById("pwRatio");

      calcHpBtn.addEventListener("click", () => {
        const weightVal = parseFloat(document.getElementById("hpWeight").value);
        const weightUnit = document.getElementById("hpWeightUnit").value;
        const etVal = parseFloat(document.getElementById("hpEt").value);

        if (!weightVal || weightVal <= 0 || !etVal || etVal <= 0) {
          alert("Please enter valid weight and ET values.");
          return;
        }

        let weightLb = weightUnit === "kg" ? kgToLb(weightVal) : weightVal;

        // HP from ET formula
        const hp = weightLb / Math.pow(etVal / 5.825, 3);
        const kw = hpToKw(hp);

        // Power-to-weight (hp per metric ton)
        const weightTon =
          (weightUnit === "kg" ? weightVal : lbToKg(weightLb)) / 1000;
        const pwRatio = hp / weightTon;

        estHpSpan.textContent = hp.toFixed(1);
        estKwSpan.textContent = kw.toFixed(1);
        pwRatioSpan.textContent = pwRatio.toFixed(1);
        hpResults.classList.remove("hidden");

        lastEtResult = {
          weightLb: weightLb,
          hp: hp,
          et: etVal,
          trapMph: NaN,
        };
      });

      resetHpBtn.addEventListener("click", () => {
        document.getElementById("hpWeight").value = "";
        document.getElementById("hpEt").value = "";
        hpResults.classList.add("hidden");
      });

      // Run comparison table
      let runs = [];
      let lastEtResult = null;
      const runsTableBody = document.getElementById("runsTableBody");
      const addRunBtn = document.getElementById("addRunBtn");
      const noRunsRow = document.getElementById("noRunsRow");

      function renderRuns() {
        runsTableBody.innerHTML = "";
        if (runs.length === 0) {
          runsTableBody.appendChild(noRunsRow);
          noRunsRow.classList.remove("hidden");
          return;
        }
        noRunsRow.classList.add("hidden");

        runs.forEach((run, index) => {
          const tr = document.createElement("tr");
          tr.className = index % 2 === 0 ? "bg-white" : "bg-gray-50";

          tr.innerHTML = `
        <td class="border px-2 py-1">${index + 1}</td>
        <td class="border px-2 py-1">${run.weightLb.toFixed(0)}</td>
        <td class="border px-2 py-1">${run.hp.toFixed(0)}</td>
        <td class="border px-2 py-1">${run.et.toFixed(2)}</td>
        <td class="border px-2 py-1">${
          isNaN(run.trapMph) ? "—" : run.trapMph.toFixed(1)
        }</td>
        <td class="border px-2 py-1">
          <input type="text" class="w-full border border-gray-200 rounded px-1 py-0.5 text-xs note-input" value="${
            run.note || ""
          }" />
        </td>
        <td class="border px-2 py-1 text-right">
          <button type="button" data-index="${index}" class="text-red-500 text-xs hover:underline remove-run">Remove</button>
        </td>
      `;
          runsTableBody.appendChild(tr);
        });

        // Attach events for remove buttons and note inputs
        runsTableBody.querySelectorAll(".remove-run").forEach((btn) => {
          btn.addEventListener("click", (e) => {
            const idx = parseInt(e.target.getAttribute("data-index"), 10);
            runs.splice(idx, 1);
            renderRuns();
          });
        });

        runsTableBody.querySelectorAll(".note-input").forEach((input, idx) => {
          input.addEventListener("change", (e) => {
            runs[idx].note = e.target.value;
          });
        });
      }

      addRunBtn.addEventListener("click", () => {
        if (!lastEtResult) {
          alert(
            "Calculate a quarter mile estimate first, then add it to the table."
          );
          return;
        }
        runs.push({ ...lastEtResult, note: "" });
        renderRuns();
      });

      document
        .querySelectorAll("#faq-container .faq-question")
        .forEach((btn) => {
          btn.addEventListener("click", () => {
            const content = btn.nextElementSibling;
            const icon = btn.querySelector("span:last-child");
            const isHidden = content.classList.contains("hidden");
            document
              .querySelectorAll("#faq-container .faq-question + div")
              .forEach((div) => div.classList.add("hidden"));
            document
              .querySelectorAll("#faq-container .faq-question span:last-child")
              .forEach((i) => (i.textContent = "+"));
            if (isHidden) {
              content.classList.remove("hidden");
              icon.textContent = "−";
            } else {
              content.classList.add("hidden");
              icon.textContent = "+";
            }
          });
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
