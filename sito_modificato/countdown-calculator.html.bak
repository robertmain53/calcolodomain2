    <script>
      // Countdown logic
      (function () {
        const dateInput = document.getElementById("cd-date");
        const timeInput = document.getElementById("cd-time");
        const tzSelect = document.getElementById("cd-timezone");
        const titleInput = document.getElementById("cd-title");
        const loopCheckbox = document.getElementById("cd-loop");
        const fullscreenCheckbox = document.getElementById("cd-fullscreen");
        const soundEnabled = document.getElementById("cd-sound-enabled");
        const soundVolume = document.getElementById("cd-sound-volume");

        const daysEl = document.getElementById("cd-days");
        const hoursEl = document.getElementById("cd-hours");
        const minutesEl = document.getElementById("cd-minutes");
        const secondsEl = document.getElementById("cd-seconds");
        const statusEl = document.getElementById("cd-status");
        const errorEl = document.getElementById("cd-error");
        const titleDisplay = document.getElementById("countdown-title-display");

        const startBtn = document.getElementById("cd-start");
        const pauseBtn = document.getElementById("cd-pause");
        const resetBtn = document.getElementById("cd-reset");
        const shareBtn = document.getElementById("cd-share");
        const shareOutput = document.getElementById("cd-share-output");

        const detailedSection = document.getElementById("cd-detailed-section");
        const totalDaysEl = document.getElementById("cd-total-days");
        const totalHoursEl = document.getElementById("cd-total-hours");
        const totalMinutesEl = document.getElementById("cd-total-minutes");
        const totalSecondsEl = document.getElementById("cd-total-seconds");

        const presetButtons = document.querySelectorAll(".preset-btn");
        const styleRadios = document.querySelectorAll('input[name="cd-style"]');

        let timerId = null;
        let targetTimeMs = null;
        let originalDurationMs = null;
        let paused = false;
        let pauseRemainingMs = null;
        let audioCtx, beepOsc;

        function initDefaultsFromNow() {
          const now = new Date();
          const in5min = new Date(now.getTime() + 5 * 60 * 1000);
          dateInput.valueAsDate = in5min;
          timeInput.value = in5min.toTimeString().slice(0, 8);
        }

        function parseTarget() {
          const dateVal = dateInput.value;
          const timeVal = timeInput.value || "00:00:00";
          if (!dateVal) return null;

          const [year, month, day] = dateVal.split("-").map(Number);
          const [hh, mm, ss] = timeVal.split(":").map(Number);

          if (tzSelect.value === "utc") {
            return Date.UTC(year, month - 1, day, hh || 0, mm || 0, ss || 0);
          } else {
            const d = new Date(year, month - 1, day, hh || 0, mm || 0, ss || 0);
            return d.getTime();
          }
        }

        function formatTwo(n) {
          return n.toString().padStart(2, "0");
        }

        function updateDisplay(diffMs) {
          const totalSeconds = Math.max(0, Math.floor(diffMs / 1000));
          const days = Math.floor(totalSeconds / 86400);
          const hours = Math.floor((totalSeconds % 86400) / 3600);
          const minutes = Math.floor((totalSeconds % 3600) / 60);
          const seconds = totalSeconds % 60;

          daysEl.textContent = days;
          hoursEl.textContent = formatTwo(hours);
          minutesEl.textContent = formatTwo(minutes);
          secondsEl.textContent = formatTwo(seconds);

          if (getSelectedStyle() === "detailed") {
            detailedSection.classList.remove("hidden");
            totalDaysEl.textContent = days;
            totalHoursEl.textContent = Math.floor(totalSeconds / 3600);
            totalMinutesEl.textContent = Math.floor(totalSeconds / 60);
            totalSecondsEl.textContent = totalSeconds;
          } else {
            detailedSection.classList.add("hidden");
          }
        }

        function getSelectedStyle() {
          for (const r of styleRadios) {
            if (r.checked) return r.value;
          }
          return "compact";
        }

        function setStatus(message, isError = false) {
          statusEl.textContent = message;
          if (isError) {
            statusEl.classList.add("text-red-600");
          } else {
            statusEl.classList.remove("text-red-600");
          }
        }

        function showError(msg) {
          errorEl.textContent = msg;
          errorEl.classList.remove("hidden");
          setStatus("Fix the error above and try again.", true);
        }

        function clearError() {
          errorEl.textContent = "";
          errorEl.classList.add("hidden");
          setStatus("Ready. Click “Start” to begin the countdown.");
        }

        function startBeep() {
          if (!soundEnabled.checked) return;
          try {
            if (!audioCtx)
              audioCtx = new (window.AudioContext ||
                window.webkitAudioContext)();
            beepOsc = audioCtx.createOscillator();
            const gainNode = audioCtx.createGain();
            beepOsc.frequency.value = 880;
            gainNode.gain.value = soundVolume.value;
            beepOsc.connect(gainNode).connect(audioCtx.destination);
            beepOsc.start();
            setTimeout(() => {
              if (beepOsc) {
                beepOsc.stop();
                beepOsc.disconnect();
                beepOsc = null;
              }
            }, 800);
          } catch (e) {
            // ignore audio errors
          }
        }

        function requestFullscreenIfNeeded() {
          if (!fullscreenCheckbox.checked) return;
          const el = document.documentElement;
          if (el.requestFullscreen) el.requestFullscreen();
          else if (el.webkitRequestFullscreen) el.webkitRequestFullscreen();
          else if (el.msRequestFullscreen) el.msRequestFullscreen();
        }

        function startTimer() {
          clearError();
          const nowMs = Date.now();
          const parsedTarget = parseTarget();
          if (!parsedTarget) {
            showError("Please select a valid target date.");
            return;
          }
          targetTimeMs = parsedTarget;
          const diffMs = targetTimeMs - nowMs;
          if (diffMs <= 0) {
            showError("The target date and time must be in the future.");
            return;
          }

          originalDurationMs = diffMs;
          paused = false;
          pauseRemainingMs = null;

          if (titleInput.value.trim()) {
            titleDisplay.textContent = titleInput.value.trim();
            titleDisplay.classList.remove("hidden");
          } else {
            titleDisplay.classList.add("hidden");
          }

          requestFullscreenIfNeeded();

          if (timerId) clearInterval(timerId);
          timerId = setInterval(tick, 250);
          updateDisplay(diffMs);
          setStatus("Countdown running…");
          startBtn.disabled = true;
          pauseBtn.disabled = false;
          resetBtn.disabled = false;
        }

        function tick() {
          if (paused || targetTimeMs == null) return;
          const nowMs = Date.now();
          let diffMs = targetTimeMs - nowMs;

          if (diffMs <= 0) {
            updateDisplay(0);
            setStatus("Finished.");
            startBeep();

            if (loopCheckbox.checked && originalDurationMs != null) {
              targetTimeMs = nowMs + originalDurationMs;
              setStatus("Looping countdown…");
              return;
            }

            clearInterval(timerId);
            timerId = null;
            startBtn.disabled = false;
            pauseBtn.disabled = true;
            return;
          }
          updateDisplay(diffMs);
        }

        function pauseTimer() {
          if (!timerId || paused) return;
          const nowMs = Date.now();
          pauseRemainingMs = Math.max(0, targetTimeMs - nowMs);
          paused = true;
          clearInterval(timerId);
          timerId = null;
          setStatus("Paused.");
          pauseBtn.textContent = "Resume";
        }

        function resumeTimer() {
          if (paused && pauseRemainingMs != null && pauseRemainingMs > 0) {
            const nowMs = Date.now();
            targetTimeMs = nowMs + pauseRemainingMs;
            paused = false;
            pauseRemainingMs = null;
            timerId = setInterval(tick, 250);
            setStatus("Countdown running…");
            pauseBtn.textContent = "Pause";
          }
        }

        function resetTimer() {
          if (timerId) clearInterval(timerId);
          timerId = null;
          targetTimeMs = null;
          originalDurationMs = null;
          paused = false;
          pauseRemainingMs = null;
          pauseBtn.textContent = "Pause";
          startBtn.disabled = false;
          pauseBtn.disabled = true;
          resetBtn.disabled = true;
          updateDisplay(0);
          titleDisplay.classList.add("hidden");
          setStatus("Reset. Set a target date and click “Start” to begin.");
        }

        function applyPreset(code) {
          const now = new Date();
          let addMs = 0;
          switch (code) {
            case "5m":
              addMs = 5 * 60 * 1000;
              break;
            case "15m":
              addMs = 15 * 60 * 1000;
              break;
            case "1h":
              addMs = 60 * 60 * 1000;
              break;
            case "24h":
              addMs = 24 * 60 * 60 * 1000;
              break;
            case "7d":
              addMs = 7 * 24 * 60 * 60 * 1000;
              break;
          }
          const target = new Date(now.getTime() + addMs);
          dateInput.valueAsDate = target;
          timeInput.value = target.toTimeString().slice(0, 8);
          clearError();
          setStatus("Preset applied. Click “Start” to begin the countdown.");
        }

        function buildShareUrl() {
          const params = new URLSearchParams();
          if (dateInput.value) params.set("date", dateInput.value);
          if (timeInput.value) params.set("time", timeInput.value);
          params.set("tz", tzSelect.value);
          if (titleInput.value.trim())
            params.set("title", titleInput.value.trim());
          params.set("loop", loopCheckbox.checked ? "1" : "0");
          params.set("style", getSelectedStyle());
          params.set("sound", soundEnabled.checked ? "1" : "0");
          const url = `${window.location.origin}${
            window.location.pathname
          }?${params.toString()}`;
          return url;
        }

        function handleShare() {
          const url = buildShareUrl();
          shareOutput.textContent = url;
          shareOutput.classList.remove("hidden");
          if (navigator.clipboard && navigator.clipboard.writeText) {
            navigator.clipboard
              .writeText(url)
              .then(() => {
                setStatus("Share link copied to clipboard.");
              })
              .catch(() => {
                setStatus("Share link generated below. Copy it manually.");
              });
          } else {
            setStatus("Share link generated below. Copy it manually.");
          }
        }

        function applyFromQuery() {
          const params = new URLSearchParams(window.location.search);
          const date = params.get("date");
          const time = params.get("time");
          const tz = params.get("tz");
          const title = params.get("title");
          const loop = params.get("loop");
          const style = params.get("style");
          const sound = params.get("sound");

          if (date) dateInput.value = date;
          if (time) timeInput.value = time;
          if (tz && (tz === "local" || tz === "utc")) tzSelect.value = tz;
          if (title) titleInput.value = title;
          if (loop === "1") loopCheckbox.checked = true;
          if (sound === "1") soundEnabled.checked = true;
          if (style === "detailed" || style === "compact") {
            styleRadios.forEach((r) => {
              r.checked = r.value === style;
            });
          }

          if (date || time || title || loop || style || sound) {
            setStatus(
              "Settings loaded from link. Click “Start” to begin the countdown."
            );
          }
        }

        // Event listeners
        startBtn.addEventListener("click", startTimer);
        pauseBtn.addEventListener("click", () => {
          if (!paused) pauseTimer();
          else resumeTimer();
        });
        resetBtn.addEventListener("click", resetTimer);
        shareBtn.addEventListener("click", handleShare);

        presetButtons.forEach((btn) => {
          btn.addEventListener("click", () => {
            const code = btn.getAttribute("data-preset");
            applyPreset(code);
          });
        });

        styleRadios.forEach((r) => {
          r.addEventListener("change", () => {
            if (targetTimeMs) {
              const nowMs = Date.now();
              const diffMs = Math.max(0, targetTimeMs - nowMs);
              updateDisplay(diffMs);
            }
          });
        });

        // Initialize
        initDefaultsFromNow();
        applyFromQuery();
        updateDisplay(0);
      })();
    </script>

