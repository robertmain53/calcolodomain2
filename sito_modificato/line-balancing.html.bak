    <script>
      // Data model
      const tasks = [];

      const taskIdInput = document.getElementById("task-id");
      const taskTimeInput = document.getElementById("task-time");
      const taskTimeUnit = document.getElementById("task-time-unit");
      const taskPreInput = document.getElementById("task-pre");
      const taskTableBody = document.getElementById("task-table-body");

      const availableTimeInput = document.getElementById("available-time");
      const availableTimeUnit = document.getElementById("available-time-unit");
      const demandInput = document.getElementById("demand");
      const numStationsInput = document.getElementById("num-stations");
      const heuristicSelect = document.getElementById("heuristic");
      const stationLimitInput = document.getElementById("station-limit");

      const kpiTakt = document.getElementById("kpi-takt");
      const kpiTaktUnit = document.getElementById("kpi-takt-unit");
      const kpiEff = document.getElementById("kpi-eff");
      const kpiDelay = document.getElementById("kpi-delay");

      const resultsSummary = document.getElementById("results-summary");
      const resultsAssignments = document.getElementById("results-assignments");
      const resTotalTime = document.getElementById("res-total-time");
      const resStationsUsed = document.getElementById("res-stations-used");
      const resCycleTime = document.getElementById("res-cycle-time");
      const resTheoretical = document.getElementById("res-theoretical");
      const resComment = document.getElementById("res-comment");
      const assignmentsBody = document.getElementById("assignments-body");

      function renderTasks() {
        taskTableBody.innerHTML = "";
        if (tasks.length === 0) {
          const tr = document.createElement("tr");
          tr.innerHTML =
            '<td colspan="4" class="px-3 py-3 text-center text-gray-400 text-sm">No tasks defined yet.</td>';
          taskTableBody.appendChild(tr);
          return;
        }
        tasks.forEach((t, idx) => {
          const tr = document.createElement("tr");
          tr.className = idx % 2 === 0 ? "bg-white" : "bg-gray-50";
          tr.innerHTML = `
        <td class="px-3 py-2 font-medium">${t.id}</td>
        <td class="px-3 py-2 text-right">${t.timeSec.toFixed(1)}</td>
        <td class="px-3 py-2 text-gray-700">${
          t.predecessors.length
            ? t.predecessors.join(", ")
            : '<span class="text-gray-400">—</span>'
        }</td>
        <td class="px-3 py-2 text-center">
          <button type="button" data-index="${idx}" class="text-xs text-red-600 hover:underline remove-task-btn">Remove</button>
        </td>
      `;
          taskTableBody.appendChild(tr);
        });
        document.querySelectorAll(".remove-task-btn").forEach((btn) => {
          btn.addEventListener("click", (e) => {
            const idx = parseInt(e.target.getAttribute("data-index"), 10);
            if (!isNaN(idx)) {
              tasks.splice(idx, 1);
              renderTasks();
            }
          });
        });
      }

      function addTask() {
        const id = (taskIdInput.value || "").trim();
        const timeVal = parseFloat(taskTimeInput.value);
        if (!id) {
          alert("Please enter a task ID.");
          return;
        }
        if (!isFinite(timeVal) || timeVal <= 0) {
          alert("Please enter a positive task time.");
          return;
        }
        if (tasks.some((t) => t.id === id)) {
          alert("Task ID must be unique.");
          return;
        }
        let timeSec = timeVal;
        if (taskTimeUnit.value === "min") timeSec = timeVal * 60;

        const preRaw = (taskPreInput.value || "").trim();
        const predecessors = preRaw
          ? preRaw
              .split(",")
              .map((s) => s.trim())
              .filter(Boolean)
          : [];

        tasks.push({ id, timeSec, predecessors });
        taskIdInput.value = "";
        taskTimeInput.value = "";
        taskPreInput.value = "";
        renderTasks();
      }

      document.getElementById("add-task").addEventListener("click", addTask);
      taskTimeInput.addEventListener("keydown", (e) => {
        if (e.key === "Enter") {
          e.preventDefault();
          addTask();
        }
      });

      document.getElementById("clear-tasks").addEventListener("click", () => {
        if (tasks.length === 0) return;
        if (confirm("Clear all tasks?")) {
          tasks.splice(0, tasks.length);
          renderTasks();
        }
      });

      document.getElementById("load-example").addEventListener("click", () => {
        tasks.splice(0, tasks.length);
        const example = [
          { id: "A", timeSec: 30, predecessors: [] },
          { id: "B", timeSec: 20, predecessors: ["A"] },
          { id: "C", timeSec: 25, predecessors: ["A"] },
          { id: "D", timeSec: 15, predecessors: ["B"] },
          { id: "E", timeSec: 35, predecessors: ["B"] },
          { id: "F", timeSec: 10, predecessors: ["C"] },
          { id: "G", timeSec: 20, predecessors: ["D", "F"] },
        ];
        example.forEach((t) => tasks.push(t));
        renderTasks();
      });

      function computeTaktTimeSec() {
        const avail = parseFloat(availableTimeInput.value);
        const demand = parseFloat(demandInput.value);
        if (!isFinite(avail) || avail <= 0 || !isFinite(demand) || demand <= 0)
          return null;
        let availSec = avail;
        if (availableTimeUnit.value === "min") availSec = avail * 60;
        else if (availableTimeUnit.value === "hr") availSec = avail * 3600;
        return availSec / demand;
      }

      function updateKpis(totalTaskTimeSec, stationsUsed, cycleTimeSec) {
        const taktSec = computeTaktTimeSec();
        if (!taktSec) {
          kpiTakt.textContent = "–";
          kpiEff.textContent = "–";
          kpiDelay.textContent = "–";
          return;
        }
        // Show takt in seconds or minutes depending on magnitude
        if (taktSec >= 60) {
          const taktMin = taktSec / 60;
          kpiTakt.textContent = taktMin.toFixed(2);
          kpiTaktUnit.textContent = "min/unit";
        } else {
          kpiTakt.textContent = taktSec.toFixed(1);
          kpiTaktUnit.textContent = "sec/unit";
        }

        if (!totalTaskTimeSec || !stationsUsed || !cycleTimeSec) {
          kpiEff.textContent = "–";
          kpiDelay.textContent = "–";
          return;
        }

        const efficiency = totalTaskTimeSec / (stationsUsed * cycleTimeSec);
        const delay = 1 - efficiency;
        kpiEff.textContent = (efficiency * 100).toFixed(1) + "%";
        kpiDelay.textContent = (delay * 100).toFixed(1) + "%";
      }

      function allPredecessorsAssigned(task, assignedSet) {
        return task.predecessors.every((p) => assignedSet.has(p));
      }

      function computePositionalWeights(taskList) {
        const map = new Map();
        taskList.forEach((t) => map.set(t.id, t));
        const succ = new Map();
        taskList.forEach((t) => succ.set(t.id, []));
        taskList.forEach((t) => {
          t.predecessors.forEach((p) => {
            if (succ.has(p)) succ.get(p).push(t.id);
          });
        });

        const memo = new Map();
        function weight(id) {
          if (memo.has(id)) return memo.get(id);
          const t = map.get(id);
          if (!t) return 0;
          let w = t.timeSec;
          const sList = succ.get(id) || [];
          sList.forEach((sid) => {
            w += weight(sid);
          });
          memo.set(id, w);
          return w;
        }

        const weights = {};
        taskList.forEach((t) => {
          weights[t.id] = weight(t.id);
        });
        return weights;
      }

      function balanceLine() {
        if (tasks.length === 0) {
          alert("Please add at least one task.");
          return;
        }
        const taktSec = computeTaktTimeSec();
        if (!taktSec) {
          alert("Please enter valid available time and demand.");
          return;
        }
        const numStations = parseInt(numStationsInput.value, 10);
        if (!isFinite(numStations) || numStations <= 0) {
          alert("Please enter a valid number of workstations.");
          return;
        }
        const stationLimit = parseInt(stationLimitInput.value, 10) || 999;

        const totalTaskTimeSec = tasks.reduce((sum, t) => sum + t.timeSec, 0);
        const theoreticalMinStations = totalTaskTimeSec / taktSec;

        // Copy tasks
        const remaining = tasks.map((t) => ({
          ...t,
          predecessors: [...t.predecessors],
        }));
        const assignedSet = new Set();
        const stations = [];
        const heuristic = heuristicSelect.value;

        // Precompute positional weights if needed
        let weights = null;
        if (heuristic === "rpw") {
          weights = computePositionalWeights(remaining);
        }

        for (let s = 0; s < numStations; s++) {
          let stationTime = 0;
          const stationTasks = [];
          let addedSomething = true;

          while (addedSomething && stationTasks.length < stationLimit) {
            addedSomething = false;
            const eligible = remaining.filter((t) =>
              allPredecessorsAssigned(t, assignedSet)
            );

            if (eligible.length === 0) break;

            // Sort eligible according to heuristic
            if (heuristic === "largest") {
              eligible.sort((a, b) => b.timeSec - a.timeSec);
            } else if (heuristic === "shortest") {
              eligible.sort((a, b) => a.timeSec - b.timeSec);
            } else if (heuristic === "rpw") {
              eligible.sort(
                (a, b) => (weights[b.id] || 0) - (weights[a.id] || 0)
              );
            }

            let chosenIndex = -1;
            for (let i = 0; i < eligible.length; i++) {
              const t = eligible[i];
              if (stationTime + t.timeSec <= taktSec * 1.0001) {
                // small tolerance
                chosenIndex = remaining.findIndex((r) => r.id === t.id);
                break;
              }
            }

            if (chosenIndex === -1) break;

            const chosen = remaining.splice(chosenIndex, 1)[0];
            stationTasks.push(chosen);
            stationTime += chosen.timeSec;
            assignedSet.add(chosen.id);
            addedSomething = true;
          }

          if (stationTasks.length > 0) {
            stations.push({
              index: s + 1,
              tasks: stationTasks,
              loadSec: stationTime,
            });
          }
          if (remaining.length === 0) break;
        }

        if (remaining.length > 0) {
          alert(
            "Not all tasks could be assigned within the given number of stations and takt time. Consider increasing stations or takt time."
          );
        }

        if (stations.length === 0) {
          alert("No feasible assignment found.");
          return;
        }

        const cycleTimeSec = Math.max(...stations.map((st) => st.loadSec));
        const stationsUsed = stations.length;

        // Update summary
        resultsSummary.classList.remove("hidden");
        resultsAssignments.classList.remove("hidden");

        resTotalTime.textContent = totalTaskTimeSec.toFixed(1) + " s";
        resStationsUsed.textContent = stationsUsed.toString();
        resCycleTime.textContent = cycleTimeSec.toFixed(1) + " s";
        resTheoretical.textContent = theoreticalMinStations.toFixed(2);

        const efficiency = totalTaskTimeSec / (stationsUsed * cycleTimeSec);
        const delay = 1 - efficiency;

        let comment = `Line efficiency is ${(efficiency * 100).toFixed(
          1
        )}% with a balance delay of ${(delay * 100).toFixed(1)}%. `;
        if (cycleTimeSec > taktSec * 1.0001) {
          comment +=
            "The bottleneck station is slower than takt time, so the line cannot meet the required demand. Consider adding a station, redistributing tasks, or reducing task times.";
        } else {
          comment +=
            "Cycle time is within takt time, so the line can meet the required demand under these assumptions.";
        }
        resComment.textContent = comment;

        // Render assignments
        assignmentsBody.innerHTML = "";
        const bottleneckLoad = cycleTimeSec;
        stations.forEach((st) => {
          const util = st.loadSec / bottleneckLoad;
          const idle = bottleneckLoad - st.loadSec;
          const isBottleneck = Math.abs(st.loadSec - bottleneckLoad) < 1e-6;
          const tr = document.createElement("tr");
          tr.className = "border-t";
          tr.innerHTML = `
        <td class="px-3 py-2 font-semibold">
          Station ${st.index}
          ${
            isBottleneck
              ? '<span class="badge bg-red-100 text-red-700 ml-1">Bottleneck</span>'
              : ""
          }
        </td>
        <td class="px-3 py-2">
          ${st.tasks
            .map(
              (t) =>
                `<span class="inline-flex items-center px-2 py-0.5 mr-1 mb-1 rounded-full bg-slate-100 text-xs">${
                  t.id
                } (${t.timeSec.toFixed(1)}s)</span>`
            )
            .join("")}
        </td>
        <td class="px-3 py-2 text-right">${st.loadSec.toFixed(1)}</td>
        <td class="px-3 py-2 text-right">${(util * 100).toFixed(1)}%</td>
        <td class="px-3 py-2 text-right">${idle.toFixed(1)}</td>
      `;
          assignmentsBody.appendChild(tr);
        });

        updateKpis(totalTaskTimeSec, stationsUsed, cycleTimeSec);
      }

      document
        .getElementById("run-balance")
        .addEventListener("click", balanceLine);

      // Update KPIs when demand/time change (without rebalancing)
      ["input", "change"].forEach((evt) => {
        availableTimeInput.addEventListener(evt, () => updateKpis());
        availableTimeUnit.addEventListener(evt, () => updateKpis());
        demandInput.addEventListener(evt, () => updateKpis());
      });

      // Initial render
      renderTasks();
      updateKpis();
    </script>

