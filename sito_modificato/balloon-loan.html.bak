    <script>
      // Utility functions
      function fmtCurrency(value) {
        if (!isFinite(value)) return "$0.00";
        return value.toLocaleString("en-US", {
          style: "currency",
          currency: "USD",
        });
      }
      function fmtPercent(value) {
        if (!isFinite(value)) return "0%";
        return value.toFixed(1).replace(/\.0$/, "") + "%";
      }

      function calculateBalloonLoan() {
        const loanAmount = parseFloat(
          document.getElementById("loanAmount").value
        );
        const annualRate =
          parseFloat(document.getElementById("interestRate").value) / 100;
        const balloonTermVal = parseFloat(
          document.getElementById("balloonTerm").value
        );
        const balloonTermUnit =
          document.getElementById("balloonTermUnit").value;
        const amortTermVal = parseFloat(
          document.getElementById("amortizationTerm").value
        );
        const amortTermUnit = document.getElementById(
          "amortizationTermUnit"
        ).value;
        const paymentsPerYear = parseInt(
          document.getElementById("paymentFrequency").value,
          10
        );
        const extraPayment =
          parseFloat(document.getElementById("extraPayment").value) || 0;
        const errorEl = document.getElementById("calcError");

        errorEl.classList.add("hidden");
        errorEl.textContent = "";

        if (
          !(loanAmount > 0) ||
          !(annualRate >= 0) ||
          !(balloonTermVal > 0) ||
          !(amortTermVal > 0)
        ) {
          errorEl.textContent =
            "Please enter positive values for loan amount, terms and interest rate.";
          errorEl.classList.remove("hidden");
          return null;
        }

        const balloonYears =
          balloonTermUnit === "years" ? balloonTermVal : balloonTermVal / 12;
        const amortYears =
          amortTermUnit === "years" ? amortTermVal : amortTermVal / 12;

        const N_b = Math.round(balloonYears * paymentsPerYear);
        const N_a = Math.round(amortYears * paymentsPerYear);

        if (N_b <= 0 || N_a <= 0 || N_b > N_a) {
          errorEl.textContent =
            "Balloon term must be shorter than or equal to the amortization period.";
          errorEl.classList.remove("hidden");
          return null;
        }

        const r = annualRate / paymentsPerYear;

        let payment;
        if (r === 0) {
          payment = loanAmount / N_a;
        } else {
          const pow = Math.pow(1 + r, N_a);
          payment = (loanAmount * (r * pow)) / (pow - 1);
        }

        // Build amortization schedule up to balloon
        let balance = loanAmount;
        const schedule = [];
        let totalInterest = 0;
        let totalPaid = 0;
        let principalPaidBeforeBalloon = 0;

        for (let n = 1; n <= N_b; n++) {
          const interestPortion = r === 0 ? 0 : balance * r;
          let principalPortion = payment - interestPortion;
          let extra = extraPayment;

          if (principalPortion + extra > balance) {
            extra = Math.max(0, balance - principalPortion);
          }

          const totalThisPayment = payment + extra;
          if (totalThisPayment <= 0) break;

          principalPortion = totalThisPayment - interestPortion;
          balance = Math.max(0, balance - principalPortion);

          totalInterest += interestPortion;
          totalPaid += totalThisPayment;
          principalPaidBeforeBalloon += principalPortion;

          schedule.push({
            period: n,
            payment: totalThisPayment,
            interest: interestPortion,
            principal: principalPortion,
            extra: extra,
            balance: balance,
          });

          if (balance <= 0) break;
        }

        const balloonPayment = balance;
        const totalWithBalloon = totalPaid + balloonPayment;
        const totalInterestWithBalloon = totalWithBalloon - loanAmount;

        // Fully amortizing comparison
        let fullInterest = 0;
        if (r === 0) {
          fullInterest = 0;
        } else {
          fullInterest = payment * N_a - loanAmount;
        }

        return {
          loanAmount,
          annualRate,
          paymentsPerYear,
          payment,
          N_b: schedule.length,
          N_a,
          schedule,
          balloonPayment,
          totalPaid,
          totalWithBalloon,
          totalInterestWithBalloon,
          fullPayment: payment,
          fullInterest,
          principalPaidBeforeBalloon,
          principalBalloon: balloonPayment,
          balloonYears,
          amortYears,
        };
      }

      function updateUI(result) {
        if (!result) return;

        document.getElementById("resultPayment").textContent = fmtCurrency(
          result.payment
        );
        document.getElementById("resultNumPayments").textContent =
          result.N_b.toString();
        document.getElementById("resultBalloon").textContent = fmtCurrency(
          result.balloonPayment
        );
        document.getElementById("resultTotalPaid").textContent = fmtCurrency(
          result.totalWithBalloon
        );
        document.getElementById("resultTotalInterest").textContent =
          fmtCurrency(result.totalInterestWithBalloon);

        document.getElementById("resultFullPayment").textContent = fmtCurrency(
          result.fullPayment
        );
        document.getElementById("resultFullInterest").textContent = fmtCurrency(
          result.fullInterest
        );
        const interestDiff =
          result.totalInterestWithBalloon - result.fullInterest;
        document.getElementById("resultInterestDiff").textContent =
          fmtCurrency(interestDiff);

        document.getElementById("resultPrincipalPaid").textContent =
          fmtCurrency(result.principalPaidBeforeBalloon);
        document.getElementById("resultPrincipalBalloon").textContent =
          fmtCurrency(result.principalBalloon);

        const paidPct =
          result.loanAmount > 0
            ? (result.principalPaidBeforeBalloon / result.loanAmount) * 100
            : 0;
        const balloonPct =
          result.loanAmount > 0
            ? (result.principalBalloon / result.loanAmount) * 100
            : 0;
        document.getElementById("barPrincipalPaid").style.width =
          Math.max(0, Math.min(100, paidPct)) + "%";

        document.getElementById("resultEffectiveTerm").textContent =
          result.balloonYears.toFixed(2).replace(/\.00$/, "") + " years";
        document.getElementById("resultPaymentsPct").textContent =
          fmtPercent(paidPct);
        document.getElementById("resultBalloonPct").textContent =
          fmtPercent(balloonPct);

        // Update schedule table
        const tbody = document.getElementById("scheduleBody");
        tbody.innerHTML = "";
        if (result.schedule.length === 0) {
          const tr = document.createElement("tr");
          tr.innerHTML =
            '<td colspan="6" class="px-3 py-4 text-center text-gray-500">No schedule available.</td>';
          tbody.appendChild(tr);
        } else {
          result.schedule.forEach((row) => {
            const tr = document.createElement("tr");
            tr.className = "border-t border-gray-100";
            tr.innerHTML = `
          <td class="px-3 py-1.5 text-left">${row.period}</td>
          <td class="px-3 py-1.5">${fmtCurrency(row.payment)}</td>
          <td class="px-3 py-1.5">${fmtCurrency(row.interest)}</td>
          <td class="px-3 py-1.5">${fmtCurrency(row.principal)}</td>
          <td class="px-3 py-1.5">${fmtCurrency(row.extra)}</td>
          <td class="px-3 py-1.5">${fmtCurrency(row.balance)}</td>
        `;
            tbody.appendChild(tr);
          });
        }

        // Store last result for CSV download
        window._balloonLastResult = result;
      }

      document
        .getElementById("calculateBtn")
        .addEventListener("click", function () {
          const res = calculateBalloonLoan();
          updateUI(res);
        });

      document
        .getElementById("resetBtn")
        .addEventListener("click", function () {
          document.getElementById("loanAmount").value = 250000;
          document.getElementById("interestRate").value = 6.5;
          document.getElementById("balloonTerm").value = 5;
          document.getElementById("balloonTermUnit").value = "years";
          document.getElementById("amortizationTerm").value = 30;
          document.getElementById("amortizationTermUnit").value = "years";
          document.getElementById("paymentFrequency").value = "12";
          document.getElementById("extraPayment").value = 0;
          document.getElementById("calcError").classList.add("hidden");
          document.getElementById("scheduleBody").innerHTML =
            '<tr><td colspan="6" class="px-3 py-4 text-center text-gray-500">Run a calculation to see the amortization schedule.</td></tr>';
          window._balloonLastResult = null;
          // Clear result fields
          [
            "resultPayment",
            "resultNumPayments",
            "resultBalloon",
            "resultTotalPaid",
            "resultTotalInterest",
            "resultFullPayment",
            "resultFullInterest",
            "resultInterestDiff",
            "resultPrincipalPaid",
            "resultPrincipalBalloon",
            "resultEffectiveTerm",
            "resultPaymentsPct",
            "resultBalloonPct",
          ].forEach((id) => {
            const el = document.getElementById(id);
            if (!el) return;
            if (id === "resultNumPayments") el.textContent = "0";
            else if (id === "resultEffectiveTerm") el.textContent = "0 years";
            else if (id === "resultPaymentsPct" || id === "resultBalloonPct")
              el.textContent = "0%";
            else el.textContent = "$0.00";
          });
          document.getElementById("barPrincipalPaid").style.width = "0%";
        });

      // Tabs
      document
        .getElementById("tabSchedule")
        .addEventListener("click", function () {
          document.getElementById("schedulePanel").classList.remove("hidden");
          document.getElementById("explanationPanel").classList.add("hidden");
          this.classList.add(
            "border-b-2",
            "border-blue-600",
            "text-blue-600",
            "font-semibold"
          );
          document
            .getElementById("tabExplanation")
            .classList.remove(
              "border-b-2",
              "border-blue-600",
              "text-blue-600",
              "font-semibold"
            );
        });
      document
        .getElementById("tabExplanation")
        .addEventListener("click", function () {
          document.getElementById("schedulePanel").classList.add("hidden");
          document
            .getElementById("explanationPanel")
            .classList.remove("hidden");
          this.classList.add(
            "border-b-2",
            "border-blue-600",
            "text-blue-600",
            "font-semibold"
          );
          document
            .getElementById("tabSchedule")
            .classList.remove(
              "border-b-2",
              "border-blue-600",
              "text-blue-600",
              "font-semibold"
            );
        });

      // CSV download
      document
        .getElementById("downloadCsvBtn")
        .addEventListener("click", function () {
          const res = window._balloonLastResult;
          if (!res || !res.schedule || res.schedule.length === 0) return;

          let csv = "Period,Payment,Interest,Principal,Extra,Balance\n";
          res.schedule.forEach((row) => {
            csv +=
              [
                row.period,
                row.payment.toFixed(2),
                row.interest.toFixed(2),
                row.principal.toFixed(2),
                row.extra.toFixed(2),
                row.balance.toFixed(2),
              ].join(",") + "\n";
          });

          const blob = new Blob([csv], { type: "text/csv;charset=utf-8;" });
          const url = URL.createObjectURL(blob);
          const a = document.createElement("a");
          a.href = url;
          a.download = "balloon-loan-amortization.csv";
          document.body.appendChild(a);
          a.click();
          document.body.removeChild(a);
          URL.revokeObjectURL(url);
        });

      document
        .querySelectorAll("#faq-container .faq-question")
        .forEach((btn) => {
          btn.addEventListener("click", () => {
            const answer = btn.parentElement.querySelector(".faq-answer");
            const isHidden = answer.classList.contains("hidden");
            document
              .querySelectorAll("#faq-container .faq-answer")
              .forEach((a) => a.classList.add("hidden"));
            document
              .querySelectorAll("#faq-container .faq-question span:last-child")
              .forEach((icon) => (icon.textContent = "+"));
            if (isHidden) {
              answer.classList.remove("hidden");
              btn.querySelector("span:last-child").textContent = "−";
            }
          });
        });

      // Auto-run initial example
      window.addEventListener("load", () => {
        const res = calculateBalloonLoan();
        updateUI(res);
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
