    <script>
      // Utility helpers
      function toNumber(id) {
        const v = parseFloat(document.getElementById(id).value);
        return isNaN(v) ? 0 : v;
      }

      function setText(id, value, decimals = 3) {
        const el = document.getElementById(id);
        if (!el) return;
        if (value === null || isNaN(value)) {
          el.textContent = "–";
        } else {
          const d = typeof decimals === "number" ? decimals : 3;
          el.textContent = typeof value === "number" ? value.toFixed(d) : value;
        }
      }

      function deg(rad) {
        return (rad * 180) / Math.PI;
      }
      function rad(degVal) {
        return (degVal * Math.PI) / 180;
      }

      // Auto-size large pulley based on speeds
      function autoSizePulley() {
        const N1 = toNumber("driverSpeed");
        const N2 = toNumber("drivenSpeed");
        const D1 = toNumber("smallDiameter");

        if (N1 <= 0 || N2 <= 0 || D1 <= 0) {
          alert(
            "Please enter positive values for driver speed, driven speed and small pulley diameter."
          );
          return;
        }

        const i = N1 / N2;
        const D2 = i * D1;

        document.getElementById("largeDiameter").value = D2.toFixed(1);

        // Recompute belt speed and driven speed (in case of rounding)
        computeBeltDrive(true);
      }

      function computeBeltDrive(fromAutoSize = false) {
        const mode = document.getElementById("designMode").value;
        const Pinput = toNumber("power");
        const powerUnit = document.getElementById("powerUnit").value;
        const PkW = powerUnit === "hp" ? Pinput * 0.7457 : Pinput;

        const N1 = toNumber("driverSpeed");
        let N2 = toNumber("drivenSpeed");

        let D1 = toNumber("smallDiameter");
        let D2 = toNumber("largeDiameter");
        const C = toNumber("centerDistance");

        const mu = toNumber("mu");
        const sigmaAllow = toNumber("maxStress"); // MPa = N/mm^2
        const b = toNumber("beltWidth"); // mm
        const t = toNumber("beltThickness"); // mm

        const wrapTargetDeg = toNumber("wrapOnSmall");

        const warnings = [];

        if (
          Pinput <= 0 ||
          N1 <= 0 ||
          D1 <= 0 ||
          C <= 0 ||
          b <= 0 ||
          t <= 0 ||
          sigmaAllow <= 0 ||
          mu <= 0
        ) {
          alert(
            "Please enter positive values for power, speeds, diameters, center distance, belt dimensions, allowable stress and friction."
          );
          return;
        }

        // If in sizeFromSpeeds mode and D2 is zero or from auto-size, compute from speeds
        if (mode === "sizeFromSpeeds" && (!D2 || D2 <= 0 || fromAutoSize)) {
          if (N2 <= 0) {
            alert("Please enter a positive driven speed N₂ (rpm).");
            return;
          }
          const i = N1 / N2;
          D2 = i * D1;
          document.getElementById("largeDiameter").value = D2.toFixed(1);
        }

        if (D2 <= 0) {
          alert("Please enter a positive large pulley diameter.");
          return;
        }

        // Recompute driven speed from diameters (geometric ratio)
        const iGeom = D2 / D1;
        if (iGeom > 0) {
          N2 = N1 / iGeom;
          document.getElementById("drivenSpeed").value = N2.toFixed(1);
        }

        // Belt speed (m/s) using small pulley
        const v = (Math.PI * (D1 / 1000) * N1) / 60;

        // Belt length L (mm) for open belt
        const Cmm = C;
        const D1mm = D1;
        const D2mm = D2;
        let L = null;
        let theta1 = null;
        let theta2 = null;

        if (Cmm > Math.abs(D2mm - D1mm) / 2) {
          const term = (D2mm - D1mm) / (2 * Cmm);
          const alpha = Math.asin(Math.min(Math.max(term, -1), 1)); // rad
          theta1 = Math.PI - 2 * alpha;
          theta2 = Math.PI + 2 * alpha;
          L =
            2 * Cmm +
            (Math.PI / 2) * (D1mm + D2mm) +
            ((D2mm - D1mm) * (D2mm - D1mm)) / (4 * Cmm);
        } else {
          warnings.push(
            "Center distance is too small relative to pulley diameters; wrap angle cannot be computed reliably."
          );
        }

        // Tensions and power capacity
        const A = b * t; // mm^2
        const Tmax = sigmaAllow * A; // N (since MPa = N/mm^2)
        let T1 = Tmax;
        let T2 = null;
        let deltaT = null;
        let Pcap = null;
        let SFpower = null;

        if (theta1 !== null) {
          const theta1rad = theta1; // already in rad
          const ratio = Math.exp(mu * theta1rad);
          T2 = T1 / ratio;
          deltaT = T1 - T2;
          Pcap = (deltaT * v) / 1000; // kW
          SFpower = PkW > 0 ? Pcap / PkW : null;
        }

        // Working stress check (simple)
        const sigmaWork = Tmax / A; // should equal sigmaAllow by definition here
        if (sigmaWork > sigmaAllow * 1.05) {
          warnings.push("Working belt stress exceeds allowable stress.");
        }

        // Belt speed warnings
        const beltType = document.getElementById("beltType").value;
        if (beltType === "v") {
          if (v < 5)
            warnings.push(
              "Belt speed is quite low for a V-belt; consider smaller pulleys or higher speed."
            );
          if (v > 25)
            warnings.push(
              "Belt speed is high for a V-belt; check manufacturer limits and consider larger pulleys or lower speed."
            );
        } else {
          if (v < 5)
            warnings.push(
              "Belt speed is low; flat belts are usually used at higher speeds for efficiency."
            );
          if (v > 30)
            warnings.push(
              "Belt speed is high for a flat belt; verify with manufacturer data."
            );
        }

        // Wrap angle warning
        if (theta1 !== null) {
          const theta1deg = deg(theta1);
          if (wrapTargetDeg > 0 && theta1deg < wrapTargetDeg) {
            warnings.push(
              "Wrap angle on the small pulley is below the target. Increase center distance or use an idler."
            );
          }
        }

        // Safety factor warning
        if (SFpower !== null) {
          if (SFpower < 1.0) {
            warnings.push(
              "Safety factor on power is below 1.0 – belt is undersized for the required power."
            );
          } else if (SFpower < 1.3) {
            warnings.push(
              "Safety factor on power is low. Consider a wider belt, multiple belts, or different diameters."
            );
          }
        }

        // Update UI
        document.getElementById("resultsSummary").classList.remove("hidden");
        document.getElementById("resultsSummary").textContent =
          "Computed belt speed, belt length, wrap angles and estimated power capacity based on your inputs. Use the warnings below as guidance and always confirm with manufacturer data.";

        setText("resSpeedRatio", iGeom, 3);
        setText("resDrivenSpeed", N2, 1);
        setText("resBeltSpeed", v, 2);
        setText("resBeltLength", L, 0);
        setText("resWrapSmall", theta1 !== null ? deg(theta1) : null, 1);
        setText("resWrapLarge", theta2 !== null ? deg(theta2) : null, 1);

        setText("resT1", T1, 0);
        setText("resT2", T2, 0);
        setText("resDeltaT", deltaT, 0);
        setText("resPowerCap", Pcap, 2);
        setText("resSFPower", SFpower, 2);
        setText("resStress", sigmaWork, 2);

        const warnBox = document.getElementById("designWarnings");
        if (warnings.length > 0) {
          warnBox.classList.remove("hidden");
          warnBox.innerHTML =
            '<strong>Design notes:</strong><ul class="list-disc ml-5 mt-1">' +
            warnings.map((w) => "<li>" + w + "</li>").join("") +
            "</ul>";
        } else {
          warnBox.classList.add("hidden");
          warnBox.innerHTML = "";
        }
      }

      document
        .getElementById("autoSizeBtn")
        .addEventListener("click", function (e) {
          e.preventDefault();
          autoSizePulley();
        });

      document
        .getElementById("computeBtn")
        .addEventListener("click", function (e) {
          e.preventDefault();
          computeBeltDrive(false);
        });

      document
        .getElementById("resetBtn")
        .addEventListener("click", function (e) {
          e.preventDefault();
          document.getElementById("designMode").value = "sizeFromSpeeds";
          document.getElementById("power").value = 5;
          document.getElementById("powerUnit").value = "kW";
          document.getElementById("driverSpeed").value = 1500;
          document.getElementById("drivenSpeed").value = 500;
          document.getElementById("beltType").value = "v";
          document.getElementById("mu").value = 0.3;
          document.getElementById("maxStress").value = 2.5;
          document.getElementById("beltWidth").value = 25;
          document.getElementById("beltThickness").value = 8;
          document.getElementById("smallDiameter").value = 150;
          document.getElementById("largeDiameter").value = 450;
          document.getElementById("centerDistance").value = 800;
          document.getElementById("wrapOnSmall").value = 150;

          [
            "resSpeedRatio",
            "resDrivenSpeed",
            "resBeltSpeed",
            "resBeltLength",
            "resWrapSmall",
            "resWrapLarge",
            "resT1",
            "resT2",
            "resDeltaT",
            "resPowerCap",
            "resSFPower",
            "resStress",
          ].forEach((id) => {
            setText(id, null);
          });

          document.getElementById("resultsSummary").classList.add("hidden");
          const warnBox = document.getElementById("designWarnings");
          warnBox.classList.add("hidden");
          warnBox.innerHTML = "";
        });

      // Initial compute for default values
      window.addEventListener("DOMContentLoaded", () => {
        computeBeltDrive(false);
      });

      // Mobile menu script (simple)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          this.setAttribute("aria-expanded", isHidden ? "true" : "false");
        });

      window.MathJax = {
        tex: {
