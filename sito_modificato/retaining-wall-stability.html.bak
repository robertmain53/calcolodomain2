    <script>
      // Utility: degrees to radians
      function deg2rad(d) {
        return (d * Math.PI) / 180;
      }

      function setStatusBadge(el, fs, target) {
        if (!isFinite(fs)) {
          el.textContent = "–";
          el.className = "badge bg-gray-100 text-gray-700";
          return;
        }
        el.textContent = fs >= target ? "OK" : "Check";
        el.className =
          fs >= target
            ? "badge bg-green-100 text-green-700"
            : "badge bg-red-100 text-red-700";
      }

      function calculateRetainingWall() {
        const errEl = document.getElementById("calcError");
        errEl.textContent = "";

        // Read inputs
        const H = parseFloat(document.getElementById("wallHeight").value);
        const B = parseFloat(document.getElementById("baseWidth").value);
        const bt = parseFloat(document.getElementById("toeWidth").value);
        const ts = parseFloat(document.getElementById("stemThickness").value);
        const gammaC = parseFloat(
          document.getElementById("gammaConcrete").value
        );
        const gamma = parseFloat(document.getElementById("gammaSoil").value);
        const phi = parseFloat(document.getElementById("phiSoil").value);
        const c = parseFloat(document.getElementById("cohesion").value);
        const q = parseFloat(document.getElementById("surcharge").value);
        const beta = parseFloat(document.getElementById("backfillSlope").value);
        const delta = parseFloat(document.getElementById("wallFriction").value);
        const mu = parseFloat(document.getElementById("muBase").value);
        const qall = parseFloat(
          document.getElementById("bearingCapacity").value
        );
        const theory = document.getElementById("earthPressureTheory").value;

        const fsSlideTarget = parseFloat(
          document.getElementById("fsSlideTarget").value
        );
        const fsOverturnTarget = parseFloat(
          document.getElementById("fsOverturnTarget").value
        );
        const fsBearingTarget = parseFloat(
          document.getElementById("fsBearingTarget").value
        );

        if (
          [H, B, bt, ts, gammaC, gamma, phi, mu, qall].some(
            (v) => !isFinite(v) || v <= 0
          )
        ) {
          errEl.textContent =
            "Please enter positive numeric values for geometry and material properties.";
          return;
        }
        if (bt >= B) {
          errEl.textContent = "Toe width must be less than total base width.";
          return;
        }

        const bh = B - bt; // heel width
        const tb = Math.max(0.4, 0.15 * H); // assumed base thickness (m)

        // 1. Active earth pressure coefficient Ka
        let Ka;
        if (theory === "rankine") {
          Ka = Math.tan(Math.PI / 4 - deg2rad(phi) / 2);
          Ka = Ka * Ka;
        } else {
          // Simplified Coulomb for vertical wall, backfill slope beta, wall friction delta
          const phir = deg2rad(phi);
          const betar = deg2rad(beta);
          const deltar = deg2rad(delta);
          const num = Math.sin(phir + betar) * Math.sin(phir - deltar);
          const den = Math.sin(betar) * Math.sin(phir + betar - deltar);
          Ka = num / den;
        }

        // 2. Active earth force Pa (kN/m)
        const Pa_tri = 0.5 * Ka * gamma * H * H;
        const Pa_surcharge = Ka * q * H;
        const Pa_cohesion = 2 * c * Math.sqrt(Ka) * H;
        const Pa = Pa_tri + Pa_surcharge - Pa_cohesion;

        // Lever arm of Pa from base (approximate)
        const z_tri = H / 3;
        const z_surcharge = H / 2;
        const Pa_eff = Pa; // total
        let z_eff;
        if (Pa_eff !== 0) {
          z_eff =
            (Pa_tri * z_tri +
              Pa_surcharge * z_surcharge -
              Pa_cohesion * (H / 2)) /
            Pa_eff;
        } else {
          z_eff = H / 3;
        }

        // 3. Vertical forces (kN/m)
        const W_stem = gammaC * ts * H;
        const W_base = gammaC * B * tb;
        const W_soilHeel = gamma * bh * H;
        const W_surcharge = q * bh;

        const V = W_stem + W_base + W_soilHeel + W_surcharge;

        // 4. Moments about toe (kN·m/m)
        // Coordinates: toe at x=0, base extends to x=B.
        const x_stem = bt + ts / 2;
        const x_base = B / 2;
        const x_soilHeel = bt + bh / 2;
        const x_surcharge = x_soilHeel;

        const MR =
          W_stem * x_stem +
          W_base * x_base +
          W_soilHeel * x_soilHeel +
          W_surcharge * x_surcharge;
        const MOT = Pa * z_eff; // overturning about toe from horizontal force at height z_eff

        // 5. Factors of safety
        const R_slide = mu * V;
        const FS_slide = Pa > 0 ? R_slide / Pa : Infinity;
        const FS_OT = MOT > 0 ? MR / MOT : Infinity;

        // 6. Bearing pressure and eccentricity
        const M_res = MR - MOT; // net stabilizing moment about toe
        const x_res = M_res / V; // distance from toe to resultant
        const e = B / 2 - x_res; // eccentricity from center (positive toward heel)

        // Bearing pressures (assuming no tension if |e| <= B/6)
        let qmax, qmin;
        const qavg = V / B;
        if (Math.abs(e) <= B / 6) {
          qmax = qavg * (1 + (6 * e) / B);
          qmin = qavg * (1 - (6 * e) / B);
        } else {
          // Toe-only bearing (triangular)
          qmax = (2 * V) / (3 * (B / 2 - Math.abs(e)));
          qmin = 0;
        }
        const FS_bearing = qmax > 0 ? qall / qmax : Infinity;

        // Update UI
        document.getElementById("kaValue").textContent = Ka.toFixed(3);
        document.getElementById("paValue").textContent = Pa.toFixed(1);
        document.getElementById("paLeverArm").textContent = z_eff.toFixed(2);

        document.getElementById("wStem").textContent = W_stem.toFixed(1);
        document.getElementById("wBase").textContent = W_base.toFixed(1);
        document.getElementById("wHeelSoil").textContent =
          W_soilHeel.toFixed(1);
        document.getElementById("wSurcharge").textContent =
          W_surcharge.toFixed(1);
        document.getElementById("vTotal").textContent = V.toFixed(1);

        document.getElementById("mResist").textContent = MR.toFixed(1);
        document.getElementById("mOverturn").textContent = MOT.toFixed(1);
        document.getElementById("eccentricity").textContent = e.toFixed(3);
        document.getElementById("qMax").textContent = qmax.toFixed(1);

        document.getElementById("fsSlide").textContent = isFinite(FS_slide)
          ? FS_slide.toFixed(2)
          : "–";
        document.getElementById("fsOverturn").textContent = isFinite(FS_OT)
          ? FS_OT.toFixed(2)
          : "–";
        document.getElementById("fsBearing").textContent = isFinite(FS_bearing)
          ? FS_bearing.toFixed(2)
          : "–";

        setStatusBadge(
          document.getElementById("slideStatus"),
          FS_slide,
          fsSlideTarget
        );
        setStatusBadge(
          document.getElementById("overturnStatus"),
          FS_OT,
          fsOverturnTarget
        );
        setStatusBadge(
          document.getElementById("bearingStatus"),
          FS_bearing,
          fsBearingTarget
        );
      }

      document
        .getElementById("calculateBtn")
        .addEventListener("click", calculateRetainingWall);

      document.getElementById("resetBtn").addEventListener("click", () => {
        document.getElementById("wallHeight").value = 4;
        document.getElementById("baseWidth").value = 2.5;
        document.getElementById("toeWidth").value = 0.8;
        document.getElementById("stemThickness").value = 0.4;
        document.getElementById("gammaConcrete").value = 24;
        document.getElementById("gammaSoil").value = 18;
        document.getElementById("phiSoil").value = 30;
        document.getElementById("cohesion").value = 0;
        document.getElementById("surcharge").value = 10;
        document.getElementById("backfillSlope").value = 0;
        document.getElementById("wallFriction").value = 0;
        document.getElementById("muBase").value = 0.5;
        document.getElementById("bearingCapacity").value = 250;
        document.getElementById("earthPressureTheory").value = "rankine";
        document.getElementById("fsSlideTarget").value = 1.5;
        document.getElementById("fsOverturnTarget").value = 2.0;
        document.getElementById("fsBearingTarget").value = 3.0;
        document.getElementById("calcError").textContent = "";
        // Clear results
        [
          "kaValue",
          "paValue",
          "paLeverArm",
          "wStem",
          "wBase",
          "wHeelSoil",
          "wSurcharge",
          "vTotal",
          "mResist",
          "mOverturn",
          "eccentricity",
          "qMax",
          "fsSlide",
          "fsOverturn",
          "fsBearing",
        ].forEach((id) => {
          document.getElementById(id).textContent = "–";
        });
        ["slideStatus", "overturnStatus", "bearingStatus"].forEach((id) => {
          const el = document.getElementById(id);
          el.textContent = "–";
          el.className = "badge bg-gray-100 text-gray-700";
        });
      });

      // Auto-run once with defaults
      window.addEventListener("load", calculateRetainingWall);

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
