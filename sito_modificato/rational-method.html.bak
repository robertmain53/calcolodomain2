    <script>
      // Mobile menu
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          this.setAttribute("aria-expanded", isHidden ? "true" : "false");
        });

      // Unit system toggle
      const btnUS = document.getElementById("btn-us");
      const btnSI = document.getElementById("btn-si");
      const intensityUnitSpan = document.getElementById("intensity-unit");
      const areaUnitSpan = document.getElementById("area-unit");
      let currentSystem = "US"; // 'US' or 'SI'

      function setUnitSystem(system) {
        currentSystem = system;
        if (system === "US") {
          btnUS.classList.add("tab-active");
          btnSI.classList.remove("tab-active");
          intensityUnitSpan.textContent = "in/hr";
          areaUnitSpan.textContent = "acres";
        } else {
          btnSI.classList.add("tab-active");
          btnUS.classList.remove("tab-active");
          intensityUnitSpan.textContent = "mm/hr";
          areaUnitSpan.textContent = "hectares";
        }
        document.getElementById("results-section").classList.add("hidden");
      }

      btnUS.addEventListener("click", () => setUnitSystem("US"));
      btnSI.addEventListener("click", () => setUnitSystem("SI"));

      // Main calculation
      function calculateRational() {
        const C = parseFloat(document.getElementById("c").value);
        const i = parseFloat(document.getElementById("intensity").value);
        const A = parseFloat(document.getElementById("area").value);
        const K =
          parseFloat(document.getElementById("safety-factor").value) || 1.0;
        const tC = parseFloat(document.getElementById("tc").value);
        const T = document.getElementById("return-period").value;

        if (!isFinite(C) || C <= 0) {
          alert("Please enter a valid runoff coefficient C (> 0).");
          return;
        }
        if (!isFinite(i) || i <= 0) {
          alert("Please enter a valid rainfall intensity i (> 0).");
          return;
        }
        if (!isFinite(A) || A <= 0) {
          alert("Please enter a valid drainage area A (> 0).");
          return;
        }

        let Q, unitLabel, formulaStr;
        if (currentSystem === "US") {
          // Q (cfs) = C * i(in/hr) * A(acres) * K
          Q = C * i * A * K;
          unitLabel = "cubic feet per second (cfs)";
          formulaStr = `Q = ${C.toFixed(3)} × ${i.toFixed(3)} × ${A.toFixed(
            3
          )} × ${K.toFixed(3)} = ${Q.toFixed(3)} cfs`;
        } else {
          // Q (m³/s) = 0.278 * C * i(mm/hr) * A(ha) * K
          Q = 0.278 * C * i * A * K;
          unitLabel = "cubic meters per second (m³/s)";
          formulaStr = `Q = 0.278 × ${C.toFixed(3)} × ${i.toFixed(
            3
          )} × ${A.toFixed(3)} × ${K.toFixed(3)} = ${Q.toFixed(3)} m³/s`;
        }

        document.getElementById("result-q").textContent = Q.toFixed(3);
        document.getElementById("result-q-unit").textContent = unitLabel;
        document.getElementById("result-formula").textContent = formulaStr;

        let notes = [];
        if (isFinite(tC) && tC > 0) {
          notes.push(
            `Time of concentration t\u2098 ≈ ${tC.toFixed(
              1
            )} min (use this duration on IDF curves).`
          );
        }
        if (T) {
          notes.push(`Return period: approximately ${T}-year design storm.`);
        }
        if (K && Math.abs(K - 1.0) > 1e-3) {
          notes.push(`Includes safety/adjustment factor K = ${K.toFixed(2)}.`);
        }
        if (!notes.length) {
          notes.push("No additional design notes specified.");
        }
        document.getElementById("result-notes").textContent = notes.join(" ");

        document.getElementById("results-section").classList.remove("hidden");
      }

      document
        .getElementById("btn-calc")
        .addEventListener("click", calculateRational);

      document.getElementById("btn-reset").addEventListener("click", () => {
        document.getElementById("c").value = 0.7;
        document.getElementById("intensity").value = 3.0;
        document.getElementById("area").value = 10;
        document.getElementById("tc").value = "";
        document.getElementById("return-period").value = "10";
        document.getElementById("safety-factor").value = 1.0;
        document.getElementById("results-section").classList.add("hidden");
      });

      // Weighted C helper
      function updateWeightedC() {
        const rows = document.querySelectorAll("#weighted-c-body tr");
        let total = 0;
        rows.forEach((row) => {
          const inputs = row.querySelectorAll("input");
          if (inputs.length === 2) {
            const Ci = parseFloat(inputs[0].value) || 0;
            const fi = parseFloat(inputs[1].value) || 0;
            const term = Ci * fi;
            row.querySelector(".weighted-term").textContent = term.toFixed(3);
            total += term;
          }
        });
        document.getElementById("weighted-c-total").textContent =
          total.toFixed(3);
      }

      document.querySelectorAll("#weighted-c-body input").forEach((input) => {
        input.addEventListener("input", updateWeightedC);
      });
      updateWeightedC();

      document
        .getElementById("btn-apply-weighted-c")
        .addEventListener("click", () => {
          const Cw = parseFloat(
            document.getElementById("weighted-c-total").textContent
          );
          if (isFinite(Cw) && Cw > 0) {
            document.getElementById("c").value = Cw.toFixed(3);
            document.getElementById("results-section").classList.add("hidden");
          }
        });

      // Initialize default unit system
      setUnitSystem("US");

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
