      </div>
      <div class="col-span-3">
        <label class="block text-[11px] text-gray-600 mb-0.5">${
          type === "reactant" ? "Amount" : "Amount (optional)"
        }</label>
        <input type="number" class="sub-amount w-full border rounded-md px-2 py-1" min="0" step="any" ${
          type === "product" ? 'placeholder="(usually blank)"' : ""
        }>
      </div>
      <div class="col-span-2">
        <label class="block text-[11px] text-gray-600 mb-0.5">Unit</label>
        <select class="sub-unit w-full border rounded-md px-2 py-1">
          <option value="mol">mol</option>
          <option value="g">g</option>
        </select>
      </div>
      <div class="col-span-2">
        <label class="block text-[11px] text-gray-600 mb-0.5">Molar mass</label>
        <input type="number" class="sub-mm w-full border rounded-md px-2 py-1" min="0" step="any" placeholder="g/mol">
      </div>
    `;

          return row;
        }

        const reactantsContainer = document.getElementById(
          "reactants-container"
        );
        const productsContainer = document.getElementById("products-container");
        const addReactantBtn = document.getElementById("add-reactant-btn");
        const addProductBtn = document.getElementById("add-product-btn");
        const referenceProductSelect = document.getElementById(
          "reference-product-select"
        );
        const modeSelect = document.getElementById("mode-select");
        const basisSelect = document.getElementById("basis-select");
        const substanceModeContainer = document.getElementById(
          "substance-mode-container"
        );
        const simpleModeContainer = document.getElementById(
          "simple-mode-container"
        );

        const enablePercentYield = document.getElementById(
          "enable-percent-yield"
        );
        const percentYieldRow = document.getElementById("percent-yield-row");

        const calculateBtn = document.getElementById("calculate-btn");
        const resetBtn = document.getElementById("reset-btn");
        const errorMessage = document.getElementById("error-message");
        const resultsSection = document.getElementById("results-section");
        const limitingSummary = document.getElementById(
          "limiting-reactant-summary"
        );
        const yieldSummary = document.getElementById("yield-summary");
        const excessSummary = document.getElementById("excess-summary");
        const percentYieldSummary = document.getElementById(
          "percent-yield-summary"
        );
        const detailsTableBody = document.getElementById("details-table-body");

        function syncReferenceProductOptions() {
          const productRows = productsContainer.querySelectorAll(
            '[data-type="product"]'
          );
          referenceProductSelect.innerHTML = "";
          productRows.forEach((row, idx) => {
            const name =
              row.querySelector(".sub-name").value || `Product ${idx + 1}`;
            const opt = document.createElement("option");
            opt.value = idx;
            opt.textContent = name;
            referenceProductSelect.appendChild(opt);
          });
        }

        addReactantBtn.addEventListener("click", () => {
          reactantsContainer.appendChild(createSubstanceRow("reactant"));
        });

        addProductBtn.addEventListener("click", () => {
          productsContainer.appendChild(createSubstanceRow("product"));
          syncReferenceProductOptions();
        });

        productsContainer.addEventListener("input", (e) => {
          if (e.target.classList.contains("sub-name")) {
            syncReferenceProductOptions();
          }
        });

        // Mode switching
        modeSelect.addEventListener("change", () => {
          const mode = modeSelect.value;
          if (mode === "substance") {
            substanceModeContainer.classList.remove("hidden");
            simpleModeContainer.classList.add("hidden");
          } else {
            substanceModeContainer.classList.add("hidden");
            simpleModeContainer.classList.remove("hidden");
          }
          clearResults();
        });

        // Percent yield toggle
        enablePercentYield.addEventListener("change", () => {
          percentYieldRow.classList.toggle(
            "hidden",
            !enablePercentYield.checked
          );
        });

        function clearResults() {
          errorMessage.classList.add("hidden");
          errorMessage.textContent = "";
          resultsSection.classList.add("hidden");
          limitingSummary.textContent = "";
          yieldSummary.textContent = "";
          excessSummary.textContent = "";
          percentYieldSummary.textContent = "";
          detailsTableBody.innerHTML = "";
        }

        resetBtn.addEventListener("click", () => {
          // Reset substance mode
          reactantsContainer.innerHTML = "";
          productsContainer.innerHTML = "";
          initializeDefaultRows();
          // Reset simple mode fields
          ["A", "B", "C"].forEach((letter) => {
            document.getElementById(`simple-${letter}-name`).value = "";
            document.getElementById(`simple-${letter}-coeff`).value = "";
            document.getElementById(`simple-${letter}-amount`).value = "";
            document.getElementById(`simple-${letter}-mm`).value = "";
          });
          document.getElementById("simple-A-unit").value = "mol";
          document.getElementById("simple-B-unit").value = "mol";
          document.getElementById("simple-C-unit").value = "mol";
          document.getElementById("basis-select").value = "product";
          document.getElementById("mode-select").value = "substance";
          substanceModeContainer.classList.remove("hidden");
          simpleModeContainer.classList.add("hidden");
          enablePercentYield.checked = false;
          percentYieldRow.classList.add("hidden");
          document.getElementById("actual-yield-amount").value = "";
          document.getElementById("actual-yield-unit").value = "mol";
          clearResults();
        });

        function initializeDefaultRows() {
          // Two reactants, one product as a starting point
          reactantsContainer.appendChild(createSubstanceRow("reactant"));
          reactantsContainer.appendChild(createSubstanceRow("reactant"));
          productsContainer.appendChild(createSubstanceRow("product"));
          syncReferenceProductOptions();
        }

        initializeDefaultRows();

        function parseSubstances() {
          const reactants = [];
          const products = [];

          reactantsContainer
            .querySelectorAll('[data-type="reactant"]')
            .forEach((row, idx) => {
              const name =
                row.querySelector(".sub-name").value || `Reactant ${idx + 1}`;
              const coeff = parseFloat(
                row.querySelector(".sub-coeff").value || "0"
              );
              const amount = parseFloat(
                row.querySelector(".sub-amount").value || "0"
              );
              const unit = row.querySelector(".sub-unit").value;
              const mm = parseFloat(row.querySelector(".sub-mm").value || "0");
              reactants.push({
                name,
                coeff,
                amount,
                unit,
                mm,
                role: "reactant",
              });
            });

          productsContainer
            .querySelectorAll('[data-type="product"]')
            .forEach((row, idx) => {
              const name =
                row.querySelector(".sub-name").value || `Product ${idx + 1}`;
              const coeff = parseFloat(
                row.querySelector(".sub-coeff").value || "0"
              );
              const amount = parseFloat(
                row.querySelector(".sub-amount").value || "0"
              );
              const unit = row.querySelector(".sub-unit").value;
              const mm = parseFloat(row.querySelector(".sub-mm").value || "0");
              products.push({ name, coeff, amount, unit, mm, role: "product" });
            });

          return { reactants, products };
        }

        function toMoles(amount, unit, mm) {
          if (!isFinite(amount) || amount < 0) return NaN;
          if (unit === "mol") return amount;
          if (unit === "g") {
            if (!mm || mm <= 0) return NaN;
            return amount / mm;
          }
          return NaN;
        }

        function fromMoles(moles, unit, mm) {
          if (unit === "mol") return moles;
          if (unit === "g") {
            if (!mm || mm <= 0) return NaN;
            return moles * mm;
          }
          return NaN;
        }

        function calculateSubstanceMode() {
          const { reactants, products } = parseSubstances();
          const basis = basisSelect.value;

          if (reactants.length === 0 || products.length === 0) {
            throw new Error(
              "Please enter at least one reactant and one product."
            );
          }

          // Validate coefficients and amounts
          if (reactants.some((r) => !r.coeff || r.coeff <= 0)) {
            throw new Error(
              "All reactants must have positive stoichiometric coefficients."
            );
          }
          if (products.some((p) => !p.coeff || p.coeff <= 0)) {
            throw new Error(
              "All products must have positive stoichiometric coefficients."
            );
          }

          // Convert reactant amounts to moles
          reactants.forEach((r) => {
            r.n0 = toMoles(r.amount, r.unit, r.mm);
          });

          if (reactants.some((r) => isNaN(r.n0))) {
            throw new Error(
              "Please provide valid amounts and molar masses for all reactants."
            );
          }

          // Compute reaction extent for each reactant
          reactants.forEach((r) => {
            r.xi = r.n0 / r.coeff;
          });

          const limiting = reactants.reduce(
            (min, r) => (r.xi < min.xi ? r : min),
            reactants[0]
          );
          const xi = limiting.xi;

          // Compute moles used and leftover for reactants
          reactants.forEach((r) => {
            r.used = r.coeff * xi;
            r.leftover = r.n0 - r.used;
          });

          // Compute product moles
          products.forEach((p) => {
            p.n = p.coeff * xi;
          });

          // Reference product
          let refProduct = products[0];
          const refIndex = parseInt(referenceProductSelect.value || "0", 10);
          if (!isNaN(refIndex) && products[refIndex]) {
            refProduct = products[refIndex];
          }

          const theoreticalMoles = refProduct.n;
          const theoreticalGrams = fromMoles(
            theoreticalMoles,
            "g",
            refProduct.mm
          );

          // Percent yield
          let percentYield = null;
          if (enablePercentYield.checked) {
            const actualAmount = parseFloat(
              document.getElementById("actual-yield-amount").value || "0"
            );
            const actualUnit =
              document.getElementById("actual-yield-unit").value;
            if (actualAmount > 0) {
              const actualMoles = toMoles(
                actualAmount,
                actualUnit,
                refProduct.mm
              );
              if (!isNaN(actualMoles) && theoreticalMoles > 0) {
                percentYield = (actualMoles / theoreticalMoles) * 100;
              }
            }
          }

          return {
            mode: "substance",
            basis,
            reactants,
            products,
            limiting,
            xi,
            refProduct,
            theoreticalMoles,
            theoreticalGrams,
            percentYield,
          };
        }

        function calculateSimpleMode() {
          const basis = "product";

          const A = {
            name:
              document.getElementById("simple-A-name").value || "Reactant A",
            coeff: parseFloat(
              document.getElementById("simple-A-coeff").value || "0"
            ),
            amount: parseFloat(
              document.getElementById("simple-A-amount").value || "0"
            ),
            unit: document.getElementById("simple-A-unit").value,
            mm: parseFloat(document.getElementById("simple-A-mm").value || "0"),
            role: "reactant",
          };
          const B = {
            name:
              document.getElementById("simple-B-name").value || "Reactant B",
            coeff: parseFloat(
              document.getElementById("simple-B-coeff").value || "0"
            ),
            amount: parseFloat(
              document.getElementById("simple-B-amount").value || "0"
            ),
            unit: document.getElementById("simple-B-unit").value,
            mm: parseFloat(document.getElementById("simple-B-mm").value || "0"),
            role: "reactant",
          };
          const C = {
            name: document.getElementById("simple-C-name").value || "Product C",
            coeff: parseFloat(
              document.getElementById("simple-C-coeff").value || "0"
            ),
            unit: document.getElementById("simple-C-unit").value,
            mm: parseFloat(document.getElementById("simple-C-mm").value || "0"),
            role: "product",
          };

          if (!A.coeff || !B.coeff || !C.coeff) {
            throw new Error(
              "Please enter positive coefficients for A, B, and C."
            );
          }
          if (A.amount <= 0 || B.amount <= 0) {
            throw new Error(
              "Please enter positive amounts for both reactants."
            );
          }

          A.n0 = toMoles(A.amount, A.unit, A.mm);
          B.n0 = toMoles(B.amount, B.unit, B.mm);
          if (isNaN(A.n0) || isNaN(B.n0)) {
            throw new Error(
              "Please provide valid amounts and molar masses for both reactants."
            );
          }

          A.xi = A.n0 / A.coeff;
          B.xi = B.n0 / B.coeff;
          const limiting = A.xi < B.xi ? A : B;
          const xi = limiting.xi;

          A.used = A.coeff * xi;
          B.used = B.coeff * xi;
          A.leftover = A.n0 - A.used;
          B.leftover = B.n0 - B.used;

          C.n = C.coeff * xi;

          const theoreticalMoles = C.n;
          const theoreticalGrams = fromMoles(theoreticalMoles, "g", C.mm);

          let percentYield = null;
          if (enablePercentYield.checked) {
            const actualAmount = parseFloat(
              document.getElementById("actual-yield-amount").value || "0"
            );
            const actualUnit =
              document.getElementById("actual-yield-unit").value;
            if (actualAmount > 0) {
              const actualMoles = toMoles(actualAmount, actualUnit, C.mm);
              if (!isNaN(actualMoles) && theoreticalMoles > 0) {
                percentYield = (actualMoles / theoreticalMoles) * 100;
              }
            }
          }

          return {
            mode: "simple",
            basis,
            reactants: [A, B],
            products: [C],
            limiting,
            xi,
            refProduct: C,
            theoreticalMoles,
            theoreticalGrams,
            percentYield,
          };
        }

        function renderResults(result) {
          clearResults();
          const {
            reactants,
            products,
            limiting,
            refProduct,
            theoreticalMoles,
            theoreticalGrams,
            percentYield,
          } = result;

          resultsSection.classList.remove("hidden");

          limitingSummary.innerHTML = `
      <strong>Limiting reactant:</strong> ${limiting.name} (coefficient ${
            limiting.coeff
          }, initial ${limiting.n0.toFixed(4)} mol)
    `;

          let yieldText = `<strong>Theoretical yield (${
            refProduct.name
          }):</strong> ${theoreticalMoles.toFixed(4)} mol`;
          if (!isNaN(theoreticalGrams)) {
            yieldText += ` (${theoreticalGrams.toFixed(4)} g)`;
          }
          yieldSummary.innerHTML = yieldText;

          const excessReactants = reactants.filter(
            (r) => r.name !== limiting.name && r.leftover > 1e-10
          );
          if (excessReactants.length > 0) {
            const parts = excessReactants.map((r) => {
              let txt = `${r.name}: ${r.leftover.toFixed(4)} mol`;
              const g = fromMoles(r.leftover, "g", r.mm);
              if (!isNaN(g)) txt += ` (${g.toFixed(4)} g)`;
              return txt;
            });
            excessSummary.innerHTML = `<strong>Excess reactants:</strong> ${parts.join(
              "; "
            )}`;
          } else {
            excessSummary.innerHTML = `<strong>Excess reactants:</strong> none (all reactants fully consumed).`;
          }

          if (percentYield !== null) {
            percentYieldSummary.innerHTML = `<strong>Percent yield:</strong> ${percentYield.toFixed(
              2
            )}%`;
          } else {
            percentYieldSummary.textContent = "";
          }

          // Details table
          detailsTableBody.innerHTML = "";
          reactants.concat(products).forEach((s) => {
            const tr = document.createElement("tr");
            const n0 = s.n0 ?? 0;
            const used = s.used ?? s.n ?? 0;
            const leftover = s.leftover ?? 0;
            tr.innerHTML = `
        <td class="border px-2 py-1">${s.name}</td>
        <td class="border px-2 py-1 capitalize">${s.role}</td>
        <td class="border px-2 py-1 text-right">${s.coeff || ""}</td>
        <td class="border px-2 py-1 text-right">${n0 ? n0.toFixed(4) : ""}</td>
        <td class="border px-2 py-1 text-right">${
          used ? used.toFixed(4) : ""
        }</td>
        <td class="border px-2 py-1 text-right">${
          leftover ? leftover.toFixed(4) : ""
        }</td>
      `;
            detailsTableBody.appendChild(tr);
          });
        }

        calculateBtn.addEventListener("click", () => {
          try {
            const mode = modeSelect.value;
            const result =
              mode === "substance"
                ? calculateSubstanceMode()
                : calculateSimpleMode();
            renderResults(result);
          } catch (err) {
            clearResults();
            errorMessage.textContent =
              err.message || "An error occurred. Please check your inputs.";
            errorMessage.classList.remove("hidden");
          }
        });

        document
          .querySelectorAll("#faq-container .faq-question")
          .forEach((btn) => {
            btn.addEventListener("click", () => {
              const answer = btn.parentElement.querySelector(".faq-answer");
              const sign = btn.querySelector("span:last-child");
              const isHidden = answer.classList.contains("hidden");
              answer.classList.toggle("hidden", !isHidden === false);
              if (isHidden) {
                answer.classList.remove("hidden");
                sign.textContent = "−";
              } else {
                answer.classList.add("hidden");
                sign.textContent = "+";
              }
            });
          });
      })();
    </script>
  
