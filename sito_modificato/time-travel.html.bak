    <script>
      // Physical constants
      const C = 299792458; // m/s, not directly needed but kept for clarity

      const modeVelocityBtn = document.getElementById("mode-velocity");
      const modeGravityBtn = document.getElementById("mode-gravity");
      const modeDescription = document.getElementById("mode-description");
      const velocityInputs = document.getElementById("velocity-inputs");
      const gravityInputs = document.getElementById("gravity-inputs");

      const velocitySlider = document.getElementById("velocity-slider");
      const velocityLabel = document.getElementById("velocity-label");
      const earthYearsInput = document.getElementById("earth-years");
      const massSelect = document.getElementById("mass-select");

      const computeBtn = document.getElementById("compute-btn");
      const gammaBadge = document.getElementById("gamma-badge");
      const earthTimeDisplay = document.getElementById("earth-time-display");
      const travelerTimeDisplay = document.getElementById(
        "traveler-time-display"
      );
      const summaryText = document.getElementById("summary-text");

      let currentMode = "velocity";

      function setMode(mode) {
        currentMode = mode;
        if (mode === "velocity") {
          modeVelocityBtn.classList.remove(
            "border-gray-300",
            "text-gray-700",
            "bg-white"
          );
          modeVelocityBtn.classList.add(
            "border-blue-600",
            "text-blue-600",
            "bg-blue-50"
          );
          modeGravityBtn.classList.remove(
            "border-blue-600",
            "text-blue-600",
            "bg-blue-50"
          );
          modeGravityBtn.classList.add(
            "border-gray-300",
            "text-gray-700",
            "bg-white"
          );
          velocityInputs.classList.remove("hidden");
          gravityInputs.classList.add("hidden");
          modeDescription.textContent =
            "Travel at a high fraction of the speed of light and see how much you age compared with people on Earth.";
        } else {
          modeGravityBtn.classList.remove(
            "border-gray-300",
            "text-gray-700",
            "bg-white"
          );
          modeGravityBtn.classList.add(
            "border-blue-600",
            "text-blue-600",
            "bg-blue-50"
          );
          modeVelocityBtn.classList.remove(
            "border-blue-600",
            "text-blue-600",
            "bg-blue-50"
          );
          modeVelocityBtn.classList.add(
            "border-gray-300",
            "text-gray-700",
            "bg-white"
          );
          gravityInputs.classList.remove("hidden");
          velocityInputs.classList.add("hidden");
          modeDescription.textContent =
            "Stay near a massive object (Earth, neutron star, black hole) and compare your aging to a distant observer.";
        }
        compute();
      }

      function formatYears(y) {
        if (!isFinite(y)) return "—";
        if (y < 1e-3) return (y * 365.25 * 24 * 3600).toFixed(2) + " s";
        if (y < 0.1) return (y * 365.25 * 24).toFixed(2) + " h";
        if (y < 1) return (y * 365.25).toFixed(1) + " days";
        return y.toFixed(1) + " years";
      }

      function computeGammaVelocity(vFraction) {
        const v2c2 = vFraction * vFraction;
        if (v2c2 >= 1) return Infinity;
        return 1 / Math.sqrt(1 - v2c2);
      }

      function gravitationalFactor(option) {
        // Simple illustrative factors; not exact astrophysical models
        switch (option) {
          case "earth":
            // At Earth surface relative to far away: tiny effect
            return 0.9999999993; // ~0.7 ns per second slower
          case "neutron":
            return 0.5; // time runs at half speed (illustrative)
          case "bh3":
            return 0.1; // very strong dilation
          case "bh10":
            return 0.05; // even stronger
          default:
            return 1;
        }
      }

      function compute() {
        let earthYears = parseFloat(earthYearsInput.value);
        if (!isFinite(earthYears) || earthYears < 0) earthYears = 0;
        let travelerYears = earthYears;
        let gamma = 1;
        let description = "";

        if (currentMode === "velocity") {
          const v = parseFloat(velocitySlider.value);
          gamma = computeGammaVelocity(v);
          if (!isFinite(gamma) || gamma < 1) gamma = 1;
          travelerYears = earthYears / gamma;
          description = `You travel at ${(v * 100).toFixed(
            1
          )}% of the speed of light for ${earthYears.toFixed(
            1
          )} years of Earth time. You age only about ${travelerYears.toFixed(
            1
          )} years.`;
        } else {
          const factor = gravitationalFactor(massSelect.value);
          // dτ/dt = factor
          travelerYears = earthYears * factor;
          gamma = 1 / factor;
          description = `You remain near the selected massive object for ${earthYears.toFixed(
            1
          )} years of distant (Earth) time. You age only about ${travelerYears.toFixed(
            1
          )} years due to gravitational time dilation.`;
        }

        const futureJump = earthYears - travelerYears;

        gammaBadge.textContent =
          "γ ≈ " + (gamma >= 1000 ? gamma.toExponential(2) : gamma.toFixed(2));
        earthTimeDisplay.textContent = earthYears.toFixed(1) + " years";
        travelerTimeDisplay.textContent = travelerYears.toFixed(1) + " years";

        let extra = "";
        if (futureJump > 0.01) {
          extra = ` You effectively jump about ${futureJump.toFixed(
            1
          )} years into Earth’s future.`;
        }

        summaryText.textContent = description + extra;
      }

      // Event listeners
      modeVelocityBtn.addEventListener("click", () => setMode("velocity"));
      modeGravityBtn.addEventListener("click", () => setMode("gravity"));

      velocitySlider.addEventListener("input", () => {
        const v = parseFloat(velocitySlider.value);
        velocityLabel.textContent = v.toFixed(3) + "c";
      });

      [velocitySlider, earthYearsInput, massSelect].forEach((el) => {
        el.addEventListener("input", compute);
        el.addEventListener("change", compute);
      });

      computeBtn.addEventListener("click", compute);

      // Initial compute
      compute();
    </script>

