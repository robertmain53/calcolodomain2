    <script>
      // Utility: format date nicely
      function formatDate(date) {
        if (!date || isNaN(date.getTime())) return "—";
        const opts = { year: "numeric", month: "short", day: "numeric" };
        return date.toLocaleDateString(undefined, opts);
      }

      function daysBetween(a, b) {
        const msPerDay = 24 * 60 * 60 * 1000;
        return Math.round((b - a) / msPerDay);
      }

      function calculateOvulation() {
        const lastPeriodInput = document.getElementById("lastPeriodDate").value;
        const cycleLengthInput = parseInt(
          document.getElementById("cycleLength").value,
          10
        );
        const lutealLengthInput = parseInt(
          document.getElementById("lutealLength").value,
          10
        );
        const cyclePattern =
          document.querySelector('input[name="cyclePattern"]:checked')?.value ||
          "regular";

        const errorEl = document.getElementById("calcError");
        errorEl.classList.add("hidden");
        errorEl.textContent = "";

        if (!lastPeriodInput) {
          errorEl.textContent =
            "Please select the first day of your last period.";
          errorEl.classList.remove("hidden");
          return;
        }

        if (
          isNaN(cycleLengthInput) ||
          cycleLengthInput < 15 ||
          cycleLengthInput > 60
        ) {
          errorEl.textContent =
            "Please enter a cycle length between 15 and 60 days.";
          errorEl.classList.remove("hidden");
          return;
        }

        if (
          isNaN(lutealLengthInput) ||
          lutealLengthInput < 8 ||
          lutealLengthInput > 18
        ) {
          errorEl.textContent =
            "Please enter a luteal phase length between 8 and 18 days.";
          errorEl.classList.remove("hidden");
          return;
        }

        if (lutealLengthInput >= cycleLengthInput) {
          errorEl.textContent =
            "Luteal phase length must be shorter than total cycle length.";
          errorEl.classList.remove("hidden");
          return;
        }

        const lastPeriodDate = new Date(lastPeriodInput + "T12:00:00"); // avoid timezone edge cases
        if (isNaN(lastPeriodDate.getTime())) {
          errorEl.textContent =
            "Invalid date. Please re-enter the first day of your last period.";
          errorEl.classList.remove("hidden");
          return;
        }

        const follicularLength = cycleLengthInput - lutealLengthInput;

        const ovulationDate = new Date(lastPeriodDate);
        ovulationDate.setDate(ovulationDate.getDate() + follicularLength);

        const fertileStart = new Date(ovulationDate);
        fertileStart.setDate(fertileStart.getDate() - 5);

        const fertileEnd = new Date(ovulationDate);

        const nextPeriodDate = new Date(lastPeriodDate);
        nextPeriodDate.setDate(nextPeriodDate.getDate() + cycleLengthInput);

        // Update UI
        document.getElementById("ovulationDateDisplay").textContent =
          formatDate(ovulationDate);
        document.getElementById(
          "fertileWindowDisplay"
        ).textContent = `${formatDate(fertileStart)} – ${formatDate(
          fertileEnd
        )}`;
        document.getElementById("nextPeriodDisplay").textContent =
          formatDate(nextPeriodDate);

        const today = new Date();
        today.setHours(12, 0, 0, 0);
        let countdownText = "";
        const diff = daysBetween(today, ovulationDate);
        if (diff > 1) {
          countdownText = `Approximately ${diff} days until ovulation.`;
        } else if (diff === 1) {
          countdownText = "Estimated ovulation is tomorrow.";
        } else if (diff === 0) {
          countdownText = "Estimated ovulation is today.";
        } else {
          countdownText = `Estimated ovulation was ${Math.abs(diff)} day${
            Math.abs(diff) === 1 ? "" : "s"
          } ago.`;
        }
        document.getElementById("ovulationCountdown").textContent =
          countdownText;

        const summaryList = document.getElementById("summaryList");
        summaryList.innerHTML = `
      <li>Cycle length: ${cycleLengthInput} days</li>
      <li>Luteal phase: ${lutealLengthInput} days (user input)</li>
      <li>Follicular phase (estimated): ${follicularLength} days</li>
    `;

        const irregularNote = document.getElementById("irregularNote");
        if (cyclePattern === "irregular") {
          irregularNote.classList.remove("hidden");
        } else {
          irregularNote.classList.add("hidden");
        }
      }

      document
        .getElementById("calculateBtn")
        .addEventListener("click", calculateOvulation);

      document.getElementById("resetBtn").addEventListener("click", () => {
        document.getElementById("lastPeriodDate").value = "";
        document.getElementById("cycleLength").value = 28;
        document.getElementById("lutealLength").value = 14;
        document.querySelector(
          'input[name="cyclePattern"][value="regular"]'
        ).checked = true;

        document.getElementById("ovulationDateDisplay").textContent = "—";
        document.getElementById("fertileWindowDisplay").textContent = "—";
        document.getElementById("nextPeriodDisplay").textContent = "—";
        document.getElementById("ovulationCountdown").textContent =
          "Enter your data to see the countdown.";
        document.getElementById("summaryList").innerHTML = `
      <li>Cycle length: —</li>
      <li>Luteal phase: —</li>
      <li>Follicular phase (estimated): —</li>
    `;
        document.getElementById("calcError").classList.add("hidden");
        document.getElementById("calcError").textContent = "";
        document.getElementById("irregularNote").classList.add("hidden");
      });

      // Show hint when irregular is selected
      const cycleRadios = document.querySelectorAll(
        'input[name="cyclePattern"]'
      );
      const irregularHint = document.getElementById("irregularHint");
      cycleRadios.forEach((r) => {
        r.addEventListener("change", () => {
          if (r.value === "irregular" && r.checked) {
            irregularHint.classList.remove("hidden");
          } else if (r.value === "regular" && r.checked) {
            irregularHint.classList.add("hidden");
          }
        });
      });

      // Set default last period date to 14 days ago for convenience
      (function setDefaultDate() {
        const input = document.getElementById("lastPeriodDate");
        if (!input) return;
        const d = new Date();
        d.setDate(d.getDate() - 14);
        const yyyy = d.getFullYear();
        const mm = String(d.getMonth() + 1).padStart(2, "0");
        const dd = String(d.getDate()).padStart(2, "0");
        input.value = `${yyyy}-${mm}-${dd}`;
      })();
    </script>

