    <script>
      // Tab handling
      const tabButtons = document.querySelectorAll(".inv-tab");
      const tabPanels = document.querySelectorAll(".inv-tab-panel");
      tabButtons.forEach((btn) => {
        btn.addEventListener("click", () => {
          const target = btn.getAttribute("data-tab");
          tabButtons.forEach((b) => {
            b.classList.remove("border-blue-600", "text-blue-600");
            b.classList.add("border-transparent", "text-gray-600");
          });
          btn.classList.add("border-blue-600", "text-blue-600");
          btn.classList.remove("border-transparent", "text-gray-600");
          tabPanels.forEach((p) => {
            p.classList.toggle("hidden", p.id !== target);
          });
        });
      });

      function formatCurrency(num) {
        if (!isFinite(num)) return "–";
        return num.toLocaleString(undefined, {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 0,
        });
      }

      // Compound growth calculations
      function calcCompoundGrowth(PV, PMT, rAnnual, years, n) {
        const r = rAnnual / 100;
        const periods = n * years;
        const i = r / n;
        let fvLump = PV * Math.pow(1 + i, periods);
        let fvContrib = 0;
        if (i === 0) {
          fvContrib = PMT * periods;
        } else {
          fvContrib = PMT * ((Math.pow(1 + i, periods) - 1) / i);
        }
        return { fv: fvLump + fvContrib, fvLump, fvContrib, periods, i };
      }

      // Tab 1: Compound growth
      const cgInitial = document.getElementById("cg-initial");
      const cgMonthly = document.getElementById("cg-monthly");
      const cgRate = document.getElementById("cg-rate");
      const cgYears = document.getElementById("cg-years");
      const cgFreq = document.getElementById("cg-frequency");
      const cgRateRange = document.getElementById("cg-rate-range");
      const cgRateRangeLabel = document.getElementById("cg-rate-range-label");
      const cgYearsRange = document.getElementById("cg-years-range");
      const cgYearsRangeLabel = document.getElementById("cg-years-range-label");

      cgRateRange.addEventListener("input", () => {
        cgRate.value = cgRateRange.value;
        cgRateRangeLabel.textContent =
          parseFloat(cgRateRange.value).toFixed(1) + "%";
      });
      cgRate.addEventListener("input", () => {
        const v = parseFloat(cgRate.value) || 0;
        cgRateRange.value = Math.min(
          Math.max(v, cgRateRange.min),
          cgRateRange.max
        );
        cgRateRangeLabel.textContent = v.toFixed(1) + "%";
      });
      cgYearsRange.addEventListener("input", () => {
        cgYears.value = cgYearsRange.value;
        cgYearsRangeLabel.textContent = cgYearsRange.value + " years";
      });
      cgYears.addEventListener("input", () => {
        const v = parseInt(cgYears.value, 10) || 0;
        cgYearsRange.value = Math.min(
          Math.max(v, cgYearsRange.min),
          cgYearsRange.max
        );
        cgYearsRangeLabel.textContent = v + " years";
      });

      document.getElementById("cg-calc").addEventListener("click", () => {
        const PV = parseFloat(cgInitial.value) || 0;
        const PMT = parseFloat(cgMonthly.value) || 0;
        const r = parseFloat(cgRate.value) || 0;
        const years = parseFloat(cgYears.value) || 0;
        const n = parseInt(cgFreq.value, 10) || 1;

        const { fv, fvLump, fvContrib, periods, i } = calcCompoundGrowth(
          PV,
          PMT,
          r,
          years,
          n
        );
        const totalContrib = PV + PMT * periods;
        const growth = fv - totalContrib;

        document.getElementById("cg-total-contrib").textContent =
          formatCurrency(totalContrib);
        document.getElementById("cg-final-value").textContent =
          formatCurrency(fv);
        document.getElementById("cg-total-growth").textContent =
          formatCurrency(growth);

        // Effective annualized rate based on total contributions treated as if invested evenly
        let effRateText = "–";
        if (years > 0 && totalContrib > 0 && fv > 0) {
          const approxRate = Math.pow(fv / totalContrib, 1 / years) - 1;
          if (isFinite(approxRate))
            effRateText = (approxRate * 100).toFixed(2) + "% (approx.)";
        }
        document.getElementById("cg-effective-rate").textContent = effRateText;

        // Build simple yearly growth table
        const tableDiv = document.getElementById("cg-growth-table");
        let html =
          '<table class="min-w-full text-left"><thead><tr class="bg-gray-100"><th class="px-2 py-1">Year</th><th class="px-2 py-1">Balance</th><th class="px-2 py-1">Total Contrib.</th></tr></thead><tbody>';
        const yearlyN = n;
        let balance = PV;
        let contribSoFar = PV;
        for (let year = 1; year <= years; year++) {
          for (let p = 0; p < yearlyN; p++) {
            balance = balance * (1 + i) + PMT;
            contribSoFar += PMT;
          }
          html += `<tr class="${
            year % 2 === 0 ? "bg-gray-50" : ""
          }"><td class="px-2 py-1">${year}</td><td class="px-2 py-1">${formatCurrency(
            balance
          )}</td><td class="px-2 py-1">${formatCurrency(
            contribSoFar
          )}</td></tr>`;
        }
        html += "</tbody></table>";
        tableDiv.innerHTML = html;
      });

      // Initial calculation
      document.getElementById("cg-calc").click();

      // Tab 2: Goal-based investing
      document.getElementById("gb-calc").addEventListener("click", () => {
        const target =
          parseFloat(document.getElementById("gb-target").value) || 0;
        const current =
          parseFloat(document.getElementById("gb-current").value) || 0;
        const years =
          parseFloat(document.getElementById("gb-years").value) || 0;
        const rate =
          (parseFloat(document.getElementById("gb-rate").value) || 0) / 100;
        const freq =
          parseInt(document.getElementById("gb-frequency").value, 10) || 12;

        if (target <= 0 || years <= 0) {
          document.getElementById("gb-required").textContent = "–";
          return;
        }

        const periods = years * freq;
        const i = rate / freq;

        // Future value of current savings
        const fvCurrent = current * Math.pow(1 + i, periods);

        const remaining = Math.max(target - fvCurrent, 0);
        let payment = 0;
        if (remaining === 0) {
          payment = 0;
        } else if (i === 0) {
          payment = remaining / periods;
        } else {
          payment = (remaining * i) / (Math.pow(1 + i, periods) - 1);
        }

        const totalContrib = payment * periods;

        const labelMap = {
          12: "month",
          26: "bi‑week",
          52: "week",
          1: "year",
        };
        const unit = labelMap[freq] || "period";

        document.getElementById("gb-required").textContent = `${formatCurrency(
          payment
        )} / ${unit}`;
        document.getElementById("gb-fv-current").textContent =
          formatCurrency(fvCurrent);
        document.getElementById("gb-total-contrib").textContent =
          formatCurrency(totalContrib);
      });

      // Tab 3: Strategy comparison
      document.getElementById("sc-calc").addEventListener("click", () => {
        function compute(labelIdPrefix) {
          const initial =
            parseFloat(
              document.getElementById(labelIdPrefix + "-initial").value
            ) || 0;
          const monthly =
            parseFloat(
              document.getElementById(labelIdPrefix + "-monthly").value
            ) || 0;
          const rate =
            parseFloat(
              document.getElementById(labelIdPrefix + "-rate").value
            ) || 0;
          const years =
            parseFloat(
              document.getElementById(labelIdPrefix + "-years").value
            ) || 0;
          const { fv, periods } = calcCompoundGrowth(
            initial,
            monthly,
            rate,
            years,
            12
          );
          const totalContrib = initial + monthly * periods;
          return { fv, totalContrib, growth: fv - totalContrib };
        }

        const a = compute("sc-a");
        const b = compute("sc-b");

        document.getElementById("sc-a-title").textContent =
          document.getElementById("sc-a-label").value || "Strategy A";
        document.getElementById("sc-b-title").textContent =
          document.getElementById("sc-b-label").value || "Strategy B";

        document.getElementById("sc-a-contrib").textContent = formatCurrency(
          a.totalContrib
        );
        document.getElementById("sc-a-final").textContent = formatCurrency(
          a.fv
        );
        document.getElementById("sc-a-growth").textContent = formatCurrency(
          a.growth
        );

        document.getElementById("sc-b-contrib").textContent = formatCurrency(
          b.totalContrib
        );
        document.getElementById("sc-b-final").textContent = formatCurrency(
          b.fv
        );
        document.getElementById("sc-b-growth").textContent = formatCurrency(
          b.growth
        );

        const diffFinal = a.fv - b.fv;
        const diffText =
          diffFinal === 0
            ? "Both strategies lead to the same projected final value under these assumptions."
            : diffFinal > 0
            ? `Strategy A ends with about ${formatCurrency(
                Math.abs(diffFinal)
              )} more than Strategy B.`
            : `Strategy B ends with about ${formatCurrency(
                Math.abs(diffFinal)
              )} more than Strategy A.`;
        document.getElementById("sc-diff").textContent = diffText;

        document.getElementById("sc-results").classList.remove("hidden");
      });

      document.querySelectorAll("#faq-container details").forEach((d) => {
        d.addEventListener("toggle", () => {
          if (d.open) {
            document
              .querySelectorAll("#faq-container details")
              .forEach((other) => {
                if (other !== d) other.removeAttribute("open");
              });
          }
        });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
