    <script>
      // Core parameters for 2024 & 2025 (can be updated as IRS publishes final numbers)
      const seConfig = {
        2024: {
          ssWageBase: 168600,
          addlMedicareThresholds: {
            single: 200000,
            mfj: 250000,
            mfs: 125000,
          },
        },
        2025: {
          // Provisional / estimated values; update when official
          ssWageBase: 174000,
          addlMedicareThresholds: {
            single: 200000,
            mfj: 250000,
            mfs: 125000,
          },
        },
      };

      function formatMoney(value) {
        if (!isFinite(value)) return "$0.00";
        return value.toLocaleString("en-US", {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 2,
        });
      }

      function getNumber(id) {
        const el = document.getElementById(id);
        if (!el) return 0;
        const v = parseFloat(el.value.replace(/,/g, ""));
        return isNaN(v) ? 0 : v;
      }

      function calculateSelfEmploymentTax() {
        const year = document.getElementById("tax-year").value;
        const filingStatus = document.getElementById("filing-status").value;
        const includeAddlMedicare = document.getElementById(
          "include-addl-medicare"
        ).value;

        const netSeIncome = Math.max(0, getNumber("net-se-income"));
        const w2Wages = Math.max(0, getNumber("w2-wages"));
        const otherEarned = Math.max(0, getNumber("other-earned-income"));

        const cfg = seConfig[year] || seConfig[2024];
        const ssWageBase = cfg.ssWageBase;
        const thresholds = cfg.addlMedicareThresholds;

        // 1. Net earnings from self-employment (92.35%)
        const netEarnings = netSeIncome * 0.9235;

        // 2. Social Security portion (12.4%)
        const ssBaseAvailable = Math.max(0, ssWageBase - w2Wages);
        const ssEarnings = Math.min(netEarnings, ssBaseAvailable);
        const ssTax = ssEarnings * 0.124;

        // 3. Medicare portion (2.9%)
        const medicareTax = netEarnings * 0.029;

        // 4. Additional Medicare tax (0.9%) estimate
        let addlMedicareTax = 0;
        if (includeAddlMedicare === "auto") {
          const threshold = thresholds[filingStatus] || thresholds.single;
          const combinedIncome = w2Wages + netEarnings + otherEarned;
          const excess = Math.max(0, combinedIncome - threshold);
          addlMedicareTax = excess * 0.009;
        }

        const totalMedicare = medicareTax + addlMedicareTax;
        const totalSeTax = ssTax + totalMedicare;
        const deductibleHalf = totalSeTax / 2;

        // Update UI
        document.getElementById("net-earnings").textContent =
          formatMoney(netEarnings);
        document.getElementById("ss-base-used").textContent =
          formatMoney(ssEarnings);
        document.getElementById("ss-tax").textContent = formatMoney(ssTax);
        document.getElementById("medicare-tax").textContent =
          formatMoney(medicareTax);
        document.getElementById("addl-medicare-tax").textContent =
          formatMoney(addlMedicareTax);
        document.getElementById("total-medicare-tax").textContent =
          formatMoney(totalMedicare);
        document.getElementById("total-se-tax").textContent =
          formatMoney(totalSeTax);
        document.getElementById("deductible-half").textContent =
          formatMoney(deductibleHalf);
      }

      document.addEventListener("DOMContentLoaded", function () {
        const calcBtn = document.getElementById("calculate-btn");
        const resetBtn = document.getElementById("reset-btn");

        if (calcBtn) {
          calcBtn.addEventListener("click", calculateSelfEmploymentTax);
        }

        if (resetBtn) {
          resetBtn.addEventListener("click", function () {
            document.getElementById("tax-year").value = "2024";
            document.getElementById("filing-status").value = "single";
            document.getElementById("net-se-income").value = "60000";
            document.getElementById("w2-wages").value = "0";
            document.getElementById("include-addl-medicare").value = "auto";
            document.getElementById("other-earned-income").value = "0";
            calculateSelfEmploymentTax();
          });
        }

        // Auto-recalculate on input changes for a smoother UX
        [
          "tax-year",
          "filing-status",
          "net-se-income",
          "w2-wages",
          "include-addl-medicare",
          "other-earned-income",
        ].forEach((id) => {
          const el = document.getElementById(id);
          if (!el) return;
          el.addEventListener("change", calculateSelfEmploymentTax);
          el.addEventListener("input", function () {
            // Debounce quick typing
            clearTimeout(el._debounceTimer);
            el._debounceTimer = setTimeout(calculateSelfEmploymentTax, 300);
          });
        });

        // Initial calculation
        calculateSelfEmploymentTax();
      });

      // Mobile menu toggle
      document.addEventListener("DOMContentLoaded", function () {
        const toggle = document.getElementById("mobile-menu-toggle");
        const menu = document.getElementById("mobile-menu");
        if (toggle && menu) {
          toggle.addEventListener("click", () => {
            const isHidden = menu.classList.contains("hidden");
            menu.classList.toggle("hidden", !isHidden);
            toggle.setAttribute("aria-expanded", isHidden ? "true" : "false");
          });
        }
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
