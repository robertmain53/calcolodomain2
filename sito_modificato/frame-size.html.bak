    <script>
      // Tab switching
      const tabPicture = document.getElementById("tab-picture");
      const tabBike = document.getElementById("tab-bike");
      const panelPicture = document.getElementById("panel-picture");
      const panelBike = document.getElementById("panel-bike");

      function activateTab(tab) {
        if (tab === "picture") {
          tabPicture.classList.add("tab-active");
          tabPicture.classList.remove("text-gray-600");
          tabBike.classList.remove("tab-active");
          tabBike.classList.add("text-gray-600");
          panelPicture.classList.remove("hidden");
          panelBike.classList.add("hidden");
        } else {
          tabBike.classList.add("tab-active");
          tabBike.classList.remove("text-gray-600");
          tabPicture.classList.remove("tab-active");
          tabPicture.classList.add("text-gray-600");
          panelBike.classList.remove("hidden");
          panelPicture.classList.add("hidden");
        }
      }

      tabPicture.addEventListener("click", () => activateTab("picture"));
      tabBike.addEventListener("click", () => activateTab("bike"));

      // Utility functions
      function toInches(value, unit) {
        if (!value || isNaN(value)) return 0;
        return unit === "cm" ? value / 2.54 : value;
      }
      function toCm(value, unit) {
        if (!value || isNaN(value)) return 0;
        return unit === "in" ? value * 2.54 : value;
      }
      function round(value, decimals) {
        const factor = Math.pow(10, decimals);
        return Math.round(value * factor) / factor;
      }

      // Standard frame sizes (inches)
      const standardFrames = [
        [4, 6],
        [5, 7],
        [6, 8],
        [8, 10],
        [8.5, 11],
        [11, 14],
        [12, 16],
        [14, 18],
        [16, 20],
        [18, 24],
        [20, 24],
        [20, 30],
        [24, 30],
        [24, 36],
        // A-series approximations in inches
        [5.8, 8.3],
        [8.3, 11.7],
        [11.7, 16.5],
        [16.5, 23.4],
        [23.4, 33.1],
      ];

      function findClosestStandard(widthIn, heightIn, allowRotation) {
        const results = [];
        const targetW = Math.max(widthIn, 0);
        const targetH = Math.max(heightIn, 0);

        standardFrames.forEach(([w, h]) => {
          const candidates = [];
          // same orientation
          if (w >= targetW && h >= targetH) {
            candidates.push({ w, h, rotated: false });
          }
          // rotated orientation
          if (allowRotation && h >= targetW && w >= targetH) {
            candidates.push({ w: h, h: w, rotated: true });
          }

          candidates.forEach((c) => {
            const extraArea = c.w * c.h - targetW * targetH;
            const extraW = c.w - targetW;
            const extraH = c.h - targetH;
            const score = extraArea + Math.abs(extraW) + Math.abs(extraH);
            results.push({
              frameW: c.w,
              frameH: c.h,
              rotated: c.rotated,
              score,
              extraW,
              extraH,
            });
          });
        });

        results.sort((a, b) => a.score - b.score);
        return results.slice(0, 5);
      }

      // Picture frame calculation
      document.getElementById("calc-picture").addEventListener("click", () => {
        const artW = parseFloat(document.getElementById("art-width").value);
        const artH = parseFloat(document.getElementById("art-height").value);
        const artWUnit = document.getElementById("art-width-unit").value;
        const artHUnit = document.getElementById("art-height-unit").value;

        if (!artW || !artH || artW <= 0 || artH <= 0) {
          alert("Please enter a positive width and height for your artwork.");
          return;
        }

        const matBorder =
          parseFloat(document.getElementById("mat-border").value) || 0;
        const matUnit = document.getElementById("mat-border-unit").value;
        const frameBorder =
          parseFloat(document.getElementById("frame-border").value) || 0;
        const frameUnit = document.getElementById("frame-border-unit").value;

        const orientation = document.querySelector(
          'input[name="orientation"]:checked'
        ).value;
        const allowRotation = document.getElementById("allow-rotation").checked;

        // Convert everything to inches
        let artWIn = toInches(artW, artWUnit);
        let artHIn = toInches(artH, artHUnit);

        if (orientation === "portrait" && artWIn > artHIn) {
          [artWIn, artHIn] = [artHIn, artWIn];
        } else if (orientation === "landscape" && artHIn > artWIn) {
          [artWIn, artHIn] = [artHIn, artWIn];
        }

        const matBorderIn = toInches(matBorder, matUnit);
        const frameBorderIn = toInches(frameBorder, frameUnit);

        // Assume mat overlap of 0.1 in each side (2.5 mm)
        const overlap = 0.1;

        const matOpeningW = Math.max(artWIn - 2 * overlap, 0);
        const matOpeningH = Math.max(artHIn - 2 * overlap, 0);

        const frameOpeningW = artWIn + 2 * matBorderIn;
        const frameOpeningH = artHIn + 2 * matBorderIn;

        const outerW = frameOpeningW + 2 * frameBorderIn;
        const outerH = frameOpeningH + 2 * frameBorderIn;

        const pictureResults = document.getElementById("picture-results");
        const summary = document.getElementById("picture-summary");
        const artSizeOut = document.getElementById("art-size-out");
        const matOpeningOut = document.getElementById("mat-opening-out");
        const frameOpeningOut = document.getElementById("frame-opening-out");
        const frameOuterOut = document.getElementById("frame-outer-out");
        const standardList = document.getElementById("standard-frame-list");

        const artWcm = round(artWIn * 2.54, 1);
        const artHcm = round(artHIn * 2.54, 1);
        const frameWcm = round(frameOpeningW * 2.54, 1);
        const frameHcm = round(frameOpeningH * 2.54, 1);

        summary.textContent =
          `For artwork ${round(artWIn, 2)} × ${round(artHIn, 2)} in ` +
          `(${artWcm} × ${artHcm} cm) with a ${round(
            matBorderIn,
            2
          )} in mat border, ` +
          `choose a frame with an opening of about ${round(
            frameOpeningW,
            2
          )} × ${round(frameOpeningH, 2)} in ` +
          `(${frameWcm} × ${frameHcm} cm).`;

        artSizeOut.textContent = `${round(artWIn, 2)} × ${round(
          artHIn,
          2
        )} in (${artWcm} × ${artHcm} cm)`;

        matOpeningOut.textContent =
          `${round(matOpeningW, 2)} × ${round(matOpeningH, 2)} in ` +
          `(${round(matOpeningW * 2.54, 1)} × ${round(
            matOpeningH * 2.54,
            1
          )} cm)`;

        frameOpeningOut.textContent =
          `${round(frameOpeningW, 2)} × ${round(frameOpeningH, 2)} in ` +
          `(${frameWcm} × ${frameHcm} cm)`;

        frameOuterOut.textContent =
          `${round(outerW, 2)} × ${round(outerH, 2)} in ` +
          `(${round(outerW * 2.54, 1)} × ${round(outerH * 2.54, 1)} cm)`;

        // Standard frames
        const closest = findClosestStandard(
          frameOpeningW,
          frameOpeningH,
          allowRotation
        );
        standardList.innerHTML = "";
        if (closest.length === 0) {
          const li = document.createElement("li");
          li.textContent =
            "No standard sizes found that are larger than your required opening.";
          standardList.appendChild(li);
        } else {
          closest.forEach((item) => {
            const li = document.createElement("li");
            const w = round(item.frameW, 2);
            const h = round(item.frameH, 2);
            const wcm = round(item.frameW * 2.54, 1);
            const hcm = round(item.frameH * 2.54, 1);
            const extraW = round(item.extraW, 2);
            const extraH = round(item.extraH, 2);
            li.innerHTML =
              `<span class="font-medium">${w} × ${h} in</span> ` +
              `(${wcm} × ${hcm} cm)` +
              ` – extra border: ${extraW.toFixed(2)} in width, ${extraH.toFixed(
                2
              )} in height` +
              (item.rotated
                ? ' <span class="text-xs text-gray-500">(requires rotating artwork)</span>'
                : "");
            standardList.appendChild(li);
          });
        }

        pictureResults.classList.remove("hidden");
      });

      // Bike frame calculation
      const bikeTypeButtons = document.querySelectorAll(".bike-type-btn");
      let currentBikeType = "road";
      bikeTypeButtons.forEach((btn) => {
        btn.addEventListener("click", () => {
          currentBikeType = btn.getAttribute("data-bike-type");
          bikeTypeButtons.forEach((b) => {
            if (b === btn) {
              b.classList.add("bg-blue-600", "text-white");
              b.classList.remove("bg-white", "text-gray-700");
            } else {
              b.classList.remove("bg-blue-600", "text-white");
              b.classList.add("bg-white", "text-gray-700");
            }
          });
        });
      });

      function letterSizeFromCm(cm, type) {
        // Very approximate mapping
        let size = "M";
        if (type === "road") {
          if (cm < 50) size = "XS";
          else if (cm < 53) size = "S";
          else if (cm < 56) size = "M";
          else if (cm < 59) size = "L";
          else size = "XL";
        } else if (type === "mtb") {
          if (cm < 38) size = "XS";
          else if (cm < 42) size = "S";
          else if (cm < 46) size = "M";
          else if (cm < 50) size = "L";
          else size = "XL";
        } else {
          // hybrid
          if (cm < 48) size = "XS";
          else if (cm < 52) size = "S";
          else if (cm < 56) size = "M";
          else if (cm < 60) size = "L";
          else size = "XL";
        }
        return size;
      }

      function rangeFromCm(cm, type) {
        let min = cm - 2;
        let max = cm + 2;
        if (type === "mtb") {
          min = cm - 3;
          max = cm + 3;
        }
        return [Math.max(0, Math.round(min)), Math.round(max)];
      }

      document.getElementById("calc-bike").addEventListener("click", () => {
        const heightVal = parseFloat(
          document.getElementById("rider-height").value
        );
        const heightUnit = document.getElementById("rider-height-unit").value;
        const inseamVal = parseFloat(document.getElementById("inseam").value);
        const inseamUnit = document.getElementById("inseam-unit").value;

        if (!heightVal || heightVal <= 0 || !inseamVal || inseamVal <= 0) {
          alert("Please enter your height and inseam length.");
          return;
        }

        const heightCm = toCm(heightVal, heightUnit);
        const inseamCm = toCm(inseamVal, inseamUnit);

        let factor;
        if (currentBikeType === "road") factor = 0.67;
        else if (currentBikeType === "mtb") factor = 0.59;
        else factor = 0.63;

        const frameCm = inseamCm * factor;
        const frameCmRounded = Math.round(frameCm);
        const frameIn = frameCm / 2.54;
        const frameInRounded = round(frameIn, 1);

        // Approximate effective top tube: similar to seat tube for road/hybrid, slightly longer for MTB
        let topTubeCm = frameCm;
        if (currentBikeType === "mtb") topTubeCm = frameCm + 2;
        const topTubeIn = topTubeCm / 2.54;

        const letter = letterSizeFromCm(frameCmRounded, currentBikeType);
        const [minCm, maxCm] = rangeFromCm(frameCmRounded, currentBikeType);

        const bikeResults = document.getElementById("bike-results");
        const summary = document.getElementById("bike-summary");
        const seatTubeOut = document.getElementById("bike-seat-tube-out");
        const topTubeOut = document.getElementById("bike-top-tube-out");
        const letterOut = document.getElementById("bike-letter-out");
        const rangeOut = document.getElementById("bike-range-out");

        summary.textContent =
          `For a rider height of ${Math.round(
            heightCm
          )} cm and inseam ${Math.round(inseamCm)} cm ` +
          `on a ${
            currentBikeType === "mtb" ? "mountain" : currentBikeType
          } bike, a frame around ` +
          `${frameCmRounded} cm (${frameInRounded} in) is a good starting point.`;

        seatTubeOut.textContent = `${frameCmRounded} cm (${frameInRounded} in) seat tube`;
        topTubeOut.textContent = `${Math.round(topTubeCm)} cm (${round(
          topTubeIn,
          1
        )} in) effective top tube (approx.)`;
        letterOut.textContent = `${letter} (varies by brand)`;
        rangeOut.textContent = `${minCm}–${maxCm} cm frame size is usually acceptable for most riders of your height.`;

        bikeResults.classList.remove("hidden");
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
