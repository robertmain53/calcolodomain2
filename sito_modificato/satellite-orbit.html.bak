    <script>
      // Constants
      const MU = 3.986004418e14; // m^3/s^2
      const EARTH_RADIUS_M = 6371000; // m
      const SIDEREAL_DAY_S = 23 * 3600 + 56 * 60 + 4; // s

      const modeAltitudeBtn = document.getElementById("mode-altitude");
      const modePeriodBtn = document.getElementById("mode-period");
      const altitudeGroup = document.getElementById("altitude-input-group");
      const periodGroup = document.getElementById("period-input-group");
      const unitSelect = document.getElementById("unit-system");
      const altitudeInput = document.getElementById("altitude");
      const altitudeUnitSpan = document.getElementById("altitude-unit");
      const periodH = document.getElementById("period-hours");
      const periodM = document.getElementById("period-minutes");
      const periodS = document.getElementById("period-seconds");
      const inclinationInput = document.getElementById("inclination");
      const eccInput = document.getElementById("eccentricity");
      const computeBtn = document.getElementById("compute-btn");
      const errorMessage = document.getElementById("error-message");

      const orbitTypeBadge = document.getElementById("orbit-type-badge");
      const orbitFamilyEl = document.getElementById("orbit-family");
      const orbitSpecialEl = document.getElementById("orbit-special");
      const outAltitude = document.getElementById("out-altitude");
      const outRadius = document.getElementById("out-radius");
      const outPeriod = document.getElementById("out-period");
      const outVelocity = document.getElementById("out-velocity");
      const outGroundSpeed = document.getElementById("out-groundspeed");
      const inclinationLabelEl = document.getElementById("inclination-label");

      let inputMode = "altitude"; // 'altitude' or 'period'

      function setMode(mode) {
        inputMode = mode;
        if (mode === "altitude") {
          altitudeGroup.classList.remove("hidden");
          periodGroup.classList.add("hidden");
          modeAltitudeBtn.classList.add(
            "bg-blue-600",
            "text-white",
            "border-blue-600"
          );
          modeAltitudeBtn.classList.remove("border-gray-300", "text-gray-700");
          modePeriodBtn.classList.remove(
            "bg-blue-600",
            "text-white",
            "border-blue-600"
          );
          modePeriodBtn.classList.add("border-gray-300", "text-gray-700");
        } else {
          altitudeGroup.classList.add("hidden");
          periodGroup.classList.remove("hidden");
          modePeriodBtn.classList.add(
            "bg-blue-600",
            "text-white",
            "border-blue-600"
          );
          modePeriodBtn.classList.remove("border-gray-300", "text-gray-700");
          modeAltitudeBtn.classList.remove(
            "bg-blue-600",
            "text-white",
            "border-blue-600"
          );
          modeAltitudeBtn.classList.add("border-gray-300", "text-gray-700");
        }
        clearOutputs();
      }

      function setUnits(system) {
        if (system === "metric") {
          altitudeUnitSpan.textContent = "km";
        } else {
          altitudeUnitSpan.textContent = "mi";
        }
        clearOutputs();
      }

      function clearOutputs() {
        errorMessage.classList.add("hidden");
        orbitTypeBadge.textContent = "–";
        orbitTypeBadge.className = "badge bg-gray-200 text-gray-800";
        orbitFamilyEl.textContent = "–";
        orbitSpecialEl.textContent = "–";
        outAltitude.textContent = "–";
        outRadius.textContent = "–";
        outPeriod.textContent = "–";
        outVelocity.textContent = "–";
        outGroundSpeed.textContent = "–";
        inclinationLabelEl.textContent = "–";
      }

      function formatNumber(value, decimals = 2) {
        if (!isFinite(value)) return "–";
        return value.toLocaleString(undefined, {
          maximumFractionDigits: decimals,
          minimumFractionDigits: decimals,
        });
      }

      function classifyOrbit(altitudeKm, periodS, inclinationDeg, ecc) {
        let family = "Unknown";
        let badgeClass = "bg-gray-200 text-gray-800";
        let special = [];

        if (altitudeKm >= 160 && altitudeKm < 2000) {
          family = "LEO (Low Earth Orbit)";
          badgeClass = "bg-blue-100 text-blue-800";
        } else if (altitudeKm >= 2000 && altitudeKm < 35786) {
          family = "MEO (Medium Earth Orbit)";
          badgeClass = "bg-green-100 text-green-800";
        } else if (altitudeKm >= 35786 - 500 && altitudeKm <= 35786 + 500) {
          family = "Near GEO";
          badgeClass = "bg-yellow-100 text-yellow-800";
        } else if (altitudeKm > 35786) {
          family = "HEO (High Earth Orbit)";
          badgeClass = "bg-purple-100 text-purple-800";
        }

        const geoTolerance = 0.02 * SIDEREAL_DAY_S;
        if (Math.abs(periodS - SIDEREAL_DAY_S) <= geoTolerance) {
          if (inclinationDeg !== null && Math.abs(inclinationDeg) < 5) {
            special.push("Geostationary equatorial orbit (GEO)");
            family = "GEO (Geostationary Orbit)";
            badgeClass = "bg-yellow-100 text-yellow-800";
          } else {
            special.push("Geosynchronous orbit (GSO)");
          }
        }

        if (inclinationDeg !== null) {
          if (inclinationDeg < 10) {
            special.push("Equatorial");
          } else if (inclinationDeg > 170) {
            special.push("Retrograde equatorial");
          } else if (inclinationDeg >= 70 && inclinationDeg <= 110) {
            special.push("Polar");
          } else if (inclinationDeg > 90 && inclinationDeg < 180) {
            special.push("Retrograde");
          }
          if (
            inclinationDeg > 96 &&
            inclinationDeg < 99 &&
            altitudeKm >= 500 &&
            altitudeKm <= 900
          ) {
            special.push("Sun-synchronous-like");
          }
        }

        if (
          ecc !== null &&
          ecc >= 0.5 &&
          Math.abs(periodS - 12 * 3600) < 0.5 * 3600 &&
          inclinationDeg !== null &&
          Math.abs(inclinationDeg - 63.4) < 5
        ) {
          special.push("Molniya-like highly elliptical orbit");
          family = "HEO (High Earth Orbit)";
          badgeClass = "bg-purple-100 text-purple-800";
        }

        return { family, badgeClass, special: special.join(", ") || "None" };
      }

      function inclinationLabel(inclinationDeg) {
        if (inclinationDeg === null || isNaN(inclinationDeg))
          return "Not specified";
        if (inclinationDeg < 10) return "Equatorial";
        if (inclinationDeg >= 70 && inclinationDeg <= 110) return "Polar";
        if (inclinationDeg > 90 && inclinationDeg < 180) return "Retrograde";
        return "Mid-inclination";
      }

      function computeOrbit() {
        clearOutputs();
        const unitSystem = unitSelect.value;
        let r_m = null;
        let h_m = null;
        let period_s = null;

        try {
          if (inputMode === "altitude") {
            const altVal = parseFloat(altitudeInput.value);
            if (isNaN(altVal) || altVal <= 0) {
              throw new Error("Please enter a positive altitude.");
            }
            if (unitSystem === "metric") {
              h_m = altVal * 1000;
            } else {
              h_m = altVal * 1609.344;
            }
            r_m = EARTH_RADIUS_M + h_m;
            period_s = 2 * Math.PI * Math.sqrt(Math.pow(r_m, 3) / MU);
          } else {
            const h = parseFloat(periodH.value) || 0;
            const m = parseFloat(periodM.value) || 0;
            const s = parseFloat(periodS.value) || 0;
            period_s = h * 3600 + m * 60 + s;
            if (!isFinite(period_s) || period_s <= 0) {
              throw new Error("Please enter a valid positive orbital period.");
            }
            r_m = Math.cbrt(MU * Math.pow(period_s / (2 * Math.PI), 2));
            h_m = r_m - EARTH_RADIUS_M;
            if (h_m <= 0) {
              throw new Error(
                "The resulting orbit radius is inside Earth. Increase the period."
              );
            }
          }

          const v_mps = Math.sqrt(MU / r_m);
          const groundSpeed_mps = (2 * Math.PI * EARTH_RADIUS_M) / period_s;

          const altitudeKm = h_m / 1000;
          const radiusKm = r_m / 1000;

          const incVal =
            inclinationInput.value.trim() === ""
              ? null
              : parseFloat(inclinationInput.value);
          const eccVal =
            eccInput.value.trim() === "" ? null : parseFloat(eccInput.value);

          if (
            incVal !== null &&
            (isNaN(incVal) || incVal < 0 || incVal > 180)
          ) {
            throw new Error("Inclination must be between 0° and 180°.");
          }
          if (eccVal !== null && (isNaN(eccVal) || eccVal < 0 || eccVal >= 1)) {
            throw new Error(
              "Eccentricity must be between 0 and 1 (non-inclusive)."
            );
          }

          const classification = classifyOrbit(
            altitudeKm,
            period_s,
            incVal,
            eccVal
          );

          orbitTypeBadge.textContent = classification.family.split(" ")[0];
          orbitTypeBadge.className = "badge " + classification.badgeClass;
          orbitFamilyEl.textContent = classification.family;
          orbitSpecialEl.textContent = classification.special;

          if (unitSystem === "metric") {
            outAltitude.textContent = formatNumber(altitudeKm, 1) + " km";
            outRadius.textContent = formatNumber(radiusKm, 1) + " km";
            outVelocity.textContent = formatNumber(v_mps / 1000, 3) + " km/s";
            outGroundSpeed.textContent =
              formatNumber(groundSpeed_mps / 1000, 3) + " km/s";
          } else {
            const altitudeMi = h_m / 1609.344;
            const radiusMi = r_m / 1609.344;
            const v_ftps = v_mps * 3.28084;
            const ground_ftps = groundSpeed_mps * 3.28084;
            outAltitude.textContent = formatNumber(altitudeMi, 1) + " mi";
            outRadius.textContent = formatNumber(radiusMi, 1) + " mi";
            outVelocity.textContent = formatNumber(v_ftps, 1) + " ft/s";
            outGroundSpeed.textContent = formatNumber(ground_ftps, 1) + " ft/s";
          }

          const hours = Math.floor(period_s / 3600);
          const minutes = Math.floor((period_s % 3600) / 60);
          const seconds = period_s % 60;
          outPeriod.textContent = `${hours} h ${minutes} min ${formatNumber(
            seconds,
            1
          )} s`;

          inclinationLabelEl.textContent = inclinationLabel(incVal);
        } catch (err) {
          errorMessage.textContent = err.message;
          errorMessage.classList.remove("hidden");
        }
      }

      modeAltitudeBtn.addEventListener("click", () => setMode("altitude"));
      modePeriodBtn.addEventListener("click", () => setMode("period"));
      unitSelect.addEventListener("change", (e) => setUnits(e.target.value));
      computeBtn.addEventListener("click", computeOrbit);

      document.querySelectorAll(".preset-btn").forEach((btn) => {
        btn.addEventListener("click", () => {
          const preset = btn.getAttribute("data-preset");
          unitSelect.value = "metric";
          setUnits("metric");
          if (preset === "leo") {
            setMode("altitude");
            altitudeInput.value = 500;
            inclinationInput.value = 98;
            eccInput.value = 0;
          } else if (preset === "gps") {
            setMode("altitude");
            altitudeInput.value = 20200;
            inclinationInput.value = 55;
            eccInput.value = 0;
          } else if (preset === "geo") {
            setMode("period");
            periodH.value = 23;
            periodM.value = 56;
            periodS.value = 0;
            inclinationInput.value = 0;
            eccInput.value = 0;
          }
          computeOrbit();
        });
      });

      setMode("altitude");
      setUnits("metric");

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
