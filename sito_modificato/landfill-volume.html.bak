    <script>
      // State
      let unitSystem = "metric"; // 'metric' or 'imperial'
      let geometryMode = "simple"; // 'simple' or 'frustum'

      const el = (id) => document.getElementById(id);

      function setUnitLabels() {
        if (unitSystem === "metric") {
          el("label-footprint-unit").textContent = "(m²)";
          el("label-depth-unit").textContent = "(m)";
          el("label-bottom-unit").textContent = "(m²)";
          el("label-top-unit").textContent = "(m²)";
          el("label-frustum-depth-unit").textContent = "(m)";
          el("label-density-unit").textContent = "(tonnes/m³)";
          el("label-inflow-unit").textContent = "(tonnes/year)";
          el("density-hint").textContent =
            "Typical MSW: 0.4–0.8 tonnes/m³ (≈ 250–500 lb/yd³).";
        } else {
          el("label-footprint-unit").textContent = "(acres)";
          el("label-depth-unit").textContent = "(ft)";
          el("label-bottom-unit").textContent = "(acres)";
          el("label-top-unit").textContent = "(acres)";
          el("label-frustum-depth-unit").textContent = "(ft)";
          el("label-density-unit").textContent = "(tons/yd³)";
          el("label-inflow-unit").textContent = "(tons/year)";
          el("density-hint").textContent =
            "Typical MSW: 500–1,200 lb/yd³ (≈ 0.3–0.7 tonnes/m³).";
        }
      }

      function setActiveTab(buttonOn, buttonOff) {
        buttonOn.classList.add("tab-active");
        buttonOff.classList.remove("tab-active");
      }

      function resetForm() {
        unitSystem = "metric";
        geometryMode = "simple";
        el("unit-metric").classList.add("tab-active");
        el("unit-imperial").classList.remove("tab-active");
        el("tab-simple").classList.add("tab-active");
        el("tab-frustum").classList.remove("tab-active");
        el("simple-geometry").classList.remove("hidden");
        el("frustum-geometry").classList.add("hidden");
        setUnitLabels();
        el("footprintArea").value = 50000;
        el("avgDepth").value = 25;
        el("bottomArea").value = "";
        el("topArea").value = "";
        el("frustumDepth").value = "";
        el("density").value = 0.6;
        el("annualInflow").value = 50000;
        el("consumedPercent").value = 40;
        el("coverLoss").value = 15;
        el("errorMsg").textContent = "";
        [
          "resVolume",
          "resEffCapacity",
          "resTotalTons",
          "resConsumedVol",
          "resRemainVol",
          "resRemainTons",
          "resLife",
        ].forEach((id) => {
          el(id).textContent = "–";
        });
      }

      function validatePositive(value, name) {
        if (isNaN(value) || value <= 0) {
          throw new Error(name + " must be greater than zero.");
        }
      }

      function calculate() {
        try {
          el("errorMsg").textContent = "";

          let volume_m3;
          if (geometryMode === "simple") {
            let A = parseFloat(el("footprintArea").value);
            let h = parseFloat(el("avgDepth").value);
            validatePositive(A, "Footprint area");
            validatePositive(h, "Average depth/height");

            if (unitSystem === "imperial") {
              // acres * ft -> m³
              const A_m2 = A * 4046.8564224;
              const h_m = h * 0.3048;
              volume_m3 = A_m2 * h_m;
            } else {
              volume_m3 = A * h;
            }
          } else {
            let Ab = parseFloat(el("bottomArea").value);
            let At = parseFloat(el("topArea").value);
            let h = parseFloat(el("frustumDepth").value);
            validatePositive(Ab, "Bottom area");
            validatePositive(At, "Top area");
            validatePositive(h, "Vertical height");

            if (unitSystem === "imperial") {
              const Ab_m2 = Ab * 4046.8564224;
              const At_m2 = At * 4046.8564224;
              const h_m = h * 0.3048;
              volume_m3 =
                (h_m / 3) * (Ab_m2 + At_m2 + Math.sqrt(Ab_m2 * At_m2));
            } else {
              volume_m3 = (h / 3) * (Ab + At + Math.sqrt(Ab * At));
            }
          }

          const densityInput = parseFloat(el("density").value);
          validatePositive(densityInput, "Density");

          const inflow = parseFloat(el("annualInflow").value);
          validatePositive(inflow, "Annual waste inflow");

          const consumedPct = parseFloat(el("consumedPercent").value);
          if (isNaN(consumedPct) || consumedPct < 0 || consumedPct > 100) {
            throw new Error("Consumed airspace must be between 0 and 100%.");
          }

          const coverLossPct = parseFloat(el("coverLoss").value);
          if (isNaN(coverLossPct) || coverLossPct < 0 || coverLossPct > 50) {
            throw new Error(
              "Cover & infrastructure loss should be between 0 and 50%."
            );
          }

          // Effective volume after cover loss
          const effVolume_m3 = volume_m3 * (1 - coverLossPct / 100);

          // Convert density to tonnes/m³ internally
          let density_t_per_m3;
          if (unitSystem === "metric") {
            density_t_per_m3 = densityInput;
          } else {
            // tons/yd³ -> tonnes/m³
            // 1 US ton = 0.907185 tonnes, 1 yd³ = 0.764554857984 m³
            density_t_per_m3 = (densityInput * 0.907185) / 0.764554857984;
          }

          const totalCapacity_tonnes = effVolume_m3 * density_t_per_m3;

          const consumedCapacity_tonnes =
            totalCapacity_tonnes * (consumedPct / 100);
          const remainingCapacity_tonnes =
            totalCapacity_tonnes - consumedCapacity_tonnes;

          // Remaining life in years (use same mass unit as inflow)
          let remainingLifeYears;
          if (unitSystem === "metric") {
            remainingLifeYears = remainingCapacity_tonnes / inflow;
          } else {
            // inflow is in tons/year, convert remaining capacity to tons
            const remaining_tons = remainingCapacity_tonnes / 0.907185;
            remainingLifeYears = remaining_tons / inflow;
          }

          // Display
          function fmt(num, decimals = 1) {
            if (!isFinite(num)) return "–";
            return num.toLocaleString(undefined, {
              maximumFractionDigits: decimals,
            });
          }

          if (unitSystem === "metric") {
            el("resVolume").textContent = fmt(volume_m3, 0) + " m³";
            el("resEffCapacity").textContent = fmt(effVolume_m3, 0) + " m³";
            el("resTotalTons").textContent =
              fmt(totalCapacity_tonnes, 0) + " tonnes";
            el("resConsumedVol").textContent =
              fmt((effVolume_m3 * consumedPct) / 100, 0) + " m³";
            el("resRemainVol").textContent =
              fmt(effVolume_m3 * (1 - consumedPct / 100), 0) + " m³";
            el("resRemainTons").textContent =
              fmt(remainingCapacity_tonnes, 0) + " tonnes";
          } else {
            const volume_yd3 = volume_m3 / 0.764554857984;
            const effVolume_yd3 = effVolume_m3 / 0.764554857984;
            const totalCapacity_tons = totalCapacity_tonnes / 0.907185;
            const consumedVol_yd3 = (effVolume_yd3 * consumedPct) / 100;
            const remainingVol_yd3 = effVolume_yd3 * (1 - consumedPct / 100);
            const remainingCapacity_tons = remainingCapacity_tonnes / 0.907185;

            el("resVolume").textContent = fmt(volume_yd3, 0) + " yd³";
            el("resEffCapacity").textContent = fmt(effVolume_yd3, 0) + " yd³";
            el("resTotalTons").textContent =
              fmt(totalCapacity_tons, 0) + " tons";
            el("resConsumedVol").textContent = fmt(consumedVol_yd3, 0) + " yd³";
            el("resRemainVol").textContent = fmt(remainingVol_yd3, 0) + " yd³";
            el("resRemainTons").textContent =
              fmt(remainingCapacity_tons, 0) + " tons";
          }

          if (remainingLifeYears <= 0 || !isFinite(remainingLifeYears)) {
            el("resLife").textContent = "N/A";
          } else if (remainingLifeYears < 1) {
            el("resLife").textContent =
              fmt(remainingLifeYears * 12, 1) + " months";
          } else {
            el("resLife").textContent = fmt(remainingLifeYears, 1) + " years";
          }
        } catch (err) {
          el("errorMsg").textContent = err.message;
        }
      }

      document.addEventListener("DOMContentLoaded", () => {
        // Unit toggle
        el("unit-metric").addEventListener("click", () => {
          unitSystem = "metric";
          setActiveTab(el("unit-metric"), el("unit-imperial"));
          setUnitLabels();
        });
        el("unit-imperial").addEventListener("click", () => {
          unitSystem = "imperial";
          setActiveTab(el("unit-imperial"), el("unit-metric"));
          setUnitLabels();
        });

        // Geometry tabs
        el("tab-simple").addEventListener("click", () => {
          geometryMode = "simple";
          setActiveTab(el("tab-simple"), el("tab-frustum"));
          el("simple-geometry").classList.remove("hidden");
          el("frustum-geometry").classList.add("hidden");
        });
        el("tab-frustum").addEventListener("click", () => {
          geometryMode = "frustum";
          setActiveTab(el("tab-frustum"), el("tab-simple"));
          el("simple-geometry").classList.add("hidden");
          el("frustum-geometry").classList.remove("hidden");
        });

        el("calcBtn").addEventListener("click", calculate);
        el("resetBtn").addEventListener("click", resetForm);

        resetForm();
      });

      // Mobile menu script (from template)
      document.addEventListener("DOMContentLoaded", function () {
        const toggle = document.getElementById("mobile-menu-toggle");
        const menu = document.getElementById("mobile-menu");
        if (toggle && menu) {
          toggle.addEventListener("click", () => {
            const isHidden = menu.classList.contains("hidden");
            menu.classList.toggle("hidden", !isHidden);
            toggle.setAttribute("aria-expanded", String(isHidden));
          });
        }
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
