    <script>
      // Mobile menu toggle
      document
        .getElementById("mobile-menu-toggle")
        ?.addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          if (!menu) return;
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          this.setAttribute("aria-expanded", String(isHidden));
        });

      // Core combination definitions (simplified, user-editable)
      const baseCombos = {
        "7-16": {
          LRFD: [
            { type: "Strength", expr: "1.4D", note: "Gravity only" },
            {
              type: "Strength",
              expr: "1.2D + 1.6L + 0.5(Lr+S+R)",
              note: "D + L + roof/snow/rain",
            },
            {
              type: "Strength",
              expr: "1.2D + 1.0W + L + 0.5(Lr+S+R)",
              note: "D + W + L",
            },
            {
              type: "Strength",
              expr: "1.2D - 1.0W + L + 0.5(Lr+S+R)",
              note: "D - W + L (opposite wind)",
            },
            {
              type: "Strength",
              expr: "1.2D + 1.0E + L + 0.2S",
              note: "D + E + L + snow",
            },
            {
              type: "Strength",
              expr: "1.2D - 1.0E + L + 0.2S",
              note: "D - E + L + snow",
            },
            {
              type: "Strength",
              expr: "0.9D + 1.0W",
              note: "Uplift / overturning (wind)",
            },
            {
              type: "Strength",
              expr: "0.9D - 1.0W",
              note: "Uplift / overturning (wind, opposite)",
            },
            {
              type: "Strength",
              expr: "0.9D + 1.0E",
              note: "Uplift / overturning (seismic)",
            },
            {
              type: "Strength",
              expr: "0.9D - 1.0E",
              note: "Uplift / overturning (seismic, opposite)",
            },
          ],
          ASD: [
            { type: "Service", expr: "D", note: "Dead only" },
            { type: "Service", expr: "D + L", note: "D + L" },
            { type: "Service", expr: "D + Lr", note: "D + roof live" },
            { type: "Service", expr: "D + S", note: "D + snow" },
            {
              type: "Service",
              expr: "D + 0.75L + 0.75(Lr+S+R)",
              note: "D + 0.75L + 0.75 roof/snow/rain",
            },
            {
              type: "Service",
              expr: "D + 0.6W + 0.75L + 0.75(Lr+S+R)",
              note: "D + wind + L",
            },
            {
              type: "Service",
              expr: "D - 0.6W + 0.75L + 0.75(Lr+S+R)",
              note: "D - wind + L",
            },
            {
              type: "Service",
              expr: "D + 0.7E + 0.75L + 0.75S",
              note: "D + seismic + L + snow",
            },
            {
              type: "Service",
              expr: "D - 0.7E + 0.75L + 0.75S",
              note: "D - seismic + L + snow",
            },
            {
              type: "Service",
              expr: "0.6D + 0.6W",
              note: "Uplift / overturning (wind)",
            },
            {
              type: "Service",
              expr: "0.6D - 0.6W",
              note: "Uplift / overturning (wind, opposite)",
            },
            {
              type: "Service",
              expr: "0.6D + 0.7E",
              note: "Uplift / overturning (seismic)",
            },
            {
              type: "Service",
              expr: "0.6D - 0.7E",
              note: "Uplift / overturning (seismic, opposite)",
            },
          ],
        },
        "7-22": {}, // will clone 7-16 defaults for now
        "7-10": {}, // will clone 7-16 defaults for now
      };
      baseCombos["7-22"].LRFD = JSON.parse(
        JSON.stringify(baseCombos["7-16"].LRFD)
      );
      baseCombos["7-22"].ASD = JSON.parse(
        JSON.stringify(baseCombos["7-16"].ASD)
      );
      baseCombos["7-10"].LRFD = JSON.parse(
        JSON.stringify(baseCombos["7-16"].LRFD)
      );
      baseCombos["7-10"].ASD = JSON.parse(
        JSON.stringify(baseCombos["7-16"].ASD)
      );

      function parseLoads() {
        const ids = ["D", "L", "Lr", "S", "W", "E", "R", "F"];
        const loads = {};
        ids.forEach((id) => {
          const v = parseFloat(document.getElementById(id).value);
          loads[id] = isNaN(v) ? 0 : v;
        });
        return loads;
      }

      function getFactors() {
        return {
          gammaD: parseFloat(document.getElementById("gammaD").value) || 1.0,
          gammaL: parseFloat(document.getElementById("gammaL").value) || 1.0,
          gammaW: parseFloat(document.getElementById("gammaW").value) || 1.0,
          gammaE: parseFloat(document.getElementById("gammaE").value) || 1.0,
        };
      }

      // Very small expression parser: replaces symbols with numeric values and evaluates
      function evalComboExpression(expr, loads, factors) {
        let e = expr;

        // Replace grouped terms like (Lr+S+R) with the max of the present loads
        e = e.replace(/\((Lr\+S\+R)\)/g, () => {
          const vals = [loads.Lr, loads.S, loads.R].filter((v) => v !== 0);
          return vals.length ? Math.max.apply(null, vals) : 0;
        });

        // Map symbols to actual numeric values with approximate factors
        const map = {
          D: loads.D * factors.gammaD,
          L: loads.L * factors.gammaL,
          Lr: loads.Lr * factors.gammaL,
          S: loads.S * factors.gammaL,
          R: loads.R * factors.gammaL,
          W: loads.W * factors.gammaW,
          E: loads.E * factors.gammaE,
          F: loads.F,
        };

        // Replace single symbols with their numeric values
        Object.keys(map).forEach((sym) => {
          const re = new RegExp(`\\b${sym}\\b`, "g");
          e = e.replace(re, `(${map[sym] || 0})`);
        });

        // If any basic load is zero, remove its term like "+ 0" or "- 0"
        e = e.replace(/([+\-])\s*\(0\)/g, "");

        try {
          // eslint-disable-next-line no-eval
          const val = eval(e);
          return typeof val === "number" && isFinite(val) ? val : NaN;
        } catch {
          return NaN;
        }
      }

      function generateCombinations() {
        const edition = document.getElementById("edition").value;
        const method = document.getElementById("method").value;
        const loads = parseLoads();
        const factors = getFactors();
        const combos = baseCombos[edition][method] || [];

        const tbody = document.getElementById("comboTableBody");
        tbody.innerHTML = "";

        if (!combos.length) {
          const tr = document.createElement("tr");
          tr.innerHTML =
            '<td colspan="5" class="px-2 py-2 text-center text-gray-500 text-xs">No combinations defined for this selection.</td>';
          tbody.appendChild(tr);
          return;
        }

        let governingVal = null;
        let governingIdx = -1;

        combos.forEach((c, idx) => {
          const result = evalComboExpression(c.expr, loads, factors);
          const tr = document.createElement("tr");
          tr.className = "border-t";

          const resultStr = isNaN(result) ? "—" : result.toFixed(3);

          if (!isNaN(result)) {
            if (
              governingVal === null ||
              Math.abs(result) > Math.abs(governingVal)
            ) {
              governingVal = result;
              governingIdx = idx;
            }
          }

          tr.innerHTML = `
        <td class="px-2 py-2 text-center">${idx + 1}</td>
        <td class="px-2 py-2">
          <span class="badge ${
            c.type === "Strength"
              ? "bg-red-100 text-red-700"
              : "bg-blue-100 text-blue-700"
          }">${c.type}</span>
        </td>
        <td class="px-2 py-2">
          <input type="text" class="w-full border rounded px-1 py-1 text-xs combo-expr" value="${
            c.expr
          }">
        </td>
        <td class="px-2 py-2 text-right font-mono">${resultStr}</td>
        <td class="px-2 py-2 text-gray-600">${c.note || ""}</td>
      `;
          tbody.appendChild(tr);
        });

        const govDiv = document.getElementById("governingSummary");
        if (governingIdx >= 0) {
          const combo = combos[governingIdx];
          govDiv.classList.remove("hidden");
          govDiv.innerHTML = `
        <strong>Governing combination:</strong>
        #${governingIdx + 1} (${combo.type}) &mdash;
        <code>${combo.expr}</code>
        <span class="ml-2">= <span class="font-mono">${governingVal.toFixed(
          3
        )}</span></span>
      `;
        } else {
          govDiv.classList.add("hidden");
          govDiv.textContent = "";
        }
      }

      document.getElementById("calculateBtn").addEventListener("click", () => {
        // Before calculating, update internal expressions from any user edits
        const edition = document.getElementById("edition").value;
        const method = document.getElementById("method").value;
        const combos = baseCombos[edition][method] || [];
        const inputs = document.querySelectorAll("#comboTableBody .combo-expr");
        inputs.forEach((inp, i) => {
          if (combos[i]) combos[i].expr = inp.value;
        });
        generateCombinations();
      });

      document.getElementById("resetDefaults").addEventListener("click", () => {
        document.getElementById("gammaD").value = 1.2;
        document.getElementById("gammaL").value = 1.6;
        document.getElementById("gammaW").value = 1.0;
        document.getElementById("gammaE").value = 1.0;
        generateCombinations();
      });

      document.getElementById("copyTableBtn").addEventListener("click", () => {
        const rows = Array.from(
          document.querySelectorAll("#comboTable tbody tr")
        );
        if (!rows.length) return;
        const lines = [];
        lines.push("No.,Type,Expression,Result,Notes");
        rows.forEach((r) => {
          const cells = r.querySelectorAll("td");
          if (cells.length < 5) return;
          const no = cells[0].innerText.trim();
          const type = cells[1].innerText.trim();
          const expr =
            cells[2].querySelector("input")?.value || cells[2].innerText.trim();
          const res = cells[3].innerText.trim();
          const note = cells[4].innerText.trim();
          lines.push([no, type, `"${expr}"`, res, `"${note}"`].join(","));
        });
        const text = lines.join("\n");
        navigator.clipboard
          .writeText(text)
          .then(() => {
            alert("Load combinations table copied to clipboard as CSV.");
          })
          .catch(() => {
            alert("Unable to copy to clipboard in this browser.");
          });
      });

      // Initialize with default table (no results yet)
      generateCombinations();
    </script>

