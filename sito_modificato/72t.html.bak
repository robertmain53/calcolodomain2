<script="">
        document.getElementById('sepp-form').addEventListener('submit', function(event) {
            event.preventDefault();
            var accountBalance = parseFloat(document.getElementById('accountBalance').value);
            var interestRate = parseFloat(document.getElementById('interestRate').value);
            var annuityFactor = parseFloat(document.getElementById('annuityFactor').value);
            
            if (!isNaN(accountBalance) && !isNaN(interestRate) && !isNaN(annuityFactor)) {
                var annualPayment = (accountBalance * (interestRate / 100)) / annuityFactor;
                document.getElementById('annualPayment').textContent = '$' + annualPayment.toFixed(2);
            } else {
                alert('Please enter valid numbers in all fields.');
            }
        });
    </script>


