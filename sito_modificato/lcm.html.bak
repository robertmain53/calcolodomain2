    <script>
      // Utility functions
      function parseNumbers(input) {
        if (!input) return [];
        const tokens = input.split(/[\s,;]+/).filter(Boolean);
        const nums = [];
        for (const t of tokens) {
          const n = Number(t);
          if (Number.isFinite(n) && Number.isInteger(n)) {
            nums.push(n);
          }
        }
        return nums;
      }

      function gcd(a, b) {
        a = Math.abs(a);
        b = Math.abs(b);
        while (b !== 0) {
          const temp = b;
          b = a % b;
          a = temp;
        }
        return a;
      }

      function lcmTwo(a, b) {
        if (a === 0 || b === 0) return 0;
        return Math.abs((a / gcd(a, b)) * b);
      }

      function lcmArrayGcdMethod(arr, steps) {
        let current = arr[0];
        steps.push("Start with first number: " + current);
        for (let i = 1; i < arr.length; i++) {
          const n = arr[i];
          const g = gcd(current, n);
          const l = Math.abs((current / g) * n);
          steps.push(
            "LCM(" +
              current +
              ", " +
              n +
              ") = |" +
              current +
              " × " +
              n +
              "| / GCD(" +
              current +
              ", " +
              n +
              ") = " +
              l
          );
          current = l;
        }
        return current;
      }

      function primeFactorization(n) {
        n = Math.abs(n);
        const factors = {};
        let d = 2;
        while (d * d <= n) {
          while (n % d === 0) {
            factors[d] = (factors[d] || 0) + 1;
            n / d;
          }
          d = d === 2 ? 3 : d + 2;
        }
        if (n > 1) {
          factors[n] = (factors[n] || 0) + 1;
        }
        return factors;
      }

      function formatFactorization(factors) {
        const parts = [];
        const primes = Object.keys(factors)
          .map(Number)
          .sort((a, b) => a - b);
        for (const p of primes) {
          const exp = factors[p];
          parts.push(exp === 1 ? p.toString() : p + "^" + exp);
        }
        return parts.join(" × ");
      }

      function lcmArrayPrimeMethod(arr, steps) {
        const allFactors = [];
        const maxPowers = {};
        steps.push("Prime factorization of each number:");
        for (const n of arr) {
          const f = primeFactorization(n);
          allFactors.push(f);
          const formatted = formatFactorization(f);
          steps.push("• " + n + " = " + (formatted || "1"));
          for (const pStr in f) {
            const p = Number(pStr);
            const exp = f[p];
            if (!maxPowers[p] || exp > maxPowers[p]) {
              maxPowers[p] = exp;
            }
          }
        }
        const primes = Object.keys(maxPowers)
          .map(Number)
          .sort((a, b) => a - b);
        const lcmParts = primes.map((p) => {
          const e = maxPowers[p];
          return e === 1 ? p.toString() : p + "^" + e;
        });
        steps.push("Take each prime with the highest exponent:");
        steps.push("LCM = " + lcmParts.join(" × "));
        let lcmVal = 1;
        for (const p of primes) {
          lcmVal *= Math.pow(p, maxPowers[p]);
        }
        steps.push("Multiply: LCM = " + lcmVal);
        return lcmVal;
      }

      function onCalculate() {
        const inputEl = document.getElementById("numbers-input");
        const errorEl = document.getElementById("error-message");
        const resultCard = document.getElementById("result-card");
        const stepsSection = document.getElementById("steps-section");
        const stepsContent = document.getElementById("steps-content");
        const lcmResultEl = document.getElementById("lcm-result");
        const inputListEl = document.getElementById("result-input-list");
        const methodBadge = document.getElementById("method-used-badge");

        errorEl.classList.add("hidden");
        errorEl.textContent = "";
        resultCard.classList.add("hidden");
        stepsSection.classList.add("hidden");
        stepsContent.innerHTML = "";

        const rawNums = parseNumbers(inputEl.value);
        const nums = rawNums.filter((n) => n !== 0);
        if (rawNums.length === 0) {
          errorEl.textContent = "Please enter at least one integer.";
          errorEl.classList.remove("hidden");
          return;
        }
        if (nums.length === 0) {
          errorEl.textContent =
            "All entered values are zero. LCM is undefined.";
          errorEl.classList.remove("hidden");
          return;
        }
        if (nums.length > 20) {
          errorEl.textContent = "Please enter no more than 20 integers.";
          errorEl.classList.remove("hidden");
          return;
        }

        const method = document.querySelector(
          'input[name="method"]:checked'
        ).value;
        const absNums = nums.map(Math.abs);
        const steps = [];
        let lcmVal;
        let methodUsed;

        if (method === "gcd") {
          methodUsed = "GCD-based method";
          lcmVal = lcmArrayGcdMethod(absNums, steps);
        } else if (method === "prime") {
          methodUsed = "Prime factorization";
          lcmVal = lcmArrayPrimeMethod(absNums, steps);
        } else {
          // auto: use prime method for small numbers, gcd for larger
          const maxAbs = Math.max(...absNums);
          if (maxAbs <= 100000 && absNums.length <= 8) {
            methodUsed = "Prime factorization (auto)";
            lcmVal = lcmArrayPrimeMethod(absNums, steps);
          } else {
            methodUsed = "GCD-based method (auto)";
            lcmVal = lcmArrayGcdMethod(absNums, steps);
          }
        }

        inputListEl.textContent = absNums.join(", ");
        lcmResultEl.textContent = lcmVal.toString();
        methodBadge.textContent = methodUsed;

        const ul = document.createElement("ul");
        ul.className = "list-disc ml-5 space-y-1";
        for (const s of steps) {
          const li = document.createElement("li");
          li.textContent = s;
          ul.appendChild(li);
        }
        stepsContent.appendChild(ul);

        resultCard.classList.remove("hidden");
        stepsSection.classList.remove("hidden");
      }

      function onClear() {
        document.getElementById("numbers-input").value = "";
        document.getElementById("error-message").classList.add("hidden");
        document.getElementById("error-message").textContent = "";
        document.getElementById("result-card").classList.add("hidden");
        document.getElementById("steps-section").classList.add("hidden");
        document.getElementById("steps-content").innerHTML = "";
      }

      document.addEventListener("DOMContentLoaded", function () {
        document
          .getElementById("calculate-btn")
          .addEventListener("click", onCalculate);
        document.getElementById("clear-btn").addEventListener("click", onClear);
        document
          .getElementById("numbers-input")
          .addEventListener("keydown", function (e) {
            if (e.key === "Enter" && (e.ctrlKey || e.metaKey)) {
              e.preventDefault();
              onCalculate();
            }
          });
        document.querySelectorAll("[data-preset]").forEach((btn) => {
          btn.addEventListener("click", () => {
            document.getElementById("numbers-input").value =
              btn.getAttribute("data-preset");
          });
        });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
