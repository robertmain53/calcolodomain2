    <script>
      // Utility formatting
      function formatCurrency(value, symbol) {
        if (isNaN(value)) return "–";
        const absVal = Math.abs(value);
        let unit = "";
        let scaled = value;

        if (absVal >= 1e9) {
          scaled = value / 1e9;
          unit = "B";
        } else if (absVal >= 1e6) {
          scaled = value / 1e6;
          unit = "M";
        } else if (absVal >= 1e3) {
          scaled = value / 1e3;
          unit = "K";
        }

        return symbol + scaled.toFixed(2) + (unit ? " " + unit : "");
      }

      function formatNumber(value, decimals = 2) {
        if (isNaN(value)) return "–";
        return value.toFixed(decimals);
      }

      function getCurrencySymbol() {
        return document.getElementById("currencySymbol").value || "$";
      }

      function updateCurrencyPrefixes() {
        const symbol = getCurrencySymbol();
        const ids = [
          "revYear1Prefix",
          "costYear1Prefix",
          "intYear1Prefix",
          "dealValuePrefix",
        ];
        ids.forEach((id) => {
          const el = document.getElementById(id);
          if (el) el.textContent = symbol;
        });
      }

      document
        .getElementById("currencySymbol")
        .addEventListener("change", () => {
          updateCurrencyPrefixes();
          // Recalculate to refresh formatted outputs
          calculateSynergies();
        });

      function calculateSynergies() {
        const symbol = getCurrencySymbol();

        const horizonYears =
          parseInt(document.getElementById("horizonYears").value, 10) || 5;
        const rampUpYears = Math.max(
          1,
          parseInt(document.getElementById("rampUpYears").value, 10) || 3
        );
        const discountRate =
          (parseFloat(document.getElementById("discountRate").value) || 0) /
          100;

        const enableRevenue = document.getElementById("enableRevenue").checked;
        const enableCost = document.getElementById("enableCost").checked;
        const enableIntegration =
          document.getElementById("enableIntegration").checked;

        const revYear1 =
          (parseFloat(document.getElementById("revYear1").value) || 0) *
          (enableRevenue ? 1 : 0);
        const revGrowth =
          (parseFloat(document.getElementById("revGrowth").value) || 0) / 100;

        const costYear1 =
          (parseFloat(document.getElementById("costYear1").value) || 0) *
          (enableCost ? 1 : 0);
        const costGrowth =
          (parseFloat(document.getElementById("costGrowth").value) || 0) / 100;

        const integrationYear1 =
          (parseFloat(document.getElementById("integrationYear1").value) || 0) *
          (enableIntegration ? 1 : 0);
        const integrationDecay =
          (parseFloat(document.getElementById("integrationDecay").value) || 0) /
          100;

        const dealValue =
          parseFloat(document.getElementById("dealValue").value) || 0;
        const dealPremiumPct =
          (parseFloat(document.getElementById("dealPremiumPct").value) || 0) /
          100;

        const tbody = document.getElementById("resultsTableBody");
        tbody.innerHTML = "";

        let npvSynergies = 0;

        for (let t = 1; t <= horizonYears; t++) {
          const revRaw = revYear1 * Math.pow(1 + revGrowth, t - 1);
          const costRaw = costYear1 * Math.pow(1 + costGrowth, t - 1);
          const intRaw =
            integrationYear1 * Math.pow(1 - integrationDecay, t - 1);

          const rampFactor = t <= rampUpYears ? t / rampUpYears : 1;

          const revEff = revRaw * rampFactor;
          const costEff = costRaw * rampFactor;
          const intEff = intRaw;

          const netCF = revEff + costEff - intEff;
          const df = 1 / Math.pow(1 + discountRate, t);
          const pv = netCF * df;

          npvSynergies += pv;

          const row = document.createElement("tr");
          row.innerHTML = `
        <td class="px-3 py-2 border-b text-right">${t}</td>
        <td class="px-3 py-2 border-b text-right">${formatCurrency(
          revEff,
          symbol
        )}</td>
        <td class="px-3 py-2 border-b text-right">${formatCurrency(
          costEff,
          symbol
        )}</td>
        <td class="px-3 py-2 border-b text-right">${formatCurrency(
          intEff,
          symbol
        )}</td>
        <td class="px-3 py-2 border-b text-right">${formatCurrency(
          netCF,
          symbol
        )}</td>
        <td class="px-3 py-2 border-b text-right">${formatNumber(df, 4)}</td>
        <td class="px-3 py-2 border-b text-right">${formatCurrency(
          pv,
          symbol
        )}</td>
      `;
          tbody.appendChild(row);
        }

        document.getElementById("npvSynergiesDisplay").textContent =
          formatCurrency(npvSynergies, symbol);
        document.getElementById("npvSynergiesTable").textContent =
          formatCurrency(npvSynergies, symbol);

        const npvNote = document.getElementById("npvSynergiesNote");
        if (npvSynergies >= 0) {
          npvNote.textContent =
            "Positive NPV indicates synergies add value under these assumptions.";
        } else {
          npvNote.textContent =
            "Negative NPV indicates integration costs outweigh synergies under these assumptions.";
        }

        // Coverage and uplift
        const coverageEl = document.getElementById("coverageDisplay");
        const upliftEl = document.getElementById("upliftDisplay");

        if (dealValue > 0) {
          const upliftPct = (npvSynergies / dealValue) * 100;
          upliftEl.textContent =
            "Synergy value as % of target EV: " +
            formatNumber(upliftPct, 1) +
            "%";

          if (dealPremiumPct > 0) {
            const premiumValue = dealValue * dealPremiumPct;
            const coveragePct =
              premiumValue !== 0 ? (npvSynergies / premiumValue) * 100 : 0;
            coverageEl.textContent =
              "Synergy coverage of premium: " +
              formatNumber(coveragePct, 1) +
              "%";
          } else {
            coverageEl.textContent =
              "Synergy coverage of premium: enter a non-zero premium to compute.";
          }
        } else {
          coverageEl.textContent =
            "Synergy coverage of premium: enter target enterprise value.";
          upliftEl.textContent =
            "Synergy value as % of target EV: enter target enterprise value.";
        }
      }

      document.getElementById("calculateBtn").addEventListener("click", (e) => {
        e.preventDefault();
        calculateSynergies();
      });

      document.getElementById("resetBtn").addEventListener("click", (e) => {
        e.preventDefault();
        document.getElementById("currencySymbol").value = "$";
        document.getElementById("horizonYears").value = 5;
        document.getElementById("revYear1").value = 10;
        document.getElementById("revGrowth").value = 5;
        document.getElementById("costYear1").value = 8;
        document.getElementById("costGrowth").value = 2;
        document.getElementById("integrationYear1").value = 12;
        document.getElementById("integrationDecay").value = 50;
        document.getElementById("rampUpYears").value = 3;
        document.getElementById("discountRate").value = 9;
        document.getElementById("dealValue").value = 200;
        document.getElementById("dealPremiumPct").value = 25;
        document.getElementById("enableRevenue").checked = true;
        document.getElementById("enableCost").checked = true;
        document.getElementById("enableIntegration").checked = true;
        updateCurrencyPrefixes();

        document.getElementById("resultsTableBody").innerHTML = `
      <tr>
        <td class="px-3 py-2 text-center text-gray-500" colspan="7">No results yet. Enter your assumptions and click “Calculate synergy value”.</td>
      </tr>
    `;
        document.getElementById("npvSynergiesDisplay").textContent = "–";
        document.getElementById("npvSynergiesTable").textContent = "–";
        document.getElementById("npvSynergiesNote").textContent =
          "Run the model to see results.";
        document.getElementById("coverageDisplay").textContent =
          "Synergy coverage of premium: –";
        document.getElementById("upliftDisplay").textContent =
          "Synergy value as % of target EV: –";
      });

      // Initialize prefixes on load
      document.addEventListener("DOMContentLoaded", () => {
        updateCurrencyPrefixes();
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
