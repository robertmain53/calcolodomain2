    <script>
      // Utility: format date as YYYY-MM-DD and as readable string
      function formatISODate(date) {
        const y = date.getFullYear();
        const m = String(date.getMonth() + 1).padStart(2, "0");
        const d = String(date.getDate()).padStart(2, "0");
        return y + "-" + m + "-" + d;
      }

      function formatReadableDate(date) {
        return date.toLocaleDateString(undefined, {
          year: "numeric",
          month: "short",
          day: "numeric",
          weekday: "short",
        });
      }

      // ISO week helpers
      function getISODay(date) {
        // Monday = 1, Sunday = 7
        let day = date.getDay();
        if (day === 0) day = 7;
        return day;
      }

      function getISOWeekInfo(date) {
        const d = new Date(
          Date.UTC(date.getFullYear(), date.getMonth(), date.getDate())
        );
        const day = d.getUTCDay() || 7; // Monday=1..Sunday=7
        // Shift to Thursday of this week
        d.setUTCDate(d.getUTCDate() + 4 - day);
        const weekYear = d.getUTCFullYear();
        const yearStart = new Date(Date.UTC(weekYear, 0, 1));
        const weekNo = Math.floor(((d - yearStart) / 86400000 + 1) / 7) + 1;

        // Get Monday of this ISO week
        const monday = new Date(d);
        const mondayDay = monday.getUTCDay() || 7;
        monday.setUTCDate(monday.getUTCDate() - (mondayDay - 1));

        const sunday = new Date(monday);
        sunday.setUTCDate(sunday.getUTCDate() + 6);

        return {
          week: weekNo,
          weekYear: weekYear,
          calendarYear: date.getFullYear(),
          monday: monday,
          sunday: sunday,
        };
      }

      function weeksInISOYear(year) {
        // ISO year has 53 weeks if Dec 31 is Thursday (4) or (Friday (5) in leap year)
        const dec31 = new Date(Date.UTC(year, 11, 31));
        const day = dec31.getUTCDay() || 7;
        if (day === 4) return 53;
        if (day === 5) {
          // check leap year
          const isLeap =
            (year % 4 === 0 && year % 100 !== 0) || year % 400 === 0;
          if (isLeap) return 53;
        }
        return 52;
      }

      function getDateFromISOWeek(weekYear, week) {
        // ISO week: Monday of week 1 is the Monday of the week containing Jan 4
        const simple = new Date(Date.UTC(weekYear, 0, 4));
        const dayOfWeek = simple.getUTCDay() || 7;
        const mondayWeek1 = new Date(simple);
        mondayWeek1.setUTCDate(simple.getUTCDate() - (dayOfWeek - 1));
        const monday = new Date(mondayWeek1);
        monday.setUTCDate(mondayWeek1.getUTCDate() + (week - 1) * 7);
        return monday;
      }

      document.addEventListener("DOMContentLoaded", function () {
        const tabDateToWeek = document.getElementById("tab-date-to-week");
        const tabWeekToDate = document.getElementById("tab-week-to-date");
        const panelDateToWeek = document.getElementById("panel-date-to-week");
        const panelWeekToDate = document.getElementById("panel-week-to-date");

        function activateTab(tab) {
          if (tab === "date") {
            tabDateToWeek.classList.add("tab-active");
            tabWeekToDate.classList.remove("tab-active");
            panelDateToWeek.classList.remove("hidden");
            panelWeekToDate.classList.add("hidden");
          } else {
            tabWeekToDate.classList.add("tab-active");
            tabDateToWeek.classList.remove("tab-active");
            panelWeekToDate.classList.remove("hidden");
            panelDateToWeek.classList.add("hidden");
          }
        }

        tabDateToWeek.addEventListener("click", () => activateTab("date"));
        tabWeekToDate.addEventListener("click", () => activateTab("week"));

        // Date to week elements
        const inputDate = document.getElementById("input-date");
        const btnToday = document.getElementById("btn-today");
        const weekNumberDisplay = document.getElementById(
          "week-number-display"
        );
        const weekYearDisplay = document.getElementById("week-year-display");
        const calendarYearDisplay = document.getElementById(
          "calendar-year-display"
        );
        const weekStartDisplay = document.getElementById("week-start-display");
        const weekEndDisplay = document.getElementById("week-end-display");
        const btnPrevWeek = document.getElementById("btn-prev-week");
        const btnNextWeek = document.getElementById("btn-next-week");
        const btnPrevYear = document.getElementById("btn-prev-year");
        const btnNextYear = document.getElementById("btn-next-year");

        function setDateInputTo(date) {
          inputDate.value = formatISODate(date);
          updateFromDate();
        }

        function updateFromDate() {
          if (!inputDate.value) return;
          const [y, m, d] = inputDate.value.split("-").map(Number);
          const date = new Date(y, m - 1, d);
          if (isNaN(date.getTime())) return;

          const info = getISOWeekInfo(date);
          weekNumberDisplay.textContent = info.week;
          weekYearDisplay.textContent = info.weekYear;
          calendarYearDisplay.textContent = info.calendarYear;
          weekStartDisplay.textContent = formatReadableDate(info.monday);
          weekEndDisplay.textContent = formatReadableDate(info.sunday);
        }

        inputDate.addEventListener("change", updateFromDate);
        btnToday.addEventListener("click", () => {
          setDateInputTo(new Date());
        });

        btnPrevWeek.addEventListener("click", () => {
          if (!inputDate.value) return;
          const [y, m, d] = inputDate.value.split("-").map(Number);
          const date = new Date(y, m - 1, d);
          date.setDate(date.getDate() - 7);
          setDateInputTo(date);
        });

        btnNextWeek.addEventListener("click", () => {
          if (!inputDate.value) return;
          const [y, m, d] = inputDate.value.split("-").map(Number);
          const date = new Date(y, m - 1, d);
          date.setDate(date.getDate() + 7);
          setDateInputTo(date);
        });

        btnPrevYear.addEventListener("click", () => {
          if (!inputDate.value) return;
          const [y, m, d] = inputDate.value.split("-").map(Number);
          const date = new Date(y - 1, m - 1, d);
          setDateInputTo(date);
        });

        btnNextYear.addEventListener("click", () => {
          if (!inputDate.value) return;
          const [y, m, d] = inputDate.value.split("-").map(Number);
          const date = new Date(y + 1, m - 1, d);
          setDateInputTo(date);
        });

        // Week to date elements
        const inputWeekYear = document.getElementById("input-week-year");
        const inputWeekNumber = document.getElementById("input-week-number");
        const btnGoWeek = document.getElementById("btn-go-week");
        const weekRangeHint = document.getElementById("week-range-hint");

        const selectedWeekDisplay = document.getElementById(
          "selected-week-display"
        );
        const selectedWeekYearDisplay = document.getElementById(
          "selected-week-year-display"
        );
        const weeksInYearDisplay = document.getElementById(
          "weeks-in-year-display"
        );
        const selectedWeekStartDisplay = document.getElementById(
          "selected-week-start-display"
        );
        const selectedWeekTueDisplay = document.getElementById(
          "selected-week-tue-display"
        );
        const selectedWeekWedDisplay = document.getElementById(
          "selected-week-wed-display"
        );
        const selectedWeekThuDisplay = document.getElementById(
          "selected-week-thu-display"
        );
        const selectedWeekFriDisplay = document.getElementById(
          "selected-week-fri-display"
        );
        const selectedWeekSatDisplay = document.getElementById(
          "selected-week-sat-display"
        );
        const selectedWeekEndDisplay = document.getElementById(
          "selected-week-end-display"
        );

        const btnCurrentWeek = document.getElementById("btn-current-week");
        const btnFirstWeek = document.getElementById("btn-first-week");
        const btnLastWeek = document.getElementById("btn-last-week");

        function updateWeekRangeHint() {
          const year = parseInt(inputWeekYear.value, 10);
          if (!year || isNaN(year)) {
            weekRangeHint.textContent = "Valid weeks for this year: –";
            return;
          }
          const w = weeksInISOYear(year);
          weekRangeHint.textContent = "Valid weeks for this year: 1–" + w;
        }

        inputWeekYear.addEventListener("input", updateWeekRangeHint);

        function updateFromWeek() {
          const year = parseInt(inputWeekYear.value, 10);
          const week = parseInt(inputWeekNumber.value, 10);
          if (!year || !week || isNaN(year) || isNaN(week)) return;

          const maxWeeks = weeksInISOYear(year);
          if (week < 1 || week > maxWeeks) {
            alert(
              "Week number must be between 1 and " +
                maxWeeks +
                " for year " +
                year +
                "."
            );
            return;
          }

          const monday = getDateFromISOWeek(year, week);
          const tuesday = new Date(monday);
          tuesday.setUTCDate(monday.getUTCDate() + 1);
          const wednesday = new Date(monday);
          wednesday.setUTCDate(monday.getUTCDate() + 2);
          const thursday = new Date(monday);
          thursday.setUTCDate(monday.getUTCDate() + 3);
          const friday = new Date(monday);
          friday.setUTCDate(monday.getUTCDate() + 4);
          const saturday = new Date(monday);
          saturday.setUTCDate(monday.getUTCDate() + 5);
          const sunday = new Date(monday);
          sunday.setUTCDate(monday.getUTCDate() + 6);

          selectedWeekDisplay.textContent = week;
          selectedWeekYearDisplay.textContent = year;
          weeksInYearDisplay.textContent = maxWeeks;
          selectedWeekStartDisplay.textContent = formatReadableDate(monday);
          selectedWeekTueDisplay.textContent = formatReadableDate(tuesday);
          selectedWeekWedDisplay.textContent = formatReadableDate(wednesday);
          selectedWeekThuDisplay.textContent = formatReadableDate(thursday);
          selectedWeekFriDisplay.textContent = formatReadableDate(friday);
          selectedWeekSatDisplay.textContent = formatReadableDate(saturday);
          selectedWeekEndDisplay.textContent = formatReadableDate(sunday);
        }

        btnGoWeek.addEventListener("click", updateFromWeek);

        btnCurrentWeek.addEventListener("click", () => {
          const today = new Date();
          const info = getISOWeekInfo(today);
          inputWeekYear.value = info.weekYear;
          inputWeekNumber.value = info.week;
          updateWeekRangeHint();
          updateFromWeek();
        });

        btnFirstWeek.addEventListener("click", () => {
          const year =
            parseInt(inputWeekYear.value, 10) || new Date().getFullYear();
          inputWeekYear.value = year;
          inputWeekNumber.value = 1;
          updateWeekRangeHint();
          updateFromWeek();
        });

        btnLastWeek.addEventListener("click", () => {
          const year =
            parseInt(inputWeekYear.value, 10) || new Date().getFullYear();
          const maxWeeks = weeksInISOYear(year);
          inputWeekYear.value = year;
          inputWeekNumber.value = maxWeeks;
          updateWeekRangeHint();
          updateFromWeek();
        });

        // Initialize defaults
        const today = new Date();
        setDateInputTo(today);

        const todayInfo = getISOWeekInfo(today);
        inputWeekYear.value = todayInfo.weekYear;
        inputWeekNumber.value = todayInfo.week;
        updateWeekRangeHint();
        updateFromWeek();
      });
    </script>

