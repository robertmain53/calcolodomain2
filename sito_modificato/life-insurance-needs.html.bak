    <script>
      // Utility: format currency
      function formatCurrency(value) {
        const v = isFinite(value) ? value : 0;
        return v.toLocaleString("en-US", {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 0,
        });
      }

      function getNumber(id) {
        const el = document.getElementById(id);
        if (!el) return 0;
        const val = parseFloat(el.value.replace(/,/g, ""));
        return isNaN(val) ? 0 : val;
      }

      function updateRangeLabels() {
        const yearsIncome = document.getElementById("years-income");
        const yearsIncomeLabel = document.getElementById("years-income-label");
        if (yearsIncome && yearsIncomeLabel) {
          yearsIncomeLabel.textContent =
            yearsIncome.value +
            (yearsIncome.value === "1" ? " year" : " years");
        }

        const inflation = document.getElementById("inflation");
        const inflationLabel = document.getElementById("inflation-label");
        if (inflation && inflationLabel) {
          inflationLabel.textContent =
            parseFloat(inflation.value).toFixed(1).replace(/\.0$/, "") + "%";
        }

        const returnRate = document.getElementById("return-rate");
        const returnRateLabel = document.getElementById("return-rate-label");
        if (returnRate && returnRateLabel) {
          returnRateLabel.textContent =
            parseFloat(returnRate.value).toFixed(1).replace(/\.0$/, "") + "%";
        }
      }

      function calculateLifeInsuranceNeed() {
        const income = getNumber("income");
        const years = getNumber("years-income");
        const realReturn = getNumber("return-rate") / 100;

        // Present value of income stream
        let pvIncome = 0;
        if (income > 0 && years > 0 && realReturn > 0) {
          pvIncome =
            (income * (1 - Math.pow(1 + realReturn, -years))) / realReturn;
        } else if (income > 0 && years > 0 && realReturn === 0) {
          pvIncome = income * years;
        }

        // Debts & one-time expenses
        const mortgage = getNumber("mortgage");
        const otherDebts = getNumber("other-debts");
        const college = getNumber("college");
        const finalExpenses = getNumber("final-expenses");
        const otherGoals = getNumber("other-goals");
        const debtsTotal =
          mortgage + otherDebts + college + finalExpenses + otherGoals;

        // Existing assets
        const savings = getNumber("savings");
        const retirement = getNumber("retirement");
        const existingLife = getNumber("existing-life");
        const assetsTotal = savings + retirement + existingLife;

        let need = pvIncome + debtsTotal - assetsTotal;
        if (need < 0) need = 0;

        // Round up to nearest 10,000
        const roundedNeed = Math.ceil(need / 10000) * 10000;

        // Range: +/- 20% as a simple band
        const low = Math.max(0, roundedNeed * 0.8);
        const high = roundedNeed * 1.2;

        // Update UI
        document.getElementById("result-coverage").textContent =
          formatCurrency(roundedNeed);
        document.getElementById("result-range").textContent =
          "Range: " + formatCurrency(low) + " – " + formatCurrency(high);

        document.getElementById("break-income").textContent =
          formatCurrency(pvIncome);
        document.getElementById("break-debts").textContent =
          formatCurrency(debtsTotal);
        document.getElementById("break-assets").textContent =
          "-" + formatCurrency(assetsTotal);
        document.getElementById("break-net").textContent =
          formatCurrency(roundedNeed);

        const noteEl = document.getElementById("result-note");
        if (need === 0 && pvIncome + debtsTotal > 0) {
          noteEl.textContent =
            "Based on your entries, your existing savings and life insurance already cover your stated goals.";
        } else {
          noteEl.textContent =
            "Adjust the sliders and amounts on the left to see how your need changes.";
        }
      }

      function resetCalculator() {
        const defaults = {
          age: 35,
          income: 80000,
          "years-income": 20,
          inflation: 2.5,
          "return-rate": 3,
          mortgage: 250000,
          "other-debts": 15000,
          college: 100000,
          "final-expenses": 15000,
          "other-goals": 0,
          savings: 50000,
          retirement: 100000,
          "existing-life": 200000,
        };
        Object.keys(defaults).forEach((id) => {
          const el = document.getElementById(id);
          if (el) el.value = defaults[id];
        });
        updateRangeLabels();
        calculateLifeInsuranceNeed();
      }

      document.addEventListener("DOMContentLoaded", function () {
        updateRangeLabels();
        calculateLifeInsuranceNeed();

        ["years-income", "inflation", "return-rate"].forEach((id) => {
          const el = document.getElementById(id);
          if (el) {
            el.addEventListener("input", () => {
              updateRangeLabels();
            });
            el.addEventListener("change", () => {
              calculateLifeInsuranceNeed();
            });
          }
        });

        const inputs = document.querySelectorAll(
          '#calculator-heading ~ div input[type="number"]'
        );
        inputs.forEach((input) => {
          input.addEventListener("change", calculateLifeInsuranceNeed);
          input.addEventListener("blur", calculateLifeInsuranceNeed);
        });

        const calcBtn = document.getElementById("calculate-btn");
        if (calcBtn)
          calcBtn.addEventListener("click", calculateLifeInsuranceNeed);

        const resetBtn = document.getElementById("reset-btn");
        if (resetBtn) resetBtn.addEventListener("click", resetCalculator);

        // Simple FAQ toggle (in addition to global script_faq if present)
        document
          .querySelectorAll("#faq-container .faq-question")
          .forEach((btn) => {
            btn.addEventListener("click", () => {
              const answer = btn.parentElement.querySelector(".faq-answer");
              const icon = btn.querySelector("span:last-child");
              if (answer.classList.contains("hidden")) {
                answer.classList.remove("hidden");
                icon.textContent = "−";
              } else {
                answer.classList.add("hidden");
                icon.textContent = "+";
              }
            });
          });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
