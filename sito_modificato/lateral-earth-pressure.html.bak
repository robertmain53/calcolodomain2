    <script>
      // Utility conversions
      function degToRad(d) {
        return (d * Math.PI) / 180;
      }

      function convertLengthToMeters(value, unit) {
        if (!isFinite(value)) return NaN;
        return unit === "ft" ? value * 0.3048 : value;
      }

      function convertGammaToSI(value, unit) {
        if (!isFinite(value)) return NaN;
        // kN/m3 to kN/m3, pcf to kN/m3
        return unit === "pcf" ? value * 0.157087 : value;
      }

      function convertStressToKPa(value, unit) {
        if (!isFinite(value)) return NaN;
        // kPa to kPa, psf to kPa
        return unit === "psf" ? value * 0.0478803 : value;
      }

      function formatNumber(x, digits) {
        if (!isFinite(x)) return "–";
        return x.toFixed(digits);
      }

      function calculateCoefficients(phiDeg, k0Method, ocr) {
        const phiRad = degToRad(phiDeg);
        const sinPhi = Math.sin(phiRad);
        const Ka = (1 - sinPhi) / (1 + sinPhi);
        const Kp = (1 + sinPhi) / (1 - sinPhi);
        let K0 = 1 - sinPhi;
        if (k0Method === "overconsolidated") {
          K0 = (1 - sinPhi) * Math.pow(ocr, sinPhi);
        }
        return { Ka, Kp, K0 };
      }

      function buildPressureChart(
        Hm,
        K,
        gamma,
        qkPa,
        hasWater,
        gwDepth,
        gammaW,
        containerId
      ) {
        const container = document.getElementById(containerId);
        container.innerHTML = "";
        if (!isFinite(Hm) || Hm <= 0) return;

        const n = 16;
        for (let i = 0; i <= n; i++) {
          const z = (Hm * i) / n; // depth from top
          let sigmaSoil = K * gamma * z; // kPa
          let sigmaWater = 0;
          if (hasWater && z > gwDepth) {
            const zw = z - gwDepth;
            sigmaSoil = K * (gamma - gammaW) * zw + K * gamma * gwDepth;
            sigmaWater = gammaW * zw;
          }
          const sigmaTotal = Math.max(0, sigmaSoil) + sigmaWater;
          const relHeight = (i + 1) / (n + 1);

          const barWrapper = document.createElement("div");
          barWrapper.className = "flex-1 flex flex-col justify-end";

          const bar = document.createElement("div");
          bar.className =
            "chart-bar mx-0.5 rounded-t bg-gradient-to-r from-blue-400 to-cyan-400";
          const maxRef =
            K * gamma * Hm +
            (hasWater ? gammaW * Math.max(0, Hm - gwDepth) : 0);
          const hPct = maxRef > 0 ? (sigmaTotal / maxRef) * 100 : 0;
          bar.style.height = hPct * 0.9 + "%";

          barWrapper.appendChild(bar);
          container.appendChild(barWrapper);
        }
      }

      document.addEventListener("DOMContentLoaded", function () {
        const gwToggle = document.getElementById("gwToggle");
        const gwDepthWrapper = document.getElementById("gwDepthWrapper");
        const heightUnitSelect = document.getElementById("heightUnit");
        const gwDepthUnitLabel = document.getElementById("gwDepthUnitLabel");
        const k0MethodSelect = document.getElementById("k0Method");
        const ocrWrapper = document.getElementById("ocrWrapper");

        gwToggle.addEventListener("change", () => {
          gwDepthWrapper.classList.toggle("hidden", !gwToggle.checked);
        });

        heightUnitSelect.addEventListener("change", () => {
          gwDepthUnitLabel.textContent = heightUnitSelect.value;
        });

        k0MethodSelect.addEventListener("change", () => {
          ocrWrapper.classList.toggle(
            "hidden",
            k0MethodSelect.value !== "overconsolidated"
          );
        });

        document.getElementById("resetBtn").addEventListener("click", () => {
          document.getElementById("wallHeight").value = 5;
          document.getElementById("heightUnit").value = "m";
          document.getElementById("gamma").value = 18;
          document.getElementById("gammaUnit").value = "kN/m3";
          document.getElementById("phi").value = 30;
          document.getElementById("cohesion").value = 0;
          document.getElementById("cohesionUnit").value = "kPa";
          document.getElementById("surcharge").value = 0;
          document.getElementById("surchargeUnit").value = "kPa";
          gwToggle.checked = false;
          gwDepthWrapper.classList.add("hidden");
          document.getElementById("gwDepth").value = 3;
          document.querySelector(
            'input[name="state"][value="active"]'
          ).checked = true;
          document.getElementById("k0Method").value = "jaky";
          ocrWrapper.classList.add("hidden");
          document.getElementById("ocr").value = 2;
          document.getElementById("errorMsg").textContent = "";
          document.getElementById("kaOut").textContent = "–";
          document.getElementById("kpOut").textContent = "–";
          document.getElementById("k0Out").textContent = "–";
          document.getElementById("pSoilOut").textContent = "–";
          document.getElementById("zSoilOut").textContent = "–";
          document.getElementById("pSurchargeOut").textContent = "–";
          document.getElementById("zSurchargeOut").textContent = "–";
          document.getElementById("pressureChart").innerHTML = "";
        });

        document
          .getElementById("calculateBtn")
          .addEventListener("click", () => {
            const errEl = document.getElementById("errorMsg");
            errEl.textContent = "";

            const H = parseFloat(document.getElementById("wallHeight").value);
            const Hunit = document.getElementById("heightUnit").value;
            const gamma = parseFloat(document.getElementById("gamma").value);
            const gammaUnit = document.getElementById("gammaUnit").value;
            const phi = parseFloat(document.getElementById("phi").value);
            const c = parseFloat(document.getElementById("cohesion").value);
            const cUnit = document.getElementById("cohesionUnit").value;
            const q = parseFloat(document.getElementById("surcharge").value);
            const qUnit = document.getElementById("surchargeUnit").value;
            const state = document.querySelector(
              'input[name="state"]:checked'
            ).value;
            const k0Method = document.getElementById("k0Method").value;
            const ocr = parseFloat(document.getElementById("ocr").value);

            if (!isFinite(H) || H <= 0) {
              errEl.textContent = "Please enter a valid positive wall height.";
              return;
            }
            if (!isFinite(gamma) || gamma <= 0) {
              errEl.textContent =
                "Please enter a valid positive soil unit weight.";
              return;
            }
            if (!isFinite(phi) || phi < 0 || phi >= 90) {
              errEl.textContent =
                "Please enter a realistic friction angle between 0° and 90°.";
              return;
            }
            if (
              k0Method === "overconsolidated" &&
              (!isFinite(ocr) || ocr < 1)
            ) {
              errEl.textContent =
                "Please enter OCR ≥ 1 for overconsolidated soils.";
              return;
            }

            const Hm = convertLengthToMeters(H, Hunit);
            const gammaSI = convertGammaToSI(gamma, gammaUnit); // kN/m3
            const cKPa = convertStressToKPa(c, cUnit);
            const qKPa = convertStressToKPa(q, qUnit);

            const hasWater = gwToggle.checked;
            let gwDepth = 0;
            const gammaW = (9.81 / 1e3) * 1000; // ~9.81 kN/m3, but keep as 9.81
            if (hasWater) {
              gwDepth = parseFloat(document.getElementById("gwDepth").value);
              if (!isFinite(gwDepth) || gwDepth < 0 || gwDepth > H) {
                errEl.textContent =
                  "Water level depth must be between 0 and wall height.";
                return;
              }
              gwDepth = convertLengthToMeters(gwDepth, Hunit);
            }

            const { Ka, Kp, K0 } = calculateCoefficients(phi, k0Method, ocr);
            document.getElementById("kaOut").textContent = formatNumber(Ka, 3);
            document.getElementById("kpOut").textContent = formatNumber(Kp, 3);
            document.getElementById("k0Out").textContent = formatNumber(K0, 3);

            let K = Ka;
            if (state === "passive") K = Kp;
            if (state === "atrest") K = K0;

            // Resultant from soil (ignoring cohesion in resultant magnitude for simplicity)
            let Psoil_kN_per_m = 0;
            let zSoil = Hm / 3;

            if (!hasWater) {
              Psoil_kN_per_m = (0.5 * K * gammaSI * Hm * Hm) / 1000; // convert kN/m2 * m to kN/m
            } else {
              const Hw = Math.max(0, Hm - gwDepth);
              const gammaSub = gammaSI - gammaW;
              const Pabove =
                (0.5 * K * gammaSI * Math.min(gwDepth, Hm) ** 2) / 1000;
              const Pbelow =
                (0.5 * K * gammaSub * Hw ** 2) / 1000 +
                (K * gammaSI * gwDepth * Hw) / 1000;
              Psoil_kN_per_m = Pabove + Pbelow;
              // Approximate centroid still near H/3 from base for engineering estimate
            }

            // Resultant from surcharge
            let Pq_kN_per_m = 0;
            let zq = Hm / 2;
            if (qKPa > 0) {
              Pq_kN_per_m = (K * qKPa * Hm) / 1000;
            }

            // Output in kN/m and lb/ft
            const kNpm_to_lbpf = 68.5218; // 1 kN/m ≈ 68.5218 lb/ft
            const Psoil_lbpf = Psoil_kN_per_m * kNpm_to_lbpf;
            const Pq_lbpf = Pq_kN_per_m * kNpm_to_lbpf;
            const zSoil_ft = Hunit === "ft" ? Hm / 0.3048 / 3 : zSoil / 0.3048;
            const zq_ft = Hunit === "ft" ? Hm / 0.3048 / 2 : zq / 0.3048;

            document.getElementById("pSoilOut").textContent = isFinite(
              Psoil_kN_per_m
            )
              ? `${formatNumber(Psoil_kN_per_m, 2)} kN/m (${formatNumber(
                  Psoil_lbpf,
                  1
                )} lb/ft)`
              : "–";
            document.getElementById("zSoilOut").textContent = isFinite(zSoil)
              ? `${formatNumber(
                  Hunit === "m" ? zSoil : zSoil / 0.3048,
                  2
                )} ${Hunit}`
              : "–";

            document.getElementById("pSurchargeOut").textContent =
              Pq_kN_per_m > 0
                ? `${formatNumber(Pq_kN_per_m, 2)} kN/m (${formatNumber(
                    Pq_lbpf,
                    1
                  )} lb/ft)`
                : "0";
            document.getElementById("zSurchargeOut").textContent =
              Pq_kN_per_m > 0
                ? `${formatNumber(
                    Hunit === "m" ? zq : zq / 0.3048,
                    2
                  )} ${Hunit}`
                : "–";

            // Build pressure chart
            buildPressureChart(
              Hm,
              K,
              gammaSI,
              qKPa,
              hasWater,
              gwDepth,
              gammaW,
              "pressureChart"
            );
          });
      });
    </script>

