    <script>
      // Core ampacity dataset (simplified, NEC 310.16 style, 30°C, up to 3 CCC)
      // Values are typical and for educational use only.
      const ampacityData = {
        CU: {
          14: { 60: 15, 75: 20, 90: 25 },
          12: { 60: 20, 75: 25, 90: 30 },
          10: { 60: 30, 75: 35, 90: 40 },
          8: { 60: 40, 75: 50, 90: 55 },
          6: { 60: 55, 75: 65, 90: 75 },
          4: { 60: 70, 75: 85, 90: 95 },
          3: { 60: 85, 75: 100, 90: 110 },
          2: { 60: 95, 75: 115, 90: 130 },
          1: { 60: 110, 75: 130, 90: 145 },
          "1/0": { 60: 125, 75: 150, 90: 170 },
          "2/0": { 60: 145, 75: 175, 90: 195 },
          "3/0": { 60: 165, 75: 200, 90: 225 },
          "4/0": { 60: 195, 75: 230, 90: 260 },
          250: { 60: 215, 75: 255, 90: 290 },
          300: { 60: 240, 75: 285, 90: 320 },
          350: { 60: 260, 75: 310, 90: 350 },
          400: { 60: 280, 75: 335, 90: 380 },
          500: { 60: 320, 75: 380, 90: 430 },
          600: { 60: 355, 75: 420, 90: 475 },
          700: { 60: 385, 75: 460, 90: 520 },
          750: { 60: 400, 75: 475, 90: 535 },
          800: { 60: 410, 75: 490, 90: 555 },
          900: { 60: 435, 75: 520, 90: 585 },
          1000: { 60: 455, 75: 545, 90: 615 },
        },
        AL: {
          12: { 60: 15, 75: 20, 90: 25 },
          10: { 60: 25, 75: 30, 90: 35 },
          8: { 60: 30, 75: 40, 90: 45 },
          6: { 60: 40, 75: 50, 90: 55 },
          4: { 60: 55, 75: 65, 90: 75 },
          3: { 60: 65, 75: 75, 90: 85 },
          2: { 60: 75, 75: 90, 90: 100 },
          1: { 60: 85, 75: 100, 90: 115 },
          "1/0": { 60: 100, 75: 120, 90: 135 },
          "2/0": { 60: 115, 75: 135, 90: 150 },
          "3/0": { 60: 130, 75: 155, 90: 175 },
          "4/0": { 60: 150, 75: 180, 90: 205 },
          250: { 60: 170, 75: 205, 90: 230 },
          300: { 60: 190, 75: 230, 90: 255 },
          350: { 60: 210, 75: 250, 90: 280 },
          400: { 60: 225, 75: 270, 90: 305 },
          500: { 60: 260, 75: 310, 90: 350 },
          600: { 60: 285, 75: 340, 90: 385 },
          700: { 60: 315, 75: 375, 90: 420 },
          750: { 60: 320, 75: 385, 90: 435 },
          800: { 60: 330, 75: 395, 90: 445 },
          900: { 60: 355, 75: 425, 90: 480 },
          1000: { 60: 375, 75: 445, 90: 500 },
        },
      };

      // Temperature correction factors (approximate) for 30–80°C
      // Keys are ambient °C upper bounds.
      const tempFactors = {
        60: {
          // 60°C rated insulation
          25: 1.08,
          30: 1.0,
          35: 0.91,
          40: 0.82,
          45: 0.71,
          50: 0.58,
          55: 0.41,
          60: 0.29,
          70: 0.0,
          80: 0.0,
        },
        75: {
          // 75°C rated insulation
          25: 1.05,
          30: 1.0,
          35: 0.94,
          40: 0.88,
          45: 0.82,
          50: 0.75,
          55: 0.67,
          60: 0.58,
          70: 0.33,
          80: 0.0,
        },
        90: {
          // 90°C rated insulation
          25: 1.04,
          30: 1.0,
          35: 0.96,
          40: 0.91,
          45: 0.87,
          50: 0.82,
          55: 0.76,
          60: 0.71,
          70: 0.58,
          80: 0.41,
        },
      };

      // Conductor count adjustment factors (NEC 310.15(B)(3)(a) style)
      function conductorFactor(n) {
        if (n <= 3) return 1.0;
        if (n <= 6) return 0.8;
        if (n <= 9) return 0.7;
        if (n <= 20) return 0.5;
        if (n <= 30) return 0.45;
        if (n <= 40) return 0.4;
        return 0.35;
      }

      function getTempFactor(rating, ambientC) {
        const table = tempFactors[String(rating)];
        if (!table) return 1.0;
        const keys = Object.keys(table)
          .map((k) => parseFloat(k))
          .sort((a, b) => a - b);
        for (let i = 0; i < keys.length; i++) {
          if (ambientC <= keys[i]) {
            return table[keys[i]];
          }
        }
        return table[keys[keys.length - 1]];
      }

      function populateSizeOptions() {
        const sizeSelect = document.getElementById("size");
        sizeSelect.innerHTML = "";
        const material = document.getElementById("material").value;
        const sizes = Object.keys(ampacityData[material]).sort((a, b) => {
          // custom sort for AWG/kcmil
          const parseSize = (s) => {
            if (s.includes("/0")) {
              const parts = s.split("/")[0];
              return 1000 + parseInt(parts, 10); // 1/0 -> 1001, 2/0 -> 1002...
            }
            return parseInt(s, 10);
          };
          return parseSize(a) - parseSize(b);
        });
        sizes.forEach((size) => {
          const opt = document.createElement("option");
          opt.value = size;
          opt.textContent =
            size +
            (parseInt(size) >= 250 || size.includes("/0") ? " kcmil" : " AWG");
          sizeSelect.appendChild(opt);
        });
      }

      function insulationToRating() {
        const ins = document.getElementById("insulation").value;
        if (ins === "THHN_90") return 90;
        if (ins === "THWN_75") return 75;
        return 60;
      }

      function formatAmp(value) {
        if (!isFinite(value)) return "–";
        if (value <= 0) return "–";
        return value.toFixed(1).replace(/\.0$/, "") + " A";
      }

      function calculateAmpacity() {
        const material = document.getElementById("material").value;
        const size = document.getElementById("size").value;
        const rating = insulationToRating();
        const ambient = parseFloat(
          document.getElementById("ambient").value || "30"
        );
        const ambientUnit = document.getElementById("ambient-unit").value;
        const ccc = parseInt(document.getElementById("ccc").value || "3", 10);
        const continuous =
          document.getElementById("continuous-load").value === "yes";

        let ambientC = ambient;
        if (ambientUnit === "F") {
          ambientC = ((ambient - 32) * 5) / 9;
        }

        const data = ampacityData[material][size];
        if (!data) {
          alert("No ampacity data available for this combination.");
          return;
        }
        const base = data[String(rating)];
        if (!base) {
          alert("No ampacity data available for this insulation rating.");
          return;
        }

        const fT = getTempFactor(rating, ambientC);
        const fN = conductorFactor(ccc);

        const tempAmp = base * fT;
        const adjAmp = tempAmp * fN;
        const contAmp = continuous ? adjAmp * 0.8 : adjAmp;

        document.getElementById("base-ampacity").textContent = formatAmp(base);
        document.getElementById("temp-ampacity").textContent =
          formatAmp(tempAmp);
        document.getElementById("adjusted-ampacity").textContent =
          formatAmp(adjAmp);
        document.getElementById("continuous-ampacity").textContent =
          formatAmp(contAmp);
      }

      function buildQuickTable() {
        const tbody = document.getElementById("quick-table-body");
        tbody.innerHTML = "";
        const sizes = new Set([
          ...Object.keys(ampacityData.CU),
          ...Object.keys(ampacityData.AL),
        ]);
        const sorted = Array.from(sizes).sort((a, b) => {
          const parseSize = (s) => {
            if (s.includes("/0")) {
              const parts = s.split("/")[0];
              return 1000 + parseInt(parts, 10);
            }
            return parseInt(s, 10);
          };
          return parseSize(a) - parseSize(b);
        });
        sorted.forEach((size) => {
          const cu = ampacityData.CU[size] || {};
          const al = ampacityData.AL[size] || {};
          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="px-2 py-1 text-left">${size}</td>
        <td class="px-2 py-1">${cu["60"] || "–"}</td>
        <td class="px-2 py-1">${cu["75"] || "–"}</td>
        <td class="px-2 py-1">${cu["90"] || "–"}</td>
        <td class="px-2 py-1">${al["60"] || "–"}</td>
        <td class="px-2 py-1">${al["75"] || "–"}</td>
        <td class="px-2 py-1">${al["90"] || "–"}</td>
      `;
          tbody.appendChild(tr);
        });
      }

      document.addEventListener("DOMContentLoaded", () => {
        populateSizeOptions();
        buildQuickTable();
        document.getElementById("material").addEventListener("change", () => {
          populateSizeOptions();
        });
        document
          .getElementById("calc-btn")
          .addEventListener("click", calculateAmpacity);
      });
    </script>

