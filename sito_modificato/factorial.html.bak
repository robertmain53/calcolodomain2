    <script>
      // Utility: compute factorial using BigInt up to a limit
      function factorialBigInt(n) {
        let result = 1n;
        for (let i = 2n; i <= n; i++) {
          result *= i;
        }
        return result;
      }

      // Utility: log10(n!) using sum of logs
      function log10Factorial(n) {
        if (n === 0 || n === 1) return 0;
        let sum = 0;
        for (let i = 2; i <= n; i++) {
          sum += Math.log10(i);
        }
        return sum;
      }

      // Stirling approximation log10(n!)
      function log10FactorialStirling(n) {
        const ln10 = Math.log(10);
        const ln2pi = Math.log(2 * Math.PI);
        const lnN = Math.log(n);
        const lnFact = 0.5 * (ln2pi + lnN) + n * (lnN - 1); // ln(n!)
        return lnFact / ln10;
      }

      function scientificFromLog10(log10nFact) {
        const digits = Math.floor(log10nFact) + 1;
        const mantissa = Math.pow(10, log10nFact - Math.floor(log10nFact));
        return { mantissa, digits };
      }

      function formatBigIntWithGrouping(bi) {
        const s = bi.toString();
        return s.replace(/\B(?=(\d{3})+(?!\d))/g, ",");
      }

      function computePermutation(n, r) {
        if (r === 0) return 1n;
        let result = 1n;
        for (let i = 0; i < r; i++) {
          result *= BigInt(n - i);
        }
        return result;
      }

      function computeCombination(n, r) {
        if (r === 0 || r === n) return 1n;
        // Use symmetry to minimize iterations
        r = Math.min(r, n - r);
        let numerator = 1n;
        let denominator = 1n;
        for (let i = 1; i <= r; i++) {
          numerator *= BigInt(n - r + i);
          denominator *= BigInt(i);
        }
        return numerator / denominator;
      }

      function updateModeUI() {
        const mode = document.querySelector('input[name="mode"]:checked').value;
        const rContainer = document.getElementById("r-container");
        if (mode === "factorial") {
          rContainer.classList.add("hidden");
        } else {
          rContainer.classList.remove("hidden");
        }
      }

      document.addEventListener("DOMContentLoaded", function () {
        const nInput = document.getElementById("input-n");
        const rInput = document.getElementById("input-r");
        const btnCalc = document.getElementById("btn-calc");
        const btnClear = document.getElementById("btn-clear");
        const errorMsg = document.getElementById("error-msg");

        const resultMain = document.getElementById("result-main");
        const resultScientific = document.getElementById("result-scientific");
        const resultDigits = document.getElementById("result-digits");
        const resultSteps = document.getElementById("result-steps");
        const resultRelated = document.getElementById("result-related");

        document.querySelectorAll('input[name="mode"]').forEach((radio) => {
          radio.addEventListener("change", updateModeUI);
        });
        updateModeUI();

        btnCalc.addEventListener("click", function () {
          errorMsg.textContent = "";
          resultScientific.textContent = "";
          resultDigits.textContent = "";

          let n = parseInt(nInput.value, 10);
          if (isNaN(n)) {
            errorMsg.textContent = "Please enter an integer value for n.";
            return;
          }
          if (n < 0) {
            errorMsg.textContent = "n must be a non-negative integer.";
            return;
          }
          if (n > 10000) {
            errorMsg.textContent = "n is too large. Please enter n ≤ 10,000.";
            return;
          }

          const mode = document.querySelector(
            'input[name="mode"]:checked'
          ).value;
          let r = null;
          if (mode !== "factorial") {
            r = parseInt(rInput.value, 10);
            if (isNaN(r)) {
              errorMsg.textContent = "Please enter an integer value for r.";
              return;
            }
            if (r < 0 || r > n) {
              errorMsg.textContent = "r must satisfy 0 ≤ r ≤ n.";
              return;
            }
          }

          if (mode === "factorial") {
            // Factorial mode
            if (n <= 500) {
              const fact = factorialBigInt(BigInt(n));
              resultMain.textContent = `${n}! = ${formatBigIntWithGrouping(
                fact
              )}`;

              // Steps
              if (n === 0 || n === 1) {
                resultSteps.textContent = `${n}! = 1`;
              } else if (n <= 30) {
                const factors = [];
                for (let i = n; i >= 1; i--) factors.push(i);
                resultSteps.textContent = `${n}! = ${factors.join(
                  " × "
                )} = ${formatBigIntWithGrouping(fact)}`;
              } else {
                resultSteps.textContent = `${n}! = 1 × 2 × 3 × ... × ${n} (exact value shown above)`;
              }

              const log10nFact = log10Factorial(n);
              const sci = scientificFromLog10(log10nFact);
              resultScientific.textContent = `Scientific notation: ≈ ${sci.mantissa.toPrecision(
                6
              )} × 10^${sci.digits - 1}`;
              resultDigits.textContent = `Number of digits: ${sci.digits.toLocaleString()}`;
            } else {
              // Large n: approximation
              const log10nFact =
                n <= 2000 ? log10Factorial(n) : log10FactorialStirling(n);
              const sci = scientificFromLog10(log10nFact);
              resultMain.textContent = `${n}! is approximately ${sci.mantissa.toPrecision(
                6
              )} × 10^${sci.digits - 1}`;
              resultSteps.textContent = `${n}! is too large to display exactly. Using logarithms/Stirling's approximation to estimate its size.`;
              resultScientific.textContent = `Scientific notation (approx): ${sci.mantissa.toPrecision(
                10
              )} × 10^${sci.digits - 1}`;
              resultDigits.textContent = `Approximate number of digits: ${sci.digits.toLocaleString()}`;
            }

            // Related values: nPn and nCn
            if (n <= 200) {
              const nFact = n <= 500 ? factorialBigInt(BigInt(n)) : null;
              if (nFact) {
                const nPn = nFact; // nPn = n!
                const nCn = 1n; // nCn = 1
                resultRelated.textContent = `For n = ${n}: nP${n} = ${formatBigIntWithGrouping(
                  nPn
                )}, nC${n} = ${nCn}`;
              } else {
                resultRelated.textContent = `For n = ${n}: nP${n} = n!, nC${n} = 1 (values too large to show exactly).`;
              }
            } else {
              resultRelated.textContent = `For n = ${n}: nP${n} = n!, nC${n} = 1 (values extremely large).`;
            }
          } else if (mode === "npr") {
            // Permutations
            if (n > 1000) {
              errorMsg.textContent =
                "For permutations, please use n ≤ 1000 to keep results manageable.";
              return;
            }
            const perm = computePermutation(n, r);
            resultMain.textContent = `${n}P${r} = ${formatBigIntWithGrouping(
              perm
            )}`;

            if (n <= 50) {
              const factors = [];
              for (let i = n; i > n - r; i--) factors.push(i);
              resultSteps.textContent = `${n}P${r} = ${n}! / (n - r)! = ${factors.join(
                " × "
              )} = ${formatBigIntWithGrouping(perm)}`;
            } else {
              resultSteps.textContent = `${n}P${r} = n! / (n - r)! (exact value shown above)`;
            }

            const log10nFact = log10Factorial(n);
            const log10nrFact = log10Factorial(n - r);
            const log10Perm = log10nFact - log10nrFact;
            const sci = scientificFromLog10(log10Perm);
            resultScientific.textContent = `Scientific notation: ≈ ${sci.mantissa.toPrecision(
              6
            )} × 10^${sci.digits - 1}`;
            resultDigits.textContent = `Number of digits: ${sci.digits.toLocaleString()}`;

            // Related combination
            const comb = computeCombination(n, r);
            resultRelated.textContent = `Related: ${n}C${r} = ${formatBigIntWithGrouping(
              comb
            )}`;
          } else if (mode === "ncr") {
            // Combinations
            if (n > 2000) {
              errorMsg.textContent =
                "For combinations, please use n ≤ 2,000 to keep results manageable.";
              return;
            }
            const comb = computeCombination(n, r);
            resultMain.textContent = `${n}C${r} = ${formatBigIntWithGrouping(
              comb
            )}`;

            if (n <= 50) {
              resultSteps.textContent = `${n}C${r} = n! / (r!(n - r)!) (exact value shown above)`;
            } else {
              resultSteps.textContent = `${n}C${r} computed using an efficient multiplicative formula (exact value shown above).`;
            }

            const log10nFact = log10Factorial(n);
            const log10rFact = log10Factorial(r);
            const log10nrFact = log10Factorial(n - r);
            const log10Comb = log10nFact - log10rFact - log10nrFact;
            const sci = scientificFromLog10(log10Comb);
            resultScientific.textContent = `Scientific notation: ≈ ${sci.mantissa.toPrecision(
              6
            )} × 10^${sci.digits - 1}`;
            resultDigits.textContent = `Number of digits: ${sci.digits.toLocaleString()}`;

            // Related permutation
            const perm = computePermutation(n, r);
            resultRelated.textContent = `Related: ${n}P${r} = ${formatBigIntWithGrouping(
              perm
            )}`;
          }
        });

        btnClear.addEventListener("click", function () {
          nInput.value = "5";
          rInput.value = "3";
          document.querySelector(
            'input[name="mode"][value="factorial"]'
          ).checked = true;
          updateModeUI();
          errorMsg.textContent = "";
          resultMain.textContent = "5! = 120";
          resultScientific.textContent = "";
          resultDigits.textContent = "";
          resultSteps.textContent = "5! = 5 × 4 × 3 × 2 × 1 = 120";
          resultRelated.textContent = "For n = 5: 5P3 = 60, 5C3 = 10";
        });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
