        function calculateForces() {
            const supplierPower = parseInt(document.getElementById('bargainingPowerOfSuppliers').value, 10);
            const buyerPower = parseInt(document.getElementById('bargainingPowerOfBuyers').value, 10);
            const newEntrants = parseInt(document.getElementById('threatOfNewEntrants').value, 10);
            const substitutes = parseInt(document.getElementById('threatOfSubstitutes').value, 10);
            const rivalry = parseInt(document.getElementById('industryRivalry').value, 10);

            if (isNaN(supplierPower) || isNaN(buyerPower) || isNaN(newEntrants) || isNaN(substitutes) || isNaN(rivalry)) {
                document.getElementById('errorMessage').textContent = 'Please enter a valid number between 1 and 5 for all fields.';
                return;
            }

            const totalScore = supplierPower + buyerPower + newEntrants + substitutes + rivalry;

            document.getElementById('totalScore').textContent = totalScore;
            document.getElementById('errorMessage').textContent = '';
        }
    </script>


