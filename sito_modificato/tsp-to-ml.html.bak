  <script>
    // teaspoon sizes in mL
    const TSP_SIZES = {
      us: 4.92892,
      metric: 5,
      uk: 5.91939,
      med: 5
    };

    const tspInput = document.getElementById('tsp-value');
    const mlInput = document.getElementById('ml-value');
    const standardRadios = document.querySelectorAll('input[name="tsp-standard"]');
    const oneTspMl = document.getElementById('one-tsp-ml');
    const stdLabel = document.getElementById('current-standard-label');

    const tbl1 = document.getElementById('tbl-1');
    const tbl2 = document.getElementById('tbl-2');
    const tbl3 = document.getElementById('tbl-3');
    const tbl4 = document.getElementById('tbl-4');
    const tbl5 = document.getElementById('tbl-5');

    function getCurrentStandard() {
      const checked = Array.from(standardRadios).find(r => r.checked);
      return checked ? checked.value : 'us';
    }

    function updateStandardLabel() {
      const std = getCurrentStandard();
      let label = '';
      if (std === 'us') label = 'US teaspoon (1 tsp = 4.9289 mL)';
      if (std === 'metric') label = 'Metric teaspoon (1 tsp = 5 mL)';
      if (std === 'uk') label = 'UK teaspoon (1 tsp = 5.919 mL)';
      if (std === 'med') label = 'Medical teaspoon (1 tsp = 5 mL)';
      stdLabel.textContent = label;
    }

    function updateTable(std) {
      const size = TSP_SIZES[std];
      tbl1.textContent = (1 * size).toFixed(4).replace(/\.?0+$/,'');
      tbl2.textContent = (2 * size).toFixed(4).replace(/\.?0+$/,'');
      tbl3.textContent = (3 * size).toFixed(4).replace(/\.?0+$/,'');
      tbl4.textContent = (4 * size).toFixed(4).replace(/\.?0+$/,'');
      tbl5.textContent = (5 * size).toFixed(4).replace(/\.?0+$/,'');
    }

    function tspToMl() {
      const std = getCurrentStandard();
      const size = TSP_SIZES[std];
      const tspVal = parseFloat(tspInput.value || 0);
      const mlVal = tspVal * size;
      mlInput.value = +mlVal.toFixed(4);
      oneTspMl.textContent = size.toFixed(4).replace(/\.?0+$/,'');
    }

    function mlToTsp() {
      const std = getCurrentStandard();
      const size = TSP_SIZES[std];
      const mlVal = parseFloat(mlInput.value || 0);
      if (size === 0) return;
      const tspVal = mlVal / size;
      tspInput.value = +tspVal.toFixed(4);
      oneTspMl.textContent = size.toFixed(4).replace(/\.?0+$/,'');
    }

    tspInput.addEventListener('input', tspToMl);
    mlInput.addEventListener('input', mlToTsp);
    standardRadios.forEach(r => {
      r.addEventListener('input', () => {
        updateStandardLabel();
        updateTable(getCurrentStandard());
        // Recompute based on current active field – default from tsp
        tspToMl();
      });
    });

    // init
    updateStandardLabel();
    updateTable('us');
    tspToMl();
  </script>

