        function calculateResults() {
            // Example calculation logic
            const beamLength = parseFloat(document.getElementById('beamLength').value);
            const concreteStrength = parseFloat(document.getElementById('concreteStrength').value);

            if (!beamLength || !concreteStrength) {
                alert('Please fill out all required fields.');
                return;
            }

            const momentCapacity = (concreteStrength * beamLength).toFixed(2);
            const neutralAxis = (beamLength / 10).toFixed(2);

            document.getElementById('momentCapacity').textContent = momentCapacity + ' kNm';
            document.getElementById('neutralAxis').textContent = neutralAxis + ' m';
        }
    </script>


