<div aria-live="polite" class="error-message" id="beamLengthError" role="alert"></div>
</div>
<div class="form-group">
<label for="steelGrade">Steel Grade *</label>
<input aria-describedby="steelGradeError" aria-required="true" class="form-control" id="steelGrade" type="text">
<div aria-live="polite" class="error-message" id="steelGradeError" role="alert"></div>
</div>
<div class="form-group">
<label for="concreteStrength">Concrete Strength (MPa) *</label>
<input aria-describedby="concreteStrengthError" aria-required="true" class="form-control" id="concreteStrength" type="number">
<div aria-live="polite" class="error-message" id="concreteStrengthError" role="alert"></div>
</div>
<button class="btn btn-primary" onclick="calculateResults()" type="button">Calculate</button>
</form>
