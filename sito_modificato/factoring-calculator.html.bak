            <script>
              document.addEventListener('DOMContentLoaded', () => {

                // --- LOGICA DI TEORIA DEI NUMERI (Fattori, Primi) ---
                const numInput = document.getElementById('num-input');
                const calcNumberBtn = document.getElementById('calc-number-btn');
                const factorsOutput = document.getElementById('factors-output');
                const primeOutput = document.getElementById('prime-output');

                function getPrimeFactors(n) {
                    let num = Math.abs(n);
                    let factors = {};
                    let divisor = 2;
                    while (num >= 2) {
                        if (num % divisor === 0) {
                            factors[divisor] = (factors[divisor] || 0) + 1;
                            num / divisor;
                        } else {
                            divisor++;
                        }
                    }
                    return factors;
                }

                calcNumberBtn.addEventListener('click', () => {
                    let n = parseInt(numInput.value);
                    if (isNaN(n) || n === 0) {
                        factorsOutput.textContent = '-';
                        primeOutput.textContent = '-';
                        return;
                    }

                    n = Math.abs(n);
                    if (n === 1) {
                         factorsOutput.textContent = '1';
                         primeOutput.textContent = 'Prime Factorization: None (1 is not prime)';
                         return;
                    }

                    // Calculate all factors
                    const allFactors = [];
                    for (let i = 1; i * i <= n; i++) {
                        if (n % i === 0) {
                            allFactors.push(i);
                            if (i * i !== n) {
                                allFactors.push(n / i);
                            }
                        }
                    }
                    allFactors.sort((a, b) => a - b);
                    factorsOutput.textContent = allFactors.join(', ');

                    // Calculate prime factorization
                    const primeFactors = getPrimeFactors(n);
                    let primeString = '';
                    for (const factor in primeFactors) {
                        const count = primeFactors[factor];
                        primeString += `${factor}${count > 1 ? `^${count}` : ''} × `;
                    }
                    primeOutput.textContent = primeString.slice(0, -3); // Rimuovi l'ultimo ' × '
                });

                // --- LOGICA STRUMENTO 1: FATTORIZZAZIONE QUADRATICA ---
                const aInput = document.getElementById('a-input');
                const bInput = document.getElementById('b-input');
                const cInput = document.getElementById('c-input');
                const calcQuadraticBtn = document.getElementById('calc-quadratic-btn');
                const quadraticOutput = document.getElementById('quadratic-output');
                const factoredFormOutput = document.getElementById('factored-form');
                const quadraticSteps = document.getElementById('quadratic-steps');
                const polyError = document.getElementById('poly-error');

                function solveQuadratic(a, b, c) {
                    const discriminant = b * b - 4 * a * c;
                    
                    if (discriminant < 0) {
                        return { error: 'Non-real roots. Cannot be factored cleanly into real binomials.' };
                    }
                    
                    // Radici (solutions)
                    const sqrtD = Math.sqrt(discriminant);
                    const root1 = (-b + sqrtD) / (2 * a);
                    const root2 = (-b - sqrtD) / (2 * a);

                    // Verifica se le radici sono interi o frazioni semplici
                    if (Math.abs(root1 - Math.round(root1)) > 1e-9 || Math.abs(root2 - Math.round(root2)) > 1e-9) {
                        return { error: 'Roots are not simple integers. Use the Quadratic Formula.' };
                    }

                    // Se le radici sono intere, i fattori sono (x - root)
                    const f1 = -Math.round(root1);
                    const f2 = -Math.round(root2);
                    
                    // The factored form is A * (x - root1)(x - root2)
                    let factoredString = '';
                    if (a !== 1) { factoredString += a; }
                    factoredString += `(x ${f1 >= 0 ? '+' : '-'} ${Math.abs(f1)})`;
                    factoredString += `(x ${f2 >= 0 ? '+' : '-'} ${Math.abs(f2)})`;
                    
                    return { factors: { r1: root1, r2: root2, f1: f1, f2: f2 }, factoredString: factoredString };
                }

                function formatBinomial(c) {
                    if (c === 0) return '';
                    const sign = c > 0 ? '+' : '-';
                    const absC = Math.abs(c);
                    return `${sign} ${absC}`;
                }

                calcQuadraticBtn.addEventListener('click', () => {
                    const a = parseFloat(aInput.value);
                    const b = parseFloat(bInput.value);
                    const c = parseFloat(cInput.value);

                    polyError.hidden = true;
                    quadraticOutput.hidden = true;
                    quadraticSteps.hidden = true;

                    if (isNaN(a) || isNaN(b) || isNaN(c)) {
                        polyError.textContent = 'Please enter valid numerical coefficients (A, B, and C).';
                        polyError.hidden = false;
                        return;
                    }
                    if (a === 0) {
                        polyError.textContent = 'A cannot be zero for a quadratic equation. This is a linear equation.';
                        polyError.hidden = false;
                        return;
                    }

                    const result = solveQuadratic(a, b, c);

                    if (result.error) {
                        polyError.textContent = result.error;
                        polyError.hidden = false;
                        return;
                    }

                    // If the result is OK
                    const roots = [-result.factors.f1, -result.factors.f2];
                    const rootsStr = roots.join(', ');
                    
                    // Generate steps (Sum-Product/Grouping method)
                    const AC = a * c;
                    const B = b;

                    // Trova M e N (i due numeri che si moltiplicano a AC e sommano a B)
                    let M = null;
                    let N = null;
                    for (let i = 1; i <= Math.sqrt(Math.abs(AC)); i++) {
                        if (AC % i === 0) {
                            const j = AC / i;
                            if (i + j === B) { M = i; N = j; break; }
                            if (i - j === B) { M = i; N = -j; break; }
                            if (-i + j === B) { M = -i; N = j; break; }
                            if (-i - j === B) { M = -i; N = -j; break; }
                        }
                    }

                    let stepsHtml = '';
                    if (M !== null) {
                        stepsHtml = `
                            <h3 class="text-lg font-semibold text-gray-800 mb-2">Step-by-Step Solution (Grouping Method):</h3>
                            <p><strong>1. Find M and N:</strong> We look for two numbers (M and N) such that M × N = AC (${formatNum(a*c)}) and M + N = B (${formatNum(b)}).</p>
                            <p>The numbers found are <strong>M = ${formatNum(M)}</strong> and <strong>N = ${formatNum(N)}</strong>.</p>
                            <p><strong>2. Rewrite the Equation:</strong> Split the middle term ($${formatNum(b)}x$) using M and N.</p>
                            <p class="formula-box">${formatNum(a)}x² ${formatBinomial(M)}x ${formatBinomial(N)}x ${formatBinomial(c)} = 0</p>
                            <p><strong>3. Factor by Grouping:</strong></p>
                            <p>Group 1: Factor out GCF from $${formatNum(a)}x^2 ${formatBinomial(M)}x$</p>
                            <p>Group 2: Factor out GCF from $${formatBinomial(N)}x ${formatBinomial(c)}$</p>
                            <p><strong>4. Final Factored Form:</strong></p>
                            <p class="formula-box">${result.factoredString}</p>
                            <p><strong>Roots/Solutions:</strong> $x = ${rootsStr}</p>
                        `;
                    } else {
                        stepsHtml = `
                            <h3 class="text-lg font-semibold text-gray-800 mb-2">Solution using Roots:</h3>
                            <p>This polynomial does not factor easily into simple binomials using the grouping method, but the roots are simple integers.</p>
                            <p>Using the Quadratic Formula, the roots are $x = ${rootsStr}$.</p>
                            <p>The factored form, based on the roots, is:</p>
                            <p class="formula-box">${result.factoredString}</p>
                        `;
                    }

                    factoredFormOutput.textContent = result.factoredString;
                    quadraticOutput.hidden = false;
                    quadraticSteps.innerHTML = stepsHtml;
                    quadraticSteps.hidden = false;
                    
                    // Rende le formule LaTeX
                    if (window.MathJax) {
                       MathJax.typesetPromise([quadraticSteps]);
                    }
                });
                
                // Imposta un esempio di default per il caricamento
                aInput.value = 1;
                bInput.value = 7;
                cInput.value = 10;
                // calcQuadraticBtn.click(); // Potrebbe essere troppo aggressivo al caricamento
              });
            </script>
            
          </div>



            <section class="mt-10" id="author">
              <h2 class="text-xl font-semibold mb-4">About the author</h2>
