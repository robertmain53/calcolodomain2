    <script>
      // UI state
      const unitsButtons = {
        us: document.getElementById("units-us"),
        si: document.getElementById("units-si"),
      };
      const channelTypeEl = document.getElementById("channel-type");
      const solveButtons = document.querySelectorAll(".solve-btn");
      let currentUnits = "us"; // 'us' or 'si'
      let solveFor = "Q"; // 'Q', 'V', 'y', 'S'

      const geomPipe = document.getElementById("geom-pipe");
      const geomRect = document.getElementById("geom-rect");
      const depthWrapper = document.getElementById("depth-wrapper");

      const diameterEl = document.getElementById("diameter");
      const depthEl = document.getElementById("depth");
      const depthRatioEl = document.getElementById("depth-ratio");
      const depthFullBtn = document.getElementById("depth-full-btn");
      const rectBEl = document.getElementById("rect-b");
      const rectYEl = document.getElementById("rect-y");

      const diameterUnitEl = document.getElementById("diameter-unit");
      const depthUnitEl = document.getElementById("depth-unit");
      const rectBUnitEl = document.getElementById("rect-b-unit");
      const rectYUnitEl = document.getElementById("rect-y-unit");
      const dischargeUnitEl = document.getElementById("discharge-unit");
      const velocityUnitEl = document.getElementById("velocity-unit");

      const nSelectEl = document.getElementById("n-select");
      const nValueEl = document.getElementById("n-value");

      const slopeEl = document.getElementById("slope");
      const dischargeEl = document.getElementById("discharge");
      const velocityEl = document.getElementById("velocity");

      const calcBtn = document.getElementById("calc-btn");
      const resetBtn = document.getElementById("reset-btn");
      const errorMsgEl = document.getElementById("error-msg");

      const resultsSection = document.getElementById("results-section");
      const resArea = document.getElementById("res-area");
      const resPerimeter = document.getElementById("res-perimeter");
      const resRadius = document.getElementById("res-radius");
      const resDepthH = document.getElementById("res-depth-h");
      const resVelocity = document.getElementById("res-velocity");
      const resDischarge = document.getElementById("res-discharge");
      const resSlope = document.getElementById("res-slope");
      const resDepth = document.getElementById("res-depth");
      const iterInfo = document.getElementById("iter-info");

      function setUnits(units) {
        currentUnits = units;
        const len = units === "us" ? "ft" : "m";
        const q = units === "us" ? "cfs" : "m³/s";
        const v = units === "us" ? "ft/s" : "m/s";

        diameterUnitEl.textContent = len;
        depthUnitEl.textContent = len;
        rectBUnitEl.textContent = len;
        rectYUnitEl.textContent = len;
        dischargeUnitEl.textContent = q;
        velocityUnitEl.textContent = v;

        unitsButtons.us.classList.toggle("bg-blue-600", units === "us");
        unitsButtons.us.classList.toggle("text-white", units === "us");
        unitsButtons.us.classList.toggle("bg-white", units !== "us");
        unitsButtons.us.classList.toggle("text-gray-700", units !== "us");

        unitsButtons.si.classList.toggle("bg-blue-600", units === "si");
        unitsButtons.si.classList.toggle("text-white", units === "si");
        unitsButtons.si.classList.toggle("bg-white", units !== "si");
        unitsButtons.si.classList.toggle("text-gray-700", units !== "si");
      }

      unitsButtons.us.addEventListener("click", () => setUnits("us"));
      unitsButtons.si.addEventListener("click", () => setUnits("si"));

      function updateChannelUI() {
        const type = channelTypeEl.value;
        if (type === "rectangular") {
          geomPipe.classList.add("hidden");
          geomRect.classList.remove("hidden");
        } else {
          geomPipe.classList.remove("hidden");
          geomRect.classList.add("hidden");
          depthWrapper.classList.toggle("hidden", type === "pipe-full");
        }
      }
      channelTypeEl.addEventListener("change", updateChannelUI);
      updateChannelUI();

      solveButtons.forEach((btn) => {
        btn.addEventListener("click", () => {
          solveFor = btn.getAttribute("data-solve");
          solveButtons.forEach((b) => {
            b.classList.remove(
              "border-blue-600",
              "bg-blue-50",
              "text-blue-700",
              "font-medium"
            );
            b.classList.add("border-gray-300", "text-gray-700");
          });
          btn.classList.add(
            "border-blue-600",
            "bg-blue-50",
            "text-blue-700",
            "font-medium"
          );
          btn.classList.remove("border-gray-300", "text-gray-700");
        });
      });

      nSelectEl.addEventListener("change", () => {
        const val = nSelectEl.value;
        if (val === "custom") {
          nValueEl.focus();
        } else {
          nValueEl.value = val;
        }
      });

      depthFullBtn.addEventListener("click", () => {
        const D = parseFloat(diameterEl.value);
        if (isFinite(D) && D > 0) {
          depthEl.value = D;
          depthRatioEl.value = 1.0;
        }
      });

      depthRatioEl.addEventListener("input", () => {
        const ratio = parseFloat(depthRatioEl.value);
        const D = parseFloat(diameterEl.value);
        if (isFinite(ratio) && isFinite(D) && D > 0) {
          depthEl.value = ratio * D;
        }
      });

      depthEl.addEventListener("input", () => {
        const y = parseFloat(depthEl.value);
        const D = parseFloat(diameterEl.value);
        if (isFinite(y) && isFinite(D) && D > 0) {
          depthRatioEl.value = y / D;
        }
      });

      function fmt(value, decimals = 4) {
        if (!isFinite(value)) return "–";
        return Number(value).toFixed(decimals);
      }

      function computeGeometry() {
        const type = channelTypeEl.value;
        let A, P, T, yUsed;

        if (type === "rectangular") {
          const b = parseFloat(rectBEl.value);
          const y = parseFloat(rectYEl.value);
          if (!(b > 0 && y > 0))
            throw new Error(
              "Enter positive width and depth for the rectangular channel."
            );
          A = b * y;
          P = b + 2 * y;
          T = b;
          yUsed = y;
        } else {
          const D = parseFloat(diameterEl.value);
          if (!(D > 0)) throw new Error("Enter a positive pipe diameter.");
          if (type === "pipe-full") {
            A = (Math.PI * D * D) / 4;
            P = Math.PI * D;
            T = D;
            yUsed = D;
          } else {
            const y = parseFloat(depthEl.value);
            if (!(y > 0 && y <= D))
              throw new Error(
                "Flow depth must be between 0 and D for a partially full pipe."
              );
            const r = D / 2;
            const term = (r - y) / r;
            if (term < -1 || term > 1)
              throw new Error("Invalid depth/diameter combination.");
            const theta = 2 * Math.acos(term);
            A = ((D * D) / 8) * (theta - Math.sin(theta));
            P = (D / 2) * theta;
            T = D * Math.sin(theta / 2);
            yUsed = y;
          }
        }
        const R = A / P;
        const Dh = A / T;
        return { A, P, R, T, y: yUsed };
      }

      function manningConstant() {
        return currentUnits === "us" ? 1.486 : 1.0;
      }

      function manningVelocity(R, S, n) {
        const C = manningConstant();
        if (!(R > 0 && S > 0 && n > 0)) return NaN;
        return (C / n) * Math.pow(R, 2 / 3) * Math.sqrt(S);
      }

      function solveNormalDepth(targetQ, S, n) {
        const type = channelTypeEl.value;
        if (type === "rectangular") {
          const b = parseFloat(rectBEl.value);
          if (!(b > 0))
            throw new Error(
              "Enter a positive width for the rectangular channel."
            );
          let yLow = 1e-6;
          let yHigh = 1000 * (currentUnits === "us" ? 1 : 1); // generous upper bound
          let Qlow, Qhigh;
          const C = manningConstant();

          function QofY(y) {
            const A = b * y;
            const P = b + 2 * y;
            const R = A / P;
            const V = (C / n) * Math.pow(R, 2 / 3) * Math.sqrt(S);
            return A * V;
          }

          Qlow = QofY(yLow);
          Qhigh = QofY(yHigh);
          if (!isFinite(Qlow) || !isFinite(Qhigh) || Qhigh <= 0)
            throw new Error(
              "Unable to bracket normal depth for rectangular channel."
            );

          if (targetQ <= Qlow) return { y: yLow, iterations: 0 };

          let iter = 0;
          const maxIter = 80;
          while (iter < maxIter) {
            const yMid = 0.5 * (yLow + yHigh);
            const Qmid = QofY(yMid);
            if (!isFinite(Qmid)) break;
            if (Math.abs(Qmid - targetQ) / targetQ < 1e-4) {
              return { y: yMid, iterations: iter + 1 };
            }
            if (Qmid < targetQ) {
              yLow = yMid;
            } else {
              yHigh = yMid;
            }
            iter++;
          }
          return { y: 0.5 * (yLow + yHigh), iterations: iter, warning: true };
        } else {
          const D = parseFloat(diameterEl.value);
          if (!(D > 0)) throw new Error("Enter a positive pipe diameter.");
          const C = manningConstant();

          function QofY(y) {
            const r = D / 2;
            const term = (r - y) / r;
            if (term < -1 || term > 1) return NaN;
            const theta = 2 * Math.acos(term);
            const A = ((D * D) / 8) * (theta - Math.sin(theta));
            const P = (D / 2) * theta;
            const R = A / P;
            const V = (C / n) * Math.pow(R, 2 / 3) * Math.sqrt(S);
            return A * V;
          }

          let yLow = 1e-6;
          let yHigh = D;
          let Qlow = QofY(yLow);
          let Qhigh = QofY(yHigh);
          if (!isFinite(Qlow) || !isFinite(Qhigh) || Qhigh <= 0)
            throw new Error("Unable to bracket normal depth for pipe.");

          if (targetQ <= Qlow) return { y: yLow, iterations: 0 };

          let iter = 0;
          const maxIter = 80;
          while (iter < maxIter) {
            const yMid = 0.5 * (yLow + yHigh);
            const Qmid = QofY(yMid);
            if (!isFinite(Qmid)) break;
            if (Math.abs(Qmid - targetQ) / targetQ < 1e-4) {
              return { y: yMid, iterations: iter + 1 };
            }
            if (Qmid < targetQ) {
              yLow = yMid;
            } else {
              yHigh = yMid;
            }
            iter++;
          }
          return { y: 0.5 * (yLow + yHigh), iterations: iter, warning: true };
        }
      }

      function calculate() {
        errorMsgEl.textContent = "";
        iterInfo.textContent = "";
        try {
          const n = parseFloat(nValueEl.value);
          if (!(n > 0))
            throw new Error("Enter a positive Manning roughness n.");

          let S = parseFloat(slopeEl.value);
          let Q = parseFloat(dischargeEl.value);
          let V = parseFloat(velocityEl.value);

          if (solveFor === "Q") {
            const geom = computeGeometry();
            const R = geom.R;
            if (!(S > 0))
              throw new Error(
                "Enter a positive slope S to solve for discharge."
              );
            const Vcalc = manningVelocity(R, S, n);
            const Qcalc = geom.A * Vcalc;
            V = Vcalc;
            Q = Qcalc;
            updateResults(geom, V, Q, S, geom.y, null);
          } else if (solveFor === "V") {
            const geom = computeGeometry();
            const R = geom.R;
            if (!(S > 0))
              throw new Error(
                "Enter a positive slope S to solve for velocity."
              );
            const Vcalc = manningVelocity(R, S, n);
            const Qcalc = geom.A * Vcalc;
            V = Vcalc;
            Q = Qcalc;
            updateResults(geom, V, Q, S, geom.y, null);
          } else if (solveFor === "S") {
            const geom = computeGeometry();
            const R = geom.R;
            if (!(V > 0)) {
              if (!(Q > 0))
                throw new Error(
                  "Enter either velocity V or discharge Q to solve for slope."
                );
              V = Q / geom.A;
            }
            const C = manningConstant();
            const Scalc = Math.pow((V * n) / (C * Math.pow(R, 2 / 3)), 2);
            S = Scalc;
            const Qcalc = geom.A * V;
            Q = Qcalc;
            slopeEl.value = S.toExponential(4);
            updateResults(geom, V, Q, S, geom.y, null);
          } else if (solveFor === "y") {
            if (!(Q > 0))
              throw new Error(
                "Enter a positive discharge Q to solve for normal depth."
              );
            if (!(S > 0))
              throw new Error(
                "Enter a positive slope S to solve for normal depth."
              );
            const res = solveNormalDepth(Q, S, n);
            if (!isFinite(res.y) || res.y <= 0)
              throw new Error("Failed to converge on a normal depth solution.");
            if (channelTypeEl.value === "rectangular") {
              rectYEl.value = res.y;
            } else {
              depthEl.value = res.y;
              depthRatioEl.value = res.y / parseFloat(diameterEl.value);
            }
            const geom = computeGeometry();
            const R = geom.R;
            const Vcalc = manningVelocity(R, S, n);
            const Qcalc = geom.A * Vcalc;
            iterInfo.textContent =
              "Normal depth solved iteratively in " +
              res.iterations +
              " iterations.";
            updateResults(geom, Vcalc, Qcalc, S, geom.y, res);
          }
        } catch (err) {
          errorMsgEl.textContent =
            err.message || "An error occurred. Please check your inputs.";
          resultsSection.classList.add("hidden");
        }
      }

      function updateResults(geom, V, Q, S, y, iterRes) {
        const lenUnit = currentUnits === "us" ? "ft" : "m";
        const areaUnit = currentUnits === "us" ? "ft²" : "m²";
        const qUnit = currentUnits === "us" ? "cfs" : "m³/s";
        const vUnit = currentUnits === "us" ? "ft/s" : "m/s";

        resArea.textContent = fmt(geom.A, 4) + " " + areaUnit;
        resPerimeter.textContent = fmt(geom.P, 4) + " " + lenUnit;
        resRadius.textContent = fmt(geom.R, 4) + " " + lenUnit;
        resDepthH.textContent =
          fmt(geom.T ? geom.A / geom.T : NaN, 4) + " " + lenUnit;

        resVelocity.textContent = fmt(V, 4) + " " + vUnit;
        resDischarge.textContent = fmt(Q, 4) + " " + qUnit;
        resSlope.textContent = isFinite(S) ? S.toExponential(4) : "–";
        resDepth.textContent = fmt(y, 4) + " " + lenUnit;

        resultsSection.classList.remove("hidden");
      }

      calcBtn.addEventListener("click", calculate);

      resetBtn.addEventListener("click", () => {
        setUnits("us");
        channelTypeEl.value = "pipe-full";
        updateChannelUI();
        solveFor = "Q";
        solveButtons.forEach((b) => {
          b.classList.remove(
            "border-blue-600",
            "bg-blue-50",
            "text-blue-700",
            "font-medium"
          );
          b.classList.add("border-gray-300", "text-gray-700");
          if (b.getAttribute("data-solve") === "Q") {
            b.classList.add(
              "border-blue-600",
              "bg-blue-50",
              "text-blue-700",
              "font-medium"
            );
            b.classList.remove("border-gray-300", "text-gray-700");
          }
        });
        diameterEl.value = 1.0;
        depthEl.value = 0.5;
        depthRatioEl.value = 0.5;
        rectBEl.value = 3.0;
        rectYEl.value = 1.0;
        nSelectEl.value = "0.012";
        nValueEl.value = "0.012";
        slopeEl.value = 0.001;
        dischargeEl.value = 10;
        velocityEl.value = 0;
        errorMsgEl.textContent = "";
        iterInfo.textContent = "";
        resultsSection.classList.add("hidden");
      });

      // Initial defaults
      setUnits("us");
    </script>

