    <script>
      // Skin effect calculator logic
      (function () {
        const MU0 = 4 * Math.PI * 1e-7; // H/m

        const materialSelect = document.getElementById("material");
        const customBlock = document.getElementById("customMaterialBlock");
        const calcBtn = document.getElementById("calcBtn");
        const resetBtn = document.getElementById("resetBtn");

        const freqInput = document.getElementById("freq");
        const freqUnit = document.getElementById("freqUnit");
        const tempInput = document.getElementById("temp");
        const radiusInput = document.getElementById("radius");
        const radiusUnit = document.getElementById("radiusUnit");

        const rhoInput = document.getElementById("rho");
        const rhoUnit = document.getElementById("rhoUnit");
        const muRelInput = document.getElementById("muRel");

        const deltaDisplay = document.getElementById("deltaDisplay");
        const deltaAltDisplay = document.getElementById("deltaAltDisplay");
        const radiusComparison = document.getElementById("radiusComparison");
        const radiusMessage = document.getElementById("radiusMessage");
        const racFactor = document.getElementById("racFactor");

        materialSelect.addEventListener("change", () => {
          if (materialSelect.value === "custom") {
            customBlock.classList.remove("hidden");
          } else {
            customBlock.classList.add("hidden");
          }
        });

        function freqToHz(value, unit) {
          const v = parseFloat(value);
          if (!isFinite(v) || v <= 0) return NaN;
          switch (unit) {
            case "Hz":
              return v;
            case "kHz":
              return v * 1e3;
            case "MHz":
              return v * 1e6;
            case "GHz":
              return v * 1e9;
            default:
              return NaN;
          }
        }

        function radiusToMeters(value, unit) {
          const v = parseFloat(value);
          if (!isFinite(v) || v <= 0) return NaN;
          switch (unit) {
            case "mm":
              return v * 1e-3;
            case "cm":
              return v * 1e-2;
            case "m":
              return v;
            case "in":
              return v * 0.0254;
            default:
              return NaN;
          }
        }

        function rhoToOhmMeter(value, unit) {
          const v = parseFloat(value);
          if (!isFinite(v) || v <= 0) return NaN;
          switch (unit) {
            case "ohm_m":
              return v;
            case "ohm_mm2_m":
              return v * 1e-6; // Ω·mm²/m -> Ω·m
            default:
              return NaN;
          }
        }

        function getMaterialProps() {
          const mat = materialSelect.value;
          const T = parseFloat(tempInput.value);
          const Tref = 20;
          let rho, muRel;

          if (mat === "cu") {
            const rhoRef = 1.72e-8;
            const alpha = 0.0039;
            const dT = isFinite(T) ? T - Tref : 0;
            rho = rhoRef * (1 + alpha * dT);
            muRel = 1;
          } else if (mat === "al") {
            const rhoRef = 2.82e-8;
            const alpha = 0.004;
            const dT = isFinite(T) ? T - Tref : 0;
            rho = rhoRef * (1 + alpha * dT);
            muRel = 1;
          } else if (mat === "fe") {
            rho = 1.0e-7;
            muRel = 100; // very approximate
          } else if (mat === "custom") {
            rho = rhoToOhmMeter(rhoInput.value, rhoUnit.value);
            muRel = parseFloat(muRelInput.value);
            if (!isFinite(muRel) || muRel <= 0) muRel = 1;
          } else {
            rho = NaN;
            muRel = 1;
          }

          return { rho, mu: MU0 * muRel };
        }

        function formatLength(m) {
          if (!isFinite(m) || m <= 0) return "—";
          if (m >= 1) return m.toFixed(4) + " m";
          if (m >= 1e-2) return (m * 100).toFixed(3) + " cm";
          if (m >= 1e-3) return (m * 1e3).toFixed(3) + " mm";
          if (m >= 1e-6) return (m * 1e6).toFixed(3) + " µm";
          return (m * 1e9).toFixed(3) + " nm";
        }

        function formatAltLength(m) {
          if (!isFinite(m) || m <= 0) return "";
          const mm = m * 1e3;
          const inch = m / 0.0254;
          return `≈ ${mm.toFixed(3)} mm  |  ≈ ${inch.toFixed(4)} in`;
        }

        function estimateRacFactor(radius, delta) {
          if (
            !isFinite(radius) ||
            !isFinite(delta) ||
            radius <= 0 ||
            delta <= 0
          )
            return "—";
          const k = radius / delta;
          if (k < 0.5) return "≈ 1.0 × Rdc (skin effect negligible)";
          if (k < 1) return "≈ 1.1–1.3 × Rdc (weak skin effect)";
          if (k < 3) return "≈ 1.3–2.0 × Rdc (moderate skin effect)";
          if (k < 5) return "≈ 2–3 × Rdc (strong skin effect)";
          return "&gt; 3 × Rdc (very strong skin effect, current near surface)";
        }

        function calculate() {
          const f = freqToHz(freqInput.value, freqUnit.value);
          const { rho, mu } = getMaterialProps();

          if (
            !isFinite(f) ||
            f <= 0 ||
            !isFinite(rho) ||
            rho <= 0 ||
            !isFinite(mu) ||
            mu <= 0
          ) {
            deltaDisplay.textContent = "Invalid input";
            deltaAltDisplay.textContent = "";
            radiusComparison.classList.add("hidden");
            racFactor.textContent = "—";
            return;
          }

          const delta = Math.sqrt(rho / (Math.PI * f * mu));

          deltaDisplay.textContent = formatLength(delta);
          deltaAltDisplay.textContent = formatAltLength(delta);

          const rMeters = radiusToMeters(radiusInput.value, radiusUnit.value);
          if (isFinite(rMeters) && rMeters > 0) {
            const k = rMeters / delta;
            let msg;
            if (k < 0.5) {
              msg = `Conductor radius is only ${k.toFixed(
                2
              )} skin depths. Skin effect is negligible; AC resistance is close to DC.`;
            } else if (k < 1.5) {
              msg = `Conductor radius is about ${k.toFixed(
                2
              )} skin depths. Skin effect is present but moderate.`;
            } else if (k < 5) {
              msg = `Conductor radius is ${k.toFixed(
                2
              )} skin depths. Skin effect is significant; AC resistance is notably higher than DC.`;
            } else {
              msg = `Conductor radius is ${k.toFixed(
                2
              )} skin depths. Skin effect is very strong; current is confined near the surface.`;
            }
            radiusMessage.textContent = msg;
            radiusComparison.classList.remove("hidden");
            racFactor.innerHTML = estimateRacFactor(rMeters, delta);
          } else {
            radiusComparison.classList.add("hidden");
            racFactor.textContent =
              "Provide a conductor radius to estimate Rac/Rdc.";
          }
        }

        calcBtn.addEventListener("click", calculate);

        resetBtn.addEventListener("click", () => {
          freqInput.value = 50;
          freqUnit.value = "Hz";
          materialSelect.value = "cu";
          tempInput.value = 20;
          radiusInput.value = "";
          radiusUnit.value = "mm";
          rhoInput.value = 1.72e-8;
          rhoUnit.value = "ohm_m";
          muRelInput.value = 1;
          customBlock.classList.add("hidden");
          deltaDisplay.textContent = "—";
          deltaAltDisplay.textContent = "";
          radiusComparison.classList.add("hidden");
          racFactor.textContent = "—";
        });

        // Initial calculation with defaults
        calculate();
      })();
    </script>

