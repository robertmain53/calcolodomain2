    <script>
      // Wheel presets (circumference in mm)
      const wheelPresets = {
        "700x25": 2105,
        "700x28": 2136,
        "700x32": 2155,
        "29x2.2": 2288,
        "27.5x2.4": 2237,
        "26x2.0": 2055,
      };

      const wheelPresetSelect = document.getElementById("wheel-preset");
      const wheelCircInput = document.getElementById("wheel-circ");
      const frontTeethInput = document.getElementById("front-teeth");
      const rearTeethInput = document.getElementById("rear-teeth");
      const gearRatioDisplay = document.getElementById("gear-ratio-display");

      const speedValueInput = document.getElementById("speed-value");
      const speedUnitSelect = document.getElementById("speed-unit");
      const btnCalcCadence = document.getElementById("btn-calc-cadence");
      const cadenceResultSpan = document.getElementById("cadence-result");
      const cadenceZoneLabel = document.getElementById("cadence-zone-label");

      const cadenceInput = document.getElementById("cadence-input");
      const speedUnit2Select = document.getElementById("speed-unit-2");
      const btnCalcSpeed = document.getElementById("btn-calc-speed");
      const speedResultSpan = document.getElementById("speed-result");
      const speedResultUnitSpan = document.getElementById("speed-result-unit");

      const tableCadenceMin = document.getElementById("table-cadence-min");
      const tableCadenceMax = document.getElementById("table-cadence-max");
      const tableCadenceStep = document.getElementById("table-cadence-step");
      const speedUnit3Select = document.getElementById("speed-unit-3");
      const btnGenerateTable = document.getElementById("btn-generate-table");
      const gearCadenceTableBody = document.getElementById(
        "gear-cadence-table-body"
      );

      // Tabs
      const tabCadenceFromSpeed = document.getElementById(
        "tab-cadence-from-speed"
      );
      const tabSpeedFromCadence = document.getElementById(
        "tab-speed-from-cadence"
      );
      const tabGearTable = document.getElementById("tab-gear-table");
      const panelCadenceFromSpeed = document.getElementById(
        "panel-cadence-from-speed"
      );
      const panelSpeedFromCadence = document.getElementById(
        "panel-speed-from-cadence"
      );
      const panelGearTable = document.getElementById("panel-gear-table");

      function updateGearRatioDisplay() {
        const ft = parseFloat(frontTeethInput.value);
        const rt = parseFloat(rearTeethInput.value);
        if (!ft || !rt || ft <= 0 || rt <= 0) {
          gearRatioDisplay.textContent = "—";
          return;
        }
        const ratio = ft / rt;
        gearRatioDisplay.textContent = ratio.toFixed(2) + " : 1";
      }

      function getWheelCircumferenceMeters() {
        const mm = parseFloat(wheelCircInput.value);
        if (!mm || mm <= 0) return null;
        return mm / 1000; // mm to meters
      }

      function getGearRatio() {
        const ft = parseFloat(frontTeethInput.value);
        const rt = parseFloat(rearTeethInput.value);
        if (!ft || !rt || ft <= 0 || rt <= 0) return null;
        return ft / rt;
      }

      function speedToMs(speed, unit) {
        if (unit === "kmh") {
          return (speed * 1000) / 3600;
        } else {
          // mph
          return (speed * 1609.344) / 3600;
        }
      }

      function msToSpeed(v, unit) {
        if (unit === "kmh") {
          return v * 3.6;
        } else {
          return v * 2.2369362921;
        }
      }

      function classifyCadence(cadence) {
        if (!isFinite(cadence) || cadence <= 0) return "";
        if (cadence < 60)
          return "Very low cadence – high torque on knees and muscles.";
        if (cadence < 75)
          return "Low cadence – fine for steep climbs, but can feel “grindy” on flats.";
        if (cadence < 95)
          return "Typical endurance cadence for many road cyclists.";
        if (cadence < 110)
          return "High cadence – good for drills and short efforts.";
        return "Very high cadence – usually only sustainable for short bursts.";
      }

      // Handle wheel preset changes
      wheelPresetSelect.addEventListener("change", () => {
        const val = wheelPresetSelect.value;
        if (val === "custom") {
          wheelCircInput.removeAttribute("readonly");
          wheelCircInput.focus();
        } else {
          wheelCircInput.value = wheelPresets[val] || 2105;
          wheelCircInput.setAttribute("readonly", "readonly");
        }
      });

      // Update gear ratio when teeth change
      frontTeethInput.addEventListener("input", updateGearRatioDisplay);
      rearTeethInput.addEventListener("input", updateGearRatioDisplay);
      updateGearRatioDisplay();

      // Calculate cadence from speed
      btnCalcCadence.addEventListener("click", () => {
        const speed = parseFloat(speedValueInput.value);
        const unit = speedUnitSelect.value;
        const C = getWheelCircumferenceMeters();
        const G = getGearRatio();

        if (!speed || speed <= 0 || !C || !G) {
          cadenceResultSpan.textContent = "—";
          cadenceZoneLabel.textContent =
            "Please enter a valid speed, wheel circumference, and gear.";
          return;
        }

        const v = speedToMs(speed, unit);
        const wheelRpm = (v / C) * 60;
        const cadence = wheelRpm / G;

        cadenceResultSpan.textContent = cadence.toFixed(1);
        cadenceZoneLabel.textContent = classifyCadence(cadence);
      });

      // Calculate speed from cadence
      btnCalcSpeed.addEventListener("click", () => {
        const cadence = parseFloat(cadenceInput.value);
        const unit = speedUnit2Select.value;
        const C = getWheelCircumferenceMeters();
        const G = getGearRatio();

        if (!cadence || cadence <= 0 || !C || !G) {
          speedResultSpan.textContent = "—";
          speedResultUnitSpan.textContent = "";
          return;
        }

        const wheelRpm = cadence * G;
        const v = (wheelRpm * C) / 60; // m/s
        const speed = msToSpeed(v, unit);

        speedResultSpan.textContent = speed.toFixed(1);
        speedResultUnitSpan.textContent = unit === "kmh" ? "km/h" : "mph";
      });

      // Generate gear/cadence table
      btnGenerateTable.addEventListener("click", () => {
        const min = parseFloat(tableCadenceMin.value);
        const max = parseFloat(tableCadenceMax.value);
        const step = parseFloat(tableCadenceStep.value);
        const unit = speedUnit3Select.value;
        const C = getWheelCircumferenceMeters();
        const G = getGearRatio();

        gearCadenceTableBody.innerHTML = "";

        if (
          !min ||
          !max ||
          !step ||
          min <= 0 ||
          max <= min ||
          step <= 0 ||
          !C ||
          !G
        ) {
          const tr = document.createElement("tr");
          tr.innerHTML =
            '<td class="px-3 py-2 text-red-600 text-sm" colspan="2">Check cadence range, step, wheel circumference, and gear values.</td>';
          gearCadenceTableBody.appendChild(tr);
          return;
        }

        for (let rpm = min; rpm <= max + 0.0001; rpm += step) {
          const wheelRpm = rpm * G;
          const v = (wheelRpm * C) / 60;
          const speed = msToSpeed(v, unit);

          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="px-3 py-2 border-b text-gray-800">${rpm.toFixed(0)}</td>
        <td class="px-3 py-2 border-b text-gray-800">${speed.toFixed(1)} ${
            unit === "kmh" ? "km/h" : "mph"
          }</td>
      `;
          gearCadenceTableBody.appendChild(tr);
        }
      });

      // Tab switching
      function activateTab(tab) {
        [tabCadenceFromSpeed, tabSpeedFromCadence, tabGearTable].forEach(
          (t) => {
            t.classList.remove("tab-active");
          }
        );
        [panelCadenceFromSpeed, panelSpeedFromCadence, panelGearTable].forEach(
          (p) => {
            p.classList.add("hidden");
          }
        );

        if (tab === "cadence-from-speed") {
          tabCadenceFromSpeed.classList.add("tab-active");
          panelCadenceFromSpeed.classList.remove("hidden");
        } else if (tab === "speed-from-cadence") {
          tabSpeedFromCadence.classList.add("tab-active");
          panelSpeedFromCadence.classList.remove("hidden");
        } else if (tab === "gear-table") {
          tabGearTable.classList.add("tab-active");
          panelGearTable.classList.remove("hidden");
        }
      }

      tabCadenceFromSpeed.addEventListener("click", () =>
        activateTab("cadence-from-speed")
      );
      tabSpeedFromCadence.addEventListener("click", () =>
        activateTab("speed-from-cadence")
      );
      tabGearTable.addEventListener("click", () => activateTab("gear-table"));

      // Initialize default wheel preset as read-only
      wheelCircInput.setAttribute("readonly", "readonly");
    </script>

