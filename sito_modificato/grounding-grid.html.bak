    <script>
      // Grounding grid calculator logic (simplified IEEE 80-style)
      function parseVal(id) {
        const el = document.getElementById(id);
        const v = parseFloat(el.value);
        return isNaN(v) ? 0 : v;
      }

      function formatNumber(val, decimals) {
        if (!isFinite(val)) return "–";
        return val.toFixed(decimals);
      }

      function calculateGroundingGrid() {
        const Lx = parseVal("gridLength");
        const Ly = parseVal("gridWidth");
        const Sx = parseVal("spacingX");
        const Sy = parseVal("spacingY");
        const h = parseVal("gridDepth");
        const d_mm = parseVal("conductorDiameter");
        const rho = parseVal("soilResistivity");
        const rho_s = parseVal("surfaceResistivity");
        const h_s = parseVal("surfaceThickness");
        const bodyWeight = parseVal("bodyWeight");
        const Ig_kA = parseVal("faultCurrent");
        const ts = parseVal("faultDuration");
        const nRods = parseVal("numRods");
        const Lr = parseVal("rodLength");

        const statusEl = document.getElementById("calcStatus");
        statusEl.textContent = "";

        if (
          Lx <= 0 ||
          Ly <= 0 ||
          Sx <= 0 ||
          Sy <= 0 ||
          rho <= 0 ||
          Ig_kA <= 0 ||
          ts <= 0
        ) {
          statusEl.textContent =
            "Please enter positive values for grid size, spacing, soil resistivity, fault current, and duration.";
          return;
        }

        // Geometry
        const area = Lx * Ly;

        const Nx = Math.floor(Lx / Sx) + 1;
        const Ny = Math.floor(Ly / Sy) + 1;

        const lenX = Nx * Ly;
        const lenY = Ny * Lx;
        const LT_grid = lenX + lenY;

        const LT_rods = nRods > 0 && Lr > 0 ? nRods * Lr : 0;
        const LT_total = LT_grid + LT_rods;

        // Grid resistance (very simplified)
        const Rg = rho / (4 * LT_total); // ohms

        const Ig = Ig_kA * 1000; // A
        const GPR = Ig * Rg; // volts

        // Surface layer factor Cs (simplified)
        let Cs = 1.0;
        if (rho_s > 0 && h_s > 0) {
          Cs = rho_s / rho / (1 + 1.5 * h_s);
          if (Cs < 0.1) Cs = 0.1;
          if (Cs > 5) Cs = 5;
        }

        // Tolerable touch voltage (simplified IEEE 80 style)
        // Use different constants for 50 kg vs 70 kg
        const kBody = bodyWeight <= 50 ? 0.116 : 0.157; // approximate
        const Etouch_tol = ((kBody * Cs * rho) / Math.sqrt(ts)) * 1000; // V
        const Estep_tol = 4 * Etouch_tol;

        // Mesh and step voltage factors (very approximate)
        const Deq = Math.sqrt((Lx * Ly) / (Nx * Ny)); // equivalent spacing
        let km = 0.8 * Math.exp(-0.15 * (h / Deq)) + 0.2;
        let ks = 0.6 * Math.exp(-0.1 * (h / Deq)) + 0.3;
        if (!isFinite(km) || km <= 0) km = 0.7;
        if (!isFinite(ks) || ks <= 0) ks = 0.7;

        const Emesh = km * GPR;
        const Estep = ks * GPR;

        // Update UI
        document.getElementById("resArea").textContent = formatNumber(area, 1);
        document.getElementById("resNx").textContent = Nx;
        document.getElementById("resNy").textContent = Ny;
        document.getElementById("resLen").textContent = formatNumber(
          LT_grid,
          1
        );
        document.getElementById("resRodLen").textContent = formatNumber(
          LT_rods,
          1
        );
        document.getElementById("resRg").textContent = formatNumber(Rg, 4);
        document.getElementById("resGpr").textContent = formatNumber(
          GPR / 1000,
          2
        );

        document.getElementById("resEtouchTol").textContent = formatNumber(
          Etouch_tol,
          0
        );
        document.getElementById("resEstepTol").textContent = formatNumber(
          Estep_tol,
          0
        );
        document.getElementById("resEmesh").textContent = formatNumber(
          Emesh,
          0
        );
        document.getElementById("resEstep").textContent = formatNumber(
          Estep,
          0
        );

        // Safety summary
        const safetyBox = document.getElementById("safetySummary");
        const safetyMsg = document.getElementById("safetyMessage");
        safetyBox.classList.remove("hidden");

        const touchOK = Emesh <= Etouch_tol;
        const stepOK = Estep <= Estep_tol;

        if (touchOK && stepOK) {
          safetyBox.classList.remove("bg-red-50", "text-red-900");
          safetyBox.classList.add("bg-green-50", "text-green-900");
          safetyMsg.textContent =
            "Based on these simplified assumptions, both mesh (touch) and step voltages are within tolerable limits. Always verify with full IEEE Std 80 calculations before final design.";
        } else {
          safetyBox.classList.remove("bg-green-50", "text-green-900");
          safetyBox.classList.add("bg-red-50", "text-red-900");
          let reasons = [];
          if (!touchOK)
            reasons.push("mesh (touch) voltage exceeds the tolerable limit");
          if (!stepOK) reasons.push("step voltage exceeds the tolerable limit");
          safetyMsg.textContent =
            "Warning: " +
            reasons.join(" and ") +
            ". Consider reducing conductor spacing, adding more/longer ground rods, or improving the surface layer, and then re-calculate. Final designs must be checked against full IEEE Std 80 requirements.";
        }

        statusEl.textContent = "Calculation updated.";
      }

      document
        .getElementById("calculateBtn")
        .addEventListener("click", calculateGroundingGrid);

      document
        .getElementById("resetBtn")
        .addEventListener("click", function () {
          document.getElementById("gridLength").value = 60;
          document.getElementById("gridWidth").value = 40;
          document.getElementById("spacingX").value = 5;
          document.getElementById("spacingY").value = 5;
          document.getElementById("gridDepth").value = 0.6;
          document.getElementById("conductorDiameter").value = 16;
          document.getElementById("soilResistivity").value = 100;
          document.getElementById("surfaceResistivity").value = 3000;
          document.getElementById("surfaceThickness").value = 0.1;
          document.getElementById("bodyWeight").value = 70;
          document.getElementById("faultCurrent").value = 20;
          document.getElementById("faultDuration").value = 0.5;
          document.getElementById("numRods").value = 20;
          document.getElementById("rodLength").value = 3;
          document.getElementById("calcStatus").textContent = "";
          document.getElementById("safetySummary").classList.add("hidden");
          const ids = [
            "resArea",
            "resNx",
            "resNy",
            "resLen",
            "resRodLen",
            "resRg",
            "resGpr",
            "resEtouchTol",
            "resEstepTol",
            "resEmesh",
            "resEstep",
          ];
          ids.forEach((id) => (document.getElementById(id).textContent = "–"));
        });

      // Auto-calc on load for default example
      window.addEventListener("DOMContentLoaded", calculateGroundingGrid);
    </script>

