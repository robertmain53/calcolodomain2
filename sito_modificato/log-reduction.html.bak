<script>
        function calculateLogReduction() {
            const initialCount = parseFloat(document.getElementById('initialCount').value);
            const finalCount = parseFloat(document.getElementById('finalCount').value);
            const errorMessage = document.getElementById('error-message');
            const logReductionValue = document.getElementById('logReductionValue');
            
            errorMessage.textContent = '';
            
            if (isNaN(initialCount) || initialCount <= 0) {
                errorMessage.textContent = 'Please enter a valid initial microbial count greater than zero.';
                return;
            }
            
            if (isNaN(finalCount) || finalCount <= 0) {
                errorMessage.textContent = 'Please enter a valid final microbial count greater than zero.';
                return;
            }
            
            if (finalCount > initialCount) {
                errorMessage.textContent = 'The final microbial count must be less than or equal to the initial count.';
                return;
            }
            
            const logReduction = Math.log10(initialCount / finalCount);
            logReductionValue.textContent = logReduction.toFixed(2);
        }
    </script>


