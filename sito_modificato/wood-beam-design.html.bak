    <script>
      // Preset sections (approximate values, for educational use)
      const presets = {
        custom: null,
        spf2x10: {
          b: 1.5,
          d: 9.25,
          Fb: 875,
          Fv: 135,
          E: 1400000,
        },
        df2x12: {
          b: 1.5,
          d: 11.25,
          Fb: 1000,
          Fv: 150,
          E: 1600000,
        },
        sp2x8: {
          b: 1.5,
          d: 7.25,
          Fb: 1100,
          Fv: 175,
          E: 1600000,
        },
        glulam24F: {
          b: 3.5,
          d: 11.25,
          Fb: 2400,
          Fv: 265,
          E: 1800000,
        },
      };

      function autoSectionProps(b, d) {
        const S = (b * d * d) / 6; // in^3
        const I = (b * Math.pow(d, 3)) / 12; // in^4
        return { S, I };
      }

      function updatePreset() {
        const sel = document.getElementById("presetSection").value;
        const data = presets[sel];
        if (!data) return;
        document.getElementById("bIn").value = data.b;
        document.getElementById("dIn").value = data.d;
        document.getElementById("Fb").value = data.Fb;
        document.getElementById("Fv").value = data.Fv;
        document.getElementById("E").value = data.E;
        // Auto-calc S and I
        const props = autoSectionProps(data.b, data.d);
        document.getElementById("SIn3").value = props.S.toFixed(1);
        document.getElementById("IIn4").value = props.I.toFixed(1);
      }

      document
        .getElementById("presetSection")
        .addEventListener("change", updatePreset);

      // Toggle load inputs
      document
        .getElementById("loadType")
        .addEventListener("change", function () {
          const type = this.value;
          document
            .getElementById("uniformLoads")
            .classList.toggle("hidden", type !== "uniform");
          document
            .getElementById("pointLoad")
            .classList.toggle("hidden", type !== "pointMid");
        });

      function setStatus(elId, ratio, pass) {
        const el = document.getElementById(elId);
        if (!ratio && ratio !== 0) {
          el.textContent = "–";
          return;
        }
        const pct = (ratio * 100).toFixed(0);
        el.textContent = `${pct}% of capacity (${pass ? "OK" : "NG"})`;
      }

      function formatInches(x) {
        return x.toFixed(3) + " in";
      }

      document.getElementById("calcBtn").addEventListener("click", function () {
        // Inputs
        const b = parseFloat(document.getElementById("bIn").value);
        const d = parseFloat(document.getElementById("dIn").value);
        let S = parseFloat(document.getElementById("SIn3").value);
        let I = parseFloat(document.getElementById("IIn4").value);
        const Fb = parseFloat(document.getElementById("Fb").value);
        const Fv = parseFloat(document.getElementById("Fv").value);
        const E = parseFloat(document.getElementById("E").value);
        const Cd = parseFloat(document.getElementById("Cd").value) || 1.0;

        const spanFt = parseFloat(document.getElementById("spanFt").value);
        const spacingFt = parseFloat(
          document.getElementById("spacingFt").value
        );
        const loadType = document.getElementById("loadType").value;
        const wDpsf = parseFloat(document.getElementById("wDpsf").value) || 0;
        const wLpsf = parseFloat(document.getElementById("wLpsf").value) || 0;
        const Pk = parseFloat(document.getElementById("Pk").value) || 0;

        const deflLimitLive = parseFloat(
          document.getElementById("deflLimitLive").value
        );
        const deflLimitTotal = parseFloat(
          document.getElementById("deflLimitTotal").value
        );

        if (!b || !d || !spanFt || !E || !Fb || !Fv) {
          alert("Please enter valid section, span, and material values.");
          return;
        }

        // Auto S and I if missing
        if (!S || !I) {
          const props = autoSectionProps(b, d);
          if (!S) S = props.S;
          if (!I) I = props.I;
          document.getElementById("SIn3").value = S.toFixed(1);
          document.getElementById("IIn4").value = I.toFixed(1);
        }

        const L_in = spanFt * 12; // span in inches

        let Mmax_lb_in = 0;
        let Vmax_lb = 0;
        let D_live_in = 0;
        let D_total_in = 0;

        if (loadType === "uniform") {
          const wD_plf = wDpsf * spacingFt;
          const wL_plf = wLpsf * spacingFt;
          const wD_lbin = wD_plf / 12;
          const wL_lbin = wL_plf / 12;
          const wTot_lbin = wD_lbin + wL_lbin;

          // Internal forces
          Mmax_lb_in = (wTot_lbin * L_in * L_in) / 8;
          Vmax_lb = (wTot_lbin * L_in) / 2;

          // Deflections
          D_live_in = (5 * wL_lbin * Math.pow(L_in, 4)) / (384 * E * I);
          D_total_in = (5 * wTot_lbin * Math.pow(L_in, 4)) / (384 * E * I);
        } else {
          const P_lb = Pk * 1000;
          Mmax_lb_in = (P_lb * L_in) / 4;
          Vmax_lb = P_lb / 2;

          // For point load, treat P as live load for deflection
          D_live_in = (P_lb * Math.pow(L_in, 3)) / (48 * E * I);
          D_total_in = D_live_in; // assuming no separate dead load in this mode
        }

        // Stresses
        const fb = Mmax_lb_in / S; // psi
        const fv = (1.5 * Vmax_lb) / (b * d); // psi

        const Fb_adj = Fb * Cd;
        const Fv_adj = Fv * Cd;

        const ratio_bend = fb / Fb_adj;
        const ratio_shear = fv / Fv_adj;

        // Deflection limits
        const D_live_allow = L_in / deflLimitLive;
        const D_total_allow = L_in / deflLimitTotal;

        const ratio_defl_live = D_live_in / D_live_allow;
        const ratio_defl_total = D_total_in / D_total_allow;

        const pass_bend = ratio_bend <= 1.0;
        const pass_shear = ratio_shear <= 1.0;
        const pass_defl_live = ratio_defl_live <= 1.0;
        const pass_defl_total = ratio_defl_total <= 1.0;

        const overallPass =
          pass_bend && pass_shear && pass_defl_live && pass_defl_total;

        // Update section outputs
        document.getElementById("out_b").textContent = b.toFixed(2);
        document.getElementById("out_d").textContent = d.toFixed(2);
        document.getElementById("out_S").textContent = S.toFixed(1);
        document.getElementById("out_I").textContent = I.toFixed(1);

        // Update demand/capacity
        setStatus("out_bend", ratio_bend, pass_bend);
        setStatus("out_shear", ratio_shear, pass_shear);
        setStatus("out_defl_live", ratio_defl_live, pass_defl_live);
        setStatus("out_defl_total", ratio_defl_total, pass_defl_total);

        // Internal forces
        const Mmax_ft_k = Mmax_lb_in / 12 / 1000; // kip-ft
        const Vmax_k = Vmax_lb / 1000;

        document.getElementById("out_Mmax").textContent =
          Mmax_ft_k.toFixed(2) + " kip-ft";
        document.getElementById("out_Vmax").textContent =
          Vmax_k.toFixed(2) + " kips";
        document.getElementById("out_Dmax").textContent =
          formatInches(D_total_in);

        // Summary
        const statusEl = document.getElementById("overallStatus");
        statusEl.textContent = overallPass ? "PASS" : "FAIL";
        statusEl.className =
          "inline-flex items-center px-2 py-0.5 rounded text-xs font-semibold " +
          (overallPass ? "result-badge-ok" : "result-badge-ng");

        const summary = [];
        summary.push(
          `Max moment ${Mmax_ft_k.toFixed(
            2
          )} kip-ft, max shear ${Vmax_k.toFixed(2)} kips.`
        );
        summary.push(
          `Bending demand is ${(ratio_bend * 100).toFixed(
            0
          )}% of allowable; shear is ${(ratio_shear * 100).toFixed(0)}%.`
        );
        summary.push(
          `Total deflection is ${formatInches(
            D_total_in
          )} (limit ${formatInches(D_total_allow)}).`
        );

        document.getElementById("summaryText").textContent = summary.join(" ");
      });

      // Initialize with preset for convenience
      updatePreset();
    </script>

