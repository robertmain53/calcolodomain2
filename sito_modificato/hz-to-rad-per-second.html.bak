    <script>
      // Core conversion logic
      (function () {
        const PI2 = 2 * Math.PI;

        const hzInput = document.getElementById("hzInput");
        const radInput = document.getElementById("radInput");
        const resultBox = document.getElementById("resultBox");
        const resultText = document.getElementById("resultText");
        const resultFormula = document.getElementById("resultFormula");
        const directionLabel = document.getElementById("directionLabel");
        const swapBtn = document.getElementById("swapDirectionBtn");
        const clearBtn = document.getElementById("clearBtn");

        // direction: true = Hz -> rad/s, false = rad/s -> Hz
        let hzToRad = true;

        function formatNumber(value) {
          if (!isFinite(value)) return "";
          const abs = Math.abs(value);
          if (abs === 0) return "0";
          if (abs >= 0.001 && abs < 1e6) {
            return value.toLocaleString(undefined, {
              maximumFractionDigits: 6,
            });
          }
          return value.toExponential(6);
        }

        function updateDirectionLabel() {
          directionLabel.textContent = hzToRad
            ? "Current: Hz → rad/s"
            : "Current: rad/s → Hz";
        }

        function showResult(text, formula) {
          if (!text) {
            resultBox.classList.add("hidden");
            resultText.textContent = "";
            resultFormula.textContent = "";
            return;
          }
          resultText.innerHTML = text;
          resultFormula.innerHTML = formula || "";
          resultBox.classList.remove("hidden");
        }

        function convertFromHz() {
          const f = parseFloat(hzInput.value);
          if (isNaN(f)) {
            showResult("", "");
            radInput.value = "";
            return;
          }
          const omega = f * PI2;
          radInput.value = formatNumber(omega);
          const exact = `${formatNumber(f)} Hz × 2π = ${formatNumber(
            omega
          )} rad/s`;
          const formula = "ω = 2πf";
          showResult(
            `<strong>${formatNumber(f)} Hz</strong> = <strong>${formatNumber(
              omega
            )} rad/s</strong>`,
            `${formula}<br>${exact}`
          );
        }

        function convertFromRad() {
          const omega = parseFloat(radInput.value);
          if (isNaN(omega)) {
            showResult("", "");
            hzInput.value = "";
            return;
          }
          const f = omega / PI2;
          hzInput.value = formatNumber(f);
          const exact = `${formatNumber(omega)} rad/s ÷ 2π = ${formatNumber(
            f
          )} Hz`;
          const formula = "f = ω / (2π)";
          showResult(
            `<strong>${formatNumber(
              omega
            )} rad/s</strong> = <strong>${formatNumber(f)} Hz</strong>`,
            `${formula}<br>${exact}`
          );
        }

        function handleHzInput() {
          hzToRad = true;
          updateDirectionLabel();
          convertFromHz();
        }

        function handleRadInput() {
          hzToRad = false;
          updateDirectionLabel();
          convertFromRad();
        }

        hzInput.addEventListener("input", handleHzInput);
        radInput.addEventListener("input", handleRadInput);

        swapBtn.addEventListener("click", () => {
          hzToRad = !hzToRad;
          updateDirectionLabel();
          // Swap values logically: if we have one, recompute the other
          if (hzToRad) {
            // Now Hz -> rad/s, recompute from Hz if available
            if (hzInput.value !== "") {
              convertFromHz();
            } else if (radInput.value !== "") {
              // back-compute Hz then forward to keep consistency
              convertFromRad();
            }
          } else {
            // Now rad/s -> Hz
            if (radInput.value !== "") {
              convertFromRad();
            } else if (hzInput.value !== "") {
              convertFromHz();
            }
          }
        });

        clearBtn.addEventListener("click", () => {
          hzInput.value = "";
          radInput.value = "";
          showResult("", "");
        });

        // Initialize
        updateDirectionLabel();
      })();

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
