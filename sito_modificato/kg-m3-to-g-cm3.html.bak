    <script>
      // Density converter logic
      (function () {
        const kgm3Input = document.getElementById("kgm3Input");
        const gcm3Input = document.getElementById("gcm3Input");
        const gcm3FromKgm3 = document.getElementById("gcm3FromKgm3");
        const kgm3FromGcm3 = document.getElementById("kgm3FromGcm3");
        const swapBtn = document.getElementById("swapBtn");
        const resetBtn = document.getElementById("resetBtn");

        const formatNumber = (value) => {
          if (value === null || isNaN(value)) return "–";
          if (!isFinite(value)) return "∞";
          const abs = Math.abs(value);
          if (abs !== 0 && (abs < 0.001 || abs >= 1e6)) {
            return value.toExponential(6).replace(/\.?0+e/, "e");
          }
          return value.toLocaleString(undefined, { maximumFractionDigits: 8 });
        };

        function updateFromKgm3() {
          const v = parseFloat(kgm3Input.value);
          if (isNaN(v)) {
            gcm3FromKgm3.textContent = "–";
            return;
          }
          const result = v / 1000;
          gcm3FromKgm3.textContent = formatNumber(result);
        }

        function updateFromGcm3() {
          const v = parseFloat(gcm3Input.value);
          if (isNaN(v)) {
            kgm3FromGcm3.textContent = "–";
            return;
          }
          const result = v * 1000;
          kgm3FromGcm3.textContent = formatNumber(result);
        }

        kgm3Input.addEventListener("input", updateFromKgm3);
        gcm3Input.addEventListener("input", updateFromGcm3);

        swapBtn.addEventListener("click", () => {
          const kgVal = kgm3Input.value;
          const gVal = gcm3Input.value;
          kgm3Input.value = gVal;
          gcm3Input.value = kgVal;
          updateFromKgm3();
          updateFromGcm3();
        });

        resetBtn.addEventListener("click", () => {
          kgm3Input.value = "";
          gcm3Input.value = "";
          gcm3FromKgm3.textContent = "–";
          kgm3FromGcm3.textContent = "–";
        });
      })();

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
