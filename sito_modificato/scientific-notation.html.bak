    <script>
      // Tab handling
      const tabs = {
        "tab-to-sci": "panel-to-sci",
        "tab-from-sci": "panel-from-sci",
        "tab-operations": "panel-operations",
      };
      Object.keys(tabs).forEach((tabId) => {
        document.getElementById(tabId).addEventListener("click", () => {
          // toggle tab styles
          Object.keys(tabs).forEach((id) => {
            const btn = document.getElementById(id);
            if (id === tabId) {
              btn.classList.add("tab-active");
              btn.classList.remove("text-gray-500");
            } else {
              btn.classList.remove("tab-active");
              btn.classList.add("text-gray-500");
            }
          });
          // toggle panels
          Object.entries(tabs).forEach(([id, panelId]) => {
            document
              .getElementById(panelId)
              .classList.toggle("hidden", id !== tabId);
          });
        });
      });

      // Utility: parse generic number or e-notation
      function parseNumber(str) {
        if (!str || typeof str !== "string") return NaN;
        str = str.replace(/\s+/g, "");
        // Replace ×10^ style if user pasted it
        str = str.replace(/×/i, "e").replace(/10\^(-?\d+)/i, "e$1");
        return Number(str);
      }

      function formatSci(num, sigfigs) {
        if (!isFinite(num)) return String(num);
        if (num === 0)
          return { mantissa: "0", exponent: 0, display: "0 × 10^0" };

        const abs = Math.abs(num);
        let exp = Math.floor(Math.log10(abs));
        let mant = num / Math.pow(10, exp);

        if (sigfigs && sigfigs > 0 && sigfigs <= 15) {
          const factor = Math.pow(10, sigfigs - 1);
          mant = Math.round(mant * factor) / factor;
          // renormalize if rounding pushed mantissa to 10
          if (Math.abs(mant) >= 10) {
            mant = mant / 10;
            exp += 1;
          }
        }

        // Trim trailing zeros
        let mantStr = mant.toString();
        if (mantStr.indexOf("e") !== -1) {
          // handle JS's own scientific output
          const n = Number(mantStr);
          mantStr = n.toString();
        }
        if (mantStr.indexOf(".") !== -1) {
          mantStr = mantStr.replace(/0+$/, "").replace(/\.$/, "");
        }

        const display = mantStr + " × 10^" + exp;
        return { mantissa: mantStr, exponent: exp, display };
      }

      // Number -> Scientific
      document.getElementById("btn-to-sci").addEventListener("click", () => {
        const input = document.getElementById("input-standard").value;
        const sigStr = document.getElementById("sigfigs").value;
        const errorEl = document.getElementById("to-sci-error");
        const outWrap = document.getElementById("to-sci-output");
        const mainEl = document.getElementById("to-sci-main");
        const altEl = document.getElementById("to-sci-alt");
        const stepsEl = document.getElementById("to-sci-steps");

        errorEl.classList.add("hidden");
        outWrap.classList.add("hidden");
        stepsEl.innerHTML = "";

        const num = parseNumber(input);
        if (isNaN(num)) {
          errorEl.textContent =
            "Please enter a valid number (e.g. 123000, 0.00045, 1.23e5).";
          errorEl.classList.remove("hidden");
          return;
        }

        let sig = parseInt(sigStr, 10);
        if (isNaN(sig) || sig <= 0 || sig > 15) sig = null;

        const sci = formatSci(num, sig);
        mainEl.textContent = sci.display;
        altEl.textContent =
          "(mantissa a = " +
          sci.mantissa +
          ", exponent n = " +
          sci.exponent +
          ")";

        const abs = Math.abs(num);
        const direction = abs >= 1 ? "left" : "right";
        const places = abs === 0 ? 0 : Math.abs(sci.exponent);

        const steps = [];
        steps.push("1. Start with the number: " + num);
        if (num === 0) {
          steps.push(
            "2. Zero is written as 0 × 10^0 in scientific notation by convention."
          );
        } else {
          steps.push(
            "2. Move the decimal point " +
              places +
              " place" +
              (places === 1 ? "" : "s") +
              " to the " +
              direction +
              " so that only one non-zero digit is to its left."
          );
          steps.push("3. The resulting mantissa is a = " + sci.mantissa + ".");
          steps.push(
            "4. Because we moved the decimal " +
              places +
              " place" +
              (places === 1 ? "" : "s") +
              " to the " +
              direction +
              ", the exponent is n = " +
              sci.exponent +
              "."
          );
          steps.push("5. Final result: " + sci.display + ".");
        }
        stepsEl.innerHTML = steps.map((s) => "<p>" + s + "</p>").join("");
        outWrap.classList.remove("hidden");
      });

      document
        .getElementById("btn-clear-to-sci")
        .addEventListener("click", () => {
          document.getElementById("input-standard").value = "";
          document.getElementById("sigfigs").value = "";
          document.getElementById("to-sci-output").classList.add("hidden");
          document.getElementById("to-sci-error").classList.add("hidden");
        });

      // Scientific -> Number
      document.getElementById("btn-from-sci").addEventListener("click", () => {
        const mStr = document.getElementById("mantissa").value;
        const eStr = document.getElementById("exponent").value;
        const errorEl = document.getElementById("from-sci-error");
        const outWrap = document.getElementById("from-sci-output");
        const mainEl = document.getElementById("from-sci-main");
        const stepsEl = document.getElementById("from-sci-steps");

        errorEl.classList.add("hidden");
        outWrap.classList.add("hidden");
        stepsEl.innerHTML = "";

        const m = parseNumber(mStr);
        const e = parseInt(eStr, 10);

        if (isNaN(m) || isNaN(e)) {
          errorEl.textContent =
            "Please enter a valid mantissa and integer exponent.";
          errorEl.classList.remove("hidden");
          return;
        }

        const value = m * Math.pow(10, e);
        mainEl.textContent = value.toString();

        const steps = [];
        steps.push(
          "1. Start with a × 10^n where a = " + m + " and n = " + e + "."
        );
        if (e > 0) {
          steps.push(
            "2. Because n is positive, move the decimal point in a to the right by " +
              e +
              " place" +
              (e === 1 ? "" : "s") +
              "."
          );
        } else if (e < 0) {
          steps.push(
            "2. Because n is negative, move the decimal point in a to the left by " +
              Math.abs(e) +
              " place" +
              (Math.abs(e) === 1 ? "" : "s") +
              "."
          );
        } else {
          steps.push("2. Because n = 0, the value is just a.");
        }
        steps.push("3. The resulting standard form is " + value + ".");

        stepsEl.innerHTML = steps.map((s) => "<p>" + s + "</p>").join("");
        outWrap.classList.remove("hidden");
      });

      document
        .getElementById("btn-clear-from-sci")
        .addEventListener("click", () => {
          document.getElementById("mantissa").value = "";
          document.getElementById("exponent").value = "";
          document.getElementById("from-sci-output").classList.add("hidden");
          document.getElementById("from-sci-error").classList.add("hidden");
        });

      // Operations
      let currentOp = "add";
      const opButtons = document.querySelectorAll(".op-btn");
      opButtons.forEach((btn) => {
        btn.addEventListener("click", () => {
          currentOp = btn.getAttribute("data-op");
          opButtons.forEach((b) => {
            if (b === btn) {
              b.classList.remove("bg-gray-100", "text-gray-700");
              b.classList.add("bg-blue-50", "text-blue-700");
            } else {
              b.classList.add("bg-gray-100", "text-gray-700");
              b.classList.remove("bg-blue-50", "text-blue-700");
            }
          });
        });
      });

      document.getElementById("btn-op-calc").addEventListener("click", () => {
        const aM = parseNumber(document.getElementById("op-a-m").value);
        const aE = parseInt(document.getElementById("op-a-e").value, 10);
        const bM = parseNumber(document.getElementById("op-b-m").value);
        const bE = parseInt(document.getElementById("op-b-e").value, 10);

        const errorEl = document.getElementById("op-error");
        const outWrap = document.getElementById("op-output");
        const sciEl = document.getElementById("op-result-sci");
        const stdEl = document.getElementById("op-result-std");
        const stepsEl = document.getElementById("op-steps");

        errorEl.classList.add("hidden");
        outWrap.classList.add("hidden");
        stepsEl.innerHTML = "";

        if ([aM, aE, bM, bE].some((v) => isNaN(v))) {
          errorEl.textContent =
            "Please enter valid mantissas and integer exponents for both numbers.";
          errorEl.classList.remove("hidden");
          return;
        }

        const A = aM * Math.pow(10, aE);
        const B = bM * Math.pow(10, bE);
        let result, opSymbol, desc;

        switch (currentOp) {
          case "add":
            result = A + B;
            opSymbol = "+";
            desc = "addition";
            break;
          case "sub":
            result = A - B;
            opSymbol = "−";
            desc = "subtraction";
            break;
          case "mul":
            result = A * B;
            opSymbol = "×";
            desc = "multiplication";
            break;
          case "div":
            if (B === 0) {
              errorEl.textContent = "Division by zero is not allowed.";
              errorEl.classList.remove("hidden");
              return;
            }
            result = A / B;
            opSymbol = "÷";
            desc = "division";
            break;
        }

        const sci = formatSci(result, null);
        sciEl.textContent = sci.display;
        stdEl.textContent = "Standard form: " + result;

        const steps = [];
        steps.push("1. First number: " + aM + " × 10^" + aE + ".");
        steps.push("2. Second number: " + bM + " × 10^" + bE + ".");

        if (currentOp === "mul") {
          steps.push(
            "3. Multiply mantissas: " + aM + " × " + bM + " = " + aM * bM + "."
          );
          steps.push(
            "4. Add exponents: " + aE + " + " + bE + " = " + (aE + bE) + "."
          );
          steps.push(
            "5. Intermediate result: " + aM * bM + " × 10^" + (aE + bE) + "."
          );
        } else if (currentOp === "div") {
          steps.push(
            "3. Divide mantissas: " + aM + " ÷ " + bM + " = " + aM / bM + "."
          );
          steps.push(
            "4. Subtract exponents: " +
              aE +
              " − " +
              bE +
              " = " +
              (aE - bE) +
              "."
          );
          steps.push(
            "5. Intermediate result: " + aM / bM + " × 10^" + (aE - bE) + "."
          );
        } else {
          // addition or subtraction: show exponent alignment
          if (aE === bE) {
            steps.push(
              "3. Exponents are equal (n = " +
                aE +
                "), so add/subtract mantissas directly."
            );
            const mantRes = currentOp === "add" ? aM + bM : aM - bM;
            steps.push(
              "4. " + aM + " " + opSymbol + " " + bM + " = " + mantRes + "."
            );
            steps.push(
              "5. Intermediate result: " + mantRes + " × 10^" + aE + "."
            );
          } else {
            const refExp = Math.max(aE, bE);
            const aAdj = aM * Math.pow(10, aE - refExp);
            const bAdj = bM * Math.pow(10, bE - refExp);
            steps.push(
              "3. Align exponents to the larger exponent n = " + refExp + "."
            );
            steps.push(
              "   First number becomes " + aAdj + " × 10^" + refExp + "."
            );
            steps.push(
              "   Second number becomes " + bAdj + " × 10^" + refExp + "."
            );
            const mantRes = currentOp === "add" ? aAdj + bAdj : aAdj - bAdj;
            steps.push(
              "4. " + aAdj + " " + opSymbol + " " + bAdj + " = " + mantRes + "."
            );
            steps.push(
              "5. Intermediate result: " + mantRes + " × 10^" + refExp + "."
            );
          }
        }
        steps.push("6. Normalize the result to get: " + sci.display + ".");

        stepsEl.innerHTML = steps.map((s) => "<p>" + s + "</p>").join("");
        outWrap.classList.remove("hidden");
      });

      document.getElementById("btn-op-clear").addEventListener("click", () => {
        ["op-a-m", "op-a-e", "op-b-m", "op-b-e"].forEach(
          (id) => (document.getElementById(id).value = "")
        );
        document.getElementById("op-output").classList.add("hidden");
        document.getElementById("op-error").classList.add("hidden");
      });

      document.querySelectorAll("#faq .faq-question").forEach((btn) => {
        btn.addEventListener("click", () => {
          const ans = btn.parentElement.querySelector(".faq-answer");
          const sign = btn.querySelector("span:last-child");
          const isHidden = ans.classList.contains("hidden");
          ans.classList.toggle("hidden");
          sign.textContent = isHidden ? "−" : "+";
        });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
