    <script>
      // Utility functions
      function roundTo(value, decimals) {
        if (!isFinite(value)) return value;
        const factor = Math.pow(10, decimals);
        return Math.round(value * factor) / factor;
      }

      function formatNumber(value, decimals = 2) {
        if (value === null || value === undefined || isNaN(value)) return "–";
        return roundTo(value, decimals).toString();
      }

      function formatPercent(value, decimals = 1) {
        if (value === null || value === undefined || isNaN(value)) return "–";
        return roundTo(value * 100, decimals).toString() + "%";
      }

      function interpretLRPlus(lr) {
        if (!isFinite(lr) || lr <= 0) return "";
        if (lr > 10) return "Large increase in probability";
        if (lr > 5) return "Moderate increase";
        if (lr > 2) return "Small increase";
        if (lr > 1) return "Minimal increase";
        return "No useful increase";
      }

      function interpretLRMinus(lr) {
        if (!isFinite(lr) || lr <= 0) return "";
        if (lr < 0.1) return "Large decrease in probability";
        if (lr < 0.2) return "Moderate decrease";
        if (lr < 0.5) return "Small decrease";
        if (lr < 1) return "Minimal decrease";
        return "No useful decrease";
      }

      function updateLRDisplay(lrPos, lrNeg) {
        const lrPosEl = document.getElementById("lr-pos");
        const lrNegEl = document.getElementById("lr-neg");
        const lrPosIntEl = document.getElementById("lr-pos-interpretation");
        const lrNegIntEl = document.getElementById("lr-neg-interpretation");

        if (lrPos !== null && isFinite(lrPos)) {
          lrPosEl.textContent = formatNumber(lrPos, 2);
          const txt = interpretLRPlus(lrPos);
          if (txt) {
            lrPosIntEl.textContent = txt;
            lrPosIntEl.classList.remove("hidden");
          } else {
            lrPosIntEl.classList.add("hidden");
          }
        } else {
          lrPosEl.textContent = "–";
          lrPosIntEl.classList.add("hidden");
        }

        if (lrNeg !== null && isFinite(lrNeg)) {
          lrNegEl.textContent = formatNumber(lrNeg, 2);
          const txt = interpretLRMinus(lrNeg);
          if (txt) {
            lrNegIntEl.textContent = txt;
            lrNegIntEl.classList.remove("hidden");
          } else {
            lrNegIntEl.classList.add("hidden");
          }
        } else {
          lrNegEl.textContent = "–";
          lrNegIntEl.classList.add("hidden");
        }
      }

      function updatePostTestDisplay(preProb, lrPos, lrNeg) {
        const preEl = document.getElementById("pretest-display");
        const postPosEl = document.getElementById("posttest-pos");
        const postNegEl = document.getElementById("posttest-neg");

        if (preProb === null || isNaN(preProb)) {
          preEl.textContent = "–";
          postPosEl.textContent = "–";
          postNegEl.textContent = "–";
          return;
        }

        preEl.textContent = formatPercent(preProb, 1);

        const preOdds = preProb / (1 - preProb);

        if (lrPos !== null && isFinite(lrPos)) {
          const postOddsPos = preOdds * lrPos;
          const postProbPos = postOddsPos / (1 + postOddsPos);
          postPosEl.textContent = formatPercent(postProbPos, 1);
        } else {
          postPosEl.textContent = "–";
        }

        if (lrNeg !== null && isFinite(lrNeg)) {
          const postOddsNeg = preOdds * lrNeg;
          const postProbNeg = postOddsNeg / (1 + postOddsNeg);
          postNegEl.textContent = formatPercent(postProbNeg, 1);
        } else {
          postNegEl.textContent = "–";
        }
      }

      // Mode toggle
      const modeSensBtn = document.getElementById("mode-sens-spec");
      const mode2x2Btn = document.getElementById("mode-2x2");
      const formSens = document.getElementById("form-sens-spec");
      const form2x2 = document.getElementById("form-2x2");

      modeSensBtn.addEventListener("click", () => {
        modeSensBtn.classList.add("bg-blue-500", "text-white");
        modeSensBtn.classList.remove("bg-white", "text-blue-600");
        mode2x2Btn.classList.remove("bg-emerald-500", "text-white");
        mode2x2Btn.classList.add("bg-white", "text-blue-600");
        formSens.classList.remove("hidden");
        form2x2.classList.add("hidden");
      });

      mode2x2Btn.addEventListener("click", () => {
        mode2x2Btn.classList.add("bg-emerald-500", "text-white");
        mode2x2Btn.classList.remove("bg-white", "text-blue-600");
        modeSensBtn.classList.remove("bg-blue-500", "text-white");
        modeSensBtn.classList.add("bg-white", "text-blue-600");
        form2x2.classList.remove("hidden");
        formSens.classList.add("hidden");
      });

      // Sensitivity/Specificity calculation
      document
        .getElementById("calc-sens-spec")
        .addEventListener("click", () => {
          const sensInput = parseFloat(document.getElementById("sens").value);
          const specInput = parseFloat(document.getElementById("spec").value);
          const preInput = document.getElementById("pretest").value;
          const errorEl = document.getElementById("error-sens-spec");
          document.getElementById("derived-metrics").classList.add("hidden");
          errorEl.textContent = "";

          if (isNaN(sensInput) || isNaN(specInput)) {
            errorEl.textContent =
              "Please enter both sensitivity and specificity.";
            updateLRDisplay(null, null);
            updatePostTestDisplay(null, null, null);
            return;
          }
          if (
            sensInput < 0 ||
            sensInput > 100 ||
            specInput < 0 ||
            specInput > 100
          ) {
            errorEl.textContent =
              "Sensitivity and specificity must be between 0 and 100%.";
            updateLRDisplay(null, null);
            updatePostTestDisplay(null, null, null);
            return;
          }

          const sens = sensInput / 100;
          const spec = specInput / 100;

          let lrPos = null;
          let lrNeg = null;

          const oneMinusSpec = 1 - spec;
          const oneMinusSens = 1 - sens;

          if (oneMinusSpec === 0) {
            lrPos = Infinity;
          } else {
            lrPos = sens / oneMinusSpec;
          }

          if (spec === 0) {
            lrNeg = Infinity;
          } else if (oneMinusSens === 0) {
            lrNeg = 0;
          } else {
            lrNeg = oneMinusSens / spec;
          }

          // Cap extreme values for display
          if (lrPos > 1e6) lrPos = 1e6;
          if (lrNeg > 1e6) lrNeg = 1e6;

          updateLRDisplay(lrPos, lrNeg);

          let preProb = null;
          if (preInput !== "") {
            const preVal = parseFloat(preInput);
            if (isNaN(preVal) || preVal < 0 || preVal > 100) {
              errorEl.textContent =
                "Pre‑test probability must be between 0 and 100%.";
              updatePostTestDisplay(null, null, null);
              return;
            }
            preProb = preVal / 100;
          }

          updatePostTestDisplay(preProb, lrPos, lrNeg);
        });

      document
        .getElementById("reset-sens-spec")
        .addEventListener("click", () => {
          document.getElementById("sens").value = "";
          document.getElementById("spec").value = "";
          document.getElementById("pretest").value = "";
          document.getElementById("error-sens-spec").textContent = "";
          updateLRDisplay(null, null);
          updatePostTestDisplay(null, null, null);
          document.getElementById("derived-metrics").classList.add("hidden");
        });

      // 2x2 calculation
      document.getElementById("calc-2x2").addEventListener("click", () => {
        const tp = parseFloat(document.getElementById("tp").value);
        const fp = parseFloat(document.getElementById("fp").value);
        const fn = parseFloat(document.getElementById("fn").value);
        const tn = parseFloat(document.getElementById("tn").value);
        const preInput = document.getElementById("pretest-2x2").value;
        const errorEl = document.getElementById("error-2x2");
        errorEl.textContent = "";

        if ([tp, fp, fn, tn].some((v) => isNaN(v))) {
          errorEl.textContent =
            "Please enter all four cell counts (TP, FP, FN, TN).";
          updateLRDisplay(null, null);
          updatePostTestDisplay(null, null, null);
          document.getElementById("derived-metrics").classList.add("hidden");
          return;
        }
        if ([tp, fp, fn, tn].some((v) => v < 0)) {
          errorEl.textContent = "Counts cannot be negative.";
          updateLRDisplay(null, null);
          updatePostTestDisplay(null, null, null);
          document.getElementById("derived-metrics").classList.add("hidden");
          return;
        }

        const diseased = tp + fn;
        const nonDiseased = fp + tn;
        const total = diseased + nonDiseased;

        if (diseased === 0 || nonDiseased === 0 || total === 0) {
          errorEl.textContent =
            "Both disease-positive and disease-negative groups must have at least one subject.";
          updateLRDisplay(null, null);
          updatePostTestDisplay(null, null, null);
          document.getElementById("derived-metrics").classList.add("hidden");
          return;
        }

        const sens = tp / diseased;
        const spec = tn / nonDiseased;
        const prev = diseased / total;

        let lrPos = null;
        let lrNeg = null;

        const oneMinusSpec = 1 - spec;
        const oneMinusSens = 1 - sens;

        if (oneMinusSpec === 0) {
          lrPos = Infinity;
        } else {
          lrPos = sens / oneMinusSpec;
        }

        if (spec === 0) {
          lrNeg = Infinity;
        } else if (oneMinusSens === 0) {
          lrNeg = 0;
        } else {
          lrNeg = oneMinusSens / spec;
        }

        if (lrPos > 1e6) lrPos = 1e6;
        if (lrNeg > 1e6) lrNeg = 1e6;

        updateLRDisplay(lrPos, lrNeg);

        // Derived metrics display
        const derivedBox = document.getElementById("derived-metrics");
        document.getElementById("sens-derived").textContent = formatPercent(
          sens,
          1
        );
        document.getElementById("spec-derived").textContent = formatPercent(
          spec,
          1
        );
        document.getElementById("prev-derived").textContent = formatPercent(
          prev,
          1
        );
        derivedBox.classList.remove("hidden");

        let preProb = null;
        if (preInput !== "") {
          const preVal = parseFloat(preInput);
          if (isNaN(preVal) || preVal < 0 || preVal > 100) {
            errorEl.textContent =
              "Pre‑test probability must be between 0 and 100%.";
            updatePostTestDisplay(null, null, null);
            return;
          }
          preProb = preVal / 100;
        }

        updatePostTestDisplay(preProb, lrPos, lrNeg);
      });

      document.getElementById("reset-2x2").addEventListener("click", () => {
        ["tp", "fp", "fn", "tn", "pretest-2x2"].forEach(
          (id) => (document.getElementById(id).value = "")
        );
        document.getElementById("error-2x2").textContent = "";
        updateLRDisplay(null, null);
        updatePostTestDisplay(null, null, null);
        document.getElementById("derived-metrics").classList.add("hidden");
      });

      document
        .querySelectorAll("#faq-container .faq-question")
        .forEach((btn) => {
          btn.addEventListener("click", () => {
            const content = btn.nextElementSibling;
            const indicator = btn.querySelector("span:last-child");
            const isHidden = content.classList.contains("hidden");
            document
              .querySelectorAll("#faq-container .faq-question + div")
              .forEach((div) => div.classList.add("hidden"));
            document
              .querySelectorAll("#faq-container .faq-question span:last-child")
              .forEach((span) => (span.textContent = "+"));
            if (isHidden) {
              content.classList.remove("hidden");
              indicator.textContent = "−";
            }
          });
        });

      // Initialize displays
      updateLRDisplay(null, null);
      updatePostTestDisplay(null, null, null);
    </script>

