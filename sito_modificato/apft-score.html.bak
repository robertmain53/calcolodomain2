    <script>
      // --- Simplified APFT scoring model (approximate, not official) ---

      const ageGroups = [
        { min: 17, max: 21, key: "17-21" },
        { min: 22, max: 26, key: "22-26" },
        { min: 27, max: 31, key: "27-31" },
        { min: 32, max: 36, key: "32-36" },
        { min: 37, max: 41, key: "37-41" },
        { min: 42, max: 46, key: "42-46" },
        { min: 47, max: 51, key: "47-51" },
        { min: 52, max: 56, key: "52-56" },
        { min: 57, max: 61, key: "57-61" },
      ];

      // Approximate min/max reps and run times for 60/100 points by gender & age group
      // Values are rough, smoothed estimates for demonstration.
      const profiles = {
        male: {
          "17-21": {
            pushups: { min60: 35, max100: 71 },
            situps: { min60: 47, max100: 78 },
            run: { t100: 780, t60: 990 }, // seconds (13:00 to 16:30)
          },
          "22-26": {
            pushups: { min60: 31, max100: 75 },
            situps: { min60: 43, max100: 80 },
            run: { t100: 786, t60: 996 },
          },
          "27-31": {
            pushups: { min60: 30, max100: 74 },
            situps: { min60: 39, max100: 78 },
            run: { t100: 792, t60: 1002 },
          },
          "32-36": {
            pushups: { min60: 26, max100: 72 },
            situps: { min60: 35, max100: 76 },
            run: { t100: 804, t60: 1014 },
          },
          "37-41": {
            pushups: { min60: 24, max100: 70 },
            situps: { min60: 33, max100: 74 },
            run: { t100: 816, t60: 1032 },
          },
          "42-46": {
            pushups: { min60: 21, max100: 66 },
            situps: { min60: 30, max100: 72 },
            run: { t100: 840, t60: 1056 },
          },
          "47-51": {
            pushups: { min60: 16, max100: 60 },
            situps: { min60: 26, max100: 68 },
            run: { t100: 864, t60: 1080 },
          },
          "52-56": {
            pushups: { min60: 12, max100: 56 },
            situps: { min60: 22, max100: 66 },
            run: { t100: 900, t60: 1110 },
          },
          "57-61": {
            pushups: { min60: 10, max100: 52 },
            situps: { min60: 20, max100: 64 },
            run: { t100: 930, t60: 1140 },
          },
        },
        female: {
          "17-21": {
            pushups: { min60: 13, max100: 42 },
            situps: { min60: 47, max100: 78 },
            run: { t100: 900, t60: 1110 }, // 15:00 to 18:30
          },
          "22-26": {
            pushups: { min60: 11, max100: 40 },
            situps: { min60: 43, max100: 78 },
            run: { t100: 906, t60: 1116 },
          },
          "27-31": {
            pushups: { min60: 10, max100: 39 },
            situps: { min60: 39, max100: 76 },
            run: { t100: 912, t60: 1122 },
          },
          "32-36": {
            pushups: { min60: 9, max100: 37 },
            situps: { min60: 35, max100: 74 },
            run: { t100: 924, t60: 1134 },
          },
          "37-41": {
            pushups: { min60: 8, max100: 36 },
            situps: { min60: 33, max100: 72 },
            run: { t100: 936, t60: 1152 },
          },
          "42-46": {
            pushups: { min60: 7, max100: 34 },
            situps: { min60: 30, max100: 70 },
            run: { t100: 960, t60: 1176 },
          },
          "47-51": {
            pushups: { min60: 6, max100: 30 },
            situps: { min60: 26, max100: 68 },
            run: { t100: 990, t60: 1206 },
          },
          "52-56": {
            pushups: { min60: 5, max100: 28 },
            situps: { min60: 22, max100: 66 },
            run: { t100: 1020, t60: 1236 },
          },
          "57-61": {
            pushups: { min60: 4, max100: 26 },
            situps: { min60: 20, max100: 64 },
            run: { t100: 1050, t60: 1260 },
          },
        },
      };

      function getAgeGroup(age) {
        for (const g of ageGroups) {
          if (age >= g.min && age <= g.max) return g.key;
        }
        // Clamp to nearest group if outside range
        if (age < ageGroups[0].min) return ageGroups[0].key;
        return ageGroups[ageGroups.length - 1].key;
      }

      function scoreReps(reps, cfg) {
        if (!cfg) return 0;
        const { min60, max100 } = cfg;
        if (reps <= 0) return 0;
        if (reps >= max100) return 100;
        if (reps >= min60) {
          const frac = (reps - min60) / (max100 - min60);
          return Math.round(60 + frac * 40);
        }
        // Below min60: drop from 60 downwards more steeply
        const minRepsZero = Math.max(0, min60 - 15);
        if (reps <= minRepsZero) return 0;
        const frac = (reps - minRepsZero) / (min60 - minRepsZero);
        return Math.round(frac * 60);
      }

      function scoreRun(timeSec, cfg) {
        if (!cfg) return 0;
        const { t100, t60 } = cfg;
        if (timeSec <= 0) return 0;
        if (timeSec <= t100) return 100;
        if (timeSec >= t60 + 240) return 0; // 4 minutes slower than 60-pt time -> 0
        if (timeSec <= t60) {
          const frac = (timeSec - t100) / (t60 - t100);
          return Math.round(100 - frac * 40);
        }
        // Between t60 and t60+240: drop from 60 to 0
        const frac = (timeSec - t60) / 240;
        return Math.max(0, Math.round(60 - frac * 60));
      }

      function calculateApft() {
        const gender = document.getElementById("gender").value;
        const age = parseInt(document.getElementById("age").value, 10);
        const pushups =
          parseInt(document.getElementById("pushups").value, 10) || 0;
        const situps =
          parseInt(document.getElementById("situps").value, 10) || 0;
        const runMin =
          parseInt(document.getElementById("runMin").value, 10) || 0;
        const runSec =
          parseInt(document.getElementById("runSec").value, 10) || 0;
        const runTime = runMin * 60 + runSec;

        const groupKey = getAgeGroup(age);
        const prof = profiles[gender][groupKey];

        const puScore = scoreReps(pushups, prof.pushups);
        const suScore = scoreReps(situps, prof.situps);
        const runScore = scoreRun(runTime, prof.run);
        const total = puScore + suScore + runScore;

        // Update inline scores
        document.getElementById("pushupsScore").textContent = puScore;
        document.getElementById("situpsScore").textContent = suScore;
        document.getElementById("runScore").textContent = runScore;

        // Update summary card
        document.getElementById("pushupsScoreSummary").textContent = puScore;
        document.getElementById("situpsScoreSummary").textContent = suScore;
        document.getElementById("runScoreSummary").textContent = runScore;
        document.getElementById("totalScore").textContent = total;

        const resultsCard = document.getElementById("resultsCard");
        resultsCard.classList.remove("hidden");

        const passStatusEl = document.getElementById("passStatus");
        const notesEl = document.getElementById("notesText");

        const eventFails = [];
        if (puScore < 60) eventFails.push("push-ups");
        if (suScore < 60) eventFails.push("sit-ups");
        if (runScore < 60) eventFails.push("2-mile run");

        const passed = total >= 180 && eventFails.length === 0;

        if (passed) {
          passStatusEl.textContent = "PASS";
          passStatusEl.className = "text-lg font-semibold text-green-600";
          notesEl.textContent =
            "You meet the typical APFT standard of at least 60 points in each event and 180 points total (approximate scoring).";
        } else {
          passStatusEl.textContent = "FAIL";
          passStatusEl.className = "text-lg font-semibold text-red-600";
          let msg = "You do not meet the usual APFT standard. ";
          if (total < 180) {
            msg += `Total score (${total}) is below 180. `;
          }
          if (eventFails.length) {
            msg += "Events below 60 points: " + eventFails.join(", ") + ".";
          }
          notesEl.textContent = msg;
        }
      }

      function resetApft() {
        document.getElementById("gender").value = "male";
        document.getElementById("age").value = 25;
        document.getElementById("pushups").value = 40;
        document.getElementById("situps").value = 50;
        document.getElementById("runMin").value = 16;
        document.getElementById("runSec").value = 0;

        [
          "pushupsScore",
          "situpsScore",
          "runScore",
          "pushupsScoreSummary",
          "situpsScoreSummary",
          "runScoreSummary",
          "totalScore",
        ].forEach((id) => (document.getElementById(id).textContent = "0"));

        document.getElementById("resultsCard").classList.add("hidden");
        document.getElementById("passStatus").textContent = "–";
        document.getElementById("passStatus").className =
          "text-lg font-semibold";
        document.getElementById("notesText").textContent = "";
      }

      document
        .getElementById("calculateBtn")
        .addEventListener("click", calculateApft);
      document.getElementById("resetBtn").addEventListener("click", resetApft);

      // Auto-update scores on input change for better UX
      ["gender", "age", "pushups", "situps", "runMin", "runSec"].forEach(
        (id) => {
          document.getElementById(id).addEventListener("change", () => {
            if (
              !document
                .getElementById("resultsCard")
                .classList.contains("hidden")
            ) {
              calculateApft();
            }
          });
        }
      );
    </script>

