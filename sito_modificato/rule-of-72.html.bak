    <script>
      // Tab switching
      const tabs = {
        time: document.getElementById("tab-time"),
        rate: document.getElementById("tab-rate"),
        doubles: document.getElementById("tab-doubles"),
      };
      const panels = {
        time: document.getElementById("panel-time"),
        rate: document.getElementById("panel-rate"),
        doubles: document.getElementById("panel-doubles"),
      };

      function setActiveTab(active) {
        Object.keys(tabs).forEach((key) => {
          const tab = tabs[key];
          const panel = panels[key];
          if (key === active) {
            tab.classList.remove("bg-gray-100", "text-gray-700");
            tab.classList.add("bg-blue-600", "text-white");
            tab.setAttribute("aria-selected", "true");
            panel.classList.remove("hidden");
          } else {
            tab.classList.add("bg-gray-100", "text-gray-700");
            tab.classList.remove("bg-blue-600", "text-white");
            tab.setAttribute("aria-selected", "false");
            panel.classList.add("hidden");
          }
        });
      }

      tabs.time.addEventListener("click", () => setActiveTab("time"));
      tabs.rate.addEventListener("click", () => setActiveTab("rate"));
      tabs.doubles.addEventListener("click", () => setActiveTab("doubles"));

      // Calculator logic
      const calculateBtn = document.getElementById("calculate-btn");
      const ruleConstEl = document.getElementById("rule-constant");
      const compEl = document.getElementById("compounding");

      const resultsContainer = document.getElementById("results-container");
      const ruleMain = document.getElementById("rule-result-main");
      const ruleSub = document.getElementById("rule-result-sub");
      const exactMain = document.getElementById("exact-result-main");
      const exactSub = document.getElementById("exact-result-sub");
      const errorNote = document.getElementById("error-note");

      function toNumber(el) {
        const v = parseFloat(el.value);
        return isNaN(v) ? 0 : v;
      }

      function formatYears(y) {
        if (!isFinite(y)) return "—";
        if (y < 1) return y.toFixed(2) + " years";
        if (y < 10) return y.toFixed(2) + " years";
        return y.toFixed(1) + " years";
      }

      function formatPercent(p) {
        if (!isFinite(p)) return "—";
        return p.toFixed(2) + "%";
      }

      function formatMoney(x) {
        if (!isFinite(x)) return "—";
        return x.toLocaleString(undefined, {
          minimumFractionDigits: 2,
          maximumFractionDigits: 2,
        });
      }

      function exactDoublingTime(ratePct, n) {
        const r = ratePct / 100;
        if (r <= 0) return Infinity;
        if (n === 0) {
          // continuous
          return Math.log(2) / r;
        }
        return Math.log(2) / Math.log(1 + r / n);
      }

      function exactRateForDoubling(years, n) {
        if (years <= 0) return Infinity;
        if (n === 0) {
          // continuous
          return (Math.log(2) / years) * 100;
        }
        const r = Math.pow(2, 1 / (years * n)) - 1;
        return r * n * 100;
      }

      calculateBtn.addEventListener("click", () => {
        const ruleConst = parseFloat(ruleConstEl.value);
        const n = parseInt(compEl.value, 10);

        let mode = "time";
        if (!panels.time.classList.contains("hidden")) mode = "time";
        if (!panels.rate.classList.contains("hidden")) mode = "rate";
        if (!panels.doubles.classList.contains("hidden")) mode = "doubles";

        let ruleValue,
          exactValue,
          principal,
          summaryRule,
          summaryExact,
          errorMsg;

        if (mode === "time") {
          const rate = toNumber(document.getElementById("rate-time"));
          principal = toNumber(document.getElementById("principal-time"));
          if (rate <= 0) {
            alert("Please enter a positive interest rate.");
            return;
          }
          const tRule = ruleConst / rate;
          const tExact = exactDoublingTime(rate, n);
          ruleValue = formatYears(tRule);
          exactValue = formatYears(tExact);

          summaryRule = `At ${rate.toFixed(
            2
          )}% per year, your money doubles in about ${tRule.toFixed(
            2
          )} years using the Rule of ${ruleConst}.`;
          summaryExact = `Using the exact compound interest formula, doubling takes ${tExact.toFixed(
            2
          )} years with ${n === 0 ? "continuous" : "periodic"} compounding.`;

          if (principal > 0 && isFinite(tRule)) {
            const approxFinal = principal * 2;
            summaryRule += ` Starting from $${formatMoney(
              principal
            )}, that’s roughly $${formatMoney(approxFinal)}.`;
          }

          const diff = tExact - tRule;
          const pctErr =
            tExact !== 0 && isFinite(tExact) ? (diff / tExact) * 100 : 0;
          errorMsg = `Rule of ${ruleConst} error: ${Math.abs(diff).toFixed(
            2
          )} years (${pctErr.toFixed(2)}% ${
            diff > 0 ? "underestimate" : "overestimate"
          } of the exact doubling time).`;
        } else if (mode === "rate") {
          const years = toNumber(document.getElementById("years-rate"));
          principal = toNumber(document.getElementById("principal-rate"));
          if (years <= 0) {
            alert("Please enter a positive number of years.");
            return;
          }
          const rRule = ruleConst / years;
          const rExact = exactRateForDoubling(years, n);
          ruleValue = formatPercent(rRule);
          exactValue = formatPercent(rExact);

          summaryRule = `To double your money in ${years.toFixed(
            2
          )} years, the Rule of ${ruleConst} suggests an annual return of about ${rRule.toFixed(
            2
          )}%.`;
          summaryExact = `Using the exact compound interest formula, you would need an annual return of ${rExact.toFixed(
            2
          )}% with ${n === 0 ? "continuous" : "periodic"} compounding.`;

          if (principal > 0 && isFinite(rRule)) {
            const approxFinal = principal * 2;
            summaryRule += ` Starting from $${formatMoney(
              principal
            )}, that’s roughly $${formatMoney(approxFinal)}.`;
          }

          const diff = rExact - rRule;
          const pctErr =
            rExact !== 0 && isFinite(rExact) ? (diff / rExact) * 100 : 0;
          errorMsg = `Rule of ${ruleConst} error: ${Math.abs(diff).toFixed(
            2
          )} percentage points (${pctErr.toFixed(2)}% ${
            diff > 0 ? "underestimate" : "overestimate"
          } of the exact required rate).`;
        } else {
          const years = toNumber(document.getElementById("years-doubles"));
          const rate = toNumber(document.getElementById("rate-doubles"));
          principal = toNumber(document.getElementById("principal-doubles"));
          if (years <= 0 || rate <= 0) {
            alert("Please enter positive values for years and rate.");
            return;
          }
          const tRule = ruleConst / rate;
          const numDoubles = years / tRule;
          ruleValue = numDoubles.toFixed(2) + " doublings";

          let finalApprox = principal;
          if (principal > 0 && isFinite(numDoubles)) {
            finalApprox = principal * Math.pow(2, numDoubles);
            summaryRule = `Over ${years.toFixed(1)} years at ${rate.toFixed(
              2
            )}% per year, the Rule of ${ruleConst} suggests about ${numDoubles.toFixed(
              2
            )} doublings. Starting from $${formatMoney(
              principal
            )}, that’s roughly $${formatMoney(finalApprox)}.`;
          } else {
            summaryRule = `Over ${years.toFixed(1)} years at ${rate.toFixed(
              2
            )}% per year, the Rule of ${ruleConst} suggests about ${numDoubles.toFixed(
              2
            )} doublings.`;
          }

          const r = rate / 100;
          let exactFactor;
          if (n === 0) {
            exactFactor = Math.exp(r * years);
          } else {
            exactFactor = Math.pow(1 + r / n, n * years);
          }
          const exactDoubles = Math.log(exactFactor) / Math.log(2);
          exactValue = exactDoubles.toFixed(2) + " doublings";
          summaryExact = `Using the exact compound interest formula, that corresponds to about ${exactDoubles.toFixed(
            2
          )} doublings (${exactFactor.toFixed(2)}× growth factor).`;

          const diff = exactDoubles - numDoubles;
          const pctErr =
            exactDoubles !== 0 && isFinite(exactDoubles)
              ? (diff / exactDoubles) * 100
              : 0;
          errorMsg = `Rule of ${ruleConst} error: ${Math.abs(diff).toFixed(
            2
          )} doublings (${pctErr.toFixed(2)}% ${
            diff > 0 ? "underestimate" : "overestimate"
          } of the exact number of doublings).`;
        }

        ruleMain.textContent = ruleValue;
        ruleSub.textContent = summaryRule;
        exactMain.textContent = exactValue;
        exactSub.textContent = summaryExact;
        errorNote.textContent = errorMsg;

        resultsContainer.classList.remove("hidden");
      });
    </script>

