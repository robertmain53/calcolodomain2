    <script>
      // Mode toggle
      const modeDecBtn = document.getElementById("mode-dec-to-time");
      const modeTimeBtn = document.getElementById("mode-time-to-dec");
      const panelDec = document.getElementById("panel-dec-to-time");
      const panelTime = document.getElementById("panel-time-to-dec");

      function setMode(mode) {
        const isDec = mode === "dec";
        panelDec.classList.toggle("hidden", !isDec);
        panelTime.classList.toggle("hidden", isDec);
        modeDecBtn.setAttribute("aria-pressed", isDec ? "true" : "false");
        modeTimeBtn.setAttribute("aria-pressed", isDec ? "false" : "true");
      }

      modeDecBtn.addEventListener("click", () => setMode("dec"));
      modeTimeBtn.addEventListener("click", () => setMode("time"));

      // Helper: rounding minutes to nearest step
      function roundMinutes(totalMinutes, step) {
        return Math.round(totalMinutes / step) * step;
      }

      // Decimal -> Time
      const decInput = document.getElementById("decimal-hours");
      const roundingDec = document.getElementById("rounding-dec");
      const decResults = document.getElementById("dec-to-time-results");
      const resHms = document.getElementById("result-hms");
      const resTimecard = document.getElementById("result-timecard");
      const resBreakdown = document.getElementById("result-breakdown");

      document
        .getElementById("convert-dec-to-time")
        .addEventListener("click", () => {
          const val = parseFloat(decInput.value.replace(",", "."));
          if (isNaN(val)) {
            decResults.classList.add("hidden");
            return;
          }

          const sign = val < 0 ? -1 : 1;
          const absVal = Math.abs(val);

          const hours = Math.trunc(absVal);
          const frac = absVal - hours;
          const totalMinutesExact = frac * 60;
          const minutes = Math.trunc(totalMinutesExact);
          const seconds = Math.round((totalMinutesExact - minutes) * 60);

          // Handle rounding overflow (e.g., 59.9s -> 60s)
          let h = hours,
            m = minutes,
            s = seconds;
          if (s === 60) {
            s = 0;
            m += 1;
          }
          if (m === 60) {
            m = 0;
            h += 1;
          }

          // Apply sign to hours
          if (sign < 0) h = -h;

          // Rounding for timecard format
          let step = 0;
          switch (roundingDec.value) {
            case "min1":
              step = 1;
              break;
            case "min5":
              step = 5;
              break;
            case "min6":
              step = 6;
              break;
            case "min15":
              step = 15;
              break;
            default:
              step = 0;
          }

          let timecardStr;
          if (step === 0) {
            // Use exact minutes from h:m:s
            const totalMinutes = Math.abs(h) * 60 + m + s / 60;
            const roundedMinutes = Math.round(totalMinutes);
            const cardHours = Math.trunc(roundedMinutes / 60);
            const cardMinutes = roundedMinutes % 60;
            const signStr = sign < 0 ? "-" : "";
            timecardStr = `${signStr}${String(cardHours).padStart(
              1,
              "0"
            )}:${String(cardMinutes).padStart(2, "0")}`;
          } else {
            const totalMinutes = Math.abs(val) * 60;
            const roundedMinutes = roundMinutes(totalMinutes, step);
            const cardHours = Math.trunc(roundedMinutes / 60);
            const cardMinutes = roundedMinutes % 60;
            const signStr = sign < 0 ? "-" : "";
            timecardStr = `${signStr}${String(cardHours).padStart(
              1,
              "0"
            )}:${String(cardMinutes).padStart(2, "0")}`;
          }

          const signStrMain = sign < 0 ? "-" : "";
          resHms.textContent = `${signStrMain}${String(Math.abs(h)).padStart(
            1,
            "0"
          )}:${String(m).padStart(2, "0")}:${String(s).padStart(2, "0")}`;
          resTimecard.textContent = timecardStr;
          resBreakdown.textContent = `${val.toFixed(
            4
          )} hours = ${signStrMain}${Math.abs(
            h
          )} hours, ${m} minutes, ${s} seconds`;

          decResults.classList.remove("hidden");
        });

      document.getElementById("swap-sign-dec").addEventListener("click", () => {
        if (!decInput.value) return;
        const v = parseFloat(decInput.value.replace(",", "."));
        if (isNaN(v)) return;
        decInput.value = (v * -1).toString();
      });

      document.getElementById("clear-dec").addEventListener("click", () => {
        decInput.value = "";
        decResults.classList.add("hidden");
      });

      // Time -> Decimal
      const hoursInput = document.getElementById("hours");
      const minutesInput = document.getElementById("minutes");
      const secondsInput = document.getElementById("seconds");
      const roundingTime = document.getElementById("rounding-time");
      const timeResults = document.getElementById("time-to-dec-results");
      const resDecimal = document.getElementById("result-decimal");
      const resDecimalExact = document.getElementById("result-decimal-exact");
      const resTotalMinutes = document.getElementById("result-total-minutes");

      document
        .getElementById("convert-time-to-dec")
        .addEventListener("click", () => {
          let h = parseFloat((hoursInput.value || "0").replace(",", "."));
          let m = parseFloat((minutesInput.value || "0").replace(",", "."));
          let s = parseFloat((secondsInput.value || "0").replace(",", "."));

          if (isNaN(h) || isNaN(m) || isNaN(s)) {
            timeResults.classList.add("hidden");
            return;
          }

          const sign = h < 0 ? -1 : 1;
          h = Math.abs(h);

          // Normalize minutes and seconds
          if (m < 0 || s < 0) {
            // If user enters negative minutes/seconds, treat sign separately
            const totalSec = sign * (h * 3600 + Math.abs(m) * 60 + Math.abs(s));
            const absSec = Math.abs(totalSec);
            h = Math.trunc(absSec / 3600);
            const remSec = absSec - h * 3600;
            m = Math.trunc(remSec / 60);
            s = remSec - m * 60;
          }

          const totalSeconds = h * 3600 + m * 60 + s;
          const totalHours = sign * (totalSeconds / 3600);
          const totalMinutes = sign * (totalSeconds / 60);

          let roundedHours = totalHours;
          const step = parseFloat(roundingTime.value);
          if (!isNaN(step) && step > 0) {
            roundedHours = Math.round(totalHours / step) * step;
          }

          resDecimal.textContent = roundedHours.toFixed(
            step === 0.001 ? 3 : step === 0.01 ? 2 : 4
          );
          resDecimalExact.textContent = totalHours.toFixed(6);
          resTotalMinutes.textContent = totalMinutes.toFixed(2) + " minutes";

          timeResults.classList.remove("hidden");
        });

      document
        .getElementById("swap-sign-time")
        .addEventListener("click", () => {
          if (hoursInput.value === "") return;
          const v = parseFloat(hoursInput.value.replace(",", "."));
          if (isNaN(v)) return;
          hoursInput.value = (v * -1).toString();
        });

      document.getElementById("clear-time").addEventListener("click", () => {
        hoursInput.value = "";
        minutesInput.value = "";
        secondsInput.value = "";
        timeResults.classList.add("hidden");
      });

      // Simple FAQ toggle (in addition to global script_faq if any)
      document
        .querySelectorAll("#faq-container .faq-question")
        .forEach((btn) => {
          btn.addEventListener("click", () => {
            const answer = btn.parentElement.querySelector(".faq-answer");
            const icon = btn.querySelector("span:last-child");
            const isHidden = answer.classList.contains("hidden");
            document
              .querySelectorAll("#faq-container .faq-answer")
              .forEach((a) => a.classList.add("hidden"));
            document
              .querySelectorAll("#faq-container .faq-question span:last-child")
              .forEach((i) => (i.textContent = "+"));
            if (isHidden) {
              answer.classList.remove("hidden");
              icon.textContent = "−";
            }
          });
        });

      // Mobile menu script (if not already handled by mobile-menu.js)
      const mobileToggle = document.getElementById("mobile-menu-toggle");
      const mobileMenu = document.getElementById("mobile-menu");
      if (mobileToggle && mobileMenu) {
        mobileToggle.addEventListener("click", () => {
          const expanded =
            mobileToggle.getAttribute("aria-expanded") === "true";
          mobileToggle.setAttribute("aria-expanded", String(!expanded));
          mobileMenu.classList.toggle("hidden");
        });
      }

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
