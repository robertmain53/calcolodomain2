    <script>
      // Data model for circuits
      const circuitsBody = document.getElementById("circuits-body");
      const supplyTypeEl = document.getElementById("supply-type");
      const voltageEl = document.getElementById("supply-voltage");
      const pfEl = document.getElementById("pf");

      const mdCurrentEl = document.getElementById("md-current");
      const mdPowerEl = document.getElementById("md-power");
      const suggestedFuseEl = document.getElementById("suggested-fuse");

      const phaseCurrEls = [
        document.getElementById("phase1-current"),
        document.getElementById("phase2-current"),
        document.getElementById("phase3-current"),
      ];
      const phasePowerEls = [
        document.getElementById("phase1-power"),
        document.getElementById("phase2-power"),
        document.getElementById("phase3-power"),
      ];

      const STANDARD_FUSES_SINGLE = [40, 50, 60, 80, 100, 125, 160];
      const STANDARD_FUSES_THREE = [63, 80, 100, 125, 160, 200, 250];

      const DIVERSITY_OPTIONS = [
        { value: "none", label: "None (100%)" },
        { value: "lighting", label: "Lighting (66%)" },
        { value: "ring", label: "Ring final (100% largest + 40% others)" },
        {
          value: "cooker",
          label: "Cooker (10 A + 30% remainder + 5 A socket)",
        },
        { value: "custom", label: "Custom %" },
      ];

      let circuits = [];

      function createCircuitRow(circuit) {
        const tr = document.createElement("tr");
        tr.className = "border-t";

        tr.innerHTML = `
      <td class="px-2 py-2 align-top">
        <select class="c-type w-32 border rounded-md px-1 py-1 text-xs">
          <option value="lighting">Lighting</option>
          <option value="ring">Ring final</option>
          <option value="radial">Radial / fixed load</option>
          <option value="cooker">Cooker</option>
          <option value="shower">Shower</option>
          <option value="motor">Motor / HVAC</option>
          <option value="other">Other</option>
        </select>
      </td>
      <td class="px-2 py-2 align-top">
        <input type="text" class="c-desc w-40 border rounded-md px-1 py-1 text-xs" placeholder="e.g. Upstairs lighting">
      </td>
      <td class="px-2 py-2 align-top">
        <input type="number" class="c-rating w-20 border rounded-md px-1 py-1 text-xs" min="0" step="0.1">
      </td>
      <td class="px-2 py-2 align-top">
        <select class="c-units w-20 border rounded-md px-1 py-1 text-xs">
          <option value="A">A</option>
          <option value="kW">kW</option>
        </select>
      </td>
      <td class="px-2 py-2 align-top">
        <input type="number" class="c-qty w-16 border rounded-md px-1 py-1 text-xs" min="1" step="1" value="1">
      </td>
      <td class="px-2 py-2 align-top">
        <select class="c-diversity w-40 border rounded-md px-1 py-1 text-xs mb-1"></select>
        <div class="flex items-center space-x-1">
          <span class="text-[10px] text-gray-500">Custom %</span>
          <input type="number" class="c-diversity-custom w-16 border rounded-md px-1 py-0.5 text-[10px]" min="0" max="100" step="1" value="100" disabled>
        </div>
      </td>
      <td class="px-2 py-2 align-top">
        <div class="text-[11px]">
          <div><span class="font-semibold c-div-amps">0.0</span> A</div>
          <div class="text-gray-500"><span class="c-div-kw">0.0</span> kW</div>
        </div>
      </td>
      <td class="px-2 py-2 align-top">
        <select class="c-phase w-20 border rounded-md px-1 py-1 text-xs">
          <option value="auto">Auto</option>
          <option value="L1">L1</option>
          <option value="L2">L2</option>
          <option value="L3">L3</option>
          <option value="3ph">3-phase</option>
        </select>
      </td>
      <td class="px-2 py-2 align-top text-right">
        <button type="button" class="c-remove text-red-600 text-xs hover:underline">Remove</button>
      </td>
    `;

        const typeEl = tr.querySelector(".c-type");
        const descEl = tr.querySelector(".c-desc");
        const ratingEl = tr.querySelector(".c-rating");
        const unitsEl = tr.querySelector(".c-units");
        const qtyEl = tr.querySelector(".c-qty");
        const diversityEl = tr.querySelector(".c-diversity");
        const diversityCustomEl = tr.querySelector(".c-diversity-custom");
        const phaseEl = tr.querySelector(".c-phase");
        const removeBtn = tr.querySelector(".c-remove");

        // Populate diversity select
        DIVERSITY_OPTIONS.forEach((opt) => {
          const o = document.createElement("option");
          o.value = opt.value;
          o.textContent = opt.label;
          diversityEl.appendChild(o);
        });

        // Apply initial values
        typeEl.value = circuit.type || "lighting";
        descEl.value = circuit.desc || "";
        ratingEl.value = circuit.rating ?? "";
        unitsEl.value = circuit.units || "A";
        qtyEl.value = circuit.qty ?? 1;
        diversityEl.value = circuit.diversity || "none";
        diversityCustomEl.value = circuit.diversityCustom ?? 100;
        phaseEl.value = circuit.phase || "auto";

        if (diversityEl.value === "custom") {
          diversityCustomEl.disabled = false;
        }

        // Event listeners
        tr.addEventListener("input", handleRowChange);
        tr.addEventListener("change", handleRowChange);

        diversityEl.addEventListener("change", () => {
          diversityCustomEl.disabled = diversityEl.value !== "custom";
          handleRowChange();
        });

        removeBtn.addEventListener("click", () => {
          tr.remove();
          circuits = circuits.filter((c) => c.id !== circuit.id);
          recalc();
        });

        circuitsBody.appendChild(tr);
        circuit._row = tr;
      }

      function handleRowChange() {
        syncFromDOM();
        recalc();
      }

      function syncFromDOM() {
        const rows = Array.from(circuitsBody.querySelectorAll("tr"));
        circuits = rows.map((tr, index) => {
          const id = tr.dataset.id || `c${index}_${Date.now()}`;
          tr.dataset.id = id;
          return {
            id,
            type: tr.querySelector(".c-type").value,
            desc: tr.querySelector(".c-desc").value.trim(),
            rating: parseFloat(tr.querySelector(".c-rating").value) || 0,
            units: tr.querySelector(".c-units").value,
            qty: parseInt(tr.querySelector(".c-qty").value, 10) || 1,
            diversity: tr.querySelector(".c-diversity").value,
            diversityCustom:
              parseFloat(tr.querySelector(".c-diversity-custom").value) || 100,
            phase: tr.querySelector(".c-phase").value,
          };
        });
      }

      function ampsFromCircuit(c, supplyType, voltage, pf) {
        if (!c.rating || c.rating <= 0) return 0;
        const qty = c.qty || 1;
        const totalRating = c.rating * qty;

        if (c.units === "A") {
          return totalRating;
        } else {
          // kW
          const kW = totalRating;
          if (supplyType === "single") {
            return (kW * 1000) / (voltage * pf);
          } else {
            // three-phase total kW, return line current
            return (kW * 1000) / (Math.sqrt(3) * voltage * pf);
          }
        }
      }

      function applyDiversity(circuits, supplyType, voltage, pf) {
        // First compute raw amps for each circuit
        const withAmps = circuits.map((c) => {
          return { ...c, amps: ampsFromCircuit(c, supplyType, voltage, pf) };
        });

        // Ring final diversity: 100% largest + 40% of others
        const ringCircuits = withAmps.filter((c) => c.diversity === "ring");
        let ringLargest = 0;
        if (ringCircuits.length > 0) {
          ringLargest = Math.max(...ringCircuits.map((c) => c.amps));
        }

        return withAmps.map((c) => {
          let diversifiedAmps = c.amps;

          switch (c.diversity) {
            case "lighting":
              diversifiedAmps = c.amps * 0.66;
              break;
            case "ring": {
              if (c.amps === ringLargest) {
                diversifiedAmps = c.amps; // 100% largest
              } else {
                diversifiedAmps = c.amps * 0.4;
              }
              break;
            }
            case "cooker": {
              // Cooker diversity: 10 A + 30% of remainder + 5 A (if socket)
              // Approximate: convert amps to equivalent
              const I = c.amps;
              if (I <= 10) {
                diversifiedAmps = I;
              } else {
                diversifiedAmps = 10 + 0.3 * (I - 10) + 5;
              }
              break;
            }
            case "custom":
              diversifiedAmps = c.amps * (c.diversityCustom / 100);
              break;
            case "none":
            default:
              diversifiedAmps = c.amps;
          }

          return { ...c, diversifiedAmps };
        });
      }

      function recalc() {
        const supplyType = supplyTypeEl.value;
        const voltage = parseFloat(voltageEl.value) || 230;
        const pf = Math.min(Math.max(parseFloat(pfEl.value) || 0.95, 0.5), 1);

        const diversified = applyDiversity(circuits, supplyType, voltage, pf);

        // Phase allocation
        const phaseCurrents = [0, 0, 0];
        const phasePowers = [0, 0, 0];

        let autoPhaseIndex = 0;

        diversified.forEach((c) => {
          const row = circuitsBody.querySelector(`tr[data-id="${c.id}"]`);
          if (row) {
            const ampsSpan = row.querySelector(".c-div-amps");
            const kwSpan = row.querySelector(".c-div-kw");
            const amps = c.diversifiedAmps;
            const kW = (amps * voltage * pf) / 1000;
            ampsSpan.textContent = amps.toFixed(1);
            kwSpan.textContent = kW.toFixed(2);
          }

          if (c.diversifiedAmps <= 0) return;

          if (supplyType === "single") {
            // All on phase 1
            phaseCurrents[0] += c.diversifiedAmps;
            phasePowers[0] += (c.diversifiedAmps * voltage * pf) / 1000;
          } else {
            // Three-phase
            if (c.phase === "3ph") {
              // Split equally across 3 phases
              const perPhase = c.diversifiedAmps;
              for (let i = 0; i < 3; i++) {
                phaseCurrents[i] += perPhase;
                phasePowers[i] += (perPhase * voltage * pf) / 1000;
              }
            } else {
              let idx;
              if (c.phase === "L1") idx = 0;
              else if (c.phase === "L2") idx = 1;
              else if (c.phase === "L3") idx = 2;
              else {
                // auto-balance: assign to phase with lowest current so far
                idx = phaseCurrents.indexOf(Math.min(...phaseCurrents));
              }
              phaseCurrents[idx] += c.diversifiedAmps;
              phasePowers[idx] += (c.diversifiedAmps * voltage * pf) / 1000;
            }
          }
        });

        // Update phase displays
        for (let i = 0; i < 3; i++) {
          phaseCurrEls[i].textContent = phaseCurrents[i].toFixed(1);
          phasePowerEls[i].textContent = phasePowers[i].toFixed(2);
        }

        const mdCurrent = Math.max(...phaseCurrents);
        const totalPower = phasePowers.reduce((a, b) => a + b, 0);

        mdCurrentEl.textContent = mdCurrent.toFixed(1);
        mdPowerEl.textContent = totalPower.toFixed(2);

        // Suggest fuse
        const fuseList =
          supplyType === "single"
            ? STANDARD_FUSES_SINGLE
            : STANDARD_FUSES_THREE;
        const suggested = fuseList.find((f) => f >= mdCurrent);
        suggestedFuseEl.textContent = suggested
          ? `${suggested} A`
          : "Above 250 A – specialist design";
      }

      function addCircuit(initial = {}) {
        const circuit = {
          id: `c${Date.now()}_${Math.random().toString(16).slice(2)}`,
          type: initial.type || "lighting",
          desc: initial.desc || "",
          rating: initial.rating ?? "",
          units: initial.units || "A",
          qty: initial.qty ?? 1,
          diversity: initial.diversity || "none",
          diversityCustom: initial.diversityCustom ?? 100,
          phase: initial.phase || "auto",
        };
        createCircuitRow(circuit);
        syncFromDOM();
        recalc();
      }

      document
        .getElementById("add-circuit")
        .addEventListener("click", () => addCircuit());

      // Presets
      const presetButtons = document.querySelectorAll(".preset-btn");
      presetButtons.forEach((btn) => {
        btn.addEventListener("click", () => {
          const preset = btn.dataset.preset;
          loadPreset(preset);
        });
      });

      function clearCircuits() {
        circuitsBody.innerHTML = "";
        circuits = [];
      }

      function loadPreset(name) {
        clearCircuits();
        supplyTypeEl.value = "single";
        voltageEl.value = 230;
        pfEl.value = 0.95;

        if (name === "small-domestic") {
          addCircuit({
            type: "lighting",
            desc: "Lighting circuits",
            rating: 6,
            units: "A",
            qty: 4,
            diversity: "lighting",
          });
          addCircuit({
            type: "ring",
            desc: "Ring final (sockets)",
            rating: 32,
            units: "A",
            qty: 1,
            diversity: "ring",
          });
          addCircuit({
            type: "cooker",
            desc: "Cooker 6 kW",
            rating: 6,
            units: "kW",
            qty: 1,
            diversity: "cooker",
          });
          addCircuit({
            type: "shower",
            desc: "Shower 8.5 kW",
            rating: 8.5,
            units: "kW",
            qty: 1,
            diversity: "none",
          });
        } else if (name === "medium-domestic") {
          addCircuit({
            type: "lighting",
            desc: "Lighting circuits",
            rating: 6,
            units: "A",
            qty: 6,
            diversity: "lighting",
          });
          addCircuit({
            type: "ring",
            desc: "Ring finals (sockets)",
            rating: 32,
            units: "A",
            qty: 2,
            diversity: "ring",
          });
          addCircuit({
            type: "cooker",
            desc: "Cooker 8 kW",
            rating: 8,
            units: "kW",
            qty: 1,
            diversity: "cooker",
          });
          addCircuit({
            type: "shower",
            desc: "Shower 9.5 kW",
            rating: 9.5,
            units: "kW",
            qty: 1,
            diversity: "none",
          });
          addCircuit({
            type: "radial",
            desc: "Immersion heater 3 kW",
            rating: 3,
            units: "kW",
            qty: 1,
            diversity: "none",
          });
        } else if (name === "small-commercial") {
          supplyTypeEl.value = "three";
          voltageEl.value = 400;
          pfEl.value = 0.9;
          addCircuit({
            type: "lighting",
            desc: "Office lighting",
            rating: 2,
            units: "kW",
            qty: 6,
            diversity: "lighting",
            phase: "auto",
          });
          addCircuit({
            type: "radial",
            desc: "Socket circuits",
            rating: 32,
            units: "A",
            qty: 3,
            diversity: "ring",
            phase: "auto",
          });
          addCircuit({
            type: "motor",
            desc: "AC units",
            rating: 3,
            units: "kW",
            qty: 3,
            diversity: "custom",
            diversityCustom: 80,
            phase: "3ph",
          });
        }

        recalc();
      }

      document.getElementById("reset-btn").addEventListener("click", () => {
        clearCircuits();
        supplyTypeEl.value = "single";
        voltageEl.value = 230;
        pfEl.value = 0.95;
        addCircuit({
          type: "lighting",
          desc: "Lighting",
          rating: 6,
          units: "A",
          qty: 1,
          diversity: "lighting",
        });
      });

      supplyTypeEl.addEventListener("change", recalc);
      voltageEl.addEventListener("input", recalc);
      pfEl.addEventListener("input", recalc);

      // Initial state
      addCircuit({
        type: "lighting",
        desc: "Lighting",
        rating: 6,
        units: "A",
        qty: 1,
        diversity: "lighting",
