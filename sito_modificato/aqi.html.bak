    <script>
      // AQI breakpoints (US EPA) for common pollutants
      // Values are simplified and cover the standard AQI range 0–500.
      const AQI_BREAKPOINTS = {
        pm25: [
          { Clow: 0.0, Chigh: 12.0, Ilow: 0, Ihigh: 50 },
          { Clow: 12.1, Chigh: 35.4, Ilow: 51, Ihigh: 100 },
          { Clow: 35.5, Chigh: 55.4, Ilow: 101, Ihigh: 150 },
          { Clow: 55.5, Chigh: 150.4, Ilow: 151, Ihigh: 200 },
          { Clow: 150.5, Chigh: 250.4, Ilow: 201, Ihigh: 300 },
          { Clow: 250.5, Chigh: 350.4, Ilow: 301, Ihigh: 400 },
          { Clow: 350.5, Chigh: 500.4, Ilow: 401, Ihigh: 500 },
        ],
        pm10: [
          { Clow: 0, Chigh: 54, Ilow: 0, Ihigh: 50 },
          { Clow: 55, Chigh: 154, Ilow: 51, Ihigh: 100 },
          { Clow: 155, Chigh: 254, Ilow: 101, Ihigh: 150 },
          { Clow: 255, Chigh: 354, Ilow: 151, Ihigh: 200 },
          { Clow: 355, Chigh: 424, Ilow: 201, Ihigh: 300 },
          { Clow: 425, Chigh: 504, Ilow: 301, Ihigh: 400 },
          { Clow: 505, Chigh: 604, Ilow: 401, Ihigh: 500 },
        ],
        o3_8h: [
          { Clow: 0.0, Chigh: 0.054, Ilow: 0, Ihigh: 50 },
          { Clow: 0.055, Chigh: 0.07, Ilow: 51, Ihigh: 100 },
          { Clow: 0.071, Chigh: 0.085, Ilow: 101, Ihigh: 150 },
          { Clow: 0.086, Chigh: 0.105, Ilow: 151, Ihigh: 200 },
          { Clow: 0.106, Chigh: 0.2, Ilow: 201, Ihigh: 300 },
          // higher ranges for 1h O3 are omitted for simplicity
        ],
        no2_1h: [
          { Clow: 0.0, Chigh: 0.053, Ilow: 0, Ihigh: 50 },
          { Clow: 0.054, Chigh: 0.1, Ilow: 51, Ihigh: 100 },
          { Clow: 0.101, Chigh: 0.36, Ilow: 101, Ihigh: 150 },
          { Clow: 0.361, Chigh: 0.649, Ilow: 151, Ihigh: 200 },
          { Clow: 0.65, Chigh: 1.249, Ilow: 201, Ihigh: 300 },
          { Clow: 1.25, Chigh: 1.649, Ilow: 301, Ihigh: 400 },
          { Clow: 1.65, Chigh: 2.049, Ilow: 401, Ihigh: 500 },
        ],
        so2_1h: [
          { Clow: 0.0, Chigh: 0.035, Ilow: 0, Ihigh: 50 },
          { Clow: 0.036, Chigh: 0.075, Ilow: 51, Ihigh: 100 },
          { Clow: 0.076, Chigh: 0.185, Ilow: 101, Ihigh: 150 },
          { Clow: 0.186, Chigh: 0.304, Ilow: 151, Ihigh: 200 },
          { Clow: 0.305, Chigh: 0.604, Ilow: 201, Ihigh: 300 },
          { Clow: 0.605, Chigh: 0.804, Ilow: 301, Ihigh: 400 },
          { Clow: 0.805, Chigh: 1.004, Ilow: 401, Ihigh: 500 },
        ],
        co_8h: [
          { Clow: 0.0, Chigh: 4.4, Ilow: 0, Ihigh: 50 },
          { Clow: 4.5, Chigh: 9.4, Ilow: 51, Ihigh: 100 },
          { Clow: 9.5, Chigh: 12.4, Ilow: 101, Ihigh: 150 },
          { Clow: 12.5, Chigh: 15.4, Ilow: 151, Ihigh: 200 },
          { Clow: 15.5, Chigh: 30.4, Ilow: 201, Ihigh: 300 },
          { Clow: 30.5, Chigh: 40.4, Ilow: 301, Ihigh: 400 },
          { Clow: 40.5, Chigh: 50.4, Ilow: 401, Ihigh: 500 },
        ],
      };

      function computeAQI(conc, breakpoints) {
        if (conc == null || isNaN(conc)) return null;
        for (const bp of breakpoints) {
          if (conc >= bp.Clow && conc <= bp.Chigh) {
            const { Clow, Chigh, Ilow, Ihigh } = bp;
            const aqi =
              ((Ihigh - Ilow) / (Chigh - Clow)) * (conc - Clow) + Ilow;
            return Math.round(aqi);
          }
        }
        return null; // out of range
      }

      function aqiCategory(aqi) {
        if (aqi == null) return { category: "N/A", class: "", advice: "" };
        if (aqi <= 50)
          return {
            category: "Good",
            class: "aqi-good",
            advice:
              "Air quality is satisfactory; outdoor activities are safe for everyone.",
          };
        if (aqi <= 100)
          return {
            category: "Moderate",
            class: "aqi-moderate",
            advice:
              "Acceptable air quality; unusually sensitive people should consider limiting prolonged outdoor exertion.",
          };
        if (aqi <= 150)
          return {
            category: "Unhealthy for Sensitive Groups",
            class: "aqi-usg",
            advice:
              "Sensitive groups should reduce prolonged or heavy outdoor exertion.",
          };
        if (aqi <= 200)
          return {
            category: "Unhealthy",
            class: "aqi-unhealthy",
            advice:
              "Everyone should reduce prolonged or heavy outdoor exertion; sensitive groups should avoid it.",
          };
        if (aqi <= 300)
          return {
            category: "Very Unhealthy",
            class: "aqi-very-unhealthy",
            advice:
              "Health alert: everyone may experience more serious health effects; stay indoors with clean air if possible.",
          };
        return {
          category: "Hazardous",
          class: "aqi-hazardous",
          advice:
            "Health warnings of emergency conditions; follow local health authority guidance and stay indoors.",
        };
      }

      function convertUnit(value, fromUnit, pollutant) {
        if (value == null || isNaN(value)) return null;
        // For simplicity, we only convert between ppm and ppb where needed.
        if (fromUnit === "ppb") {
          return value / 1000; // ppb -> ppm
        }
        return value;
      }

      function calculateAQI() {
        const pm25 = parseFloat(document.getElementById("pm25").value);
        const pm10 = parseFloat(document.getElementById("pm10").value);
        const o3 = parseFloat(document.getElementById("o3").value);
        const no2 = parseFloat(document.getElementById("no2").value);
        const so2 = parseFloat(document.getElementById("so2").value);
        const co = parseFloat(document.getElementById("co").value);

        const o3Unit = document.getElementById("o3-unit").value;
        const no2Unit = document.getElementById("no2-unit").value;
        const so2Unit = document.getElementById("so2-unit").value;

        const pm25AQI = computeAQI(pm25, AQI_BREAKPOINTS.pm25);
        const pm10AQI = computeAQI(pm10, AQI_BREAKPOINTS.pm10);
        const o3Conc = convertUnit(o3, o3Unit, "o3");
        const o3AQI = computeAQI(o3Conc, AQI_BREAKPOINTS.o3_8h);
        const no2Conc = convertUnit(no2, no2Unit, "no2");
        const no2AQI = computeAQI(no2Conc, AQI_BREAKPOINTS.no2_1h);
        const so2Conc = convertUnit(so2, so2Unit, "so2");
        const so2AQI = computeAQI(so2Conc, AQI_BREAKPOINTS.so2_1h);
        const coAQI = computeAQI(co, AQI_BREAKPOINTS.co_8h);

        document.getElementById("pm25-aqi").textContent = pm25AQI ?? "–";
        document.getElementById("pm10-aqi").textContent = pm10AQI ?? "–";
        document.getElementById("o3-aqi").textContent = o3AQI ?? "–";
        document.getElementById("no2-aqi").textContent = no2AQI ?? "–";
        document.getElementById("so2-aqi").textContent = so2AQI ?? "–";
        document.getElementById("co-aqi").textContent = coAQI ?? "–";

        const pollutantAQIs = [
          { name: "PM₂.₅", key: "pm25", value: pm25AQI },
          { name: "PM₁₀", key: "pm10", value: pm10AQI },
          { name: "O₃", key: "o3", value: o3AQI },
          { name: "NO₂", key: "no2", value: no2AQI },
          { name: "SO₂", key: "so2", value: so2AQI },
          { name: "CO", key: "co", value: coAQI },
        ].filter((p) => p.value != null);

        if (pollutantAQIs.length === 0) {
          document.getElementById("aqi-result-card").classList.add("hidden");
          return;
        }

        let overall = pollutantAQIs[0];
        for (const p of pollutantAQIs) {
          if (p.value > overall.value) overall = p;
        }

        const cat = aqiCategory(overall.value);
        const header = document.getElementById("aqi-result-header");
        header.className =
          "px-4 py-3 text-white font-semibold flex items-center justify-between " +
          cat.class;

        document.getElementById("aqi-result-value").textContent = overall.value;
        document.getElementById("aqi-result-category").textContent =
          cat.category;
        document.getElementById("aqi-result-dominant").textContent =
          overall.name;
        document.getElementById("aqi-result-advice").textContent = cat.advice;

        document.getElementById("aqi-result-card").classList.remove("hidden");
      }

      document
        .getElementById("aqi-calc-btn")
        .addEventListener("click", calculateAQI);

      document.getElementById("aqi-clear-btn").addEventListener("click", () => {
        ["pm25", "pm10", "o3", "no2", "so2", "co"].forEach((id) => {
          document.getElementById(id).value = "";
        });
        [
          "pm25-aqi",
          "pm10-aqi",
          "o3-aqi",
          "no2-aqi",
          "so2-aqi",
          "co-aqi",
        ].forEach((id) => {
          document.getElementById(id).textContent = "–";
        });
        document.getElementById("aqi-result-card").classList.add("hidden");
      });
    </script>

