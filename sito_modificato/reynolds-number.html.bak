    <script>
      // Utility: show/hide
      function show(el) {
        el.classList.remove("hidden");
      }
      function hide(el) {
        el.classList.add("hidden");
      }

      const flowTypeButtons = document.querySelectorAll(".flow-type-btn");
      const viscTypeButtons = document.querySelectorAll(".visc-type-btn");
      const presetButtons = document.querySelectorAll(".preset-btn");

      const flowTypeHelp = document.getElementById("flow-type-help");
      const lengthLabelExtra = document.getElementById("length-label-extra");
      const lengthType = document.getElementById("length-type");

      const densityRow = document.getElementById("density-row");
      const dynamicViscRow = document.getElementById("dynamic-visc-row");
      const kinematicViscRow = document.getElementById("kinematic-visc-row");

      const velocityInput = document.getElementById("velocity");
      const velocityUnit = document.getElementById("velocity-unit");
      const lengthInput = document.getElementById("length");
      const lengthUnit = document.getElementById("length-unit");
      const densityInput = document.getElementById("density");
      const densityUnit = document.getElementById("density-unit");
      const dynamicViscInput = document.getElementById("dynamic-visc");
      const dynamicViscUnit = document.getElementById("dynamic-visc-unit");
      const kinematicViscInput = document.getElementById("kinematic-visc");
      const kinematicViscUnit = document.getElementById("kinematic-visc-unit");

      const reResult = document.getElementById("re-result");
      const flowRegime = document.getElementById("flow-regime");
      const flowRegimeRange = document.getElementById("flow-regime-range");
      const lengthSummary = document.getElementById("length-summary");
      const reFormulaUsed = document.getElementById("re-formula-used");
      const errorMessage = document.getElementById("error-message");

      let currentFlowType = "internal"; // 'internal' or 'external'
      let currentViscType = "dynamic"; // 'dynamic' or 'kinematic'

      function setActiveButton(buttons, activeValue, attr) {
        buttons.forEach((btn) => {
          if (btn.dataset[attr] === activeValue) {
            btn.classList.remove("bg-gray-100", "text-gray-800");
            btn.classList.add("bg-blue-600", "text-white");
          } else {
            btn.classList.add("bg-gray-100", "text-gray-800");
            btn.classList.remove("bg-blue-600", "text-white");
          }
        });
      }

      function updateFlowTypeUI() {
        if (currentFlowType === "internal") {
          flowTypeHelp.textContent =
            "For internal flow, the characteristic length is typically the pipe or duct diameter (or hydraulic diameter).";
          lengthLabelExtra.textContent = "(pipe diameter)";
          lengthType.textContent =
            "Internal flow – using diameter as characteristic length.";
        } else {
          flowTypeHelp.textContent =
            "For external flow, the characteristic length is usually plate length, chord length, or body diameter.";
          lengthLabelExtra.textContent = "(plate length / chord / body size)";
          lengthType.textContent =
            "External flow – using body length as characteristic length.";
        }
      }

      function updateViscTypeUI() {
        if (currentViscType === "dynamic") {
          show(densityRow);
          show(dynamicViscRow);
          hide(kinematicViscRow);
          reFormulaUsed.textContent = "Formula: Re = ρ v L / μ";
        } else {
          hide(densityRow);
          hide(dynamicViscRow);
          show(kinematicViscRow);
          reFormulaUsed.textContent = "Formula: Re = v L / ν";
        }
      }

      flowTypeButtons.forEach((btn) => {
        btn.addEventListener("click", () => {
          currentFlowType = btn.dataset.flowType;
          setActiveButton(flowTypeButtons, currentFlowType, "flowType");
          updateFlowTypeUI();
          computeRe();
        });
      });

      viscTypeButtons.forEach((btn) => {
        btn.addEventListener("click", () => {
          currentViscType = btn.dataset.viscType;
          setActiveButton(viscTypeButtons, currentViscType, "viscType");
          updateViscTypeUI();
          computeRe();
        });
      });

      // Unit conversion helpers
      function velocityToSI(v, unit) {
        if (unit === "m_s") return v;
        if (unit === "ft_s") return v * 0.3048;
        if (unit === "km_h") return v * (1000 / 3600);
        if (unit === "mph") return v * 0.44704;
        return NaN;
      }

      function lengthToSI(L, unit) {
        if (unit === "m") return L;
        if (unit === "mm") return L / 1000;
        if (unit === "cm") return L / 100;
        if (unit === "ft") return L * 0.3048;
        if (unit === "in") return L * 0.0254;
        return NaN;
      }

      function densityToSI(rho, unit) {
        if (unit === "kg_m3") return rho;
        if (unit === "g_cm3") return rho * 1000; // 1 g/cm3 = 1000 kg/m3
        if (unit === "lb_ft3") return rho * 16.018463;
        return NaN;
      }

      function dynamicViscToSI(mu, unit) {
        if (unit === "pa_s") return mu;
        if (unit === "cp") return mu * 0.001; // 1 cP = 0.001 Pa·s
        if (unit === "lb_ft_s") return mu * 1.4881639; // approx
        return NaN;
      }

      function kinematicViscToSI(nu, unit) {
        if (unit === "m2_s") return nu;
        if (unit === "cst") return nu * 1e-6; // 1 cSt = 1e-6 m2/s
        if (unit === "ft2_s") return nu * 0.09290304;
        return NaN;
      }

      function classifyFlow(Re) {
        if (!isFinite(Re) || Re <= 0) return { regime: "–", note: "" };

        if (currentFlowType === "internal") {
          if (Re < 2300)
            return {
              regime: "Laminar",
              note: "Re < 2,300 (pipe flow guideline)",
            };
          if (Re <= 4000)
            return {
              regime: "Transitional",
              note: "2,300 ≤ Re ≤ 4,000 (pipe flow guideline)",
            };
          return {
            regime: "Turbulent",
            note: "Re > 4,000 (pipe flow guideline)",
          };
        } else {
          // external flow – use rough guideline around 5e5
          if (Re < 1e5)
            return {
              regime: "Likely laminar",
              note: "Re below typical transition range for external flow",
            };
          if (Re < 5e5)
            return {
              regime: "Approaching transition",
              note: "Re near typical transition for flat plate",
            };
          return {
            regime: "Likely turbulent",
            note: "Re above typical transition for external flow",
          };
        }
      }

      function formatRe(Re) {
        if (!isFinite(Re)) return "–";
        if (Re === 0) return "0";
        if (Re >= 1e5 || Re < 1e-2) {
          return Re.toExponential(3);
        }
        return Re.toLocaleString(undefined, { maximumFractionDigits: 3 });
      }

      function computeRe() {
        errorMessage.classList.add("hidden");
        errorMessage.textContent = "";

        const v = parseFloat(velocityInput.value);
        const L = parseFloat(lengthInput.value);

        if (!isFinite(v) || !isFinite(L) || v <= 0 || L <= 0) {
          reResult.textContent = "–";
          flowRegime.textContent = "–";
          flowRegimeRange.textContent =
            currentFlowType === "internal"
              ? "Re < 2,300: laminar • 2,300–4,000: transitional • > 4,000: turbulent (pipe flow)"
              : "Transition often near Re ≈ 5×10⁵ for external flow over a flat plate.";
          lengthSummary.textContent = "–";
          return;
        }

        const vSI = velocityToSI(v, velocityUnit.value);
        const LSI = lengthToSI(L, lengthUnit.value);

        let Re;
        if (currentViscType === "dynamic") {
          const rho = parseFloat(densityInput.value);
          const mu = parseFloat(dynamicViscInput.value);
          if (!isFinite(rho) || !isFinite(mu) || rho <= 0 || mu <= 0) {
            errorMessage.textContent =
              "Please enter positive values for density and dynamic viscosity.";
            errorMessage.classList.remove("hidden");
            return;
          }
          const rhoSI = densityToSI(rho, densityUnit.value);
          const muSI = dynamicViscToSI(mu, dynamicViscUnit.value);
          Re = (rhoSI * vSI * LSI) / muSI;
        } else {
          const nu = parseFloat(kinematicViscInput.value);
          if (!isFinite(nu) || nu <= 0) {
            errorMessage.textContent =
              "Please enter a positive value for kinematic viscosity.";
            errorMessage.classList.remove("hidden");
            return;
          }
          const nuSI = kinematicViscToSI(nu, kinematicViscUnit.value);
          Re = (vSI * LSI) / nuSI;
        }

        reResult.textContent = formatRe(Re);
        const cls = classifyFlow(Re);
        flowRegime.textContent = cls.regime;
        flowRegimeRange.textContent = cls.note || flowRegimeRange.textContent;

        const lengthVal = lengthInput.value;
        const lengthUnitText =
          lengthUnit.options[lengthUnit.selectedIndex].textContent;
        lengthSummary.textContent = `${lengthVal} ${lengthUnitText}`;
      }

      document
        .getElementById("calculate-btn")
        .addEventListener("click", computeRe);

      document.getElementById("reset-btn").addEventListener("click", () => {
        setTimeout(() => {
          currentFlowType = "internal";
          currentViscType = "dynamic";
          setActiveButton(flowTypeButtons, currentFlowType, "flowType");
          setActiveButton(viscTypeButtons, currentViscType, "viscType");
          updateFlowTypeUI();
          updateViscTypeUI();
          reResult.textContent = "–";
          flowRegime.textContent = "–";
          flowRegimeRange.textContent =
            "Re < 2,300: laminar • 2,300–4,000: transitional • > 4,000: turbulent (pipe flow)";
          lengthSummary.textContent = "–";
          errorMessage.classList.add("hidden");
          errorMessage.textContent = "";
        }, 0);
      });

      presetButtons.forEach((btn) => {
        btn.addEventListener("click", () => {
          const preset = btn.dataset.preset;
          // Reset first
          document.getElementById("reynolds-form").reset();
          currentFlowType = preset === "air-20c" ? "external" : "internal";
          currentViscType = "dynamic";
          setActiveButton(flowTypeButtons, currentFlowType, "flowType");
          setActiveButton(viscTypeButtons, currentViscType, "viscType");
          updateFlowTypeUI();
          updateViscTypeUI();

          if (preset === "water-20c") {
            velocityInput.value = 1.0;
            velocityUnit.value = "m_s";
            lengthInput.value = 0.05;
            lengthUnit.value = "m";
            densityInput.value = 998;
            densityUnit.value = "kg_m3";
            dynamicViscInput.value = 1.0;
            dynamicViscUnit.value = "cp";
          } else if (preset === "air-20c") {
            velocityInput.value = 5.0;
            velocityUnit.value = "m_s";
            lengthInput.value = 1.0;
            lengthUnit.value = "m";
            densityInput.value = 1.204;
            densityUnit.value = "kg_m3";
            dynamicViscInput.value = 1.81e-5;
            dynamicViscUnit.value = "pa_s";
          } else if (preset === "oil") {
            velocityInput.value = 0.2;
            velocityUnit.value = "m_s";
            lengthInput.value = 0.02;
            lengthUnit.value = "m";
            densityInput.value = 850;
            densityUnit.value = "kg_m3";
            dynamicViscInput.value = 0.1;
            dynamicViscUnit.value = "pa_s";
          }

          computeRe();
        });
      });

      // Initialize UI state
      setActiveButton(flowTypeButtons, currentFlowType, "flowType");
      setActiveButton(viscTypeButtons, currentViscType, "viscType");
      updateFlowTypeUI();
      updateViscTypeUI();
    </script>

