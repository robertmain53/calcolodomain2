    <script>
      // Core calculator logic
      (function () {
        const knownSelect = document.getElementById("known-quantity");
        const knownValueInput = document.getElementById("known-value");
        const unitSelect = document.getElementById("length-unit");
        const errorMsg = document.getElementById("error-msg");

        const resSide = document.getElementById("res-side");
        const resHeight = document.getElementById("res-height");
        const resPerimeter = document.getElementById("res-perimeter");
        const resArea = document.getElementById("res-area");
        const resInradius = document.getElementById("res-inradius");
        const resCircumradius = document.getElementById("res-circumradius");

        const calcBtn = document.getElementById("calc-btn");
        const resetBtn = document.getElementById("reset-btn");

        function formatNumber(value, decimals = 4) {
          if (!isFinite(value)) return "–";
          if (Math.abs(value) >= 1e6 || Math.abs(value) <= 1e-4) {
            return value.toExponential(4);
          }
          return parseFloat(value.toFixed(decimals)).toString();
        }

        function clearResults() {
          [
            resSide,
            resHeight,
            resPerimeter,
            resArea,
            resInradius,
            resCircumradius,
          ].forEach((el) => (el.textContent = "–"));
        }

        function calculate() {
          errorMsg.textContent = "";
          clearResults();

          const knownType = knownSelect.value;
          const rawVal = parseFloat(knownValueInput.value);

          if (isNaN(rawVal)) {
            errorMsg.textContent = "Please enter a numeric value.";
            return;
          }
          if (rawVal <= 0) {
            errorMsg.textContent = "Value must be greater than zero.";
            return;
          }

          // Work in base units (same as input unit, since everything is relative)
          let s; // side length in chosen unit

          switch (knownType) {
            case "side":
              s = rawVal;
              break;
            case "height":
              s = (2 / Math.sqrt(3)) * rawVal;
              break;
            case "area":
              s = Math.sqrt((4 * rawVal) / Math.sqrt(3));
              break;
            case "perimeter":
              s = rawVal / 3;
              break;
            case "inradius":
              s = 2 * Math.sqrt(3) * rawVal; // s = 2√3 r
              break;
            case "circumradius":
              s = Math.sqrt(3) * rawVal; // s = √3 R
              break;
            default:
              errorMsg.textContent = "Unknown input type.";
              return;
          }

          if (!isFinite(s) || s <= 0) {
            errorMsg.textContent =
              "Input leads to an invalid triangle. Please check your value.";
            return;
          }

          const h = (Math.sqrt(3) / 2) * s;
          const P = 3 * s;
          const A = (Math.sqrt(3) / 4) * s * s;
          const r = (Math.sqrt(3) / 6) * s;
          const R = 2 * r;

          const unit = unitSelect.value;
          const unit2 = unit + "²";

          resSide.textContent = formatNumber(s, 4) + " " + unit;
          resHeight.textContent = formatNumber(h, 4) + " " + unit;
          resPerimeter.textContent = formatNumber(P, 4) + " " + unit;
          resArea.textContent = formatNumber(A, 4) + " " + unit2;
          resInradius.textContent = formatNumber(r, 4) + " " + unit;
          resCircumradius.textContent = formatNumber(R, 4) + " " + unit;
        }

        function resetAll() {
          knownSelect.value = "side";
          knownValueInput.value = "";
          unitSelect.value = "cm";
          errorMsg.textContent = "";
          clearResults();
        }

        calcBtn.addEventListener("click", calculate);
        resetBtn.addEventListener("click", resetAll);
        knownValueInput.addEventListener("keydown", function (e) {
          if (e.key === "Enter") {
            e.preventDefault();
            calculate();
          }
        });
      })();

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
