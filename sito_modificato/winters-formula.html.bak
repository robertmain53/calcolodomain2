    <script>
      // Winter's formula calculator logic
      function roundTo(value, decimals) {
        const factor = Math.pow(10, decimals);
        return Math.round(value * factor) / factor;
      }

      function calculateWinters() {
        const hco3Input = document.getElementById("hco3");
        const pco2Input = document.getElementById("pco2");
        const lowerOffsetInput = document.getElementById("lower-offset");
        const upperOffsetInput = document.getElementById("upper-offset");
        const errorEl = document.getElementById("calc-error");
        const resultsSection = document.getElementById("results-section");

        const hco3 = parseFloat(hco3Input.value);
        const measuredPco2 = parseFloat(pco2Input.value);
        const lowerOffset = parseFloat(lowerOffsetInput.value);
        const upperOffset = parseFloat(upperOffsetInput.value);

        if (
          isNaN(hco3) ||
          hco3 <= 0 ||
          hco3 > 60 ||
          isNaN(lowerOffset) ||
          isNaN(upperOffset)
        ) {
          errorEl.classList.remove("hidden");
          resultsSection.classList.add("hidden");
          return;
        }

        errorEl.classList.add("hidden");

        // Central estimate uses the midpoint of offsets
        const centralOffset = (lowerOffset + upperOffset) / 2;
        const expectedCentral = 1.5 * hco3 + centralOffset;
        const expectedLow = 1.5 * hco3 + lowerOffset;
        const expectedHigh = 1.5 * hco3 + upperOffset;

        document.getElementById("expected-pco2-main").textContent = roundTo(
          expectedCentral,
          1
        );
        document.getElementById("expected-pco2-range").textContent =
          roundTo(expectedLow, 1) + " – " + roundTo(expectedHigh, 1);

        const compStatusEl = document.getElementById("comp-status");
        const compDetailEl = document.getElementById("comp-detail");

        if (!isNaN(measuredPco2)) {
          let statusText = "";
          let detailText = "";

          if (measuredPco2 >= expectedLow && measuredPco2 <= expectedHigh) {
            statusText = "Appropriate respiratory compensation";
            detailText =
              "Measured pCO₂ (" +
              roundTo(measuredPco2, 1) +
              " mmHg) is within the expected range for metabolic acidosis.";
          } else if (measuredPco2 > expectedHigh) {
            statusText = "Concomitant respiratory acidosis suspected";
            detailText =
              "Measured pCO₂ (" +
              roundTo(measuredPco2, 1) +
              " mmHg) is higher than the expected compensatory range, suggesting hypoventilation.";
          } else if (measuredPco2 < expectedLow) {
            statusText = "Concomitant respiratory alkalosis suspected";
            detailText =
              "Measured pCO₂ (" +
              roundTo(measuredPco2, 1) +
              " mmHg) is lower than the expected compensatory range, suggesting excessive hyperventilation.";
          }

          compStatusEl.textContent = statusText;
          compDetailEl.textContent = detailText;
        } else {
          compStatusEl.textContent =
            "Enter measured pCO₂ to assess compensation.";
          compDetailEl.textContent = "";
        }

        resultsSection.classList.remove("hidden");
      }

      document
        .getElementById("calculate-btn")
        .addEventListener("click", calculateWinters);

      document
        .getElementById("reset-btn")
        .addEventListener("click", function () {
          document.getElementById("winters-form").reset();
          document.getElementById("calc-error").classList.add("hidden");
          document.getElementById("results-section").classList.add("hidden");
        });

      // Recalculate on Enter key in inputs
      ["hco3", "pco2", "lower-offset", "upper-offset"].forEach(function (id) {
        const el = document.getElementById(id);
        el.addEventListener("keydown", function (e) {
          if (e.key === "Enter") {
            e.preventDefault();
            calculateWinters();
          }
        });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
