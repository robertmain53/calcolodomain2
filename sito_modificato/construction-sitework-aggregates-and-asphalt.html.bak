    <script>
      // Utility functions
      function inchesToFeet(inches) {
        return inches / 12;
      }
      function mmToMeters(mm) {
        return mm / 1000;
      }
      function feetToMeters(ft) {
        return ft * 0.3048;
      }
      function metersToFeet(m) {
        return m / 0.3048;
      }
      function yd3FromFt3(ft3) {
        return ft3 / 27;
      }
      function ft3FromYd3(yd3) {
        return yd3 * 27;
      }
      function tonnesToUsTons(t) {
        return t * 1.10231;
      }
      function usTonsToTonnes(tons) {
        return tons / 1.10231;
      }
      function round(val, decimals) {
        if (!isFinite(val)) return NaN;
        const p = Math.pow(10, decimals);
        return Math.round(val * p) / p;
      }

      // Material density presets (t/m3 and lb/ft3)
      const aggDensities = {
        crushed_stone: { t: 1.6, lbft3: 100 },
        gravel: { t: 1.5, lbft3: 95 },
        sand: { t: 1.6, lbft3: 100 },
        asphalt_base: { t: 1.9, lbft3: 120 },
      };

      const aspDensities = {
        hma: { t: 2.35, lbft3: 145 },
        wma: { t: 2.3, lbft3: 143 },
        binder: { t: 2.4, lbft3: 150 },
      };

      document.addEventListener("DOMContentLoaded", function () {
        const lengthUnitEl = document.getElementById("lengthUnit");
        const weightUnitEl = document.getElementById("weightUnit");
        const weightLabels = document.querySelectorAll(".weight-unit-label");

