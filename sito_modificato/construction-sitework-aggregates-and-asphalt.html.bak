          btn.addEventListener("click", () => {
            const mode = btn.getAttribute("data-mode");
            document.querySelectorAll(".mode-tab").forEach((b) => {
              b.classList.remove("border-blue-600", "text-blue-600");
              b.classList.add("border-transparent", "text-gray-600");
            });
            btn.classList.add("border-blue-600", "text-blue-600");
            btn.classList.remove("text-gray-600");

            document
              .querySelectorAll(".calculator-mode")
              .forEach((sec) => sec.classList.add("hidden"));
            document.getElementById("mode-" + mode).classList.remove("hidden");
          });
        });

        // Unit toggles
        function updateLengthUnits() {
          const isImperial = lengthUnitEl.value === "imperial";
          document
            .getElementById("aggDimensionsImperial")
            .classList.toggle("hidden", !isImperial);
          document
            .getElementById("aggDimensionsMetric")
            .classList.toggle("hidden", isImperial);
          document
            .getElementById("aspDimensionsImperial")
            .classList.toggle("hidden", !isImperial);
          document
            .getElementById("aspDimensionsMetric")
            .classList.toggle("hidden", isImperial);
        }

        function updateWeightUnits() {
          const isUsTon = weightUnitEl.value === "us_ton";
          weightLabels.forEach(
            (el) => (el.textContent = isUsTon ? "US tons" : "tonnes")
          );
          document.getElementById("trkTotalWeightUnit").textContent = isUsTon
            ? "US tons"
            : "tonnes";
          document.getElementById("trkCapacityUnit").textContent = isUsTon
            ? "US tons"
            : "tonnes";
        }

        lengthUnitEl.addEventListener("change", updateLengthUnits);
        weightUnitEl.addEventListener("change", updateWeightUnits);
        updateLengthUnits();
        updateWeightUnits();

        // Aggregate material change
        const aggMaterialEl = document.getElementById("aggMaterial");
        const aggCustomDensityRow = document.getElementById(
          "aggCustomDensityRow"
        );
        aggMaterialEl.addEventListener("change", () => {
          aggCustomDensityRow.classList.toggle(
            "hidden",
            aggMaterialEl.value !== "custom"
          );
        });

        // Asphalt mix type change
        const aspMixTypeEl = document.getElementById("aspMixType");
        const aspCustomDensityRow = document.getElementById(
          "aspCustomDensityRow"
        );
        aspMixTypeEl.addEventListener("change", () => {
          aspCustomDensityRow.classList.toggle(
            "hidden",
            aspMixTypeEl.value !== "custom"
          );
        });

        // Aggregate calculator
        document.getElementById("btnAggCalc").addEventListener("click", () => {
          const shape = document.getElementById("aggShape").value;
          const isImperial = lengthUnitEl.value === "imperial";

          let Lm, Wm, Tm;
          if (isImperial) {
            const Lft =
              parseFloat(document.getElementById("aggLenFt").value) || 0;
            const Wft =
              parseFloat(document.getElementById("aggWidFt").value) || 0;
            const Tin =
              parseFloat(document.getElementById("aggThkIn").value) || 0;
            Lm = feetToMeters(Lft);
            Wm = feetToMeters(Wft);
            Tm = feetToMeters(inchesToFeet(Tin));
          } else {
            const L = parseFloat(document.getElementById("aggLenM").value) || 0;
            const W = parseFloat(document.getElementById("aggWidM").value) || 0;
            const Tmm =
              parseFloat(document.getElementById("aggThkMm").value) || 0;
            Lm = L;
            Wm = W;
            Tm = mmToMeters(Tmm);
          }

          let volumeM3 = 0;
          if (shape === "rect" || shape === "trench") {
            volumeM3 = Lm * Wm * Tm;
          } else if (shape === "circle") {
            // Interpret L as diameter, ignore W
            const Dm = Lm;
            volumeM3 = Math.PI * Math.pow(Dm / 2, 2) * Tm;
          }

          const volumeFt3 = ft3FromYd3(yd3FromFt3(0)); // placeholder to keep helpers used

          const volumeYd3 = volumeM3 / 0.764554857984; // 1 yd³ = 0.764554857984 m³

          // Density
          let densityT = 0;
          if (aggMaterialEl.value === "custom") {
            const dT = parseFloat(
              document.getElementById("aggDensityMetric").value
            );
            const dLb = parseFloat(
              document.getElementById("aggDensityImperial").value
            );
            if (dT) densityT = dT;
            else if (dLb) densityT = dLb * 0.0160185; // 1 lb/ft³ ≈ 0.0160185 t/m³
          } else {
            const d = aggDensities[aggMaterialEl.value];
            densityT = d ? d.t : 0;
          }

          const weightTonnes = volumeM3 * densityT;
          const weightUsTons = tonnesToUsTons(weightTonnes);

          // Coverage in ft² at given thickness (imperial thickness)
          let coverageFt2 = 0;
          if (isImperial) {
            const Tin =
              parseFloat(document.getElementById("aggThkIn").value) || 0;
            const Tft = inchesToFeet(Tin);
            const areaFt2 = volumeM3 / 0.0283168 / Tft; // 1 ft³ = 0.0283168 m³
            coverageFt2 = areaFt2;
          } else {
            const Tmm =
              parseFloat(document.getElementById("aggThkMm").value) || 0;
            const Tft = metersToFeet(mmToMeters(Tmm));
            const areaFt2 = volumeM3 / 0.0283168 / (Tft || 1);
            coverageFt2 = areaFt2;
          }

          document.getElementById("aggVolCyd").textContent = isNaN(volumeYd3)
            ? "–"
            : round(volumeYd3, 2);
          document.getElementById("aggVolM3").textContent = isNaN(volumeM3)
            ? "–"
            : round(volumeM3, 2);

          const useUsTons = weightUnitEl.value === "us_ton";
          document.getElementById("aggWeightTons").textContent = isNaN(
            weightUsTons
          )
            ? "–"
            : round(useUsTons ? weightUsTons : weightTonnes, 2);
          document.getElementById("aggWeightTonnes").textContent = isNaN(
            weightTonnes
          )
            ? "–"
            : round(weightTonnes, 2);

          document.getElementById("aggCoverage").textContent = isNaN(
            coverageFt2
          )
            ? "–"
            : round(coverageFt2, 1);
        });

        document.getElementById("btnAggReset").addEventListener("click", () => {
          document.getElementById("aggLenFt").value = 100;
          document.getElementById("aggWidFt").value = 20;
          document.getElementById("aggThkIn").value = 6;
          document.getElementById("aggLenM").value = "";
          document.getElementById("aggWidM").value = "";
          document.getElementById("aggThkMm").value = "";
          document.getElementById("aggMaterial").value = "crushed_stone";
          document
            .getElementById("aggCustomDensityRow")
            .classList.add("hidden");
          document.getElementById("aggDensityMetric").value = "";
          document.getElementById("aggDensityImperial").value = "";
          document.getElementById("aggVolCyd").textContent = "–";
          document.getElementById("aggVolM3").textContent = "–";
          document.getElementById("aggWeightTons").textContent = "–";
          document.getElementById("aggWeightTonnes").textContent = "–";
          document.getElementById("aggCoverage").textContent = "–";
        });

        // Asphalt calculator
        document.getElementById("btnAspCalc").addEventListener("click", () => {
          const isImperial = lengthUnitEl.value === "imperial";

          let Lm, Wm, Tm;
          if (isImperial) {
            const Lft =
              parseFloat(document.getElementById("aspLenFt").value) || 0;
            const Wft =
              parseFloat(document.getElementById("aspWidFt").value) || 0;
            const Tin =
              parseFloat(document.getElementById("aspThkIn").value) || 0;
            Lm = feetToMeters(Lft);
            Wm = feetToMeters(Wft);
            Tm = feetToMeters(inchesToFeet(Tin));
          } else {
            const L = parseFloat(document.getElementById("aspLenM").value) || 0;
            const W = parseFloat(document.getElementById("aspWidM").value) || 0;
            const Tmm =
              parseFloat(document.getElementById("aspThkMm").value) || 0;
            Lm = L;
            Wm = W;
            Tm = mmToMeters(Tmm);
          }

          const areaM2 = Lm * Wm;
          const volumeM3 = areaM2 * Tm;
          const volumeYd3 = volumeM3 / 0.764554857984;

          // Area in ft²
          const areaFt2 = areaM2 / 0.092903;

          // Density
          let densityT = 0;
          if (aspMixTypeEl.value === "custom") {
            const dT = parseFloat(
              document.getElementById("aspDensityMetric").value
            );
            const dLb = parseFloat(
              document.getElementById("aspDensityImperial").value
            );
            if (dT) densityT = dT;
            else if (dLb) densityT = dLb * 0.0160185;
          } else {
            const d = aspDensities[aspMixTypeEl.value];
            densityT = d ? d.t : 0;
          }

          const weightTonnes = volumeM3 * densityT;
          const weightUsTons = tonnesToUsTons(weightTonnes);
          const useUsTons = weightUnitEl.value === "us_ton";

          document.getElementById("aspAreaFt2").textContent = isNaN(areaFt2)
            ? "–"
            : round(areaFt2, 1);
          document.getElementById("aspAreaM2").textContent = isNaN(areaM2)
            ? "–"
            : round(areaM2, 2);
          document.getElementById("aspVolCyd").textContent = isNaN(volumeYd3)
            ? "–"
            : round(volumeYd3, 2);
          document.getElementById("aspVolM3").textContent = isNaN(volumeM3)
            ? "–"
            : round(volumeM3, 2);
          document.getElementById("aspWeightTons").textContent = isNaN(
            weightUsTons
          )
            ? "–"
            : round(useUsTons ? weightUsTons : weightTonnes, 2);
          document.getElementById("aspWeightTonnes").textContent = isNaN(
            weightTonnes
          )
            ? "–"
            : round(weightTonnes, 2);
        });

        document.getElementById("btnAspReset").addEventListener("click", () => {
          document.getElementById("aspLenFt").value = 200;
          document.getElementById("aspWidFt").value = 24;
          document.getElementById("aspThkIn").value = 3;
          document.getElementById("aspLenM").value = "";
          document.getElementById("aspWidM").value = "";
          document.getElementById("aspThkMm").value = "";
          document.getElementById("aspMixType").value = "hma";
          document
            .getElementById("aspCustomDensityRow")
            .classList.add("hidden");
          document.getElementById("aspDensityMetric").value = "";
          document.getElementById("aspDensityImperial").value = "";
          document.getElementById("aspAreaFt2").textContent = "–";
          document.getElementById("aspAreaM2").textContent = "–";
          document.getElementById("aspVolCyd").textContent = "–";
          document.getElementById("aspVolM3").textContent = "–";
          document.getElementById("aspWeightTons").textContent = "–";
          document.getElementById("aspWeightTonnes").textContent = "–";
        });

        // Truckloads & cost
        document
          .getElementById("btnImportAgg")
          .addEventListener("click", () => {
            const useUsTons = weightUnitEl.value === "us_ton";
            const aggWeightTons = parseFloat(
              document.getElementById("aggWeightTons").textContent
            );
            const aggWeightTonnes = parseFloat(
              document.getElementById("aggWeightTonnes").textContent
            );
            if (!isNaN(aggWeightTons) && !isNaN(aggWeightTonnes)) {
              document.getElementById("trkTotalWeight").value = useUsTons
                ? aggWeightTons
                : aggWeightTonnes;
            }
          });

        document
          .getElementById("btnImportAsp")
          .addEventListener("click", () => {
            const useUsTons = weightUnitEl.value === "us_ton";
            const aspWeightTons = parseFloat(
              document.getElementById("aspWeightTons").textContent
            );
            const aspWeightTonnes = parseFloat(
              document.getElementById("aspWeightTonnes").textContent
            );
            if (!isNaN(aspWeightTons) && !isNaN(aspWeightTonnes)) {
              document.getElementById("trkTotalWeight").value = useUsTons
                ? aspWeightTons
                : aspWeightTonnes;
            }
          });

        document.getElementById("btnTrkCalc").addEventListener("click", () => {
          const totalWeight =
            parseFloat(document.getElementById("trkTotalWeight").value) || 0;
          const capacity =
            parseFloat(document.getElementById("trkCapacity").value) || 0;
          const costPerTon =
            parseFloat(document.getElementById("trkCostPerTon").value) || 0;
          const costPerLoad =
            parseFloat(document.getElementById("trkCostPerLoad").value) || 0;

          if (totalWeight <= 0 || capacity <= 0) {
            document.getElementById("trkLoads").textContent = "–";
            document.getElementById("trkMatCost").textContent = "–";
            document.getElementById("trkHaulCost").textContent = "–";
            document.getElementById("trkTotalCost").textContent = "–";
            return;
          }

          const loads = Math.ceil(totalWeight / capacity);
          const matCost = totalWeight * costPerTon;
          const haulCost = loads * costPerLoad;
          const totalCost = matCost + haulCost;

          document.getElementById("trkLoads").textContent = loads;
          document.getElementById("trkMatCost").textContent = isNaN(matCost)
            ? "–"
            : round(matCost, 2).toFixed(2);
          document.getElementById("trkHaulCost").textContent = isNaN(haulCost)
            ? "–"
            : round(haulCost, 2).toFixed(2);
          document.getElementById("trkTotalCost").textContent = isNaN(totalCost)
            ? "–"
            : round(totalCost, 2).toFixed(2);
        });

        document.getElementById("btnTrkReset").addEventListener("click", () => {
          document.getElementById("trkTotalWeight").value = "";
          document.getElementById("trkCapacity").value = 20;
          document.getElementById("trkCostPerTon").value = "";
          document.getElementById("trkCostPerLoad").value = "";
          document.getElementById("trkLoads").textContent = "–";
          document.getElementById("trkMatCost").textContent = "–";
          document.getElementById("trkHaulCost").textContent = "–";
          document.getElementById("trkTotalCost").textContent = "–";
        });
      });
    </script>

