<script>
  // Simple tab switching
  const tabs = document.querySelectorAll('[data-tab]');
  const panels = {
    basic: document.getElementById('panel-basic'),
    factors: document.getElementById('panel-factors'),
    result: document.getElementById('panel-result')
  };

  tabs.forEach(btn => {
    btn.addEventListener('click', () => {
      const target = btn.getAttribute('data-tab');
      tabs.forEach(b => {
        b.classList.remove('tab-active');
        b.classList.add('bg-white', 'text-gray-700');
      });
      btn.classList.add('tab-active');
      btn.classList.remove('bg-white', 'text-gray-700');

      Object.keys(panels).forEach(key => {
        panels[key].classList.toggle('hidden', key !== target);
      });
    });
  });

  function recalcEC1() {
    const gk = parseFloat(document.getElementById('gk').value) || 0;
    const qkLeading = parseFloat(document.getElementById('qk_leading').value) || 0;
    const qkSnow = parseFloat(document.getElementById('qk_snow').value) || 0;
    const qkWind = parseFloat(document.getElementById('qk_wind').value) || 0;

    const gammaG = parseFloat(document.getElementById('gamma_g').value) || 1;
    const gammaQ = parseFloat(document.getElementById('gamma_q').value) || 1;
    const psi0Snow = parseFloat(document.getElementById('psi0_snow').value) || 0;
    const psi0Wind = parseFloat(document.getElementById('psi0_wind').value) || 0;
    const psi2 = parseFloat(document.getElementById('psi2_perm').value) || 0;

    const Ed = gammaG * gk +
               gammaQ * qkLeading +
               gammaQ * psi0Snow * qkSnow +
               gammaQ * psi0Wind * qkWind;

    const Eqp = gk + psi2 * qkLeading;

    document.getElementById('ed-value').textContent = Ed.toFixed(2);
    document.getElementById('eqp-value').textContent = Eqp.toFixed(2);
  }

  document.getElementById('btn-calc').addEventListener('click', recalcEC1);
  // Initial calculation
  recalcEC1();
</script>

