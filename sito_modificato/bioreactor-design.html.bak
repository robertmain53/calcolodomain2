<script>
        function calculateDesign() {
            const volume = parseFloat(document.getElementById('volume').value);
            const flowRate = parseFloat(document.getElementById('flowRate').value);
            let hasError = false;

            if (isNaN(volume) || volume <= 0) {
                document.getElementById('volumeError').textContent = 'Please enter a valid volume greater than 0.';
                hasError = true;
            } else {
                document.getElementById('volumeError').textContent = '';
            }

            if (isNaN(flowRate) || flowRate <= 0) {
                document.getElementById('flowRateError').textContent = 'Please enter a valid flow rate greater than 0.';
                hasError = true;
            } else {
                document.getElementById('flowRateError').textContent = '';
            }

            if (!hasError) {
                const designParameter = volume / flowRate;
                document.getElementById('designParameterValue').textContent = designParameter.toFixed(2);
            }
        }
    </script>


