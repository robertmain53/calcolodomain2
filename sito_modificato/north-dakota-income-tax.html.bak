    <script>
      // --- Simplified North Dakota tax model ---
      // NOTE: These brackets and deductions are approximate and for educational use only.
      const ND_TAX_DATA = {
        2024: {
          standardDeduction: {
            single: 14000,
            married_joint: 28000,
            married_separate: 14000,
            hoh: 21000,
          },
          brackets: {
            // thresholds are taxable income limits for each bracket
            single: [
              { upTo: 45000, rate: 0.012 }, // 1.2%
              { upTo: 100000, rate: 0.02 }, // 2.0%
              { upTo: Infinity, rate: 0.026 }, // 2.6%
            ],
            married_joint: [
              { upTo: 75000, rate: 0.012 },
              { upTo: 200000, rate: 0.02 },
              { upTo: Infinity, rate: 0.026 },
            ],
            married_separate: [
              { upTo: 37500, rate: 0.012 },
              { upTo: 100000, rate: 0.02 },
              { upTo: Infinity, rate: 0.026 },
            ],
            hoh: [
              { upTo: 60000, rate: 0.012 },
              { upTo: 150000, rate: 0.02 },
              { upTo: Infinity, rate: 0.026 },
            ],
          },
        },
        2023: {
          standardDeduction: {
            single: 13850,
            married_joint: 27700,
            married_separate: 13850,
            hoh: 20800,
          },
          brackets: {
            single: [
              { upTo: 44000, rate: 0.012 },
              { upTo: 99000, rate: 0.02 },
              { upTo: Infinity, rate: 0.026 },
            ],
            married_joint: [
              { upTo: 73000, rate: 0.012 },
              { upTo: 195000, rate: 0.02 },
              { upTo: Infinity, rate: 0.026 },
            ],
            married_separate: [
              { upTo: 36500, rate: 0.012 },
              { upTo: 97500, rate: 0.02 },
              { upTo: Infinity, rate: 0.026 },
            ],
            hoh: [
              { upTo: 59000, rate: 0.012 },
              { upTo: 148000, rate: 0.02 },
              { upTo: Infinity, rate: 0.026 },
            ],
          },
        },
      };

      function formatCurrency(value) {
        if (!isFinite(value)) return "$0";
        return value.toLocaleString("en-US", {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 0,
        });
      }

      function formatPercent(value) {
        if (!isFinite(value)) return "0%";
        return (value * 100).toFixed(1).replace(/\.0$/, "") + "%";
      }

      function getSelectedFilingStatus() {
        const radios = document.querySelectorAll('input[name="filingStatus"]');
        for (const r of radios) {
          if (r.checked) return r.value;
        }
        return "single";
      }

      function getDeductionType() {
        const radios = document.querySelectorAll('input[name="deductionType"]');
        for (const r of radios) {
          if (r.checked) return r.value;
        }
        return "standard";
      }

      function calculateNdTax() {
        const year = parseInt(document.getElementById("tax-year").value, 10);
        const filingStatus = getSelectedFilingStatus();
        const data = ND_TAX_DATA[year];

        const wage =
          parseFloat(document.getElementById("wage-income").value) || 0;
        const otherIncome =
          parseFloat(document.getElementById("other-income").value) || 0;
        const pretaxRet =
          parseFloat(document.getElementById("pretax-retirement").value) || 0;
        const pretaxOther =
          parseFloat(document.getElementById("pretax-other").value) || 0;
        const credits =
          parseFloat(document.getElementById("nd-credits").value) || 0;

        const grossIncome = Math.max(wage + otherIncome, 0);
        const adjustedIncome = Math.max(
          grossIncome - pretaxRet - pretaxOther,
          0
        );

        let deductions = 0;
        const deductionType = getDeductionType();
        if (deductionType === "standard") {
          deductions = data.standardDeduction[filingStatus] || 0;
        } else {
          deductions =
            parseFloat(document.getElementById("itemized-amount").value) || 0;
        }

        let taxableIncome = Math.max(adjustedIncome - deductions, 0);

        // Calculate tax by brackets
        const brackets = data.brackets[filingStatus] || [];
        let remaining = taxableIncome;
        let tax = 0;
        const bracketDetails = [];

        let lowerLimit = 0;
        for (const bracket of brackets) {
          if (remaining <= 0) break;
          const upper = bracket.upTo;
          const taxableInBracket = Math.max(
            Math.min(remaining, upper - lowerLimit),
            0
          );
          const taxForBracket = taxableInBracket * bracket.rate;
          if (taxableInBracket > 0) {
            bracketDetails.push({
              range: `${formatCurrency(lowerLimit)} – ${
                upper === Infinity ? "∞" : formatCurrency(upper)
              }`,
              amount: taxableInBracket,
              rate: bracket.rate,
              tax: taxForBracket,
            });
          }
          tax += taxForBracket;
          remaining -= taxableInBracket;
          lowerLimit = upper;
        }

        let taxAfterCredits = Math.max(tax - credits, 0);
        const effectiveRate =
          grossIncome > 0 ? taxAfterCredits / grossIncome : 0;

        // Take-home per selected period (state tax only)
        const period = document.getElementById("pay-frequency").value;
        let divisor = 1;
        let periodLabel = "year";
        if (period === "monthly") {
          divisor = 12;
          periodLabel = "month";
        } else if (period === "biweekly") {
          divisor = 26;
          periodLabel = "2 weeks";
        } else if (period === "weekly") {
          divisor = 52;
          periodLabel = "week";
        }

        const takeHomeAnnual = grossIncome - taxAfterCredits;
        const takeHomePeriod = takeHomeAnnual / divisor;

        // Update UI
        document.getElementById("nd-tax-output").textContent =
          formatCurrency(taxAfterCredits);
        document.getElementById(
          "nd-effective-rate-label"
        ).textContent = `Effective ND tax rate: ${formatPercent(
          effectiveRate
        )}`;
        document.getElementById("takehome-output").textContent =
          formatCurrency(takeHomePeriod);
        document.getElementById("takehome-period-label").textContent =
          periodLabel;
        document.getElementById(
          "gross-income-label"
        ).textContent = `Based on gross income of ${formatCurrency(
          grossIncome
        )}`;
        document.getElementById("taxable-income-output").textContent =
          formatCurrency(taxableIncome);
        document.getElementById("deductions-output").textContent =
          formatCurrency(deductions);
        document.getElementById("credits-output").textContent =
          formatCurrency(credits);

        // Bracket table
        const tbody = document.getElementById("bracket-table-body");
        tbody.innerHTML = "";
        if (bracketDetails.length === 0) {
          const tr = document.createElement("tr");
          tr.innerHTML =
            '<td colspan="4" class="px-3 py-3 text-center text-gray-500">No taxable income for this year.</td>';
          tbody.appendChild(tr);
        } else {
          bracketDetails.forEach((b, idx) => {
            const tr = document.createElement("tr");
            tr.className = idx % 2 === 0 ? "bg-white" : "bg-gray-50";
            tr.innerHTML = `
          <td class="px-3 py-2 text-gray-700">${b.range}</td>
          <td class="px-3 py-2 text-right text-gray-700">${formatCurrency(
            b.amount
          )}</td>
          <td class="px-3 py-2 text-right text-gray-700">${formatPercent(
            b.rate
          )}</td>
          <td class="px-3 py-2 text-right text-gray-700">${formatCurrency(
            b.tax
          )}</td>
        `;
            tbody.appendChild(tr);
          });
        }
      }

      document.addEventListener("DOMContentLoaded", function () {
        const itemizedWrapper = document.getElementById("itemized-wrapper");
        const deductionRadios = document.querySelectorAll(
          'input[name="deductionType"]'
        );
        deductionRadios.forEach((r) => {
          r.addEventListener("change", () => {
            if (getDeductionType() === "itemized") {
              itemizedWrapper.classList.remove("hidden");
            } else {
              itemizedWrapper.classList.add("hidden");
            }
          });
        });

        document
          .getElementById("calculate-btn")
          .addEventListener("click", calculateNdTax);
        document
          .getElementById("pay-frequency")
          .addEventListener("change", calculateNdTax);

        document.getElementById("reset-btn").addEventListener("click", () => {
          document.getElementById("tax-year").value = "2024";
          document.querySelector(
            'input[name="filingStatus"][value="single"]'
          ).checked = true;
          document.getElementById("wage-income").value = 60000;
          document.getElementById("other-income").value = 0;
          document.getElementById("pretax-retirement").value = 0;
          document.getElementById("pretax-other").value = 0;
          document.querySelector(
            'input[name="deductionType"][value="standard"]'
          ).checked = true;
          document.getElementById("itemized-amount").value = 0;
          document.getElementById("nd-credits").value = 0;
          document.getElementById("dependents").value = 0;
          document.getElementById("pay-frequency").value = "annual";
          document.getElementById("itemized-wrapper").classList.add("hidden");
          calculateNdTax();
        });

        // Initial calculation
