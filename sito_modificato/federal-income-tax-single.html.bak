    <script>
      // --- Tax bracket data (approximate; for educational use only) ---
      const TAX_DATA = {
        2024: {
          standardDeduction: 14600, // Single
          brackets: [
            { upTo: 11600, rate: 0.1 },
            { upTo: 47150, rate: 0.12 },
            { upTo: 100525, rate: 0.22 },
            { upTo: 191950, rate: 0.24 },
            { upTo: 243725, rate: 0.32 },
            { upTo: 609350, rate: 0.35 },
            { upTo: Infinity, rate: 0.37 },
          ],
        },
        2025: {
          // Projected values
          standardDeduction: 15000,
          brackets: [
            { upTo: 11900, rate: 0.1 },
            { upTo: 48500, rate: 0.12 },
            { upTo: 103000, rate: 0.22 },
            { upTo: 196000, rate: 0.24 },
            { upTo: 249000, rate: 0.32 },
            { upTo: 622000, rate: 0.35 },
            { upTo: Infinity, rate: 0.37 },
          ],
        },
      };

      function formatCurrency(value) {
        if (!isFinite(value)) return "–";
        return value.toLocaleString("en-US", {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 0,
        });
      }

      function formatPercent(value) {
        if (!isFinite(value)) return "–";
        return (value * 100).toFixed(1) + "%";
      }

      function calculateTax(
        yearKey,
        grossIncome,
        deductionType,
        itemizedAmount
      ) {
        const yearData = TAX_DATA[yearKey];
        if (!yearData) return null;

        let deduction = 0;
        if (deductionType === "standard") {
          deduction = yearData.standardDeduction;
        } else if (deductionType === "itemized") {
          deduction = Math.max(0, itemizedAmount || 0);
        } else {
          deduction = 0;
        }

        const taxableIncome = Math.max(0, grossIncome - deduction);
        let remaining = taxableIncome;
        let lastCap = 0;
        let totalTax = 0;
        const breakdown = [];

        for (const bracket of yearData.brackets) {
          if (remaining <= 0) break;
          const cap = bracket.upTo;
          const span = Math.min(remaining, cap - lastCap);
          if (span > 0) {
            const taxInBracket = span * bracket.rate;
            breakdown.push({
              from: lastCap,
              to: cap,
              rate: bracket.rate,
              amount: span,
              tax: taxInBracket,
            });
            totalTax += taxInBracket;
            remaining -= span;
          }
          lastCap = cap;
        }

        const marginalRate = breakdown.length
          ? breakdown[breakdown.length - 1].rate
          : 0;
        const effectiveRate = grossIncome > 0 ? totalTax / grossIncome : 0;

        return {
          grossIncome,
          deduction,
          taxableIncome,
          totalTax,
          marginalRate,
          effectiveRate,
          breakdown,
        };
      }

      function updateStandardDeductionNote() {
        const yearKey = document.getElementById("tax-year").value;
        const data = TAX_DATA[yearKey];
        const noteEl = document.getElementById("standard-deduction-note");
        if (!data || !noteEl) return;
        noteEl.textContent = `Standard deduction for Single filers in ${yearKey}: ${formatCurrency(
          data.standardDeduction
        )} (approximate).`;
      }

      function renderResults(result) {
        if (!result) return;
        document.getElementById("total-tax").textContent = formatCurrency(
          result.totalTax
        );
        document.getElementById("after-tax-income").textContent =
          formatCurrency(result.grossIncome - result.totalTax);
        document.getElementById("marginal-rate").textContent = formatPercent(
          result.marginalRate
        );
        document.getElementById("effective-rate").textContent = formatPercent(
          result.effectiveRate
        );

        const container = document.getElementById("bracket-breakdown");
        if (!result.breakdown.length) {
          container.innerHTML =
            '<p class="text-gray-500">No taxable income after deductions, so no federal income tax is due.</p>';
          return;
        }

        let html =
          '<table class="min-w-full text-xs border border-gray-200"><thead class="bg-gray-50"><tr><th class="px-2 py-1 text-left border-b">Bracket</th><th class="px-2 py-1 text-right border-b">Taxed amount</th><th class="px-2 py-1 text-right border-b">Rate</th><th class="px-2 py-1 text-right border-b">Tax</th></tr></thead><tbody>';
        result.breakdown.forEach((b) => {
          const from = formatCurrency(b.from);
          const to = b.to === Infinity ? "and above" : formatCurrency(b.to);
          html += `<tr>
        <td class="px-2 py-1 border-b">${from} – ${to}</td>
        <td class="px-2 py-1 border-b text-right">${formatCurrency(
          b.amount
        )}</td>
        <td class="px-2 py-1 border-b text-right">${formatPercent(b.rate)}</td>
        <td class="px-2 py-1 border-b text-right">${formatCurrency(b.tax)}</td>
      </tr>`;
        });
        html += "</tbody></table>";
        container.innerHTML = html;
      }

      function syncSliderToIncome() {
        const incomeInput = document.getElementById("gross-income");
        const slider = document.getElementById("taxable-slider");
        const output = document.getElementById("slider-output");
        const val = Number(incomeInput.value) || 0;
        slider.value = Math.max(slider.min, Math.min(slider.max, val));
        output.textContent = formatCurrency(val).replace("$", "$");
      }

      function syncIncomeToSlider() {
        const incomeInput = document.getElementById("gross-income");
        const slider = document.getElementById("taxable-slider");
        const output = document.getElementById("slider-output");
        const val = Number(slider.value) || 0;
        incomeInput.value = val;
        output.textContent = formatCurrency(val).replace("$", "$");
      }

      document.addEventListener("DOMContentLoaded", () => {
        const yearSelect = document.getElementById("tax-year");
        const deductionTypeSelect = document.getElementById("deduction-type");
        const calculateBtn = document.getElementById("calculate-btn");
        const incomeInput = document.getElementById("gross-income");
        const slider = document.getElementById("taxable-slider");

        updateStandardDeductionNote();
        syncSliderToIncome();

        yearSelect.addEventListener("change", () => {
          updateStandardDeductionNote();
          if (deductionTypeSelect.value === "standard") {
            // Recalculate immediately for convenience
            calculateBtn.click();
          }
        });

        deductionTypeSelect.addEventListener("change", () => {
          const wrapper = document.getElementById("itemized-wrapper");
          if (deductionTypeSelect.value === "itemized") {
            wrapper.classList.remove("hidden");
          } else {
            wrapper.classList.add("hidden");
          }
        });

        incomeInput.addEventListener("input", () => {
          syncSliderToIncome();
        });

        slider.addEventListener("input", () => {
          syncIncomeToSlider();
        });

        calculateBtn.addEventListener("click", () => {
          const yearKey = yearSelect.value;
          const grossIncome = Math.max(0, Number(incomeInput.value) || 0);
          const deductionType = deductionTypeSelect.value;
          const itemized =
            Number(document.getElementById("itemized-deductions").value) || 0;

          const result = calculateTax(
            yearKey,
            grossIncome,
            deductionType,
            itemized
          );
          renderResults(result);
        });

        // Initial auto-calc
        calculateBtn.click();
      });
    </script>

