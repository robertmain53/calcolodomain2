    <script>
      // Pauling electronegativity data (subset of common elements)
      const EN_DATA = [
        { Z: 1, symbol: "H", name: "Hydrogen", en: 2.2 },
        { Z: 2, symbol: "He", name: "Helium", en: null },
        { Z: 3, symbol: "Li", name: "Lithium", en: 0.98 },
        { Z: 4, symbol: "Be", name: "Beryllium", en: 1.57 },
        { Z: 5, symbol: "B", name: "Boron", en: 2.04 },
        { Z: 6, symbol: "C", name: "Carbon", en: 2.55 },
        { Z: 7, symbol: "N", name: "Nitrogen", en: 3.04 },
        { Z: 8, symbol: "O", name: "Oxygen", en: 3.44 },
        { Z: 9, symbol: "F", name: "Fluorine", en: 3.98 },
        { Z: 10, symbol: "Ne", name: "Neon", en: null },
        { Z: 11, symbol: "Na", name: "Sodium", en: 0.93 },
        { Z: 12, symbol: "Mg", name: "Magnesium", en: 1.31 },
        { Z: 13, symbol: "Al", name: "Aluminium", en: 1.61 },
        { Z: 14, symbol: "Si", name: "Silicon", en: 1.9 },
        { Z: 15, symbol: "P", name: "Phosphorus", en: 2.19 },
        { Z: 16, symbol: "S", name: "Sulfur", en: 2.58 },
        { Z: 17, symbol: "Cl", name: "Chlorine", en: 3.16 },
        { Z: 18, symbol: "Ar", name: "Argon", en: null },
        { Z: 19, symbol: "K", name: "Potassium", en: 0.82 },
        { Z: 20, symbol: "Ca", name: "Calcium", en: 1.0 },
        { Z: 26, symbol: "Fe", name: "Iron", en: 1.83 },
        { Z: 29, symbol: "Cu", name: "Copper", en: 1.9 },
        { Z: 30, symbol: "Zn", name: "Zinc", en: 1.65 },
        { Z: 35, symbol: "Br", name: "Bromine", en: 2.96 },
        { Z: 53, symbol: "I", name: "Iodine", en: 2.66 },
        { Z: 80, symbol: "Hg", name: "Mercury", en: 2.0 },
      ];

      function populateElementSelects() {
        const selectA = document.getElementById("elementA");
        const selectB = document.getElementById("elementB");
        if (!selectA || !selectB) return;

        EN_DATA.forEach((el) => {
          if (el.en === null) return; // skip elements without defined EN
          const optA = document.createElement("option");
          optA.value = el.symbol;
          optA.textContent = `${el.symbol} — ${el.name} (${el.en})`;
          selectA.appendChild(optA);

          const optB = document.createElement("option");
          optB.value = el.symbol;
          optB.textContent = `${el.symbol} — ${el.name} (${el.en})`;
          selectB.appendChild(optB);
        });

        // Default selection: H and Cl
        selectA.value = "H";
        selectB.value = "Cl";
        syncENInputsFromSelect("A");
        syncENInputsFromSelect("B");
      }

      function syncENInputsFromSelect(which) {
        const select = document.getElementById(`element${which}`);
        const input = document.getElementById(`en${which}`);
        if (!select || !input) return;
        const el = EN_DATA.find((e) => e.symbol === select.value);
        if (el && typeof el.en === "number") {
          input.value = el.en.toFixed(2);
        }
      }

      function buildENTable() {
        const tbody = document.getElementById("enTableBody");
        if (!tbody) return;
        tbody.innerHTML = "";
        EN_DATA.filter((e) => typeof e.en === "number").forEach((el) => {
          const tr = document.createElement("tr");
          tr.dataset.symbol = el.symbol.toLowerCase();
          tr.dataset.name = el.name.toLowerCase();
          tr.innerHTML = `
        <td class="px-3 py-2 whitespace-nowrap text-gray-700">${el.Z}</td>
        <td class="px-3 py-2 whitespace-nowrap font-semibold text-gray-800">${
          el.symbol
        }</td>
        <td class="px-3 py-2 whitespace-nowrap text-gray-700">${el.name}</td>
        <td class="px-3 py-2 whitespace-nowrap text-gray-700">${el.en.toFixed(
          2
        )}</td>
      `;
          tbody.appendChild(tr);
        });
      }

      function filterENTable() {
        const filterInput = document.getElementById("tableFilter");
        const tbody = document.getElementById("enTableBody");
        if (!filterInput || !tbody) return;
        const q = filterInput.value.trim().toLowerCase();
        Array.from(tbody.rows).forEach((row) => {
          if (!q) {
            row.classList.remove("hidden");
            return;
          }
          const sym = row.dataset.symbol || "";
          const name = row.dataset.name || "";
          if (sym.includes(q) || name.includes(q)) {
            row.classList.remove("hidden");
          } else {
            row.classList.add("hidden");
          }
        });
      }

      function classifyBond(deltaEN) {
        let type = "";
        let badgeText = "";
        let badgeClass = "";

        if (deltaEN < 0.4) {
          type = "Nonpolar covalent";
          badgeText = "≈ equal sharing";
          badgeClass = "bg-green-100 text-green-800 border border-green-300";
        } else if (deltaEN < 1.7) {
          type = "Polar covalent";
          badgeText = "unequal sharing";
          badgeClass = "bg-yellow-100 text-yellow-800 border border-yellow-300";
        } else {
          type = "Ionic (strongly polar)";
          badgeText = "large ΔEN";
          badgeClass = "bg-red-100 text-red-800 border border-red-300";
        }
        return { type, badgeText, badgeClass };
      }

      function estimateIonicCharacter(deltaEN) {
        // Pauling-like empirical relation
        const pct = (1 - Math.exp(-0.25 * deltaEN * deltaEN)) * 100;
        return Math.max(0, Math.min(100, pct));
      }

      function calculateEN() {
        const enAInput = document.getElementById("enA");
        const enBInput = document.getElementById("enB");
        const selA = document.getElementById("elementA");
        const selB = document.getElementById("elementB");
        const errorEl = document.getElementById("calcError");
        const card = document.getElementById("resultsCard");

        const enA = parseFloat(enAInput.value);
        const enB = parseFloat(enBInput.value);

        if (!isFinite(enA) || !isFinite(enB)) {
          errorEl.classList.remove("hidden");
          card.classList.add("hidden");
          return;
        }
        errorEl.classList.add("hidden");

        const delta = Math.abs(enA - enB);
        const { type, badgeText, badgeClass } = classifyBond(delta);
        const ionicPct = estimateIonicCharacter(delta);

        const nameA = selA.value || "A";
        const nameB = selB.value || "B";

        document.getElementById("resNameA").textContent = nameA;
        document.getElementById("resNameB").textContent = nameB;
        document.getElementById("resDelta").textContent = delta.toFixed(2);
        document.getElementById("resBondType").textContent = type;
        const badge = document.getElementById("resBondBadge");
        badge.textContent = badgeText;
        badge.className = "badge ml-2 " + badgeClass;
        document.getElementById("resIonicChar").textContent =
          ionicPct.toFixed(1) + "%";

        let moreEN = "";
        let moreENVal = "";
        if (enA > enB) {
          moreEN = nameA;
          moreENVal = enA.toFixed(2);
        } else if (enB > enA) {
          moreEN = nameB;
          moreENVal = enB.toFixed(2);
        } else {
          moreEN = "Neither (equal electronegativity)";
          moreENVal = enA.toFixed(2);
        }
        document.getElementById("resMoreEN").textContent = moreEN;
        document.getElementById("resMoreENVal").textContent = moreENVal;

        const note = document.getElementById("resPolarityNote");
        if (delta < 0.4) {
          note.textContent =
            "The bond is essentially nonpolar. Any dipole moment will be very small.";
        } else if (delta < 1.7) {
          note.textContent =
            "The bond is polar covalent. The more electronegative atom will carry a partial negative charge (δ−).";
        } else {
          note.textContent =
            "The bond has strong ionic character. In many cases, the more electronegative atom effectively gains electron density, forming an ionic bond.";
        }

        card.classList.remove("hidden");
      }

      function setupTabs() {
        const tabExpl = document.getElementById("tab-explainer");
        const tabTable = document.getElementById("tab-table");
        const panelExpl = document.getElementById("panel-explainer");
        const panelTable = document.getElementById("panel-table");

        if (!tabExpl || !tabTable) return;

        tabExpl.addEventListener("click", () => {
          tabExpl.classList.add("border-blue-600", "text-blue-600");
          tabExpl.classList.remove("text-gray-500", "border-transparent");
          tabTable.classList.remove("border-blue-600", "text-blue-600");
          tabTable.classList.add("text-gray-500");
          panelExpl.classList.remove("hidden");
          panelTable.classList.add("hidden");
        });

        tabTable.addEventListener("click", () => {
          tabTable.classList.add("border-blue-600", "text-blue-600");
          tabTable.classList.remove("text-gray-500", "border-transparent");
          tabExpl.classList.remove("border-blue-600", "text-blue-600");
          tabExpl.classList.add("text-gray-500");
          panelTable.classList.remove("hidden");
          panelExpl.classList.add("hidden");
        });
      }

      document.addEventListener("DOMContentLoaded", () => {
        populateElementSelects();
        buildENTable();
        setupTabs();

        document
          .getElementById("elementA")
          .addEventListener("change", () => syncENInputsFromSelect("A"));
        document
          .getElementById("elementB")
          .addEventListener("change", () => syncENInputsFromSelect("B"));

        document
          .getElementById("calcBtn")
          .addEventListener("click", calculateEN);
        document.getElementById("swapBtn").addEventListener("click", () => {
          const selA = document.getElementById("elementA");
          const selB = document.getElementById("elementB");
          const tmp = selA.value;
          selA.value = selB.value;
          selB.value = tmp;
          syncENInputsFromSelect("A");
          syncENInputsFromSelect("B");
          calculateEN();
        });
        document.getElementById("resetBtn").addEventListener("click", () => {
          document.getElementById("elementA").value = "H";
          document.getElementById("elementB").value = "Cl";
          syncENInputsFromSelect("A");
          syncENInputsFromSelect("B");
          document.getElementById("resultsCard").classList.add("hidden");
          document.getElementById("calcError").classList.add("hidden");
        });

        document
          .getElementById("tableFilter")
          .addEventListener("input", filterENTable);
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
