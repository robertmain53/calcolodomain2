    <script>
      // Utility: degrees <-> radians
      function degToRad(deg) {
        return (deg * Math.PI) / 180;
      }
      function radToDeg(rad) {
        return (rad * 180) / Math.PI;
      }

      // Unit conversions to meters
      function lambdaToMeters(value, unit) {
        const v = Number(value);
        if (!isFinite(v)) return NaN;
        switch (unit) {
          case "nm":
            return v * 1e-9;
          case "um":
            return v * 1e-6;
          case "m":
            return v;
          default:
            return NaN;
        }
      }

      function metersToLambda(m, unit) {
        switch (unit) {
          case "nm":
            return m * 1e9;
          case "um":
            return m * 1e6;
          case "m":
            return m;
          default:
            return NaN;
        }
      }

      function dToMeters(value, unit) {
        const v = Number(value);
        if (!isFinite(v)) return NaN;
        switch (unit) {
          case "nm":
            return v * 1e-9;
          case "um":
            return v * 1e-6;
          case "m":
            return v;
          default:
            return NaN;
        }
      }

      function metersToD(m, unit) {
        switch (unit) {
          case "nm":
            return m * 1e9;
          case "um":
            return m * 1e6;
          case "m":
            return m;
          default:
            return NaN;
        }
      }

      function NToMeters(value, unit) {
        const v = Number(value);
        if (!isFinite(v) || v <= 0) return NaN;
        // N is lines per unit length; d = 1/N
        switch (unit) {
          case "lines_per_mm":
            return 1 / (v * 1e3); // mm^-1 -> m
          case "lines_per_cm":
            return 1 / (v * 1e2); // cm^-1 -> m
          case "lines_per_m":
            return 1 / v; // m^-1
          default:
            return NaN;
        }
      }

      function metersToN(d_m, unit) {
        if (!(d_m > 0)) return NaN;
        switch (unit) {
          case "lines_per_mm":
            return 1 / (d_m * 1e3);
          case "lines_per_cm":
            return 1 / (d_m * 1e2);
          case "lines_per_m":
            return 1 / d_m;
          default:
            return NaN;
        }
      }

      function syncDAndN(from) {
        const dInput = document.getElementById("d-input");
        const dUnit = document.getElementById("d-unit");
        const NInput = document.getElementById("N-input");
        const NUnit = document.getElementById("N-unit");

        if (from === "d") {
          const d_m = dToMeters(dInput.value, dUnit.value);
          if (isFinite(d_m) && d_m > 0) {
            const N_val = metersToN(d_m, NUnit.value);
            if (isFinite(N_val))
              NInput.value = (Math.round(N_val * 1e6) / 1e6).toString();
          }
        } else if (from === "N") {
          const d_m = NToMeters(NInput.value, NUnit.value);
          if (isFinite(d_m) && d_m > 0) {
            const d_val = metersToD(d_m, dUnit.value);
            if (isFinite(d_val))
              dInput.value = (Math.round(d_val * 1e6) / 1e6).toString();
          }
        }
      }

      function updateGeometryDefaults() {
        const geometry = document.getElementById("geometry").value;
        const thetaIInput = document.getElementById("theta-i-input");
        const thetaIGroup = document.getElementById("theta-i-group");

        if (geometry === "normal") {
          thetaIInput.value = "0";
          thetaIInput.disabled = true;
          thetaIGroup.classList.add("opacity-60");
        } else if (geometry === "littrow") {
          thetaIInput.disabled = true;
          thetaIGroup.classList.add("opacity-60");
        } else {
          thetaIInput.disabled = false;
          thetaIGroup.classList.remove("opacity-60");
        }
      }

      function calculate() {
        const mode = document.getElementById("mode").value;
        const geometry = document.getElementById("geometry").value;
        const statusMsg = document.getElementById("status-msg");
        const resultsCard = document.getElementById("results-card");
        const resultsContent = document.getElementById("results-content");

        statusMsg.textContent = "";
        resultsCard.classList.add("hidden");
        resultsContent.innerHTML = "";

        const lambdaVal = document.getElementById("lambda-input").value;
        const lambdaUnit = document.getElementById("lambda-unit").value;
        const dVal = document.getElementById("d-input").value;
        const dUnit = document.getElementById("d-unit").value;
        const NVal = document.getElementById("N-input").value;
        const NUnit = document.getElementById("N-unit").value;
        const mVal = document.getElementById("m-input").value;
        const thetaIVal = document.getElementById("theta-i-input").value;
        const thetaMVal = document.getElementById("theta-m-input").value;

        // Determine d in meters (prefer N if provided)
        let d_m = NaN;
        if (NVal !== "") {
          d_m = NToMeters(NVal, NUnit);
        } else if (dVal !== "") {
          d_m = dToMeters(dVal, dUnit);
        }

        // Lambda in meters
        let lambda_m = NaN;
        if (lambdaVal !== "") {
          lambda_m = lambdaToMeters(lambdaVal, lambdaUnit);
        }

        // Order m
        let m = NaN;
        if (mVal !== "") {
          m = Number(mVal);
        }

        // Angles in radians
        let theta_i = NaN;
        let theta_m = NaN;
        if (thetaIVal !== "") theta_i = degToRad(Number(thetaIVal));
        if (thetaMVal !== "") theta_m = degToRad(Number(thetaMVal));

        // Geometry constraints
        if (geometry === "normal") {
          theta_i = 0;
        } else if (geometry === "littrow") {
          // θi = θm; will enforce after solving depending on mode
        }

        try {
          let solvedValue;
          let sinTerm;
          const eps = 1e-12;

          if (mode === "theta_m") {
            if (
              !isFinite(lambda_m) ||
              !isFinite(d_m) ||
              !isFinite(m) ||
              !isFinite(theta_i)
            ) {
              throw new Error(
                "Please enter wavelength, grating spacing or line density, order, and incidence angle."
              );
            }
            if (Math.abs(m) < eps)
              throw new Error("Order m cannot be zero when solving for θₘ.");

            if (geometry === "littrow") {
              // m λ = 2 d sin θL  => sin θL = m λ / (2 d)
              sinTerm = (m * lambda_m) / (2 * d_m);
            } else {
              // General: m λ = d (sin θm + sin θi)
              sinTerm = (m * lambda_m) / d_m - Math.sin(theta_i);
            }

            if (Math.abs(sinTerm) > 1 + 1e-9) {
              throw new Error(
                "No real solution: |sin θₘ| > 1. This order is not physically allowed for the given parameters."
              );
            }
            sinTerm = Math.max(-1, Math.min(1, sinTerm));
            theta_m = Math.asin(sinTerm);

            if (geometry === "littrow") {
              theta_i = theta_m;
            }

            solvedValue = radToDeg(theta_m);
            const lambdaDisplay = metersToLambda(lambda_m, lambdaUnit);
            const dDisplay = metersToD(d_m, dUnit);
            const NDisplay = metersToN(d_m, NUnit);

            resultsContent.innerHTML = `
          <p><span class="font-semibold">Diffraction angle θ<sub>m</sub>:</span>
             <span class="result-badge">${solvedValue.toFixed(4)}°</span></p>
          <p><span class="font-semibold">Input wavelength λ:</span>
             ${lambdaDisplay.toPrecision(6)} ${lambdaUnit}</p>
          <p><span class="font-semibold">Grating spacing d:</span>
             ${dDisplay.toPrecision(6)} ${dUnit}</p>
          <p><span class="font-semibold">Line density N:</span>
             ${NDisplay.toPrecision(6)} ${NUnit.replace("_", " ")}</p>
          <p><span class="font-semibold">Order m:</span> ${m}</p>
          <p><span class="font-semibold">Incidence angle θ<sub>i</sub>:</span>
             ${radToDeg(theta_i).toFixed(4)}°</p>
        `;
          } else if (mode === "lambda") {
            if (
              !isFinite(d_m) ||
              !isFinite(m) ||
              !isFinite(theta_i) ||
              !isFinite(theta_m)
            ) {
              throw new Error(
                "Please enter grating spacing or line density, order, incidence angle, and diffraction angle."
              );
            }
            if (Math.abs(m) < eps)
              throw new Error("Order m cannot be zero when solving for λ.");

            if (geometry === "littrow") {
              // θi = θm
              theta_i = theta_m;
              lambda_m = (2 * d_m * Math.sin(theta_m)) / m;
            } else {
              lambda_m = (d_m * (Math.sin(theta_m) + Math.sin(theta_i))) / m;
            }

            const lambdaDisplay = metersToLambda(lambda_m, lambdaUnit);
            const dDisplay = metersToD(d_m, dUnit);
            const NDisplay = metersToN(d_m, NUnit);

            resultsContent.innerHTML = `
          <p><span class="font-semibold">Wavelength λ:</span>
             <span class="result-badge">${lambdaDisplay.toPrecision(
               6
             )} ${lambdaUnit}</span></p>
          <p><span class="font-semibold">Grating spacing d:</span>
             ${dDisplay.toPrecision(6)} ${dUnit}</p>
          <p><span class="font-semibold">Line density N:</span>
             ${NDisplay.toPrecision(6)} ${NUnit.replace("_", " ")}</p>
          <p><span class="font-semibold">Order m:</span> ${m}</p>
          <p><span class="font-semibold">Incidence angle θ<sub>i</sub>:</span>
             ${radToDeg(theta_i).toFixed(4)}°</p>
          <p><span class="font-semibold">Diffraction angle θ<sub>m</sub>:</span>
             ${radToDeg(theta_m).toFixed(4)}°</p>
        `;
          } else if (mode === "d") {
            if (
              !isFinite(lambda_m) ||
              !isFinite(m) ||
              !isFinite(theta_i) ||
              !isFinite(theta_m)
            ) {
              throw new Error(
                "Please enter wavelength, order, incidence angle, and diffraction angle."
              );
            }
            if (Math.abs(m) < eps)
              throw new Error("Order m cannot be zero when solving for d.");

            if (geometry === "littrow") {
              theta_i = theta_m;
              d_m = (m * lambda_m) / (2 * Math.sin(theta_m));
            } else {
              const denom = Math.sin(theta_m) + Math.sin(theta_i);
              if (Math.abs(denom) < eps)
                throw new Error(
                  "Denominator is zero: sin θₘ + sin θᵢ = 0. Cannot solve for d."
                );
              d_m = (m * lambda_m) / denom;
            }

            const dDisplay = metersToD(d_m, dUnit);
            const NDisplay = metersToN(d_m, NUnit);
            const lambdaDisplay = metersToLambda(lambda_m, lambdaUnit);

            resultsContent.innerHTML = `
          <p><span class="font-semibold">Grating spacing d:</span>
             <span class="result-badge">${dDisplay.toPrecision(
               6
             )} ${dUnit}</span></p>
          <p><span class="font-semibold">Line density N:</span>
             ${NDisplay.toPrecision(6)} ${NUnit.replace("_", " ")}</p>
          <p><span class="font-semibold">Input wavelength λ:</span>
             ${lambdaDisplay.toPrecision(6)} ${lambdaUnit}</p>
          <p><span class="font-semibold">Order m:</span> ${m}</p>
          <p><span class="font-semibold">Incidence angle θ<sub>i</sub>:</span>
             ${radToDeg(theta_i).toFixed(4)}°</p>
          <p><span class="font-semibold">Diffraction angle θ<sub>m</sub>:</span>
             ${radToDeg(theta_m).toFixed(4)}°</p>
        `;
          } else if (mode === "N") {
            if (
              !isFinite(lambda_m) ||
              !isFinite(m) ||
              !isFinite(theta_i) ||
              !isFinite(theta_m)
            ) {
              throw new Error(
                "Please enter wavelength, order, incidence angle, and diffraction angle."
              );
            }
            if (Math.abs(m) < eps)
              throw new Error("Order m cannot be zero when solving for N.");

            if (geometry === "littrow") {
              theta_i = theta_m;
              d_m = (m * lambda_m) / (2 * Math.sin(theta_m));
            } else {
              const denom = Math.sin(theta_m) + Math.sin(theta_i);
              if (Math.abs(denom) < eps)
                throw new Error(
                  "Denominator is zero: sin θₘ + sin θᵢ = 0. Cannot solve for N."
                );
              d_m = (m * lambda_m) / denom;
            }

            const NDisplay = metersToN(d_m, NUnit);
            const dDisplay = metersToD(d_m, dUnit);
            const lambdaDisplay = metersToLambda(lambda_m, lambdaUnit);

            resultsContent.innerHTML = `
          <p><span class="font-semibold">Line density N:</span>
             <span class="result-badge">${NDisplay.toPrecision(
               6
             )} ${NUnit.replace("_", " ")}</span></p>
          <p><span class="font-semibold">Grating spacing d:</span>
             ${dDisplay.toPrecision(6)} ${dUnit}</p>
          <p><span class="font-semibold">Input wavelength λ:</span>
             ${lambdaDisplay.toPrecision(6)} ${lambdaUnit}</p>
          <p><span class="font-semibold">Order m:</span> ${m}</p>
          <p><span class="font-semibold">Incidence angle θ<sub>i</sub>:</span>
             ${radToDeg(theta_i).toFixed(4)}°</p>
          <p><span class="font-semibold">Diffraction angle θ<sub>m</sub>:</span>
             ${radToDeg(theta_m).toFixed(4)}°</p>
        `;
          } else if (mode === "m") {
            if (
              !isFinite(lambda_m) ||
              !isFinite(d_m) ||
              !isFinite(theta_i) ||
              !isFinite(theta_m)
            ) {
              throw new Error(
                "Please enter wavelength, grating spacing or line density, incidence angle, and diffraction angle."
              );
            }

            if (geometry === "littrow") {
              theta_i = theta_m;
              const denom = lambda_m;
              if (Math.abs(denom) < eps)
                throw new Error("Wavelength cannot be zero.");
              m = (2 * d_m * Math.sin(theta_m)) / denom;
            } else {
              const denom = lambda_m;
              if (Math.abs(denom) < eps)
                throw new Error("Wavelength cannot be zero.");
              m = (d_m * (Math.sin(theta_m) + Math.sin(theta_i))) / denom;
            }

            const mRounded = Math.round(m);
            const isClose = Math.abs(m - mRounded) < 1e-3;

            const lambdaDisplay = metersToLambda(lambda_m, lambdaUnit);
            const dDisplay = metersToD(d_m, dUnit);
            const NDisplay = metersToN(d_m, NUnit);

            resultsContent.innerHTML = `
          <p><span class="font-semibold">Computed order m:</span>
             <span class="result-badge">${m.toFixed(4)}</span></p>
          <p><span class="font-semibold">Nearest integer order:</span>
             ${mRounded} ${
              isClose
                ? "(physically plausible)"
                : "(warning: not close to an integer)"
            }
          </p>
          <p><span class="font-semibold">Input wavelength λ:</span>
             ${lambdaDisplay.toPrecision(6)} ${lambdaUnit}</p>
          <p><span class="font-semibold">Grating spacing d:</span>
             ${dDisplay.toPrecision(6)} ${dUnit}</p>
          <p><span class="font-semibold">Line density N:</span>
             ${NDisplay.toPrecision(6)} ${NUnit.replace("_", " ")}</p>
          <p><span class="font-semibold">Incidence angle θ<sub>i</sub>:</span>
             ${radToDeg(theta_i).toFixed(4)}°</p>
          <p><span class="font-semibold">Diffraction angle θ<sub>m</sub>:</span>
             ${radToDeg(theta_m).toFixed(4)}°</p>
        `;
          }

          resultsCard.classList.remove("hidden");
        } catch (err) {
          statusMsg.textContent =
            err.message || "Unable to compute. Please check your inputs.";
        }
      }

      function resetCalculator() {
        document.getElementById("mode").value = "theta_m";
        document.getElementById("geometry").value = "transmission";
        document.getElementById("lambda-input").value = "";
        document.getElementById("lambda-unit").value = "nm";
        document.getElementById("d-input").value = "";
        document.getElementById("d-unit").value = "um";
        document.getElementById("N-input").value = "";
        document.getElementById("N-unit").value = "lines_per_mm";
        document.getElementById("m-input").value = "1";
        document.getElementById("theta-i-input").value = "0";
        document.getElementById("theta-m-input").value = "";
        document.getElementById("status-msg").textContent = "";
        document.getElementById("results-card").classList.add("hidden");
        document.getElementById("results-content").innerHTML = "";
        updateGeometryDefaults();
      }

      document.addEventListener("DOMContentLoaded", function () {
        document
          .getElementById("calc-btn")
          .addEventListener("click", calculate);
        document
          .getElementById("reset-btn")
          .addEventListener("click", resetCalculator);
        document
          .getElementById("geometry")
          .addEventListener("change", updateGeometryDefaults);

        document
          .getElementById("d-input")
          .addEventListener("input", () => syncDAndN("d"));
        document
          .getElementById("d-unit")
          .addEventListener("change", () => syncDAndN("d"));
        document
          .getElementById("N-input")
          .addEventListener("input", () => syncDAndN("N"));
        document
          .getElementById("N-unit")
          .addEventListener("change", () => syncDAndN("N"));

        updateGeometryDefaults();
        document.getElementById("m-input").value = "1";
      });

      // Mobile menu script (simple)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          this.setAttribute("aria-expanded", isHidden ? "true" : "false");
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
