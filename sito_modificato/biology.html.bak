    <script>
      // Biology explorer logic
      (function () {
        const modeEl = document.getElementById("bio-mode");
        const knownEl = document.getElementById("bio-known");
        const N0El = document.getElementById("bio-N0");
        const NEl = document.getElementById("bio-N");
        const tEl = document.getElementById("bio-t");
        const tUnitEl = document.getElementById("bio-t-unit");
        const kEl = document.getElementById("bio-k");
        const kUnitEl = document.getElementById("bio-k-unit");
        const calcBtn = document.getElementById("bio-calc-btn");
        const resetBtn = document.getElementById("bio-reset-btn");
        const errorEl = document.getElementById("bio-error");
        const resultsBox = document.getElementById("bio-results");
        const solvedEl = document.getElementById("bio-solved");
        const solvedExtraEl = document.getElementById("bio-solved-extra");
        const dtHlEl = document.getElementById("bio-dt-hl");

        function timeToHours(value, unit) {
          const v = parseFloat(value);
          if (isNaN(v)) return NaN;
          switch (unit) {
            case "minutes":
              return v / 60;
            case "days":
              return v * 24;
            default:
              return v; // hours
          }
        }

        function rateToPerHour(value, unit) {
          const v = parseFloat(value);
          if (isNaN(v)) return NaN;
          switch (unit) {
            case "per_minute":
              return v * 60;
            case "per_day":
              return v / 24;
            default:
              return v; // per hour
          }
        }

        function perHourToUnit(value, unit) {
          switch (unit) {
            case "per_minute":
              return value / 60;
            case "per_day":
              return value * 24;
            default:
              return value;
          }
        }

        function hoursToUnit(value, unit) {
          switch (unit) {
            case "minutes":
              return value * 60;
            case "days":
              return value / 24;
            default:
              return value;
          }
        }

        function updateVisibleInputs() {
          const known = knownEl.value;
          document
            .querySelectorAll("#bio-inputs [data-field]")
            .forEach((el) => {
              const field = el.getAttribute("data-field");
              el.classList.remove("opacity-100", "opacity-60");
              if (field === known) {
                el.classList.add("opacity-60");
              } else {
                el.classList.add("opacity-100");
              }
            });
        }

        knownEl.addEventListener("change", updateVisibleInputs);
        updateVisibleInputs();

        resetBtn.addEventListener("click", function () {
          modeEl.value = "growth";
          knownEl.value = "N";
          N0El.value = 1e6;
          NEl.value = 8e6;
          tEl.value = 6;
          tUnitEl.value = "hours";
          kEl.value = "";
          kUnitEl.value = "per_hour";
          errorEl.textContent = "";
          resultsBox.classList.add("hidden");
          updateVisibleInputs();
        });

        calcBtn.addEventListener("click", function () {
          errorEl.textContent = "";
          resultsBox.classList.add("hidden");

          const mode = modeEl.value; // growth or decay
          const sign = mode === "growth" ? 1 : -1;
          const known = knownEl.value;

          const N0 = parseFloat(N0El.value);
          const N = parseFloat(NEl.value);
          const tHours = timeToHours(tEl.value, tUnitEl.value);
          const kPerHour = rateToPerHour(kEl.value, kUnitEl.value);

          if (!isFinite(N0) || N0 <= 0) {
            errorEl.textContent =
              "Please enter a valid positive initial amount N₀.";
            return;
          }

          let solvedValue, kUsedPerHour, tUsedHours;

          try {
            if (known === "N") {
              // solve for N(t)
              if (
                !isFinite(tHours) ||
                tHours < 0 ||
                !isFinite(kPerHour) ||
                kPerHour <= 0
              ) {
                errorEl.textContent =
                  "Please enter valid positive values for time and rate constant.";
                return;
              }
              kUsedPerHour = kPerHour;
              tUsedHours = tHours;
              const exponent = sign * kUsedPerHour * tUsedHours;
              solvedValue = N0 * Math.exp(exponent);
              solvedEl.textContent = `N(t) ≈ ${solvedValue.toExponential(3)}`;
              solvedExtraEl.textContent = `From N₀ = ${N0.toExponential(
                3
              )}, k = ${kUsedPerHour.toFixed(4)} h⁻¹, t = ${tUsedHours.toFixed(
                2
              )} h.`;
            } else if (known === "t") {
              // solve for t
              if (
                !isFinite(N) ||
                N <= 0 ||
                !isFinite(kPerHour) ||
                kPerHour <= 0
              ) {
                errorEl.textContent =
                  "Please enter valid positive values for N(t) and rate constant.";
                return;
              }
              kUsedPerHour = kPerHour;
              const ratio = N / N0;
              if (
                (mode === "growth" && ratio <= 0) ||
                (mode === "decay" && ratio <= 0)
              ) {
                errorEl.textContent = "N(t) and N₀ must be positive.";
                return;
              }
              const lnRatio = Math.log(ratio);
              const tHoursSolved = lnRatio / (sign * kUsedPerHour);
              if (!isFinite(tHoursSolved) || tHoursSolved < 0) {
                errorEl.textContent =
                  "The combination of N₀, N(t), and k is not consistent with the selected mode.";
                return;
              }
              solvedValue = hoursToUnit(tHoursSolved, tUnitEl.value);
              tUsedHours = tHoursSolved;
              solvedEl.textContent = `t ≈ ${solvedValue.toFixed(3)} ${
                tUnitEl.value
              }`;
              solvedExtraEl.textContent = `Equivalent to ${tHoursSolved.toFixed(
                3
              )} hours for N₀ = ${N0.toExponential(
                3
              )} → N(t) = ${N.toExponential(3)}.`;
            } else if (known === "k") {
              // solve for k
              if (!isFinite(N) || N <= 0 || !isFinite(tHours) || tHours <= 0) {
                errorEl.textContent =
                  "Please enter valid positive values for N(t) and time.";
                return;
              }
              const ratio = N / N0;
              const lnRatio = Math.log(ratio);
              const kSolvedPerHour = lnRatio / (sign * tHours);
              if (!isFinite(kSolvedPerHour) || kSolvedPerHour <= 0) {
                errorEl.textContent =
                  "The combination of N₀, N(t), and t is not consistent with the selected mode.";
                return;
              }
              solvedValue = perHourToUnit(kSolvedPerHour, kUnitEl.value);
              kUsedPerHour = kSolvedPerHour;
              tUsedHours = tHours;
              solvedEl.textContent = `k ≈ ${solvedValue.toFixed(
                4
              )} ${kUnitEl.value.replace("_", " ").replace("per ", "")}⁻¹`;
              solvedExtraEl.textContent = `Equivalent to ${kSolvedPerHour.toFixed(
                4
              )} h⁻¹ from N₀ = ${N0.toExponential(
                3
              )} → N(t) = ${N.toExponential(3)} in ${tHours.toFixed(2)} h.`;
            } else if (known === "doubling") {
              // solve for doubling time / half-life from k
              if (!isFinite(kPerHour) || kPerHour <= 0) {
                errorEl.textContent =
                  "Please enter a valid positive rate constant k.";
                return;
              }
              kUsedPerHour = kPerHour;
              const tHalfHours = Math.log(2) / kUsedPerHour;
              const label = mode === "growth" ? "Doubling time" : "Half‑life";
              solvedValue = hoursToUnit(tHalfHours, tUnitEl.value);
              solvedEl.textContent = `${label} ≈ ${solvedValue.toFixed(3)} ${
                tUnitEl.value
              }`;
              solvedExtraEl.textContent = `Computed from k = ${kUsedPerHour.toFixed(
                4
              )} h⁻¹ using t₁/₂ = ln(2)/k.`;
            }

            // Always compute doubling time / half-life if we have a valid k
            if (!kUsedPerHour || !isFinite(kUsedPerHour) || kUsedPerHour <= 0) {
              dtHlEl.textContent =
                "Not available – enter or solve for a positive k.";
            } else {
              const tHalfHours = Math.log(2) / kUsedPerHour;
              const label = mode === "growth" ? "Doubling time" : "Half‑life";
              const tHalfInUnit = hoursToUnit(tHalfHours, tUnitEl.value);
              dtHlEl.textContent = `${label}: ${tHalfInUnit.toFixed(3)} ${
                tUnitEl.value
              } (≈ ${tHalfHours.toFixed(3)} h)`;
            }

            resultsBox.classList.remove("hidden");
          } catch (e) {
            console.error(e);
            errorEl.textContent =
              "Unable to compute result. Please check your inputs.";
          }
        });
      })();

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
