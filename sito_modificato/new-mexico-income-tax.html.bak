    <script>
      // Simplified illustrative brackets for New Mexico.
      // NOTE: These are example values for demonstration and may not match official current law.
      const nmBrackets = {
        2024: {
          single: [
            { upTo: 5500, rate: 0.017 },
            { upTo: 11000, rate: 0.032 },
            { upTo: 16000, rate: 0.047 },
            { upTo: 210000, rate: 0.049 },
            { upTo: Infinity, rate: 0.059 },
          ],
          mfj: [
            { upTo: 8000, rate: 0.017 },
            { upTo: 16000, rate: 0.032 },
            { upTo: 24000, rate: 0.047 },
            { upTo: 315000, rate: 0.049 },
            { upTo: Infinity, rate: 0.059 },
          ],
          mfs: [
            { upTo: 4000, rate: 0.017 },
            { upTo: 8000, rate: 0.032 },
            { upTo: 12000, rate: 0.047 },
            { upTo: 157500, rate: 0.049 },
            { upTo: Infinity, rate: 0.059 },
          ],
          hoh: [
            { upTo: 8000, rate: 0.017 },
            { upTo: 16000, rate: 0.032 },
            { upTo: 24000, rate: 0.047 },
            { upTo: 210000, rate: 0.049 },
            { upTo: Infinity, rate: 0.059 },
          ],
        },
        2023: {
          single: [
            { upTo: 5500, rate: 0.017 },
            { upTo: 11000, rate: 0.032 },
            { upTo: 16000, rate: 0.047 },
            { upTo: 210000, rate: 0.049 },
            { upTo: Infinity, rate: 0.059 },
          ],
          mfj: [
            { upTo: 8000, rate: 0.017 },
            { upTo: 16000, rate: 0.032 },
            { upTo: 24000, rate: 0.047 },
            { upTo: 315000, rate: 0.049 },
            { upTo: Infinity, rate: 0.059 },
          ],
          mfs: [
            { upTo: 4000, rate: 0.017 },
            { upTo: 8000, rate: 0.032 },
            { upTo: 12000, rate: 0.047 },
            { upTo: 157500, rate: 0.049 },
            { upTo: Infinity, rate: 0.059 },
          ],
          hoh: [
            { upTo: 8000, rate: 0.017 },
            { upTo: 16000, rate: 0.032 },
            { upTo: 24000, rate: 0.047 },
            { upTo: 210000, rate: 0.049 },
            { upTo: Infinity, rate: 0.059 },
          ],
        },
      };

      // Simplified standard deductions (illustrative only)
      const nmStandardDeduction = {
        2024: { single: 14000, mfj: 28000, mfs: 14000, hoh: 21000 },
        2023: { single: 13000, mfj: 26000, mfs: 13000, hoh: 19500 },
      };

      function formatCurrency(n) {
        if (!isFinite(n)) return "$0";
        return n.toLocaleString("en-US", {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 0,
        });
      }

      function formatPercent(n) {
        if (!isFinite(n)) return "0%";
        return (n * 100).toFixed(2).replace(/\.00$/, "") + "%";
      }

      function calcNmTax() {
        const year = document.getElementById("nm-tax-year").value;
        const status = document.getElementById("nm-filing-status").value;
        const income = Math.max(
          0,
          Number(document.getElementById("nm-income").value) || 0
        );
        const credits = Math.max(
          0,
          Number(document.getElementById("nm-credits").value) || 0
        );

        const deductionType =
          document.querySelector('input[name="nm-deduction-type"]:checked')
            ?.value || "standard";
        const stdDed = nmStandardDeduction[year]?.[status] ?? 0;
        let itemized = Math.max(
          0,
          Number(document.getElementById("nm-itemized").value) || 0
        );
        let usedDeduction = stdDed;

        if (deductionType === "itemized") {
          usedDeduction = Math.max(stdDed, itemized); // still use larger of the two
        }

        const taxableAfterDed = Math.max(0, income - usedDeduction);
        const brackets = nmBrackets[year]?.[status] || [];

        let remaining = taxableAfterDed;
        let lastLimit = 0;
        let grossTax = 0;
        const breakdownLines = [];

        for (const bracket of brackets) {
          if (remaining <= 0) break;
          const upper = bracket.upTo;
          const span = Math.max(
            0,
            Math.min(taxableAfterDed, upper) - lastLimit
          );
          if (span > 0) {
            const taxHere = span * bracket.rate;
            grossTax += taxHere;
            breakdownLines.push(
              `${formatCurrency(lastLimit)} – ${
                upper === Infinity ? "and above" : formatCurrency(upper)
              } @ ${formatPercent(bracket.rate)}: ${formatCurrency(taxHere)}`
            );
          }
          lastLimit = upper;
          remaining = taxableAfterDed - lastLimit;
        }

        const netTax = Math.max(0, grossTax - credits);
        const effRate = income > 0 ? netTax / income : 0;

        document.getElementById("nm-result-taxable").textContent =
          formatCurrency(taxableAfterDed);
        document.getElementById("nm-result-gross").textContent =
          formatCurrency(grossTax);
        document.getElementById("nm-result-credits").textContent =
          "-" + formatCurrency(credits);
        document.getElementById("nm-result-net").textContent =
          formatCurrency(netTax);
        document.getElementById("nm-result-effective").textContent =
          formatPercent(effRate);

        const breakdownDiv = document.getElementById("nm-bracket-breakdown");
        breakdownDiv.innerHTML = "";
        if (breakdownLines.length === 0) {
          breakdownDiv.textContent = "No taxable income after deductions.";
        } else {
          breakdownLines.forEach((line) => {
            const p = document.createElement("p");
            p.textContent = line;
            breakdownDiv.appendChild(p);
          });
        }
      }

      document.addEventListener("DOMContentLoaded", () => {
        const itemizedInput = document.getElementById("nm-itemized");
        document
          .querySelectorAll('input[name="nm-deduction-type"]')
          .forEach((radio) => {
            radio.addEventListener("change", () => {
              if (radio.value === "itemized" && radio.checked) {
                itemizedInput.disabled = false;
              } else if (radio.value === "standard" && radio.checked) {
                itemizedInput.disabled = true;
              }
              calcNmTax();
            });
          });

        [
          "nm-tax-year",
          "nm-filing-status",
          "nm-income",
          "nm-itemized",
          "nm-credits",
        ].forEach((id) => {
          const el = document.getElementById(id);
          if (el) el.addEventListener("input", () => calcNmTax());
          if (el && el.tagName === "SELECT")
            el.addEventListener("change", () => calcNmTax());
        });

        document
          .getElementById("nm-calc-btn")
          .addEventListener("click", (e) => {
            e.preventDefault();
            calcNmTax();
          });

        calcNmTax();
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
