    <script>
      // Core unit definitions: factor relative to base unit for each category
      const unitCategories = {
        length: {
          base: "m",
          units: {
            m: { name: "Meter (m)", factor: 1 },
            km: { name: "Kilometer (km)", factor: 1000 },
            cm: { name: "Centimeter (cm)", factor: 0.01 },
            mm: { name: "Millimeter (mm)", factor: 0.001 },
            in: { name: "Inch (in)", factor: 0.0254 },
            ft: { name: "Foot (ft)", factor: 0.3048 },
            yd: { name: "Yard (yd)", factor: 0.9144 },
            mi: { name: "Mile (mi)", factor: 1609.344 },
            nmi: { name: "Nautical mile (nmi)", factor: 1852 },
          },
        },
        weight: {
          base: "kg",
          units: {
            kg: { name: "Kilogram (kg)", factor: 1 },
            g: { name: "Gram (g)", factor: 0.001 },
            mg: { name: "Milligram (mg)", factor: 0.000001 },
            t: { name: "Metric ton (t)", factor: 1000 },
            lb: { name: "Pound (lb)", factor: 0.45359237 },
            oz: { name: "Ounce (oz)", factor: 0.028349523125 },
            st: { name: "Stone (st)", factor: 6.35029318 },
          },
        },
        volume: {
          base: "L",
          units: {
            L: { name: "Liter (L)", factor: 1 },
            mL: { name: "Milliliter (mL)", factor: 0.001 },
            m3: { name: "Cubic meter (m³)", factor: 1000 },
            gal: { name: "US gallon (gal)", factor: 3.785411784 },
            qt: { name: "US quart (qt)", factor: 0.946352946 },
            pt: { name: "US pint (pt)", factor: 0.473176473 },
            cup: { name: "US cup", factor: 0.24 },
            tbsp: { name: "Tablespoon (tbsp)", factor: 0.0147867648 },
            tsp: { name: "Teaspoon (tsp)", factor: 0.00492892159 },
          },
        },
        area: {
          base: "m2",
          units: {
            m2: { name: "Square meter (m²)", factor: 1 },
            km2: { name: "Square kilometer (km²)", factor: 1e6 },
            ft2: { name: "Square foot (ft²)", factor: 0.09290304 },
            yd2: { name: "Square yard (yd²)", factor: 0.83612736 },
            ac: { name: "Acre (ac)", factor: 4046.8564224 },
            ha: { name: "Hectare (ha)", factor: 10000 },
          },
        },
        temperature: {
          base: "C",
          units: {
            C: { name: "Celsius (°C)" },
            F: { name: "Fahrenheit (°F)" },
            K: { name: "Kelvin (K)" },
          },
        },
        speed: {
          base: "mps",
          units: {
            mps: { name: "Meters per second (m/s)", factor: 1 },
            kph: { name: "Kilometers per hour (km/h)", factor: 1000 / 3600 },
            mph: { name: "Miles per hour (mph)", factor: 1609.344 / 3600 },
            kt: { name: "Knot (kt)", factor: 1852 / 3600 },
          },
        },
        power: {
          base: "W",
          units: {
            W: { name: "Watt (W)", factor: 1 },
            kW: { name: "Kilowatt (kW)", factor: 1000 },
            MW: { name: "Megawatt (MW)", factor: 1e6 },
            hp: { name: "Horsepower (mechanical)", factor: 745.69987158227022 },
            hpM: { name: "Horsepower (metric)", factor: 735.49875 },
          },
        },
        energy: {
          base: "J",
          units: {
            J: { name: "Joule (J)", factor: 1 },
            kJ: { name: "Kilojoule (kJ)", factor: 1000 },
            Wh: { name: "Watt-hour (Wh)", factor: 3600 },
            kWh: { name: "Kilowatt-hour (kWh)", factor: 3.6e6 },
            cal: { name: "Calorie (cal)", factor: 4.184 },
            kcal: { name: "Kilocalorie (kcal)", factor: 4184 },
            BTU: { name: "BTU (IT)", factor: 1055.05585262 },
          },
        },
        pressure: {
          base: "Pa",
          units: {
            Pa: { name: "Pascal (Pa)", factor: 1 },
            kPa: { name: "Kilopascal (kPa)", factor: 1000 },
            bar: { name: "Bar", factor: 100000 },
            atm: { name: "Standard atmosphere (atm)", factor: 101325 },
            psi: {
              name: "Pound per square inch (psi)",
              factor: 6894.757293168,
            },
            mmHg: {
              name: "Millimeter of mercury (mmHg)",
              factor: 133.322387415,
            },
          },
        },
        time: {
          base: "s",
          units: {
            s: { name: "Second (s)", factor: 1 },
            min: { name: "Minute (min)", factor: 60 },
            h: { name: "Hour (h)", factor: 3600 },
            d: { name: "Day (d)", factor: 86400 },
            wk: { name: "Week (wk)", factor: 604800 },
            mo: { name: "Month (30 d)", factor: 2592000 },
            yr: { name: "Year (365 d)", factor: 31536000 },
          },
        },
        data: {
          base: "B",
          units: {
            b: { name: "Bit (b)", factor: 0.125 },
            B: { name: "Byte (B)", factor: 1 },
            kB: { name: "Kilobyte (kB, 10³)", factor: 1e3 },
            MB: { name: "Megabyte (MB, 10⁶)", factor: 1e6 },
            GB: { name: "Gigabyte (GB, 10⁹)", factor: 1e9 },
            TB: { name: "Terabyte (TB, 10¹²)", factor: 1e12 },
            KiB: { name: "Kibibyte (KiB, 2¹⁰)", factor: 1024 },
            MiB: { name: "Mebibyte (MiB, 2²⁰)", factor: 1024 ** 2 },
            GiB: { name: "Gibibyte (GiB, 2³⁰)", factor: 1024 ** 3 },
            TiB: { name: "Tebibyte (TiB, 2⁴⁰)", factor: 1024 ** 4 },
          },
        },
        currency: {
          base: "CUR",
          units: {
            USD: { name: "US Dollar (USD)" },
            EUR: { name: "Euro (EUR)" },
            GBP: { name: "British Pound (GBP)" },
            JPY: { name: "Japanese Yen (JPY)" },
            AUD: { name: "Australian Dollar (AUD)" },
            CAD: { name: "Canadian Dollar (CAD)" },
            CHF: { name: "Swiss Franc (CHF)" },
          },
        },
      };

      const categoryButtons = document.querySelectorAll(".category-pill");
      const fromUnitSelect = document.getElementById("from-unit");
      const toUnitSelect = document.getElementById("to-unit");
      const fromValueInput = document.getElementById("from-value");
      const toValueInput = document.getElementById("to-value");
      const swapButton = document.getElementById("swap-units");
      const clearButton = document.getElementById("clear-inputs");
      const resultText = document.getElementById("result-text");
      const resultFactor = document.getElementById("result-factor");
      const currencyOptions = document.getElementById("currency-options");
      const currencyRateInput = document.getElementById("currency-rate");
      const currencyRateLabel = document.getElementById("currency-rate-label");
      const pinCurrentButton = document.getElementById("pin-current");
      const favoritesContainer = document.getElementById("favorites-container");

      let currentCategory = "length";
      let lastEdited = "from"; // 'from' or 'to'
      let favorites = [];

      function populateUnits(category) {
        const cat = unitCategories[category];
        fromUnitSelect.innerHTML = "";
        toUnitSelect.innerHTML = "";
        const unitKeys = Object.keys(cat.units);

        unitKeys.forEach((key, index) => {
          const optFrom = document.createElement("option");
          optFrom.value = key;
          optFrom.textContent = cat.units[key].name;
          fromUnitSelect.appendChild(optFrom);

          const optTo = document.createElement("option");
          optTo.value = key;
          optTo.textContent = cat.units[key].name;
          toUnitSelect.appendChild(optTo);
        });

        // Sensible defaults per category
        if (category === "length") {
          fromUnitSelect.value = "m";
          toUnitSelect.value = "ft";
        } else if (category === "weight") {
          fromUnitSelect.value = "kg";
          toUnitSelect.value = "lb";
        } else if (category === "volume") {
          fromUnitSelect.value = "L";
          toUnitSelect.value = "gal";
        } else if (category === "area") {
          fromUnitSelect.value = "m2";
          toUnitSelect.value = "ft2";
        } else if (category === "temperature") {
          fromUnitSelect.value = "C";
          toUnitSelect.value = "F";
        } else if (category === "speed") {
          fromUnitSelect.value = "kph";
          toUnitSelect.value = "mph";
        } else if (category === "power") {
          fromUnitSelect.value = "kW";
          toUnitSelect.value = "hp";
        } else if (category === "energy") {
          fromUnitSelect.value = "kWh";
          toUnitSelect.value = "J";
        } else if (category === "pressure") {
          fromUnitSelect.value = "bar";
          toUnitSelect.value = "psi";
        } else if (category === "time") {
          fromUnitSelect.value = "h";
          toUnitSelect.value = "min";
        } else if (category === "data") {
          fromUnitSelect.value = "GB";
          toUnitSelect.value = "GiB";
        } else if (category === "currency") {
          fromUnitSelect.value = "USD";
          toUnitSelect.value = "EUR";
        }

        updateCurrencyUI();
        recalc();
      }

      function updateCurrencyUI() {
        if (currentCategory === "currency") {
          currencyOptions.classList.remove("hidden");
          updateCurrencyRateLabel();
        } else {
          currencyOptions.classList.add("hidden");
        }
      }

      function updateCurrencyRateLabel() {
        const fromCode = fromUnitSelect.value;
        const toCode = toUnitSelect.value;
        currencyRateLabel.textContent = `Rate meaning: 1 ${fromCode} = [rate] ${toCode}`;
      }

      function convertTemperature(value, fromUnit, toUnit) {
        let c;
        if (fromUnit === "C") {
          c = value;
        } else if (fromUnit === "F") {
          c = ((value - 32) * 5) / 9;
        } else if (fromUnit === "K") {
          c = value - 273.15;
        }

        if (toUnit === "C") return c;
        if (toUnit === "F") return (c * 9) / 5 + 32;
        if (toUnit === "K") return c + 273.15;
        return value;
      }

      function getLinearFactor(category, fromUnit, toUnit) {
        const cat = unitCategories[category];
        const from = cat.units[fromUnit];
        const to = cat.units[toUnit];
        if (!from || !to) return 1;
        return from.factor / to.factor;
      }

      function recalc() {
        const fromUnit = fromUnitSelect.value;
        const toUnit = toUnitSelect.value;
        let fromVal = parseFloat(fromValueInput.value);
        let toVal = parseFloat(toValueInput.value);

        if (currentCategory === "temperature") {
          if (lastEdited === "from") {
            if (isNaN(fromVal)) {
              toValueInput.value = "";
              updateResultDisplay(null);
              return;
            }
            const result = convertTemperature(fromVal, fromUnit, toUnit);
            toValueInput.value = result;
            updateResultDisplay(result / fromVal, true);
          } else {
            if (isNaN(toVal)) {
              fromValueInput.value = "";
              updateResultDisplay(null);
              return;
            }
            const result = convertTemperature(toVal, toUnit, fromUnit);
            fromValueInput.value = result;
            updateResultDisplay(toVal / result, true);
          }
          return;
        }

        if (currentCategory === "currency") {
          const rate = parseFloat(currencyRateInput.value);
          if (!rate || rate <= 0) {
            updateResultDisplay(null);
            return;
          }
          if (lastEdited === "from") {
            if (isNaN(fromVal)) {
              toValueInput.value = "";
              updateResultDisplay(null);
              return;
            }
            const result = fromVal * rate;
            toValueInput.value = result;
          } else {
            if (isNaN(toVal)) {
              fromValueInput.value = "";
              updateResultDisplay(null);
              return;
            }
            const result = toVal / rate;
            fromValueInput.value = result;
          }
          updateResultDisplay(rate, false, true);
          return;
        }

        const factor = getLinearFactor(currentCategory, fromUnit, toUnit);

        if (lastEdited === "from") {
          if (isNaN(fromVal)) {
            toValueInput.value = "";
            updateResultDisplay(null);
            return;
          }
          const result = fromVal * factor;
          toValueInput.value = result;
        } else {
          if (isNaN(toVal)) {
            fromValueInput.value = "";
            updateResultDisplay(null);
            return;
          }
          const result = toVal / factor;
          fromValueInput.value = result;
        }

        updateResultDisplay(factor);
      }

      function updateResultDisplay(factor, isTemp = false, isCurrency = false) {
        const fromUnit = fromUnitSelect.value;
        const toUnit = toUnitSelect.value;
        const fromVal = parseFloat(fromValueInput.value);

        if (factor === null || isNaN(factor) || !isFinite(factor)) {
          resultText.textContent = "Enter a valid value to see the result.";
          resultFactor.textContent = "";
          return;
        }

        const displayFactor = isTemp
          ? "non-linear (temperature offset applied)"
          : isCurrency
          ? `× ${factor}`
          : `× ${factor}`;

        if (!isNaN(fromVal)) {
          const toVal = parseFloat(toValueInput.value);
          resultText.textContent = `${fromVal} ${fromUnit} = ${toVal} ${toUnit}`;
        } else {
          resultText.textContent = `Conversion from ${fromUnit} to ${toUnit}`;
        }

        if (isTemp) {
          resultFactor.textContent =
            "Conversion uses temperature formulas with offsets (not a single constant factor).";
        } else if (isCurrency) {
          resultFactor.textContent = `Conversion factor (exchange rate): ${displayFactor}`;
        } else {
          resultFactor.textContent = `Conversion factor: ${displayFactor}`;
        }
      }

      // Favorites handling
      function renderFavorites() {
        favoritesContainer.innerHTML = "";
        if (favorites.length === 0) {
          const p = document.createElement("p");
          p.className = "text-xs text-gray-400";
          p.textContent =
            "No favorites yet. Pin a conversion pair you use often.";
          favoritesContainer.appendChild(p);
          return;
        }

        favorites.forEach((fav, index) => {
          const btn = document.createElement("button");
          btn.type = "button";
          btn.className =
            "px-2 py-1 rounded-full border border-gray-200 bg-gray-50 hover:bg-blue-50 text-gray-700 flex items-center space-x-1";
          btn.innerHTML = `<span>${fav.category}: ${fav.fromUnit} → ${fav.toUnit}</span>`;
          btn.addEventListener("click", () => {
            currentCategory = fav.category;
            categoryButtons.forEach((b) => {
              b.classList.toggle(
                "active",
                b.dataset.category === currentCategory
              );
            });
            populateUnits(currentCategory);
            fromUnitSelect.value = fav.fromUnit;
            toUnitSelect.value = fav.toUnit;
            if (currentCategory === "currency" && fav.rate) {
              currencyRateInput.value = fav.rate;
            }
            recalc();
          });
          favoritesContainer.appendChild(btn);
        });
      }

      function loadFavorites() {
        try {
          const stored = localStorage.getItem("universalConverterFavorites");
          if (stored) {
            favorites = JSON.parse(stored);
          }
        } catch (e) {
          favorites = [];
        }
        renderFavorites();
      }

      function saveFavorites() {
        try {
          localStorage.setItem(
            "universalConverterFavorites",
            JSON.stringify(favorites)
          );
        } catch (e) {
          // ignore
        }
      }

      // Event listeners
      categoryButtons.forEach((btn) => {
        btn.addEventListener("click", () => {
          currentCategory = btn.dataset.category;
          categoryButtons.forEach((b) => b.classList.remove("active"));
          btn.classList.add("active");
          populateUnits(currentCategory);
        });
      });

      fromUnitSelect.addEventListener("change", () => {
        if (currentCategory === "currency") updateCurrencyRateLabel();
        recalc();
      });
      toUnitSelect.addEventListener("change", () => {
        if (currentCategory === "currency") updateCurrencyRateLabel();
        recalc();
      });

      fromValueInput.addEventListener("input", () => {
        lastEdited = "from";
        recalc();
      });
      toValueInput.addEventListener("input", () => {
        lastEdited = "to";
        recalc();
      });

      swapButton.addEventListener("click", () => {
        const tmp = fromUnitSelect.value;
        fromUnitSelect.value = toUnitSelect.value;
        toUnitSelect.value = tmp;
        if (currentCategory === "currency") updateCurrencyRateLabel();
        recalc();
      });

      clearButton.addEventListener("click", () => {
        fromValueInput.value = "";
        toValueInput.value = "";
        resultText.textContent = "Enter a value to convert.";
        resultFactor.textContent = "";
      });

      currencyRateInput.addEventListener("input", () => {
        recalc();
      });

      pinCurrentButton.addEventListener("click", () => {
        const fav = {
          category: currentCategory,
          fromUnit: fromUnitSelect.value,
          toUnit: toUnitSelect.value,
        };
        if (currentCategory === "currency") {
          const rate = parseFloat(currencyRateInput.value);
          if (rate && rate > 0) fav.rate = rate;
        }
        const exists = favorites.some(
          (f) =>
            f.category === fav.category &&
            f.fromUnit === fav.fromUnit &&
            f.toUnit === fav.toUnit
        );
        if (!exists) {
          favorites.push(fav);
          saveFavorites();
          renderFavorites();
        }
      });

      // Initialize
      loadFavorites();
      populateUnits(currentCategory);
    </script>

