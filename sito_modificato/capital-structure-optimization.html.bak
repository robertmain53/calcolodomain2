    <script>
      // Scenario management and calculations
      const scenariosContainer = document.getElementById("scenariosContainer");
      const addScenarioBtn = document.getElementById("addScenarioBtn");
      const resetScenariosBtn = document.getElementById("resetScenariosBtn");
      const calculateBtn = document.getElementById("calculateBtn");

      const resultsSummary = document.getElementById("resultsSummary");
      const resultsWarning = document.getElementById("resultsWarning");
      const resultsTableWrapper = document.getElementById(
        "resultsTableWrapper"
      );
      const resultsTableBody = document.getElementById("resultsTableBody");
      const chartContainer = document.getElementById("chartContainer");
      const waccChartCanvas = document.getElementById("waccChart");

      let scenarioCount = 0;
      let waccChart = null;

      function createScenarioRow(name, dToV, rd, re) {
        scenarioCount++;
        const id = scenarioCount;

        const wrapper = document.createElement("div");
        wrapper.className = "border border-gray-200 rounded-md p-3 card-hover";
        wrapper.dataset.scenarioId = id;

        wrapper.innerHTML = `
      <div class="flex justify-between items-center mb-2">
        <div class="flex items-center gap-2">
          <span class="inline-flex items-center justify-center w-6 h-6 rounded-full bg-gray-100 text-xs font-semibold text-gray-700">${id}</span>
          <input type="text" class="scenario-name text-sm font-semibold border-b border-dashed border-gray-300 focus:outline-none focus:border-blue-500" value="${name}">
        </div>
        <button type="button" class="text-xs text-red-600 hover:underline remove-scenario">Remove</button>
      </div>
      <div class="grid md:grid-cols-3 gap-3">
        <div>
          <label class="block text-xs font-medium text-gray-700 mb-1">Debt / Value (%)</label>
          <input type="number" class="scenario-dv w-full border rounded-md px-2 py-1.5" value="${dToV}" min="0" max="90" step="1">
        </div>
        <div>
          <label class="block text-xs font-medium text-gray-700 mb-1">Cost of Debt r<sub>d</sub> (%)</label>
          <input type="number" class="scenario-rd w-full border rounded-md px-2 py-1.5" value="${rd}" min="0" max="30" step="0.1">
        </div>
        <div>
          <label class="block text-xs font-medium text-gray-700 mb-1">Cost of Equity r<sub>e</sub> (%)</label>
          <input type="number" class="scenario-re w-full border rounded-md px-2 py-1.5" value="${re}" min="0" max="40" step="0.1">
        </div>
      </div>
    `;

        wrapper
          .querySelector(".remove-scenario")
          .addEventListener("click", () => {
            wrapper.remove();
            if (!scenariosContainer.children.length) {
              initDefaultScenarios();
            }
          });

        scenariosContainer.appendChild(wrapper);
      }

      function initDefaultScenarios() {
        scenariosContainer.innerHTML = "";
        scenarioCount = 0;
        createScenarioRow("Conservative", 10, 4, 9.5);
        createScenarioRow("Balanced", 30, 5, 11);
        createScenarioRow("Aggressive", 50, 7, 14);
      }

      addScenarioBtn.addEventListener("click", () => {
        createScenarioRow("New Scenario", 40, 6, 12);
      });

      resetScenariosBtn.addEventListener("click", () => {
        initDefaultScenarios();
        resultsSummary.classList.add("hidden");
        resultsWarning.classList.add("hidden");
        resultsTableWrapper.classList.add("hidden");
        chartContainer.classList.add("hidden");
      });

      function parsePercentInput(value, fallback) {
        const v = parseFloat(value);
        return isFinite(v) ? v : fallback;
      }

      function calculate() {
        const taxRate =
          parsePercentInput(document.getElementById("taxRate").value, 25) / 100;
        const unleveredCostEquity =
          parsePercentInput(
            document.getElementById("unleveredCostEquity").value,
            10
          ) / 100;
        const enterpriseValue =
          parseFloat(document.getElementById("enterpriseValue").value) || 0;
        const coverageThresholdRaw = document.getElementById(
          "interestCoverageThreshold"
        ).value;
        const coverageThreshold =
          coverageThresholdRaw === "" ? null : parseFloat(coverageThresholdRaw);

        const scenarios = [];
        scenariosContainer
          .querySelectorAll("[data-scenario-id]")
          .forEach((el) => {
            const name =
              el.querySelector(".scenario-name").value.trim() ||
              `Scenario ${el.dataset.scenarioId}`;
            const dToV =
              parsePercentInput(el.querySelector(".scenario-dv").value, 0) /
              100;
            const rd =
              parsePercentInput(el.querySelector(".scenario-rd").value, 0) /
              100;
            const re =
              parsePercentInput(el.querySelector(".scenario-re").value, 0) /
              100;
            if (dToV < 0 || dToV > 0.9) return;
            scenarios.push({ name, dToV, rd, re });
          });

        if (!scenarios.length) {
          alert("Please define at least one valid scenario.");
          return;
        }

        // Infer baseline FCFF from EV and unlevered cost of equity
        let fcff = null;
        if (enterpriseValue > 0 && unleveredCostEquity > 0) {
          fcff = enterpriseValue * unleveredCostEquity;
        }

        const results = scenarios.map((s) => {
          const D_over_V = s.dToV;
          const E_over_V = 1 - D_over_V;
          const wacc = D_over_V * s.rd * (1 - taxRate) + E_over_V * s.re;

          let impliedEV = null;
          let interestCoverage = null;

          if (fcff && wacc > 0) {
            impliedEV = fcff / wacc;

            const D = impliedEV * D_over_V;
            const interest = D * s.rd;
            const ebit = fcff / (1 - taxRate); // FCFF â‰ˆ EBIT * (1 - T)
            interestCoverage = interest > 0 ? ebit / interest : null;
          }

          return {
            ...s,
            wacc,
            impliedEV,
            interestCoverage,
          };
        });

        // Find optimal scenario (lowest WACC)
        let optimal = results[0];
        results.forEach((r) => {
          if (r.wacc < optimal.wacc) {
            optimal = r;
          }
        });

        // Render summary
        resultsSummary.classList.remove("hidden");
        const optimalDToV = (optimal.dToV * 100).toFixed(1);
        const optimalWACC = (optimal.wacc * 100).toFixed(2);
        const optimalEV = optimal.impliedEV
          ? optimal.impliedEV.toLocaleString(undefined, {
              maximumFractionDigits: 0,
            })
          : "N/A";
        resultsSummary.innerHTML = `
      <strong>Optimal scenario (by lowest WACC):</strong> <span class="font-semibold">${optimal.name}</span><br>
      Debt / Value: <span class="font-mono">${optimalDToV}%</span>,
      WACC: <span class="font-mono">${optimalWACC}%</span>,
      Implied EV: <span class="font-mono">${optimalEV}</span>
    `;

        // Render warnings
        let riskyScenarios = [];
        if (coverageThreshold !== null) {
          riskyScenarios = results.filter(
            (r) =>
              r.interestCoverage !== null &&
              r.interestCoverage < coverageThreshold
          );
        }
        if (riskyScenarios.length) {
          resultsWarning.classList.remove("hidden");
          const names = riskyScenarios.map((r) => r.name).join(", ");
          resultsWarning.innerHTML = `
        <strong>Risk flag:</strong> The following scenarios fall below your minimum interest coverage of
        <span class="font-mono">${coverageThreshold.toFixed(
          1
        )}x</span>: ${names}.
        Consider reducing leverage or improving earnings in these cases.
      `;
        } else {
          resultsWarning.classList.add("hidden");
        }

        // Render table
        resultsTableWrapper.classList.remove("hidden");
        resultsTableBody.innerHTML = "";
        results.forEach((r) => {
          const tr = document.createElement("tr");
          const isOptimal = r === optimal;
          tr.className = isOptimal ? "bg-green-50" : "";
          tr.innerHTML = `
        <td class="px-3 py-2 border-b text-sm ${
          isOptimal ? "font-semibold text-green-800" : ""
        }">${r.name}${isOptimal ? " (optimal)" : ""}</td>
        <td class="px-3 py-2 border-b text-right text-sm">${(
          r.dToV * 100
        ).toFixed(1)}%</td>
        <td class="px-3 py-2 border-b text-right text-sm">${(
          r.rd * 100
        ).toFixed(2)}%</td>
        <td class="px-3 py-2 border-b text-right text-sm">${(
          r.re * 100
        ).toFixed(2)}%</td>
        <td class="px-3 py-2 border-b text-right text-sm">${(
          r.wacc * 100
        ).toFixed(2)}%</td>
        <td class="px-3 py-2 border-b text-right text-sm">${
          r.impliedEV
            ? r.impliedEV.toLocaleString(undefined, {
                maximumFractionDigits: 0,
              })
            : "N/A"
        }</td>
        <td class="px-3 py-2 border-b text-right text-sm">
          ${r.interestCoverage ? r.interestCoverage.toFixed(2) + "x" : "N/A"}
        </td>
      `;
          resultsTableBody.appendChild(tr);
        });

        // Render simple chart (no external library) using Canvas 2D
        chartContainer.classList.remove("hidden");
        const ctx = waccChartCanvas.getContext("2d");
        const width = (waccChartCanvas.width =
          waccChartCanvas.clientWidth || 600);
        const height = (waccChartCanvas.height = 260);

        ctx.clearRect(0, 0, width, height);

        const padding = { left: 50, right: 20, top: 20, bottom: 40 };
        const xs = results.map((r) => r.dToV * 100);
        const ys = results.map((r) => r.wacc * 100);
        const minX = Math.min(...xs);
        const maxX = Math.max(...xs);
        const minY = Math.min(...ys);
        const maxY = Math.max(...ys);

        const xMin = Math.max(0, Math.floor(minX / 10) * 10);
        const xMax = Math.min(90, Math.ceil(maxX / 10) * 10);
        const yMin = Math.floor(minY * 10) / 10;
        const yMax = Math.ceil(maxY * 10) / 10;

        function xScale(x) {
          return (
            padding.left +
            ((x - xMin) / (xMax - xMin || 1)) *
              (width - padding.left - padding.right)
          );
        }
        function yScale(y) {
          return (
            height -
            padding.bottom -
            ((y - yMin) / (yMax - yMin || 1)) *
              (height - padding.top - padding.bottom)
          );
        }

        // Axes
        ctx.strokeStyle = "#9CA3AF";
        ctx.lineWidth = 1;
        ctx.beginPath();
        ctx.moveTo(padding.left, padding.top);
        ctx.lineTo(padding.left, height - padding.bottom);
        ctx.lineTo(width - padding.right, height - padding.bottom);
        ctx.stroke();

        ctx.fillStyle = "#374151";
        ctx.font = "11px Inter, system-ui, sans-serif";
        ctx.textAlign = "center";
        ctx.fillText(
          "Debt / Value (%)",
          (width - padding.left - padding.right) / 2 + padding.left,
          height - 8
        );

        ctx.save();
        ctx.translate(
          12,
          (height - padding.top - padding.bottom) / 2 + padding.top
        );
        ctx.rotate(-Math.PI / 2);
        ctx.textAlign = "center";
        ctx.fillText("WACC (%)", 0, 0);
        ctx.restore();

        // X ticks
        ctx.fillStyle = "#6B7280";
        ctx.textAlign = "center";
        const xStep = Math.max(
          10,
          Math.round((xMax - xMin) / 5 / 10) * 10 || 10
        );
        for (let x = xMin; x <= xMax; x += xStep) {
          const px = xScale(x);
          ctx.beginPath();
          ctx.moveTo(px, height - padding.bottom);
          ctx.lineTo(px, height - padding.bottom + 4);
          ctx.stroke();
          ctx.fillText(x.toString(), px, height - padding.bottom + 16);
        }

        // Y ticks
        ctx.textAlign = "right";
        const yStep = (yMax - yMin) / 4 || 1;
        for (let i = 0; i <= 4; i++) {
          const y = yMin + i * yStep;
          const py = yScale(y);
          ctx.beginPath();
          ctx.moveTo(padding.left - 4, py);
          ctx.lineTo(padding.left, py);
          ctx.stroke();
          ctx.fillText(y.toFixed(1), padding.left - 6, py + 3);
        }

        // Line
        ctx.strokeStyle = "#2563EB";
        ctx.lineWidth = 2;
        ctx.beginPath();
        results
          .slice()
          .sort((a, b) => a.dToV - b.dToV)
          .forEach((r, idx) => {
            const px = xScale(r.dToV * 100);
            const py = yScale(r.wacc * 100);
            if (idx === 0) ctx.moveTo(px, py);
            else ctx.lineTo(px, py);
          });
        ctx.stroke();

        // Points
        results.forEach((r) => {
          const px = xScale(r.dToV * 100);
          const py = yScale(r.wacc * 100);
          const isOpt = r === optimal;
          ctx.beginPath();
          ctx.fillStyle = isOpt ? "#16A34A" : "#2563EB";
          ctx.arc(px, py, isOpt ? 5 : 4, 0, Math.PI * 2);
          ctx.fill();
        });
      }

      calculateBtn.addEventListener("click", calculate);

      // Initialize defaults on load
      initDefaultScenarios();
    </script>

    <script>
      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
