    <script>
      // Data model
      const initiatives = [];

      function createInitiative(quadrant) {
        return {
          id: Date.now().toString() + Math.random().toString(16).slice(2),
          quadrant,
          title: "",
          description: "",
          fit: 3,
          attractiveness: 3,
          difficulty: 3,
        };
      }

      function quadrantLabel(q) {
        switch (q) {
          case "penetration":
            return "Market penetration";
          case "product":
            return "Product development";
          case "market":
            return "Market development";
          case "diversification":
            return "Diversification";
          default:
            return "";
        }
      }

      function computeScore(item) {
        const fit = Number(item.fit) || 0;
        const attr = Number(item.attractiveness) || 0;
        const diff = Number(item.difficulty) || 0;
        return fit + attr - diff;
      }

      function riskClass(score) {
        if (score >= 7)
          return { cls: "risk-low", label: "Low risk / high potential" };
        if (score >= 4) return { cls: "risk-medium", label: "Moderate risk" };
        return { cls: "risk-high", label: "High risk / low clarity" };
      }

      function renderInitiatives() {
        const containers = {
          penetration: document.getElementById("list-penetration"),
          product: document.getElementById("list-product"),
          market: document.getElementById("list-market"),
          diversification: document.getElementById("list-diversification"),
        };
        Object.values(containers).forEach((c) => (c.innerHTML = ""));

        initiatives.forEach((item) => {
          const container = containers[item.quadrant];
          if (!container) return;
          const score = computeScore(item);
          const risk = riskClass(score);

          const wrapper = document.createElement("div");
          wrapper.className = `border border-gray-200 rounded-md p-2 text-xs card-hover ${risk.cls}`;
          wrapper.dataset.id = item.id;

          wrapper.innerHTML = `
        <div class="flex justify-between items-start mb-1">
          <input type="text" class="title-input w-2/3 border border-gray-300 rounded px-2 py-1 text-xs font-medium"
                 placeholder="Initiative title (e.g., Loyalty program)"
                 value="${item.title.replace(/"/g, "&quot;")}">
          <div class="flex items-center space-x-2">
            <span class="badge border border-gray-300 bg-white text-gray-700">${
              risk.label
            }</span>
            <button type="button" class="delete-btn text-red-600 hover:text-red-800" title="Remove initiative">&times;</button>
          </div>
        </div>
        <textarea class="desc-input w-full border border-gray-300 rounded px-2 py-1 text-xs mb-2"
                  rows="2" placeholder="Short description or key assumptions...">${
                    item.description
                  }</textarea>
        <div class="grid grid-cols-3 gap-2 items-center">
          <label class="flex flex-col">
            <span class="text-[11px] text-gray-700">Strategic fit</span>
            <input type="number" min="1" max="5" class="fit-input border border-gray-300 rounded px-1 py-0.5 text-xs" value="${
              item.fit
            }">
          </label>
          <label class="flex flex-col">
            <span class="text-[11px] text-gray-700">Market attractiveness</span>
            <input type="number" min="1" max="5" class="attr-input border border-gray-300 rounded px-1 py-0.5 text-xs" value="${
              item.attractiveness
            }">
          </label>
          <label class="flex flex-col">
            <span class="text-[11px] text-gray-700">Execution difficulty</span>
            <input type="number" min="1" max="5" class="diff-input border border-gray-300 rounded px-1 py-0.5 text-xs" value="${
              item.difficulty
            }">
          </label>
        </div>
        <div class="mt-1 text-[11px] text-gray-700">
          Composite score: <span class="font-semibold score-value">${score}</span>
        </div>
      `;

          // Attach listeners
          wrapper
            .querySelector(".title-input")
            .addEventListener("input", (e) => {
              item.title = e.target.value;
              updateSummaryAndRanking();
            });
          wrapper
            .querySelector(".desc-input")
            .addEventListener("input", (e) => {
              item.description = e.target.value;
              updateSummaryAndRanking();
            });
          wrapper.querySelector(".fit-input").addEventListener("input", (e) => {
            item.fit = clampScore(e.target.value);
            e.target.value = item.fit;
            updateCardScore(wrapper, item);
          });
          wrapper
            .querySelector(".attr-input")
            .addEventListener("input", (e) => {
              item.attractiveness = clampScore(e.target.value);
              e.target.value = item.attractiveness;
              updateCardScore(wrapper, item);
            });
          wrapper
            .querySelector(".diff-input")
            .addEventListener("input", (e) => {
              item.difficulty = clampScore(e.target.value);
              e.target.value = item.difficulty;
              updateCardScore(wrapper, item);
            });
          wrapper.querySelector(".delete-btn").addEventListener("click", () => {
            const idx = initiatives.findIndex((x) => x.id === item.id);
            if (idx >= 0) {
              initiatives.splice(idx, 1);
              renderInitiatives();
              updateSummaryAndRanking();
            }
          });

          container.appendChild(wrapper);
        });
      }

      function clampScore(val) {
        let n = parseInt(val, 10);
        if (isNaN(n)) n = 3;
        if (n < 1) n = 1;
        if (n > 5) n = 5;
        return n;
      }

      function updateCardScore(card, item) {
        const score = computeScore(item);
        const risk = riskClass(score);
        const scoreSpan = card.querySelector(".score-value");
        if (scoreSpan) scoreSpan.textContent = score;
        card.classList.remove("risk-low", "risk-medium", "risk-high");
        card.classList.add(risk.cls);
        const badge = card.querySelector(".badge");
        if (badge) badge.textContent = risk.label;
        updateSummaryAndRanking();
      }

      function updateSummaryAndRanking() {
        const prioritizedContainer =
          document.getElementById("prioritized-list");
        const summaryOutput = document.getElementById("summary-output");

        if (!initiatives.length) {
          prioritizedContainer.className =
            "border border-dashed border-gray-300 rounded-md p-3 text-xs text-gray-500";
          prioritizedContainer.textContent =
            "Add initiatives and scores above to see a ranked list here.";
          summaryOutput.value = "";
          return;
        }

        const sorted = [...initiatives].sort(
          (a, b) => computeScore(b) - computeScore(a)
        );
        const lines = [];
        const htmlLines = [];

        const company = (
          document.getElementById("company-name").value || ""
        ).trim();
        const horizon = (
          document.getElementById("planning-horizon").value || ""
        ).trim();
        const context = (
          document.getElementById("context-notes").value || ""
        ).trim();

        if (company || horizon) {
          lines.push(
            `Ansoff Matrix summary for ${company || "our organization"}${
              horizon ? " (" + horizon + ")" : ""
            }`
          );
          lines.push("");
        }

        if (context) {
          lines.push("Context:");
          lines.push(context);
          lines.push("");
        }

        htmlLines.push(
          '<ol class="list-decimal ml-4 space-y-1 text-xs text-gray-700">'
        );

        sorted.forEach((item, idx) => {
          const score = computeScore(item);
          const risk = riskClass(score);
          const title = item.title || "(Untitled initiative)";
          const quad = quadrantLabel(item.quadrant);
          const desc = item.description ? ` – ${item.description}` : "";

          lines.push(`${idx + 1}. [${quad}] ${title}`);
          lines.push(
            `   Score: ${score} (fit ${item.fit}, attractiveness ${item.attractiveness}, difficulty ${item.difficulty})`
          );
          lines.push(`   Risk: ${risk.label}`);
          if (item.description) lines.push(`   Notes: ${item.description}`);
          lines.push("");

          htmlLines.push(
            `<li><span class="font-semibold">[${quad}] ${title}</span> – score ${score} (fit ${item.fit}, attractiveness ${item.attractiveness}, difficulty ${item.difficulty})<br><span class="text-[11px] text-gray-500">${risk.label}${desc}</span></li>`
          );
        });

        htmlLines.push("</ol>");
        prioritizedContainer.className =
          "border border-gray-200 rounded-md p-3 text-xs bg-white";
        prioritizedContainer.innerHTML = htmlLines.join("\n");
        summaryOutput.value = lines.join("\n");
      }

      document.addEventListener("DOMContentLoaded", () => {
        document.querySelectorAll(".add-initiative-btn").forEach((btn) => {
          btn.addEventListener("click", () => {
            const quadrant = btn.getAttribute("data-quadrant");
            initiatives.push(createInitiative(quadrant));
            renderInitiatives();
            updateSummaryAndRanking();
          });
        });

        document
          .getElementById("recalculate-btn")
          .addEventListener("click", () => {
            updateSummaryAndRanking();
          });

        document
          .getElementById("copy-summary-btn")
          .addEventListener("click", async () => {
            const summary = document.getElementById("summary-output").value;
            const feedback = document.getElementById("copy-feedback");
            try {
              await navigator.clipboard.writeText(summary);
              feedback.classList.remove("hidden");
              setTimeout(() => feedback.classList.add("hidden"), 2000);
            } catch (e) {
              feedback.textContent =
                "Unable to copy automatically. Please select and copy manually.";
              feedback.classList.remove("hidden");
              setTimeout(() => feedback.classList.add("hidden"), 4000);
            }
          });
      });

      // Mobile menu script (simple toggle)
      document.addEventListener("DOMContentLoaded", () => {
        const toggle = document.getElementById("mobile-menu-toggle");
        const menu = document.getElementById("mobile-menu");
        if (toggle && menu) {
          toggle.addEventListener("click", () => {
            const isHidden = menu.classList.contains("hidden");
            menu.classList.toggle("hidden", !isHidden);
            toggle.setAttribute("aria-expanded", isHidden ? "true" : "false");
          });
        }
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
