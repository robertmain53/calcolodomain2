    <script>
      (function () {
        const hbar = 1.054571817e-34; // J*s
        const eV_to_J = 1.602176634e-19;
        const nm_to_m = 1e-9;

        const particleMasses = {
          electron: 9.1093837015e-31,
          proton: 1.67262192369e-27,
          alpha: 6.6446573357e-27,
        };

        const particleSelect = document.getElementById("particle-select");
        const customMassWrapper = document.getElementById(
          "custom-mass-wrapper"
        );
        const massInput = document.getElementById("mass-input");
        const energyInput = document.getElementById("energy-input");
        const barrierHeightInput = document.getElementById(
          "barrier-height-input"
        );
        const barrierWidthInput = document.getElementById(
          "barrier-width-input"
        );
        const showAdvancedToggle = document.getElementById(
          "show-advanced-toggle"
        );
        const advancedParams = document.getElementById("advanced-params");
        const calculateBtn = document.getElementById("calculate-btn");
        const resetBtn = document.getElementById("reset-btn");
        const errorMessage = document.getElementById("error-message");

        const transmissionOutput = document.getElementById(
          "transmission-output"
        );
        const reflectionOutput = document.getElementById("reflection-output");
        const wkbOutput = document.getElementById("wkb-output");
        const regimeOutput = document.getElementById("regime-output");
        const kappaAOutput = document.getElementById("kappa-a-output");

        function formatProb(x) {
          if (!isFinite(x) || isNaN(x)) return "–";
          if (x === 0) return "0";
          if (x === 1) return "1";
          if (x < 1e-6) return x.toExponential(2);
          return x.toPrecision(4);
        }

        function clearOutputs() {
          transmissionOutput.textContent = "–";
          reflectionOutput.textContent = "–";
          wkbOutput.textContent = "–";
          regimeOutput.textContent = "–";
          kappaAOutput.textContent = "–";
        }

        function showError(msg) {
          errorMessage.textContent = msg;
          errorMessage.classList.remove("hidden");
        }

        function clearError() {
          errorMessage.textContent = "";
          errorMessage.classList.add("hidden");
        }

        particleSelect.addEventListener("change", () => {
          if (particleSelect.value === "custom") {
            customMassWrapper.classList.remove("hidden");
          } else {
            customMassWrapper.classList.add("hidden");
          }
        });

        showAdvancedToggle.addEventListener("change", () => {
          if (showAdvancedToggle.checked) {
            advancedParams.classList.remove("hidden");
          } else {
            advancedParams.classList.add("hidden");
          }
        });

        resetBtn.addEventListener("click", () => {
          particleSelect.value = "electron";
          customMassWrapper.classList.add("hidden");
          massInput.value = "";
          energyInput.value = "";
          barrierHeightInput.value = "";
          barrierWidthInput.value = "";
          showAdvancedToggle.checked = false;
          advancedParams.classList.add("hidden");
          clearError();
          clearOutputs();
        });

        calculateBtn.addEventListener("click", () => {
          clearError();
          clearOutputs();

          let m;
          if (particleSelect.value === "custom") {
            m = parseFloat(massInput.value);
            if (!(m > 0)) {
              showError("Please enter a positive custom mass in kilograms.");
              return;
            }
          } else {
            m = particleMasses[particleSelect.value];
          }

          const E_eV = parseFloat(energyInput.value);
          const V0_eV = parseFloat(barrierHeightInput.value);
          const a_nm = parseFloat(barrierWidthInput.value);

          if (!(E_eV >= 0) || !(V0_eV > 0) || !(a_nm > 0)) {
            showError(
              "Please enter non-negative energy, positive barrier height, and positive barrier width."
            );
            return;
          }

          const E = E_eV * eV_to_J;
          const V0 = V0_eV * eV_to_J;
          const a = a_nm * nm_to_m;

          if (E === 0 && V0 === 0) {
            showError("Energy and barrier height cannot both be zero.");
            return;
          }

          let T_exact = 0;
          let T_wkb = 0;
          let regime = "";

          try {
            if (E < V0) {
              // Tunneling regime
              regime = "Tunneling (E < V₀)";
              const kappa = Math.sqrt(2 * m * (V0 - E)) / hbar;
              const kappa_a = kappa * a;

              const sinh_kappa_a = Math.sinh(kappa_a);
              const numerator = V0 * V0 * sinh_kappa_a * sinh_kappa_a;
              const denom = 4 * E * (V0 - E);
              const factor = denom !== 0 ? numerator / denom : Infinity;
              T_exact = 1 / (1 + factor);

              // WKB approximation
              T_wkb = Math.exp(-2 * kappa_a);

              kappaAOutput.textContent = isFinite(kappa_a)
                ? kappa_a.toPrecision(4)
                : "–";
            } else if (E > V0) {
              // Over-the-barrier regime
              regime = "Over-the-barrier (E > V₀)";
              const k2 = Math.sqrt(2 * m * (E - V0)) / hbar;
              const k2_a = k2 * a;

              const sin_k2_a = Math.sin(k2_a);
              const numerator = V0 * V0 * sin_k2_a * sin_k2_a;
              const denom = 4 * E * (E - V0);
              const factor = denom !== 0 ? numerator / denom : Infinity;
              T_exact = 1 / (1 + factor);

              // No tunneling in the strict sense; WKB not used here
              T_wkb = NaN;
              kappaAOutput.textContent = "–";
            } else {
              // E == V0
              regime = "Threshold (E = V₀)";
              // At threshold, the standard formulas are singular; use limiting behavior.
              // For a finite-width barrier, T tends to a non-zero value; we approximate as 0.5.
              T_exact = 0.5;
              T_wkb = NaN;
              kappaAOutput.textContent = "–";
            }
          } catch (e) {
            showError(
              "Numerical error while computing tunneling probability. Try smaller values or different units."
            );
            return;
          }

          // Clamp and compute reflection
          if (!isFinite(T_exact) || isNaN(T_exact)) {
            T_exact = 0;
          }
          if (T_exact < 0) T_exact = 0;
          if (T_exact > 1) T_exact = 1;

          const R = 1 - T_exact;

          transmissionOutput.textContent = formatProb(T_exact);
          reflectionOutput.textContent = formatProb(R);
          wkbOutput.textContent = isNaN(T_wkb) ? "–" : formatProb(T_wkb);
          regimeOutput.textContent = regime || "–";
        });
      })();
    </script>
  
