    <script>
      // Fuel conversion logic
      (function () {
        const lPer100Input = document.getElementById("lPer100Input");
        const kmPerLInput = document.getElementById("kmPerLInput");
        const lPer100Slider = document.getElementById("lPer100Slider");
        const kmPerLSlider = document.getElementById("kmPerLSlider");

        const summaryLPer100 = document.getElementById("summaryLPer100");
        const summaryKmPerL = document.getElementById("summaryKmPerL");
        const summaryFuel100 = document.getElementById("summaryFuel100");
        const summaryDistancePerL = document.getElementById(
          "summaryDistancePerL"
        );

        let isUpdating = false;

        function clamp(value, min, max) {
          return Math.min(Math.max(value, min), max);
        }

        function updateSummaries(lPer100, kmPerL) {
          const lVal = isFinite(lPer100) ? lPer100 : 0;
          const kVal = isFinite(kmPerL) ? kmPerL : 0;

          summaryLPer100.textContent = lVal.toFixed(2);
          summaryFuel100.textContent = lVal.toFixed(2);
          summaryKmPerL.textContent = kVal.toFixed(2);
          summaryDistancePerL.textContent = kVal.toFixed(2);
        }

        function fromLPer100() {
          if (isUpdating) return;
          isUpdating = true;
          let lVal = parseFloat(lPer100Input.value);
          if (!isFinite(lVal) || lVal <= 0) {
            isUpdating = false;
            return;
          }
          lVal = clamp(lVal, 0.1, 50);
          const kmVal = 100 / lVal;

          kmPerLInput.value = kmVal.toFixed(2);
          lPer100Slider.value = clamp(
            lVal,
            parseFloat(lPer100Slider.min),
            parseFloat(lPer100Slider.max)
          );
          kmPerLSlider.value = clamp(
            kmVal,
            parseFloat(kmPerLSlider.min),
            parseFloat(kmPerLSlider.max)
          );

          updateSummaries(lVal, kmVal);
          isUpdating = false;
        }

        function fromKmPerL() {
          if (isUpdating) return;
          isUpdating = true;
          let kmVal = parseFloat(kmPerLInput.value);
          if (!isFinite(kmVal) || kmVal <= 0) {
            isUpdating = false;
            return;
          }
          kmVal = clamp(kmVal, 0.1, 50);
          const lVal = 100 / kmVal;

          lPer100Input.value = lVal.toFixed(2);
          kmPerLSlider.value = clamp(
            kmVal,
            parseFloat(kmPerLSlider.min),
            parseFloat(kmPerLSlider.max)
          );
          lPer100Slider.value = clamp(
            lVal,
            parseFloat(lPer100Slider.min),
            parseFloat(lPer100Slider.max)
          );

          updateSummaries(lVal, kmVal);
          isUpdating = false;
        }

        // Input listeners
        lPer100Input.addEventListener("input", fromLPer100);
        kmPerLInput.addEventListener("input", fromKmPerL);

        // Slider listeners
        lPer100Slider.addEventListener("input", () => {
          lPer100Input.value = parseFloat(lPer100Slider.value).toFixed(2);
          fromLPer100();
        });

        kmPerLSlider.addEventListener("input", () => {
          kmPerLInput.value = parseFloat(kmPerLSlider.value).toFixed(2);
          fromKmPerL();
        });

        // Initialize summaries
        fromLPer100();
      })();
    </script>

