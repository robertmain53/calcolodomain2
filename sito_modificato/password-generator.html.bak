      function buildCharset() {
        const lower = document.getElementById("opt-lowercase").checked;
        const upper = document.getElementById("opt-uppercase").checked;
        const numbers = document.getElementById("opt-numbers").checked;
        const symbols = document.getElementById("opt-symbols").checked;
        const ambiguous = document.getElementById("opt-ambiguous").checked;

        let charset = "";
        if (lower) charset += LOWER;
        if (upper) charset += UPPER;
        if (numbers) charset += NUMBERS;
        if (symbols) charset += SYMBOLS;

        if (ambiguous) {
          charset = charset
            .split("")
            .filter((ch) => !AMBIGUOUS.includes(ch))
            .join("");
        }
        return charset;
      }

      function generateRandomPassword(length) {
        const pronounceable =
          document.getElementById("opt-pronounceable").checked;
        let charset = buildCharset();
        if (!charset) {
          // fallback: lowercase if user unchecked everything
          charset = LOWER;
        }
        if (!pronounceable) {
          let pwd = "";
          for (let i = 0; i < length; i++) {
            const idx = secureRandomInt(charset.length);
            pwd += charset[idx];
          }
          return pwd;
        } else {
          // Simple pronounceable pattern: alternating consonant/vowel with allowed chars
          const vowels = "aeiou";
          const consonants = "bcdfghjklmnpqrstvwxyz";
          let pwd = "";
          for (let i = 0; i < length; i++) {
            const useVowel = i % 2 === 1;
            let pool = useVowel ? vowels : consonants;
            // intersect with charset if possible
            const filtered = pool
              .split("")
              .filter((ch) => charset.includes(ch));
            const effectivePool = filtered.length ? filtered.join("") : charset;
            const idx = secureRandomInt(effectivePool.length);
            pwd += effectivePool[idx];
          }
          return pwd;
        }
      }

      function generatePassphrase(
        wordsCount,
        separator,
        capitalizeWords,
        addNumberSymbol
      ) {
        const words = [];
        for (let i = 0; i < wordsCount; i++) {
          const idx = secureRandomInt(WORD_LIST.length);
          let w = WORD_LIST[idx];
          if (capitalizeWords) {
            w = w.charAt(0).toUpperCase() + w.slice(1);
          }
          words.push(w);
        }
        let phrase = words.join(separator || "-");
        if (addNumberSymbol) {
          const num = NUMBERS[secureRandomInt(NUMBERS.length)];
          const sym = SYMBOLS[secureRandomInt(SYMBOLS.length)];
          phrase += separator || "-";
          phrase += num + sym;
        }
        return phrase;
      }

      function calculateEntropy(password, mode) {
        if (!password) return 0;
        if (mode === "passphrase") {
          // Approximate: log2(wordlistSize^words) = words * log2(wordlistSize)
          const wordsCount = (password.match(/[-\s_]/g) || []).length + 1;
          const wordlistSize = WORD_LIST.length;
          return wordsCount * Math.log2(wordlistSize);
        } else {
          const charset = buildCharset();
          const N = charset.length || 1;
          return password.length * Math.log2(N);
        }
      }

      function estimateCrackTime(entropyBits) {
        if (!entropyBits || entropyBits <= 0) return "–";
        // Assume 10^11 guesses per second (fast offline attack)
        const guessesPerSecond = 1e11;
        const totalGuesses = Math.pow(2, entropyBits);
        const seconds = totalGuesses / guessesPerSecond;

        if (seconds < 1) return "< 1 second";
        const minute = 60;
        const hour = 3600;
        const day = 86400;
        const year = 31557600;

        if (seconds < minute) return seconds.toFixed(1) + " seconds";
        if (seconds < hour) return (seconds / minute).toFixed(1) + " minutes";
        if (seconds < day) return (seconds / hour).toFixed(1) + " hours";
        if (seconds < year) return (seconds / day).toFixed(1) + " days";
        if (seconds < year * 1000)
          return (seconds / year).toFixed(1) + " years";
        return "> 1,000 years";
      }

      function updateStrengthUI(entropyBits) {
        const bar = document.getElementById("strength-bar-inner");
        const label = document.getElementById("strength-label");
        const entropyLabel = document.getElementById("entropy-label");
        const crackLabel = document.getElementById("crack-time-label");

        entropyLabel.textContent = entropyBits
          ? "Entropy: " + entropyBits.toFixed(1) + " bits"
          : "Entropy: – bits";

        let strengthText = "Very weak";
        let color = "bg-red-500";
        let percent = 10;

        if (entropyBits >= 100) {
          strengthText = "Excellent";
          color = "bg-emerald-500";
          percent = 100;
        } else if (entropyBits >= 80) {
          strengthText = "Very strong";
          color = "bg-green-500";
          percent = 85;
        } else if (entropyBits >= 60) {
          strengthText = "Strong";
          color = "bg-lime-500";
          percent = 70;
        } else if (entropyBits >= 40) {
          strengthText = "Fair";
          color = "bg-yellow-400";
          percent = 50;
        } else if (entropyBits >= 20) {
          strengthText = "Weak";
          color = "bg-orange-500";
          percent = 30;
        }

        bar.className = "h-full rounded-full " + color;
        bar.style.width = percent + "%";
        label.textContent = "Strength: " + strengthText;

        crackLabel.textContent =
          "Estimated offline crack time: " + estimateCrackTime(entropyBits);
      }

      function getMode() {
        return document
          .getElementById("mode-passphrase")
          .classList.contains("border-blue-600")
          ? "passphrase"
          : "password";
      }

      function generateAndDisplay() {
        const mode = getMode();
        let result;
        if (mode === "passphrase") {
          const words =
            parseInt(document.getElementById("words-count").value, 10) || 6;
          const sep = document.getElementById("separator-input").value || "-";
          const cap = document.getElementById("opt-capitalize-words").checked;
          const add = document.getElementById("opt-add-number-symbol").checked;
          result = generatePassphrase(words, sep, cap, add);
        } else {
          let length =
            parseInt(document.getElementById("length-input").value, 10) || 16;
          if (length < 4) length = 4;
          if (length > 128) length = 128;
          result = generateRandomPassword(length);
        }
        const output = document.getElementById("password-output");
        output.value = result;
        const entropy = calculateEntropy(result, mode);
        updateStrengthUI(entropy);
      }

      function generateBulk(count) {
        const mode = getMode();
        const lines = [];
        for (let i = 0; i < count; i++) {
          if (mode === "passphrase") {
            const words =
              parseInt(document.getElementById("words-count").value, 10) || 6;
            const sep = document.getElementById("separator-input").value || "-";
            const cap = document.getElementById("opt-capitalize-words").checked;
            const add = document.getElementById(
              "opt-add-number-symbol"
            ).checked;
            lines.push(generatePassphrase(words, sep, cap, add));
          } else {
            let length =
              parseInt(document.getElementById("length-input").value, 10) || 16;
            if (length < 4) length = 4;
            if (length > 128) length = 128;
            lines.push(generateRandomPassword(length));
          }
        }
        document.getElementById("bulk-output").value = lines.join("\n");
      }

      document.addEventListener("DOMContentLoaded", function () {
        const lengthInput = document.getElementById("length-input");
        const lengthRange = document.getElementById("length-range");
        const copyBtn = document.getElementById("copy-password-btn");
        const regenBtn = document.getElementById("regenerate-btn");
        const bulkBtn = document.getElementById("bulk-generate-btn");
        const clearBulkBtn = document.getElementById("clear-bulk-btn");
        const copyFeedback = document.getElementById("copy-feedback");

        // Sync length controls
        lengthInput.addEventListener("input", function () {
          let v = parseInt(lengthInput.value, 10);
          if (isNaN(v)) v = 16;
          if (v < 4) v = 4;
          if (v > 128) v = 128;
          lengthInput.value = v;
          if (v <= 64) lengthRange.value = v;
          generateAndDisplay();
        });
        lengthRange.addEventListener("input", function () {
          lengthInput.value = lengthRange.value;
          generateAndDisplay();
        });

        // Mode switching
        const modePasswordBtn = document.getElementById("mode-password");
        const modePassphraseBtn = document.getElementById("mode-passphrase");
        const charsetOptions = document.getElementById("charset-options");
        const passphraseOptions = document.getElementById("passphrase-options");

        function setMode(mode) {
          if (mode === "passphrase") {
            modePassphraseBtn.classList.add(
              "border-blue-600",
              "bg-blue-50",
              "text-blue-700"
            );
            modePasswordBtn.classList.remove(
              "border-blue-600",
              "bg-blue-50",
              "text-blue-700"
            );
            modePasswordBtn.classList.add("border-gray-300", "text-gray-700");
            modePassphraseBtn.classList.remove(
              "border-gray-300",
              "text-gray-700"
            );
            charsetOptions.classList.add("hidden");
            passphraseOptions.classList.remove("hidden");
          } else {
            modePasswordBtn.classList.add(
              "border-blue-600",
              "bg-blue-50",
              "text-blue-700"
            );
            modePassphraseBtn.classList.remove(
              "border-blue-600",
              "bg-blue-50",
              "text-blue-700"
            );
            modePassphraseBtn.classList.add("border-gray-300", "text-gray-700");
            modePasswordBtn.classList.remove(
              "border-gray-300",
              "text-gray-700"
            );
            charsetOptions.classList.remove("hidden");
            passphraseOptions.classList.add("hidden");
          }
          generateAndDisplay();
        }

        modePasswordBtn.addEventListener("click", function () {
          setMode("password");
        });
        modePassphraseBtn.addEventListener("click", function () {
          setMode("passphrase");
        });

        // Charset and options change triggers regeneration
        [
          "opt-lowercase",
          "opt-uppercase",
          "opt-numbers",
          "opt-symbols",
          "opt-ambiguous",
          "opt-pronounceable",
          "words-count",
          "separator-input",
          "opt-capitalize-words",
          "opt-add-number-symbol",
        ].forEach((id) => {
          const el = document.getElementById(id);
          if (el) {
            el.addEventListener("change", generateAndDisplay);
            el.addEventListener("input", generateAndDisplay);
          }
        });

        regenBtn.addEventListener("click", generateAndDisplay);

        copyBtn.addEventListener("click", function () {
          const output = document.getElementById("password-output");
          if (!output.value) return;
          navigator.clipboard
            .writeText(output.value)
            .then(() => {
              copyFeedback.classList.remove("hidden");
              setTimeout(() => copyFeedback.classList.add("hidden"), 1500);
            })
            .catch(() => {
              copyFeedback.textContent = "Copy failed. Please copy manually.";
              copyFeedback.classList.remove("hidden");
              setTimeout(() => {
                copyFeedback.classList.add("hidden");
                copyFeedback.textContent = "Copied to clipboard.";
              }, 2000);
            });
        });

