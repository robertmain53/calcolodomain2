    <script>
      (function () {
        const typeTabs = document.querySelectorAll(".qr-type-tab");
        const typePanels = document.querySelectorAll(".qr-type-panel");
        const statusEl = document.getElementById("qr-status");
        const canvas = document.getElementById("qr-canvas");
        const ctx = canvas.getContext("2d");
        const logoFileInput = document.getElementById("qr-logo-file");
        const removeLogoBtn = document.getElementById("qr-remove-logo");
        const logoPreview = document.getElementById("qr-logo-preview");
        let currentType = "url";
        let logoImage = null;
        let lastSvg = "";

        function setStatus(msg, isError = false) {
          statusEl.textContent = msg || "";
          statusEl.className =
            "text-xs " + (isError ? "text-red-600" : "text-gray-500");
        }

        // Tab switching
        typeTabs.forEach((tab) => {
          tab.addEventListener("click", () => {
            const type = tab.getAttribute("data-type");
            currentType = type;
            typeTabs.forEach((t) =>
              t.classList.remove("tab-active", "tab-inactive")
            );
            typeTabs.forEach((t) => t.classList.add("tab-inactive"));
            tab.classList.remove("tab-inactive");
            tab.classList.add("tab-active");
            typePanels.forEach((panel) => {
              panel.classList.toggle(
                "hidden",
                panel.getAttribute("data-type-panel") !== type
              );
            });
            generateQR();
          });
        });

        // Color sync helpers
        function syncColorInputs(colorInput, hexInput) {
          colorInput.addEventListener("input", () => {
            hexInput.value = colorInput.value.toLowerCase();
            generateQR();
          });
          hexInput.addEventListener("change", () => {
            let val = hexInput.value.trim();
            if (!val.startsWith("#")) val = "#" + val;
            if (/^#([0-9a-fA-F]{3}|[0-9a-fA-F]{6})$/.test(val)) {
              colorInput.value = val;
              hexInput.value = val.toLowerCase();
              generateQR();
            } else {
              hexInput.value = colorInput.value.toLowerCase();
            }
          });
        }

        syncColorInputs(
          document.getElementById("qr-foreground"),
          document.getElementById("qr-foreground-hex")
        );
        syncColorInputs(
          document.getElementById("qr-background"),
          document.getElementById("qr-background-hex")
        );

        // Build content string based on type
        function buildContent() {
          switch (currentType) {
            case "url": {
              const url = document.getElementById("qr-url").value.trim();
              if (!url) throw new Error("Please enter a URL.");
              return url;
            }
            case "text": {
              const text = document.getElementById("qr-text").value;
              if (!text.trim()) throw new Error("Please enter some text.");
              return text;
            }
            case "wifi": {
              const ssid = document.getElementById("qr-wifi-ssid").value;
              const password =
                document.getElementById("qr-wifi-password").value;
              const enc = document.getElementById("qr-wifi-encryption").value;
              const hidden = document.getElementById("qr-wifi-hidden").checked;
              if (!ssid.trim())
                throw new Error("Please enter the Wi‑Fi network name (SSID).");
              // Escape special chars ; , : " \
              const esc = (s) => s.replace(/([\\;,:"])/g, "\\$1");
              let wifi = `WIFI:T:${enc};S:${esc(ssid)};`;
              if (enc !== "nopass") wifi += `P:${esc(password)};`;
              if (hidden) wifi += "H:true;";
              wifi += ";";
              return wifi;
            }
            case "email": {
              const to = document.getElementById("qr-email-to").value.trim();
              const subject = document.getElementById("qr-email-subject").value;
              const body = document.getElementById("qr-email-body").value;
              if (!to)
                throw new Error("Please enter a recipient email address.");
              const params = new URLSearchParams();
              if (subject) params.append("subject", subject);
              if (body) params.append("body", body);
              const query = params.toString();
              return query ? `mailto:${to}?${query}` : `mailto:${to}`;
            }
            case "vcard": {
              const first = document
                .getElementById("qr-vcard-first")
                .value.trim();
              const last = document
                .getElementById("qr-vcard-last")
                .value.trim();
              const org = document.getElementById("qr-vcard-org").value.trim();
              const title = document
                .getElementById("qr-vcard-title")
                .value.trim();
              const phone = document
                .getElementById("qr-vcard-phone")
                .value.trim();
              const email = document
                .getElementById("qr-vcard-email")
                .value.trim();
              const website = document
                .getElementById("qr-vcard-website")
                .value.trim();
              const address = document
                .getElementById("qr-vcard-address")
                .value.trim();
              if (!first && !last)
                throw new Error("Please enter at least a first or last name.");
              let v = "BEGIN:VCARD\nVERSION:3.0\n";
              v += `N:${last};${first};;;\n`;
              v += `FN:${[first, last].filter(Boolean).join(" ")}\n`;
              if (org) v += `ORG:${org}\n`;
              if (title) v += `TITLE:${title}\n`;
              if (phone) v += `TEL;TYPE=CELL,VOICE:${phone}\n`;
              if (email) v += `EMAIL;TYPE=INTERNET:${email}\n`;
              if (website) v += `URL:${website}\n`;
              if (address)
                v += `ADR;TYPE=WORK:;;${address.replace(/\n/g, "\\n")};;;;\n`;
              v += "END:VCARD";
              return v;
            }
            case "sms": {
              const phone = document
                .getElementById("qr-sms-phone")
                .value.trim();
              const msg = document.getElementById("qr-sms-message").value;
              if (!phone) throw new Error("Please enter a phone number.");
              const params = new URLSearchParams();
              if (msg) params.append("body", msg);
              const query = params.toString();
              return query ? `SMSTO:${phone}:${msg}` : `SMSTO:${phone}`;
            }
            default:
              throw new Error("Unsupported QR type.");
          }
        }

        function hexToRgb(hex) {
          hex = hex.replace("#", "");
          if (hex.length === 3) {
            hex = hex
              .split("")
              .map((c) => c + c)
              .join("");
          }
          const num = parseInt(hex, 16);
          return { r: (num >> 16) & 255, g: (num >> 8) & 255, b: num & 255 };
        }

        function generateQR() {
          try {
            const text = buildContent();
            const ecLevel =
              document.getElementById("qr-error-correction").value || "M";
            const size =
              parseInt(document.getElementById("qr-size").value, 10) || 512;
            const fg =
              document.getElementById("qr-foreground").value || "#000000";
            const bg =
              document.getElementById("qr-background").value || "#ffffff";
            const quiet = document.getElementById("qr-quiet-zone").checked;

            setStatus("Generating…");

            const qr = new QRCodeGenerator(0, ecLevel);
            qr.addData(text);
            qr.make();
            const count = qr.getModuleCount();

            const marginModules = quiet ? 4 : 0;
            const totalModules = count + marginModules * 2;
            const cellSize = Math.floor(size / totalModules);
            const actualSize = cellSize * totalModules;

            canvas.width = actualSize;
            canvas.height = actualSize;

            const fgRgb = hexToRgb(fg);
            const bgRgb = hexToRgb(bg);

            ctx.fillStyle = `rgb(${bgRgb.r},${bgRgb.g},${bgRgb.b})`;
            ctx.fillRect(0, 0, actualSize, actualSize);

            for (let r = 0; r < count; r++) {
              for (let c = 0; c < count; c++) {
                if (qr.isDark(r, c)) {
                  const x = (c + marginModules) * cellSize;
                  const y = (r + marginModules) * cellSize;
                  ctx.fillStyle = `rgb(${fgRgb.r},${fgRgb.g},${fgRgb.b})`;
                  ctx.fillRect(x, y, cellSize, cellSize);
                }
              }
            }

            // Draw logo if present
            if (logoImage) {
              const logoScale = 0.22; // 22% of QR width
              const logoSize = actualSize * logoScale;
              const x = (actualSize - logoSize) / 2;
              const y = (actualSize - logoSize) / 2;
              ctx.save();
              ctx.beginPath();
              const radius = logoSize * 0.18;
              ctx.moveTo(x + radius, y);
              ctx.lineTo(x + logoSize - radius, y);
              ctx.quadraticCurveTo(x + logoSize, y, x + logoSize, y + radius);
              ctx.lineTo(x + logoSize, y + logoSize - radius);
              ctx.quadraticCurveTo(
                x + logoSize,
                y + logoSize,
                x + logoSize - radius,
                y + logoSize
              );
              ctx.lineTo(x + radius, y + logoSize);
              ctx.quadraticCurveTo(x, y + logoSize, x, y + logoSize - radius);
              ctx.lineTo(x, y + radius);
              ctx.quadraticCurveTo(x, y, x + radius, y);
              ctx.closePath();
              ctx.fillStyle = `rgb(${bgRgb.r},${bgRgb.g},${bgRgb.b})`;
              ctx.fill();
              ctx.clip();
              ctx.drawImage(logoImage, x, y, logoSize, logoSize);
              ctx.restore();
            }

            // Build SVG string for download
            let svg = `<svg xmlns="http://www.w3.org/2000/svg" width="${actualSize}" height="${actualSize}" viewBox="0 0 ${actualSize} ${actualSize}">`;
            svg += `<rect width="100%" height="100%" fill="${bg}"/>`;
            const rects = [];
            for (let r = 0; r < count; r++) {
              for (let c = 0; c < count; c++) {
                if (qr.isDark(r, c)) {
                  const x = (c + marginModules) * cellSize;
                  const y = (r + marginModules) * cellSize;
                  rects.push(
                    `<rect x="${x}" y="${y}" width="${cellSize}" height="${cellSize}" fill="${fg}"/>`
                  );
                }
              }
            }
            svg += rects.join("");
            // Note: logo overlay is not embedded into SVG for simplicity
            svg += "</svg>";
            lastSvg = svg;

            setStatus("QR code ready. Download as PNG or SVG.");
          } catch (err) {
            console.error(err);
            setStatus(err.message || "Could not generate QR code.", true);
          }
        }

        // Logo upload handling
        logoFileInput.addEventListener("change", () => {
          const file = logoFileInput.files && logoFileInput.files[0];
          if (!file) {
            logoImage = null;
            removeLogoBtn.classList.add("hidden");
            generateQR();
            return;
          }
          if (!file.type.startsWith("image/")) {
            setStatus("Please select a valid image file for the logo.", true);
            logoFileInput.value = "";
            return;
          }
          const reader = new FileReader();
          reader.onload = (e) => {
            const img = new Image();
            img.onload = () => {
              logoImage = img;
              removeLogoBtn.classList.remove("hidden");
              generateQR();
            };
            img.onerror = () => {
              setStatus("Could not load the logo image.", true);
            };
            img.src = e.target.result;
          };
          reader.readAsDataURL(file);
        });

        removeLogoBtn.addEventListener("click", () => {
          logoImage = null;
          logoFileInput.value = "";
          removeLogoBtn.classList.add("hidden");
          generateQR();
        });

        // Generate button
        document
          .getElementById("qr-generate")
          .addEventListener("click", generateQR);

        // Download PNG
        document
          .getElementById("qr-download-png")
          .addEventListener("click", () => {
            try {
              const link = document.createElement("a");
              link.download = "qr-code.png";
              link.href = canvas.toDataURL("image/png");
              link.click();
            } catch (err) {
              console.error(err);
              setStatus("Could not download PNG.", true);
            }
          });

        // Download SVG
        document
          .getElementById("qr-download-svg")
          .addEventListener("click", () => {
            try {
              if (!lastSvg) generateQR();
              const blob = new Blob([lastSvg], {
                type: "image/svg+xml;charset=utf-8",
              });
              const url = URL.createObjectURL(blob);
              const link = document.createElement("a");
              link.download = "qr-code.svg";
              link.href = url;
              link.click();
              setTimeout(() => URL.revokeObjectURL(url), 1000);
            } catch (err) {
              console.error(err);
              setStatus("Could not download SVG.", true);
            }
          });

        // Auto-generate initial QR
        window.addEventListener("load", generateQR);
      })();
    </script>

