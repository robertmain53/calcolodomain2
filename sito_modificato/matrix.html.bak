    <script>
      // Utility to create matrix grids
      function buildMatrixGrid(containerId, rows, cols) {
        const container = document.getElementById(containerId);
        container.innerHTML = "";
        container.style.gridTemplateColumns = `repeat(${cols}, minmax(2.5rem, 1fr))`;
        for (let i = 0; i < rows * cols; i++) {
          const input = document.createElement("input");
          input.type = "number";
          input.step = "any";
          input.className =
            "w-full border rounded px-1 py-1 text-xs text-center";
          input.value = "0";
          container.appendChild(input);
        }
      }

      function getMatrix(containerId, rows, cols) {
        const container = document.getElementById(containerId);
        const inputs = container.querySelectorAll("input");
        const M = [];
        let idx = 0;
        for (let i = 0; i < rows; i++) {
          const row = [];
          for (let j = 0; j < cols; j++) {
            const v = parseFloat(inputs[idx].value);
            row.push(isNaN(v) ? 0 : v);
            idx++;
          }
          M.push(row);
        }
        return M;
      }

      function setMatrix(containerId, M) {
        const rows = M.length;
        const cols = M[0].length;
        buildMatrixGrid(containerId, rows, cols);
        const container = document.getElementById(containerId);
        const inputs = container.querySelectorAll("input");
        let idx = 0;
        for (let i = 0; i < rows; i++) {
          for (let j = 0; j < cols; j++) {
            inputs[idx].value = M[i][j];
            idx++;
          }
        }
      }

      function showError(msg) {
        const el = document.getElementById("matrixError");
        el.textContent = msg;
        el.classList.remove("hidden");
      }

      function clearError() {
        const el = document.getElementById("matrixError");
        el.textContent = "";
        el.classList.add("hidden");
      }

      function formatMatrix(M) {
        const rows = M.length;
        const cols = M[0].length;
        let html = '<table class="border-collapse text-xs"><tbody>';
        for (let i = 0; i < rows; i++) {
          html += "<tr>";
          for (let j = 0; j < cols; j++) {
            const v = Math.abs(M[i][j]) < 1e-12 ? 0 : M[i][j];
            html += `<td class="border px-2 py-1 text-right">${Number(
              v.toFixed(6)
            )}</td>`;
          }
          html += "</tr>";
        }
        html += "</tbody></table>";
        return html;
      }

      function formatVector(v) {
        let html = '<table class="border-collapse text-xs"><tbody>';
        for (let i = 0; i < v.length; i++) {
          const val = Math.abs(v[i]) < 1e-12 ? 0 : v[i];
          html += `<tr><td class="border px-2 py-1 text-right">${Number(
            val.toFixed(6)
          )}</td></tr>`;
        }
        html += "</tbody></table>";
        return html;
      }

      function addMatrices(A, B) {
        const m = A.length,
          n = A[0].length;
        const C = Array.from({ length: m }, () => Array(n).fill(0));
        for (let i = 0; i < m; i++) {
          for (let j = 0; j < n; j++) {
            C[i][j] = A[i][j] + B[i][j];
          }
        }
        return C;
      }

      function subMatrices(A, B) {
        const m = A.length,
          n = A[0].length;
        const C = Array.from({ length: m }, () => Array(n).fill(0));
        for (let i = 0; i < m; i++) {
          for (let j = 0; j < n; j++) {
            C[i][j] = A[i][j] - B[i][j];
          }
        }
        return C;
      }

      function mulMatrices(A, B) {
        const m = A.length,
          n = A[0].length,
          p = B[0].length;
        const C = Array.from({ length: m }, () => Array(p).fill(0));
        for (let i = 0; i < m; i++) {
          for (let k = 0; k < n; k++) {
            for (let j = 0; j < p; j++) {
              C[i][j] += A[i][k] * B[k][j];
            }
          }
        }
        return C;
      }

      function transpose(A) {
        const m = A.length,
          n = A[0].length;
        const T = Array.from({ length: n }, () => Array(m).fill(0));
        for (let i = 0; i < m; i++) {
          for (let j = 0; j < n; j++) {
            T[j][i] = A[i][j];
          }
        }
        return T;
      }

      function determinant(A) {
        const n = A.length;
        const M = A.map((row) => row.slice());
        let det = 1;
        let sign = 1;
        for (let i = 0; i < n; i++) {
          // pivot
          let pivot = i;
          for (let r = i + 1; r < n; r++) {
            if (Math.abs(M[r][i]) > Math.abs(M[pivot][i])) pivot = r;
          }
          if (Math.abs(M[pivot][i]) < 1e-12) return 0;
          if (pivot !== i) {
            [M[i], M[pivot]] = [M[pivot], M[i]];
            sign *= -1;
          }
          det *= M[i][i];
          const pivotVal = M[i][i];
          for (let r = i + 1; r < n; r++) {
            const factor = M[r][i] / pivotVal;
            for (let c = i; c < n; c++) {
              M[r][c] -= factor * M[i][c];
            }
          }
        }
        return det * sign;
      }

      function inverse(A) {
        const n = A.length;
        const M = A.map((row, i) => {
          const extended = row.slice();
          for (let j = 0; j < n; j++) {
            extended.push(i === j ? 1 : 0);
          }
          return extended;
        });

        for (let i = 0; i < n; i++) {
          // pivot
          let pivot = i;
          for (let r = i + 1; r < n; r++) {
            if (Math.abs(M[r][i]) > Math.abs(M[pivot][i])) pivot = r;
          }
          if (Math.abs(M[pivot][i]) < 1e-12) return null;
          if (pivot !== i) {
            [M[i], M[pivot]] = [M[pivot], M[i]];
          }
          const pivotVal = M[i][i];
          for (let c = 0; c < 2 * n; c++) {
            M[i][c] / pivotVal;
          }
          for (let r = 0; r < n; r++) {
            if (r === i) continue;
            const factor = M[r][i];
            for (let c = 0; c < 2 * n; c++) {
              M[r][c] -= factor * M[i][c];
            }
          }
        }

        const inv = [];
        for (let i = 0; i < n; i++) {
          inv.push(M[i].slice(n, 2 * n));
        }
        return inv;
      }

      function rank(A) {
        const m = A.length,
          n = A[0].length;
        const M = A.map((row) => row.slice());
        let rank = 0;
        let row = 0;
        const eps = 1e-10;

        for (let col = 0; col < n && row < m; col++) {
          // find pivot
          let pivot = row;
          for (let r = row + 1; r < m; r++) {
            if (Math.abs(M[r][col]) > Math.abs(M[pivot][col])) pivot = r;
          }
          if (Math.abs(M[pivot][col]) < eps) continue;
          [M[row], M[pivot]] = [M[pivot], M[row]];
          const pivotVal = M[row][col];
          for (let c = col; c < n; c++) {
            M[row][c] / pivotVal;
          }
          for (let r = 0; r < m; r++) {
            if (r === row) continue;
            const factor = M[r][col];
            for (let c = col; c < n; c++) {
              M[r][c] -= factor * M[row][c];
            }
          }
          row++;
          rank++;
        }
        return rank;
      }

      function solveLinearSystem(A, b) {
        const n = A.length;
        const M = A.map((row, i) => row.concat([b[i]]));
        const eps = 1e-12;

        for (let i = 0; i < n; i++) {
          let pivot = i;
          for (let r = i + 1; r < n; r++) {
            if (Math.abs(M[r][i]) > Math.abs(M[pivot][i])) pivot = r;
          }
          if (Math.abs(M[pivot][i]) < eps) return null;
          if (pivot !== i) {
            [M[i], M[pivot]] = [M[pivot], M[i]];
          }
          const pivotVal = M[i][i];
          for (let c = i; c <= n; c++) {
            M[i][c] / pivotVal;
          }
          for (let r = 0; r < n; r++) {
            if (r === i) continue;
            const factor = M[r][i];
            for (let c = i; c <= n; c++) {
              M[r][c] -= factor * M[i][c];
            }
          }
        }

        const x = new Array(n);
        for (let i = 0; i < n; i++) {
          x[i] = M[i][n];
        }
        return x;
      }

      function eigenvalues2x2(A) {
        const a = A[0][0],
          b = A[0][1],
          c = A[1][0],
          d = A[1][1];
        const tr = a + d;
        const det = a * d - b * c;
        const disc = tr * tr - 4 * det;
        if (disc >= 0) {
          const sqrtDisc = Math.sqrt(disc);
          return [(tr + sqrtDisc) / 2, (tr - sqrtDisc) / 2];
        } else {
          const real = tr / 2;
          const imag = Math.sqrt(-disc) / 2;
          return [
            { re: real, im: imag },
            { re: real, im: -imag },
          ];
        }
      }

      document.addEventListener("DOMContentLoaded", () => {
        // Initial grids
        buildMatrixGrid("matrixA", 3, 3);
        buildMatrixGrid("matrixB", 3, 3);

        const rowsAInput = document.getElementById("rowsA");
        const colsAInput = document.getElementById("colsA");
        const rowsBInput = document.getElementById("rowsB");
        const colsBInput = document.getElementById("colsB");

        document.getElementById("resizeA").addEventListener("click", () => {
          const r = Math.min(6, Math.max(1, parseInt(rowsAInput.value) || 1));
          const c = Math.min(6, Math.max(1, parseInt(colsAInput.value) || 1));
          rowsAInput.value = r;
          colsAInput.value = c;
          buildMatrixGrid("matrixA", r, c);
          clearError();
          document.getElementById("resultContainer").innerHTML = "";
        });

        document.getElementById("resizeB").addEventListener("click", () => {
          const r = Math.min(6, Math.max(1, parseInt(rowsBInput.value) || 1));
          const c = Math.min(6, Math.max(1, parseInt(colsBInput.value) || 1));
          rowsBInput.value = r;
          colsBInput.value = c;
          buildMatrixGrid("matrixB", r, c);
          clearError();
          document.getElementById("resultContainer").innerHTML = "";
        });

        document
          .getElementById("fillIdentityA")
          .addEventListener("click", () => {
            let n = parseInt(rowsAInput.value) || 1;
            n = Math.min(6, Math.max(1, n));
            rowsAInput.value = n;
            colsAInput.value = n;
            const I = Array.from({ length: n }, (_, i) =>
              Array.from({ length: n }, (_, j) => (i === j ? 1 : 0))
            );
            setMatrix("matrixA", I);
            clearError();
          });

        document.getElementById("fillZeroA").addEventListener("click", () => {
          const r = parseInt(rowsAInput.value) || 1;
          const c = parseInt(colsAInput.value) || 1;
          const Z = Array.from({ length: r }, () => Array(c).fill(0));
          setMatrix("matrixA", Z);
          clearError();
        });

        document.getElementById("fillRandomA").addEventListener("click", () => {
          const r = parseInt(rowsAInput.value) || 1;
          const c = parseInt(colsAInput.value) || 1;
          const R = Array.from({ length: r }, () =>
            Array.from({ length: c }, () => Math.floor(Math.random() * 11) - 5)
          );
          setMatrix("matrixA", R);
          clearError();
        });

        document.getElementById("swapAB").addEventListener("click", () => {
          const rA = parseInt(rowsAInput.value) || 1;
          const cA = parseInt(colsAInput.value) || 1;
          const rB = parseInt(rowsBInput.value) || 1;
          const cB = parseInt(colsBInput.value) || 1;
          const A = getMatrix("matrixA", rA, cA);
          const B = getMatrix("matrixB", rB, cB);
          rowsAInput.value = rB;
          colsAInput.value = cB;
          rowsBInput.value = rA;
          colsBInput.value = cA;
          setMatrix("matrixA", B);
          setMatrix("matrixB", A);
          clearError();
          document.getElementById("resultContainer").innerHTML = "";
        });

        document.getElementById("clearAll").addEventListener("click", () => {
          buildMatrixGrid("matrixA", 3, 3);
          buildMatrixGrid("matrixB", 3, 3);
          rowsAInput.value = 3;
          colsAInput.value = 3;
          rowsBInput.value = 3;
          colsBInput.value = 3;
          clearError();
          document.getElementById("resultContainer").innerHTML = "";
        });

        document.querySelectorAll(".op-btn").forEach((btn) => {
          btn.addEventListener("click", () => {
            clearError();
            const op = btn.getAttribute("data-op");
            const rA = parseInt(rowsAInput.value) || 1;
            const cA = parseInt(colsAInput.value) || 1;
            const rB = parseInt(rowsBInput.value) || 1;
            const cB = parseInt(colsBInput.value) || 1;
            const A = getMatrix("matrixA", rA, cA);
            const B = getMatrix("matrixB", rB, cB);
            const resultEl = document.getElementById("resultContainer");

            try {
              let resHtml = "";
              if (op === "add" || op === "sub") {
                if (rA !== rB || cA !== cB) {
                  showError(
                    "For A ± B, matrices must have the same dimensions."
                  );
                  return;
                }
                const C = op === "add" ? addMatrices(A, B) : subMatrices(A, B);
                resHtml = formatMatrix(C);
              } else if (op === "mul") {
                if (cA !== rB) {
                  showError("For A × B, columns of A must equal rows of B.");
                  return;
                }
                const C = mulMatrices(A, B);
                resHtml = formatMatrix(C);
              } else if (op === "mulBA") {
                if (cB !== rA) {
                  showError("For B × A, columns of B must equal rows of A.");
                  return;
                }
                const C = mulMatrices(B, A);
                resHtml = formatMatrix(C);
              } else if (op === "transposeA") {
                const T = transpose(A);
                resHtml = formatMatrix(T);
              } else if (op === "detA") {
                if (rA !== cA) {
                  showError("Determinant is defined only for square matrices.");
                  return;
                }
                const detA = determinant(A);
                resHtml = `<p class="text-sm">det(A) = <strong>${Number(
                  detA.toFixed(8)
                )}</strong></p>`;
              } else if (op === "invA") {
                if (rA !== cA) {
                  showError("Inverse is defined only for square matrices.");
                  return;
                }
                const invA = inverse(A);
                if (!invA) {
                  showError(
                    "Matrix A is singular and does not have an inverse."
                  );
                  return;
                }
                resHtml = formatMatrix(invA);
              } else if (op === "rankA") {
                const r = rank(A);
                resHtml = `<p class="text-sm">rank(A) = <strong>${r}</strong></p>`;
              } else if (op === "traceA") {
                if (rA !== cA) {
                  showError("Trace is defined only for square matrices.");
                  return;
                }
                let tr = 0;
                for (let i = 0; i < rA; i++) tr += A[i][i];
                resHtml = `<p class="text-sm">tr(A) = <strong>${Number(
                  tr.toFixed(8)
                )}</strong></p>`;
              } else if (op === "eigen2A") {
                if (rA !== 2 || cA !== 2) {
                  showError("Eigenvalues (2×2) requires A to be a 2×2 matrix.");
                  return;
                }
                const eig = eigenvalues2x2(A);
                if (typeof eig[0] === "number") {
                  resHtml = `<p class="text-sm">Eigenvalues: λ₁ = <strong>${Number(
                    eig[0].toFixed(8)
                  )}</strong>, λ₂ = <strong>${Number(
                    eig[1].toFixed(8)
                  )}</strong></p>`;
                } else {
                  const e1 = eig[0],
                    e2 = eig[1];
                  const fmt = (e) =>
                    `${Number(e.re.toFixed(8))} ${
                      e.im >= 0 ? "+" : "-"
                    } ${Number(Math.abs(e.im).toFixed(8))}i`;
                  resHtml = `<p class="text-sm">Eigenvalues (complex): λ₁ = <strong>${fmt(
                    e1
                  )}</strong>, λ₂ = <strong>${fmt(e2)}</strong></p>`;
                }
              } else if (op === "solveAx") {
                if (rA !== cA) {
                  showError("To solve A·x = b, A must be square.");
                  return;
                }
                if (rB !== rA || cB !== 1) {
                  showError(
                    "For A·x = b, B must be a column vector with the same number of rows as A."
                  );
                  return;
                }
                const bVec = B.map((row) => row[0]);
                const x = solveLinearSystem(A, bVec);
                if (!x) {
                  showError(
                    "System A·x = b has no unique solution (matrix may be singular)."
                  );
                  return;
                }
                resHtml =
                  '<p class="text-sm mb-2">Solution vector x:</p>' +
                  formatVector(x);
              }

              resultEl.innerHTML = resHtml;
            } catch (e) {
              console.error(e);
              showError(
                "An unexpected error occurred while performing the operation."
              );
            }
          });
        });
      });
    </script>

