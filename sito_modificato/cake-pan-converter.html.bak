    <script>
      // Utility functions
      const PI = Math.PI;
      const CUP_TO_ML = 236.588;

      function toCm(value, unit) {
        if (!value || isNaN(value)) return NaN;
        return unit === "in" ? value * 2.54 : value;
      }

      function volumeFromShape(shape, dims) {
        // dims in cm
        switch (shape) {
          case "round":
            return PI * Math.pow(dims.diameter / 2, 2) * dims.depth;
          case "square":
            return Math.pow(dims.side, 2) * dims.depth;
          case "rect":
            return dims.length * dims.width * dims.depth;
          default:
            return NaN;
        }
      }

      function mlFromInput(vol, unit) {
        if (!vol || isNaN(vol)) return NaN;
        return unit === "cup" ? vol * CUP_TO_ML : vol;
      }

      function formatMl(ml) {
        if (!isFinite(ml)) return "—";
        if (ml < 1) return ml.toFixed(1) + " mL";
        if (ml < 1000) return ml.toFixed(0) + " mL";
        return (ml / 1000).toFixed(2) + " L";
      }

      function createIngredientRow() {
        const tr = document.createElement("tr");
        tr.innerHTML = `
      <td class="border-t border-gray-200 px-2 py-1">
        <input type="text" class="orig-amt w-full border rounded-md px-2 py-1 text-sm" placeholder="e.g. 200 g">
      </td>
      <td class="border-t border-gray-200 px-2 py-1">
        <input type="text" class="ing-name w-full border rounded-md px-2 py-1 text-sm" placeholder="flour">
      </td>
      <td class="border-t border-gray-200 px-2 py-1">
        <input type="text" class="scaled-amt w-full border rounded-md px-2 py-1 text-sm bg-gray-50" readonly>
      </td>
    `;
        return tr;
      }

      function updateDimVisibility(prefix) {
        const shape = document.getElementById(prefix + "-shape").value;
        ["round", "square", "rect"].forEach((s) => {
          const el = document.getElementById(prefix + "-dim-" + s);
          if (el) el.classList.toggle("hidden", s !== shape);
        });
      }

      function getDims(prefix) {
        const shape = document.getElementById(prefix + "-shape").value;
        if (shape === "round") {
          const d = parseFloat(
            document.getElementById(prefix + "-diameter").value
          );
          const du = document.getElementById(prefix + "-diameter-unit").value;
          const h = parseFloat(
            document.getElementById(prefix + "-depth").value
          );
          const hu = document.getElementById(prefix + "-depth-unit").value;
          return {
            shape,
            diameter: toCm(d, du),
            depth: toCm(h, hu),
          };
        } else if (shape === "square") {
          const s = parseFloat(document.getElementById(prefix + "-side").value);
          const su = document.getElementById(prefix + "-side-unit").value;
          const h = parseFloat(
            document.getElementById(prefix + "-depth-square").value
          );
          const hu = document.getElementById(
            prefix + "-depth-square-unit"
          ).value;
          return {
            shape,
            side: toCm(s, su),
            depth: toCm(h, hu),
          };
        } else {
          const L = parseFloat(
            document.getElementById(prefix + "-length").value
          );
          const Lu = document.getElementById(prefix + "-length-unit").value;
          const W = parseFloat(
            document.getElementById(prefix + "-width").value
          );
          const Wu = document.getElementById(prefix + "-width-unit").value;
          const h = parseFloat(
            document.getElementById(prefix + "-depth-rect").value
          );
          const hu = document.getElementById(prefix + "-depth-rect-unit").value;
          return {
            shape,
            length: toCm(L, Lu),
            width: toCm(W, Wu),
            depth: toCm(h, hu),
          };
        }
      }

      function computeVolumes() {
        const origDims = getDims("orig");
        const newDims = getDims("new");

        if (
          Object.values(origDims).some((v) => isNaN(v)) ||
          Object.values(newDims).some((v) => isNaN(v))
        ) {
          return { error: "Please enter all pan dimensions." };
        }

        const origFill =
          parseFloat(document.getElementById("orig-fill").value) / 100;
        const newFill =
          parseFloat(document.getElementById("new-fill").value) / 100;

        if (
          !isFinite(origFill) ||
          !isFinite(newFill) ||
          origFill <= 0 ||
          newFill <= 0
        ) {
          return { error: "Fill levels must be positive percentages." };
        }

        let origVolMl;
        const userVol = parseFloat(
          document.getElementById("orig-volume").value
        );
        const userVolUnit = document.getElementById("orig-volume-unit").value;
        if (userVol && !isNaN(userVol)) {
          origVolMl = mlFromInput(userVol, userVolUnit);
        } else {
          const origPanVol = volumeFromShape(origDims.shape, origDims);
          origVolMl = origPanVol * origFill;
        }

        const newPanVol = volumeFromShape(newDims.shape, newDims);
        const newVolMl = newPanVol * newFill;

        return {
          origDims,
          newDims,
          origVolMl,
          newVolMl,
          origFill,
          newFill,
        };
      }

      function buildSteps(scale, depthRatio, origServ, newServ) {
        const steps = [];
        steps.push(
          `Multiply every ingredient in your recipe by <strong>${scale.toFixed(
            2
          )}</strong>. For example, 200 g flour becomes ${(200 * scale).toFixed(
            0
          )} g.`
        );
        if (origServ && newServ) {
          steps.push(
            `Your servings change from ${origServ} to approximately ${newServ} (servings factor: ${(
              newServ / origServ
            ).toFixed(2)}×).`
          );
        }
        if (depthRatio) {
          if (depthRatio < 0.9) {
            steps.push(
              "Your batter layer will be <strong>shallower</strong> than before. Start checking for doneness 5–10 minutes earlier than the original recipe suggests."
            );
          } else if (depthRatio > 1.1) {
            steps.push(
              "Your batter layer will be <strong>deeper</strong>. Expect a longer baking time and consider lowering the oven temperature by 10–15 °C (25 °F)."
            );
          } else {
            steps.push(
              "Batter depth is similar, so baking time should be close to the original. Start checking a few minutes early to be safe."
            );
          }
        }
        steps.push(
          "Use visual cues: the cake should spring back when lightly pressed, pull slightly from the sides, and a toothpick in the center should come out with a few moist crumbs."
        );
        return steps;
      }

      function updateIngredientTable(scale) {
        const rows = document.querySelectorAll("#ingredient-rows tr");
        rows.forEach((row) => {
          const origInput = row.querySelector(".orig-amt");
          const scaledInput = row.querySelector(".scaled-amt");
          const text = origInput.value.trim();
          if (!text) {
            scaledInput.value = "";
            return;
          }
          // Try to scale leading number if present
          const match = text.match(/^([\d.,]+)\s*(.*)$/);
          if (!match) {
            scaledInput.value = text;
            return;
          }
          const num = parseFloat(match[1].replace(",", "."));
          if (isNaN(num)) {
            scaledInput.value = text;
            return;
          }
          const rest = match[2];
          const scaledNum = num * scale;
          // Simple formatting: keep 2 decimals if not near integer
          const formatted =
            Math.abs(scaledNum - Math.round(scaledNum)) < 0.01
              ? Math.round(scaledNum).toString()
              : scaledNum.toFixed(2);
          scaledInput.value = formatted + (rest ? " " + rest : "");
        });
      }

      document.addEventListener("DOMContentLoaded", () => {
        // Init dim visibility
        ["orig", "new"].forEach((prefix) => {
          updateDimVisibility(prefix);
          document
            .getElementById(prefix + "-shape")
            .addEventListener("change", () => updateDimVisibility(prefix));
        });

        // Init ingredient table with a few rows
        const tbody = document.getElementById("ingredient-rows");
        for (let i = 0; i < 5; i++) {
          tbody.appendChild(createIngredientRow());
        }

        document.getElementById("add-row-btn").addEventListener("click", () => {
          tbody.appendChild(createIngredientRow());
        });

        document.getElementById("convert-btn").addEventListener("click", () => {
          const errorEl = document.getElementById("error-msg");
          errorEl.textContent = "";
          const res = computeVolumes();
          if (res.error) {
            errorEl.textContent = res.error;
            document.getElementById("results-section").classList.add("hidden");
            return;
          }

          const { origDims, newDims, origVolMl, newVolMl } = res;
          if (
            !isFinite(origVolMl) ||
            !isFinite(newVolMl) ||
            origVolMl <= 0 ||
            newVolMl <= 0
          ) {
            errorEl.textContent =
              "Could not compute volumes. Please check your inputs.";
            document.getElementById("results-section").classList.add("hidden");
            return;
          }

          const scale = newVolMl / origVolMl;
          const scaleBox = document.getElementById("scale-factor");
          scaleBox.textContent = scale.toFixed(2) + "×";

          document.getElementById("orig-volume-display").textContent =
            formatMl(origVolMl);
          document.getElementById("new-volume-display").textContent =
            formatMl(newVolMl);

          // Depth ratio for baking time guidance
          const origDepth = origDims.depth;
          const newDepth = newDims.depth;
          const depthRatio = newDepth / origDepth;
          let timeText;
          if (depthRatio < 0.8) {
            timeText =
              "Shallower cake: start checking 25–30% earlier than the original time.";
          } else if (depthRatio < 0.95) {
            timeText =
              "Slightly shallower: start checking 10–15% earlier than the original time.";
          } else if (depthRatio <= 1.05) {
            timeText =
              "Similar depth: baking time should be close to the original recipe.";
          } else if (depthRatio <= 1.3) {
            timeText = "Slightly deeper: expect 10–20% longer baking time.";
          } else {
            timeText =
              "Much deeper cake: expect significantly longer baking time and consider lowering oven temperature by 10–15 °C (25 °F).";
          }
          document.getElementById("time-adjust").textContent = timeText;

          // Steps
          const origServ = parseFloat(
            document.getElementById("orig-servings").value
          );
          const newServInput = document.getElementById("new-servings").value;
          let newServ = parseFloat(newServInput);
          if (!isNaN(origServ) && isNaN(newServ) && isFinite(scale)) {
            newServ = Math.round(origServ * scale);
          }
          const steps = buildSteps(scale, depthRatio, origServ, newServ);
          const stepList = document.getElementById("step-list");
          stepList.innerHTML = "";
          steps.forEach((s) => {
            const li = document.createElement("li");
            li.innerHTML = s;
            stepList.appendChild(li);
          });

          // Update ingredient table
          updateIngredientTable(scale);

          document.getElementById("results-section").classList.remove("hidden");
        });

        document.getElementById("reset-btn").addEventListener("click", () => {
          window.location.reload();
        });

        // Re-scale ingredient table when user edits amounts (debounced)
        let scaleDebounce;
        document
          .getElementById("ingredient-rows")
          .addEventListener("input", () => {
            clearTimeout(scaleDebounce);
            scaleDebounce = setTimeout(() => {
              const scaleText =
                document.getElementById("scale-factor").textContent || "1.00×";
              const m = scaleText.match(/([\d.]+)/);
              const scale = m ? parseFloat(m[1]) : 1;
              if (isFinite(scale)) updateIngredientTable(scale);
            }, 300);
          });
      });
    </script>

