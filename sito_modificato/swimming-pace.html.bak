<script>
  function toSeconds(min, sec, ms) {
    min = Number(min) || 0;
    sec = Number(sec) || 0;
    ms  = Number(ms)  || 0;
    return min * 60 + sec + ms / 1000;
  }

  function formatPace(seconds) {
    if (!isFinite(seconds) || seconds <= 0) return '-';
    const totalMs = Math.round(seconds * 1000);
    const s = Math.floor(totalMs / 1000);
    const ms = totalMs % 1000;
    const m = Math.floor(s / 60);
    const rem = s % 60;
    const msStr = ms ? '.' + String(ms).padStart(3, '0') : '';
    return `${m}:${String(rem).padStart(2, '0')}${msStr}`;
  }

  function formatTime(seconds) {
    if (!isFinite(seconds) || seconds <= 0) return '-';
    const s = Math.round(seconds);
    const h = Math.floor(s / 3600);
    const rem = s % 3600;
    const m = Math.floor(rem / 60);
    const sec = rem % 60;
    if (h > 0) {
      return `${h}:${String(m).padStart(2, '0')}:${String(sec).padStart(2, '0')}`;
    }
    return `${m}:${String(sec).padStart(2, '0')}`;
  }

  function updateDistanceUnitLabel() {
    const unit = document.getElementById('pool-unit').value;
    const label = unit === 'yards' ? 'yd' : 'm';
    document.getElementById('distance-unit-label').textContent = label;
    document.getElementById('pace-per-100-unit-label').textContent = label;
    document.getElementById('pace-50-unit-label').textContent = label;
    document.getElementById('pace-100-unit-label').textContent = label;
    const poolLen = document.getElementById('pool-length').value;
    document.getElementById('length-label').textContent = poolLen + label;
  }

  function calcPace() {
    const dist = Number(document.getElementById('distance').value);
    const t = toSeconds(
      document.getElementById('time-min').value,
      document.getElementById('time-sec').value,
      document.getElementById('time-ms').value
    );
    const errorEl = document.getElementById('pace-error');
    if (!dist || dist <= 0 || !t || t <= 0) {
      errorEl.classList.remove('hidden');
      document.getElementById('pace-results').classList.add('hidden');
      return;
    }
    errorEl.classList.add('hidden');

    const unit = document.getElementById('pool-unit').value;
    const poolLen = Number(document.getElementById('pool-length').value);

    const pacePerUnit = t / dist; // sec per meter or yard
    const pacePer100 = pacePerUnit * 100;
    const pacePer50 = pacePerUnit * 50;
    const pacePerLength = pacePerUnit * poolLen;

    document.getElementById('pace-per-100').textContent = formatPace(pacePer100) + ` /100${unit === 'yards' ? 'yd' : 'm'}`;
    document.getElementById('pace-per-100-small').textContent = formatPace(pacePer100);
    document.getElementById('pace-per-50').textContent = formatPace(pacePer50);
    document.getElementById('pace-per-length').textContent = formatPace(pacePerLength);

    // Speed
    let speedStr;
    if (unit === 'yards') {
      const ydPerSec = dist / t;
      speedStr = ydPerSec.toFixed(2) + ' yd/s';
    } else {
      const mPerSec = dist / t;
      const kmPerH = mPerSec * 3.6;
      speedStr = mPerSec.toFixed(2) + ' m/s (' + kmPerH.toFixed(2) + ' km/h)';
    }
    document.getElementById('pace-speed').textContent = speedStr;

    // Race projections
    const projectionsBody = document.getElementById('race-projections');
    projectionsBody.innerHTML = '';
    const labelUnit = unit === 'yards' ? 'yd' : 'm';
    const distances = unit === 'yards'
      ? [100, 200, 500, 1000, 1650]
      : [100, 200, 400, 800, 1500];

    distances.forEach(d => {
      const time = pacePerUnit * d;
      const tr = document.createElement('tr');
      tr.innerHTML = `
        <td class="px-3 py-2 border-b">${d}${labelUnit}</td>
        <td class="px-3 py-2 border-b">${formatTime(time)}</td>
      `;
      projectionsBody.appendChild(tr);
    });

    document.getElementById('pace-results').classList.remove('hidden');
  }

  function resetPace() {
    ['distance','time-min','time-sec','time-ms'].forEach(id => {
      document.getElementById(id).value = '';
    });
    document.getElementById('pace-results').classList.add('hidden');
    document.getElementById('pace-error').classList.add('hidden');
  }

  function calcCSS() {
    const t400 = toSeconds(
      document.getElementById('css-400-min').value,
      document.getElementById('css-400-sec').value,
      document.getElementById('css-400-ms').value
    );
    const t200 = toSeconds(
      document.getElementById('css-200-min').value,
      document.getElementById('css-200-sec').value,
      document.getElementById('css-200-ms').value
    );
    const errorEl = document.getElementById('css-error');
    if (!t400 || !t200 || t400 <= t200) {
      errorEl.classList.remove('hidden');
      document.getElementById('css-results').classList.add('hidden');
      return;
    }
    errorEl.classList.add('hidden');

    const cssSpeed = 200 / (t400 - t200); // m/s
    const cssPer100 = 100 / cssSpeed;
    const cssPer25 = 25 / cssSpeed;

    document.getElementById('css-per-100').textContent = formatPace(cssPer100) + ' /100m';
    document.getElementById('css-per-25').textContent = formatPace(cssPer25) + ' /25m';
    document.getElementById('css-speed').textContent = cssSpeed.toFixed(2) + ' m/s';

    // Zones (per 100m)
    const zonesBody = document.getElementById('css-zones');
    zonesBody.innerHTML = '';

    function addZone(name, purpose, factorLow, factorHigh) {
      const low = cssPer100 * factorLow;
      const high = cssPer100 * factorHigh;
      const tr = document.createElement('tr');
      tr.innerHTML = `
        <td class="px-3 py-2 border-b font-medium">${name}</td>
        <td class="px-3 py-2 border-b">${purpose}</td>
        <td class="px-3 py-2 border-b">${formatPace(low)} – ${formatPace(high)} /100m</td>
      `;
      zonesBody.appendChild(tr);
    }

    addZone('Easy / Recovery', 'Technique focus, warm‑up, cool‑down', 1.10, 1.25);
    addZone('Endurance (Aerobic)', 'Steady aerobic base work', 1.03, 1.10);
    addZone('CSS / Threshold', 'Lactate threshold, race‑pace sets', 0.98, 1.03);
    addZone('Tempo / VO₂', 'Shorter, harder intervals', 0.92, 0.98);
    addZone('Sprint', 'Max speed, short distances', 0.80, 0.92);

    document.getElementById('css-results').classList.remove('hidden');
  }

  function resetCSS() {
    ['css-400-min','css-400-sec','css-400-ms','css-200-min','css-200-sec','css-200-ms'].forEach(id => {
      document.getElementById(id).value = '';
    });
    document.getElementById('css-results').classList.add('hidden');
    document.getElementById('css-error').classList.add('hidden');
  }

  function initTabs() {
    const tabBasic = document.getElementById('tab-basic');
    const tabCss = document.getElementById('tab-css');
    const panelBasic = document.getElementById('panel-basic');
    const panelCss = document.getElementById('panel-css');

    tabBasic.addEventListener('click', () => {
      tabBasic.classList.add('tab-active');
      tabCss.classList.remove('tab-active');
      panelBasic.classList.remove('hidden');
      panelCss.classList.add('hidden');
    });

    tabCss.addEventListener('click', () => {
      tabCss.classList.add('tab-active');
      tabBasic.classList.remove('tab-active');
      panelCss.classList.remove('hidden');
      panelBasic.classList.add('hidden');
    });
  }

  document.addEventListener('DOMContentLoaded', () => {
    updateDistanceUnitLabel();
    initTabs();

    document.getElementById('pool-unit').addEventListener('change', updateDistanceUnitLabel);
    document.getElementById('pool-length').addEventListener('change', updateDistanceUnitLabel);

    document.getElementById('btn-calc-pace').addEventListener('click', calcPace);
    document.getElementById('btn-reset-pace').addEventListener('click', resetPace);

    document.getElementById('btn-calc-css').addEventListener('click', calcCSS);
    document.getElementById('btn-reset-css').addEventListener('click', resetCSS);
  });

  window.MathJax = {
    tex: { inlineMath: [['\\(','\\)'], ['$', '$']], displayMath: [['$','$'], ['\\[','\\]']] },
    svg: { fontCache: 'global' }
  };
</script>
