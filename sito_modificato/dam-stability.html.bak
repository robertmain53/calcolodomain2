    <script>
      // Simple dam stability computation
      function computeDamStability() {
        const H = parseFloat(document.getElementById("damHeight").value);
        const B = parseFloat(document.getElementById("baseWidth").value);
        const mu = parseFloat(document.getElementById("frictionCoeff").value);
        const gammaC = parseFloat(
          document.getElementById("unitWeightConcrete").value
        );
        const gammaW = parseFloat(
          document.getElementById("unitWeightWater").value
        );
        const hu = parseFloat(document.getElementById("upstreamDepth").value);
        const hd = parseFloat(document.getElementById("downstreamDepth").value);
        const alpha = parseFloat(document.getElementById("upliftCoeff").value);
        const kh = parseFloat(document.getElementById("seismicCoeff").value);
        const muSlope = parseFloat(
          document.getElementById("upstreamSlope").value
        );
        const mdSlope = parseFloat(
          document.getElementById("downstreamSlope").value
        );

        const statusEl = document.getElementById("calcStatus");

        if ([H, B, gammaC, gammaW].some((v) => !isFinite(v) || v <= 0)) {
          statusEl.textContent =
            "Please enter valid positive values for height, base width, and unit weights.";
          return;
        }

        // Geometry
        const xu = muSlope * H; // upstream crest offset from heel
        const xd = B - mdSlope * H; // downstream crest offset from heel
        const topWidth = B + xu - xd;
        const Ac = 0.5 * (B + topWidth) * H; // area per m length

        // Approximate centroid x from heel (trapezoid)
        const xCentroid =
          (B * B + B * topWidth + topWidth * topWidth) / (3 * (B + topWidth));

        // Self weight
        const W = gammaC * Ac;

        // Hydrostatic forces
        const Hu = 0.5 * gammaW * Math.max(hu, 0) ** 2;
        const Hd = 0.5 * gammaW * Math.max(hd, 0) ** 2;

        // Lever arms (from base)
        const zHu = Math.max(hu, 0) / 3;
        const zHd = Math.max(hd, 0) / 3;

        // Uplift
        const pu = gammaW * Math.max(hu, 0);
        const pd = gammaW * Math.max(hd, 0);
        const pAvg = alpha * 0.5 * (pu + pd);
        const U = pAvg * B;
        const zU = H / 2; // assume uplift resultant at mid-height of base

        // Seismic inertia
        const He = kh * W;
        const zHe = H / 2; // centroid height

        // Resultant vertical and horizontal
        const N = W - U;
        const Htot = Hu - Hd + He;

        // Sliding FS
        const R = mu * N;
        const fsSliding = Htot !== 0 ? R / Htot : Infinity;

        // Moments about downstream toe (x = B)
        const xToe = B;

        const zW = H / 2;
        const MW = W * (xToe - xCentroid); // resisting

        const MHu = Hu * (xToe - 0); // overturning (upstream at heel)
        const MHd = Hd * (xToe - B); // resisting (downstream at toe)
        const MU = U * (xToe - B / 2); // overturning (uplift acts upward)
        const MHe = He * (xToe - xCentroid); // overturning (assume inertia acts downstream)

        const Mresisting = MW + MHd;
        const Moverturn = MHu + MU + MHe;

        const fsOverturning =
          Moverturn !== 0 ? Mresisting / Moverturn : Infinity;

        // Resultant moment and eccentricity
        const V = N;
        const Mnet = Mresisting - Moverturn; // positive = net resisting
        const xResultant = V !== 0 ? Mnet / V : B / 2;
        const e = xResultant - B / 2;

        // Base stresses (kPa)
        let sigmaToe = NaN,
          sigmaHeel = NaN,
          middleThird = "N/A";
        if (V > 0) {
          const sigmaAvg = V / B;
          const sigmaDiff = (6 * V * e) / (B * B);
          sigmaToe = (sigmaAvg + sigmaDiff) / 1.0;
          sigmaHeel = (sigmaAvg - sigmaDiff) / 1.0;
          middleThird =
            Math.abs(e) <= B / 6
              ? "OK (within middle third)"
              : "Warning (outside middle third)";
        }

        // Update UI
        function fmt(v, digits = 2) {
          if (!isFinite(v)) return "–";
          return v.toFixed(digits);
        }

        document.getElementById("resW").textContent = fmt(W);
        document.getElementById("resHu").textContent = fmt(Hu);
        document.getElementById("resHd").textContent = fmt(Hd);
        document.getElementById("resHe").textContent = fmt(He);
        document.getElementById("resHtot").textContent = fmt(Htot);
        document.getElementById("resU").textContent = fmt(U);
        document.getElementById("resN").textContent = fmt(N);
        document.getElementById("resEcc").textContent = fmt(e, 3);
        document.getElementById("resSigmaToe").textContent = fmt(sigmaToe);
        document.getElementById("resSigmaHeel").textContent = fmt(sigmaHeel);
        document.getElementById("resMiddleThird").textContent = middleThird;

        document.getElementById("fsSliding").textContent = fmt(fsSliding, 2);
        document.getElementById("fsOverturning").textContent = fmt(
          fsOverturning,
          2
        );

        const slidingStatus = document.getElementById("slidingStatus");
        const overturningStatus = document.getElementById("overturningStatus");

        function setStatus(el, fs, targetNormal, targetExtreme) {
          el.className = "result-badge";
          if (!isFinite(fs)) {
            el.textContent = "N/A";
            el.classList.add("bg-gray-200", "text-gray-800");
          } else if (fs >= targetNormal) {
            el.textContent = "OK";
            el.classList.add("bg-green-100", "text-green-800");
          } else if (fs >= targetExtreme) {
            el.textContent = "Borderline";
            el.classList.add("bg-amber-100", "text-amber-800");
          } else {
            el.textContent = "Low";
            el.classList.add("bg-red-100", "text-red-800");
          }
        }

        setStatus(slidingStatus, fsSliding, 1.5, 1.1);
        setStatus(overturningStatus, fsOverturning, 2.0, 1.5);

        statusEl.textContent = "Computation updated.";
      }

      document
        .getElementById("computeBtn")
        .addEventListener("click", computeDamStability);
      document.getElementById("resetBtn").addEventListener("click", () => {
        document.getElementById("damHeight").value = 30;
        document.getElementById("baseWidth").value = 20;
        document.getElementById("upstreamSlope").value = 0.0;
        document.getElementById("downstreamSlope").value = 0.7;
        document.getElementById("unitWeightConcrete").value = 24;
        document.getElementById("frictionCoeff").value = 0.7;
        document.getElementById("upstreamDepth").value = 28;
        document.getElementById("downstreamDepth").value = 2;
        document.getElementById("unitWeightWater").value = 9.81;
        document.getElementById("upliftCoeff").value = 0.7;
        document.getElementById("seismicCoeff").value = 0.0;
        computeDamStability();
      });

      window.addEventListener("load", computeDamStability);
    </script>

