            <script>
              document.addEventListener('DOMContentLoaded', function() {
                const inputs = {
                  a: document.getElementById('val-a'),
                  b: document.getElementById('val-b'),
                  c: document.getElementById('val-c'),
                  d: document.getElementById('val-d')
                };
                const calculateBtn = document.getElementById('calculate-btn');
                const resultsContainer = document.getElementById('results-container');
                const errorMessage = document.getElementById('error-message');
                const resultEquation = document.getElementById('result-equation');
                const stepByStep = document.getElementById('step-by-step');

                // --- Helper: Parse fractions and decimals ---
                function parseValue(val) {
                  val = String(val).trim();
                  if (val.toLowerCase() === 'x' || val === '') return 'x';
                  
                  if (val.includes('/')) {
                    const parts = val.split('/');
                    if (parts.length === 2) {
                      const num = parseFloat(parts[0]);
                      const den = parseFloat(parts[1]);
                      if (den !== 0 && !isNaN(num) && !isNaN(den)) {
                        return num / den;
                      }
                    }
                  }
                  const num = parseFloat(val);
                  return isNaN(num) ? 'invalid' : num;
                }
                
                function formatNumber(num) {
                    // Remove trailing zeros after decimal
                    return parseFloat(num.toFixed(10)).toString();
                }
                
                function showError(message) {
                    errorMessage.textContent = `Error: ${message}`;
                    resultsContainer.classList.add('hidden');
                }

                // --- Calculation Logic ---
                calculateBtn.addEventListener('click', () => {
                  let unknownVar = null;
                  let unknownCount = 0;
                  const values = {};
                  const rawValues = {};

                  // Parse all inputs
                  for (const key in inputs) {
                    const parsed = parseValue(inputs[key].value);
                    rawValues[key] = inputs[key].value || 'x';
                    if (parsed === 'x') {
                      unknownVar = key;
                      unknownCount++;
                    } else if (parsed === 'invalid') {
                      showError(`Invalid input for ${key.toUpperCase()}. Please use numbers or 'x'.`);
                      return;
                    }
                    values[key] = parsed;
                  }

                  // Validate inputs
                  if (unknownCount === 0) {
                    showError("Please leave one box blank or enter 'x' for the value you want to solve.");
                    return;
                  }
                  if (unknownCount > 1) {
                    showError("Please enter 'x' for only one unknown value.");
                    return;
                  }
                  
                  errorMessage.textContent = '';
                  let result;
                  let steps = '';
                  const A = values.a === 'x' ? 'x' : values.a;
                  const B = values.b === 'x' ? 'x' : values.b;
                  const C = values.c === 'x' ? 'x' : values.c;
                  const D = values.d === 'x' ? 'x' : values.d;
                  
                  const fA = formatNumber(A);
                  const fB = formatNumber(B);
                  const fC = formatNumber(C);
                  const fD = formatNumber(D);
                  
                  steps += `<p>1. Set up the proportion:</p>`;
                  steps += `<p class="text-center text-lg">${rawValues.a} / ${rawValues.b} = ${rawValues.c} / ${rawValues.d}</p>`;
                  steps += `<p>2. Cross-multiply (A * D = B * C):</p>`;

                  try {
                    if (unknownVar === 'a') {
                      if (Math.abs(D) < 1e-10) throw new Error("Cannot solve for A when D is zero (division by zero).");
                      result = (B * C) / D;
                      steps += `<p class="text-center text-lg">(x) * (${fD}) = (${fB}) * (${fC})</p>`;
                      steps += `<p class="text-center text-lg">x * ${fD} = ${formatNumber(B * C)}</p>`;
                      steps += `<p>3. Isolate x:</p><p class="text-center text-lg">x = ${formatNumber(B * C)} / ${fD}</p>`;
                    } else if (unknownVar === 'b') {
                      if (Math.abs(C) < 1e-10) throw new Error("Cannot solve for B when C is zero (division by zero).");
                      result = (A * D) / C;
                      steps += `<p class="text-center text-lg">(${fA}) * (${fD}) = (x) * (${fC})</p>`;
                      steps += `<p class="text-center text-lg">${formatNumber(A * D)} = x * ${fC}</p>`;
                      steps += `<p>3. Isolate x:</p><p class="text-center text-lg">x = ${formatNumber(A * D)} / ${fC}</p>`;
                    } else if (unknownVar === 'c') {
                      if (Math.abs(B) < 1e-10) throw new Error("Cannot solve for C when B is zero (division by zero).");
                      result = (A * D) / B;
                      steps += `<p class="text-center text-lg">(${fA}) * (${fD}) = (${fB}) * (x)</p>`;
                      steps += `<p class="text-center text-lg">${formatNumber(A * D)} = ${fB} * x</p>`;
                      steps += `<p>3. Isolate x:</p><p class="text-center text-lg">x = ${formatNumber(A * D)} / ${fB}</p>`;
                    } else if (unknownVar === 'd') {
                      if (Math.abs(A) < 1e-10) throw new Error("Cannot solve for D when A is zero (division by zero).");
                      result = (B * C) / A;
                      steps += `<p class="text-center text-lg">(${fA}) * (x) = (${fB}) * (${fC})</p>`;
                      steps += `<p class="text-center text-lg">${fA} * x = ${formatNumber(B * C)}</p>`;
                      steps += `<p>3. Isolate x:</p><p class="text-center text-lg">x = ${formatNumber(B * C)} / ${fA}</p>`;
                    }
                    
                    if (isNaN(result)) {
                      throw new Error("Calculation resulted in an invalid number. Check your inputs.");
                    }
                    
                    const formattedResult = formatNumber(result);
                    steps += `<p>4. Solve for x:</p><p class="text-center text-lg"><strong>x = ${formattedResult}</strong></p>`;
                    
                    resultEquation.textContent = `x = ${formattedResult}`;
                    stepByStep.innerHTML = steps;
                    resultsContainer.classList.remove('hidden');

                  } catch (e) {
                    showError(e.message);
                  }
                });
              });
            </script>

