    <script>
      // Utility: unit conversion factors to SI
      function toSI_D(value, unit) {
        if (unit === "m2_s") return value;
        if (unit === "cm2_s") return value * 1e-4;
        if (unit === "mm2_s") return value * 1e-6;
        return NaN;
      }
      function fromSI_D(valueSI, unit) {
        if (unit === "m2_s") return valueSI;
        if (unit === "cm2_s") return valueSI / 1e-4;
        if (unit === "mm2_s") return valueSI / 1e-6;
        return NaN;
      }

      function toSI_dC(value, unit) {
        // base SI: mol/m3 or g/m3
        if (unit === "mol_m3") return { val: value, kind: "mol" };
        if (unit === "mol_L") return { val: value * 1000, kind: "mol" }; // 1 L = 1e-3 m3
        if (unit === "g_m3") return { val: value, kind: "g" };
        if (unit === "g_L") return { val: value * 1000, kind: "g" };
        return { val: NaN, kind: null };
      }
      function fromSI_dC(valueSI, unit) {
        if (unit === "mol_m3") return valueSI;
        if (unit === "mol_L") return valueSI / 1000;
        if (unit === "g_m3") return valueSI;
        if (unit === "g_L") return valueSI / 1000;
        return NaN;
      }

      function toSI_dx(value, unit) {
        if (unit === "m") return value;
        if (unit === "cm") return value * 1e-2;
        if (unit === "mm") return value * 1e-3;
        if (unit === "um") return value * 1e-6;
        return NaN;
      }
      function fromSI_dx(valueSI, unit) {
        if (unit === "m") return valueSI;
        if (unit === "cm") return valueSI / 1e-2;
        if (unit === "mm") return valueSI / 1e-3;
        if (unit === "um") return valueSI / 1e-6;
        return NaN;
      }

      function toSI_J(value, unit) {
        // base SI: mol/(m2 s) or g/(m2 s)
        if (unit === "mol_m2_s") return { val: value, kind: "mol" };
        if (unit === "mol_cm2_s") return { val: value * 1e4, kind: "mol" }; // 1 cm2 = 1e-4 m2
        if (unit === "g_m2_s") return { val: value, kind: "g" };
        if (unit === "g_cm2_s") return { val: value * 1e4, kind: "g" };
        return { val: NaN, kind: null };
      }
      function fromSI_J(valueSI, unit) {
        if (unit === "mol_m2_s") return valueSI;
        if (unit === "mol_cm2_s") return valueSI / 1e4;
        if (unit === "g_m2_s") return valueSI;
        if (unit === "g_cm2_s") return valueSI / 1e4;
        return NaN;
      }

      // Mode handling
      const modeButtons = document.querySelectorAll(".solve-mode-btn");
      let solveFor = "flux";

      function updateFieldStates() {
        const D = document.getElementById("D_value");
        const dC = document.getElementById("dC_value");
        const dx = document.getElementById("dx_value");
        const J = document.getElementById("J_value");

        [D, dC, dx, J].forEach((inp) => {
          inp.disabled = false;
          inp.classList.remove("field-disabled");
        });

        if (solveFor === "flux") {
          J.disabled = true;
          J.classList.add("field-disabled");
          J.value = "";
        } else if (solveFor === "D") {
          D.disabled = true;
          D.classList.add("field-disabled");
          D.value = "";
        } else if (solveFor === "gradient") {
          dC.disabled = true;
          dC.classList.add("field-disabled");
          dC.value = "";
        } else if (solveFor === "thickness") {
          dx.disabled = true;
          dx.classList.add("field-disabled");
          dx.value = "";
        }
      }

      modeButtons.forEach((btn) => {
        btn.addEventListener("click", () => {
          solveFor = btn.getAttribute("data-solve");
          modeButtons.forEach((b) => {
            if (b === btn) {
              b.classList.remove(
                "border-gray-300",
                "text-gray-700",
                "bg-white"
              );
              b.classList.add("border-blue-600", "text-blue-700", "bg-blue-50");
            } else {
              b.classList.add("border-gray-300", "text-gray-700", "bg-white");
              b.classList.remove(
                "border-blue-600",
                "text-blue-700",
                "bg-blue-50"
              );
            }
          });
          updateFieldStates();
          document.getElementById("resultsBox").classList.add("hidden");
          document.getElementById("errorMsg").classList.add("hidden");
        });
      });

      updateFieldStates();

      // Compute
      document.getElementById("computeBtn").addEventListener("click", () => {
        const errorEl = document.getElementById("errorMsg");
        const resultsBox = document.getElementById("resultsBox");
        const resultMain = document.getElementById("resultMain");
        const resultSI = document.getElementById("resultSI");
        const resultDetails = document.getElementById("resultDetails");
        errorEl.classList.add("hidden");
        resultsBox.classList.add("hidden");

        const D_val = parseFloat(document.getElementById("D_value").value);
        const D_unit = document.getElementById("D_unit").value;
        const dC_val = parseFloat(document.getElementById("dC_value").value);
        const dC_unit = document.getElementById("dC_unit").value;
        const dx_val = parseFloat(document.getElementById("dx_value").value);
        const dx_unit = document.getElementById("dx_unit").value;
        const J_val = parseFloat(document.getElementById("J_value").value);
        const J_unit = document.getElementById("J_unit").value;

        function showError(msg) {
          errorEl.textContent = msg;
          errorEl.classList.remove("hidden");
        }

        try {
          let D_SI, dC_SI, dx_SI, J_SI;
          let kindC = null,
            kindJ = null;

          if (solveFor !== "D") {
            if (isNaN(D_val))
              return showError(
                "Please enter a value for the diffusion coefficient D."
              );
            D_SI = toSI_D(D_val, D_unit);
            if (!isFinite(D_SI) || D_SI <= 0)
              return showError(
                "Diffusion coefficient D must be a positive number."
              );
          }

          if (solveFor !== "gradient") {
            if (isNaN(dC_val))
              return showError(
                "Please enter a value for the concentration difference ΔC."
              );
            const tmp = toSI_dC(dC_val, dC_unit);
            dC_SI = tmp.val;
            kindC = tmp.kind;
            if (!kindC) return showError("Unsupported concentration unit.");
            if (!isFinite(dC_SI) || dC_SI === 0)
              return showError("Concentration difference ΔC must be non-zero.");
          }

          if (solveFor !== "thickness") {
            if (isNaN(dx_val))
              return showError("Please enter a value for the thickness Δx.");
            dx_SI = toSI_dx(dx_val, dx_unit);
            if (!isFinite(dx_SI) || dx_SI <= 0)
              return showError("Thickness Δx must be a positive number.");
          }

          if (solveFor !== "flux") {
            if (isNaN(J_val))
              return showError(
                "Please enter a value for the diffusion flux J."
              );
            const tmpJ = toSI_J(J_val, J_unit);
            J_SI = tmpJ.val;
            kindJ = tmpJ.kind;
            if (!kindJ) return showError("Unsupported flux unit.");
            if (!isFinite(J_SI) || J_SI === 0)
              return showError("Flux J must be non-zero.");
          }

          if (kindC && kindJ && kindC !== kindJ) {
            return showError(
              "Please use consistent mass or molar units for ΔC and J."
            );
          }

          let solvedValueSI, mainText, siText, detailText;

          if (solveFor === "flux") {
            const tmp = toSI_dC(dC_val, dC_unit);
            dC_SI = tmp.val;
            kindC = tmp.kind;
            if (!kindC) return showError("Unsupported concentration unit.");
            const dxTmp = toSI_dx(dx_val, dx_unit);
            dx_SI = dxTmp;
            J_SI = D_SI * (dC_SI / dx_SI);
            const J_out = fromSI_J(J_SI, J_unit);
            if (!isFinite(J_out))
              return showError("Could not convert flux to selected units.");
            solvedValueSI = J_SI;
            mainText = `Flux J = ${J_out.toExponential(3)} ${
              document.querySelector("#J_unit option:checked").textContent
            }`;
            siText = `In SI units: J = ${J_SI.toExponential(3)} ${
              kindC === "mol" ? "mol" : "g"
            }/(m²·s)`;
            detailText = `Computed using J = D · (ΔC / Δx) with D = ${D_SI.toExponential(
              3
            )} m²/s, ΔC = ${dC_SI.toExponential(3)} ${
              kindC === "mol" ? "mol" : "g"
            }/m³, and Δx = ${dx_SI.toExponential(3)} m.`;
          } else if (solveFor === "D") {
            const tmp = toSI_dC(dC_val, dC_unit);
            dC_SI = tmp.val;
            kindC = tmp.kind;
            const tmpJ = toSI_J(J_val, J_unit);
            J_SI = tmpJ.val;
            kindJ = tmpJ.kind;
            if (kindC && kindJ && kindC !== kindJ)
              return showError(
                "Please use consistent mass or molar units for ΔC and J."
              );
            D_SI = J_SI * (dx_SI / dC_SI);
            if (!isFinite(D_SI) || D_SI <= 0)
              return showError(
                "Resulting diffusion coefficient is not physically meaningful (check signs and magnitudes)."
              );
            const D_out = fromSI_D(D_SI, D_unit);
            solvedValueSI = D_SI;
            mainText = `Diffusion coefficient D = ${D_out.toExponential(3)} ${
              document.querySelector("#D_unit option:checked").textContent
            }`;
            siText = `In SI units: D = ${D_SI.toExponential(3)} m²/s`;
            detailText = `Computed using D = J · (Δx / ΔC) with J = ${J_SI.toExponential(
              3
            )} ${
              kindC === "mol" ? "mol" : "g"
            }/(m²·s), ΔC = ${dC_SI.toExponential(3)} ${
              kindC === "mol" ? "mol" : "g"
            }/m³, and Δx = ${dx_SI.toExponential(3)} m.`;
          } else if (solveFor === "gradient") {
            const tmpJ2 = toSI_J(J_val, J_unit);
            J_SI = tmpJ2.val;
            kindJ = tmpJ2.kind;
            const grad_SI = J_SI / D_SI; // ΔC/Δx
            solvedValueSI = grad_SI;
            const grad_out = grad_SI; // gradient is independent of chosen ΔC unit; we show in SI
            mainText = `Concentration gradient ΔC/Δx = ${grad_out.toExponential(
              3
            )} ${kindJ === "mol" ? "mol" : "g"}/(m⁴)`;
            siText = `In SI units: ΔC/Δx = ${grad_SI.toExponential(3)} ${
              kindJ === "mol" ? "mol" : "g"
            }/(m⁴)`;
            detailText = `Computed using ΔC/Δx = J / D with J = ${J_SI.toExponential(
              3
            )} ${
              kindJ === "mol" ? "mol" : "g"
            }/(m²·s) and D = ${D_SI.toExponential(3)} m²/s.`;
          } else if (solveFor === "thickness") {
            const tmp = toSI_dC(dC_val, dC_unit);
            dC_SI = tmp.val;
            kindC = tmp.kind;
            const tmpJ3 = toSI_J(J_val, J_unit);
            J_SI = tmpJ3.val;
            kindJ = tmpJ3.kind;
            if (kindC && kindJ && kindC !== kindJ)
              return showError(
                "Please use consistent mass or molar units for ΔC and J."
              );
            dx_SI = D_SI * (dC_SI / J_SI);
            if (!isFinite(dx_SI) || dx_SI <= 0)
              return showError(
                "Resulting thickness is not physically meaningful (check signs and magnitudes)."
              );
            const dx_out = fromSI_dx(dx_SI, dx_unit);
            solvedValueSI = dx_SI;
            mainText = `Thickness Δx = ${dx_out.toExponential(3)} ${
              document.querySelector("#dx_unit option:checked").textContent
            }`;
            siText = `In SI units: Δx = ${dx_SI.toExponential(3)} m`;
            detailText = `Computed using Δx = D · (ΔC / J) with D = ${D_SI.toExponential(
              3
            )} m²/s, ΔC = ${dC_SI.toExponential(3)} ${
              kindC === "mol" ? "mol" : "g"
            }/m³, and J = ${J_SI.toExponential(3)} ${
              kindC === "mol" ? "mol" : "g"
            }/(m²·s).`;
          }

          resultMain.textContent = mainText;
          resultSI.textContent = siText;
          resultDetails.textContent = detailText;
          resultsBox.classList.remove("hidden");
        } catch (e) {
          showError("An unexpected error occurred. Please check your inputs.");
          console.error(e);
        }
      });

      document.getElementById("resetBtn").addEventListener("click", () => {
        ["D_value", "dC_value", "dx_value", "J_value"].forEach(
          (id) => (document.getElementById(id).value = "")
        );
        document.getElementById("errorMsg").classList.add("hidden");
        document.getElementById("resultsBox").classList.add("hidden");
      });

      // Mobile menu script (simple)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", () => {
          const menu = document.getElementById("mobile-menu");
          const expanded = menu.classList.contains("hidden") ? "true" : "false";
          menu.classList.toggle("hidden");
          document
            .getElementById("mobile-menu-toggle")
            .setAttribute("aria-expanded", expanded);
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
