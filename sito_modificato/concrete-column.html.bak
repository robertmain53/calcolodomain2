    <script>
      // Utility conversions
      const PI = Math.PI;
      const MM_TO_M = 0.001;
      const IN_TO_M = 0.0254;
      const FT_TO_M = 0.3048;
      const M3_TO_FT3 = 35.3146667;
      const FT3_TO_YD3 = 1 / 27;
      const KG_TO_LB = 2.2046226218;

      const unitSystemEl = document.getElementById("unitSystem");
      const columnShapeEl = document.getElementById("columnShape");

      const rectInputsEl = document.getElementById("rectInputs");
      const circularInputsEl = document.getElementById("circularInputs");

      const rectWidthEl = document.getElementById("rectWidth");
      const rectDepthEl = document.getElementById("rectDepth");
      const rectWidthUnitEl = document.getElementById("rectWidthUnit");
      const rectDepthUnitEl = document.getElementById("rectDepthUnit");

      const circularDiameterEl = document.getElementById("circularDiameter");
      const circularDiameterUnitEl = document.getElementById(
        "circularDiameterUnit"
      );

      const columnHeightEl = document.getElementById("columnHeight");
      const columnHeightUnitEl = document.getElementById("columnHeightUnit");

      const concreteDensityEl = document.getElementById("concreteDensity");
      const concreteDensityUnitEl = document.getElementById(
        "concreteDensityUnit"
      );

      const numBarsEl = document.getElementById("numBars");
      const barDiameterEl = document.getElementById("barDiameter");
      const barDiameterUnitEl = document.getElementById("barDiameterUnit");
      const steelDensityEl = document.getElementById("steelDensity");
      const steelDensityUnitEl = document.getElementById("steelDensityUnit");

      const resVolumeM3El = document.getElementById("resVolumeM3");
      const resVolumeFt3El = document.getElementById("resVolumeFt3");
      const resVolumeYd3El = document.getElementById("resVolumeYd3");
      const resConcWeightKgEl = document.getElementById("resConcWeightKg");
      const resConcWeightLbEl = document.getElementById("resConcWeightLb");

      const resBarLengthMEl = document.getElementById("resBarLengthM");
      const resBarLengthFtEl = document.getElementById("resBarLengthFt");
      const resSteelVolM3El = document.getElementById("resSteelVolM3");
      const resSteelWeightKgEl = document.getElementById("resSteelWeightKg");
      const resSteelWeightLbEl = document.getElementById("resSteelWeightLb");

      function formatNumber(value, decimals = 3) {
        if (!isFinite(value)) return "–";
        return Number(value).toFixed(decimals);
      }

      function updateUnitLabels() {
        const system = unitSystemEl.value;
        if (system === "metric") {
          rectWidthUnitEl.textContent = "mm";
          rectDepthUnitEl.textContent = "mm";
          circularDiameterUnitEl.textContent = "mm";
          columnHeightUnitEl.textContent = "m";
          barDiameterUnitEl.textContent = "mm";
          concreteDensityUnitEl.textContent = "kg/m³";
          steelDensityUnitEl.textContent = "kg/m³";
        } else {
          rectWidthUnitEl.textContent = "in";
          rectDepthUnitEl.textContent = "in";
          circularDiameterUnitEl.textContent = "in";
          columnHeightUnitEl.textContent = "ft";
          barDiameterUnitEl.textContent = "in";
          concreteDensityUnitEl.textContent = "lb/ft³";
          steelDensityUnitEl.textContent = "lb/ft³";
        }
      }

      function updateShapeVisibility() {
        const shape = columnShapeEl.value;
        if (shape === "rect") {
          rectInputsEl.classList.remove("hidden");
          circularInputsEl.classList.add("hidden");
        } else {
          rectInputsEl.classList.add("hidden");
          circularInputsEl.classList.remove("hidden");
        }
      }

      function getLengthToMeters(value, unitLabel) {
        const unit = unitLabel.textContent.trim();
        const v = parseFloat(value);
        if (!isFinite(v) || v <= 0) return NaN;
        if (unit === "mm") return v * MM_TO_M;
        if (unit === "in") return v * IN_TO_M;
        if (unit === "m") return v;
        if (unit === "ft") return v * FT_TO_M;
        return NaN;
      }

      function getDensityToKgPerM3(value, unitLabel) {
        const unit = unitLabel.textContent.trim();
        const v = parseFloat(value);
        if (!isFinite(v) || v <= 0) return NaN;
        if (unit === "kg/m³") return v;
        if (unit === "lb/ft³") {
          // 1 lb/ft³ ≈ 16.018463 kg/m³
          return v * 16.018463;
        }
        return NaN;
      }

      function calculate() {
        const system = unitSystemEl.value;
        const shape = columnShapeEl.value;

        // Geometry to meters
        let areaM2 = NaN;
        const heightM = getLengthToMeters(
          columnHeightEl.value,
          columnHeightUnitEl
        );

        if (shape === "rect") {
          const b = getLengthToMeters(rectWidthEl.value, rectWidthUnitEl);
          const hc = getLengthToMeters(rectDepthEl.value, rectDepthUnitEl);
          if (isFinite(b) && isFinite(hc)) {
            areaM2 = b * hc;
          }
        } else {
          const d = getLengthToMeters(
            circularDiameterEl.value,
            circularDiameterUnitEl
          );
          if (isFinite(d)) {
            areaM2 = (PI * Math.pow(d, 2)) / 4;
          }
        }

        if (
          !isFinite(areaM2) ||
          !isFinite(heightM) ||
          areaM2 <= 0 ||
          heightM <= 0
        ) {
          resVolumeM3El.textContent = "–";
          resVolumeFt3El.textContent = "–";
          resVolumeYd3El.textContent = "–";
          resConcWeightKgEl.textContent = "–";
          resConcWeightLbEl.textContent = "–";
          resBarLengthMEl.textContent = "–";
          resBarLengthFtEl.textContent = "–";
          resSteelVolM3El.textContent = "–";
          resSteelWeightKgEl.textContent = "–";
          resSteelWeightLbEl.textContent = "–";
          return;
        }

        const volumeM3 = areaM2 * heightM;
        const volumeFt3 = volumeM3 * M3_TO_FT3;
        const volumeYd3 = volumeFt3 * FT3_TO_YD3;

        const concDensityKgM3 = getDensityToKgPerM3(
          concreteDensityEl.value,
          concreteDensityUnitEl
        );
        const concWeightKg = isFinite(concDensityKgM3)
          ? concDensityKgM3 * volumeM3
          : NaN;
        const concWeightLb = isFinite(concWeightKg)
          ? concWeightKg * KG_TO_LB
          : NaN;

        resVolumeM3El.textContent = formatNumber(volumeM3, 3);
        resVolumeFt3El.textContent = formatNumber(volumeFt3, 2);
        resVolumeYd3El.textContent = formatNumber(volumeYd3, 3);
        resConcWeightKgEl.textContent = formatNumber(concWeightKg, 1);
        resConcWeightLbEl.textContent = formatNumber(concWeightLb, 1);

        // Rebar calculations
        const nBars = parseInt(numBarsEl.value, 10);
        const barDiamM = getLengthToMeters(
          barDiameterEl.value,
          barDiameterUnitEl
        );
        const steelDensityKgM3 = getDensityToKgPerM3(
          steelDensityEl.value,
          steelDensityUnitEl
        );

        if (
          isFinite(nBars) &&
          nBars > 0 &&
          isFinite(barDiamM) &&
          barDiamM > 0 &&
          isFinite(steelDensityKgM3)
        ) {
          const barAreaM2 = (PI * Math.pow(barDiamM, 2)) / 4;
          const totalBarLengthM = nBars * heightM;
          const steelVolM3 = barAreaM2 * totalBarLengthM;
          const steelWeightKg = steelDensityKgM3 * steelVolM3;
          const totalBarLengthFt = totalBarLengthM * (1 / FT_TO_M);
          const steelWeightLb = steelWeightKg * KG_TO_LB;

          resBarLengthMEl.textContent = formatNumber(totalBarLengthM, 2);
          resBarLengthFtEl.textContent = formatNumber(totalBarLengthFt, 1);
          resSteelVolM3El.textContent = formatNumber(steelVolM3, 4);
          resSteelWeightKgEl.textContent = formatNumber(steelWeightKg, 1);
          resSteelWeightLbEl.textContent = formatNumber(steelWeightLb, 1);
        } else {
          resBarLengthMEl.textContent = "–";
          resBarLengthFtEl.textContent = "–";
          resSteelVolM3El.textContent = "–";
          resSteelWeightKgEl.textContent = "–";
          resSteelWeightLbEl.textContent = "–";
        }
      }

      function resetForm() {
        unitSystemEl.value = "metric";
        columnShapeEl.value = "rect";
        rectWidthEl.value = 300;
        rectDepthEl.value = 300;
        circularDiameterEl.value = 400;
        columnHeightEl.value = 3;
        concreteDensityEl.value = 2400;
        numBarsEl.value = 4;
        barDiameterEl.value = 16;
        steelDensityEl.value = 7850;
        updateUnitLabels();
        updateShapeVisibility();
        calculate();
      }

      document
        .getElementById("calculateBtn")
        .addEventListener("click", calculate);
      document.getElementById("resetBtn").addEventListener("click", resetForm);
      unitSystemEl.addEventListener("change", () => {
        updateUnitLabels();
        calculate();
      });
      columnShapeEl.addEventListener("change", () => {
        updateShapeVisibility();
        calculate();
      });

      // Initial setup
      updateUnitLabels();
      updateShapeVisibility();
      calculate();
    </script>

