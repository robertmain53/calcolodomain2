      let currentWords = [...DEFAULT_WORDS];
      let baseWords = [...DEFAULT_WORDS];

      // --- Quick pattern filter ---
      function applyQuickFilter() {
        const patternRaw = document
          .getElementById("pattern-input")
          .value.trim()
          .toUpperCase();
        const includeRaw = document
          .getElementById("include-input")
          .value.trim()
          .toUpperCase();
        const excludeRaw = document
          .getElementById("exclude-input")
          .value.trim()
          .toUpperCase();

        const pattern = patternRaw.padEnd(5, ".").slice(0, 5);
        const includeSet = new Set(includeRaw.replace(/[^A-Z]/g, "").split(""));
        const excludeSet = new Set(excludeRaw.replace(/[^A-Z]/g, "").split(""));

        currentWords = baseWords.filter((word) => {
          // pattern
          for (let i = 0; i < 5; i++) {
            const p = pattern[i];
            if (p && p !== "." && p !== "?" && word[i] !== p) return false;
          }
          // include
          for (const ch of includeSet) {
            if (!word.includes(ch)) return false;
          }
          // exclude (only if not also in include)
          for (const ch of excludeSet) {
            if (!includeSet.has(ch) && word.includes(ch)) return false;
          }
          return true;
        });

        document.getElementById("quick-filter-count").textContent =
          currentWords.length
            ? `${currentWords.length} words match current filters`
            : "No words match current filters";
        renderSuggestions();
      }

      function clearQuickFilter() {
        document.getElementById("pattern-input").value = "";
        document.getElementById("include-input").value = "";
        document.getElementById("exclude-input").value = "";
        currentWords = [...baseWords];
        document.getElementById("quick-filter-count").textContent = "";
        renderSuggestions();
      }

      // --- Grid-based solver ---
      const MAX_GUESSES = 6;
      const guessRowsContainer = document.getElementById(
        "guess-rows-container"
      );

      function createGuessRow(index) {
        const row = document.createElement("div");
        row.className = "flex items-center gap-2";
        row.dataset.index = index;

        const label = document.createElement("span");
        label.className = "w-5 text-xs text-gray-500";
        label.textContent = index + 1;

        const tilesWrapper = document.createElement("div");
        tilesWrapper.className = "flex gap-1";

        for (let i = 0; i < 5; i++) {
          const tile = document.createElement("button");
          tile.type = "button";
          tile.className =
            "tile border border-gray-300 bg-white text-gray-800 text-base font-semibold";
          tile.dataset.state = "blank";
          tile.dataset.letter = "";
          tile.textContent = "";
          tile.addEventListener("click", () => cycleTileState(tile));
          tilesWrapper.appendChild(tile);
        }

        const input = document.createElement("input");
        input.type = "text";
        input.maxLength = 5;
        input.placeholder = "GUESS";
        input.className =
          "ml-2 w-24 border rounded-md px-2 py-1 text-sm font-mono uppercase focus:ring-2 focus:ring-blue-500 focus:border-blue-500";
        input.addEventListener("input", () => syncInputToTiles(row, input));

        row.appendChild(label);
        row.appendChild(tilesWrapper);
        row.appendChild(input);
        return row;
      }

      function cycleTileState(tile) {
        const states = ["blank", "gray", "yellow", "green"];
        let idx = states.indexOf(tile.dataset.state || "blank");
        idx = (idx + 1) % states.length;
        const next = states[idx];
        tile.dataset.state = next;

        tile.classList.remove("tile-gray", "tile-yellow", "tile-green");
        tile.classList.remove("border-gray-300", "border-transparent");
        tile.classList.add("border-gray-300");

        if (next === "blank") {
          tile.textContent = tile.dataset.letter || "";
          tile.style.backgroundColor = "white";
          tile.style.color = "#111827";
        } else if (next === "gray") {
          tile.classList.add("tile-gray");
          tile.classList.remove("border-gray-300");
          tile.classList.add("border-transparent");
        } else if (next === "yellow") {
          tile.classList.add("tile-yellow");
          tile.classList.remove("border-gray-300");
          tile.classList.add("border-transparent");
        } else if (next === "green") {
          tile.classList.add("tile-green");
          tile.classList.remove("border-gray-300");
          tile.classList.add("border-transparent");
        }
      }

      function syncInputToTiles(row, input) {
        const value = input.value.toUpperCase().replace(/[^A-Z]/g, "");
        input.value = value;
        const tiles = row.querySelectorAll(".tile");
        for (let i = 0; i < 5; i++) {
          const ch = value[i] || "";
          const tile = tiles[i];
          tile.dataset.letter = ch;
          if (tile.dataset.state === "blank") {
            tile.textContent = ch;
          }
        }
      }

      function initGuessRows() {
        guessRowsContainer.innerHTML = "";
        for (let i = 0; i < MAX_GUESSES; i++) {
          guessRowsContainer.appendChild(createGuessRow(i));
        }
      }

      function collectConstraintsFromRows() {
        const greens = Array(5).fill(null);
        const yellows = []; // {letter, position}
        const presentLetters = new Set();
        const absentLetters = new Set();

        const rows = guessRowsContainer.querySelectorAll("[data-index]");
        rows.forEach((row) => {
          const tiles = row.querySelectorAll(".tile");
          const letters = [];
          const states = [];

          tiles.forEach((tile) => {
            letters.push((tile.dataset.letter || "").toUpperCase());
            states.push(tile.dataset.state || "blank");
          });

          const hasAnyColor = states.some(
            (s) => s === "gray" || s === "yellow" || s === "green"
          );
          if (!hasAnyColor) return;

          // Greens and yellows
          for (let i = 0; i < 5; i++) {
            const ch = letters[i];
            const st = states[i];
            if (!ch) continue;
            if (st === "green") {
              greens[i] = ch;
              presentLetters.add(ch);
            } else if (st === "yellow") {
              yellows.push({ letter: ch, position: i });
              presentLetters.add(ch);
            }
          }

          // Grays (only if letter never yellow/green in this row)
          for (let i = 0; i < 5; i++) {
            const ch = letters[i];
            const st = states[i];
            if (!ch || st !== "gray") continue;
            const appearsColoredElsewhere = letters.some(
              (c, idx) =>
                c === ch &&
                (states[idx] === "yellow" || states[idx] === "green")
            );
            if (!appearsColoredElsewhere) {
              absentLetters.add(ch);
            }
          }
        });

        return { greens, yellows, presentLetters, absentLetters };
      }

      function applyGridFilter() {
        const { greens, yellows, presentLetters, absentLetters } =
          collectConstraintsFromRows();
        const hardMode = document.getElementById("hard-mode-toggle").checked;

        currentWords = currentWords.filter((word) => {
          // Greens
          for (let i = 0; i < 5; i++) {
            if (greens[i] && word[i] !== greens[i]) return false;
          }
          // Yellows
          for (const y of yellows) {
            if (word[y.position] === y.letter) return false;
            if (!word.includes(y.letter)) return false;
          }
          // Absent letters (unless also present somewhere)
          for (const ch of absentLetters) {
            if (presentLetters.has(ch)) continue;
            if (word.includes(ch)) return false;
          }
          // Hard mode: must contain all present letters
          if (hardMode) {
            for (const ch of presentLetters) {
              if (!word.includes(ch)) return false;
            }
          }
          return true;
        });

        const remainingCountEl = document.getElementById("remaining-count");
        remainingCountEl.textContent = currentWords.length
          ? `${currentWords.length} possible word${
              currentWords.length === 1 ? "" : "s"
            } remain`
          : "No words remain – check your inputs.";

        renderSuggestions();
      }

      // --- Suggestion ranking (approximate information score) ---
      function scoreWord(word, candidates) {
        // Frequency of letters in remaining candidates
        const freq = {};
        for (const w of candidates) {
          const seen = new Set();
          for (const ch of w) {
            if (!seen.has(ch)) {
              seen.add(ch);
              freq[ch] = (freq[ch] || 0) + 1;
            }
          }
        }
        const total = candidates.length || 1;

        let score = 0;
        const seenLetters = new Set();
        for (const ch of word) {
          if (seenLetters.has(ch)) {
            score += 0.1; // small bonus for duplicates
          } else {
            seenLetters.add(ch);
            const f = (freq[ch] || 0) / total;
            score += f; // more common letters get higher score
          }
        }
        return score;
      }

      function renderSuggestions() {
        const container = document.getElementById("suggestions-container");
        const showInfo = document.getElementById(
          "show-info-score-toggle"
        ).checked;

        if (!currentWords.length) {
          container.innerHTML =
            '<p class="text-sm text-red-600">No words match the current constraints. Try relaxing your inputs.</p>';
          return;
        }

        // If many words, score only a subset for performance
        const candidates = currentWords;
        const toScore =
          candidates.length > 300 ? candidates.slice(0, 300) : candidates;

        const scored = toScore.map((w) => ({
          word: w,
          score: scoreWord(w, candidates),
        }));
        scored.sort((a, b) => b.score - a.score);

        const top = scored.slice(0, 30);
        const html = top
          .map(
            (item) => `
      <div class="flex items-center justify-between py-0.5">
        <span class="font-mono tracking-wide">${item.word}</span>
        ${
          showInfo
            ? `<span class="text-[0.7rem] text-gray-500">score: ${item.score.toFixed(
                2
              )}</span>`
            : ""
        }
      </div>
    `
          )
          .join("");

        container.innerHTML =
          html ||
          '<p class="text-sm text-gray-600">No suggestions available.</p>';
      }

      // --- Custom word list ---
      function applyCustomWordlist() {
        const raw = document.getElementById("custom-wordlist-input").value;
        const lines = raw
          .split(/\r?\n/)
          .map((l) => l.trim().toUpperCase())
          .filter(Boolean);
        const words = Array.from(
          new Set(lines.filter((w) => /^[A-Z]{5}$/.test(w)))
        );
        const status = document.getElementById("wordlist-status");

        if (!words.length) {
          status.textContent = "No valid 5-letter words found in custom list.";
          status.className = "text-xs text-red-600";
          return;
        }

        baseWords = words;
        currentWords = [...baseWords];
        status.textContent = `Using custom list (${words.length} words).`;
        status.className = "text-xs text-emerald-700";
        document.getElementById("quick-filter-count").textContent = "";
        renderSuggestions();
      }

      function resetWordlist() {
        baseWords = [...DEFAULT_WORDS];
        currentWords = [...DEFAULT_WORDS];
        document.getElementById("custom-wordlist-input").value = "";
        const status = document.getElementById("wordlist-status");
        status.textContent = "Reset to default dictionary.";
        status.className = "text-xs text-gray-600";
        document.getElementById("quick-filter-count").textContent = "";
        renderSuggestions();
      }

      function resetSolver() {
        currentWords = [...baseWords];
        initGuessRows();
        document.getElementById("remaining-count").textContent = "";
        document.getElementById("quick-filter-count").textContent = "";
        renderSuggestions();
      }

      // --- Event bindings ---
      document.addEventListener("DOMContentLoaded", () => {
        initGuessRows();
        renderSuggestions();

        document
          .getElementById("quick-filter-btn")
          .addEventListener("click", applyQuickFilter);
        document
          .getElementById("quick-clear-btn")
          .addEventListener("click", clearQuickFilter);
        document
          .getElementById("apply-feedback-btn")
          .addEventListener("click", applyGridFilter);
        document
          .getElementById("reset-solver-btn")
          .addEventListener("click", resetSolver);
        document
          .getElementById("apply-custom-wordlist-btn")
          .addEventListener("click", applyCustomWordlist);
        document
          .getElementById("reset-wordlist-btn")
          .addEventListener("click", resetWordlist);
        document
          .getElementById("show-info-score-toggle")
          .addEventListener("change", renderSuggestions);
      });
    </script>

