    <script>
      // Approximate theobromine concentrations (mg/g)
      const THEOBROMINE_MG_PER_G = {
        white: 0.05, // effectively negligible
        milk: 2.0,
        dark: 7.0,
        baking: 16.0,
        cocoa: 20.0,
        unknown: 5.0, // conservative middle value
      };

      function formatNumber(value, decimals) {
        if (!isFinite(value)) return "–";
        return value.toFixed(decimals);
      }

      function getRiskCategory(doseMgKg) {
        if (!isFinite(doseMgKg) || doseMgKg <= 0) {
          return {
            label: "Unknown",
            badgeClass: "badge bg-gray-200 text-gray-800",
            advice:
              "Enter your cat’s weight, chocolate type, and amount eaten, then click “Calculate risk”.",
          };
        }

        if (doseMgKg < 10) {
          return {
            label: "Very low / unlikely in most cats",
            badgeClass: "badge bg-green-100 text-green-800",
            advice:
              "Toxicity is unlikely in most healthy cats at this dose, but mild stomach upset is still possible. Monitor your cat for vomiting, diarrhea, or restlessness for the next 24 hours and contact your vet if any signs appear or if you are worried.",
          };
        } else if (doseMgKg < 20) {
          return {
            label: "Low to mild risk",
            badgeClass: "badge bg-yellow-100 text-yellow-800",
            advice:
              "This dose may cause mild signs such as vomiting, diarrhea, or restlessness. Call your regular veterinarian or a pet poison hotline for advice, especially if your cat is very young, old, or has heart, kidney, or seizure problems.",
          };
        } else if (doseMgKg < 40) {
          return {
            label: "Mild to moderate risk",
            badgeClass: "badge bg-orange-100 text-orange-800",
            advice:
              "This dose can cause significant signs of chocolate poisoning. Contact a veterinarian or emergency clinic immediately. Early treatment (such as inducing vomiting and giving activated charcoal) is often most effective within a few hours of ingestion.",
          };
        } else if (doseMgKg < 80) {
          return {
            label: "Moderate to severe risk",
            badgeClass: "badge bg-red-100 text-red-800",
            advice:
              "This is a potentially serious exposure. Call an emergency veterinary clinic or pet poison hotline right away. Your cat may need hospital monitoring, IV fluids, and medications to control heart rhythm, blood pressure, and seizures.",
          };
        } else {
          return {
            label: "Emergency / life‑threatening risk",
            badgeClass: "badge bg-red-600 text-white",
            advice:
              "This dose is in the range associated with life‑threatening toxicity. Seek emergency veterinary care immediately. Bring the chocolate packaging and tell the vet when and how much your cat may have eaten.",
          };
        }
      }

      document
        .getElementById("calc-btn")
        .addEventListener("click", function () {
          const weightInput = parseFloat(
            document.getElementById("cat-weight").value
          );
          const weightUnit = document.getElementById("weight-unit").value;
          const chocType = document.getElementById("choc-type").value;
          const amountInput = parseFloat(
            document.getElementById("choc-amount").value
          );
          const amountUnit = document.getElementById("amount-unit").value;
          const includeCaffeine =
            document.getElementById("include-caffeine").checked;

          const warningEl = document.getElementById("results-warning");
          warningEl.classList.add("hidden");

          if (
            !weightInput ||
            weightInput <= 0 ||
            !amountInput ||
            amountInput <= 0
          ) {
            warningEl.textContent =
              "Please enter a positive value for both cat weight and chocolate amount.";
            warningEl.classList.remove("hidden");
            document.getElementById("dose-mgkg").textContent = "–";
            document.getElementById("dose-total").textContent = "–";
            document.getElementById("risk-label").textContent = "–";
            document.getElementById("risk-badge").innerHTML = "";
            document.getElementById("advice-text").textContent =
              "Enter your cat’s weight, chocolate type, and amount eaten, then click “Calculate risk”.";
            return;
          }

          // Convert weight to kg
          const weightKg =
            weightUnit === "kg" ? weightInput : weightInput * 0.45359237;

          // Convert amount to grams
          const amountG =
            amountUnit === "g" ? amountInput : amountInput * 28.3495231;

          // Get base theobromine concentration
          let conc =
            THEOBROMINE_MG_PER_G[chocType] || THEOBROMINE_MG_PER_G.unknown;

          // Optionally adjust for caffeine (rough +10% to represent combined effect)
          if (includeCaffeine) {
            conc *= 1.1;
          }

          const totalTheobromineMg = amountG * conc;
          const doseMgKg = totalTheobromineMg / weightKg;

          const risk = getRiskCategory(doseMgKg);

          document.getElementById("dose-mgkg").textContent = formatNumber(
            doseMgKg,
            doseMgKg < 10 ? 2 : 1
          );
          document.getElementById("dose-total").textContent = formatNumber(
            totalTheobromineMg,
            totalTheobromineMg < 10 ? 2 : 1
          );
          document.getElementById("risk-label").textContent = risk.label;

          const badgeEl = document.getElementById("risk-badge");
          badgeEl.className = "";
          badgeEl.classList.add(...risk.badgeClass.split(" "));
          badgeEl.textContent = risk.label;

          document.getElementById("advice-text").textContent = risk.advice;
        });
    </script>

