    <script>
      // Mode toggle
      const modeBasicBtn = document.getElementById("mode-basic");
      const modeDupontBtn = document.getElementById("mode-dupont");
      const basicForm = document.getElementById("basic-form");
      const dupontForm = document.getElementById("dupont-form");

      function setMode(mode) {
        if (mode === "basic") {
          basicForm.classList.remove("hidden");
          dupontForm.classList.add("hidden");
          modeBasicBtn.classList.add("toggle-btn-active");
          modeBasicBtn.classList.remove("toggle-btn-inactive");
          modeDupontBtn.classList.add("toggle-btn-inactive");
          modeDupontBtn.classList.remove("toggle-btn-active");
        } else {
          basicForm.classList.add("hidden");
          dupontForm.classList.remove("hidden");
          modeDupontBtn.classList.add("toggle-btn-active");
          modeDupontBtn.classList.remove("toggle-btn-inactive");
          modeBasicBtn.classList.add("toggle-btn-inactive");
          modeBasicBtn.classList.remove("toggle-btn-active");
        }
        // Clear results when switching mode
        document.getElementById("roeResult").textContent = "ROE: –";
        document.getElementById("roeSummary").textContent = "";
        document.getElementById("dupontBreakdown").classList.add("hidden");
      }

      modeBasicBtn.addEventListener("click", () => setMode("basic"));
      modeDupontBtn.addEventListener("click", () => setMode("dupont"));

      // Helper: compute average equity
      document
        .getElementById("btnComputeAvgEquity")
        .addEventListener("click", () => {
          const beg = parseFloat(document.getElementById("begEquity").value);
          const end = parseFloat(document.getElementById("endEquity").value);
          if (isFinite(beg) && isFinite(end)) {
            const avg = (beg + end) / 2;
            document.getElementById("avgEquity").value = avg.toFixed(2);
          }
        });

      // Helper: compute DuPont components from FS data
      document
        .getElementById("btnComputeDupontComponents")
        .addEventListener("click", () => {
          const rev = parseFloat(document.getElementById("dupRevenue").value);
          const ni = parseFloat(document.getElementById("dupNetIncome").value);
          const assets = parseFloat(document.getElementById("dupAssets").value);
          const equity = parseFloat(document.getElementById("dupEquity").value);

          if (isFinite(rev) && rev !== 0 && isFinite(ni)) {
            const pm = (ni / rev) * 100;
            document.getElementById("profitMargin").value = pm.toFixed(2);
          }
          if (isFinite(rev) && isFinite(assets) && assets !== 0) {
            const at = rev / assets;
            document.getElementById("assetTurnover").value = at.toFixed(3);
          }
          if (isFinite(assets) && isFinite(equity) && equity !== 0) {
            const em = assets / equity;
            document.getElementById("equityMultiplier").value = em.toFixed(3);
          }
        });

      // Main calculation
      document
        .getElementById("btnCalculateRoe")
        .addEventListener("click", () => {
          const roeResultEl = document.getElementById("roeResult");
          const roeSummaryEl = document.getElementById("roeSummary");
          const dupontBox = document.getElementById("dupontBreakdown");
          const outPM = document.getElementById("outProfitMargin");
          const outAT = document.getElementById("outAssetTurnover");
          const outEM = document.getElementById("outEquityMultiplier");
          const dupNote = document.getElementById("dupontNote");

          let roePercent = null;
          let mode = basicForm.classList.contains("hidden")
            ? "dupont"
            : "basic";

          if (mode === "basic") {
            const ni = parseFloat(document.getElementById("netIncome").value);
            const eq = parseFloat(document.getElementById("avgEquity").value);

            if (!isFinite(ni) || !isFinite(eq) || eq === 0) {
              roeResultEl.textContent = "ROE: –";
              roeSummaryEl.textContent =
                "Please enter valid values for net income and average equity (equity must be non-zero).";
              dupontBox.classList.add("hidden");
              return;
            }
            roePercent = (ni / eq) * 100;
            roeResultEl.textContent = "ROE: " + roePercent.toFixed(2) + "%";
            roeSummaryEl.textContent = interpretRoe(roePercent);
            dupontBox.classList.add("hidden");
          } else {
            const pm = parseFloat(
              document.getElementById("profitMargin").value
            ); // %
            const at = parseFloat(
              document.getElementById("assetTurnover").value
            );
            const em = parseFloat(
              document.getElementById("equityMultiplier").value
            );

            if (!isFinite(pm) || !isFinite(at) || !isFinite(em)) {
              roeResultEl.textContent = "ROE: –";
              roeSummaryEl.textContent =
                "Please enter valid values for profit margin, asset turnover, and equity multiplier.";
              dupontBox.classList.add("hidden");
              return;
            }
            const pmDecimal = pm / 100;
            roePercent = pmDecimal * at * em * 100;
            roeResultEl.textContent = "ROE: " + roePercent.toFixed(2) + "%";
            roeSummaryEl.textContent = interpretRoe(roePercent);

            outPM.textContent = pm.toFixed(2) + " %";
            outAT.textContent = at.toFixed(3) + " ×";
            outEM.textContent = em.toFixed(3) + " ×";
            dupNote.textContent =
              "ROE = Profit margin × Asset turnover × Equity multiplier = " +
              pm.toFixed(2) +
              "% × " +
              at.toFixed(3) +
              " × " +
              em.toFixed(3) +
              " ≈ " +
              roePercent.toFixed(2) +
              "%.";
            dupontBox.classList.remove("hidden");
          }
        });

      // Reset
      document.getElementById("btnResetRoe").addEventListener("click", () => {
        document
          .querySelectorAll("#basic-form input, #dupont-form input")
          .forEach((inp) => (inp.value = ""));
        document.getElementById("roeResult").textContent = "ROE: –";
        document.getElementById("roeSummary").textContent = "";
        document.getElementById("dupontBreakdown").classList.add("hidden");
      });

      function interpretRoe(roe) {
        if (!isFinite(roe)) return "";
        const abs = Math.abs(roe);
        if (roe < 0) {
          return "Negative ROE: the company reported a net loss relative to its equity during this period.";
        } else if (abs < 5) {
          return "Low ROE: this level of profitability on equity is modest and may be below the cost of equity in many industries.";
        } else if (abs < 15) {
          return "Moderate ROE: this is a reasonable return on equity in many capital‑intensive or mature industries.";
        } else if (abs < 25) {
          return "Strong ROE: this suggests the company is generating attractive returns on shareholders’ equity.";
        } else {
          return "Very high ROE: investigate whether this is driven by sustainable profitability or by high leverage / unusually low equity.";
        }
      }

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
