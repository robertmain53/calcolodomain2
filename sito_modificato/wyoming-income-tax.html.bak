    <script>
      // Simple federal tax brackets for current year (example values; update annually)
      const FEDERAL_BRACKETS = {
        single: [
          { upTo: 11000, rate: 0.1 },
          { upTo: 44725, rate: 0.12 },
          { upTo: 95375, rate: 0.22 },
          { upTo: 182100, rate: 0.24 },
          { upTo: 231250, rate: 0.32 },
          { upTo: 578125, rate: 0.35 },
          { upTo: Infinity, rate: 0.37 },
        ],
        married_joint: [
          { upTo: 22000, rate: 0.1 },
          { upTo: 89450, rate: 0.12 },
          { upTo: 190750, rate: 0.22 },
          { upTo: 364200, rate: 0.24 },
          { upTo: 462500, rate: 0.32 },
          { upTo: 693750, rate: 0.35 },
          { upTo: Infinity, rate: 0.37 },
        ],
        married_separate: [
          { upTo: 11000, rate: 0.1 },
          { upTo: 44725, rate: 0.12 },
          { upTo: 95375, rate: 0.22 },
          { upTo: 182100, rate: 0.24 },
          { upTo: 231250, rate: 0.32 },
          { upTo: 346875, rate: 0.35 },
          { upTo: Infinity, rate: 0.37 },
        ],
        hoh: [
          { upTo: 15700, rate: 0.1 },
          { upTo: 59850, rate: 0.12 },
          { upTo: 95350, rate: 0.22 },
          { upTo: 182100, rate: 0.24 },
          { upTo: 231250, rate: 0.32 },
          { upTo: 578100, rate: 0.35 },
          { upTo: Infinity, rate: 0.37 },
        ],
      };

      const STANDARD_DEDUCTION = {
        single: 14600,
        married_joint: 29200,
        married_separate: 14600,
        hoh: 21900,
      };

      const SS_RATE = 0.062;
      const MEDICARE_RATE = 0.0145;
      const SS_WAGE_BASE = 168600; // update annually

      function formatCurrency(n) {
        if (!isFinite(n)) return "$0";
        return n.toLocaleString("en-US", {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 0,
        });
      }

      function formatPercent(p) {
        if (!isFinite(p)) return "0%";
        return (p * 100).toFixed(1) + "%";
      }

      function calcFederalTax(taxable, status) {
        const brackets = FEDERAL_BRACKETS[status] || FEDERAL_BRACKETS.single;
        let remaining = taxable;
        let lastCap = 0;
        let tax = 0;

        for (const b of brackets) {
          if (remaining <= 0) break;
          const band = Math.min(remaining, b.upTo - lastCap);
          tax += band * b.rate;
          remaining -= band;
          lastCap = b.upTo;
        }
        return Math.max(0, tax);
      }

      function calcFica(wages) {
        const ssTaxable = Math.min(wages, SS_WAGE_BASE);
        const ss = ssTaxable * SS_RATE;
        const medicare = wages * MEDICARE_RATE;
        return ss + medicare;
      }

      function updateBars(federal, fica, salesProp, net, income) {
        const total = income || 1;
        const pFed = Math.max(0, Math.min(100, (federal / total) * 100));
        const pFica = Math.max(0, Math.min(100, (fica / total) * 100));
        const pSales = Math.max(0, Math.min(100, (salesProp / total) * 100));
        const pNet = Math.max(0, Math.min(100, (net / total) * 100));

        document.getElementById("barFederal").style.width = pFed + "%";
        document.getElementById("barFica").style.width = pFica + "%";
        document.getElementById("barSalesProp").style.width = pSales + "%";
        document.getElementById("barNet").style.width = pNet + "%";
      }

      function runCalculation() {
        const status = document.getElementById("filingStatus").value;
        const income =
          parseFloat(document.getElementById("annualIncome").value) || 0;
        const preTax401k =
          parseFloat(document.getElementById("preTax401k").value) || 0;
        const otherPreTax =
          parseFloat(document.getElementById("otherPreTax").value) || 0;
        const salesPropRate =
          parseFloat(document.getElementById("salesPropRate").value) || 0;

        const totalPreTax = Math.min(preTax401k + otherPreTax, income);
        const agi = Math.max(0, income - totalPreTax);
        const stdDed = STANDARD_DEDUCTION[status] || STANDARD_DEDUCTION.single;
        const taxable = Math.max(0, agi - stdDed);

        const federalTax = calcFederalTax(taxable, status);
        const fica = calcFica(income);
        const salesPropTax = income * (salesPropRate / 100);
        const wyStateTax = 0;

        const totalTax = federalTax + fica + salesPropTax + wyStateTax;
        const takeHome = income - totalTax;
        const effRate = income > 0 ? totalTax / income : 0;

        document.getElementById("federalTaxable").textContent =
          formatCurrency(taxable);
        document.getElementById("federalTax").textContent =
          formatCurrency(federalTax);
        document.getElementById("ficaTax").textContent = formatCurrency(fica);
        document.getElementById("salesPropTax").textContent =
          formatCurrency(salesPropTax);
        document.getElementById("totalTax").textContent =
          formatCurrency(totalTax);
        document.getElementById("effectiveRate").textContent =
          formatPercent(effRate);
        document.getElementById("takeHome").textContent =
          formatCurrency(takeHome);

        updateBars(federalTax, fica, salesPropTax, takeHome, income);
      }

      document.addEventListener("DOMContentLoaded", () => {
        const salesSlider = document.getElementById("salesPropRate");
        const salesOutput = document.getElementById("salesPropRateOutput");
        salesSlider.addEventListener("input", () => {
          salesOutput.textContent =
            parseFloat(salesSlider.value).toFixed(1) + "%";
        });

        document
          .getElementById("calcBtn")
          .addEventListener("click", runCalculation);

        // Recalculate on key changes
        [
          "annualIncome",
          "preTax401k",
          "otherPreTax",
          "salesPropRate",
          "filingStatus",
        ].forEach((id) => {
          const el = document.getElementById(id);
          el.addEventListener("change", runCalculation);
          if (el.tagName === "INPUT") {
            el.addEventListener("keyup", (e) => {
              if (["Enter", "NumpadEnter"].includes(e.key)) runCalculation();
            });
          }
        });

        runCalculation();
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
