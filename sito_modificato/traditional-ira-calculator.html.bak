    <script>
      function fmtMoney(value) {
        if (!isFinite(value)) return "$0";
        const abs = Math.abs(value);
        let suffix = "";
        let num = value;
        if (abs >= 1e9) {
          num = value / 1e9;
          suffix = "B";
        } else if (abs >= 1e6) {
          num = value / 1e6;
          suffix = "M";
        } else if (abs >= 1e3) {
          num = value / 1e3;
          suffix = "k";
        }
        return (
          "$" + num.toFixed(2).replace(/\B(?=(\d{3})+(?!\d))/g, ",") + suffix
        );
      }

      function clamp(val, min, max) {
        return Math.min(Math.max(val, min), max);
      }

      function calculateIRA() {
        const currentAge =
          parseFloat(document.getElementById("current-age").value) || 0;
        const retirementAge =
          parseFloat(document.getElementById("retirement-age").value) || 0;
        const currentBalance = Math.max(
          0,
          parseFloat(document.getElementById("current-balance").value) || 0
        );
        const annualContribution = Math.max(
          0,
          parseFloat(document.getElementById("annual-contribution").value) || 0
        );
        const yearsOfContrib = Math.max(
          0,
          parseFloat(document.getElementById("years-of-contrib").value) || 0
        );

        let grossReturn =
          parseFloat(document.getElementById("pre-tax-rate").value) || 0;
        let feeRate =
          parseFloat(document.getElementById("annual-fee").value) || 0;
        let currentTaxRate =
          parseFloat(document.getElementById("current-tax-rate").value) || 0;
        let retirementTaxRate =
          parseFloat(document.getElementById("retirement-tax-rate").value) || 0;
        let deductiblePercent =
          parseFloat(document.getElementById("deductible-percent").value) || 0;

        grossReturn = clamp(grossReturn, 0, 20);
        feeRate = clamp(feeRate, 0, 5);
        currentTaxRate = clamp(currentTaxRate, 0, 60);
        retirementTaxRate = clamp(retirementTaxRate, 0, 60);
        deductiblePercent = clamp(deductiblePercent, 0, 100);

        const yearsToRetirement = Math.max(0, retirementAge - currentAge);
        const n = yearsOfContrib > 0 ? yearsOfContrib : yearsToRetirement;

        const rNet = (grossReturn - feeRate) / 100;
        const rTaxable = Math.max(
          0,
          (grossReturn * (1 - currentTaxRate / 100) - feeRate) / 100
        );

        const iraBalance = (() => {
          if (n <= 0 || rNet === 0) {
            return currentBalance + annualContribution * n;
          }
          const fvCurrent = currentBalance * Math.pow(1 + rNet, n);
          const fvContrib =
            annualContribution * ((Math.pow(1 + rNet, n) - 1) / rNet);
          return fvCurrent + fvContrib;
        })();

        const deductibleAnnual = annualContribution * (deductiblePercent / 100);
        const annualTaxSavings = deductibleAnnual * (currentTaxRate / 100);
        const totalTaxSavings = annualTaxSavings * n;

        const afterTaxIRA = iraBalance * (1 - retirementTaxRate / 100);

        const afterTaxContribution =
          annualContribution * (1 - currentTaxRate / 100);

        const taxableBalance = (() => {
          if (n <= 0 || rTaxable === 0) {
            return afterTaxContribution * n;
          }
          const fvCurrent = 0;
          const fvContrib =
            afterTaxContribution * ((Math.pow(1 + rTaxable, n) - 1) / rTaxable);
          return fvCurrent + fvContrib;
        })();

        const rothBalance = (() => {
          if (n <= 0 || rNet === 0) {
            return afterTaxContribution * n;
          }
          const fvContrib =
            afterTaxContribution * ((Math.pow(1 + rNet, n) - 1) / rNet);
          return fvContrib;
        })();

        document.getElementById("ira-balance").textContent =
          fmtMoney(iraBalance);
        document.getElementById("ira-years").textContent =
          n > 0 ? `Over ${n.toFixed(0)} year(s) of contributions` : "";
        document.getElementById("tax-savings").textContent =
          fmtMoney(totalTaxSavings);
        document.getElementById("after-tax-ira").textContent =
          fmtMoney(afterTaxIRA);
        document.getElementById(
          "after-tax-note"
        ).textContent = `Assuming a ${retirementTaxRate.toFixed(
          0
        )}% tax rate in retirement.`;

        document.getElementById("taxable-balance").textContent =
          fmtMoney(taxableBalance);
        document.getElementById("roth-balance").textContent =
          fmtMoney(rothBalance);

        const insights = [];
        if (afterTaxIRA > rothBalance * 1.05) {
          insights.push(
            "With your assumptions, a traditional IRA produces a higher estimated after-tax value than a Roth-style account."
          );
        } else if (rothBalance > afterTaxIRA * 1.05) {
          insights.push(
            "With your assumptions, a Roth-style account produces a higher estimated after-tax value than a traditional IRA."
          );
        } else {
          insights.push(
            "With your assumptions, traditional and Roth-style accounts produce similar after-tax values."
          );
        }

        if (totalTaxSavings > 0) {
          insights.push(
            "Your estimated tax savings from deductible contributions are significant over time. Consider how you will use those savings (e.g., reinvest or spend)."
          );
        } else {
          insights.push(
            "You entered 0% deductibility, so this behaves more like a non-deductible IRA. In that case, a Roth IRA may be more attractive if you are eligible."
          );
        }

        if (feeRate > 1) {
          insights.push(
            "Your fee assumption is relatively high. Lower-cost investments could materially increase your retirement balance."
          );
        }

        if (grossReturn <= 4) {
          insights.push(
            "You are using a conservative return assumption. That can be prudent for planning; try a range of returns to see best- and worst-case outcomes."
          );
        }

        const insightsList = document.getElementById("insights-list");
        insightsList.innerHTML = "";
        insights.forEach((text) => {
          const li = document.createElement("li");
          li.textContent = text;
          insightsList.appendChild(li);
        });

        document.getElementById("results-section").classList.remove("hidden");
      }

      document
        .getElementById("calculate-btn")
        .addEventListener("click", calculateIRA);

      document.getElementById("reset-btn").addEventListener("click", () => {
        document.getElementById("current-age").value = 35;
        document.getElementById("retirement-age").value = 67;
        document.getElementById("current-balance").value = 20000;
        document.getElementById("annual-contribution").value = 6000;
        document.getElementById("years-of-contrib").value = 30;
        document.getElementById("pre-tax-rate").value = 7;
        document.getElementById("annual-fee").value = 0.5;
        document.getElementById("current-tax-rate").value = 24;
        document.getElementById("retirement-tax-rate").value = 22;
        document.getElementById("deductible-percent").value = 100;
        document.getElementById("results-section").classList.add("hidden");
      });

      window.addEventListener("DOMContentLoaded", () => {
        calculateIRA();
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
