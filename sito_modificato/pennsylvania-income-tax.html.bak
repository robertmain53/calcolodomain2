    <script>
      // Helper: format currency
      function formatCurrency(value) {
        if (!isFinite(value)) return "$0.00";
        return value.toLocaleString("en-US", {
          style: "currency",
          currency: "USD",
        });
      }

      // Helper: round to 2 decimals
      function round2(num) {
        return Math.round((num + Number.EPSILON) * 100) / 100;
      }

      function getAnnualIncome(gross, freq, hoursPerWeek) {
        switch (freq) {
          case "annual":
            return gross;
          case "monthly":
            return gross * 12;
          case "biweekly":
            return gross * 26;
          case "weekly":
            return gross * 52;
          case "hourly":
            const hours = isNaN(hoursPerWeek) ? 0 : hoursPerWeek;
            return gross * hours * 52;
          default:
            return gross;
        }
      }

      function getPeriodDivisor(freq) {
        switch (freq) {
          case "annual":
            return 1;
          case "monthly":
            return 12;
          case "biweekly":
            return 26;
          case "weekly":
            return 52;
          case "hourly":
            return 52; // we treat hourly as weekly pay aggregated over year
          default:
            return 1;
        }
      }

      function calculatePaTax() {
        const incomeInput = document.getElementById("income");
        const freqSelect = document.getElementById("frequency");
        const hoursInput = document.getElementById("hoursPerWeek");
        const localRateInput = document.getElementById("localRate");
        const pretaxInput = document.getElementById("pretax");

        const gross = parseFloat(incomeInput.value) || 0;
        const freq = freqSelect.value;
        const hoursPerWeek = parseFloat(hoursInput.value) || 0;
        const localRate = parseFloat(localRateInput.value) || 0;
        const pretaxPerPeriod = parseFloat(pretaxInput.value) || 0;

        if (gross <= 0) {
          updateResults(0, 0, 0, 0);
          return;
        }

        const annualGross = getAnnualIncome(gross, freq, hoursPerWeek);
        const divisor = getPeriodDivisor(freq);

        const annualPretax = pretaxPerPeriod * divisor;
        const annualTaxable = Math.max(0, annualGross - annualPretax);

        const stateRate = 0.0307;
        const localRateDecimal = localRate / 100;

        const annualStateTax = annualTaxable * stateRate;
        const annualLocalTax = annualTaxable * localRateDecimal;
        const annualTotalTax = annualStateTax + annualLocalTax;

        const annualNet = annualGross - annualTotalTax;
        const effectiveRate =
          annualGross > 0 ? (annualTotalTax / annualGross) * 100 : 0;

        const periodGross = annualGross / divisor;
        const periodTaxable = annualTaxable / divisor;
        const periodStateTax = annualStateTax / divisor;
        const periodLocalTax = annualLocalTax / divisor;
        const periodTotalTax = annualTotalTax / divisor;
        const periodNet = annualNet / divisor;

        updateResults(
          {
            annualGross,
            annualTaxable,
            annualStateTax,
            annualLocalTax,
            annualTotalTax,
            annualNet,
            effectiveRate,
          },
          {
            periodGross,
            periodTaxable,
            periodStateTax,
            periodLocalTax,
            periodTotalTax,
            periodNet,
          }
        );
      }

      function updateResults(annual, period) {
        const totalTaxPeriodEl = document.getElementById("totalTaxPeriod");
        const effectiveRateEl = document.getElementById("effectiveRate");
        const netPayPeriodEl = document.getElementById("netPayPeriod");
        const netPayAnnualEl = document.getElementById("netPayAnnual");
        const stateTaxPeriodEl = document.getElementById("stateTaxPeriod");
        const stateTaxAnnualEl = document.getElementById("stateTaxAnnual");
        const localTaxPeriodEl = document.getElementById("localTaxPeriod");
        const localTaxAnnualEl = document.getElementById("localTaxAnnual");
        const taxablePeriodEl = document.getElementById("taxablePeriod");
        const taxableAnnualEl = document.getElementById("taxableAnnual");

        if (!annual || !period) {
          totalTaxPeriodEl.textContent = "$0.00";
          effectiveRateEl.textContent = "Effective PA tax rate: 0.00%";
          netPayPeriodEl.textContent = "$0.00";
          netPayAnnualEl.textContent = "Annual net after PA taxes: $0.00";
          stateTaxPeriodEl.textContent = "$0.00 / period";
          stateTaxAnnualEl.textContent = "$0.00 per year";
          localTaxPeriodEl.textContent = "$0.00 / period";
          localTaxAnnualEl.textContent = "$0.00 per year";
          taxablePeriodEl.textContent = "$0.00 / period";
          taxableAnnualEl.textContent = "$0.00 per year";
          return;
        }

        totalTaxPeriodEl.textContent = formatCurrency(
          round2(period.periodTotalTax)
        );
        effectiveRateEl.textContent =
          "Effective PA tax rate: " +
          round2(annual.effectiveRate).toFixed(2) +
          "%";
        netPayPeriodEl.textContent = formatCurrency(round2(period.periodNet));
        netPayAnnualEl.textContent =
          "Annual net after PA taxes: " +
          formatCurrency(round2(annual.annualNet));

        stateTaxPeriodEl.textContent =
          formatCurrency(round2(period.periodStateTax)) + " / period";
        stateTaxAnnualEl.textContent =
          formatCurrency(round2(annual.annualStateTax)) + " per year";

        localTaxPeriodEl.textContent =
          formatCurrency(round2(period.periodLocalTax)) + " / period";
        localTaxAnnualEl.textContent =
          formatCurrency(round2(annual.annualLocalTax)) + " per year";

        taxablePeriodEl.textContent =
          formatCurrency(round2(period.periodTaxable)) + " / period";
        taxableAnnualEl.textContent =
          formatCurrency(round2(annual.annualTaxable)) + " per year";
      }

      document.addEventListener("DOMContentLoaded", function () {
        const freqSelect = document.getElementById("frequency");
        const hoursWrapper = document.getElementById("hours-per-week-wrapper");
        const calculateBtn = document.getElementById("calculateBtn");
        const resetBtn = document.getElementById("resetBtn");

        freqSelect.addEventListener("change", function () {
          if (this.value === "hourly") {
            hoursWrapper.classList.remove("hidden");
          } else {
            hoursWrapper.classList.add("hidden");
          }
        });

        calculateBtn.addEventListener("click", calculatePaTax);

        resetBtn.addEventListener("click", function () {
          document.getElementById("pa-tax-form").reset();
          document
            .getElementById("hours-per-week-wrapper")
            .classList.add("hidden");
          updateResults(0, 0);
        });

        // Auto-calc on input changes (debounced)
        let debounceTimer;
        document
          .getElementById("pa-tax-form")
          .addEventListener("input", function () {
            clearTimeout(debounceTimer);
            debounceTimer = setTimeout(calculatePaTax, 400);
          });

        // Set sensible defaults
        document.getElementById("income").value = 60000;
        document.getElementById("frequency").value = "annual";
        document.getElementById("localRate").value = 1.5;
        calculatePaTax();
      });
    </script>

