    <script>
      (function () {
        const MMHG_TO_PA = 133.322387415; // exact factor
        const mmhgInput = document.getElementById("mmhgInput");
        const paInput = document.getElementById("paInput");
        const swapBtn = document.getElementById("swapBtn");
        const factorDisplay = document.getElementById("factorDisplay");
        const mmhgClear = document.getElementById("mmhgClear");
        const paClear = document.getElementById("paClear");

        let mmhgIsSource = true;
        let isUpdating = false;

        function formatNumber(value) {
          if (!isFinite(value)) return "";
          const abs = Math.abs(value);
          if (abs === 0) return "0";
          if (abs >= 0.01 && abs < 1e6) {
            return value.toString().indexOf(".") === -1
              ? value.toString()
              : value.toFixed(6).replace(/\.?0+$/, "");
          }
          return value.toExponential(6).replace(/0+e/, "e").replace(/\.e/, "e");
        }

        function convertFromMMHg() {
          if (isUpdating) return;
          isUpdating = true;
          const v = parseFloat(mmhgInput.value);
          if (isNaN(v)) {
            paInput.value = "";
          } else {
            const pa = v * MMHG_TO_PA;
            paInput.value = formatNumber(pa);
          }
          isUpdating = false;
        }

        function convertFromPa() {
          if (isUpdating) return;
          isUpdating = true;
          const v = parseFloat(paInput.value);
          if (isNaN(v)) {
            mmhgInput.value = "";
          } else {
            const mmhg = v / MMHG_TO_PA;
            mmhgInput.value = formatNumber(mmhg);
          }
          isUpdating = false;
        }

        mmhgInput.addEventListener("input", function () {
          mmhgIsSource = true;
          convertFromMMHg();
        });

        paInput.addEventListener("input", function () {
          mmhgIsSource = false;
          convertFromPa();
        });

        swapBtn.addEventListener("click", function () {
          mmhgIsSource = !mmhgIsSource;
          if (mmhgIsSource) {
            factorDisplay.textContent = "1 mmHg = 133.322387415 Pa";
            convertFromMMHg();
          } else {
            factorDisplay.textContent = "1 Pa ≈ 0.00750061683 mmHg";
            convertFromPa();
          }
        });

        mmhgClear.addEventListener("click", function () {
          mmhgInput.value = "";
          convertFromMMHg();
        });

        paClear.addEventListener("click", function () {
          paInput.value = "";
          convertFromPa();
        });
      })();
    </script>

