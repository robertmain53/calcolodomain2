    <script>
      // Mode switching
      const modeSelect = document.getElementById("modeSelect");
      const sections = {
        framing: document.getElementById("mode-framing"),
        concrete: document.getElementById("mode-concrete"),
        roof: document.getElementById("mode-roof"),
        stairs: document.getElementById("mode-stairs"),
        materials: document.getElementById("mode-materials"),
      };

      function showMode(mode) {
        Object.keys(sections).forEach((key) => {
          sections[key].classList.toggle("hidden", key !== mode);
        });
      }
      modeSelect.addEventListener("change", () => showMode(modeSelect.value));
      showMode(modeSelect.value);

      // Helpers
      function toFeet(value, unit) {
        if (isNaN(value)) return 0;
        switch (unit) {
          case "ft":
            return value;
          case "m":
            return value * 3.280839895;
          default:
            return value;
        }
      }
      function toInches(value, unit) {
        if (isNaN(value)) return 0;
        switch (unit) {
          case "in":
            return value;
          case "mm":
            return value / 25.4;
          case "cm":
            return value / 2.54;
          case "ft":
            return value * 12;
          default:
            return value;
        }
      }
      function toMeters(value, unit) {
        if (isNaN(value)) return 0;
        switch (unit) {
          case "m":
            return value;
          case "ft":
            return value * 0.3048;
          default:
            return value;
        }
      }
      function toSquareFeetFrom(value, unit) {
        if (isNaN(value)) return 0;
        switch (unit) {
          case "sqft":
            return value;
          case "sqm":
            return value * 10.7639104167;
          default:
            return value;
        }
      }
      function formatNumber(x, decimals = 2) {
        return Number(x).toLocaleString(undefined, {
          maximumFractionDigits: decimals,
        });
      }

      // Framing calc
      document
        .getElementById("framingCalcBtn")
        .addEventListener("click", () => {
          const L = parseFloat(document.getElementById("framingLength").value);
          const Lunit = document.getElementById("framingLengthUnit").value;
          const S = parseFloat(document.getElementById("framingSpacing").value);
          const Sunit = document.getElementById("framingSpacingUnit").value;
          const extra = parseInt(
            document.getElementById("framingExtra").value || "0",
            10
          );
          const plates = parseInt(
            document.getElementById("framingPlates").value || "0",
            10
          );

          if (!L || !S) {
            document.getElementById("framingResult").textContent =
              "Enter wall length and spacing.";
            return;
          }

          const L_in = toInches(L, Lunit === "ft" ? "ft" : "m"); // convert via ft/m
          const spacing_in = toInches(S, Sunit);
          const spaces = Math.floor(L_in / spacing_in);
          const studs = spaces + 1 + extra;
          const plateLen_ft = toFeet(L, Lunit) * plates;

          document.getElementById(
            "framingResult"
          ).innerHTML = `<p><strong>Results:</strong></p>
       <ul class="list-disc ml-5">
         <li>Studs required (approx.): <strong>${formatNumber(
           studs,
           0
         )}</strong></li>
         <li>Plate length total: <strong>${formatNumber(
           plateLen_ft,
           2
         )} ft</strong> (${formatNumber(plateLen_ft / 16, 2)} pcs of 16 ft)</li>
       </ul>
       <p class="mt-1 text-gray-600 text-xs">Note: Always round up and add extra for waste, defects, and layout changes.</p>`;
        });

      // Concrete calc
      document.getElementById("concCalcBtn").addEventListener("click", () => {
        const L = parseFloat(document.getElementById("concLen").value);
        const W = parseFloat(document.getElementById("concWidth").value);
        const T = parseFloat(document.getElementById("concDepth").value);
        const Lunit = document.getElementById("concLenUnit").value;
        const Wunit = document.getElementById("concWidthUnit").value;
        const Tunit = document.getElementById("concDepthUnit").value;
        const waste = parseFloat(
          document.getElementById("concWaste").value || "0"
        );

        if (!L || !W || !T) {
          document.getElementById("concResult").textContent =
            "Enter length, width, and thickness.";
          return;
        }

        const L_ft = toFeet(L, Lunit);
        const W_ft = toFeet(W, Wunit);
        const T_ft = toFeet(T, Tunit === "in" ? "ft" : "ft"); // handle via inches/cm
        const T_ft_final =
          Tunit === "in"
            ? T / 12
            : Tunit === "cm"
            ? (T / 100) * 3.280839895
            : T_ft;

        const volume_ft3 = L_ft * W_ft * T_ft_final;
        const volume_ft3_waste = volume_ft3 * (1 + waste / 100);
        const volume_yd3 = volume_ft3_waste / 27;
        const volume_m3 = volume_ft3_waste * 0.0283168466;

        document.getElementById(
          "concResult"
        ).innerHTML = `<p><strong>Concrete volume (including waste):</strong></p>
       <ul class="list-disc ml-5">
         <li>${formatNumber(volume_ft3_waste, 2)} ft³</li>
         <li>${formatNumber(volume_yd3, 2)} yd³</li>
         <li>${formatNumber(volume_m3, 2)} m³</li>
       </ul>
       <p class="mt-1 text-gray-600 text-xs">Order is typically rounded up to the nearest 0.25 or 0.5 yd³ depending on your supplier.</p>`;
      });

      // Roof calc
      document.getElementById("roofCalcBtn").addEventListener("click", () => {
        const span = parseFloat(document.getElementById("roofSpan").value);
        const spanUnit = document.getElementById("roofSpanUnit").value;
        const risePer12 = parseFloat(
          document.getElementById("roofPitchRise").value
        );
        const overhang = parseFloat(
          document.getElementById("roofOverhang").value
        );
        const overhangUnit = document.getElementById("roofOverhangUnit").value;

        if (!span || !risePer12) {
          document.getElementById("roofResult").textContent =
            "Enter building span and roof pitch.";
          return;
        }

        const span_ft = toFeet(span, spanUnit);
        const halfSpan_ft = span_ft / 2;
        const overhang_ft = toFeet(
          overhang,
          overhangUnit === "in" ? "ft" : "ft"
        );
        const overhang_ft_final =
          overhangUnit === "in"
            ? overhang / 12
            : overhangUnit === "mm"
            ? (overhang / 1000) * 3.280839895
            : overhang_ft;

        const run_ft = halfSpan_ft + overhang_ft_final;
        const rise_ft = (risePer12 / 12) * run_ft;
        const rafter_ft = Math.sqrt(run_ft * run_ft + rise_ft * rise_ft);
        const pitch_deg = (Math.atan(risePer12 / 12) * 180) / Math.PI;

        document.getElementById(
          "roofResult"
        ).innerHTML = `<p><strong>Roof geometry:</strong></p>
       <ul class="list-disc ml-5">
         <li>Horizontal run: <strong>${formatNumber(run_ft, 2)} ft</strong></li>
         <li>Vertical rise: <strong>${formatNumber(rise_ft, 2)} ft</strong></li>
         <li>Rafter length (plumb): <strong>${formatNumber(
           rafter_ft,
           2
         )} ft</strong></li>
         <li>Pitch angle: <strong>${formatNumber(
           pitch_deg,
           1
         )}°</strong> (${formatNumber(risePer12, 2)} in / 12 in)</li>
       </ul>`;
      });

      // Stair calc
      document.getElementById("stairCalcBtn").addEventListener("click", () => {
        const H = parseFloat(document.getElementById("stairRiseTotal").value);
        const Hunit = document.getElementById("stairRiseUnit").value;
        const Hmax = parseFloat(document.getElementById("stairMaxRiser").value);
        const tread = parseFloat(document.getElementById("stairTread").value);

        if (!H || !Hmax || !tread) {
          document.getElementById("stairResult").textContent =
            "Enter total rise, max riser, and tread depth.";
          return;
        }

        const H_in = Hunit === "mm" ? H / 25.4 : H;
        const nRisers = Math.ceil(H_in / Hmax);
        const riserActual = H_in / nRisers;
        const nTreads = nRisers - 1;
        const totalRun_in = nTreads * tread;
        const slope_deg = (Math.atan(riserActual / tread) * 180) / Math.PI;

        document.getElementById(
          "stairResult"
        ).innerHTML = `<p><strong>Stair layout:</strong></p>
       <ul class="list-disc ml-5">
         <li>Number of risers: <strong>${nRisers}</strong></li>
         <li>Actual riser height: <strong>${formatNumber(
           riserActual,
           2
         )} in</strong></li>
         <li>Number of treads: <strong>${nTreads}</strong></li>
         <li>Total run: <strong>${formatNumber(
           totalRun_in / 12,
           2
         )} ft</strong> (${formatNumber(totalRun_in, 1)} in)</li>
         <li>Stair angle: <strong>${formatNumber(slope_deg, 1)}°</strong></li>
       </ul>
       <p class="mt-1 text-gray-600 text-xs">Always verify riser and tread dimensions against your local building code.</p>`;
      });

      // Materials calc
      document.getElementById("matCalcBtn").addEventListener("click", () => {
        const L = parseFloat(document.getElementById("matLen").value);
        const W = parseFloat(document.getElementById("matWidth").value);
        const Lunit = document.getElementById("matLenUnit").value;
        const Wunit = document.getElementById("matWidthUnit").value;
        const coverage = parseFloat(
          document.getElementById("matCoverage").value
        );
        const covUnit = document.getElementById("matCoverageUnit").value;
        const waste = parseFloat(
          document.getElementById("matWaste").value || "0"
        );
        const unitCost = parseFloat(
          document.getElementById("matUnitCost").value || "0"
        );
        const currency = document.getElementById("matCurrency").value;

        if (!L || !W || !coverage) {
          document.getElementById("matResult").textContent =
            "Enter area dimensions and coverage per unit.";
          return;
        }

        const L_ft = toFeet(L, Lunit);
        const W_ft = toFeet(W, Wunit);
        const area_ft2 = L_ft * W_ft;
        const area_ft2_waste = area_ft2 * (1 + waste / 100);
        const coverage_ft2 = toSquareFeetFrom(coverage, covUnit);
        const units = Math.ceil(area_ft2_waste / coverage_ft2);
        const totalCost = units * unitCost;

        document.getElementById(
          "matResult"
        ).innerHTML = `<p><strong>Material &amp; cost estimate:</strong></p>
       <ul class="list-disc ml-5">
         <li>Net area: <strong>${formatNumber(area_ft2, 1)} sq ft</strong></li>
         <li>Area with waste: <strong>${formatNumber(
           area_ft2_waste,
           1
         )} sq ft</strong></li>
         <li>Units required (rounded up): <strong>${units}</strong></li>
         <li>Estimated material cost: <strong>${currency}${formatNumber(
          totalCost,
          2
        )}</strong></li>
       </ul>
       <p class="mt-1 text-gray-600 text-xs">Does not include labor, equipment, or taxes. Always confirm quantities with a detailed takeoff.</p>`;
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
