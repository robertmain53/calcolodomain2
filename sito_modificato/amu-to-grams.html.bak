    <script>
      // Constants (CODATA 2018)
      const AMU_TO_GRAMS = 1.6605390666e-24; // g per amu
      const GRAMS_TO_AMU = 1 / AMU_TO_GRAMS;

      const inputEl = document.getElementById("input-value");
      const outputEl = document.getElementById("output-value");
      const decimalPlacesEl = document.getElementById("decimal-places");
      const notationRadios = document.querySelectorAll(".notation-radio");
      const modeButtons = document.querySelectorAll(".toggle-unit-btn");
      const inputUnitLabel = document.getElementById("input-unit-label");
      const outputUnitLabel = document.getElementById("output-unit-label");
      const summaryEl = document.getElementById("conversion-summary");

      let currentMode = "amu-to-g";
      let currentNotation = "auto";

      function formatNumber(value, decimals, notation) {
        if (!isFinite(value)) return "–";

        const absVal = Math.abs(value);
        const useSciAuto = absVal !== 0 && (absVal < 1e-6 || absVal >= 1e6);

        if (notation === "scientific" || (notation === "auto" && useSciAuto)) {
          return value.toExponential(decimals).replace("e", " × 10^");
        }

        return value.toFixed(decimals);
      }

      function updateLabels() {
        if (currentMode === "amu-to-g") {
          inputUnitLabel.textContent = "(amu)";
          outputUnitLabel.textContent = "(grams)";
        } else {
          inputUnitLabel.textContent = "(grams)";
          outputUnitLabel.textContent = "(amu)";
        }
      }

      function updateSummary(input, output) {
        if (!isFinite(input) || !isFinite(output)) {
          summaryEl.textContent = "";
          return;
        }

        if (currentMode === "amu-to-g") {
          summaryEl.textContent = `${input} amu = ${output} g`;
        } else {
          summaryEl.textContent = `${input} g = ${output} amu`;
        }
      }

      function recalc() {
        const raw = parseFloat(inputEl.value);
        const decimals = Math.min(
          Math.max(parseInt(decimalPlacesEl.value, 10) || 0, 0),
          15
        );

        if (!isFinite(raw)) {
          outputEl.textContent = "–";
          summaryEl.textContent = "";
          return;
        }

        let result;
        if (currentMode === "amu-to-g") {
          result = raw * AMU_TO_GRAMS;
        } else {
          result = raw * GRAMS_TO_AMU;
        }

        const formatted = formatNumber(result, decimals, currentNotation);
        outputEl.textContent = formatted;
        updateSummary(raw, formatted);
      }

      // Mode switching
      modeButtons.forEach((btn) => {
        btn.addEventListener("click", () => {
          const newMode = btn.getAttribute("data-mode");
          if (!newMode || newMode === currentMode) return;

          currentMode = newMode;
          modeButtons.forEach((b) => {
            b.classList.toggle("active", b === btn);
            b.setAttribute("aria-selected", b === btn ? "true" : "false");
          });
          updateLabels();
          recalc();
        });
      });

      // Notation change
      notationRadios.forEach((radio) => {
        radio.addEventListener("change", () => {
          if (radio.checked) {
            currentNotation = radio.value;
            recalc();
          }
        });
      });

      // Inputs
      inputEl.addEventListener("input", recalc);
      decimalPlacesEl.addEventListener("input", recalc);

      // Quick examples
      document.querySelectorAll("[data-example]").forEach((btn) => {
        btn.addEventListener("click", () => {
          const value = btn.getAttribute("data-example");
          const mode = btn.getAttribute("data-mode") || "amu-to-g";

          // Switch mode if needed
          if (mode !== currentMode) {
            const targetBtn = document.querySelector(
              `.toggle-unit-btn[data-mode="${mode}"]`
            );
            if (targetBtn) targetBtn.click();
          }

          inputEl.value = value;
          recalc();
        });
      });

      // Initialize
      updateLabels();
      recalc;

      // Fix: ensure recalc is actually called on load
      window.addEventListener("DOMContentLoaded", () => {
        recalc();
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
