    <script>
      // Tab switching
      const tabParams = document.getElementById("tab-params");
      const tabTable = document.getElementById("tab-table");
      const panelParams = document.getElementById("panel-params");
      const panelTable = document.getElementById("panel-table");

      function activateTab(tab) {
        if (tab === "params") {
          tabParams.classList.add("tab-active");
          tabTable.classList.remove("tab-active");
          panelParams.classList.remove("hidden");
          panelTable.classList.add("hidden");
        } else {
          tabTable.classList.add("tab-active");
          tabParams.classList.remove("tab-active");
          panelTable.classList.remove("hidden");
          panelParams.classList.add("hidden");
        }
      }

      tabParams.addEventListener("click", () => activateTab("params"));
      tabTable.addEventListener("click", () => activateTab("table"));

      // Helper functions
      function parsePercent(value) {
        if (value === "" || value === null || value === undefined) return null;
        const num = Number(value);
        if (!isFinite(num)) return null;
        return num / 100;
      }

      function formatPercent(p) {
        if (p === null || isNaN(p)) return "–";
        return (p * 100).toFixed(2).replace(/\.00$/, "") + "%";
      }

      function formatProb(p) {
        if (p === null || isNaN(p)) return "–";
        return (p * 100).toFixed(1).replace(/\.0$/, "") + "%";
      }

      function updateInterpretation(ppv, npv) {
        const el = document.getElementById("interpretation");
        if (ppv === null || npv === null || isNaN(ppv) || isNaN(npv)) {
          el.textContent =
            "Enter values above and click “Calculate” to see how likely a positive or negative test result is to be correct.";
          return;
        }
        const ppvPct = formatProb(ppv);
        const npvPct = formatProb(npv);
        el.textContent = `In this setting, a positive test result has a ${ppvPct} chance of being a true positive (PPV), while a negative result has a ${npvPct} chance of being a true negative (NPV).`;
      }

      function clearErrors() {
        document.getElementById("error-params").textContent = "";
        document.getElementById("error-table").textContent = "";
      }

      function setResults({ ppv, npv, sens, spec, prev }) {
        document.getElementById("ppv-out").textContent = formatProb(ppv);
        document.getElementById("npv-out").textContent = formatProb(npv);
        document.getElementById("sens-out").textContent = formatPercent(sens);
        document.getElementById("spec-out").textContent = formatPercent(spec);
        document.getElementById("prev-out").textContent = formatPercent(prev);
        updateInterpretation(ppv, npv);
      }

      function resetResults() {
        setResults({
          ppv: null,
          npv: null,
          sens: null,
          spec: null,
          prev: null,
        });
      }

      // Mode 1: parameters
      document.getElementById("calc-params").addEventListener("click", () => {
        clearErrors();
        const sensInput = document.getElementById("sens");
        const specInput = document.getElementById("spec");
        const prevInput = document.getElementById("prev");
        [sensInput, specInput, prevInput].forEach((el) =>
          el.classList.remove("input-error")
        );

        const Se = parsePercent(sensInput.value);
        const Sp = parsePercent(specInput.value);
        const P = parsePercent(prevInput.value);

        if (Se === null || Sp === null || P === null) {
          document.getElementById("error-params").textContent =
            "Please enter sensitivity, specificity, and prevalence (0–100%).";
          if (Se === null) sensInput.classList.add("input-error");
          if (Sp === null) specInput.classList.add("input-error");
          if (P === null) prevInput.classList.add("input-error");
          resetResults();
          return;
        }
        if (Se < 0 || Se > 1 || Sp < 0 || Sp > 1 || P < 0 || P > 1) {
          document.getElementById("error-params").textContent =
            "All percentages must be between 0 and 100.";
          resetResults();
          return;
        }

        const numeratorPPV = Se * P;
        const denomPPV = numeratorPPV + (1 - Sp) * (1 - P);
        const numeratorNPV = Sp * (1 - P);
        const denomNPV = (1 - Se) * P + numeratorNPV;

        const ppv = denomPPV === 0 ? null : numeratorPPV / denomPPV;
        const npv = denomNPV === 0 ? null : numeratorNPV / denomNPV;

        setResults({ ppv, npv, sens: Se, spec: Sp, prev: P });
      });

      document.getElementById("reset-params").addEventListener("click", () => {
        document.getElementById("sens").value = "";
        document.getElementById("spec").value = "";
        document.getElementById("prev").value = "";
        clearErrors();
        resetResults();
      });

      // Mode 2: 2x2 table
      const tpInput = document.getElementById("tp");
      const fpInput = document.getElementById("fp");
      const fnInput = document.getElementById("fn");
      const tnInput = document.getElementById("tn");

      function updateTableTotals() {
        const tp = Number(tpInput.value) || 0;
        const fp = Number(fpInput.value) || 0;
        const fn = Number(fnInput.value) || 0;
        const tn = Number(tnInput.value) || 0;

        const rowPos = tp + fp;
        const rowNeg = fn + tn;
        const colDpos = tp + fn;
        const colDneg = fp + tn;
        const total = rowPos + rowNeg;

        document.getElementById("row-pos-total").textContent = total
          ? rowPos
          : "–";
        document.getElementById("row-neg-total").textContent = total
          ? rowNeg
          : "–";
        document.getElementById("col-dpos-total").textContent = total
          ? colDpos
          : "–";
        document.getElementById("col-dneg-total").textContent = total
          ? colDneg
          : "–";
        document.getElementById("grand-total").textContent = total
          ? total
          : "–";
      }

      [tpInput, fpInput, fnInput, tnInput].forEach((el) => {
        el.addEventListener("input", updateTableTotals);
      });

      document.getElementById("calc-table").addEventListener("click", () => {
        clearErrors();
        [tpInput, fpInput, fnInput, tnInput].forEach((el) =>
          el.classList.remove("input-error")
        );

        const tp = Number(tpInput.value);
        const fp = Number(fpInput.value);
        const fn = Number(fnInput.value);
        const tn = Number(tnInput.value);

        if (![tp, fp, fn, tn].every((v) => Number.isFinite(v) && v >= 0)) {
          document.getElementById("error-table").textContent =
            "Please enter non-negative integer counts for TP, FP, FN, and TN.";
          resetResults();
          return;
        }

        const total = tp + fp + fn + tn;
        if (total === 0) {
          document.getElementById("error-table").textContent =
            "At least one of TP, FP, FN, or TN must be greater than zero.";
          resetResults();
          return;
        }

        const ppv = tp + fp === 0 ? null : tp / (tp + fp);
        const npv = tn + fn === 0 ? null : tn / (tn + fn);
        const sens = tp + fn === 0 ? null : tp / (tp + fn);
        const spec = tn + fp === 0 ? null : tn / (tn + fp);
        const prev = (tp + fn) / total;

        setResults({ ppv, npv, sens, spec, prev });
      });

      document.getElementById("reset-table").addEventListener("click", () => {
        tpInput.value = "";
        fpInput.value = "";
        fnInput.value = "";
        tnInput.value = "";
        clearErrors();
        updateTableTotals();
        resetResults();
      });

      // Initialize
      resetResults();
      updateTableTotals();

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
