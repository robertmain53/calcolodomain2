    <script>
      // Basic country metadata for display and heuristics
      const COUNTRY_META = {
        US: {
          name: "United States / Canada",
          code: "+1",
          minLen: 10,
          maxLen: 10,
          mobilePrefixes: ["2", "3", "4", "5", "6", "7", "8", "9"],
        },
        CA: {
          name: "Canada",
          code: "+1",
          minLen: 10,
          maxLen: 10,
          mobilePrefixes: ["2", "3", "4", "5", "6", "7", "8", "9"],
        },
        GB: {
          name: "United Kingdom",
          code: "+44",
          minLen: 10,
          maxLen: 10,
          mobilePrefixes: ["7"],
        },
        AU: {
          name: "Australia",
          code: "+61",
          minLen: 9,
          maxLen: 9,
          mobilePrefixes: ["4"],
        },
        NZ: {
          name: "New Zealand",
          code: "+64",
          minLen: 8,
          maxLen: 9,
          mobilePrefixes: ["2"],
        },
        IN: {
          name: "India",
          code: "+91",
          minLen: 10,
          maxLen: 10,
          mobilePrefixes: ["6", "7", "8", "9"],
        },
        DE: {
          name: "Germany",
          code: "+49",
          minLen: 10,
          maxLen: 13,
          mobilePrefixes: ["15", "16", "17"],
        },
        FR: {
          name: "France",
          code: "+33",
          minLen: 9,
          maxLen: 9,
          mobilePrefixes: ["6", "7"],
        },
        ES: {
          name: "Spain",
          code: "+34",
          minLen: 9,
          maxLen: 9,
          mobilePrefixes: ["6", "7"],
        },
        IT: {
          name: "Italy",
          code: "+39",
          minLen: 9,
          maxLen: 11,
          mobilePrefixes: ["3"],
        },
        BR: {
          name: "Brazil",
          code: "+55",
          minLen: 10,
          maxLen: 11,
          mobilePrefixes: ["9"],
        },
        MX: {
          name: "Mexico",
          code: "+52",
          minLen: 10,
          maxLen: 10,
          mobilePrefixes: ["1", "2", "3", "4", "5", "6", "7", "8", "9"],
        },
        ZA: {
          name: "South Africa",
          code: "+27",
          minLen: 9,
          maxLen: 9,
          mobilePrefixes: ["6", "7", "8"],
        },
        JP: {
          name: "Japan",
          code: "+81",
          minLen: 9,
          maxLen: 10,
          mobilePrefixes: ["7", "8", "9"],
        },
        CN: {
          name: "China",
          code: "+86",
          minLen: 11,
          maxLen: 11,
          mobilePrefixes: ["1"],
        },
      };

      const COUNTRY_CODES = Object.values(COUNTRY_META)
        .map((c) => c.code)
        .sort((a, b) => b.length - a.length);

      function normalizeInput(raw) {
        if (!raw) return "";
        return raw.replace(/[^\d+]/g, "").replace(/^00/, "+");
      }

      function detectCountry(norm, defaultCountry) {
        if (!norm) return { country: null, national: "", reason: "empty" };

        // If starts with +, try to match known country codes
        if (norm.startsWith("+")) {
          for (const code of COUNTRY_CODES) {
            if (norm.startsWith(code)) {
              const meta = Object.values(COUNTRY_META).find(
                (c) => c.code === code
              );
              const national = norm.slice(code.length);
              return {
                country: meta ? meta : null,
                national,
                reason: "by_code",
              };
            }
          }
          // Unknown country code, strip leading + and treat as unknown
          return {
            country: null,
            national: norm.replace(/^\+/, ""),
            reason: "unknown_code",
          };
        }

        // No + sign: use default country if provided
        if (defaultCountry && COUNTRY_META[defaultCountry]) {
          const meta = COUNTRY_META[defaultCountry];
          return { country: meta, national: norm, reason: "default_country" };
        }

        // Fallback: heuristic by length
        const digits = norm.replace(/\D/g, "");
        if (digits.length === 10) {
          return {
            country: COUNTRY_META["US"],
            national: digits,
            reason: "length_10_assume_US",
          };
        }

        return { country: null, national: digits, reason: "no_country" };
      }

      function isSequential(str) {
        if (str.length < 4) return false;
        const asc = "0123456789";
        const desc = "9876543210";
        return asc.includes(str) || desc.includes(str);
      }

      function detectFakePatterns(national) {
        const flags = [];
        if (!national) return flags;

        if (/^0+$/.test(national)) flags.push("all zeros");
        if (/^(\d)\1{5,}$/.test(national)) flags.push("all same digit");
        if (isSequential(national)) flags.push("sequential digits");
        if (national.length < 6) flags.push("too short");
        if (national.length > 15) flags.push("too long");

        // US 555 test numbers (e.g., 55501xx)
        if (national.length === 10 && /^55501\d{2}$/.test(national.slice(3))) {
          flags.push("test / fake US 555 range");
        }

        return flags;
      }

      function classifyLineType(country, national) {
        if (!country || !national)
          return { type: "unknown", detail: "Not enough information" };

        const meta = country;
        const n = national.replace(/^0+/, ""); // strip leading zeros for some regions
        const first2 = n.slice(0, 2);
        const first1 = n.slice(0, 1);

        // Toll-free / premium heuristics for NANP
        if (meta.code === "+1" && n.length === 10) {
          const area = n.slice(0, 3);
          if (
            ["800", "888", "877", "866", "855", "844", "833", "822"].includes(
              area
            )
          ) {
            return { type: "toll-free", detail: "NANP toll-free area code" };
          }
          if (["900"].includes(area)) {
            return { type: "premium", detail: "NANP premium-rate area code" };
          }
        }

        // Mobile vs landline heuristics
        if (meta.mobilePrefixes) {
          if (
            meta.mobilePrefixes.includes(first2) ||
            meta.mobilePrefixes.includes(first1)
          ) {
            return {
              type: "mobile",
              detail: "Matches known mobile prefix pattern",
            };
          }
        }

        return {
          type: "landline_or_unknown",
          detail: "Could be landline or mobile; prefix not conclusive",
        };
      }

      function validatePhone(raw, defaultCountryCode) {
        const cleaned = normalizeInput(raw);
        const digitsOnly = cleaned.replace(/[^\d]/g, "");
        const countryInfo = detectCountry(cleaned, defaultCountryCode);
        const country = countryInfo.country;
        const national = countryInfo.national.replace(/[^\d]/g, "");

        const flags = detectFakePatterns(national);
        let valid = true;
        let lengthNote = "Unknown length";

        if (!digitsOnly) {
          valid = false;
          lengthNote = "No digits found";
        } else if (country) {
          if (
            national.length < country.minLen ||
            national.length > country.maxLen
          ) {
            valid = false;
            lengthNote = `Expected ${country.minLen}–${country.maxLen} digits, got ${national.length}`;
            flags.push("length out of range for country");
          } else {
            lengthNote = `Within expected range (${country.minLen}–${country.maxLen} digits)`;
          }
        } else {
          // Unknown country: apply generic ITU range 6–15 digits
          if (national.length < 6 || national.length > 15) {
            valid = false;
            lengthNote = `Generic range 6–15 digits, got ${national.length}`;
            flags.push("length out of generic range");
          } else {
            lengthNote = `Generic plausible length (${national.length} digits)`;
          }
        }

        if (flags.length > 0) {
          // downgrade validity if strong fake patterns
          if (
            flags.includes("all zeros") ||
            flags.includes("all same digit") ||
            flags.includes("sequential digits")
          ) {
            valid = false;
          }
        }

        const lineInfo = classifyLineType(country, national);
        let e164 = null;
        if (country && national) {
          e164 = country.code + national;
        } else if (
          cleaned.startsWith("+") &&
          digitsOnly.length >= 6 &&
          digitsOnly.length <= 15
        ) {
          e164 = "+" + digitsOnly;
        }

        // Risk level
        let risk = "low";
        if (flags.length > 0) risk = "medium";
        if (
          flags.includes("all zeros") ||
          flags.includes("all same digit") ||
          flags.includes("sequential digits") ||
          flags.includes("length out of range for country")
        ) {
          risk = "high";
        }

        return {
          input: raw,
          normalized: cleaned,
          e164,
          countryName: country ? country.name : "Unknown",
          countryCode: country ? country.code : null,
          national,
          valid,
          lengthNote,
          flags,
          lineType: lineInfo.type,
          lineDetail: lineInfo.detail,
          risk,
        };
      }

      function updateSingleUI(result) {
        const card = document.getElementById("result-card");
        const validityPill = document.getElementById("validity-pill");
        const riskPill = document.getElementById("risk-pill");
        const lineTypePill = document.getElementById("line-type-pill");

        document.getElementById("res-input").textContent = result.input || "—";
        document.getElementById("res-e164").textContent =
          result.e164 || "Not available";
        document.getElementById("res-country").textContent =
          result.countryName +
          (result.countryCode ? ` (${result.countryCode})` : "");
        document.getElementById("res-region").textContent = result.countryCode
          ? `National length: ${result.national.length} digits`
          : "Unknown region";
        document.getElementById("res-line-type").textContent =
          result.lineType.replace(/_/g, " ") +
          (result.lineDetail ? ` – ${result.lineDetail}` : "");
        document.getElementById("res-length").textContent = result.lengthNote;
        document.getElementById("res-flags").textContent = result.flags.length
          ? result.flags.join(", ")
          : "No obvious risk patterns detected.";

        // Validity pill
        validityPill.textContent = result.valid
          ? "Valid format"
          : "Invalid format";
        validityPill.className =
          "badge text-white " +
          (result.valid ? "bg-emerald-600" : "bg-red-600");

        // Risk pill
        let riskClass = "bg-emerald-600";
        if (result.risk === "medium") riskClass = "bg-amber-500";
        if (result.risk === "high") riskClass = "bg-red-600";
        riskPill.textContent = `Risk: ${result.risk}`;
        riskPill.className = "badge text-white " + riskClass;

        // Line type pill
        lineTypePill.textContent =
          "Line type: " + result.lineType.replace(/_/g, " ");
        lineTypePill.className =
          "badge border " +
          (result.lineType === "mobile"
            ? "bg-blue-50 text-blue-700 border-blue-300"
            : "bg-gray-50 text-gray-700 border-gray-300");

        card.classList.remove("hidden");
      }

      function handleSingleValidation() {
        const input = document.getElementById("phone-input").value.trim();
        const defaultCountry =
          document.getElementById("country-select").value || null;
        const statusEl = document.getElementById("live-status");

        if (!input) {
          statusEl.textContent = "Please enter a phone number to validate.";
          return;
        }

        statusEl.textContent = "Validating…";
        const result = validatePhone(input, defaultCountry);
        updateSingleUI(result);
        statusEl.textContent = result.valid
          ? "Number appears structurally valid."
          : "Number appears invalid or risky.";
      }

      function handleBulkValidation() {
        const textarea = document.getElementById("bulk-input");
        const lines = textarea.value
          .split(/\r?\n/)
          .map((l) => l.trim())
          .filter((l) => l.length > 0);
        const tbody = document.getElementById("bulk-table-body");
        const statusEl = document.getElementById("bulk-status");
        const downloadBtn = document.getElementById("bulk-download-btn");

        if (lines.length === 0) {
          statusEl.textContent =
            "Paste at least one phone number (one per line).";
          return;
        }

        statusEl.textContent = "Validating list…";
        tbody.innerHTML = "";
        const results = [];

        lines.forEach((line, idx) => {
          const res = validatePhone(line, null);
          results.push(res);

          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="px-3 py-2 text-gray-500">${idx + 1}</td>
        <td class="px-3 py-2 text-gray-800 break-all">${res.input || "—"}</td>
        <td class="px-3 py-2 ${
          res.valid ? "text-emerald-700" : "text-red-600"
        } font-semibold">${res.valid ? "Yes" : "No"}</td>
        <td class="px-3 py-2 text-gray-800 break-all">${res.e164 || "—"}</td>
        <td class="px-3 py-2 text-gray-800">${res.countryName}${
            res.countryCode ? " (" + res.countryCode + ")" : ""
          }</td>
        <td class="px-3 py-2 text-gray-800">${res.lineType.replace(
          /_/g,
          " "
        )}</td>
        <td class="px-3 py-2 text-gray-800">${
          res.flags.length ? res.flags.join("; ") : "—"
        }</td>
      `;
          tbody.appendChild(tr);
        });

        // Store results for CSV download
        downloadBtn.dataset.csv = JSON.stringify(results);
        downloadBtn.disabled = false;
        statusEl.textContent = `Validated ${results.length} number${
          results.length !== 1 ? "s" : ""
        }.`;
      }

      function downloadCSV() {
        const btn = document.getElementById("bulk-download-btn");
        const dataStr = btn.dataset.csv;
        if (!dataStr) return;
        const results = JSON.parse(dataStr);

        const header = [
          "input",
          "valid",
          "e164",
          "country_name",
          "country_code",
          "national_number",
          "line_type",
          "risk",
          "length_note",
          "flags",
        ];
        const rows = [header.join(",")];

        results.forEach((r) => {
          const row = [
            `"${(r.input || "").replace(/"/g, '""')}"`,
            r.valid ? "true" : "false",
            `"${(r.e164 || "").replace(/"/g, '""')}"`,
            `"${(r.countryName || "").replace(/"/g, '""')}"`,
            `"${(r.countryCode || "").replace(/"/g, '""')}"`,
            `"${(r.national || "").replace(/"/g, '""')}"`,
            `"${(r.lineType || "").replace(/"/g, '""')}"`,
            `"${(r.risk || "").replace(/"/g, '""')}"`,
            `"${(r.lengthNote || "").replace(/"/g, '""')}"`,
            `"${(r.flags && r.flags.length ? r.flags.join("; ") : "").replace(
              /"/g,
              '""'
            )}"`,
          ];
          rows.push(row.join(","));
        });

        const blob = new Blob([rows.join("\n")], {
          type: "text/csv;charset=utf-8;",
        });
        const url = URL.createObjectURL(blob);
        const a = document.createElement("a");
        a.href = url;
        a.download = "phone-validation-results.csv";
        document.body.appendChild(a);
        a.click();
        document.body.removeChild(a);
        URL.revokeObjectURL(url);
      }

      document.addEventListener("DOMContentLoaded", () => {
        document
          .getElementById("validate-btn")
          .addEventListener("click", handleSingleValidation);
        document.getElementById("clear-btn").addEventListener("click", () => {
          document.getElementById("phone-input").value = "";
          document.getElementById("live-status").textContent = "";
          document.getElementById("result-card").classList.add("hidden");
        });
        document
          .getElementById("phone-input")
          .addEventListener("keydown", (e) => {
            if (e.key === "Enter") {
              e.preventDefault();
              handleSingleValidation();
            }
          });

        document
          .getElementById("bulk-validate-btn")
          .addEventListener("click", handleBulkValidation);
        document
          .getElementById("bulk-clear-btn")
          .addEventListener("click", () => {
            document.getElementById("bulk-input").value = "";
            document.getElementById("bulk-status").textContent = "";
            document.getElementById("bulk-table-body").innerHTML =
              '<tr><td colspan="7" class="px-3 py-3 text-center text-gray-400">No results yet. Validate a list to see details here.</td></tr>';
            const btn = document.getElementById("bulk-download-btn");
            btn.disabled = true;
            delete btn.dataset.csv;
          });
        document
          .getElementById("bulk-download-btn")
          .addEventListener("click", downloadCSV);
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
