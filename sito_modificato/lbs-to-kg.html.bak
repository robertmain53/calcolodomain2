            <script>
              document.addEventListener('DOMContentLoaded', () => {
                const kgInput = document.getElementById('kg-input');
                const lbsInput = document.getElementById('lbs-input');
                const lbsOzInput = document.getElementById('lbs-oz-input');
                const ozOzInput = document.getElementById('oz-oz-input');

                const inputs = [kgInput, lbsInput, lbsOzInput, ozOzInput];

                const LBS_TO_KG = 0.45359237;
                const KG_TO_LBS = 2.2046226218;
                const OZ_IN_LBS = 16.0;

                // Funzione per formattare i numeri
                function formatNum(num, precision = 4) {
                    if (isNaN(num) || num === 0) return '';
                    return parseFloat(num.toFixed(precision)).toString();
                }
                
                function calculate(source, value) {
                    let kg = 0;
                    let totalLbs = 0;
                    
                    // If the input is 0 or empty, check if it is an lbs-oz input
                    if (isNaN(value) || value === 0) {
                        if (source !== 'lbs-oz' || ( (parseFloat(lbsOzInput.value) || 0) === 0 && (parseFloat(ozOzInput.value) || 0) === 0 )) {
                             inputs.forEach(input => input.value = '');
                             return;
                        }
                    }

                    if (source === 'kg') {
                        kg = value;
                        totalLbs = kg * KG_TO_LBS;
                    } else if (source === 'lbs_decimal') {
                        totalLbs = value;
                        kg = totalLbs * LBS_TO_KG;
                    } else if (source === 'lbs-oz') {
                        const lbsPart = parseFloat(lbsOzInput.value) || 0;
                        const ozPart = parseFloat(ozOzInput.value) || 0;
                        totalLbs = lbsPart + (ozPart / OZ_IN_LBS);
                        kg = totalLbs * LBS_TO_KG;
                    }

                    // Aggiorna tutti i campi tranne quelli sorgente
                    if (source !== 'kg') {
                        kgInput.value = formatNum(kg);
                    }
                    if (source !== 'lbs_decimal') {
                        lbsInput.value = formatNum(totalLbs);
                    }
                    if (source !== 'lbs-oz') {
                        const lbs_oz = Math.floor(totalLbs);
                        const oz_oz = (totalLbs - lbs_oz) * OZ_IN_LBS;
                        
                        lbsOzInput.value = formatNum(lbs_oz, 0);
                        ozOzInput.value = formatNum(oz_oz);
                    }
                }

                inputs.forEach(input => {
                  input.addEventListener('input', (e) => {
                    const source = e.target.dataset.source;
                    // Se l'utente sta modificando libbre o once, the "source" is 'lbs-oz'
                    if (source === 'lbs-oz') {
                         calculate('lbs-oz', 1); // Passa un valore fittizio > 0 per triggerare
                    } else {
                         const value = parseFloat(e.target.value);
                         calculate(source, value);
                    }
                  });
                });
              });
            </script>
            
