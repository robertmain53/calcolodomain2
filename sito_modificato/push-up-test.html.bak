    <script>
      // Age bracket helper
      function getAgeBracket(age) {
        if (age < 20) return { label: "10–19", key: "10-19" };
        if (age < 30) return { label: "20–29", key: "20-29" };
        if (age < 40) return { label: "30–39", key: "30-39" };
        if (age < 50) return { label: "40–49", key: "40-49" };
        if (age < 60) return { label: "50–59", key: "50-59" };
        return { label: "60+", key: "60+" };
      }

      // Norms data
      const norms = {
        male: {
          "20-29": { excellent: 47, good: 35, average: 20, below: 10 },
          "30-39": { excellent: 41, good: 30, average: 17, below: 8 },
          "40-49": { excellent: 34, good: 25, average: 13, below: 6 },
          "50-59": { excellent: 30, good: 20, average: 10, below: 5 },
          "60+": { excellent: 25, good: 17, average: 8, below: 3 },
          "10-19": { excellent: 40, good: 30, average: 18, below: 8 },
        },
        female: {
          "20-29": { excellent: 35, good: 25, average: 15, below: 6 },
          "30-39": { excellent: 30, good: 22, average: 12, below: 5 },
          "40-49": { excellent: 25, good: 18, average: 10, below: 4 },
          "50-59": { excellent: 20, good: 15, average: 8, below: 3 },
          "60+": { excellent: 15, good: 10, average: 6, below: 2 },
          "10-19": { excellent: 25, good: 18, average: 10, below: 4 },
        },
      };

      function classifyPushups(gender, age, reps) {
        const bracket = getAgeBracket(age);
        const groupNorms = norms[gender][bracket.key];
        if (!groupNorms)
          return {
            rating: "Not available",
            color: "bg-gray-200 text-gray-800",
            explanation: "No norms available for this combination.",
          };

        const { excellent, good, average, below } = groupNorms;
        let rating, color, explanation;

        if (reps >= excellent) {
          rating = "Excellent";
          color = "bg-green-100 text-green-800";
          explanation =
            "Your upper-body strength and endurance are well above average for your age and gender.";
        } else if (reps >= good) {
          rating = "Good";
          color = "bg-emerald-100 text-emerald-800";
          explanation =
            "You are above average for your age and gender. With focused training you can reach the excellent range.";
        } else if (reps >= average) {
          rating = "Average";
          color = "bg-blue-100 text-blue-800";
          explanation =
            "You are in the typical range for your age and gender. Improving your score will further reduce health risks.";
        } else if (reps >= below) {
          rating = "Below average";
          color = "bg-yellow-100 text-yellow-800";
          explanation =
            "Your result is below average. Gradual strength training can significantly improve your push-up capacity.";
        } else {
          rating = "Poor";
          color = "bg-red-100 text-red-800";
          explanation =
            "Your score is well below norms. Start with easier push-up variations and consider consulting a professional if you have health concerns.";
        }

        return { rating, color, explanation };
      }

      function estimateVo2max(gender, age, reps) {
        // Baseline by age decade
        let baseline;
        const decade = Math.min(
          5,
          Math.max(1, Math.floor((age - 20) / 10) + 1)
        ); // 20-29 ->1, 30-39->2, etc.
        if (gender === "male") {
          baseline = 50 - (decade - 1) * 3; // 50,47,44,41,38,35
        } else {
          baseline = 40 - (decade - 1) * 3; // 40,37,34,31,28,25
        }
        const bonus = 0.15 * Math.max(0, reps - 10);
        const vo2 = baseline + bonus;
        return Math.max(20, Math.min(70, vo2)); // clamp to reasonable range
      }

      document.addEventListener("DOMContentLoaded", function () {
        const genderEl = document.getElementById("gender");
        const ageEl = document.getElementById("age");
        const repsEl = document.getElementById("reps");
        const testTypeEl = document.getElementById("test-type");
        const calcBtn = document.getElementById("calculate-btn");

        const ageGroupLabel = document.getElementById("age-group-label");
        const testTypeLabel = document.getElementById("test-type-label");
        const repsLabel = document.getElementById("reps-label");
        const ratingBadge = document.getElementById("rating-badge");
        const ratingText = document.getElementById("rating-text");
        const vo2Value = document.getElementById("vo2max-value");
        const vo2Unit = document.getElementById("vo2max-unit");
        const vo2Note = document.getElementById("vo2max-note");

        function updateLabels() {
          const age = parseInt(ageEl.value, 10);
          const reps = parseInt(repsEl.value, 10);
          const testType = testTypeEl.value;

          const bracket = getAgeBracket(isNaN(age) ? 30 : age);
          ageGroupLabel.textContent = bracket.label;
          repsLabel.textContent = isNaN(reps) ? "—" : reps.toString();

          let testLabel = "Max reps";
          if (testType === "1min") testLabel = "1-minute test";
          if (testType === "2min") testLabel = "2-minute test";
          testTypeLabel.textContent = testLabel;
        }

        function calculate() {
          const gender = genderEl.value;
          const age = parseInt(ageEl.value, 10);
          const reps = parseInt(repsEl.value, 10);

          if (isNaN(age) || age < 10 || age > 80) {
            alert("Please enter an age between 10 and 80 years.");
            return;
          }
          if (isNaN(reps) || reps < 0) {
            alert("Please enter a non-negative number of push-ups.");
            return;
          }

          updateLabels();

          const { rating, color, explanation } = classifyPushups(
            gender,
            age,
            reps
          );
          ratingBadge.textContent = rating;
          ratingBadge.className = "badge " + color;
          ratingText.textContent = explanation;

          if (reps === 0) {
            vo2Value.textContent = "—";
            vo2Unit.textContent = "";
            vo2Note.textContent =
              "Perform at least a few push-ups to get a rough VO₂max estimate.";
          } else {
            const vo2 = estimateVo2max(gender, age, reps);
            vo2Value.textContent = vo2.toFixed(1);
            vo2Unit.textContent = "ml·kg⁻¹·min⁻¹ (approx.)";
            vo2Note.textContent =
              "This is an educational estimate based on push-up capacity and age, not a clinical measurement.";
          }
        }

        calcBtn.addEventListener("click", calculate);
        genderEl.addEventListener("change", updateLabels);
        ageEl.addEventListener("input", updateLabels);
        repsEl.addEventListener("input", updateLabels);
        testTypeEl.addEventListener("change", updateLabels);

        updateLabels();
      });

