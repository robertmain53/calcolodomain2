<script>
  // 1RM calculator logic
  (function() {
    const weightInput = document.getElementById('weight');
    const repsInput = document.getElementById('reps');
    const unitRadios = document.querySelectorAll('input[name="unit"]');
    const rpeSelect = document.getElementById('rpe');
    const calcBtn = document.getElementById('calculate-btn');
    const clearBtn = document.getElementById('clear-btn');
    const errorMsg = document.getElementById('error-msg');

    const resultsSection = document.getElementById('results-section');
    const avgOrmEl = document.getElementById('avg-orm');
    const avgOrmUnitEl = document.getElementById('avg-orm-unit');
    const rangeOrmEl = document.getElementById('range-orm');
    const suggestedLoadEl = document.getElementById('suggested-load');
    const suggestedLoadNoteEl = document.getElementById('suggested-load-note');
    const formulaTableBody = document.getElementById('formula-table-body');
    const percentTableBody = document.getElementById('percent-table-body');
    const historyContainer = document.getElementById('history-container');

    function getUnit() {
      return Array.from(unitRadios).find(r => r.checked)?.value || 'kg';
    }

    function roundTo(value, step) {
      if (!isFinite(value)) return value;
      return Math.round(value / step) * step;
    }

    function formatWeight(value, unit) {
      if (!isFinite(value)) return '–';
      const step = unit === 'kg' ? 0.5 : 1;
      const rounded = roundTo(value, step);
      return rounded.toFixed(step === 0.5 ? 1 : 0) + ' ' + unit;
    }

    function computeFormulas(w, r) {
      const results = [];

      // Guard against invalid denominators
      const epley = w * (1 + r / 30);
      if (isFinite(epley) && epley > 0) {
        results.push({ name: 'Epley', value: epley, note: 'Popular for 1–10 reps.' });
      }

      const brzyDen = 1.0278 - 0.0278 * r;
      if (brzyDen > 0) {
        const brzycki = w / brzyDen;
        if (isFinite(brzycki) && brzycki > 0) {
          results.push({ name: 'Brzycki', value: brzycki, note: 'Common in strength research.' });
        }
      }

      const lombardi = w * Math.pow(r, 0.10);
      if (isFinite(lombardi) && lombardi > 0) {
        results.push({ name: 'Lombardi', value: lombardi, note: 'Handles higher reps reasonably well.' });
      }

      const oconner = w * (1 + 0.025 * r);
      if (isFinite(oconner) && oconner > 0) {
        results.push({ name: 'O’Conner', value: oconner, note: 'Simple linear model.' });
      }

      const wathanDen = 48.8 + 53.8 * Math.exp(-0.075 * r);
      if (wathanDen > 0) {
        const wathan = (100 * w) / wathanDen;
        if (isFinite(wathan) && wathan > 0) {
          results.push({ name: 'Wathan', value: wathan, note: 'Fits some lifters well at moderate reps.' });
        }
      }

      return results;
    }

    function buildPercentTable(avg1RM, unit) {
      percentTableBody.innerHTML = '';
      if (!isFinite(avg1RM) || avg1RM <= 0) return;

      const rows = [
        { pct: 40, reps: '15–25+', goal: 'Technique, rehab, speed' },
        { pct: 50, reps: '12–20', goal: 'Endurance, warm-up' },
        { pct: 60, reps: '10–15', goal: 'Hypertrophy, volume' },
        { pct: 70, reps: '8–12', goal: 'Hypertrophy & strength' },
        { pct: 80, reps: '4–8', goal: 'Strength focus' },
        { pct: 85, reps: '3–5', goal: 'Heavy strength' },
        { pct: 90, reps: '1–3', goal: 'Max strength / peaking' },
        { pct: 95, reps: '1–2', goal: 'Near-max attempts' }
      ];

      rows.forEach(row => {
        const tr = document.createElement('tr');
        const weight = avg1RM * (row.pct / 100);
        tr.innerHTML = `
          <td class="px-2 py-1">${row.pct}%</td>
          <td class="px-2 py-1 text-right">${formatWeight(weight, unit)}</td>
          <td class="px-2 py-1">${row.reps}</td>
          <td class="px-2 py-1">${row.goal}</td>
        `;
        percentTableBody.appendChild(tr);
      });
    }

    function rpeToPercent(rpe) {
      // Approximate %1RM for sets of ~1–8 reps at given RPE
      const map = {
        '10': 1.00,
        '9.5': 0.975,
        '9': 0.95,
        '8.5': 0.925,
        '8': 0.90,
        '7.5': 0.875,
        '7': 0.85
      };
      return map[String(rpe)] || null;
    }

    function updateHistory(entry) {
      try {
        const key = 'orm_history_v1';
        const existing = JSON.parse(localStorage.getItem(key) || '[]');
        existing.unshift(entry);
        const trimmed = existing.slice(0, 10);
        localStorage.setItem(key, JSON.stringify(trimmed));
        renderHistory(trimmed);
      } catch (e) {
        // ignore storage errors
      }
    }

    function renderHistory(list) {
      historyContainer.innerHTML = '';
      if (!list || !list.length) {
        historyContainer.innerHTML = '<p class="text-sm text-gray-500">No sets saved yet. Calculate a 1RM to start your history.</p>';
        return;
      }
      list.forEach(item => {
        const div = document.createElement('div');
        div.className = 'flex justify-between items-center text-xs border border-gray-100 rounded px-2 py-1';
        const exercise = item.exercise ? `<span class="font-semibold">${item.exercise}</span> – ` : '';
        div.innerHTML = `
          <div>
            ${exercise}${item.weight}${item.unit} × ${item.reps} reps
            <span class="badge bg-blue-50 text-blue-700 ml-1">1RM ~ ${item.estimated}</span>
          </div>
          <div class="text-gray-400">${item.date}</div>
        `;
        historyContainer.appendChild(div);
      });
    }

    function loadHistory() {
      try {
        const key = 'orm_history_v1';
        const existing = JSON.parse(localStorage.getItem(key) || '[]');
        renderHistory(existing);
      } catch (e) {
        renderHistory([]);
      }
    }

    calcBtn.addEventListener('click', () => {
      errorMsg.textContent = '';
      const w = parseFloat(weightInput.value);
      const r = parseInt(repsInput.value, 10);
      const unit = getUnit();
      const rpe = rpeSelect.value;

      if (!isFinite(w) || w <= 0) {
        errorMsg.textContent = 'Please enter a valid weight.';
        resultsSection.classList.add('hidden');
        return;
      }
      if (!isFinite(r) || r < 1 || r > 20) {
        errorMsg.textContent = 'Please enter reps between 1 and 20.';
        resultsSection.classList.add('hidden');
        return;
      }

      const formulas = computeFormulas(w, r);
      if (!formulas.length) {
        errorMsg.textContent = 'Unable to compute a valid 1RM estimate with these inputs.';
        resultsSection.classList.add('hidden');
        return;
      }

      const values = formulas.map(f => f.value);
      const avg = values.reduce((a, b) => a + b, 0) / values.length;
      const min = Math.min(...values);
      const max = Math.max(...values);

      avgOrmEl.textContent = formatWeight(avg, unit);
      avgOrmUnitEl.textContent = `Average of ${formulas.length} formulas`;

      rangeOrmEl.textContent = `${formatWeight(min, unit)} – ${formatWeight(max, unit)}`;

      // Fill formula table
      formulaTableBody.innerHTML = '';
      formulas.forEach(f => {
        const tr = document.createElement('tr');
        tr.innerHTML = `
          <td class="px-3 py-2">${f.name}</td>
          <td class="px-3 py-2 text-right">${formatWeight(f.value, unit)}</td>
          <td class="px-3 py-2 text-gray-600">${f.note}</td>
        `;
        formulaTableBody.appendChild(tr);
      });

      // Suggested load based on RPE if provided
      if (rpe) {
        const pct = rpeToPercent(rpe);
        if (pct) {
          const load = avg * pct;
          suggestedLoadEl.textContent = formatWeight(load, unit);
          suggestedLoadNoteEl.textContent = `Approx. ${Math.round(pct * 100)}% of 1RM for RPE ${rpe}. Adjust based on how it feels.`;
        } else {
          suggestedLoadEl.textContent = '–';
          suggestedLoadNoteEl.textContent = 'Select an RPE to get a suggested training load.';
        }
      } else {
        suggestedLoadEl.textContent = '–';
        suggestedLoadNoteEl.textContent = 'Select an RPE above to see a suggested training load.';
      }

      buildPercentTable(avg, unit);
      resultsSection.classList.remove('hidden');

      // Save to history
      const exercise = document.getElementById('exercise').value.trim();
      const now = new Date();
      const dateStr = now.toLocaleDateString(undefined, { month: 'short', day: 'numeric' });
      updateHistory({
        exercise,
        weight: formatWeight(w, unit),
        reps: r,
        unit,
        estimated: formatWeight(avg, unit),
        date: dateStr
      });
    });

    clearBtn.addEventListener('click', () => {
      document.getElementById('orm-form').reset();
      errorMsg.textContent = '';
      resultsSection.classList.add('hidden');
    });

    // Load history on page load
    document.addEventListener('DOMContentLoaded', loadHistory);
  })();
</script>

