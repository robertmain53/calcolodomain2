    <script>
      function formatCurrency(value) {
        if (isNaN(value) || value === null) return "-";
        return (
          "$" + value.toLocaleString(undefined, { maximumFractionDigits: 0 })
        );
      }

      function interpretRatio(ratio) {
        if (!isFinite(ratio) || ratio < 0) {
          return {
            label: "Invalid / distressed",
            comment:
              "The inputs suggest negative assets or liabilities, which usually indicates accounting issues or severe financial distress.",
          };
        }
        if (ratio < 0.3) {
          return {
            label: "Low leverage",
            comment:
              "The company is conservatively financed with relatively little debt. This usually means lower financial risk but may also mean underutilized leverage.",
          };
        }
        if (ratio < 0.6) {
          return {
            label: "Moderate leverage",
            comment:
              "A balanced level of debt financing. For many industries this range is considered healthy, assuming cash flows are stable.",
          };
        }
        if (ratio < 0.8) {
          return {
            label: "High leverage",
            comment:
              "A significant portion of assets is financed by debt. Lenders may scrutinize cash flows and collateral more closely.",
          };
        }
        if (ratio <= 1.2) {
          return {
            label: "Very high leverage",
            comment:
              "Liabilities are close to or exceed assets. This is a red flag for solvency and may limit access to new financing.",
          };
        }
        return {
          label: "Insolvent / negative equity",
          comment:
            "Debt clearly exceeds assets, implying negative equity. This typically signals severe financial distress or technical insolvency.",
        };
      }

      document.addEventListener("DOMContentLoaded", function () {
        const assetsInput = document.getElementById("total-assets");
        const debtInput = document.getElementById("total-debt");
        const calcBtn = document.getElementById("calculate-btn");
        const resetBtn = document.getElementById("reset-btn");
        const results = document.getElementById("results-container");
        const ratioValue = document.getElementById("ratio-value");
        const ratioPercent = document.getElementById("ratio-percent");
        const ratioInterpretation = document.getElementById(
          "ratio-interpretation"
        );
        const ratioComment = document.getElementById("ratio-comment");
        const summaryAssets = document.getElementById("summary-assets");
        const summaryDebt = document.getElementById("summary-debt");

        calcBtn.addEventListener("click", function () {
          const assets = parseFloat(assetsInput.value);
          const debt = parseFloat(debtInput.value);

          if (isNaN(assets) || assets <= 0) {
            alert("Please enter a valid positive value for total assets.");
            assetsInput.focus();
            return;
          }
          if (isNaN(debt) || debt < 0) {
            alert(
              "Please enter a valid non-negative value for total debt/liabilities."
            );
            debtInput.focus();
            return;
          }

          const ratio = debt / assets;
          const pct = ratio * 100;

          ratioValue.textContent = ratio.toFixed(2);
          ratioPercent.textContent = pct.toFixed(1) + "%";

          const interp = interpretRatio(ratio);
          ratioInterpretation.textContent = interp.label;
          ratioComment.textContent = interp.comment;

          summaryAssets.textContent = formatCurrency(assets);
          summaryDebt.textContent = formatCurrency(debt);

          results.classList.remove("hidden");
        });

        resetBtn.addEventListener("click", function () {
          assetsInput.value = "";
          debtInput.value = "";
          results.classList.add("hidden");
        });

        document.querySelectorAll(".scenario-btn").forEach((btn) => {
          btn.addEventListener("click", () => {
            const scenario = btn.getAttribute("data-scenario");
            let assets = 1000000;
            let debt = 250000;
            if (scenario === "balanced") {
              debt = 500000;
            } else if (scenario === "high") {
              debt = 850000;
            }
            assetsInput.value = assets;
            debtInput.value = debt;
            calcBtn.click();
          });
        });
      });
    </script>

