            <script>
              document.addEventListener('DOMContentLoaded', function() {
                const tabs = document.querySelectorAll('.tab-button');
                const panels = document.querySelectorAll('.tab-panel');
                const calculateBtn = document.getElementById('calculate-btn');
                const resultsContainer = document.getElementById('results-container');
                const errorMessage = document.getElementById('error-message');
                const resultEquation = document.getElementById('result-equation');
                const stepByStep = document.getElementById('step-by-step');
                
                let activeTab = 'standard-form';

                // Tab switching logic
                tabs.forEach(tab => {
                  tab.addEventListener('click', () => {
                    // Deactivate all
                    tabs.forEach(t => t.classList.remove('active'));
                    panels.forEach(p => p.classList.remove('active'));
                    
                    // Activate clicked
                    tab.classList.add('active');
                    activeTab = tab.getAttribute('data-tab');
                    document.getElementById(activeTab).classList.add('active');
                    
                    // Clear results
                    resultsContainer.classList.add('hidden');
                    errorMessage.textContent = '';
                  });
                });

                // --- Helper: Parse fractions and decimals ---
                function parseValue(val) {
                  val = String(val).trim();
                  if (val.includes('/')) {
                    const parts = val.split('/');
                    if (parts.length === 2) {
                      const num = parseFloat(parts[0]);
                      const den = parseFloat(parts[1]);
                      if (den !== 0 && !isNaN(num) && !isNaN(den)) {
                        return num / den;
                      }
                    }
                  }
                  return parseFloat(val);
                }
                
                function formatNumber(num) {
                    // Remove trailing zeros after decimal
                    return parseFloat(num.toFixed(10)).toString();
                }
                
                function showError(message) {
                    errorMessage.textContent = `Error: ${message}`;
                    resultsContainer.classList.add('hidden');
                }

                // --- Calculation Logic ---
                calculateBtn.addEventListener('click', () => {
                  let result, a, b, c, h, k, p, q;
                  let steps = '';
                  
                  try {
                    errorMessage.textContent = '';
                    
                    if (activeTab === 'standard-form') {
                      a = parseValue(document.getElementById('std-a').value);
                      b = parseValue(document.getElementById('std-b').value);
                      c = parseValue(document.getElementById('std-c').value);
                      
                      if (isNaN(a) || isNaN(b) || isNaN(c)) throw new Error('Please enter valid numbers for a, b, and c.');
                      if (Math.abs(a) < 1e-10) throw new Error("'a' cannot be zero for a parabola.");
                      
                      result = -b / (2 * a);
                      
                      steps = `
                        <p>1. Using Standard Form: $y = ax^2 + bx + c$</p>
                        <p>2. Identify coefficients: $a = ${formatNumber(a)}$, $b = ${formatNumber(b)}$, $c = ${formatNumber(c)}$</p>
                        <p>3. Apply the formula: $x = \\frac{-b}{2a}$</p>
                        <p>4. Substitute values: $x = \\frac{-(${formatNumber(b)})}{2(${formatNumber(a)})}$</p>
                        <p>5. Calculate: $x = \\frac{${formatNumber(-b)}}{${formatNumber(2*a)}} = ${formatNumber(result)}$</p>
                      `;
                      
                    } else if (activeTab === 'vertex-form') {
                      h = parseValue(document.getElementById('vtx-h').value);
                      
                      if (isNaN(h)) throw new Error("Please enter a valid number for 'h'.");
                      
                      result = h;
                      
                      steps = `
                        <p>1. Using Vertex Form: $y = a(x - h)^2 + k$</p>
                        <p>2. Identify the vertex $(h, k)$. From your equation, $h = ${formatNumber(h)}$.</p>
                        <p>3. The axis of symmetry is the vertical line $x = h$.</p>
                        <p>4. Result: $x = ${formatNumber(result)}$</p>
                      `;

                    } else if (activeTab === 'intercept-form') {
                      p = parseValue(document.getElementById('int-p').value);
                      q = parseValue(document.getElementById('int-q').value);
                      
                      if (isNaN(p) || isNaN(q)) throw new Error("Please enter valid numbers for 'p' and 'q'.");
                      
                      result = (p + q) / 2;
                      
                      steps = `
                        <p>1. Using Intercept Form: $y = a(x - p)(x - q)$</p>
                        <p>2. Identify the x-intercepts $p$ and $q$: $p = ${formatNumber(p)}$, $q = ${formatNumber(q)}$</p>
                        <p>3. Apply the midpoint formula: $x = \\frac{p + q}{2}$</p>
                        <p>4. Substitute values: $x = \\frac{${formatNumber(p)} + ${formatNumber(q)}}{2}$</p>
                        <p>5. Calculate: $x = \\frac{${formatNumber(p+q)}}{2} = ${formatNumber(result)}$</p>
                      `;
                    }
                    
                    // Display results
                    resultEquation.textContent = `x = ${formatNumber(result)}`;
                    stepByStep.innerHTML = steps;
                    resultsContainer.classList.remove('hidden');
                    
                    // Re-render MathJax
                    if (window.MathJax) {
                        window.MathJax.typesetPromise([stepByStep]);
                    }
                    
                  } catch (e) {
                    showError(e.message);
                  }
                });

              });
            </script>

