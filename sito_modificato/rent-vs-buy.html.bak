    <script src="https://cdn.jsdelivr.net/npm/chart.js@4.4.1/dist/chart.umd.min.js"></script>
    <script>
      (function () {
        const fmt = (v) => {
          if (!isFinite(v)) return "-";
          const sign = v < 0 ? "-" : "";
          v = Math.abs(v);
          if (v >= 1e6) return sign + "$" + (v / 1e6).toFixed(2) + "M";
          if (v >= 1e3) return sign + "$" + (v / 1e3).toFixed(1) + "k";
          return sign + "$" + v.toFixed(0);
        };

        const horizonInput = document.getElementById("horizon-years");
        const horizonLabel = document.getElementById("horizon-label");
        const summaryYears = document.getElementById("summary-years");
        horizonInput.addEventListener("input", () => {
          horizonLabel.textContent = horizonInput.value;
          summaryYears.textContent = horizonInput.value;
        });

        let chart;
        const ctx = document
          .getElementById("comparison-chart")
          .getContext("2d");
        chart = new Chart(ctx, {
          type: "line",
          data: {
            labels: [],
            datasets: [
              {
                label: "Net worth – Rent",
                data: [],
                borderColor: "#2563eb",
                backgroundColor: "rgba(37,99,235,0.1)",
                tension: 0.15,
              },
              {
                label: "Net worth – Buy",
                data: [],
                borderColor: "#16a34a",
                backgroundColor: "rgba(22,163,74,0.1)",
                tension: 0.15,
              },
            ],
          },
          options: {
            responsive: true,
            maintainAspectRatio: false,
            interaction: { mode: "index", intersect: false },
            plugins: {
              legend: { display: true },
              tooltip: {
                callbacks: {
                  label: function (ctx) {
                    return ctx.dataset.label + ": " + fmt(ctx.parsed.y);
                  },
                },
              },
            },
            scales: {
              y: {
                ticks: {
                  callback: function (value) {
                    return fmt(value);
                  },
                },
              },
            },
          },
        });

        function calculate() {
          const rentCurrent =
            parseFloat(document.getElementById("rent-current").value) || 0;
          const rentIncrease =
            (parseFloat(document.getElementById("rent-increase").value) || 0) /
            100;
          const rentIns =
            (parseFloat(document.getElementById("rent-insurance").value) || 0) *
            12;

          const price =
            parseFloat(document.getElementById("home-price").value) || 0;
          const downPct =
            (parseFloat(document.getElementById("down-payment-pct").value) ||
              0) / 100;
          const rate =
            (parseFloat(document.getElementById("mortgage-rate").value) || 0) /
            100;
          const termYears =
            parseInt(document.getElementById("loan-term").value) || 30;

          const propTaxRate =
            (parseFloat(document.getElementById("property-tax-rate").value) ||
              0) / 100;
          const insRate =
            (parseFloat(document.getElementById("home-insurance-rate").value) ||
              0) / 100;
          const maintRate =
            (parseFloat(document.getElementById("maintenance-rate").value) ||
              0) / 100;

          const homeGrowth =
            (parseFloat(document.getElementById("home-growth-rate").value) ||
              0) / 100;
          const investReturn =
            (parseFloat(document.getElementById("investment-return").value) ||
              0) / 100;
          const years =
            parseInt(document.getElementById("horizon-years").value) || 10;

          const buyerClosingRate =
            (parseFloat(
              document.getElementById("buyer-closing-costs-rate").value
            ) || 0) / 100;
          const sellerCostRate =
            (parseFloat(document.getElementById("seller-costs-rate").value) ||
              0) / 100;
          const includeOppCost = document.getElementById(
            "include-opportunity-cost"
          ).checked;

          const loanAmount = price * (1 - downPct);
          const downPayment = price * downPct;
          const buyerClosing = price * buyerClosingRate;
          const upfrontOwnerCash = downPayment + buyerClosing;

          const nMonths = termYears * 12;
          const mRate = rate / 12;
          let mortgagePayment = 0;
          if (rate > 0 && loanAmount > 0) {
            const pow = Math.pow(1 + mRate, nMonths);
            mortgagePayment = (loanAmount * (mRate * pow)) / (pow - 1);
          } else if (loanAmount > 0 && rate === 0) {
            mortgagePayment = loanAmount / nMonths;
          }

          let rent = rentCurrent * 12;
          let homeValue = price;
          let mortgageBalance = loanAmount;
          let investedRent = upfrontOwnerCash; // renter invests what buyer spends upfront
          let investedBuy = 0;

          const labels = [];
          const rentNW = [];
          const buyNW = [];
          const tbody = document.getElementById("results-table-body");
          tbody.innerHTML = "";

          let breakevenYear = null;

          for (let year = 1; year <= years; year++) {
            // Rent path
            const rentCost = rent + rentIns;
            // Owner path
            const ownerPropTax = homeValue * propTaxRate;
            const ownerIns = homeValue * insRate;
            const ownerMaint = homeValue * maintRate;
            const ownerAnnualCosts = ownerPropTax + ownerIns + ownerMaint;

            // Mortgage amortization for this year
            let interestPaidYear = 0;
            let principalPaidYear = 0;
            for (let m = 0; m < 12; m++) {
              if (mortgageBalance <= 0) break;
              const interest = mortgageBalance * mRate;
              let principal = mortgagePayment - interest;
              if (principal > mortgageBalance) principal = mortgageBalance;
              interestPaidYear += interest;
              principalPaidYear += principal;
              mortgageBalance -= principal;
            }
            const mortgagePaidYear = interestPaidYear + principalPaidYear;

            const ownerTotalCost = mortgagePaidYear + ownerAnnualCosts;

            // Opportunity cost: invest difference in annual housing cost
            if (includeOppCost) {
              const diff = ownerTotalCost - rentCost;
              // if renting cheaper, invest diff; if owning cheaper, invest for owner
              if (diff > 0) {
                // renting cheaper by diff
                investedRent = investedRent * (1 + investReturn) + diff;
                investedBuy = investedBuy * (1 + investReturn);
              } else {
                // owning cheaper by -diff
                investedRent = investedRent * (1 + investReturn);
                investedBuy = investedBuy * (1 + investReturn) + -diff;
              }
            } else {
              investedRent = investedRent * (1 + investReturn);
              investedBuy = investedBuy * (1 + investReturn);
            }

            // Home value growth
            homeValue = homeValue * (1 + homeGrowth);
            // Rent growth
            rent = rent * (1 + rentIncrease);

            // Net worths
            const renterNW = investedRent;
            const equityBeforeSell = homeValue - mortgageBalance;
            const sellingCosts = homeValue * sellerCostRate;
            const ownerNW =
              Math.max(equityBeforeSell - sellingCosts, 0) + investedBuy;

            labels.push("Year " + year);
            rentNW.push(renterNW);
            buyNW.push(ownerNW);

            if (breakevenYear === null && ownerNW > renterNW) {
              breakevenYear = year;
            }

            const tr = document.createElement("tr");
            const cells = [
              year,
              fmt(rentCost),
              fmt(investedRent),
              fmt(renterNW),
              fmt(principalPaidYear),
              fmt(homeValue),
              fmt(ownerAnnualCosts),
              fmt(ownerNW),
            ];
            cells.forEach((c, idx) => {
              const td = document.createElement("td");
              td.className = "px-2 py-1 border text-right";
              if (idx === 0) {
                td.className = "px-2 py-1 border text-center";
              }
              td.textContent = c;
              tr.appendChild(td);
            });
            tbody.appendChild(tr);
          }

          chart.data.labels = labels;
          chart.data.datasets[0].data = rentNW;
          chart.data.datasets[1].data = buyNW;
          chart.update();

          const nwRentEnd = rentNW[rentNW.length - 1] || 0;
          const nwBuyEnd = buyNW[buyNW.length - 1] || 0;
          document.getElementById("nw-rent").textContent = fmt(nwRentEnd);
          document.getElementById("nw-buy").textContent = fmt(nwBuyEnd);

          const rec = document.getElementById("recommendation-text");
          const diff = nwBuyEnd - nwRentEnd;
          const yrs = years;
          summaryYears.textContent = yrs;

          if (Math.abs(diff) < 1000) {
            rec.textContent = `Financially, renting and buying are very close over ${yrs} years (difference of about ${fmt(
              diff
            )}). In this case, non‑financial factors like flexibility, schools, and lifestyle may matter more.`;
          } else if (diff > 0) {
            rec.textContent = `Based on your assumptions, buying is projected to build about ${fmt(
              diff
            )} more net worth than renting after ${yrs} years.`;
          } else {
            rec.textContent = `Based on your assumptions, renting and investing the difference is projected to build about ${fmt(
              -diff
            )} more net worth than buying after ${yrs} years.`;
          }

          const beText = document.getElementById("breakeven-text");
          if (breakevenYear !== null) {
            beText.textContent = `Buying first overtakes renting in year ${breakevenYear}. If you expect to stay longer than this, buying becomes increasingly attractive.`;
            beText.classList.remove("hidden");
          } else {
            beText.textContent = `Over the entire ${yrs}-year horizon, buying never overtakes renting on a pure net‑worth basis under these assumptions.`;
            beText.classList.remove("hidden");
          }
        }

        document
          .getElementById("calculate-btn")
          .addEventListener("click", calculate);

        // Initial calculation
        calculate();
      })();
    </script>
  
