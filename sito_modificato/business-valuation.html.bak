    <script>
      function formatCurrency(value) {
        if (isNaN(value)) return "–";
        const abs = Math.abs(value);
        const sign = value < 0 ? "-" : "";
        let formatted;
        if (abs >= 1e9) {
          formatted = (abs / 1e9).toFixed(2) + "B";
        } else if (abs >= 1e6) {
          formatted = (abs / 1e6).toFixed(2) + "M";
        } else if (abs >= 1e3) {
          formatted = (abs / 1e3).toFixed(2) + "k";
        } else {
          formatted = abs.toFixed(0);
        }
        return sign + "$" + formatted;
      }

      function parseNum(id) {
        const el = document.getElementById(id);
        if (!el) return 0;
        const v = parseFloat(el.value);
        return isNaN(v) ? 0 : v;
      }

      function updateSummary(key, value) {
        const map = {
          dcf: "summary-dcf",
          ebitda: "summary-ebitda",
          revenue: "summary-revenue",
          asset: "summary-asset",
        };
        if (map[key]) {
          document.getElementById(map[key]).textContent = isNaN(value)
            ? "–"
            : formatCurrency(value);
        }
        const vals = [];
        [
          "summary-dcf",
          "summary-ebitda",
          "summary-revenue",
          "summary-asset",
        ].forEach((id) => {
          const el = document.getElementById(id);
          if (!el) return;
          const txt = el.textContent || "";
          if (txt === "–") return;
          const num = Number(txt.replace(/[^0-9.-]/g, ""));
          if (!isNaN(num)) vals.push(num);
        });
        const rangeEl = document.getElementById("summary-range");
        const noteEl = document.getElementById("summary-range-note");
        if (vals.length >= 2) {
          const min = Math.min(...vals);
          const max = Math.max(...vals);
          rangeEl.textContent =
            formatCurrency(min) + " – " + formatCurrency(max);
          noteEl.textContent =
            "Range based on " +
            vals.length +
            " methods. Actual deal terms, working capital, and structure can shift the final price.";
        } else if (vals.length === 1) {
          rangeEl.textContent = formatCurrency(vals[0]);
          noteEl.textContent =
            "Only one method has been calculated so far. Run at least one more for a range.";
        } else {
          rangeEl.textContent = "Run at least two methods to see a range.";
          noteEl.textContent = "";
        }
      }

      document.addEventListener("DOMContentLoaded", function () {
        // Tabs
        document.querySelectorAll(".tab-btn").forEach((btn) => {
          btn.addEventListener("click", () => {
            const target = btn.getAttribute("data-tab");
            document
              .querySelectorAll(".tab-btn")
              .forEach((b) => b.classList.remove("tab-active"));
            btn.classList.add("tab-active");
            document.querySelectorAll(".tab-panel").forEach((p) => {
              p.classList.toggle("hidden", p.id !== target);
            });
          });
        });

        // DCF
        document.getElementById("dcf-calc").addEventListener("click", () => {
          const fcf0 = parseNum("dcf-fcf0");
          const years = Math.max(
            1,
            Math.min(10, parseInt(parseNum("dcf-years")) || 0)
          );
          const g = parseNum("dcf-growth") / 100;
          const r = parseNum("dcf-discount") / 100;
          const gT = parseNum("dcf-terminal-growth") / 100;
          const excessCash = parseNum("dcf-excess-cash");
          const debt = parseNum("dcf-debt");

          if (r <= gT) {
            alert(
              "Discount rate must be greater than terminal growth rate for a stable DCF. Please adjust your inputs."
            );
            return;
          }

          let ev = 0;
          let fcf = fcf0;
          for (let t = 1; t <= years; t++) {
            fcf = fcf * (1 + g);
            const pv = fcf / Math.pow(1 + r, t);
            ev += pv;
          }
          const fcfNext = fcf * (1 + gT);
          const tv = fcfNext / (r - gT);
          const pvTv = tv / Math.pow(1 + r, years);
          ev += pvTv;

          const equity = ev + excessCash - debt;

          document.getElementById("dcf-enterprise").textContent =
            formatCurrency(ev);
          document.getElementById("dcf-equity").textContent =
            formatCurrency(equity);
          document.getElementById("dcf-note").textContent =
            "Includes " +
            years +
            " years of forecast cash flows plus a terminal value using the Gordon growth model.";
          document.getElementById("dcf-output").classList.remove("hidden");

          updateSummary("dcf", equity);
        });

        document.getElementById("dcf-reset").addEventListener("click", () => {
          document.getElementById("dcf-fcf0").value = 250000;
          document.getElementById("dcf-years").value = 5;
          document.getElementById("dcf-growth").value = 5;
          document.getElementById("dcf-discount").value = 18;
          document.getElementById("dcf-terminal-growth").value = 2;
          document.getElementById("dcf-excess-cash").value = 0;
          document.getElementById("dcf-debt").value = 0;
          document.getElementById("dcf-output").classList.add("hidden");
          updateSummary("dcf", NaN);
        });

        // Multiples
        document.getElementById("mult-calc").addEventListener("click", () => {
          const ebitda = parseNum("mult-ebitda");
          const ebitdaMult = parseNum("mult-ebitda-multiple");
          const revenue = parseNum("mult-revenue");
          const revenueMult = parseNum("mult-revenue-multiple");
          const excessCash = parseNum("mult-excess-cash");
          const debt = parseNum("mult-debt");

          let ebitdaEquity = NaN;
          let revenueEquity = NaN;

          if (ebitda > 0 && ebitdaMult > 0) {
            const evE = ebitda * ebitdaMult;
            ebitdaEquity = evE + excessCash - debt;
            document.getElementById("mult-ebitda-value").textContent =
              formatCurrency(ebitdaEquity);
          } else {
            document.getElementById("mult-ebitda-value").textContent = "–";
          }

          if (revenue > 0 && revenueMult > 0) {
            const evR = revenue * revenueMult;
            revenueEquity = evR + excessCash - debt;
            document.getElementById("mult-revenue-value").textContent =
              formatCurrency(revenueEquity);
          } else {
            document.getElementById("mult-revenue-value").textContent = "–";
          }

          document.getElementById("mult-note").textContent =
            "Values are based on the multiples you entered. For best results, source multiples from comparable deals in your industry.";
          document.getElementById("mult-output").classList.remove("hidden");

          if (!isNaN(ebitdaEquity)) updateSummary("ebitda", ebitdaEquity);
          if (!isNaN(revenueEquity)) updateSummary("revenue", revenueEquity);
        });

        document.getElementById("mult-reset").addEventListener("click", () => {
          document.getElementById("mult-ebitda").value = 300000;
          document.getElementById("mult-ebitda-multiple").value = 4.5;
          document.getElementById("mult-revenue").value = 1500000;
          document.getElementById("mult-revenue-multiple").value = 1.2;
          document.getElementById("mult-excess-cash").value = 0;
          document.getElementById("mult-debt").value = 0;
          document.getElementById("mult-output").classList.add("hidden");
          updateSummary("ebitda", NaN);
          updateSummary("revenue", NaN);
        });

        // Asset-based
        document.getElementById("asset-calc").addEventListener("click", () => {
          const cash = parseNum("asset-cash");
          const ar = parseNum("asset-ar");
          const inv = parseNum("asset-inventory");
          const ppe = parseNum("asset-ppe");
          const other = parseNum("asset-other");
          const liab = parseNum("asset-liabilities");

          const totalAssets = cash + ar + inv + ppe + other;
          const equity = totalAssets - liab;

          document.getElementById("asset-total").textContent =
            formatCurrency(totalAssets);
          document.getElementById("asset-liab-out").textContent =
            formatCurrency(liab);
          document.getElementById("asset-equity").textContent =
            formatCurrency(equity);
          document.getElementById("asset-output").classList.remove("hidden");

          updateSummary("asset", equity);
        });

        document.getElementById("asset-reset").addEventListener("click", () => {
          document.getElementById("asset-cash").value = 50000;
          document.getElementById("asset-ar").value = 80000;
          document.getElementById("asset-inventory").value = 60000;
          document.getElementById("asset-ppe").value = 400000;
          document.getElementById("asset-other").value = 30000;
          document.getElementById("asset-liabilities").value = 250000;
          document.getElementById("asset-output").classList.add("hidden");
          updateSummary("asset", NaN);
        });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
