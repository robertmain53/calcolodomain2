    <script>
      // Crack width calculator logic
      function getNumeric(id) {
        const v = parseFloat(document.getElementById(id).value);
        return isNaN(v) ? 0 : v;
      }

      function formatNumber(val, decimals) {
        return Number(val).toFixed(decimals);
      }

      document
        .getElementById("exposureClass")
        .addEventListener("change", function () {
          const wrapper = document.getElementById("customLimitWrapper");
          if (this.value === "custom") {
            wrapper.classList.remove("hidden");
          } else {
            wrapper.classList.add("hidden");
          }
        });

      document.getElementById("calcBtn").addEventListener("click", function () {
        const memberType = document.getElementById("memberType").value;
        const phi = getNumeric("phi"); // mm
        const s = getNumeric("s"); // mm
        const cEff = getNumeric("cEff"); // mm
        const rhoPpermil = getNumeric("rhoPeff"); // per mille
        const sigmaS = getNumeric("sigmaS"); // MPa
        const epsCmInput = getNumeric("epsilonCm"); // x1e-3
        const kT = parseFloat(document.getElementById("kT").value);

        if (phi <= 0 || s <= 0 || cEff <= 0 || rhoPpermil <= 0 || sigmaS <= 0) {
          alert(
            "Please enter positive values for bar diameter, spacing, cover, reinforcement ratio and steel stress."
          );
          return;
        }

        // Convert reinforcement ratio from per mille to decimal
        const rhoPeff = rhoPpermil / 1000.0;

        // Eurocode coefficients (typical values)
        const k1 = 0.8; // high bond bars
        const k2 = memberType === "bending" ? 0.5 : 1.0;
        const k3 = 3.4;
        const k4 = 0.425;

        // Limiting crack spacing sr,max (mm)
        const srMax = k3 * cEff + k1 * k2 * k4 * (phi / rhoPeff);

        // Mean strain difference
        const Es = 200000; // MPa
        const epsSmMinusCm = Math.max(
          0,
          (kT * sigmaS) / Es - epsCmInput / 1000.0
        ); // convert epsCm from x1e-3

        // Crack width (mm)
        const wk = srMax * epsSmMinusCm;

        // Limit
        const exposureSel = document.getElementById("exposureClass").value;
        let wkLimit;
        if (exposureSel === "custom") {
          wkLimit = getNumeric("wkLimit");
          if (wkLimit <= 0) wkLimit = 0.3;
        } else {
          wkLimit = parseFloat(exposureSel);
        }

        // Update UI
        document.getElementById("srmaxOut").textContent = formatNumber(
          srMax,
          1
        );
        document.getElementById("strainDiffOut").textContent = formatNumber(
          epsSmMinusCm * 1000,
          3
        ); // back to x1e-3
        document.getElementById("wkOut").textContent = formatNumber(wk, 3);
        document.getElementById("wkLimitOut").textContent = formatNumber(
          wkLimit,
          3
        );

        const passFail = document.getElementById("passFailMsg");
        if (wk <= wkLimit) {
          passFail.textContent =
            "OK: Calculated crack width is within the selected limit.";
          passFail.className = "mt-3 text-sm font-medium text-green-700";
        } else {
          passFail.textContent =
            "Not OK: Calculated crack width exceeds the selected limit. Consider reducing bar spacing, using smaller bars, or increasing reinforcement.";
          passFail.className = "mt-3 text-sm font-medium text-red-700";
        }

        document.getElementById("resultsCard").classList.remove("hidden");
      });

      document
        .getElementById("resetBtn")
        .addEventListener("click", function () {
          document.getElementById("memberType").value = "bending";
          document.getElementById("exposureClass").value = "0.3";
          document.getElementById("customLimitWrapper").classList.add("hidden");
          document.getElementById("phi").value = 16;
          document.getElementById("s").value = 150;
          document.getElementById("cEff").value = 30;
          document.getElementById("rhoPeff").value = 5;
          document.getElementById("sigmaS").value = 200;
          document.getElementById("epsilonCm").value = 0.1;
          document.getElementById("kT").value = "0.6";
          document.getElementById("wkLimit").value = 0.3;
          document.getElementById("resultsCard").classList.add("hidden");
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
