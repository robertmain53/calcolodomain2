    <script>
      // A* Search Calculator Script
      (function () {
        const gridContainer = document.getElementById("grid-container");
        const rowsInput = document.getElementById("rows-input");
        const colsInput = document.getElementById("cols-input");
        const resizeBtn = document.getElementById("resize-grid-btn");
        const heuristicSelect = document.getElementById("heuristic-select");
        const allowDiagonalCheckbox = document.getElementById("allow-diagonal");
        const runBtn = document.getElementById("run-btn");
        const stepBtn = document.getElementById("step-btn");
        const resetBtn = document.getElementById("reset-btn");
        const clearWallsBtn = document.getElementById("clear-walls-btn");
        const statusText = document.getElementById("status-text");
        const stepCountEl = document.getElementById("step-count");
        const openSizeEl = document.getElementById("open-size");
        const closedSizeEl = document.getElementById("closed-size");
        const pathCostEl = document.getElementById("path-cost");
        const nodeDetails = document.getElementById("node-details");
        const pathOutput = document.getElementById("path-output");
        const stepDelayInput = document.getElementById("step-delay");
        const editModeButtons = document.querySelectorAll(".edit-mode-btn");

        let grid = [];
        let rows = parseInt(rowsInput.value, 10);
        let cols = parseInt(colsInput.value, 10);
        let start = { r: 2, c: 2 };
        let goal = { r: rows - 3, c: cols - 3 };
        let editMode = "wall";
        let running = false;
        let finished = false;
        let openSet = [];
        let closedSet = new Set();
        let cameFrom = new Map();
        let gScore = new Map();
        let fScore = new Map();
        let stepCount = 0;
        let autoRunTimer = null;

        function key(r, c) {
          return r + "," + c;
        }

        function initGrid() {
          grid = [];
          gridContainer.innerHTML = "";
          gridContainer.style.display = "grid";
          gridContainer.style.gridTemplateRows = `repeat(${rows}, minmax(0, 1fr))`;
          gridContainer.style.gridTemplateColumns = `repeat(${cols}, minmax(0, 1fr))`;

          for (let r = 0; r < rows; r++) {
            const row = [];
            for (let c = 0; c < cols; c++) {
              const cell = {
                r,
                c,
                type: "empty", // empty, wall, start, goal
                element: null,
              };
              const div = document.createElement("button");
              div.type = "button";
              div.className =
                "grid-cell cell-empty text-[10px] md:text-[9px] flex flex-col items-center justify-center focus:outline-none";
              div.setAttribute("data-r", r);
              div.setAttribute("data-c", c);
              div.addEventListener("click", onCellClick);
              cell.element = div;
              gridContainer.appendChild(div);
              row.push(cell);
            }
            grid.push(row);
          }
          // Set default start & goal
          setCellType(start.r, start.c, "start");
          setCellType(goal.r, goal.c, "goal");
          resetSearchState();
        }

        function setCellType(r, c, type) {
          const cell = grid[r][c];
          cell.type = type;
          const el = cell.element;
          el.className = "grid-cell";
          el.innerHTML = "";
          el.classList.add("cell-" + (type === "empty" ? "empty" : type));
          if (type === "start") {
            el.textContent = "S";
            el.classList.add("font-semibold", "text-white");
          } else if (type === "goal") {
            el.textContent = "G";
            el.classList.add("font-semibold", "text-white");
          }
        }

        function onCellClick(e) {
          if (running) return;
          const r = parseInt(e.currentTarget.getAttribute("data-r"), 10);
          const c = parseInt(e.currentTarget.getAttribute("data-c"), 10);
          const cell = grid[r][c];

          if (editMode === "start") {
            // Clear previous start
            setCellType(start.r, start.c, "empty");
            start = { r, c };
            setCellType(r, c, "start");
          } else if (editMode === "goal") {
            setCellType(goal.r, goal.c, "empty");
            goal = { r, c };
            setCellType(r, c, "goal");
          } else if (editMode === "wall") {
            if (cell.type === "start" || cell.type === "goal") return;
            cell.type = "wall";
            cell.element.className = "grid-cell cell-wall";
          } else if (editMode === "erase") {
            if (cell.type === "start" || cell.type === "goal") return;
            setCellType(r, c, "empty");
          }

          resetSearchState();
        }

        editModeButtons.forEach((btn) => {
          btn.addEventListener("click", () => {
            editModeButtons.forEach((b) =>
              b.classList.remove("bg-gray-100", "border-gray-300")
            );
            btn.classList.add("bg-gray-100", "border-gray-300");
            editMode = btn.getAttribute("data-mode");
          });
        });

        function heuristic(a, b) {
          const dr = Math.abs(a.r - b.r);
          const dc = Math.abs(a.c - b.c);
          const type = heuristicSelect.value;
          if (type === "euclidean") {
            return Math.sqrt(dr * dr + dc * dc);
          } else if (type === "diagonal") {
            return Math.max(dr, dc);
          }
          // Manhattan
          return dr + dc;
        }

        function getNeighbors(node) {
          const dirs4 = [
            { dr: -1, dc: 0 },
            { dr: 1, dc: 0 },
            { dr: 0, dc: -1 },
            { dr: 0, dc: 1 },
          ];
          const dirsDiag = [
            { dr: -1, dc: -1 },
            { dr: -1, dc: 1 },
            { dr: 1, dc: -1 },
            { dr: 1, dc: 1 },
          ];
          const dirs = allowDiagonalCheckbox.checked
            ? dirs4.concat(dirsDiag)
            : dirs4;
          const neighbors = [];
          for (const d of dirs) {
            const nr = node.r + d.dr;
            const nc = node.c + d.dc;
            if (nr < 0 || nr >= rows || nc < 0 || nc >= cols) continue;
            const cell = grid[nr][nc];
            if (cell.type === "wall") continue;
            neighbors.push(cell);
          }
          return neighbors;
        }

        function resetSearchState() {
          if (autoRunTimer) {
            clearInterval(autoRunTimer);
            autoRunTimer = null;
          }
          running = false;
          finished = false;
          openSet = [];
          closedSet = new Set();
          cameFrom = new Map();
          gScore = new Map();
          fScore = new Map();
          stepCount = 0;
          stepCountEl.textContent = "0";
          openSizeEl.textContent = "0";
          closedSizeEl.textContent = "0";
          pathCostEl.textContent = "–";
          statusText.textContent =
            "Ready. Edit the grid, then click “Run A*” or “Step”.";
          nodeDetails.innerHTML =
            '<p class="text-gray-500">No node selected.</p>';
          pathOutput.innerHTML =
            '<p class="text-gray-500">No path computed yet.</p>';

          // Reset cell visual states except walls/start/goal
          for (let r = 0; r < rows; r++) {
            for (let c = 0; c < cols; c++) {
              const cell = grid[r][c];
              const el = cell.element;
              el.onclick = onCellClick;
              el.classList.remove("cell-open", "cell-closed", "cell-path");
              if (cell.type === "wall") {
                el.className = "grid-cell cell-wall";
              } else if (cell.type === "start") {
                setCellType(r, c, "start");
              } else if (cell.type === "goal") {
                setCellType(r, c, "goal");
              } else {
                el.className = "grid-cell cell-empty";
              }
            }
          }

          // Initialize scores for start node
          const startCell = grid[start.r][start.c];
          const startKey = key(start.r, start.c);
          gScore.set(startKey, 0);
          fScore.set(startKey, heuristic(startCell, goal));
          openSet.push(startCell);
          updateSetsDisplay();
        }

        function updateSetsDisplay() {
          openSizeEl.textContent = openSet.length.toString();
          closedSizeEl.textContent = closedSet.size.toString();
        }

        function reconstructPath(current) {
          const path = [current];
          let curKey = key(current.r, current.c);
          while (cameFrom.has(curKey)) {
            const prev = cameFrom.get(curKey);
            path.push(prev);
            curKey = key(prev.r, prev.c);
          }
          return path.reverse();
        }

        function highlightPath(path) {
          let totalCost = 0;
          for (let i = 0; i < path.length; i++) {
            const cell = path[i];
            if (cell.type === "start" || cell.type === "goal") continue;
            cell.element.className = "grid-cell cell-path";
            // approximate cost from gScore
          }
          const goalKey = key(goal.r, goal.c);
          const g = gScore.get(goalKey);
          if (g !== undefined) {
            totalCost = g;
          }
          pathCostEl.textContent = totalCost.toFixed(3);

          // Output coordinates
          const lines = path.map((p) => `(${p.r}, ${p.c})`).join(" → ");
          pathOutput.textContent = lines || "No path.";
        }

        function setCellVisitedVisual(cell, state) {
          if (cell.type === "start" || cell.type === "goal") return;
          if (state === "open") {
            cell.element.className = "grid-cell cell-open";
          } else if (state === "closed") {
            cell.element.className = "grid-cell cell-closed";
          }
        }

        function aStarStep() {
          if (finished) return;

          if (openSet.length === 0) {
            statusText.textContent = "No path found. Open set is empty.";
            finished = true;
            running = false;
            if (autoRunTimer) {
              clearInterval(autoRunTimer);
              autoRunTimer = null;
            }
            return;
          }

          // Find node in open set with lowest fScore
          let bestIndex = 0;
          let bestF = Infinity;
          for (let i = 0; i < openSet.length; i++) {
            const cell = openSet[i];
            const k = key(cell.r, cell.c);
            const f = fScore.get(k) ?? Infinity;
            if (f < bestF) {
              bestF = f;
              bestIndex = i;
            }
          }
          const current = openSet.splice(bestIndex, 1)[0];
          const currentKey = key(current.r, current.c);
          closedSet.add(currentKey);
          setCellVisitedVisual(current, "closed");

          stepCount++;
          stepCountEl.textContent = stepCount.toString();
          updateSetsDisplay();

          if (current.r === goal.r && current.c === goal.c) {
            const path = reconstructPath(current);
            highlightPath(path);
            statusText.textContent =
              "Goal reached. Optimal path highlighted in yellow.";
            finished = true;
            running = false;
            if (autoRunTimer) {
              clearInterval(autoRunTimer);
              autoRunTimer = null;
            }
            return;
          }

          const neighbors = getNeighbors(current);
          const currentG = gScore.get(currentKey) ?? Infinity;

          for (const neighbor of neighbors) {
            const nKey = key(neighbor.r, neighbor.c);
            if (closedSet.has(nKey)) continue;

            const dr = Math.abs(neighbor.r - current.r);
            const dc = Math.abs(neighbor.c - current.c);
            const moveCost = dr === 1 && dc === 1 ? Math.SQRT2 : 1;
            const tentativeG = currentG + moveCost;

            const existingG = gScore.get(nKey);
            if (existingG === undefined || tentativeG < existingG) {
              cameFrom.set(nKey, current);
              gScore.set(nKey, tentativeG);
              const h = heuristic(neighbor, goal);
              fScore.set(nKey, tentativeG + h);

              if (
                !openSet.some((c) => c.r === neighbor.r && c.c === neighbor.c)
              ) {
                openSet.push(neighbor);
                setCellVisitedVisual(neighbor, "open");
              }
            }
          }

          statusText.textContent =
            "Searching… A* has not reached the goal yet.";
        }

        function onRunClick() {
          if (running) {
            // Stop auto-run
            running = false;
            if (autoRunTimer) {
              clearInterval(autoRunTimer);
              autoRunTimer = null;
            }
            statusText.textContent = "Paused. You can resume, step, or reset.";
            runBtn.textContent = "Run A*";
            return;
          }
          if (finished) {
            resetSearchState();
          }
          running = true;
          runBtn.textContent = "Pause";
          const delay = Math.max(
            0,
            Math.min(2000, parseInt(stepDelayInput.value, 10) || 0)
          );
          if (autoRunTimer) clearInterval(autoRunTimer);
          autoRunTimer = setInterval(() => {
            if (!running || finished) {
              clearInterval(autoRunTimer);
              autoRunTimer = null;
              runBtn.textContent = "Run A*";
              return;
            }
            aStarStep();
          }, delay);
        }

        function onStepClick() {
          if (finished) {
            resetSearchState();
          }
          if (running) {
            // If auto-running, pause and then step
            running = false;
            if (autoRunTimer) {
              clearInterval(autoRunTimer);
              autoRunTimer = null;
            }
            runBtn.textContent = "Run A*";
          }
          aStarStep();
        }

        function onResetClick() {
          resetSearchState();
        }

        function onClearWallsClick() {
          for (let r = 0; r < rows; r++) {
            for (let c = 0; c < cols; c++) {
              const cell = grid[r][c];
              if (cell.type === "wall") {
                setCellType(r, c, "empty");
              }
            }
          }
          resetSearchState();
        }

        resizeBtn.addEventListener("click", () => {
          const newRows = Math.max(
            4,
            Math.min(30, parseInt(rowsInput.value, 10) || rows)
          );
          const newCols = Math.max(
            4,
            Math.min(30, parseInt(colsInput.value, 10) || cols)
          );
          rows = newRows;
          cols = newCols;
          start = { r: 2, c: 2 };
          goal = { r: rows - 3, c: cols - 3 };
          initGrid();
        });

        runBtn.addEventListener("click", onRunClick);
        stepBtn.addEventListener("click", onStepClick);
        resetBtn.addEventListener("click", onResetClick);
        clearWallsBtn.addEventListener("click", onClearWallsClick);

        // Node details: click visited cells to inspect g, h, f
        gridContainer.addEventListener("click", function (e) {
          const target = e.target.closest(".grid-cell");
          if (!target) return;
          const r = parseInt(target.getAttribute("data-r"), 10);
          const c = parseInt(target.getAttribute("data-c"), 10);
          const cell = grid[r][c];
          const k = key(r, c);
          if (!gScore.has(k) && cell.type !== "start") {
            // Not visited
            return;
          }
          const g = gScore.get(k) ?? 0;
          const h = heuristic(cell, goal);
          const f = g + h;
          nodeDetails.innerHTML = `
        <p><span class="font-semibold">Node:</span> (${r}, ${c})</p>
        <p><span class="font-semibold">Type:</span> ${cell.type}</p>
        <p><span class="font-semibold">g(n):</span> ${g.toFixed(3)}</p>
        <p><span class="font-semibold">h(n):</span> ${h.toFixed(3)}</p>
        <p><span class="font-semibold">f(n) = g + h:</span> ${f.toFixed(3)}</p>
      `;
        });

        // Initialize
        initGrid();
      })();
    </script>

