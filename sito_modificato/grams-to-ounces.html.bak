      function updateFromGrams() {
        const grams = parseFloat(gramsInput.value);
        if (isNaN(grams)) {
          ouncesInput.value = '';
          return;
        }
        const factor = OZ_FACTORS[ozSystem.value];
        const oz = grams / factor;
        ouncesInput.value = oz.toFixed(4).replace(/\.?0+$/, '');
      }

      function updateFromOunces() {
        const ounces = parseFloat(ouncesInput.value);
        if (isNaN(ounces)) {
          gramsInput.value = '';
          return;
        }
        const factor = OZ_FACTORS[ozSystem.value];
        const grams = ounces * factor;
        gramsInput.value = grams.toFixed(2).replace(/\.?0+$/, '');
      }

      // initial render
      updateFromGrams();

      gramsInput.addEventListener('input', function() {
        if (direction === 'g-to-oz') updateFromGrams();
      });

      ouncesInput.addEventListener('input', function() {
        if (direction === 'oz-to-g') updateFromOunces();
      });

      ozSystem.addEventListener('change', function() {
        if (direction === 'g-to-oz') {
          updateFromGrams();
        } else {
          updateFromOunces();
        }
      });

      swapBtn.addEventListener('click', function() {
        if (direction === 'g-to-oz') {
          direction = 'oz-to-g';
          swapBtn.textContent = 'Swap direction (g → oz)';
          updateFromOunces();
          roundedHint.textContent = 'Rounded to 2 decimals for grams.';
        } else {
          direction = 'g-to-oz';
          swapBtn.textContent = 'Swap direction (oz → g)';
          updateFromGrams();
          roundedHint.textContent = 'Rounded to 4 decimals for practicality.';
        }
      });
    </script>

