    <script>
      // Utility functions
      function cmToMeters(cm) {
        return cm / 100;
      }
      function inchesToCm(inches) {
        return inches * 2.54;
      }
      function cmToInches(cm) {
        return cm / 2.54;
      }
      function kgToLb(kg) {
        return kg * 2.2046226218;
      }
      function lbToKg(lb) {
        return lb / 2.2046226218;
      }
      function round(value, decimals) {
        if (!isFinite(value)) return NaN;
        const factor = Math.pow(10, decimals);
        return Math.round(value * factor) / factor;
      }

      function getHeightInCm() {
        const metricActive = !document
          .getElementById("height-metric-group")
          .classList.contains("hidden");
        if (metricActive) {
          const cm = parseFloat(document.getElementById("height-cm").value);
          return isNaN(cm) ? null : cm;
        } else {
          const ft = parseFloat(document.getElementById("height-ft").value);
          const inch =
            parseFloat(document.getElementById("height-in").value) || 0;
          if (isNaN(ft) || ft <= 0) return null;
          const totalInches = ft * 12 + inch;
          return inchesToCm(totalInches);
        }
      }

      function getWeightInKg() {
        const kgActive = document
          .getElementById("weight-unit-kg")
          .classList.contains("bg-blue-600");
        const w = parseFloat(document.getElementById("weight").value);
        if (isNaN(w) || w <= 0) return null;
        return kgActive ? w : lbToKg(w);
      }

      function bmiCategory(bmi) {
        if (!isFinite(bmi))
          return { label: "–", color: "bg-gray-100 text-gray-700" };
        if (bmi < 18.5)
          return { label: "Underweight", color: "bg-blue-100 text-blue-800" };
        if (bmi < 25)
          return {
            label: "Healthy weight",
            color: "bg-green-100 text-green-800",
          };
        if (bmi < 30)
          return {
            label: "Overweight",
            color: "bg-yellow-100 text-yellow-800",
          };
        if (bmi < 35)
          return {
            label: "Obesity class I",
            color: "bg-orange-100 text-orange-800",
          };
        if (bmi < 40)
          return {
            label: "Obesity class II",
            color: "bg-red-100 text-red-800",
          };
        return { label: "Obesity class III", color: "bg-red-200 text-red-900" };
      }

      function computeIBW(heightInches, sex) {
        const results = {};

        // Devine
        if (sex === "male") {
          results.devine = 50 + 2.3 * (heightInches - 60);
        } else {
          results.devine = 45.5 + 2.3 * (heightInches - 60);
        }

        // Hamwi
        if (sex === "male") {
          results.hamwi = 48.0 + 2.7 * (heightInches - 60);
        } else {
          results.hamwi = 45.5 + 2.2 * (heightInches - 60);
        }

        // Robinson
        if (sex === "male") {
          results.robinson = 52 + 1.9 * (heightInches - 60);
        } else {
          results.robinson = 49 + 1.7 * (heightInches - 60);
        }

        // Miller
        if (sex === "male") {
          results.miller = 56.2 + 1.41 * (heightInches - 60);
        } else {
          results.miller = 53.1 + 1.36 * (heightInches - 60);
        }

        return results;
      }

      function updateUnitToggles() {
        const heightMetricBtn = document.getElementById("height-unit-metric");
        const heightImperialBtn = document.getElementById(
          "height-unit-imperial"
        );
        const heightMetricGroup = document.getElementById(
          "height-metric-group"
        );
        const heightImperialGroup = document.getElementById(
          "height-imperial-group"
        );

        heightMetricBtn.addEventListener("click", () => {
          heightMetricBtn.classList.add("bg-blue-600", "text-white");
          heightImperialBtn.classList.remove("bg-blue-600", "text-white");
          heightMetricGroup.classList.remove("hidden");
          heightImperialGroup.classList.add("hidden");
        });

        heightImperialBtn.addEventListener("click", () => {
          heightImperialBtn.classList.add("bg-blue-600", "text-white");
          heightMetricBtn.classList.remove("bg-blue-600", "text-white");
          heightMetricGroup.classList.add("hidden");
          heightImperialGroup.classList.remove("hidden");
        });

        const weightKgBtn = document.getElementById("weight-unit-kg");
        const weightLbBtn = document.getElementById("weight-unit-lb");

        weightKgBtn.addEventListener("click", () => {
          weightKgBtn.classList.add("bg-blue-600", "text-white");
          weightLbBtn.classList.remove("bg-blue-600", "text-white");
        });

        weightLbBtn.addEventListener("click", () => {
          weightLbBtn.classList.add("bg-blue-600", "text-white");
          weightKgBtn.classList.remove("bg-blue-600", "text-white");
        });
      }

      function calculate() {
        const errorEl = document.getElementById("error-msg");
        errorEl.textContent = "";

        const sex = document.getElementById("sex").value;
        const heightCm = getHeightInCm();
        const weightKg = getWeightInKg();

        if (!heightCm || heightCm < 120 || heightCm > 250) {
          errorEl.textContent = "Please enter a realistic adult height.";
          document.getElementById("summary-section").classList.add("hidden");
          document.getElementById("results-section").classList.add("hidden");
          return;
        }
        if (!weightKg || weightKg < 30 || weightKg > 400) {
          errorEl.textContent = "Please enter a realistic current weight.";
          document.getElementById("summary-section").classList.add("hidden");
          document.getElementById("results-section").classList.add("hidden");
          return;
        }

        const heightM = cmToMeters(heightCm);
        const heightInches = cmToInches(heightCm);
        const bmi = weightKg / (heightM * heightM);

        const ibwAll = computeIBW(heightInches, sex);

        const selectedFormulas = Array.from(
          document.querySelectorAll(".formula-checkbox")
        )
          .filter((cb) => cb.checked && cb.value !== "bmi")
          .map((cb) => cb.value);

        const tbody = document.getElementById("results-tbody");
        tbody.innerHTML = "";

        let sumIbw = 0;
        let countIbw = 0;

        const formulaNames = {
          devine: "Devine",
          hamwi: "Hamwi",
          robinson: "Robinson",
          miller: "Miller",
        };

        selectedFormulas.forEach((key) => {
          const ibwKg = ibwAll[key];
          if (!isFinite(ibwKg)) return;
          sumIbw += ibwKg;
          countIbw++;

          const ibwLb = kgToLb(ibwKg);
          const diffKg = weightKg - ibwKg;
          const diffLb = kgToLb(diffKg);
          const sign = diffKg > 0 ? "+" : "";

          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="px-3 py-2 border-b">${formulaNames[key] || key}</td>
        <td class="px-3 py-2 border-b text-right">${round(ibwKg, 1)}</td>
        <td class="px-3 py-2 border-b text-right">${round(ibwLb, 1)}</td>
        <td class="px-3 py-2 border-b text-right">
          ${sign}${round(diffKg, 1)} kg
          <span class="text-xs text-gray-500 ml-1">(${sign}${round(
            diffLb,
            1
          )} lb)</span>
        </td>
      `;
          tbody.appendChild(tr);
        });

        // Average IBW
        const avgIbwKg = countIbw > 0 ? sumIbw / countIbw : NaN;
        const avgIbwLb = kgToLb(avgIbwKg);

        document.getElementById("avg-ibw").textContent = isFinite(avgIbwKg)
          ? `${round(avgIbwKg, 1)} kg`
          : "–";
        document.getElementById("avg-ibw-lb").textContent = isFinite(avgIbwLb)
          ? `${round(avgIbwLb, 1)} lb`
          : "–";

        // BMI range
        const bmiMin = 18.5;
        const bmiMax = 24.9;
        const wMinKg = bmiMin * heightM * heightM;
        const wMaxKg = bmiMax * heightM * heightM;
        const wMinLb = kgToLb(wMinKg);
        const wMaxLb = kgToLb(wMaxKg);

        if (document.querySelector('.formula-checkbox[value="bmi"]').checked) {
          document.getElementById("bmi-range-kg").textContent = `${round(
            wMinKg,
            1
          )} – ${round(wMaxKg, 1)} kg`;
          document.getElementById("bmi-range-lb").textContent = `${round(
            wMinLb,
            1
          )} – ${round(wMaxLb, 1)} lb`;
        } else {
          document.getElementById("bmi-range-kg").textContent = "–";
          document.getElementById("bmi-range-lb").textContent = "–";
        }

        // BMI and category
        document.getElementById("bmi-value").textContent = round(bmi, 1);
        const cat = bmiCategory(bmi);
        const bmiCatEl = document.getElementById("bmi-category");
        bmiCatEl.textContent = cat.label;
        bmiCatEl.className =
          "text-xs font-medium pill inline-block px-2 py-0.5 mt-1 " + cat.color;

        // Adjusted body weight (using Devine IBW)
        const devineIbw = ibwAll.devine;
        const adjBwKgEl = document.getElementById("adj-bw-kg");
        const adjBwLbEl = document.getElementById("adj-bw-lb");
        const adjNoteEl = document.getElementById("adj-bw-note");

        if (isFinite(devineIbw) && weightKg > 1.2 * devineIbw) {
          const adjBwKg = devineIbw + 0.4 * (weightKg - devineIbw);
          const adjBwLb = kgToLb(adjBwKg);
          adjBwKgEl.textContent = `${round(adjBwKg, 1)} kg`;
          adjBwLbEl.textContent = `(${round(adjBwLb, 1)} lb)`;
          adjNoteEl.textContent =
            "Using Devine IBW and a 40% correction factor because your actual weight is more than 120% of ideal.";
        } else {
          adjBwKgEl.textContent = "Not applicable";
          adjBwLbEl.textContent = "";
          adjNoteEl.textContent =
            "Adjusted body weight is typically used only when actual weight is significantly above ideal (≥120% of IBW).";
        }

        // Range bar markers
        const markerCurrent = document.getElementById("marker-current");
        const markerIbw = document.getElementById("marker-ibw");

        // Map weight to 0–100% scale based on BMI 15–40 for visualization
        const bmiMinVis = 15;
        const bmiMaxVis = 40;
        const bmiClamped = Math.min(Math.max(bmi, bmiMinVis), bmiMaxVis);
        const posCurrent =
          ((bmiClamped - bmiMinVis) / (bmiMaxVis - bmiMinVis)) * 100;

        let posIbw = 50;
        if (isFinite(avgIbwKg)) {
          const bmiIbw = avgIbwKg / (heightM * heightM);
          const bmiIbwClamped = Math.min(
            Math.max(bmiIbw, bmiMinVis),
            bmiMaxVis
          );
          posIbw =
            ((bmiIbwClamped - bmiMinVis) / (bmiMaxVis - bmiMinVis)) * 100;
        }

        markerCurrent.style.left = `${posCurrent}%`;
        markerIbw.style.left = `${posIbw}%`;

        document.getElementById("summary-section").classList.remove("hidden");
        document.getElementById("results-section").classList.remove("hidden");
      }

      function resetCalculator() {
        document.getElementById("sex").value = "male";
        document.getElementById("age").value = "";
        document.getElementById("height-cm").value = "";
        document.getElementById("height-ft").value = "";
        document.getElementById("height-in").value = "";
        document.getElementById("weight").value = "";
        document.getElementById("error-msg").textContent = "";

        document
          .querySelectorAll(".formula-checkbox")
          .forEach((cb) => (cb.checked = true));

        document.getElementById("summary-section").classList.add("hidden");
        document.getElementById("results-section").classList.add("hidden");
      }

      document.addEventListener("DOMContentLoaded", () => {
        updateUnitToggles();
        document
          .getElementById("calculate-btn")
          .addEventListener("click", calculate);
        document
          .getElementById("reset-btn")
          .addEventListener("click", resetCalculator);
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
