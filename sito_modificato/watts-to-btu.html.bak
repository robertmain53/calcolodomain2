<script>
  // Core conversion factor
  const FACTOR_W_TO_BTU = 3.412142;

  const modeWToBTU = document.getElementById('mode-w-to-btu');
  const modeBTUToW = document.getElementById('mode-btu-to-w');
  const inputValueEl = document.getElementById('input-value');
  const outputValueEl = document.getElementById('output-value');
  const inputLabelEl = document.getElementById('input-label');
  const outputLabelEl = document.getElementById('output-label');
  const inputUnitEl = document.getElementById('input-unit');
  const outputUnitEl = document.getElementById('output-unit');
  const outputNoteEl = document.getElementById('output-note');

  const btnExampleSpaceHeater = document.getElementById('btn-example-space-heater');
  const btnExampleWindowAC = document.getElementById('btn-example-window-ac');
  const btnClear = document.getElementById('btn-clear');

  let currentMode = 'W_TO_BTU'; // or 'BTU_TO_W'

  function formatNumber(value, decimals) {
    if (!isFinite(value)) return '';
    return Number(value).toLocaleString(undefined, {
      maximumFractionDigits: decimals,
      minimumFractionDigits: 0
    });
  }

  function convert() {
    const raw = inputValueEl.value.trim();
    if (raw === '') {
      outputValueEl.value = '';
      return;
    }

    const val = Number(raw);
    if (!isFinite(val) || val < 0) {
      outputValueEl.value = '';
      return;
    }

    let result;
    if (currentMode === 'W_TO_BTU') {
      result = val * FACTOR_W_TO_BTU;
      outputValueEl.value = formatNumber(result, 2);
    } else {
      result = val / FACTOR_W_TO_BTU;
      outputValueEl.value = formatNumber(result, 2);
    }
  }

  function setMode(mode) {
    currentMode = mode;

    if (mode === 'W_TO_BTU') {
      modeWToBTU.classList.add('toggle-active');
      modeWToBTU.classList.remove('toggle-inactive');
      modeWToBTU.setAttribute('aria-pressed', 'true');

      modeBTUToW.classList.add('toggle-inactive');
      modeBTUToW.classList.remove('toggle-active');
      modeBTUToW.setAttribute('aria-pressed', 'false');

      inputLabelEl.textContent = 'Power in watts (W)';
      outputLabelEl.textContent = 'Heat in BTU per hour (BTU/hr)';
      inputUnitEl.textContent = 'W';
      outputUnitEl.textContent = 'BTU/hr';
      outputNoteEl.textContent = 'Using 1 W = 3.412142 BTU/hr.';
    } else {
      modeBTUToW.classList.add('toggle-active');
      modeBTUToW.classList.remove('toggle-inactive');
      modeBTUToW.setAttribute('aria-pressed', 'true');

      modeWToBTU.classList.add('toggle-inactive');
      modeWToBTU.classList.remove('toggle-active');
      modeWToBTU.setAttribute('aria-pressed', 'false');

      inputLabelEl.textContent = 'Heat in BTU per hour (BTU/hr)';
      outputLabelEl.textContent = 'Power in watts (W)';
      inputUnitEl.textContent = 'BTU/hr';
      outputUnitEl.textContent = 'W';
      outputNoteEl.textContent = 'Using 1 W = 3.412142 BTU/hr (W = BTU/hr ÷ 3.412142).';
    }

    convert();
  }

  // Event listeners
  inputValueEl.addEventListener('input', convert);

  modeWToBTU.addEventListener('click', () => setMode('W_TO_BTU'));
  modeBTUToW.addEventListener('click', () => setMode('BTU_TO_W'));

  btnExampleSpaceHeater.addEventListener('click', () => {
    setMode('W_TO_BTU');
    inputValueEl.value = '1500';
    convert();
  });

  btnExampleWindowAC.addEventListener('click', () => {
    setMode('BTU_TO_W');
    inputValueEl.value = '12000';
    convert();
  });

  btnClear.addEventListener('click', () => {
    inputValueEl.value = '';
    outputValueEl.value = '';
    inputValueEl.focus();
  });

  // Build conversion table
  (function buildTable() {
    const tbody = document.getElementById('conversion-table-body');
    if (!tbody) return;

    const steps = [
      100, 200, 300, 400, 500,
      750, 1000, 1250, 1500, 1750,
      2000, 2500, 3000, 3500, 4000,
      4500, 5000
    ];

    steps.forEach(w => {
      const btu = Math.round(w * FACTOR_W_TO_BTU);
      const tr = document.createElement('tr');

      const tdW = document.createElement('td');
      tdW.className = 'px-3 py-1.5 whitespace-nowrap';
      tdW.textContent = formatNumber(w, 0);

      const tdBTU = document.createElement('td');
      tdBTU.className = 'px-3 py-1.5 whitespace-nowrap';
      tdBTU.textContent = formatNumber(btu, 0);

      tr.appendChild(tdW);
      tr.appendChild(tdBTU);
      tbody.appendChild(tr);
    });
  })();
</script>

