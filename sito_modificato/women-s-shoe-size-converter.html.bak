    <script>
      // Reference table: typical women's sizes mapped to foot length (cm)
      // Based on common industry charts; values are approximate.
      const sizeRows = [
        { US: 4, UK: 2, EU: 35, AU: 4, JP: 21, cm: 21.0 },
        { US: 4.5, UK: 2.5, EU: 35, AU: 4.5, JP: 21.5, cm: 21.5 },
        { US: 5, UK: 3, EU: 35.5, AU: 5, JP: 22, cm: 22.0 },
        { US: 5.5, UK: 3.5, EU: 36, AU: 5.5, JP: 22.5, cm: 22.5 },
        { US: 6, UK: 4, EU: 36.5, AU: 6, JP: 23, cm: 23.0 },
        { US: 6.5, UK: 4.5, EU: 37, AU: 6.5, JP: 23.5, cm: 23.5 },
        { US: 7, UK: 5, EU: 37.5, AU: 7, JP: 24, cm: 24.0 },
        { US: 7.5, UK: 5.5, EU: 38, AU: 7.5, JP: 24.5, cm: 24.5 },
        { US: 8, UK: 6, EU: 38.5, AU: 8, JP: 25, cm: 25.0 },
        { US: 8.5, UK: 6.5, EU: 39, AU: 8.5, JP: 25.5, cm: 25.5 },
        { US: 9, UK: 7, EU: 40, AU: 9, JP: 26, cm: 26.0 },
        { US: 9.5, UK: 7.5, EU: 40.5, AU: 9.5, JP: 26.5, cm: 26.5 },
        { US: 10, UK: 8, EU: 41, AU: 10, JP: 27, cm: 27.0 },
        { US: 10.5, UK: 8.5, EU: 41.5, AU: 10.5, JP: 27.5, cm: 27.5 },
        { US: 11, UK: 9, EU: 42, AU: 11, JP: 28, cm: 28.0 },
      ];

      function populateTable() {
        const tbody = document.getElementById("size-table-body");
        tbody.innerHTML = "";
        sizeRows.forEach((row) => {
          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="px-3 py-1.5 border-b">${row.US}</td>
        <td class="px-3 py-1.5 border-b">${row.UK}</td>
        <td class="px-3 py-1.5 border-b">${row.EU}</td>
        <td class="px-3 py-1.5 border-b">${row.AU}</td>
        <td class="px-3 py-1.5 border-b">${row.JP}</td>
        <td class="px-3 py-1.5 border-b">${row.cm.toFixed(1)}</td>
      `;
          tbody.appendChild(tr);
        });
      }

      function getHint(system, isFrom) {
        if (system === "US")
          return "Enter your women’s US/CA size (e.g. 8, 7.5).";
        if (system === "UK") return "Enter your women’s UK size (e.g. 5, 6.5).";
        if (system === "EU") return "Enter your EU size (e.g. 38, 39).";
        if (system === "AU")
          return "Enter your women’s AU/NZ size (e.g. 6, 7.5).";
        if (system === "JP") return "Enter Japanese size in cm (e.g. 24.5).";
        if (system === "CM") return "Enter your foot length in centimeters.";
        if (system === "IN") return "Enter your foot length in inches.";
        return isFrom ? "Enter your size." : "Result updates automatically.";
      }

      function findRowBySystemValue(system, value) {
        const key = system;
        let bestRow = null;
        let bestDiff = Infinity;
        sizeRows.forEach((row) => {
          const rowVal =
            system === "CM"
              ? row.cm
              : system === "IN"
              ? row.cm / 2.54
              : row[key];
          if (rowVal == null) return;
          const diff = Math.abs(rowVal - value);
          if (diff < bestDiff) {
            bestDiff = diff;
            bestRow = row;
          }
        });
        return bestRow;
      }

      function convertSize() {
        const fromSystem = document.getElementById("from-system").value;
        const toSystem = document.getElementById("to-system").value;
        const fromInput = document.getElementById("from-size");
        const toOutput = document.getElementById("to-size");
        const warning = document.getElementById("converter-warning");

        const raw = parseFloat(fromInput.value);
        if (isNaN(raw)) {
          toOutput.value = "";
          warning.classList.add("hidden");
          return;
        }

        let sourceValue = raw;
        if (fromSystem === "IN") {
          sourceValue = raw; // inches, handled in findRow
        }

        const row = findRowBySystemValue(fromSystem, sourceValue);
        if (!row) {
          toOutput.value = "";
          warning.classList.remove("hidden");
          return;
        }

        let result;
        if (toSystem === "CM") {
          result = row.cm.toFixed(1) + " cm";
        } else if (toSystem === "IN") {
          result = (row.cm / 2.54).toFixed(2) + " in";
        } else if (toSystem === "JP") {
          result = row.JP.toFixed(1);
        } else {
          result = row[toSystem];
        }

        toOutput.value = result;
        warning.classList.remove("hidden");
      }

      document.addEventListener("DOMContentLoaded", () => {
        populateTable();

        const fromSystemEl = document.getElementById("from-system");
        const toSystemEl = document.getElementById("to-system");
        const fromSizeEl = document.getElementById("from-size");
        const fromHintEl = document.getElementById("from-hint");
        const toHintEl = document.getElementById("to-hint");
        const swapBtn = document.getElementById("swap-systems");
        const resetBtn = document.getElementById("reset-form");

        function updateHints() {
          fromHintEl.textContent = getHint(fromSystemEl.value, true);
          toHintEl.textContent = getHint(toSystemEl.value, false);
        }

        fromSystemEl.addEventListener("change", () => {
          updateHints();
          convertSize();
        });

        toSystemEl.addEventListener("change", () => {
          updateHints();
          convertSize();
        });

        fromSizeEl.addEventListener("input", convertSize);

        swapBtn.addEventListener("click", () => {
          const oldFrom = fromSystemEl.value;
          fromSystemEl.value = toSystemEl.value;
          toSystemEl.value = oldFrom;
          updateHints();
          convertSize();
        });

        resetBtn.addEventListener("click", () => {
          fromSystemEl.value = "US";
          toSystemEl.value = "EU";
          fromSizeEl.value = "";
          document.getElementById("to-size").value = "";
          document.getElementById("converter-warning").classList.add("hidden");
          updateHints();
        });

        updateHints();
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
