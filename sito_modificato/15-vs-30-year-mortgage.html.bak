    <script>
      // Core mortgage and investment calculations
      function calcMonthlyPayment(P, annualRate, years) {
        const n = years * 12;
        const r = annualRate / 100 / 12;
        if (r === 0) return P / n;
        const factor = Math.pow(1 + r, n);
        return (P * (r * factor)) / (factor - 1);
      }

      function futureValueMonthly(monthly, annualRate, years) {
        const n = years * 12;
        const r = annualRate / 100 / 12;
        if (r === 0) return monthly * n;
        const factor = Math.pow(1 + r, n);
        return (monthly * (factor - 1)) / r;
      }

      function fmtCurrency(x) {
        if (!isFinite(x)) return "$0";
        return x.toLocaleString(undefined, {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 0,
        });
      }

      function fmtCurrency2(x) {
        if (!isFinite(x)) return "$0";
        return x.toLocaleString(undefined, {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 2,
        });
      }

      function updateBars(val1, val2, bar1, bar2) {
        const max = Math.max(val1, val2, 1);
        bar1.style.width = ((val1 / max) * 100).toFixed(1) + "%";
        bar2.style.width = ((val2 / max) * 100).toFixed(1) + "%";
      }

      function calculate() {
        const loan =
          parseFloat(document.getElementById("loanAmount").value) || 0;
        const rate15 = parseFloat(document.getElementById("rate15").value) || 0;
        const rate30 = parseFloat(document.getElementById("rate30").value) || 0;
        const term15 =
          parseFloat(document.getElementById("term15").value) || 15;
        const term30 =
          parseFloat(document.getElementById("term30").value) || 30;

        const tax =
          parseFloat(document.getElementById("propertyTax").value) || 0;
        const ins = parseFloat(document.getElementById("insurance").value) || 0;
        const hoa = parseFloat(document.getElementById("hoa").value) || 0;
        const escrow = tax + ins + hoa;

        if (
          loan <= 0 ||
          rate15 < 0 ||
          rate30 < 0 ||
          term15 <= 0 ||
          term30 <= 0
        ) {
          return;
        }

        const pi15 = calcMonthlyPayment(loan, rate15, term15);
        const pi30 = calcMonthlyPayment(loan, rate30, term30);

        const n15 = term15 * 12;
        const n30 = term30 * 12;

        const total15 = pi15 * n15;
        const total30 = pi30 * n30;

        const interest15 = total15 - loan;
        const interest30 = total30 - loan;

        // Update labels
        document.getElementById("pi15Label").textContent = fmtCurrency2(pi15);
        document.getElementById("pi30Label").textContent = fmtCurrency2(pi30);
        document.getElementById("interest15Label").textContent =
          fmtCurrency(interest15);
        document.getElementById("interest30Label").textContent =
          fmtCurrency(interest30);

        // Bars
        updateBars(
          pi15,
          pi30,
          document.getElementById("pi15Bar"),
          document.getElementById("pi30Bar")
        );
        updateBars(
          interest15,
          interest30,
          document.getElementById("interest15Bar"),
          document.getElementById("interest30Bar")
        );

        // Total monthly housing cost
        const totalMonthly15 = pi15 + escrow;
        const totalMonthly30 = pi30 + escrow;

        document.getElementById("total15Label").textContent =
          fmtCurrency2(totalMonthly15);
        document.getElementById("total30Label").textContent =
          fmtCurrency2(totalMonthly30);
        document.getElementById("pi15Break").textContent = fmtCurrency2(pi15);
        document.getElementById("pi30Break").textContent = fmtCurrency2(pi30);
        document.getElementById("escrow15Break").textContent =
          fmtCurrency2(escrow);
        document.getElementById("escrow30Break").textContent =
          fmtCurrency2(escrow);

        // Summary badges
        const summary = document.getElementById("summaryBadges");
        const interestSaved = interest30 - interest15;
        const monthlyDiff = pi15 - pi30;

        let cheaperLabel = "15-year loan saves more interest";
        let cheaperDetail =
          "You pay " +
          fmtCurrency(interestSaved) +
          " less interest with the 15-year mortgage.";
        if (interestSaved <= 0) {
          cheaperLabel = "30-year loan has similar or lower interest";
          cheaperDetail =
            "With the current rates and terms, the 30-year mortgage does not cost more interest than the 15-year.";
        }

        const cashFlowLabel =
          monthlyDiff > 0
            ? "30-year loan improves monthly cash flow"
            : "15-year loan has similar or lower payment";
        const cashFlowDetail =
          monthlyDiff > 0
            ? "Your required monthly payment is " +
              fmtCurrency2(monthlyDiff) +
              " lower with the 30-year mortgage."
            : "The monthly payments are similar based on the inputs.";

        summary.innerHTML = `
      <div class="bg-green-50 border border-green-200 rounded-md p-3 text-sm">
        <p class="font-semibold text-green-800 mb-1">${cheaperLabel}</p>
        <p class="text-green-700">${cheaperDetail}</p>
      </div>
      <div class="bg-blue-50 border border-blue-200 rounded-md p-3 text-sm">
        <p class="font-semibold text-blue-800 mb-1">${cashFlowLabel}</p>
        <p class="text-blue-700">${cashFlowDetail}</p>
      </div>
    `;

        // Investment comparison
        const investRate =
          parseFloat(document.getElementById("investReturn").value) || 0;
        const investYears =
          parseFloat(document.getElementById("investYears").value) || 0;
        const investSection = document.getElementById("investSection");

        if (investRate > 0 && investYears > 0 && monthlyDiff > 0) {
          const monthlyToInvest = monthlyDiff; // 15-year payment minus 30-year payment
          const fv = futureValueMonthly(
            monthlyToInvest,
            investRate,
            investYears
          );

          document.getElementById("investRateLabel").textContent =
            investRate.toFixed(1) + "%";
          document.getElementById("investYearsLabel").textContent =
            investYears.toFixed(0);
          document.getElementById("monthlyDiffLabel").textContent =
            fmtCurrency2(monthlyToInvest);
          document.getElementById("investFutureLabel").textContent =
            fmtCurrency(fv);
          document.getElementById("extraInterestLabel").textContent =
            fmtCurrency(interestSaved > 0 ? interestSaved : 0);

          let conclusion = "";
          if (fv > interestSaved) {
            conclusion =
              "If you consistently invest the payment difference and actually earn about " +
              investRate.toFixed(1) +
              "% per year, the projected investment value could exceed the extra interest cost of the 30-year mortgage.";
          } else {
            conclusion =
              "Even if you invest the payment difference at about " +
              investRate.toFixed(1) +
              "% per year, the interest savings from the 15-year mortgage are larger than the projected investment value.";
          }
          document.getElementById("investConclusion").textContent = conclusion;
          investSection.classList.remove("hidden");
        } else {
          investSection.classList.add("hidden");
        }
      }

      document
        .getElementById("calcBtn")
        .addEventListener("click", function (e) {
          e.preventDefault();
          calculate();
        });

      // Auto-calc on load
      document.addEventListener("DOMContentLoaded", calculate);

      // Simple FAQ toggle (in addition to global script_faq if any)
      document
        .querySelectorAll("#faq-container .faq-question")
        .forEach((btn) => {
          btn.addEventListener("click", () => {
            const answer = btn.parentElement.querySelector(".faq-answer");
            const icon = btn.querySelector("span:last-child");
            const isHidden = answer.classList.contains("hidden");
            document
              .querySelectorAll("#faq-container .faq-answer")
              .forEach((a) => a.classList.add("hidden"));
            document
              .querySelectorAll("#faq-container .faq-question span:last-child")
              .forEach((i) => (i.textContent = "+"));
            if (isHidden) {
              answer.classList.remove("hidden");
              icon.textContent = "−";
            }
          });
        });
    </script>

