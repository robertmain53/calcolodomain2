    <script>
      // Core conversion factors
      const FACTOR_US = 3.785411784; // 1 US GPM to LPM
      const FACTOR_UK = 4.54609; // 1 UK GPM to LPM

      const gpmInput = document.getElementById("gpmInput");
      const lpmInput = document.getElementById("lpmInput");
      const roundingSelect = document.getElementById("roundingSelect");
      const gallonTypeSelect = document.getElementById("gallonTypeSelect");
      const liveExplanation = document.getElementById("liveExplanation");
      const quickTableBody = document.getElementById("quickTableBody");

      let activeField = null; // 'gpm' or 'lpm'

      function getFactor() {
        return gallonTypeSelect.value === "uk" ? FACTOR_UK : FACTOR_US;
      }

      function formatNumber(value) {
        const decimals = parseInt(roundingSelect.value, 10);
        if (!isFinite(value)) return "";
        return value.toFixed(decimals);
      }

      function updateFromGPM() {
        const gpm = parseFloat(gpmInput.value);
        if (isNaN(gpm)) {
          lpmInput.value = "";
          liveExplanation.textContent =
            "Enter a GPM value to convert it to LPM.";
          return;
        }
        const factor = getFactor();
        const lpm = gpm * factor;
        lpmInput.value = formatNumber(lpm);
        const typeLabel = gallonTypeSelect.value === "uk" ? "UK" : "US";
        liveExplanation.textContent =
          gpm +
          " GPM (" +
          typeLabel +
          ") × " +
          factor +
          " = " +
          lpm.toFixed(6) +
          " LPM → " +
          formatNumber(lpm) +
          " LPM (rounded).";
      }

      function updateFromLPM() {
        const lpm = parseFloat(lpmInput.value);
        if (isNaN(lpm)) {
          gpmInput.value = "";
          liveExplanation.textContent =
            "Enter an LPM value to convert it to GPM.";
          return;
        }
        const factor = getFactor();
        const gpm = lpm / factor;
        gpmInput.value = formatNumber(gpm);
        const typeLabel = gallonTypeSelect.value === "uk" ? "UK" : "US";
        liveExplanation.textContent =
          lpm +
          " LPM ÷ " +
          factor +
          " = " +
          gpm.toFixed(6) +
          " GPM (" +
          typeLabel +
          ") → " +
          formatNumber(gpm) +
          " GPM (rounded).";
      }

      gpmInput.addEventListener("input", () => {
        activeField = "gpm";
        updateFromGPM();
      });

      lpmInput.addEventListener("input", () => {
        activeField = "lpm";
        updateFromLPM();
      });

      roundingSelect.addEventListener("change", () => {
        if (activeField === "gpm") {
          updateFromGPM();
        } else if (activeField === "lpm") {
          updateFromLPM();
        }
      });

      gallonTypeSelect.addEventListener("change", () => {
        if (activeField === "gpm") {
          updateFromGPM();
        } else if (activeField === "lpm") {
          updateFromLPM();
        }
      });

      document.getElementById("clearBtn").addEventListener("click", () => {
        gpmInput.value = "";
        lpmInput.value = "";
        activeField = null;
        liveExplanation.textContent =
          "Enter a value in either box to see the step-by-step conversion.";
        gpmInput.focus();
      });

      document
        .getElementById("swapDirectionBtn")
        .addEventListener("click", () => {
          if (activeField === "gpm") {
            activeField = "lpm";
            updateFromLPM();
            lpmInput.focus();
            lpmInput.select();
          } else {
            activeField = "gpm";
            updateFromGPM();
            gpmInput.focus();
            gpmInput.select();
          }
        });

      // Preset buttons
      document.querySelectorAll(".preset-btn").forEach((btn) => {
        btn.addEventListener("click", () => {
          const gpm = parseFloat(btn.getAttribute("data-preset-gpm"));
          gpmInput.value = gpm;
          activeField = "gpm";
          updateFromGPM();
          gpmInput.focus();
          gpmInput.select();
        });
      });

      // Build quick reference table
      function buildQuickTable() {
        const rowsGpm = [1, 2, 5, 10, 20, 50, 100];
        quickTableBody.innerHTML = "";
        rowsGpm.forEach((gpm) => {
          const lpmUs = gpm * FACTOR_US;
          const gpmUk = lpmUs / FACTOR_UK; // convert via liters
          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="px-3 py-2 text-gray-800">${gpm}</td>
        <td class="px-3 py-2 text-gray-800">${lpmUs.toFixed(2)}</td>
        <td class="px-3 py-2 text-gray-800">${gpmUk.toFixed(2)}</td>
      `;
          quickTableBody.appendChild(tr);
        });
      }

      buildQuickTable();
    </script>

