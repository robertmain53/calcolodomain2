            <script>
              document.addEventListener('DOMContentLoaded', () => {
                const kInput = document.getElementById('kelvin-input');
                const cInput = document.getElementById('celsius-input');
                const fInput = document.getElementById('fahrenheit-input');
                const stepsBox = document.getElementById('calculation-steps');
                
                const inputs = [kInput, cInput, fInput];

                // Funzioni di conversione
                const C_TO_F = (c) => (c * 9/5) + 32;
                const F_TO_C = (f) => (f - 32) * 5/9;
                const C_TO_K = (c) => c + 273.15;
                const K_TO_C = (k) => k - 273.15;
                
                // Formattatore
                function formatNum(num) {
                    if (isNaN(num)) return '';
                    return parseFloat(num.toFixed(2)).toString();
                }

                function update(sourceUnit, value) {
                    if (isNaN(value)) {
                        inputs.forEach(input => {
                            if (input.dataset.unit !== sourceUnit) input.value = '';
                        });
                        stepsBox.innerHTML = '<h3 class="text-lg font-semibold text-gray-800 mb-2">Calculation Steps:</h3><p class="text-gray-600">Enter a value to see the step-by-step conversion.</p>';
                        return;
                    }

                    let k, c, f;
                    let stepsHtml = '<h3 class="text-lg font-semibold text-gray-800 mb-2">Calculation Steps:</h3>';

                    if (sourceUnit === 'k') {
                        k = value;
                        c = K_TO_C(k);
                        f = C_TO_F(c);
                        cInput.value = formatNum(c);
                        fInput.value = formatNum(f);
                        
                        stepsHtml += `
                          <p><strong>1. Kelvin to Celsius:</strong></p>
                          <p class="formula-box">°C = ${formatNum(k)} K - 273.15 = <strong>${formatNum(c)} °C</strong></p>
                          <p><strong>2. Celsius to Fahrenheit:</strong></p>
                          <p class="formula-box">°F = (${formatNum(c)} °C × 9/5) + 32 = <strong>${formatNum(f)} °F</strong></p>
                        `;
                    } 
                    else if (sourceUnit === 'c') {
                        c = value;
                        k = C_TO_K(c);
                        f = C_TO_F(c);
                        kInput.value = formatNum(k);
                        fInput.value = formatNum(f);

                        stepsHtml += `
                          <p><strong>1. Celsius to Kelvin:</strong></p>
                          <p class="formula-box">K = ${formatNum(c)} °C + 273.15 = <strong>${formatNum(k)} K</strong></p>
                          <p><strong>2. Celsius to Fahrenheit:</strong></p>
                          <p class="formula-box">°F = (${formatNum(c)} °C × 9/5) + 32 = <strong>${formatNum(f)} °F</strong></p>
                        `;
                    } 
                    else if (sourceUnit === 'f') {
                        f = value;
                        c = F_TO_C(f);
                        k = C_TO_K(c);
                        cInput.value = formatNum(c);
                        kInput.value = formatNum(k);

                        stepsHtml += `
                          <p><strong>1. Fahrenheit to Celsius:</strong></p>
                          <p class="formula-box">°C = (${formatNum(f)} °F - 32) × 5/9 = <strong>${formatNum(c)} °C</strong></p>
                          <p><strong>2. Celsius to Kelvin:</strong></p>
                          <p class="formula-box">K = ${formatNum(c)} °C + 273.15 = <strong>${formatNum(k)} K</strong></p>
                        `;
                    }
                    
                    stepsBox.innerHTML = stepsHtml;
                }

                inputs.forEach(input => {
                  input.addEventListener('input', (e) => {
                    const sourceUnit = e.target.dataset.unit;
                    const value = parseFloat(e.target.value);
                    update(sourceUnit, value);
                  });
                });
              });
            </script>
            
