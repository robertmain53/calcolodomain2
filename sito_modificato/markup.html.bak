    <script>
      // Tab handling
      const tabSimple = document.getElementById("tab-simple");
      const tabBulk = document.getElementById("tab-bulk");
      const panelSimple = document.getElementById("panel-simple");
      const panelBulk = document.getElementById("panel-bulk");

      function activateTab(tab) {
        if (tab === "simple") {
          tabSimple.className = "py-2 tab-active";
          tabBulk.className = "py-2 tab-inactive";
          panelSimple.classList.remove("hidden");
          panelBulk.classList.add("hidden");
        } else {
          tabSimple.className = "py-2 tab-inactive";
          tabBulk.className = "py-2 tab-active";
          panelSimple.classList.add("hidden");
          panelBulk.classList.remove("hidden");
        }
      }

      tabSimple.addEventListener("click", () => activateTab("simple"));
      tabBulk.addEventListener("click", () => activateTab("bulk"));

      // Simple calculator logic
      const costInput = document.getElementById("cost");
      const priceInput = document.getElementById("price");
      const markupInput = document.getElementById("markupPercent");
      const marginInput = document.getElementById("marginPercent");
      const btnCalc = document.getElementById("btn-calc");
      const btnClear = document.getElementById("btn-clear");
      const errorEl = document.getElementById("calc-error");
      const resultsCard = document.getElementById("results-card");
      const resCost = document.getElementById("res-cost");
      const resPrice = document.getElementById("res-price");
      const resProfit = document.getElementById("res-profit");
      const resMarkup = document.getElementById("res-markup");
      const resMargin = document.getElementById("res-margin");
      const resBreakeven = document.getElementById("res-breakeven");

      function parseVal(el) {
        const v = parseFloat(el.value);
        return isNaN(v) ? null : v;
      }

      function formatMoney(v) {
        return "$" + v.toFixed(2);
      }

      function formatPercent(v) {
        return v.toFixed(2) + "%";
      }

      btnCalc.addEventListener("click", () => {
        errorEl.classList.add("hidden");
        errorEl.textContent = "";

        let cost = parseVal(costInput);
        let price = parseVal(priceInput);
        let markup = parseVal(markupInput);
        let margin = parseVal(marginInput);

        // Count how many core values are provided (cost, price, markup, margin)
        let provided = 0;
        if (cost !== null) provided++;
        if (price !== null) provided++;
        if (markup !== null) provided++;
        if (margin !== null) provided++;

        if (provided < 2) {
          errorEl.textContent =
            "Please enter at least two of: cost, price, markup %, or margin %.";
          errorEl.classList.remove("hidden");
          resultsCard.classList.add("hidden");
          return;
        }

        // Solve based on combinations
        // 1) cost & price -> derive markup & margin
        if (cost !== null && price !== null) {
          if (cost < 0 || price < 0) {
            errorEl.textContent = "Cost and price must be non-negative.";
            errorEl.classList.remove("hidden");
            resultsCard.classList.add("hidden");
            return;
          }
          const profit = price - cost;
          markup = cost === 0 ? 0 : (profit / cost) * 100;
          margin = price === 0 ? 0 : (profit / price) * 100;
        }
        // 2) cost & markup -> derive price, margin
        else if (cost !== null && markup !== null) {
          if (cost < 0) {
            errorEl.textContent = "Cost must be non-negative.";
            errorEl.classList.remove("hidden");
            resultsCard.classList.add("hidden");
            return;
          }
          price = cost * (1 + markup / 100);
          const profit = price - cost;
          margin = price === 0 ? 0 : (profit / price) * 100;
        }
        // 3) cost & margin -> derive price, markup
        else if (cost !== null && margin !== null) {
          if (cost < 0 || margin >= 100) {
            errorEl.textContent =
              "Cost must be non-negative and margin must be less than 100%.";
            errorEl.classList.remove("hidden");
            resultsCard.classList.add("hidden");
            return;
          }
          price = cost / (1 - margin / 100);
          const profit = price - cost;
          markup = cost === 0 ? 0 : (profit / cost) * 100;
        }
        // 4) price & markup -> derive cost, margin
        else if (price !== null && markup !== null) {
          if (price < 0 || markup <= -100) {
            errorEl.textContent =
              "Price must be non-negative and markup must be greater than -100%.";
            errorEl.classList.remove("hidden");
            resultsCard.classList.add("hidden");
            return;
          }
          cost = price / (1 + markup / 100);
          const profit = price - cost;
          margin = price === 0 ? 0 : (profit / price) * 100;
        }
        // 5) price & margin -> derive cost, markup
        else if (price !== null && margin !== null) {
          if (price < 0 || margin >= 100) {
            errorEl.textContent =
              "Price must be non-negative and margin must be less than 100%.";
            errorEl.classList.remove("hidden");
            resultsCard.classList.add("hidden");
            return;
          }
          cost = price * (1 - margin / 100);
          const profit = price - cost;
          markup = cost === 0 ? 0 : (profit / cost) * 100;
        } else {
          errorEl.textContent =
            "This combination is not supported. Please provide at least one of cost or price.";
          errorEl.classList.remove("hidden");
          resultsCard.classList.add("hidden");
          return;
        }

        const profit = price - cost;

        // Update inputs with solved values
        costInput.value = cost.toFixed(2);
        priceInput.value = price.toFixed(2);
        markupInput.value = markup.toFixed(2);
        marginInput.value = margin.toFixed(2);

        // Update results
        resCost.textContent = formatMoney(cost);
        resPrice.textContent = formatMoney(price);
        resProfit.textContent = formatMoney(profit);
        resMarkup.textContent = formatPercent(markup);
        resMargin.textContent = formatPercent(margin);
        resBreakeven.textContent = formatMoney(cost);

        resultsCard.classList.remove("hidden");
      });

      btnClear.addEventListener("click", () => {
        [costInput, priceInput, markupInput, marginInput].forEach(
          (el) => (el.value = "")
        );
        errorEl.classList.add("hidden");
        resultsCard.classList.add("hidden");
      });

      // Bulk / multi-item logic
      const bulkBody = document.getElementById("bulk-body");
      const bulkAddRowBtn = document.getElementById("bulk-add-row");
      const bulkClearBtn = document.getElementById("bulk-clear");
      const bulkError = document.getElementById("bulk-error");
      const bulkTotalPrice = document.getElementById("bulk-total-price");
      const bulkTotalProfit = document.getElementById("bulk-total-profit");
      const bulkTotalRevenue = document.getElementById("bulk-total-revenue");

      let bulkRowId = 0;

      function createBulkRow() {
        bulkRowId++;
        const tr = document.createElement("tr");
        tr.dataset.rowId = bulkRowId;
        tr.className = "border-t";

        tr.innerHTML = `
      <td class="px-3 py-1.5">
        <input type="text" class="w-full border border-gray-200 rounded px-2 py-1 text-sm" placeholder="Item ${bulkRowId}">
      </td>
      <td class="px-3 py-1.5 text-right">
        <input type="number" class="w-24 border border-gray-200 rounded px-2 py-1 text-right text-sm bulk-cost" min="0" step="0.01" placeholder="0.00">
      </td>
      <td class="px-3 py-1.5 text-right">
        <input type="number" class="w-20 border border-gray-200 rounded px-2 py-1 text-right text-sm bulk-markup" step="0.01" placeholder="40">
      </td>
      <td class="px-3 py-1.5 text-right">
        <input type="number" class="w-16 border border-gray-200 rounded px-2 py-1 text-right text-sm bulk-qty" min="0" step="1" value="1">
      </td>
      <td class="px-3 py-1.5 text-right text-gray-900 bulk-price">–</td>
      <td class="px-3 py-1.5 text-right text-gray-900 bulk-profit">–</td>
      <td class="px-3 py-1.5 text-right text-gray-900 bulk-revenue">–</td>
      <td class="px-3 py-1.5 text-right">
        <button type="button" class="text-xs text-red-600 hover:underline bulk-remove">Remove</button>
      </td>
    `;
        bulkBody.appendChild(tr);
      }

      function recalcBulk() {
        bulkError.classList.add("hidden");
        let totalPrice = 0;
        let totalProfit = 0;
        let totalRevenue = 0;

        const rows = bulkBody.querySelectorAll("tr");
        rows.forEach((row) => {
          const costEl = row.querySelector(".bulk-cost");
          const markupEl = row.querySelector(".bulk-markup");
          const qtyEl = row.querySelector(".bulk-qty");
          const priceCell = row.querySelector(".bulk-price");
          const profitCell = row.querySelector(".bulk-profit");
          const revenueCell = row.querySelector(".bulk-revenue");

          const cost = parseFloat(costEl.value);
          const markup = parseFloat(markupEl.value);
          const qty = parseFloat(qtyEl.value);

          if (
            isNaN(cost) ||
            isNaN(markup) ||
            isNaN(qty) ||
            cost < 0 ||
            qty < 0 ||
            markup <= -100
          ) {
            priceCell.textContent = "–";
            profitCell.textContent = "–";
            revenueCell.textContent = "–";
            return;
          }

          const price = cost * (1 + markup / 100);
          const profitPerUnit = price - cost;
          const revenue = price * qty;
          const profitTotal = profitPerUnit * qty;

          priceCell.textContent = formatMoney(price);
          profitCell.textContent = formatMoney(profitTotal);
          revenueCell.textContent = formatMoney(revenue);

          totalPrice += price;
          totalProfit += profitTotal;
          totalRevenue += revenue;
        });

        if (rows.length === 0) {
          bulkTotalPrice.textContent = "–";
          bulkTotalProfit.textContent = "–";
          bulkTotalRevenue.textContent = "–";
        } else {
          bulkTotalPrice.textContent = formatMoney(totalPrice);
          bulkTotalProfit.textContent = formatMoney(totalProfit);
          bulkTotalRevenue.textContent = formatMoney(totalRevenue);
        }
      }

      bulkAddRowBtn.addEventListener("click", () => {
        createBulkRow();
      });

      bulkClearBtn.addEventListener("click", () => {
        bulkBody.innerHTML = "";
        bulkTotalPrice.textContent = "–";
        bulkTotalProfit.textContent = "–";
        bulkTotalRevenue.textContent = "–";
        bulkError.classList.add("hidden");
      });

      bulkBody.addEventListener("input", (e) => {
        if (
          e.target.classList.contains("bulk-cost") ||
          e.target.classList.contains("bulk-markup") ||
          e.target.classList.contains("bulk-qty")
        ) {
          recalcBulk();
        }
      });

      bulkBody.addEventListener("click", (e) => {
        if (e.target.classList.contains("bulk-remove")) {
          const row = e.target.closest("tr");
          if (row) {
            row.remove();
            recalcBulk();
          }
        }
      });

      // Initialize with one row
      createBulkRow();

      // Mobile menu script (from template)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const expanded = this.getAttribute("aria-expanded") === "true";
          this.setAttribute("aria-expanded", String(!expanded));
          menu.classList.toggle("hidden");
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
