    <script>
      (function () {
        const MI_TO_KM = 1.60934;

        const modeSelect = document.getElementById("mode-select");
        const decimalsSelect = document.getElementById("decimals-select");
        const inputModeRadios = document.querySelectorAll(
          'input[name="input-mode"]'
        );
        const singleWrapper = document.getElementById("single-input-wrapper");
        const listWrapper = document.getElementById("list-input-wrapper");

        const inputValueEl = document.getElementById("input-value");
        const outputValueEl = document.getElementById("output-value");
        const inputListEl = document.getElementById("input-list");
        const outputListEl = document.getElementById("output-list");

        const inputUnitLabel = document.getElementById("input-unit-label");
        const outputUnitLabel = document.getElementById("output-unit-label");
        const inputListUnitLabel = document.getElementById(
          "input-list-unit-label"
        );
        const outputListUnitLabel = document.getElementById(
          "output-list-unit-label"
        );

        const convertBtn = document.getElementById("convert-btn");
        const swapBtn = document.getElementById("swap-btn");
        const clearBtn = document.getElementById("clear-btn");
        const errorMsg = document.getElementById("error-msg");
        const quickSummary = document.getElementById("quick-summary");
        const tableBody = document.getElementById("reference-table-body");

        function formatNumber(value, decimals) {
          if (!isFinite(value)) return "";
          return Number(value).toFixed(decimals);
        }

        function getDecimals() {
          return parseInt(decimalsSelect.value, 10) || 2;
        }

        function updateUnitLabels() {
          const mode = modeSelect.value;
          if (mode === "miToKm") {
            inputUnitLabel.textContent = "(mi/L)";
            outputUnitLabel.textContent = "(km/L)";
            inputListUnitLabel.textContent = "(mi/L, one per line)";
            outputListUnitLabel.textContent = "(km/L)";
          } else {
            inputUnitLabel.textContent = "(km/L)";
            outputUnitLabel.textContent = "(mi/L)";
            inputListUnitLabel.textContent = "(km/L, one per line)";
            outputListUnitLabel.textContent = "(mi/L)";
          }
        }

        function convertSingle() {
          errorMsg.textContent = "";
          quickSummary.textContent = "";

          const raw = inputValueEl.value.trim();
          if (raw === "") {
            outputValueEl.value = "";
            return;
          }

          const value = Number(raw);
          if (!isFinite(value)) {
            errorMsg.textContent = "Please enter a valid numeric value.";
            return;
          }
          if (value < 0) {
            errorMsg.textContent = "Fuel economy cannot be negative.";
            return;
          }

          const decimals = getDecimals();
          const mode = modeSelect.value;
          let result, fromUnit, toUnit;

          if (mode === "miToKm") {
            result = value * MI_TO_KM;
            fromUnit = "mi/L";
            toUnit = "km/L";
          } else {
            result = value / MI_TO_KM;
            fromUnit = "km/L";
            toUnit = "mi/L";
          }

          outputValueEl.value = formatNumber(result, decimals);

          if (isFinite(result)) {
            quickSummary.textContent = `${formatNumber(
              value,
              decimals
            )} ${fromUnit} is equal to ${formatNumber(
              result,
              decimals
            )} ${toUnit}.`;
          }
        }

        function convertList() {
          errorMsg.textContent = "";
          quickSummary.textContent = "";

          const raw = inputListEl.value;
          if (!raw.trim()) {
            outputListEl.value = "";
            return;
          }

          const lines = raw.split(/\r?\n/);
          const decimals = getDecimals();
          const mode = modeSelect.value;
          const results = [];
          let fromUnit, toUnit;

          if (mode === "miToKm") {
            fromUnit = "mi/L";
            toUnit = "km/L";
          } else {
            fromUnit = "km/L";
            toUnit = "mi/L";
          }

          for (let line of lines) {
            const trimmed = line.trim();
            if (trimmed === "") {
              results.push("");
              continue;
            }
            const num = Number(trimmed);
            if (!isFinite(num) || num < 0) {
              results.push("Invalid");
              continue;
            }
            let converted;
            if (mode === "miToKm") {
              converted = num * MI_TO_KM;
            } else {
              converted = num / MI_TO_KM;
            }
            results.push(formatNumber(converted, decimals));
          }

          outputListEl.value = results.join("\n");
        }

        function handleConvert() {
          const inputMode = document.querySelector(
            'input[name="input-mode"]:checked'
          ).value;
          if (inputMode === "single") {
            convertSingle();
          } else {
            convertList();
          }
        }

        function handleSwap() {
          modeSelect.value =
            modeSelect.value === "miToKm" ? "kmToMi" : "miToKm";
          updateUnitLabels();
          handleConvert();
        }

        function handleClear() {
          inputValueEl.value = "";
          outputValueEl.value = "";
          inputListEl.value = "";
          outputListEl.value = "";
          errorMsg.textContent = "";
          quickSummary.textContent = "";
        }

        function handleInputModeChange() {
          const inputMode = document.querySelector(
            'input[name="input-mode"]:checked'
          ).value;
          if (inputMode === "single") {
            singleWrapper.classList.remove("hidden");
            listWrapper.classList.add("hidden");
          } else {
            singleWrapper.classList.add("hidden");
            listWrapper.classList.remove("hidden");
          }
          errorMsg.textContent = "";
          quickSummary.textContent = "";
        }

        function buildReferenceTable() {
          const sampleValues = [5, 8, 10, 12, 15, 20];
          const decimals = 2;
          tableBody.innerHTML = "";
          sampleValues.forEach((miPerL) => {
            const kmPerL = miPerL * MI_TO_KM;
            const tr = document.createElement("tr");
            tr.innerHTML = `
          <td class="px-3 py-2 border-b">${formatNumber(miPerL, decimals)}</td>
          <td class="px-3 py-2 border-b">${formatNumber(kmPerL, decimals)}</td>
        `;
            tableBody.appendChild(tr);
          });
        }

        // Event listeners
        convertBtn.addEventListener("click", handleConvert);
        swapBtn.addEventListener("click", handleSwap);
        clearBtn.addEventListener("click", handleClear);
        modeSelect.addEventListener("change", () => {
          updateUnitLabels();
          handleConvert();
        });
        decimalsSelect.addEventListener("change", handleConvert);
        inputValueEl.addEventListener("input", () => {
          const inputMode = document.querySelector(
            'input[name="input-mode"]:checked'
          ).value;
          if (inputMode === "single") convertSingle();
        });
        inputListEl.addEventListener("input", () => {
          const inputMode = document.querySelector(
            'input[name="input-mode"]:checked'
          ).value;
          if (inputMode === "list") convertList();
        });
        inputModeRadios.forEach((r) =>
          r.addEventListener("change", handleInputModeChange)
        );

        // Init
        updateUnitLabels();
        buildReferenceTable();
      })();
    </script>

