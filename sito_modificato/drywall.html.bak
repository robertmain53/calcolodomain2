    <script>
      // Utility functions
      function toFeet(value, unit) {
        if (!value || value <= 0) return 0;
        return unit === "m" ? value * 3.28084 : value;
      }
      function toSqFeet(value, unit) {
        if (!value || value <= 0) return 0;
        return unit === "m2" ? value * 10.7639 : value;
      }
      function round(value, decimals) {
        const factor = Math.pow(10, decimals);
        return Math.round(value * factor) / factor;
      }

      // Presets
      const presetButtons = document.querySelectorAll("button[data-preset]");
      presetButtons.forEach((btn) => {
        btn.addEventListener("click", () => {
          const preset = btn.getAttribute("data-preset");
          const lengthInput = document.getElementById("length");
          const widthInput = document.getElementById("width");
          const heightInput = document.getElementById("height");
          const lengthUnit = document.getElementById("lengthUnit");
          const widthUnit = document.getElementById("widthUnit");
          const heightUnit = document.getElementById("heightUnit");

          lengthUnit.value = "ft";
          widthUnit.value = "ft";
          heightUnit.value = "ft";

          if (preset === "small-bedroom") {
            lengthInput.value = 10;
            widthInput.value = 10;
            heightInput.value = 8;
          } else if (preset === "medium-room") {
            lengthInput.value = 12;
            widthInput.value = 15;
            heightInput.value = 8;
          } else if (preset === "garage") {
            lengthInput.value = 12;
            widthInput.value = 20;
            heightInput.value = 9;
          }
        });
      });

      document.getElementById("resetBtn").addEventListener("click", () => {
        document.getElementById("length").value = "";
        document.getElementById("width").value = "";
        document.getElementById("height").value = "";
        document.getElementById("doors").value = 1;
        document.getElementById("windows").value = 1;
        document.getElementById("extraOpeningsArea").value = "";
        document.getElementById("includeCeiling").value = "yes";
        document.getElementById("sheetLength").value = "8";
        document.getElementById("wasteFactor").value = "0.10";
        document.getElementById("screwSpacingWalls").value = "16";
        document.getElementById("screwSpacingCeiling").value = "16";
        document.getElementById("compoundLevel").value = "standard";
        document.getElementById("coverageUnit").value = "imperial";

        // Clear results
        const fields = [
          "resultSheets",
          "resultSheetsDetail",
          "resultArea",
          "resultAreaDetail",
          "resultWaste",
          "resultWasteDetail",
          "resultScrews",
          "resultScrewBoxes",
          "resultCompound",
          "resultTape",
          "resultBead",
          "resultWallArea",
          "resultCeilingArea",
          "resultOpeningsArea",
          "resultPerimeter",
        ];
        fields.forEach((id) => {
          const el = document.getElementById(id);
          if (!el) return;
          if (id === "resultSheetsDetail") {
            el.textContent = "Enter dimensions to calculate.";
          } else {
            el.textContent = "–";
          }
        });
      });

      document.getElementById("calculateBtn").addEventListener("click", () => {
        const L = toFeet(
          parseFloat(document.getElementById("length").value),
          document.getElementById("lengthUnit").value
        );
        const W = toFeet(
          parseFloat(document.getElementById("width").value),
          document.getElementById("widthUnit").value
        );
        const H = toFeet(
          parseFloat(document.getElementById("height").value),
          document.getElementById("heightUnit").value
        );
        const includeCeiling = document.getElementById("includeCeiling").value;

        if (!L || !W || !H) {
          alert("Please enter room length, width, and height.");
          return;
        }

        const doors = parseInt(
          document.getElementById("doors").value || "0",
          10
        );
        const windows = parseInt(
          document.getElementById("windows").value || "0",
          10
        );
        const extraOpeningsArea = toSqFeet(
          parseFloat(document.getElementById("extraOpeningsArea").value),
          document.getElementById("extraOpeningsUnit").value
        );

        const sheetWidth = 4; // ft
        const sheetLength = parseFloat(
          document.getElementById("sheetLength").value
        );
        const wasteFactor = parseFloat(
          document.getElementById("wasteFactor").value
        );
        const coverageUnit = document.getElementById("coverageUnit").value;
        const screwSpacingWalls = parseInt(
          document.getElementById("screwSpacingWalls").value,
          10
        );
        const screwSpacingCeiling = parseInt(
          document.getElementById("screwSpacingCeiling").value,
          10
        );
        const compoundLevel = document.getElementById("compoundLevel").value;

        const perimeter = 2 * (L + W);
        let wallArea = perimeter * H;
        let ceilingArea = L * W;

        if (includeCeiling === "walls") {
          ceilingArea = 0;
        } else if (includeCeiling === "ceiling") {
          wallArea = 0;
        }

        const doorArea = doors * (3 * 7); // 21 ft²
        const windowArea = windows * (3 * 4); // 12 ft²
        const openingsArea = doorArea + windowArea + (extraOpeningsArea || 0);

        const grossArea = wallArea + ceilingArea;
        const netArea = Math.max(grossArea - openingsArea, 0);

        const sheetArea = sheetWidth * sheetLength;
        const baseSheets = netArea / sheetArea;
        const finalSheets = Math.ceil(baseSheets * (1 + wasteFactor));

        // Screw estimation
        const wallSheetsArea =
          wallArea > 0 ? (wallArea / grossArea) * finalSheets : 0;
        const ceilingSheetsArea =
          ceilingArea > 0 ? (ceilingArea / grossArea) * finalSheets : 0;

        // Approximate screws per sheet based on spacing
        const screwsPerWallSheet = screwSpacingWalls === 16 ? 32 : 24;
        const screwsPerCeilingSheet = screwSpacingCeiling === 16 ? 40 : 30;

        const totalScrews = Math.round(
          wallSheetsArea * screwsPerWallSheet +
            ceilingSheetsArea * screwsPerCeilingSheet
        );
        const screwBoxes = Math.ceil(totalScrews / 1000);

        // Joint compound estimation
        let compoundPerSqFt;
        if (compoundLevel === "light") compoundPerSqFt = 0.05;
        else if (compoundLevel === "high") compoundPerSqFt = 0.08;
        else compoundPerSqFt = 0.06;

        const compoundGallons = netArea * compoundPerSqFt;

        // Tape & bead estimation (very approximate)
        const verticalJoints = Math.floor(H / sheetLength) + 1;
        const tapeLengthFeet =
          perimeter * verticalJoints +
          perimeter * (includeCeiling !== "walls" ? 1 : 0);
        const tapeRolls = Math.ceil(tapeLengthFeet / 250); // 250 ft per roll

        const cornerBeadFeet = H * 4; // four main corners
        const beadPieces8ft = Math.ceil(cornerBeadFeet / 8);

        // Output formatting
        const areaUnitLabel = coverageUnit === "imperial" ? "ft²" : "m²";
        const lengthUnitLabel = coverageUnit === "imperial" ? "ft" : "m";

        const netAreaOut =
          coverageUnit === "imperial" ? netArea : netArea / 10.7639;
        const wallAreaOut =
          coverageUnit === "imperial" ? wallArea : wallArea / 10.7639;
        const ceilingAreaOut =
          coverageUnit === "imperial" ? ceilingArea : ceilingArea / 10.7639;
        const openingsAreaOut =
          coverageUnit === "imperial" ? openingsArea : openingsArea / 10.7639;
        const perimeterOut =
          coverageUnit === "imperial" ? perimeter : perimeter / 3.28084;

        document.getElementById("resultSheets").textContent = isNaN(finalSheets)
          ? "–"
          : finalSheets;
        document.getElementById(
          "resultSheetsDetail"
        ).textContent = `${sheetWidth}×${sheetLength} ft sheets (approx. ${round(
          sheetArea,
          1
        )} ft² each)`;

        document.getElementById("resultArea").textContent = isNaN(netAreaOut)
          ? "–"
          : `${round(netAreaOut, 1)} ${areaUnitLabel}`;
        document.getElementById(
          "resultAreaDetail"
        ).textContent = `Gross: ${round(
          coverageUnit === "imperial" ? grossArea : grossArea / 10.7639,
          1
        )} ${areaUnitLabel}`;

        document.getElementById("resultWaste").textContent = `${Math.round(
          wasteFactor * 100
        )}%`;
        document.getElementById(
          "resultWasteDetail"
        ).textContent = `Base sheets: ${round(
          baseSheets,
          1
        )} → Final: ${finalSheets}`;

        document.getElementById("resultScrews").textContent = isNaN(totalScrews)
          ? "–"
          : `${totalScrews.toLocaleString()} screws (approx.)`;
        document.getElementById("resultScrewBoxes").textContent = isNaN(
          screwBoxes
        )
          ? "–"
          : `${screwBoxes} box${screwBoxes === 1 ? "" : "es"} of 1,000`;

        document.getElementById("resultCompound").textContent = isNaN(
          compoundGallons
        )
          ? "–"
          : `${round(compoundGallons, 1)} gal (ready-mix)`;
        document.getElementById("resultTape").textContent = isNaN(
          tapeLengthFeet
        )
          ? "–"
          : `${round(
              coverageUnit === "imperial"
                ? tapeLengthFeet
                : tapeLengthFeet / 3.28084,
              1
            )} ${lengthUnitLabel} (~${tapeRolls} roll${
              tapeRolls === 1 ? "" : "s"
            })`;
        document.getElementById("resultBead").textContent = isNaN(
          cornerBeadFeet
        )
          ? "–"
          : `${round(
              coverageUnit === "imperial"
                ? cornerBeadFeet
                : cornerBeadFeet / 3.28084,
              1
            )} ${lengthUnitLabel} (~${beadPieces8ft} × 8 ft pieces)`;

        document.getElementById("resultWallArea").textContent = isNaN(
          wallAreaOut
        )
          ? "–"
          : `${round(wallAreaOut, 1)} ${areaUnitLabel}`;
        document.getElementById("resultCeilingArea").textContent = isNaN(
          ceilingAreaOut
        )
          ? "–"
          : `${round(ceilingAreaOut, 1)} ${areaUnitLabel}`;
        document.getElementById("resultOpeningsArea").textContent = isNaN(
          openingsAreaOut
        )
          ? "–"
          : `${round(openingsAreaOut, 1)} ${areaUnitLabel}`;
