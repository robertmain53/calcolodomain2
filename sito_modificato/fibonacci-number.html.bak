    <script>
      // Mobile menu toggle (fallback if mobile-menu.js doesn't handle it)
      document.addEventListener("DOMContentLoaded", function () {
        const toggle = document.getElementById("mobile-menu-toggle");
        const menu = document.getElementById("mobile-menu");
        if (toggle && menu) {
          toggle.addEventListener("click", () => {
            const isHidden = menu.classList.contains("hidden");
            menu.classList.toggle("hidden", !isHidden);
            toggle.setAttribute("aria-expanded", String(isHidden));
          });
        }
      });

      // Fibonacci calculator logic
      document.addEventListener("DOMContentLoaded", function () {
        const nInput = document.getElementById("fib-n");
        const modeSelect = document.getElementById("fib-mode");
        const digitLimitSelect = document.getElementById("fib-digit-limit");
        const showNeighborsCheckbox =
          document.getElementById("fib-show-neighbors");
        const useRatioLimitCheckbox = document.getElementById(
          "fib-use-ratio-limit"
        );

        const calcBtn = document.getElementById("fib-calc-btn");
        const clearBtn = document.getElementById("fib-clear-btn");
        const ex10Btn = document.getElementById("fib-example-10-btn");
        const ex20Btn = document.getElementById("fib-example-20-btn");
        const ex100Btn = document.getElementById("fib-example-100-btn");

        const errorEl = document.getElementById("fib-error");
        const resultsCard = document.getElementById("fib-results-card");
        const mainEl = document.getElementById("fib-main");
        const mainMetaEl = document.getElementById("fib-main-meta");
        const neighborsSection = document.getElementById(
          "fib-neighbors-section"
        );
        const neighborsEl = document.getElementById("fib-neighbors");
        const seqSection = document.getElementById("fib-sequence-section");
        const seqEl = document.getElementById("fib-sequence");
        const seqNoteEl = document.getElementById("fib-sequence-note");
        const ratioSection = document.getElementById("fib-ratio-section");
        const ratioEl = document.getElementById("fib-ratio");
        const ratioNoteEl = document.getElementById("fib-ratio-note");

        const PHI = (1 + Math.sqrt(5)) / 2;

        function resetResults() {
          errorEl.textContent = "";
          errorEl.classList.add("hidden");
          resultsCard.classList.add("hidden");
          mainEl.textContent = "";
          mainMetaEl.textContent = "";
          neighborsSection.classList.add("hidden");
          neighborsEl.textContent = "";
          seqSection.classList.add("hidden");
          seqEl.textContent = "";
          seqNoteEl.textContent = "";
          ratioSection.classList.add("hidden");
          ratioEl.textContent = "";
          ratioNoteEl.textContent = "";
        }

        function parseIndex(value) {
          const trimmed = (value || "").trim();
          if (!trimmed) {
            throw new Error("The index n is required.");
          }
          if (!/^\d+$/.test(trimmed)) {
            throw new Error("The index n must be a non-negative integer.");
          }
          const nNum = Number(trimmed);
          if (!Number.isInteger(nNum) || nNum < 0) {
            throw new Error("The index n must be a non-negative integer.");
          }
          if (nNum > 10000) {
            throw new Error(
              "This calculator is limited to n ≤ 10000 to keep computations responsive."
            );
          }
          return nNum;
        }

        // Fast doubling Fibonacci, returns [F_n, F_{n+1}] as BigInt
        function fibFastDoubling(nBig) {
          if (nBig === 0n) return [0n, 1n];
          const [a, b] = fibFastDoubling(nBig >> 1n);
          const twoBminusA = 2n * b - a;
          const c = a * twoBminusA; // F(2k)
          const d = a * a + b * b; // F(2k+1)
          if ((nBig & 1n) === 0n) {
            return [c, d];
          } else {
            return [d, c + d];
          }
        }

        function truncateBigIntString(str, limit) {
          if (limit === "all") return { text: str, truncated: false };
          const maxLen = Number(limit);
          if (!Number.isFinite(maxLen) || maxLen <= 0)
            return { text: str, truncated: false };
          if (str.length <= maxLen) return { text: str, truncated: false };
          const head = str.slice(0, maxLen);
          const tail = str.slice(-10);
          return {
            text: head + "… (…" + tail + ")",
            truncated: true,
          };
        }

        function buildSequence(nNum, F_n) {
          // For sequence listing, cap at maxNSeq
          const maxNSeq = 200;
          const effectiveN = Math.min(nNum, maxNSeq);
          let seq = [];
          let a = 0n;
          let b = 1n;
          for (let i = 0; i <= effectiveN; i++) {
            seq.push("F_" + i + " = " + a.toString());
            const next = a + b;
            a = b;
            b = next;
          }
          let note = "";
          if (nNum > maxNSeq) {
            note =
              "Sequence listing is truncated at n = " +
              maxNSeq +
              " to keep the output manageable. Use the main value and digit count for larger n.";
          } else {
            note = "Full sequence from F_0 through F_" + nNum + " is shown.";
          }
          return { text: seq.join("\n"), note };
        }

        function buildRatio(nNum, F_n, F_n1) {
          if (F_n === 0n) {
            return {
              ratioText:
                "The ratio F_{n+1} / F_n is undefined for n = 0 because F_0 = 0.",
              note: "Start from n ≥ 1 to study convergence to the golden ratio.",
            };
          }

          let ratioStr;
          let note;
          const limit = useRatioLimitCheckbox.checked ? 70 : 500;

          if (nNum > limit) {
            ratioStr =
              "For large n, the ratio F_{n+1} / F_n is extremely close to the golden ratio φ ≈ " +
              PHI.toFixed(12) +
              ".";
            note =
              "Direct floating-point computation of the ratio is restricted to n ≤ " +
              limit +
              " to avoid precision loss. For larger n, theoretical convergence guarantees apply.";
          } else {
            // Convert to Number cautiously (values are still huge but within demonstration range)
            const numFn = Number(F_n.toString());
            const numFn1 = Number(F_n1.toString());
            if (!Number.isFinite(numFn) || !Number.isFinite(numFn1)) {
              ratioStr =
                "The ratio is too large to convert reliably to standard floating-point format.";
              note =
                "Use symbolic or high-precision tools if you need an explicit decimal ratio for very large n.";
            } else {
              const ratio = numFn1 / numFn;
              const diff = Math.abs(ratio - PHI);
              ratioStr =
                "F_{n+1} / F_n ≈ " +
                ratio.toPrecision(15) +
                "  (φ ≈ " +
                PHI.toPrecision(15) +
                ")";
              note =
                "Absolute difference |ratio − φ| ≈ " +
                diff.toExponential(3) +
                ". This illustrates how quickly the sequence converges toward the golden ratio.";
            }
          }

          return { ratioText: ratioStr, note };
        }

        function compute() {
          resetResults();
          try {
            const nNum = parseIndex(nInput.value);
            const nBig = BigInt(nNum);
            const mode = modeSelect.value;
            const digitLimit = digitLimitSelect.value;
            const showNeighbors = showNeighborsCheckbox.checked;

            const [F_n, F_n1] = fibFastDoubling(nBig);
            const F_n_str = F_n.toString();
            const digitCount = F_n_str.length;

            const truncated = truncateBigIntString(F_n_str, digitLimit);
            const displayIndex = "F_" + nNum + " = ";
            mainEl.textContent = displayIndex + truncated.text;

            let metaText =
              "Index n = " +
              nNum +
              ". Exact value has " +
              digitCount +
              " decimal digits.";
            if (truncated.truncated) {
              metaText +=
                " Display truncated according to the selected digit limit.";
            }
            mainMetaEl.textContent = metaText;

            // neighbors
            if (showNeighbors && nNum > 0) {
              const F_n1_big = F_n1;
              const F_n_minus1 = F_n1_big - F_n; // since F_{n+1} = F_n + F_{n-1}
              const neighParts = [];
              neighParts.push(
                "F_" + (nNum - 1) + " = " + F_n_minus1.toString()
              );
              neighParts.push("F_" + (nNum + 1) + " = " + F_n1_big.toString());
              neighborsEl.textContent = neighParts.join("\n");
              neighborsSection.classList.remove("hidden");
            }

            // sequence
            if (mode === "sequence" || mode === "both") {
              const seqData = buildSequence(nNum, F_n);
              seqEl.textContent = seqData.text;
              seqNoteEl.textContent = seqData.note;
              seqSection.classList.remove("hidden");
            }

            // ratio
            if ((mode === "ratio" || mode === "both") && nNum >= 0) {
              const ratioData = buildRatio(nNum, F_n, F_n1);
              ratioEl.textContent = ratioData.ratioText;
              ratioNoteEl.textContent = ratioData.note;
              ratioSection.classList.remove("hidden");
            }

            resultsCard.classList.remove("hidden");
          } catch (e) {
            errorEl.textContent =
              e.message ||
              "An error occurred while computing the Fibonacci number.";
            errorEl.classList.remove("hidden");
          }
        }

        function clearAll() {
          nInput.value = "";
          modeSelect.value = "value";
          digitLimitSelect.value = "500";
          showNeighborsCheckbox.checked = false;
          useRatioLimitCheckbox.checked = true;
          resetResults();
        }

        function loadExample(n) {
          nInput.value = String(n);
          modeSelect.value = n <= 20 ? "both" : "value";
          digitLimitSelect.value = "500";
          showNeighborsCheckbox.checked = true;
          useRatioLimitCheckbox.checked = true;
          compute();
        }

        calcBtn.addEventListener("click", compute);
        clearBtn.addEventListener("click", clearAll);
        ex10Btn.addEventListener("click", function () {
          loadExample(10);
        });
        ex20Btn.addEventListener("click", function () {
          loadExample(20);
        });
        ex100Btn.addEventListener("click", function () {
          loadExample(100);
        });

        // Ctrl+Enter on n triggers calculation
        nInput.addEventListener("keydown", function (e) {
          if (e.key === "Enter" && (e.ctrlKey || e.metaKey)) {
            e.preventDefault();
            compute();
          }
        });
      });
    </script>
  
