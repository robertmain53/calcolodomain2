    <script>
      // Utility: DMS to decimal
      function dmsToDecimal(d, m, s) {
        d = Number(d) || 0;
        m = Number(m) || 0;
        s = Number(s) || 0;
        const sign = d < 0 ? -1 : 1;
        return sign * (Math.abs(d) + m / 60 + s / 3600);
      }

      // Utility: format number
      function fmt(x, decimals = 3) {
        if (!isFinite(x)) return "–";
        return Number(x).toFixed(decimals);
      }

      const legsBody = document.getElementById("legsBody");
      const addLegBtn = document.getElementById("addLegBtn");
      const clearLegsBtn = document.getElementById("clearLegsBtn");
      const computeBtn = document.getElementById("computeBtn");
      const angleFormatEl = document.getElementById("angleFormat");
      const adjustmentMethodEl = document.getElementById("adjustmentMethod");
      const closedTraverseEl = document.getElementById("closedTraverse");
      const errorBox = document.getElementById("errorBox");
      const resultsSection = document.getElementById("resultsSection");
      const misclosureENEl = document.getElementById("misclosureEN");
      const misclosureLenEl = document.getElementById("misclosureLen");
      const relativePrecisionEl = document.getElementById("relativePrecision");
      const coordBody = document.getElementById("coordBody");
      const areaResultEl = document.getElementById("areaResult");

      function createLegRow(index) {
        const tr = document.createElement("tr");
        tr.innerHTML = `
      <td class="px-2 py-1 text-gray-600">${index + 1}</td>
      <td class="px-2 py-1">
        <input type="number" step="any" class="input-small w-24 border-gray-300 rounded-md text-xs px-1.5 py-1" placeholder="Distance">
      </td>
      <td class="px-2 py-1">
        <div class="flex items-center gap-1 angle-decimal">
          <input type="number" step="any" class="input-small w-24 border-gray-300 rounded-md text-xs px-1.5 py-1" placeholder="Azimuth °">
        </div>
        <div class="hidden flex-wrap items-center gap-1 angle-dms mt-1">
          <input type="number" class="input-small w-14 border-gray-300 rounded-md text-xs px-1.5 py-1" placeholder="°">
          <input type="number" class="input-small w-14 border-gray-300 rounded-md text-xs px-1.5 py-1" placeholder="'">
          <input type="number" class="input-small w-14 border-gray-300 rounded-md text-xs px-1.5 py-1" placeholder='"'>
        </div>
      </td>
      <td class="px-2 py-1">
        <select class="quadrant w-20 border-gray-300 rounded-md text-xs px-1.5 py-1">
          <option value="">—</option>
          <option value="NE">NE</option>
          <option value="SE">SE</option>
          <option value="SW">SW</option>
          <option value="NW">NW</option>
        </select>
      </td>
      <td class="px-2 py-1">
        <button type="button" class="removeLeg text-xs text-red-600 hover:underline">Remove</button>
      </td>
    `;
        return tr;
      }

      function refreshAngleInputs() {
        const isDms = angleFormatEl.value === "dms";
        legsBody.querySelectorAll("tr").forEach((row) => {
          const dec = row.querySelector(".angle-decimal");
          const dms = row.querySelector(".angle-dms");
          if (isDms) {
            dec.classList.add("hidden");
            dms.classList.remove("hidden");
          } else {
            dec.classList.remove("hidden");
            dms.classList.add("hidden");
          }
        });
      }

      function addLeg() {
        const index = legsBody.children.length;
        const row = createLegRow(index);
        legsBody.appendChild(row);
        refreshAngleInputs();
      }

      addLegBtn.addEventListener("click", addLeg);

      clearLegsBtn.addEventListener("click", () => {
        legsBody.innerHTML = "";
        addLeg();
        resultsSection.classList.add("hidden");
        errorBox.classList.add("hidden");
      });

      angleFormatEl.addEventListener("change", refreshAngleInputs);

      legsBody.addEventListener("click", (e) => {
        if (e.target.classList.contains("removeLeg")) {
          const row = e.target.closest("tr");
          row.remove();
          // renumber
          Array.from(legsBody.children).forEach((tr, i) => {
            tr.querySelector("td").textContent = i + 1;
          });
        }
      });

      function parseLegs() {
        const isDms = angleFormatEl.value === "dms";
        const legs = [];
        Array.from(legsBody.children).forEach((tr, i) => {
          const distInput = tr.querySelector("td:nth-child(2) input");
          const distance = parseFloat(distInput.value);
          if (!isFinite(distance) || distance <= 0) {
            throw new Error(
              `Leg ${i + 1}: distance must be a positive number.`
            );
          }

          let azimuthDeg;
          if (isDms) {
            const dmsInputs = tr.querySelectorAll(".angle-dms input");
            const d = parseFloat(dmsInputs[0].value);
            const m = parseFloat(dmsInputs[1].value);
            const s = parseFloat(dmsInputs[2].value);
            if (!isFinite(d)) {
              throw new Error(`Leg ${i + 1}: enter degrees for bearing.`);
            }
            const theta = dmsToDecimal(d, m, s);
            const quadrant = tr.querySelector(".quadrant").value;
            if (quadrant) {
              const absTheta = Math.abs(theta);
              switch (quadrant) {
                case "NE":
                  azimuthDeg = absTheta;
                  break;
                case "SE":
                  azimuthDeg = 180 - absTheta;
                  break;
                case "SW":
                  azimuthDeg = 180 + absTheta;
                  break;
                case "NW":
                  azimuthDeg = 360 - absTheta;
                  break;
              }
            } else {
              azimuthDeg = theta;
            }
          } else {
            const azInput = tr.querySelector(".angle-decimal input");
            azimuthDeg = parseFloat(azInput.value);
            if (!isFinite(azimuthDeg)) {
              throw new Error(`Leg ${i + 1}: enter azimuth in degrees.`);
            }
          }

          // normalize 0-360
          azimuthDeg = ((azimuthDeg % 360) + 360) % 360;
          legs.push({ distance, azimuthDeg });
        });

        if (legs.length < 1) {
          throw new Error("Enter at least one traverse leg.");
        }
        return legs;
      }

      function computeTraverse() {
        errorBox.classList.add("hidden");
        resultsSection.classList.add("hidden");
        coordBody.innerHTML = "";
        areaResultEl.textContent = "–";

        let legs;
        try {
          legs = parseLegs();
        } catch (err) {
          errorBox.textContent = err.message;
          errorBox.classList.remove("hidden");
          return;
        }

        const startE = parseFloat(document.getElementById("startE").value) || 0;
        const startN = parseFloat(document.getElementById("startN").value) || 0;
        const closed = closedTraverseEl.checked;
        const method = adjustmentMethodEl.value;

        const n = legs.length;
        const deltas = [];
        let totalLength = 0;
        let sumDE = 0;
        let sumDN = 0;

        legs.forEach((leg, i) => {
          const L = leg.distance;
          const azRad = (leg.azimuthDeg * Math.PI) / 180;
          const dE = L * Math.sin(azRad);
          const dN = L * Math.cos(azRad);
          deltas.push({ dE, dN, L });
          totalLength += L;
          sumDE += dE;
          sumDN += dN;
        });

        const misE = closed ? sumDE : 0;
        const misN = closed ? sumDN : 0;
        const misLen = closed ? Math.hypot(misE, misN) : 0;
        const relPrec = closed && misLen > 0 ? totalLength / misLen : null;

        misclosureENEl.textContent = closed
          ? `ΔE = ${fmt(misE)} , ΔN = ${fmt(misN)}`
          : "Open traverse (no enforced closure)";
        misclosureLenEl.textContent = closed ? `${fmt(misLen)} (units)` : "–";
        relativePrecisionEl.textContent =
          closed && relPrec ? `1 : ${fmt(relPrec, 0)}` : "–";

        // Raw coordinates
        const rawCoords = [{ E: startE, N: startN }];
        let curE = startE;
        let curN = startN;
        deltas.forEach((d) => {
          curE += d.dE;
          curN += d.dN;
          rawCoords.push({ E: curE, N: curN });
        });

        // Adjusted coordinates (Bowditch)
        let adjCoords = rawCoords.map((c) => ({ ...c }));
        if (closed && method === "bowditch" && totalLength > 0 && misLen > 0) {
          const corr = deltas.map((d) => {
            const factor = d.L / totalLength;
            return {
              dE: d.dE - misE * factor,
              dN: d.dN - misN * factor,
            };
          });
          adjCoords = [{ E: startE, N: startN }];
          let aE = startE;
          let aN = startN;
          corr.forEach((c) => {
            aE += c.dE;
            aN += c.dN;
            adjCoords.push({ E: aE, N: aN });
          });
        }

        // Populate coordinates table
        const rows = [];
        for (let i = 0; i < rawCoords.length; i++) {
          const r = rawCoords[i];
          const a = adjCoords[i];
          rows.push(`
        <tr>
          <td class="px-2 py-1">P${i}</td>
          <td class="px-2 py-1 font-mono">${fmt(r.E)}</td>
          <td class="px-2 py-1 font-mono">${fmt(r.N)}</td>
          <td class="px-2 py-1 font-mono">${fmt(a.E)}</td>
          <td class="px-2 py-1 font-mono">${fmt(a.N)}</td>
        </tr>
      `);
        }
        coordBody.innerHTML = rows.join("");

        // Area (use adjusted if closed & available, else raw)
        let poly = rawCoords;
        if (closed && method === "bowditch") {
          poly = adjCoords;
        }
        if (closed && poly.length > 2) {
          let area = 0;
          for (let i = 0; i < poly.length - 1; i++) {
            const p1 = poly[i];
            const p2 = poly[i + 1];
            area += p1.E * p2.N - p2.E * p1.N;
          }
          // close polygon
          const pLast = poly[poly.length - 1];
          const p0 = poly[0];
          area += pLast.E * p0.N - p0.E * pLast.N;
          area = Math.abs(area) / 2;
          areaResultEl.textContent = `${fmt(area)} square units`;
        } else {
          areaResultEl.textContent = closed
            ? "Not enough points to compute area."
            : "Area only computed for closed traverses.";
        }

        resultsSection.classList.remove("hidden");
      }

      computeBtn.addEventListener("click", computeTraverse);

      // Initialize with a few rows
      addLeg();
      addLeg();
    </script>

