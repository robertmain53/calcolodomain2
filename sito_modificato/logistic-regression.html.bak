  <script>
    // mobile menu toggle
    const mobileToggle = document.getElementById('mobile-menu-toggle');
    const mobileMenu = document.getElementById('mobile-menu');
    if (mobileToggle && mobileMenu) {
      mobileToggle.addEventListener('click', () => {
        const isHidden = mobileMenu.classList.contains('hidden');
        mobileMenu.classList.toggle('hidden', !isHidden);
        mobileToggle.setAttribute('aria-expanded', String(isHidden));
      });
    }

    // --- logistic regression JS ---
    function parseData(raw) {
      // split lines
      const lines = raw.split(/\r?\n/).map(l => l.trim()).filter(l => l.length > 0);
      if (lines.length === 0) return null;

      // detect header by checking last token numeric
      const firstTokens = lines[0].split(/[,;\s\t]+/).filter(t => t.length>0);
      const hasHeader = isNaN(parseFloat(firstTokens[firstTokens.length - 1]));
      const dataLines = hasHeader ? lines.slice(1) : lines;

      const X = [];
      const y = [];
      dataLines.forEach(line => {
        const parts = line.split(/[,;\s\t]+/).filter(t => t.length>0);
        if (parts.length < 2) return;
        const target = parseFloat(parts[parts.length - 1]);
        const feats = parts.slice(0, parts.length - 1).map(Number);
        if (feats.some(v => isNaN(v)) || isNaN(target)) return;
        X.push(feats);
        y.push(target);
      });

      let featureNames = [];
      if (hasHeader) {
        featureNames = firstTokens.slice(0, firstTokens.length - 1);
      } else {
        featureNames = X[0] ? X[0].map((_, i) => "x" + (i+1)) : [];
      }

      return { X, y, featureNames };
    }

    function sigmoid(z) {
      return 1 / (1 + Math.exp(-z));
    }

    function trainLogReg(X, y, lr, epochs) {
      const n = X.length;
      if (n === 0) return null;
      const m = X[0].length;
      // weights + bias
      let w = new Array(m).fill(0);
      let b = 0;

      for (let epoch = 0; epoch < epochs; epoch++) {
        let gradW = new Array(m).fill(0);
        let gradB = 0;

        for (let i = 0; i < n; i++) {
          const xi = X[i];
          const yi = y[i];
          let z = b;
          for (let j = 0; j < m; j++) z += w[j] * xi[j];
          const p = sigmoid(z);
          const diff = p - yi;
          for (let j = 0; j < m; j++) {
            gradW[j] += diff * xi[j];
          }
          gradB += diff;
        }

        for (let j = 0; j < m; j++) {
          w[j] -= lr * gradW[j] / n;
        }
        b -= lr * gradB / n;
      }

      // compute loss
      let loss = 0;
      for (let i=0; i<n; i++) {
        const xi = X[i], yi = y[i];
        let z = b;
        for (let j=0; j<m; j++) z += w[j] * xi[j];
        const p = sigmoid(z);
        loss += -(yi * Math.log(p + 1e-12) + (1 - yi) * Math.log(1 - p + 1e-12));
      }
      loss / n;

      return { w, b, loss };
    }

    function buildModelOutput(model, featureNames, decimals) {
      if (!model) return "<p class='text-sm text-red-500'>Could not train model.</p>";
      let html = "";
      html += "<p class='text-sm text-gray-700 mb-2'><strong>Loss (cross-entropy):</strong> " + model.loss.toFixed(decimals) + "</p>";
      html += "<p class='text-sm text-gray-700 mb-2'><strong>Intercept (β₀):</strong> " + model.b.toFixed(decimals) + "</p>";
      html += "<div class='scroll-x'><table class='tbl'><thead><tr><th>Feature</th><th>Coefficient (β)</th><th>Odds ratio e^(β)</th></tr></thead><tbody>";
      model.w.forEach((coef, i) => {
        const name = featureNames[i] || ("x" + (i+1));
        const or = Math.exp(coef);
        html += "<tr><td>" + name + "</td><td>" + coef.toFixed(decimals) + "</td><td>" + or.toFixed(decimals) + "</td></tr>";
      });
      html += "</tbody></table></div>";
      html += "<p class='text-xs text-gray-500 mt-2'>Probability for an input x is p = 1 / (1 + exp(-(β₀ + Σ βᵢ xᵢ))).</p>";
      return html;
    }

    const dataInputEl = document.getElementById('data-input');
    const trainBtn = document.getElementById('train-btn');
    const errBox = document.getElementById('err-box');
    const modelOutputEl = document.getElementById('model-output');
    const predictInputsEl = document.getElementById('predict-inputs');
    const predictBtn = document.getElementById('predict-btn');
    const predictOutEl = document.getElementById('predict-output');

    let currentModel = null;
    let currentFeatures = [];

    trainBtn.addEventListener('click', () => {
      errBox.textContent = "";
      predictInputsEl.innerHTML = "";
      predictBtn.disabled = true;
      predictOutEl.textContent = "";
      currentModel = null;
      currentFeatures = [];

      const parsed = parseData(dataInputEl.value);
      if (!parsed || parsed.X.length === 0) {
        errBox.textContent = "Could not parse data. Check format and that last column is numeric (0/1).";
        return;
      }

      const lr = parseFloat(document.getElementById('lr').value);
      const epochs = parseInt(document.getElementById('epochs').value, 10);
      const decimals = parseInt(document.getElementById('decimals').value, 10);

      if (isNaN(lr) || lr <= 0) {
        errBox.textContent = "Learning rate must be positive.";
        return;
      }
      if (isNaN(epochs) || epochs <= 0) {
        errBox.textContent = "Epochs must be positive.";
        return;
      }

      const model = trainLogReg(parsed.X, parsed.y, lr, epochs);
      if (!model) {
        errBox.textContent = "Training failed.";
        return;
      }

      currentModel = model;
      currentFeatures = parsed.featureNames;

      modelOutputEl.innerHTML = buildModelOutput(model, parsed.featureNames, decimals);

      // build predictor inputs
      parsed.featureNames.forEach((name, idx) => {
        const div = document.createElement('div');
        div.innerHTML = `
          <label class="text-sm font-medium text-gray-700">${name}</label>
          <input type="number" step="any" data-pred-idx="${idx}" class="mt-1 w-full border rounded-md px-3 py-2" value="0">
        `;
        predictInputsEl.appendChild(div);
      });
      predictBtn.disabled = false;
    });

    predictBtn.addEventListener('click', () => {
      if (!currentModel) return;
      const decimals = parseInt(document.getElementById('decimals').value, 10);
      const m = currentModel.w.length;
      const x = new Array(m).fill(0);
      const inputs = predictInputsEl.querySelectorAll('input[data-pred-idx]');
      inputs.forEach(inp => {
        const idx = parseInt(inp.getAttribute('data-pred-idx'), 10);
        x[idx] = parseFloat(inp.value) || 0;
      });
      let z = currentModel.b;
      for (let j=0; j<m; j++) z += currentModel.w[j] * x[j];
      const p = sigmoid(z);
      predictOutEl.textContent = `Predicted probability (y=1): ${p.toFixed(decimals)} (${(p*100).toFixed(1)}%)`;
    });

    // run once with default
    trainBtn.click();
  </script>

