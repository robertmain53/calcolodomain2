    <script>
      (function () {
        const remInput = document.getElementById("remInput");
        const msvInput = document.getElementById("msvInput");
        const remToMsvResult = document.getElementById("remToMsvResult");
        const msvToRemResult = document.getElementById("msvToRemResult");
        const errorMsg = document.getElementById("errorMsg");
        const resetBtn = document.getElementById("resetBtn");
        const swapBtn = document.getElementById("swapBtn");
        const remMaxBtn = document.getElementById("remMaxBtn");
        const msvMaxBtn = document.getElementById("msvMaxBtn");

        const FACTOR = 10; // 1 rem = 10 mSv

        function formatNumber(value) {
          if (!isFinite(value)) return "–";
          if (Math.abs(value) === 0) return "0";
          if (Math.abs(value) < 0.001 || Math.abs(value) >= 1e6) {
            return value.toExponential(4);
          }
          return parseFloat(value.toFixed(6)).toString();
        }

        function showError(show) {
          errorMsg.classList.toggle("hidden", !show);
        }

        function convertFromRem() {
          const val = remInput.value.trim();
          if (val === "") {
            remToMsvResult.textContent = "–";
            showError(false);
            return;
          }
          const num = Number(val);
          if (!isFinite(num) || num < 0) {
            showError(true);
            remToMsvResult.textContent = "–";
            return;
          }
          showError(false);
          const msv = num * FACTOR;
          remToMsvResult.textContent = formatNumber(msv);
          // keep other field in sync without triggering loop
          msvInput.value = formatNumber(msv);
          msvToRemResult.textContent = formatNumber(num);
        }

        function convertFromMsv() {
          const val = msvInput.value.trim();
          if (val === "") {
            msvToRemResult.textContent = "–";
            showError(false);
            return;
          }
          const num = Number(val);
          if (!isFinite(num) || num < 0) {
            showError(true);
            msvToRemResult.textContent = "–";
            return;
          }
          showError(false);
          const rem = num / FACTOR;
          msvToRemResult.textContent = formatNumber(rem);
          remInput.value = formatNumber(rem);
          remToMsvResult.textContent = formatNumber(num);
        }

        remInput.addEventListener("input", convertFromRem);
        msvInput.addEventListener("input", convertFromMsv);

        resetBtn.addEventListener("click", () => {
          remInput.value = "";
          msvInput.value = "";
          remToMsvResult.textContent = "–";
          msvToRemResult.textContent = "–";
          showError(false);
          remInput.focus();
        });

        swapBtn.addEventListener("click", () => {
          const remVal = remInput.value;
          const msvVal = msvInput.value;
          remInput.value = msvVal;
          msvInput.value = remVal;
          if (remInput.value) {
            convertFromRem();
          } else if (msvInput.value) {
            convertFromMsv();
          } else {
            remToMsvResult.textContent = "–";
            msvToRemResult.textContent = "–";
            showError(false);
          }
        });

        remMaxBtn.addEventListener("click", () => {
          const current = Number(remInput.value) || 0;
          remInput.value = current + 10;
          convertFromRem();
        });

        msvMaxBtn.addEventListener("click", () => {
          const current = Number(msvInput.value) || 0;
          msvInput.value = current + 10;
          convertFromMsv();
        });
      })();
    </script>

