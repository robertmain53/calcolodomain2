    <script>
      // --- Tax model (illustrative, not official) ---
      const FEDERAL_BRACKETS = [
        { upTo: 55000, rate: 0.15 },
        { upTo: 110000, rate: 0.205 },
        { upTo: 165000, rate: 0.26 },
        { upTo: 235000, rate: 0.29 },
        { upTo: Infinity, rate: 0.33 },
      ];
      const FEDERAL_BASIC_PERSONAL = 16000; // illustrative

      const NL_BRACKETS = [
        { upTo: 42000, rate: 0.087 },
        { upTo: 84000, rate: 0.145 },
        { upTo: 150000, rate: 0.158 },
        { upTo: 200000, rate: 0.173 },
        { upTo: Infinity, rate: 0.183 }, // approximates surtax impact
      ];
      const NL_BASIC_PERSONAL = 10000; // illustrative

      // CPP/EI assumptions (approximate 2025-style)
      const CPP_RATE_EMP = 0.0595;
      const CPP_RATE_SELF = CPP_RATE_EMP * 2;
      const CPP_BASIC_EXEMPT = 3500;
      const CPP_MAX_PENSIONABLE = 75000;

      const EI_RATE_EMP = 0.0166;
      const EI_MAX_INSURABLE = 65000;

      function calcBracketTax(income, brackets) {
        let remaining = income;
        let lastRate = 0;
        let tax = 0;
        let prevLimit = 0;
        for (const b of brackets) {
          const span = Math.min(remaining, b.upTo - prevLimit);
          if (span <= 0) break;
          tax += span * b.rate;
          remaining -= span;
          lastRate = b.rate;
          prevLimit = b.upTo;
          if (remaining <= 0) break;
        }
        return { tax, marginalRate: lastRate };
      }

      function calcFederalTax(taxable) {
        if (taxable <= 0) return { tax: 0, marginalRate: 0 };
        const { tax, marginalRate } = calcBracketTax(taxable, FEDERAL_BRACKETS);
        const credit = FEDERAL_BASIC_PERSONAL * FEDERAL_BRACKETS[0].rate;
        const net = Math.max(0, tax - credit);
        return { tax: net, marginalRate };
      }

      function calcNLTax(taxable) {
        if (taxable <= 0) return { tax: 0, marginalRate: 0 };
        const { tax, marginalRate } = calcBracketTax(taxable, NL_BRACKETS);
        const credit = NL_BASIC_PERSONAL * NL_BRACKETS[0].rate;
        const net = Math.max(0, tax - credit);
        return { tax: net, marginalRate };
      }

      function calcCPP(income, type) {
        if (type === "none") return 0;
        const pensionable = Math.max(
          0,
          Math.min(income, CPP_MAX_PENSIONABLE) - CPP_BASIC_EXEMPT
        );
        if (pensionable <= 0) return 0;
        const rate = type === "self" ? CPP_RATE_SELF : CPP_RATE_EMP;
        return pensionable * rate;
      }

      function calcEI(income, type) {
        if (type !== "employee") return 0;
        const insurable = Math.min(income, EI_MAX_INSURABLE);
        if (insurable <= 0) return 0;
        return insurable * EI_RATE_EMP;
      }

      function formatMoney(v) {
        if (!isFinite(v)) return "$0";
        return v.toLocaleString("en-CA", {
          style: "currency",
          currency: "CAD",
          maximumFractionDigits: 0,
        });
      }

      function formatPercent(v) {
        if (!isFinite(v)) return "0%";
        return (v * 100).toFixed(1).replace(/\.0$/, "") + "%";
      }

      function calculate() {
        const income = parseFloat(document.getElementById("income").value) || 0;
        const otherIncome =
          parseFloat(document.getElementById("other-income").value) || 0;
        const rrsp = parseFloat(document.getElementById("rrsp").value) || 0;
        const employmentType = document.getElementById("employment-type").value;
        const errorEl = document.getElementById("calc-error");

        if (income < 0 || otherIncome < 0 || rrsp < 0) {
          errorEl.textContent = "Inputs cannot be negative.";
          errorEl.classList.remove("hidden");
          return;
        }
        errorEl.classList.add("hidden");

        const gross = income + otherIncome;
        const taxable = Math.max(0, gross - rrsp);

        const fed = calcFederalTax(taxable);
        const nl = calcNLTax(taxable);

        const cpp = calcCPP(income, employmentType);
        const ei = calcEI(income, employmentType);

        const totalTax = fed.tax + nl.tax;
        const totalPayroll = cpp + ei;
        const net = gross - totalTax - totalPayroll;

        const avgRate = taxable > 0 ? totalTax / taxable : 0;
        const marginalRate = fed.marginalRate + nl.marginalRate;

        // Update UI
        document.getElementById("result-net").textContent = formatMoney(net);
        document.getElementById("result-total-tax").textContent =
          formatMoney(totalTax);
        document.getElementById("result-federal-tax").textContent = formatMoney(
          fed.tax
        );
        document.getElementById("result-provincial-tax").textContent =
          formatMoney(nl.tax);
        document.getElementById("result-cpp").textContent = formatMoney(cpp);
        document.getElementById("result-ei").textContent = formatMoney(ei);
        document.getElementById("result-cpp-ei").textContent =
          formatMoney(totalPayroll);
        document.getElementById("result-avg-rate").textContent =
          formatPercent(avgRate);
        document.getElementById("result-marginal-rate").textContent =
          formatPercent(marginalRate);
      }

      document.addEventListener("DOMContentLoaded", () => {
        const incomeInput = document.getElementById("income");
        const slider = document.getElementById("income-slider");
        const sliderOut = document.getElementById("income-slider-output");

        function syncSliderFromInput() {
          let val = parseFloat(incomeInput.value) || 0;
          if (val < parseFloat(slider.min)) val = parseFloat(slider.min);
          if (val > parseFloat(slider.max)) val = parseFloat(slider.max);
          slider.value = val;
          sliderOut.textContent = "$" + Math.round(val / 1000) + "k";
        }

        function syncInputFromSlider() {
          const val = parseFloat(slider.value) || 0;
          incomeInput.value = val;
          sliderOut.textContent = "$" + Math.round(val / 1000) + "k";
          calculate();
        }

        document
          .getElementById("calculate-btn")
          .addEventListener("click", calculate);
        incomeInput.addEventListener("change", () => {
          syncSliderFromInput();
          calculate();
        });
        slider.addEventListener("input", syncInputFromSlider);

        // Initial sync and calculation
        syncSliderFromInput();
        calculate();
      });

      // Mobile menu toggle
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          this.setAttribute("aria-expanded", String(isHidden));
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
