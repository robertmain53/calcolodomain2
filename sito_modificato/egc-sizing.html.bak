      // Populate selects for upsizing mode
      function populateConductorSelects() {
        const minSel = document.getElementById("minPhaseSize");
        const actSel = document.getElementById("actualPhaseSize");
        const baseEgcSel = document.getElementById("baseEgcSize");
        [minSel, actSel, baseEgcSel].forEach((sel) => {
          sel.innerHTML = "";
        });

        conductorSizes.forEach((size) => {
          const opt1 = document.createElement("option");
          opt1.value = size.label;
          opt1.textContent = size.label;
          minSel.appendChild(opt1);

          const opt2 = document.createElement("option");
          opt2.value = size.label;
          opt2.textContent = size.label;
          actSel.appendChild(opt2);

          const opt3 = document.createElement("option");
          opt3.value = size.label;
          opt3.textContent = size.label;
          baseEgcSel.appendChild(opt3);
        });

        // Set some sensible defaults
        minSel.value = "3 AWG";
        actSel.value = "1/0 AWG";
        baseEgcSel.value = "6 AWG";
      }

      function findEgcSize(amps, material) {
        const table = material === "al" ? egcTableAl : egcTableCu;
        for (const row of table) {
          if (amps <= row.maxA) return row.size;
        }
        return table[table.length - 1].size;
      }

      function getAreaByLabel(label) {
        const item = conductorSizes.find((s) => s.label === label);
        return item ? item.area : null;
      }

      function showResult(mode, mainText, detailText) {
        const card = document.getElementById("resultsCard");
        const text = document.getElementById("resultText");
        const detail = document.getElementById("resultDetail");
        const badge = document.getElementById("resultModeBadge");

        text.textContent = mainText;
        detail.textContent = detailText || "";

        if (mode === "ocpd") {
          badge.textContent = "OCPD-based";
          badge.className = "badge bg-blue-100 text-blue-700";
        } else {
          badge.textContent = "Upsizing factor";
          badge.className = "badge bg-emerald-100 text-emerald-700";
        }

        card.classList.remove("hidden");
      }

      document.addEventListener("DOMContentLoaded", () => {
        populateConductorSelects();

        const tabOcpd = document.getElementById("tab-ocpd");
        const tabUpsizing = document.getElementById("tab-upsizing");
        const panelOcpd = document.getElementById("panel-ocpd");
        const panelUpsizing = document.getElementById("panel-upsizing");

        tabOcpd.addEventListener("click", () => {
          tabOcpd.className =
            "px-3 py-1.5 text-sm rounded-md bg-blue-600 text-white font-medium";
          tabUpsizing.className =
            "px-3 py-1.5 text-sm rounded-md bg-gray-100 text-gray-700";
          panelOcpd.classList.remove("hidden");
          panelUpsizing.classList.add("hidden");
        });

        tabUpsizing.addEventListener("click", () => {
          tabUpsizing.className =
            "px-3 py-1.5 text-sm rounded-md bg-emerald-600 text-white font-medium";
          tabOcpd.className =
            "px-3 py-1.5 text-sm rounded-md bg-gray-100 text-gray-700";
          panelUpsizing.classList.remove("hidden");
          panelOcpd.classList.add("hidden");
        });

        document.getElementById("btnCalcOcpd").addEventListener("click", () => {
          const amps = parseFloat(document.getElementById("ocpdRating").value);
          const material = document.getElementById("egcMaterial").value;
          const type = document.getElementById("ocpdType").value;

          if (!amps || amps <= 0) {
            showResult(
              "ocpd",
              "Please enter a valid OCPD rating in amperes.",
              "The rating must be a positive number."
            );
            return;
          }

          const size = findEgcSize(amps, material);
          const matLabel =
            material === "al" ? "Aluminum / Cu-Clad Al" : "Copper";

          const main = `Minimum EGC size: ${size} ${matLabel}`;
          const detail =
            `Based on an overcurrent device rating of approximately ${amps.toFixed(
              0
            )} A ` +
            `(${
              type === "breaker" ? "circuit breaker" : "fuse"
            }) and a NEC-style Table 250.122 structure. ` +
            `Always verify against the actual NEC table for your edition and jurisdiction.`;

          showResult("ocpd", main, detail);
        });

        document
          .getElementById("btnCalcUpsizing")
          .addEventListener("click", () => {
            const minSize = document.getElementById("minPhaseSize").value;
            const actSize = document.getElementById("actualPhaseSize").value;
            const baseEgc = document.getElementById("baseEgcSize").value;
            const phaseMat = document.getElementById("phaseMaterial").value;
            const egcMat = document.getElementById("egcMaterial").value;

            const aMin = getAreaByLabel(minSize);
            const aAct = getAreaByLabel(actSize);
            const aBase = getAreaByLabel(baseEgc);

            if (!aMin || !aAct || !aBase) {
              showResult(
                "upsizing",
                "Please select valid conductor sizes.",
                "One or more conductor sizes could not be interpreted."
              );
              return;
            }

            if (aAct <= aMin) {
              showResult(
                "upsizing",
                "No upsizing required for the EGC based on the selected sizes.",
                `The actual phase conductor (${actSize}) is not larger in cross-sectional area than the minimum (${minSize}), ` +
                  `so NEC 250.122(B) proportional increase does not apply.`
              );
              return;
            }

            const k = aAct / aMin;
            const requiredArea = aBase * k;

            // Find next conductor size with area >= requiredArea
            let newEgc = conductorSizes.find((s) => s.area >= requiredArea);
            if (!newEgc) newEgc = conductorSizes[conductorSizes.length - 1];

            const main =
              `Proportional increase factor k ≈ ${k.toFixed(2)}. ` +
              `Base EGC ${baseEgc} should be increased to at least ${newEgc.label}.`;

            const detail =
              `Minimum phase conductor: ${minSize} (${aMin.toFixed(
                2
              )} kcmil). ` +
              `Actual phase conductor: ${actSize} (${aAct.toFixed(
                2
              )} kcmil). ` +
              `Base EGC: ${baseEgc} (${aBase.toFixed(2)} kcmil). ` +
              `Required EGC area ≥ ${requiredArea.toFixed(
                2
              )} kcmil, so the next standard size is ${newEgc.label}. ` +
              `This follows the proportional increase concept of NEC 250.122(B). Final selection must be verified against the Code.`;

            showResult("upsizing", main, detail);
          });
      });

      // MathJax config
      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
