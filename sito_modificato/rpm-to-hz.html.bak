    <script>
      // Mode handling
      const modeRpmToHzBtn = document.getElementById("mode-rpm-to-hz");
      const modeHzToRpmBtn = document.getElementById("mode-hz-to-rpm");
      const inputValueEl = document.getElementById("input-value");
      const outputValueEl = document.getElementById("output-value");
      const inputUnitLabelEl = document.getElementById("input-unit-label");
      const outputUnitLabelEl = document.getElementById("output-unit-label");
      const inputErrorEl = document.getElementById("input-error");
      const relationTextEl = document.getElementById("relation-text");
      const presetButtons = document.querySelectorAll("button[data-preset]");
      let currentMode = "rpm-to-hz";

      function setMode(mode) {
        currentMode = mode;
        if (mode === "rpm-to-hz") {
          modeRpmToHzBtn.classList.add("toggle-btn-active");
          modeRpmToHzBtn.classList.remove("toggle-btn-inactive");
          modeHzToRpmBtn.classList.add("toggle-btn-inactive");
          modeHzToRpmBtn.classList.remove("toggle-btn-active");
          inputUnitLabelEl.textContent = "RPM";
          outputUnitLabelEl.textContent = "Hz";
          relationTextEl.innerHTML =
            '60 RPM is equal to <span class="font-semibold">1 Hz</span>.';
        } else {
          modeHzToRpmBtn.classList.add("toggle-btn-active");
          modeHzToRpmBtn.classList.remove("toggle-btn-inactive");
          modeRpmToHzBtn.classList.add("toggle-btn-inactive");
          modeRpmToHzBtn.classList.remove("toggle-btn-active");
          inputUnitLabelEl.textContent = "Hz";
          outputUnitLabelEl.textContent = "RPM";
          relationTextEl.innerHTML =
            '1 Hz is equal to <span class="font-semibold">60 RPM</span>.';
        }
        // Recalculate with existing input
        handleInputChange();
      }

      function formatNumber(value) {
        if (!isFinite(value)) return "";
        // Show up to 6 decimal places, trim trailing zeros
        return parseFloat(value.toFixed(6)).toString();
      }

      function handleInputChange() {
        const raw = inputValueEl.value.trim();
        if (raw === "") {
          outputValueEl.value = "";
          inputErrorEl.classList.add("hidden");
          return;
        }
        const num = Number(raw);
        if (!isFinite(num)) {
          inputErrorEl.classList.remove("hidden");
          outputValueEl.value = "";
          return;
        }
        inputErrorEl.classList.add("hidden");

        let result;
        if (currentMode === "rpm-to-hz") {
          result = num / 60;
        } else {
          result = num * 60;
        }
        outputValueEl.value = formatNumber(result);
      }

      modeRpmToHzBtn.addEventListener("click", () => setMode("rpm-to-hz"));
      modeHzToRpmBtn.addEventListener("click", () => setMode("hz-to-rpm"));
      inputValueEl.addEventListener("input", handleInputChange);

      presetButtons.forEach((btn) => {
        btn.addEventListener("click", () => {
          const val = btn.getAttribute("data-preset");
          if (currentMode === "rpm-to-hz") {
            inputValueEl.value = val;
          } else {
            // When in Hz→RPM mode, interpret presets as RPM equivalents in Hz
            // e.g. 60 RPM preset becomes 1 Hz
            const rpm = Number(val);
            const hz = rpm / 60;
            inputValueEl.value = formatNumber(hz);
          }
          handleInputChange();
        });
      });

      // Populate quick reference table
      const quickTableBody = document.getElementById("quick-table-body");
      const rpmValues = [
        60, 120, 300, 600, 900, 1200, 1500, 1800, 2400, 3000, 3600,
      ];

      function buildQuickTable() {
        quickTableBody.innerHTML = "";
        rpmValues.forEach((rpm) => {
          const hz = rpm / 60;
          const tr = document.createElement("tr");
          tr.className = "odd:bg-white even:bg-gray-50";
          tr.innerHTML = `
        <td class="px-3 py-2 border-b text-gray-800">${rpm}</td>
        <td class="px-3 py-2 border-b text-gray-800">${formatNumber(hz)}</td>
      `;
          quickTableBody.appendChild(tr);
        });
      }

      buildQuickTable();
      setMode("rpm-to-hz");
    </script>

