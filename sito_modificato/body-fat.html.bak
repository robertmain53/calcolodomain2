    <script>
      // Helper conversions
      function cmFrom(value, unit) {
        if (!value || isNaN(value)) return null;
        return unit === "in" ? value * 2.54 : value;
      }
      function kgFrom(value, unit) {
        if (!value || isNaN(value)) return null;
        return unit === "lb" ? value * 0.45359237 : value;
      }
      function clamp(num, min, max) {
        return Math.min(Math.max(num, min), max);
      }

      function calcNavyBF(sex, heightCm, neckCm, waistCm, hipCm) {
        if (!heightCm || !neckCm || !waistCm) return null;
        if (sex === "male") {
          const diff = waistCm - neckCm;
          if (diff <= 0) return null;
          const bf =
            495 /
              (1.0324 -
                0.19077 * Math.log10(diff) +
                0.15456 * Math.log10(heightCm)) -
            450;
          return bf;
        } else {
          if (!hipCm) return null;
          const sum = waistCm + hipCm - neckCm;
          if (sum <= 0) return null;
          const bf =
            495 /
              (1.29579 -
                0.35004 * Math.log10(sum) +
                0.221 * Math.log10(heightCm)) -
            450;
          return bf;
        }
      }

      function calcYMCA(sex, weightKg, waistCm) {
        if (!weightKg || !waistCm) return null;
        const w = waistCm;
        const wt = weightKg;
        if (sex === "male") {
          return 0.74 * w - 0.082 * wt - 44.74;
        } else {
          return 0.74 * w - 0.082 * wt - 34.89;
        }
      }

      function calcBMIbf(sex, age, weightKg, heightCm) {
        if (!age || !weightKg || !heightCm) return null;
        const hM = heightCm / 100;
        const bmi = weightKg / (hM * hM);
        const S = sex === "male" ? 1 : 0;
        return 1.2 * bmi + 0.23 * age - 10.8 * S - 5.4;
      }

      function calcSkinfoldBF(sex, age, skinfoldSumMm) {
        if (!age || !skinfoldSumMm) return null;
        const SF = skinfoldSumMm;
        let bf;
        if (sex === "male") {
          bf = 0.29288 * SF - 0.0005 * SF * SF + 0.15845 * age - 5.76377;
        } else {
          bf = 0.29669 * SF - 0.00043 * SF * SF + 0.02963 * age + 1.4072;
        }
        return bf;
      }

      function classifyBF(sex, bf) {
        if (bf == null || isNaN(bf)) return "Unknown";
        const v = bf;
        if (sex === "male") {
          if (v < 2) return "Below essential fat";
          if (v < 6) return "Essential fat";
          if (v < 14) return "Athlete";
          if (v < 18) return "Fitness";
          if (v < 25) return "Average";
          return "Obese";
        } else {
          if (v < 10) return "Below essential fat";
          if (v < 14) return "Essential fat";
          if (v < 21) return "Athlete";
          if (v < 25) return "Fitness";
          if (v < 32) return "Average";
          return "Obese";
        }
      }

      function updateResults() {
        const sex = document.querySelector('input[name="sex"]:checked').value;
        const age = parseFloat(document.getElementById("age").value);
        const height = parseFloat(document.getElementById("height").value);
        const heightUnit = document.getElementById("heightUnit").value;
        const weight = parseFloat(document.getElementById("weight").value);
        const weightUnit = document.getElementById("weightUnit").value;
        const waist = parseFloat(document.getElementById("waist").value);
        const hip = parseFloat(document.getElementById("hip").value);
        const neck = parseFloat(document.getElementById("neck").value);
        const wrist = parseFloat(document.getElementById("wrist").value);
        const circUnit = document.getElementById("circUnit").value;
        const skinfold = parseFloat(document.getElementById("skinfold").value);

        const hCm = cmFrom(height, heightUnit);
        const wKg = kgFrom(weight, weightUnit);
        const waistCm = cmFrom(waist, circUnit);
        const hipCm = cmFrom(hip, circUnit);
        const neckCm = cmFrom(neck, circUnit);
        const wristCm = cmFrom(wrist, circUnit);

        const navy = calcNavyBF(sex, hCm, neckCm, waistCm, hipCm);
        const ymca = calcYMCA(sex, wKg, waistCm);
        const bmiBF = calcBMIbf(sex, age, wKg, hCm);
        const skinBF = skinfold ? calcSkinfoldBF(sex, age, skinfold) : null;

        // Choose primary: prefer Navy, then skinfold, then YMCA, then BMI
        let primary = null;
        if (!isNaN(navy)) primary = navy;
        else if (!isNaN(skinBF)) primary = skinBF;
        else if (!isNaN(ymca)) primary = ymca;
        else if (!isNaN(bmiBF)) primary = bmiBF;

        const primaryEl = document.getElementById("primaryBf");
        const catEl = document.getElementById("primaryCategory");
        const barEl = document.getElementById("bfBar");
        const fatMassEl = document.getElementById("fatMass");
        const leanMassEl = document.getElementById("leanMass");

        if (primary != null && !isNaN(primary) && wKg) {
          const bfClamped = clamp(primary, 0, 70);
          primaryEl.textContent = bfClamped.toFixed(1);
          catEl.textContent = classifyBF(sex, bfClamped);
          const barWidth = clamp((bfClamped / 50) * 100, 0, 100);
          barEl.style.width = barWidth + "%";

          const fatMassKg = wKg * (bfClamped / 100);
          const leanMassKg = wKg - fatMassKg;
          fatMassEl.textContent = fatMassKg.toFixed(1);
          leanMassEl.textContent = leanMassKg.toFixed(1);
        } else {
          primaryEl.textContent = "—";
          catEl.textContent = "—";
          barEl.style.width = "0%";
          fatMassEl.textContent = "—";
          leanMassEl.textContent = "—";
        }

        document.getElementById("navyBf").textContent =
          navy != null && !isNaN(navy) ? clamp(navy, 0, 70).toFixed(1) : "—";
        document.getElementById("ymcaBf").textContent =
          ymca != null && !isNaN(ymca) ? clamp(ymca, 0, 70).toFixed(1) : "—";
        document.getElementById("bmiBf").textContent =
          bmiBF != null && !isNaN(bmiBF) ? clamp(bmiBF, 0, 70).toFixed(1) : "—";
        document.getElementById("skinfoldBf").textContent =
          skinBF != null && !isNaN(skinBF)
            ? clamp(skinBF, 0, 70).toFixed(1)
            : "—";
      }

      document
        .getElementById("calculateBtn")
        .addEventListener("click", updateResults);

      document
        .getElementById("resetBtn")
        .addEventListener("click", function () {
          document.getElementById("bodyFatForm").reset();
          document.getElementById("age").value = 30;
          document.getElementById("height").value = 175;
          document.getElementById("weight").value = 75;
          document.getElementById("waist").value = 85;
          document.getElementById("hip").value = 95;
          document.getElementById("neck").value = 38;
          document.getElementById("wrist").value = 17;
          document.getElementById("heightUnit").value = "cm";
          document.getElementById("weightUnit").value = "kg";
          document.getElementById("circUnit").value = "cm";
          document.getElementById("skinfold").value = "";
          document.getElementById("primaryBf").textContent = "—";
          document.getElementById("primaryCategory").textContent = "—";
          document.getElementById("bfBar").style.width = "0%";
          document.getElementById("fatMass").textContent = "—";
          document.getElementById("leanMass").textContent = "—";
          document.getElementById("navyBf").textContent = "—";
          document.getElementById("ymcaBf").textContent = "—";
          document.getElementById("bmiBf").textContent = "—";
          document.getElementById("skinfoldBf").textContent = "—";
        });

      // Simple FAQ toggle (in addition to global script_faq if present)
      document.querySelectorAll("#faq .faq-question").forEach((btn) => {
        btn.addEventListener("click", () => {
          const answer = btn.parentElement.querySelector(".faq-answer");
          const isHidden = answer.classList.contains("hidden");
          document
            .querySelectorAll("#faq .faq-answer")
            .forEach((a) => a.classList.add("hidden"));
          if (isHidden) {
            answer.classList.remove("hidden");
          }
        });
      });

      // Auto-calc on load with defaults
      window.addEventListener("load", updateResults);
    </script>

