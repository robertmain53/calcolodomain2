    <script>
      // Utility: format numbers
      function fmt(value, decimals = 2) {
        if (!isFinite(value)) return "–";
        return Number(value).toFixed(decimals);
      }

      function updateUnits() {
        const unitSystem = document.getElementById("unitSystem").value;
        const volumeUnit = document.getElementById("volumeUnit");
        const bagSizeUnit = document.getElementById("bagSizeUnit");
        const densityCementUnit = document.getElementById("densityCementUnit");
        const densitySandUnit = document.getElementById("densitySandUnit");
        const densityAggUnit = document.getElementById("densityAggUnit");

        if (unitSystem === "metric") {
          volumeUnit.textContent = "m³";
          bagSizeUnit.textContent = "kg";
          densityCementUnit.textContent = "kg/m³";
          densitySandUnit.textContent = "kg/m³";
          densityAggUnit.textContent = "kg/m³";
          // set typical metric densities
          if (!document.getElementById("densityCement").dataset.userSet) {
            document.getElementById("densityCement").value = 1440;
          }
          if (!document.getElementById("densitySand").dataset.userSet) {
            document.getElementById("densitySand").value = 1600;
          }
          if (!document.getElementById("densityAgg").dataset.userSet) {
            document.getElementById("densityAgg").value = 1500;
          }
        } else {
          volumeUnit.textContent = "ft³";
          bagSizeUnit.textContent = "lb";
          densityCementUnit.textContent = "lb/ft³";
          densitySandUnit.textContent = "lb/ft³";
          densityAggUnit.textContent = "lb/ft³";
          // typical imperial bulk densities
          if (!document.getElementById("densityCement").dataset.userSet) {
            document.getElementById("densityCement").value = 90; // lb/ft³
          }
          if (!document.getElementById("densitySand").dataset.userSet) {
            document.getElementById("densitySand").value = 100;
          }
          if (!document.getElementById("densityAgg").dataset.userSet) {
            document.getElementById("densityAgg").value = 94;
          }
        }
      }

      function markDensityUserSet(id) {
        document.getElementById(id).dataset.userSet = "1";
      }

      document
        .getElementById("densityCement")
        .addEventListener("input", () => markDensityUserSet("densityCement"));
      document
        .getElementById("densitySand")
        .addEventListener("input", () => markDensityUserSet("densitySand"));
      document
        .getElementById("densityAgg")
        .addEventListener("input", () => markDensityUserSet("densityAgg"));

      document
        .getElementById("unitSystem")
        .addEventListener("change", updateUnits);
      updateUnits();

      // Mode toggle
      document.getElementById("mode").addEventListener("change", function () {
        const mode = this.value;
        const volumeRow = document.getElementById("volumeRow");
        const bagsRow = document.getElementById("bagsRow");
        if (mode === "volume") {
          volumeRow.classList.remove("hidden");
          bagsRow.classList.add("hidden");
        } else {
          volumeRow.classList.add("hidden");
          bagsRow.classList.remove("hidden");
        }
      });

      // Preset buttons
      document.querySelectorAll(".preset-btn").forEach((btn) => {
        btn.addEventListener("click", () => {
          const preset = btn.getAttribute("data-preset"); // e.g. "1:2:3"
          const parts = preset.split(":").map(Number);
          if (parts.length === 3 && parts.every((p) => p > 0)) {
            document.getElementById("ratioCement").value = parts[0];
            document.getElementById("ratioSand").value = parts[1];
            document.getElementById("ratioAgg").value = parts[2];
          }
        });
      });

      // Main calculation
      document.getElementById("calcBtn").addEventListener("click", () => {
        const errorMsg = document.getElementById("errorMsg");
        errorMsg.classList.add("hidden");
        errorMsg.textContent = "";

        const mode = document.getElementById("mode").value;
        const unitSystem = document.getElementById("unitSystem").value;

        let Vtarget; // target concrete volume (m³ or ft³ depending on system)
        const waste = parseFloat(document.getElementById("waste").value) || 0;

        if (mode === "volume") {
          const V = parseFloat(document.getElementById("volume").value);
          if (!(V > 0)) {
            errorMsg.textContent = "Please enter a positive concrete volume.";
            errorMsg.classList.remove("hidden");
            return;
          }
          Vtarget = V * (1 + waste / 100);
        } else {
          const bags = parseFloat(document.getElementById("cementBags").value);
          const bagSize = parseFloat(document.getElementById("bagSize").value);
          if (!(bags > 0) || !(bagSize > 0)) {
            errorMsg.textContent =
              "Please enter a valid number of bags and bag size.";
            errorMsg.classList.remove("hidden");
            return;
          }
          const densityCement = parseFloat(
            document.getElementById("densityCement").value
          );
          if (!(densityCement > 0)) {
            errorMsg.textContent = "Please enter a valid cement bulk density.";
            errorMsg.classList.remove("hidden");
            return;
          }
          const totalCementMass = bags * bagSize; // kg or lb
          const cementVolume = totalCementMass / densityCement; // m³ or ft³
          const rc =
            parseFloat(document.getElementById("ratioCement").value) || 0;
          const rs =
            parseFloat(document.getElementById("ratioSand").value) || 0;
          const ra = parseFloat(document.getElementById("ratioAgg").value) || 0;
          const totalParts = rc + rs + ra;
          if (!(totalParts > 0) || !(rc > 0)) {
            errorMsg.textContent = "Please enter a valid mix ratio.";
            errorMsg.classList.remove("hidden");
            return;
          }
          // cementVolume = Vtarget * (rc / totalParts)
          Vtarget = cementVolume * (totalParts / rc);
          Vtarget = Vtarget * (1 + waste / 100);
        }

        const rc =
          parseFloat(document.getElementById("ratioCement").value) || 0;
        const rs = parseFloat(document.getElementById("ratioSand").value) || 0;
        const ra = parseFloat(document.getElementById("ratioAgg").value) || 0;
        const totalParts = rc + rs + ra;

        if (!(totalParts > 0) || !(rc > 0)) {
          errorMsg.textContent =
            "Please enter a valid mix ratio (all parts must be positive).";
          errorMsg.classList.remove("hidden");
          return;
        }

        const densityCement = parseFloat(
          document.getElementById("densityCement").value
        );
        const densitySand = parseFloat(
          document.getElementById("densitySand").value
        );
        const densityAgg = parseFloat(
          document.getElementById("densityAgg").value
        );
        const wcr = parseFloat(document.getElementById("wcr").value);

        if (
          ![densityCement, densitySand, densityAgg].every((d) => d > 0) ||
          !(wcr > 0)
        ) {
          errorMsg.textContent =
            "Please enter valid densities and water–cement ratio.";
          errorMsg.classList.remove("hidden");
          return;
        }

        // Component volumes
        const Vcement = Vtarget * (rc / totalParts);
        const Vsand = Vtarget * (rs / totalParts);
        const Vagg = Vtarget * (ra / totalParts);

        // Masses
        const Mcement = Vcement * densityCement;
        const Msand = Vsand * densitySand;
        const Magg = Vagg * densityAgg;

        const Mwater = Mcement * wcr;

        // Bags
        const bagSize =
          parseFloat(document.getElementById("bagSize").value) ||
          (unitSystem === "metric" ? 25 : 94);
        const bagsNeeded = Math.ceil(Mcement / bagSize);

        // Convert to display units
        const outTotalVolume = document.getElementById("outTotalVolume");
        const outTotalVolumeAlt = document.getElementById("outTotalVolumeAlt");
        const outCementBags = document.getElementById("outCementBags");
        const outCementMass = document.getElementById("outCementMass");
        const tbody = document.getElementById("resultTableBody");
        const outNotes = document.getElementById("outNotes");

        tbody.innerHTML = "";

        if (unitSystem === "metric") {
          outTotalVolume.textContent = fmt(Vtarget, 3) + " m³ (incl. waste)";
          const Vft3 = Vtarget * 35.3147;
          outTotalVolumeAlt.textContent =
            fmt(Vft3, 1) + " ft³ of concrete (approx.)";
          outCementBags.textContent =
            bagsNeeded + " bags of " + fmt(bagSize, 0) + " kg cement";
          outCementMass.textContent = fmt(Mcement, 1) + " kg cement total";

          const litersWater = Mwater; // 1 kg ≈ 1 L
          const gallonsWater = litersWater * 0.264172;

          const rows = [
            {
              name: "Cement",
              V: Vcement,
              M: Mcement,
              guide: bagsNeeded + " bags (" + fmt(bagSize, 0) + " kg)",
            },
            {
              name: "Sand",
              V: Vsand,
              M: Msand,
              guide: fmt(Vsand * 1000, 0) + " L (loose)",
            },
            {
              name: "Coarse aggregate",
              V: Vagg,
              M: Magg,
              guide: fmt(Vagg * 1000, 0) + " L (loose)",
            },
            {
              name: "Water",
              V: litersWater / 1000,
              M: Mwater,
              guide:
                fmt(litersWater, 0) +
                " L (" +
                fmt(gallonsWater, 1) +
                " US gal)",
            },
          ];

          rows.forEach((r) => {
            const tr = document.createElement("tr");
            tr.innerHTML = `
          <td class="px-3 py-2 text-gray-800">${r.name}</td>
          <td class="px-3 py-2 text-right text-gray-700">${
            r.name === "Water" ? fmt(r.V, 3) + " m³ eq." : fmt(r.V, 3) + " m³"
          }</td>
          <td class="px-3 py-2 text-right text-gray-700">${fmt(r.M, 1)} kg</td>
          <td class="px-3 py-2 text-right text-gray-600 text-xs">${r.guide}</td>
        `;
            tbody.appendChild(tr);
          });

          outNotes.textContent =
            "Volumes are loose material volumes. Finished compacted concrete volume is " +
            fmt(Vtarget / (1 + waste / 100), 3) +
            " m³ before waste allowance.";
        } else {
          // Imperial
          outTotalVolume.textContent = fmt(Vtarget, 1) + " ft³ (incl. waste)";
          const Vyd3 = Vtarget / 27;
          outTotalVolumeAlt.textContent =
            fmt(Vyd3, 2) + " yd³ of concrete (approx.)";
          outCementBags.textContent =
            bagsNeeded + " bags of " + fmt(bagSize, 0) + " lb cement";
          outCementMass.textContent = fmt(Mcement, 1) + " lb cement total";

          const lbPerGal = 8.34;
          const gallonsWater = Mwater / lbPerGal;
          const litersWater = gallonsWater * 3.78541;

          const rows = [
            {
              name: "Cement",
              V: Vcement,
              M: Mcement,
              guide: bagsNeeded + " bags (" + fmt(bagSize, 0) + " lb)",
            },
            {
              name: "Sand",
              V: Vsand,
              M: Msand,
              guide: fmt(Vsand, 1) + " ft³ (loose)",
            },
            {
              name: "Coarse aggregate",
              V: Vagg,
              M: Magg,
              guide: fmt(Vagg, 1) + " ft³ (loose)",
            },
            {
              name: "Water",
              V: gallonsWater / 7.48052,
              M: Mwater,
              guide:
                fmt(gallonsWater, 1) +
                " US gal (" +
                fmt(litersWater, 0) +
                " L)",
            },
          ];

          rows.forEach((r) => {
            const tr = document.createElement("tr");
            tr.innerHTML = `
          <td class="px-3 py-2 text-gray-800">${r.name}</td>
          <td class="px-3 py-2 text-right text-gray-700">${
            r.name === "Water" ? fmt(r.V, 2) + " ft³ eq." : fmt(r.V, 2) + " ft³"
          }</td>
          <td class="px-3 py-2 text-right text-gray-700">${fmt(r.M, 1)} lb</td>
          <td class="px-3 py-2 text-right text-gray-600 text-xs">${r.guide}</td>
        `;
            tbody.appendChild(tr);
          });

          outNotes.textContent =
            "Volumes are loose material volumes. Finished compacted concrete volume is " +
            fmt(Vtarget / (1 + waste / 100), 1) +
            " ft³ before waste allowance.";
        }

        document.getElementById("resultsSection").classList.remove("hidden");
      });

      // Reset
      document.getElementById("resetBtn").addEventListener("click", () => {
        document.getElementById("mode").value = "volume";
        document.getElementById("unitSystem").value = "metric";
        document.getElementById("volume").value = 1;
        document.getElementById("cementBags").value = 4;
        document.getElementById("bagSize").value = 25;
        document.getElementById("ratioCement").value = 1;
        document.getElementById("ratioSand").value = 2;
        document.getElementById("ratioAgg").value = 3;
        document.getElementById("wcr").value = 0.5;
        document.getElementById("waste").value = 5;
        delete document.getElementById("densityCement").dataset.userSet;
        delete document.getElementById("densitySand").dataset.userSet;
        delete document.getElementById("densityAgg").dataset.userSet;
        updateUnits();
        document.getElementById("resultsSection").classList.add("hidden");
        const errorMsg = document.getElementById("errorMsg");
        errorMsg.classList.add("hidden");
        errorMsg.textContent = "";
        document.getElementById("volumeRow").classList.remove("hidden");
        document.getElementById("bagsRow").classList.add("hidden");
      });

      // Initialize
      document.addEventListener("DOMContentLoaded", () => {
        updateUnits();
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
