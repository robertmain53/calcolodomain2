              "bg-blue-50",
              "text-blue-700",
              "border",
              "border-blue-200"
            );
            panels[key].classList.add("hidden");
          }
        });
      }

      tabs.basic.addEventListener("click", () => setActiveTab("basic"));
      tabs.advanced.addEventListener("click", () => setActiveTab("advanced"));
      tabs.compare.addEventListener("click", () => setActiveTab("compare"));

      // Utility formatters
      function fmtMoney(v) {
        if (!isFinite(v)) return "$0";
        return "$" + v.toFixed(0).replace(/\B(?=(\d{3})+(?!\d))/g, ",");
      }
      function fmtMoney2(v) {
        if (!isFinite(v)) return "$0.00";
        return "$" + v.toFixed(2).replace(/\B(?=(\d{3})+(?!\d))/g, ",");
      }

      function getNumber(id) {
        const el = document.getElementById(id);
        const val = parseFloat(el.value.replace?.(/,/g, "") || el.value);
        return isNaN(val) ? 0 : val;
      }

      function calcLease() {
        const errorEl = document.getElementById("error-msg");
        errorEl.classList.add("hidden");
        errorEl.textContent = "";

        const msrp = getNumber("msrp");
        const sellPrice = getNumber("sell-price");
        const term = getNumber("lease-term");
        const residualPct = getNumber("residual-percent");
        let apr = getNumber("apr");
        const down = getNumber("down-payment");
        const tradeIn = getNumber("trade-in");
        const rebates = getNumber("rebates");

        const acqFee = getNumber("acq-fee");
        const docFee = getNumber("doc-fee");
        const otherFees = getNumber("other-fees");
        const taxRate = getNumber("tax-rate") / 100;
        const taxMethod = document.getElementById("tax-method").value;
        const milesPerYear = getNumber("miles-per-year");
        const dispositionFee = getNumber("disposition-fee");
        const securityDeposit = getNumber("security-deposit");

        const loanTerm = getNumber("loan-term");
        const loanApr = getNumber("loan-apr") / 100;
        const ownershipYears = getNumber("ownership-years");

        if (msrp <= 0 || sellPrice <= 0 || term <= 0) {
          errorEl.textContent =
            "Please enter a valid MSRP, negotiated price, and lease term.";
          errorEl.classList.remove("hidden");
          return;
        }

        // Residual and cap cost
        const residual = msrp * (residualPct / 100);
        const capFees = acqFee; // assume only acquisition fee is capitalized by default
        const grossCap = sellPrice + capFees;
        const capReductions = down + tradeIn + rebates;
        const adjCap = Math.max(grossCap - capReductions, 0);

        // Money factor from APR
        let moneyFactor = apr / 2400;
        if (moneyFactor < 0) moneyFactor = 0;

        // Depreciation and finance
        const totalDepreciation = Math.max(adjCap - residual, 0);
        const monthlyDepreciation = totalDepreciation / term;
        const monthlyFinance = (adjCap + residual) * moneyFactor;
        const basePayment = monthlyDepreciation + monthlyFinance;

        // Taxes
        let monthlyTax = 0;
        let upfrontTax = 0;

        if (taxRate > 0) {
          if (taxMethod === "payment") {
            monthlyTax = basePayment * taxRate;
          } else if (taxMethod === "cap-cost") {
            upfrontTax = sellPrice * taxRate;
          } else if (taxMethod === "depreciation") {
            const depPortion = totalDepreciation / term;
            monthlyTax = depPortion * taxRate;
          }
        }

        const monthlyPayment = basePayment + monthlyTax;

        // Due at signing: down + trade-in is already applied to cap cost, but still cash outlay.
        const upfrontFees = docFee + otherFees + acqFee;
        const dueAtSigning =
          down +
          docFee +
          otherFees +
          (taxMethod === "cap-cost" ? upfrontTax : 0);

        // Totals over lease term
        const totalMonthly = monthlyPayment * term;
        const totalTaxes = monthlyTax * term + upfrontTax;
        const totalLeaseCost = totalMonthly + dueAtSigning + dispositionFee; // exclude refundable security deposit

        const effMonthly = totalLeaseCost / term;
        const totalMiles = milesPerYear * (term / 12);
        const effPerMile = totalMiles > 0 ? totalLeaseCost / totalMiles : 0;

        // Lease vs buy comparison (simplified)
        // Loan principal: negotiated price + doc/other fees + tax on purchase
        const purchaseTax = sellPrice * taxRate;
        const loanPrincipal =
          sellPrice +
          docFee +
          otherFees +
          purchaseTax -
          down -
          tradeIn -
          rebates;
        let loanPayment = 0;
        if (loanTerm > 0 && loanPrincipal > 0) {
          if (loanApr === 0) {
            loanPayment = loanPrincipal / loanTerm;
          } else {
            const r = loanApr / 12;
            loanPayment =
              (loanPrincipal * r) / (1 - Math.pow(1 + r, -loanTerm));
          }
        }

        const monthsOwned = ownershipYears * 12;
        const monthsConsidered = Math.min(monthsOwned, loanTerm);
        const totalLoanPaid =
          loanPayment * monthsConsidered +
          down +
          docFee +
          otherFees +
          purchaseTax;
        const valueAtEnd = msrp * (residualPct / 100) * (monthsOwned / term); // rough: scale residual with time
        const netBuyCost = Math.max(totalLoanPaid - valueAtEnd, 0);
        const buyEffMonthly = monthsOwned > 0 ? netBuyCost / monthsOwned : 0;
        const leaseEffMonthly = effMonthly;

        let recommend = "Depends on assumptions";
        if (leaseEffMonthly && buyEffMonthly) {
          if (leaseEffMonthly < buyEffMonthly * 0.97) {
            recommend = "Leasing (lower effective monthly cost)";
          } else if (buyEffMonthly < leaseEffMonthly * 0.97) {
            recommend = "Buying (loan) is more cost‑effective";
          } else {
            recommend = "Roughly similar cost";
          }
        }

        // Update UI
        document.getElementById("result-monthly").textContent =
          fmtMoney2(monthlyPayment);
        document.getElementById("result-due-signing").textContent =
          fmtMoney2(dueAtSigning);
        document.getElementById("result-mf").textContent =
          "MF: " + moneyFactor.toFixed(5);
        document.getElementById("result-apr").textContent =
          "APR: " + (moneyFactor * 2400).toFixed(2) + "%";

        document.getElementById("result-adj-cap").textContent =
          fmtMoney2(adjCap);
        document.getElementById("result-residual").textContent =
          fmtMoney2(residual);
        document.getElementById("result-depr").textContent =
          fmtMoney2(totalDepreciation);
        document.getElementById("result-finance").textContent = fmtMoney2(
          monthlyFinance * term
        );
        document.getElementById("result-taxes").textContent =
          fmtMoney2(totalTaxes);
        document.getElementById("result-total-lease").textContent =
          fmtMoney2(totalLeaseCost);
        document.getElementById("result-eff-month").textContent =
          fmtMoney2(effMonthly);
        document.getElementById("result-eff-mile").textContent =
          effPerMile > 0 ? "$" + effPerMile.toFixed(3) : "$0.000";

        document.getElementById("result-lease-eff").textContent =
          fmtMoney2(leaseEffMonthly);
        document.getElementById("result-loan-pay").textContent =
          fmtMoney2(loanPayment);
        document.getElementById("result-buy-eff").textContent =
          fmtMoney2(buyEffMonthly);
        document.getElementById("result-recommend").textContent = recommend;
      }

      document.getElementById("calc-btn").addEventListener("click", calcLease);

      document.getElementById("reset-btn").addEventListener("click", () => {
        document.getElementById("msrp").value = 40000;
        document.getElementById("sell-price").value = 38000;
        document.getElementById("lease-term").value = 36;
        document.getElementById("residual-percent").value = 58;
        document.getElementById("apr").value = 4.0;
        document.getElementById("down-payment").value = 2000;
        document.getElementById("trade-in").value = 0;
        document.getElementById("rebates").value = 1000;
        document.getElementById("acq-fee").value = 795;
        document.getElementById("doc-fee").value = 300;
        document.getElementById("other-fees").value = 0;
        document.getElementById("tax-rate").value = 7.0;
        document.getElementById("tax-method").value = "payment";
        document.getElementById("miles-per-year").value = 12000;
        document.getElementById("disposition-fee").value = 395;
        document.getElementById("security-deposit").value = 0;
        document.getElementById("loan-term").value = 72;
        document.getElementById("loan-apr").value = 5.0;
        document.getElementById("ownership-years").value = 6;
        calcLease();
      });

      // Auto-calc on load
      window.addEventListener("DOMContentLoaded", calcLease);
    </script>

