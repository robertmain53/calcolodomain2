    <script>
      // Tab handling
      document.querySelectorAll(".tab-btn").forEach((btn) => {
        btn.addEventListener("click", () => {
          const target = btn.getAttribute("data-tab");
          document
            .querySelectorAll(".tab-btn")
            .forEach((b) => b.classList.remove("tab-active"));
          btn.classList.add("tab-active");
          document
            .querySelectorAll(".tab-panel")
            .forEach((p) => p.classList.add("hidden"));
          document.getElementById("tab-" + target).classList.remove("hidden");
        });
      });

      function toCm(value, unit) {
        if (!isFinite(value)) return NaN;
        return unit === "in" ? value * 2.54 : value;
      }
      function fromCm(valueCm, unit) {
        if (!isFinite(valueCm)) return NaN;
        return unit === "in" ? valueCm / 2.54 : valueCm;
      }
      function toMeters(value, unit) {
        if (!isFinite(value)) return NaN;
        return unit === "yd" ? value * 0.9144 : value;
      }
      function fromMeters(valueM, unit) {
        if (!isFinite(valueM)) return NaN;
        return unit === "yd" ? valueM / 0.9144 : valueM;
      }
      function toGrams(value, unit) {
        if (!isFinite(value)) return NaN;
        return unit === "oz" ? value * 28.349523125 : value;
      }
      function fromGrams(valueG, unit) {
        if (!isFinite(valueG)) return NaN;
        return unit === "oz" ? valueG / 28.349523125 : valueG;
      }

      function getGauge() {
        const gw = parseFloat(document.getElementById("gaugeWidth").value);
        const gwUnit = document.getElementById("gaugeWidthUnit").value;
        const gh = parseFloat(document.getElementById("gaugeHeight").value);
        const ghUnit = document.getElementById("gaugeHeightUnit").value;
        const sts = parseFloat(
          document.getElementById("stitchesInWidth").value
        );
        const rows = parseFloat(document.getElementById("rowsInHeight").value);

        if (!(gw > 0 && gh > 0 && sts > 0 && rows > 0)) return null;

        const gwCm = toCm(gw, gwUnit);
        const ghCm = toCm(gh, ghUnit);
        const stsPerCm = sts / gwCm;
        const rowsPerCm = rows / ghCm;
        return { stsPerCm, rowsPerCm, gwCm, ghCm, gwUnit, ghUnit };
      }

      function getYarnInfo() {
        const ballLen = parseFloat(document.getElementById("ballLength").value);
        const ballLenUnit = document.getElementById("ballLengthUnit").value;
        const ballWt = parseFloat(document.getElementById("ballWeight").value);
        const ballWtUnit = document.getElementById("ballWeightUnit").value;
        const swWt = parseFloat(document.getElementById("swatchWeight").value);
        const swWtUnit = document.getElementById("swatchWeightUnit").value;

        const hasBall = ballLen > 0 && ballWt > 0;
        const hasSwatch = swWt > 0;

        return {
          hasBall,
          hasSwatch,
          ballLenM: hasBall ? toMeters(ballLen, ballLenUnit) : NaN,
          ballWtG: hasBall ? toGrams(ballWt, ballWtUnit) : NaN,
          swWtG: hasSwatch ? toGrams(swWt, swWtUnit) : NaN,
        };
      }

      // Size & yardage
      document.getElementById("btnCalcSize").addEventListener("click", () => {
        const gauge = getGauge();
        const err = document.getElementById("sizeError");
        const resBox = document.getElementById("sizeResults");
        if (!gauge) {
          err.classList.remove("hidden");
          resBox.classList.add("hidden");
          return;
        }
        err.classList.add("hidden");

        const pw = parseFloat(document.getElementById("projectWidth").value);
        const pwUnit = document.getElementById("projectWidthUnit").value;
        const ph = parseFloat(document.getElementById("projectHeight").value);
        const phUnit = document.getElementById("projectHeightUnit").value;
        if (!(pw > 0 && ph > 0)) {
          err.classList.remove("hidden");
          resBox.classList.add("hidden");
          return;
        }

        const widthCm = toCm(pw, pwUnit);
        const heightCm = toCm(ph, phUnit);
        const stsPerCm = gauge.stsPerCm;
        const rowsPerCm = gauge.rowsPerCm;

        const stsPerUnit = pwUnit === "in" ? stsPerCm * 2.54 : stsPerCm;
        const rowsPerUnit = phUnit === "in" ? rowsPerCm * 2.54 : rowsPerCm;

        const totalSts = Math.round(stsPerCm * widthCm * rowsPerCm * heightCm);
        const totalRows = Math.round(rowsPerCm * heightCm);

        document.getElementById("resStsPerUnit").textContent =
          stsPerUnit.toFixed(2) + " sts per " + pwUnit;
        document.getElementById("resRowsPerUnit").textContent =
          rowsPerUnit.toFixed(2) + " rows per " + phUnit;
        document.getElementById("resTotalStitches").textContent =
          totalSts.toLocaleString();
        document.getElementById("resTotalRows").textContent =
          totalRows.toLocaleString();

        const yarn = getYarnInfo();
        let yarnLenText = "Enter yarn and swatch weight for yardage estimate.";
        let ballsText = "—";

        const swatchArea = gauge.gwCm * gauge.ghCm;
        const projArea = widthCm * heightCm;

        if (yarn.hasSwatch && yarn.hasBall) {
          const lenPerG = yarn.ballLenM / yarn.ballWtG; // m per g
          const swLenM = yarn.swWtG * lenPerG;
          const lenPerCm2 = swLenM / swatchArea;
          const projLenM = lenPerCm2 * projArea;
          const projLenYd = fromMeters(projLenM, "yd");
          const balls = projLenM / yarn.ballLenM;

          yarnLenText =
            projLenM.toFixed(1) +
            " m (" +
            projLenYd.toFixed(1) +
            " yd) approx.";
          ballsText =
            balls.toFixed(2) + " balls (round up and add extra for safety)";
        } else if (yarn.hasBall) {
          // Rough estimate: assume yarn usage proportional to area and that swatch uses 1/10 of a ball for 10x10cm if no swatch weight
          const assumedSwLenM = yarn.ballLenM / 10;
          const lenPerCm2 = assumedSwLenM / swatchArea;
          const projLenM = lenPerCm2 * projArea;
          const projLenYd = fromMeters(projLenM, "yd");
          const balls = projLenM / yarn.ballLenM;

          yarnLenText =
            projLenM.toFixed(1) +
            " m (" +
            projLenYd.toFixed(1) +
            " yd) approx. (rough estimate)";
          ballsText = balls.toFixed(2) + " balls (rough estimate)";
        }

        document.getElementById("resYarnLength").textContent = yarnLenText;
        document.getElementById("resBalls").textContent = ballsText;

        resBox.classList.remove("hidden");
      });

      // Starting chain
      document.getElementById("btnCalcChain").addEventListener("click", () => {
        const gauge = getGauge();
        const err = document.getElementById("chainError");
        const resBox = document.getElementById("chainResults");
        if (!gauge) {
          err.classList.remove("hidden");
          resBox.classList.add("hidden");
          return;
        }
        err.classList.add("hidden");

        const width = parseFloat(document.getElementById("chainWidth").value);
        const widthUnit = document.getElementById("chainWidthUnit").value;
        if (!(width > 0)) {
          err.classList.remove("hidden");
          resBox.classList.add("hidden");
          return;
        }

        const targetWidthCm = toCm(width, widthUnit);
        const stsPerCm = gauge.stsPerCm;
        let idealChains = targetWidthCm * stsPerCm;
        if (!isFinite(idealChains) || idealChains <= 0) {
          err.classList.remove("hidden");
          resBox.classList.add("hidden");
          return;
        }

        const multiple = parseInt(
          document.getElementById("stitchMultiple").value,
          10
        );
        const extra =
          parseInt(document.getElementById("extraStitches").value, 10) || 0;

        let finalChains = Math.round(idealChains);

        if (multiple && multiple > 0) {
          // Find nearest chain count that satisfies multiple * k + extra
          const kApprox = (idealChains - extra) / multiple;
          let k = Math.max(1, Math.round(kApprox));
          finalChains = multiple * k + extra;
        }

        const effectiveWidthCm = finalChains / stsPerCm;
        const effectiveWidth = fromCm(effectiveWidthCm, widthUnit);

        document.getElementById("resChainCount").textContent =
          finalChains + " ch";
        let detail =
          "This starting chain gives an effective width of approximately " +
          effectiveWidth.toFixed(1) +
          " " +
          widthUnit +
          " with your gauge.";
        if (multiple && multiple > 0) {
          detail +=
            " It matches the pattern requirement of " +
            multiple +
            "× repeats + " +
            extra +
            " extra stitches.";
        }
        document.getElementById("resChainDetails").textContent = detail;

        resBox.classList.remove("hidden");
      });

      // Rows -> length
      document
        .getElementById("btnRowsToLength")
        .addEventListener("click", () => {
          const gauge = getGauge();
          const err = document.getElementById("rowsToLengthError");
          const res = document.getElementById("rowsToLengthResult");
          if (!gauge) {
            err.classList.remove("hidden");
            res.classList.add("hidden");
            return;
          }
          const rows = parseInt(
            document.getElementById("rowsToLength").value,
            10
          );
          if (!(rows > 0)) {
            err.classList.remove("hidden");
            res.classList.add("hidden");
            return;
          }
          err.classList.add("hidden");

          const lengthCm = rows / gauge.rowsPerCm;
          const lengthIn = fromCm(lengthCm, "in");
          res.textContent =
            rows +
            " rows ≈ " +
            lengthCm.toFixed(1) +
            " cm (" +
            lengthIn.toFixed(1) +
            " in)";
          res.classList.remove("hidden");
        });

      // Length -> rows
      document
        .getElementById("btnLengthToRows")
        .addEventListener("click", () => {
          const gauge = getGauge();
          const err = document.getElementById("lengthToRowsError");
          const res = document.getElementById("lengthToRowsResult");
          if (!gauge) {
            err.classList.remove("hidden");
            res.classList.add("hidden");
            return;
          }
          const len = parseFloat(document.getElementById("lengthToRows").value);
          const unit = document.getElementById("lengthToRowsUnit").value;
          if (!(len > 0)) {
            err.classList.remove("hidden");
            res.classList.add("hidden");
            return;
          }
          err.classList.add("hidden");

          const lenCm = toCm(len, unit);
          const rows = Math.round(lenCm * gauge.rowsPerCm);
          res.textContent = len + " " + unit + " ≈ " + rows + " rows";
          res.classList.remove("hidden");
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
