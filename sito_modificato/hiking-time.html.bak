    <script>
      // Utility functions
      function toMeters(value, unit) {
        if (!value || value <= 0) return 0;
        return unit === "ft" ? value * 0.3048 : value;
      }

      function toKilometers(value, unit) {
        if (!value || value <= 0) return 0;
        return unit === "mi" ? value * 1.609344 : value;
      }

      function paceToKph(pace, unit) {
        if (!pace || pace <= 0) return 0;
        return unit === "mph" ? pace * 1.609344 : pace;
      }

      function formatHours(hours) {
        if (!isFinite(hours) || hours <= 0) return "0 min";
        const totalMinutes = Math.round(hours * 60);
        const h = Math.floor(totalMinutes / 60);
        const m = totalMinutes % 60;
        if (h === 0) return m + " min";
        if (m === 0) return h + (h === 1 ? " hr" : " hrs");
        return h + (h === 1 ? " hr " : " hrs ") + m + " min";
      }

      function formatPace(distanceKm, hours) {
        if (!distanceKm || !hours || hours <= 0) return "—";
        const kph = distanceKm / hours;
        const mph = kph / 1.609344;
        return kph.toFixed(1) + " km/h · " + mph.toFixed(1) + " mph";
      }

      function getTerrainFactor(terrain) {
        switch (terrain) {
          case "easy":
            return 0.95;
          case "moderate":
            return 1.0;
          case "rough":
            return 1.15;
          case "offtrail":
            return 1.3;
          default:
            return 1.0;
        }
      }

      function getDescentFactor(terrain) {
        // Multiplier on flat time for descent portion
        switch (terrain) {
          case "easy":
            return 0.8; // faster than flat
          case "moderate":
            return 1.0; // similar to flat
          case "rough":
            return 1.2; // slower
          case "offtrail":
            return 1.4; // much slower
          default:
            return 1.0;
        }
      }

      function calculateModernHybrid(params) {
        const {
          distanceKm,
          gainM,
          lossM,
          baseKph,
          terrain,
          fitnessFactor,
          breaksMin,
          safetyPct,
        } = params;

        if (distanceKm <= 0 || baseKph <= 0) {
          return null;
        }

        const terrainFactor = getTerrainFactor(terrain);
        const descentFactor = getDescentFactor(terrain);

        // Base flat time adjusted for terrain
        const flatTime = distanceKm / (baseKph * (1 / terrainFactor)); // hours

        // Uphill time via Naismith
        const uphillTime = gainM > 0 ? gainM / 600 : 0; // hours

        // Approximate descent distance as proportion of total distance
        // If both gain and loss are given, use their ratio to split distance
        let descentDistanceKm = 0;
        if (lossM > 0 && gainM + lossM > 0) {
          const descentRatio = lossM / (gainM + lossM);
          descentDistanceKm = distanceKm * descentRatio;
        } else if (lossM > 0) {
          descentDistanceKm = distanceKm * 0.5;
        }

        const flatDistanceKm = Math.max(distanceKm - descentDistanceKm, 0);
        const flatTimeAdj = flatDistanceKm / (baseKph * (1 / terrainFactor));
        const descentTime =
          (descentDistanceKm / (baseKph * (1 / terrainFactor))) * descentFactor;

        // Replace flatTime with split-based time for better modeling
        const movingTimeBase = flatTimeAdj + uphillTime + descentTime;

        // Apply fitness factor
        const movingTimeFitness = movingTimeBase * fitnessFactor;

        // Apply safety margin on moving time only
        const movingWithMargin = movingTimeFitness * (1 + safetyPct / 100);

        const breaksHours = (breaksMin || 0) / 60;
        const totalTime = movingWithMargin + breaksHours;

        return {
          movingTime: movingTimeFitness,
          movingWithMargin,
          breaksHours,
          totalTime,
        };
      }

      function calculateNaismith(distanceKm, gainM) {
        if (distanceKm <= 0) return null;
        const time = distanceKm / 5 + (gainM > 0 ? gainM / 600 : 0);
        return time;
      }

      function calculateBookTime(distanceKm) {
        if (distanceKm <= 0) return null;
        return distanceKm / 3; // 3 km/h
      }

      document.addEventListener("DOMContentLoaded", function () {
        const distanceEl = document.getElementById("distance");
        const distanceUnitEl = document.getElementById("distance-unit");
        const elevGainEl = document.getElementById("elev-gain");
        const elevLossEl = document.getElementById("elev-loss");
        const elevUnitEl = document.getElementById("elev-unit");
        const elevLossUnitLabel = document.getElementById(
          "elev-loss-unit-label"
        );
        const terrainEl = document.getElementById("terrain");
        const paceEl = document.getElementById("pace");
        const paceUnitEl = document.getElementById("pace-unit");
        const fitnessEl = document.getElementById("fitness");
        const breaksEl = document.getElementById("breaks");
        const safetyEl = document.getElementById("safety");
        const safetyLabel = document.getElementById("safety-label");
        const methodRadios = document.querySelectorAll('input[name="method"]');

        const totalTimeEl = document.getElementById("total-time");
        const movingTimeEl = document.getElementById("moving-time");
        const breaksTimeEl = document.getElementById("breaks-time");
        const avgPaceEl = document.getElementById("avg-pace");
        const methodLabelEl = document.getElementById("method-label");
        const timeWindowEl = document.getElementById("time-window");

        const calcBtn = document.getElementById("calculate-btn");
        const resetBtn = document.getElementById("reset-btn");

        // Sync elevation loss unit label
        function syncElevLossUnit() {
          elevLossUnitLabel.textContent = elevUnitEl.value;
        }
        elevUnitEl.addEventListener("change", syncElevLossUnit);
        syncElevLossUnit();

        // Safety label
        safetyEl.addEventListener("input", () => {
          safetyLabel.textContent = "+" + safetyEl.value + "%";
        });

        function getSelectedMethod() {
          let val = "modern";
          methodRadios.forEach((r) => {
            if (r.checked) val = r.value;
          });
          return val;
        }

        function updateMethodLabel(method) {
          if (method === "naismith") {
            methodLabelEl.textContent = "Classic Naismith’s rule";
          } else if (method === "booktime") {
            methodLabelEl.textContent = "“Book time” (distance-only)";
          } else {
            methodLabelEl.textContent = "Modern hybrid";
          }
        }

        function calculate() {
          const distance = parseFloat(distanceEl.value);
          const distanceUnit = distanceUnitEl.value;
          const gain = parseFloat(elevGainEl.value);
          const loss = parseFloat(elevLossEl.value);
          const elevUnit = elevUnitEl.value;
          const terrain = terrainEl.value;
          const pace = parseFloat(paceEl.value);
          const paceUnit = paceUnitEl.value;
          const fitnessFactor = parseFloat(fitnessEl.value);
          const breaksMin = parseFloat(breaksEl.value) || 0;
          const safetyPct = parseFloat(safetyEl.value) || 0;
          const method = getSelectedMethod();

          const distanceKm = toKilometers(distance, distanceUnit);
          const gainM = toMeters(gain, elevUnit);
          const lossM = toMeters(loss, elevUnit);
          const baseKph = paceToKph(pace, paceUnit);

          if (!distanceKm || !baseKph) {
            totalTimeEl.textContent = "—";
            movingTimeEl.textContent = "—";
            breaksTimeEl.textContent = "—";
            avgPaceEl.textContent = "—";
            timeWindowEl.textContent =
              "Please enter a valid distance and base pace.";
            return;
          }

          let movingTime, totalTime, breaksHours, movingWithMargin;

          if (method === "naismith") {
            const t = calculateNaismith(distanceKm, gainM);
            if (t == null) return;
            movingTime = t;
            breaksHours = breaksMin / 60;
            movingWithMargin = movingTime;
            totalTime = movingTime + breaksHours;
          } else if (method === "booktime") {
            const t = calculateBookTime(distanceKm);
            if (t == null) return;
            movingTime = t;
            breaksHours = breaksMin / 60;
            movingWithMargin = movingTime;
            totalTime = movingTime + breaksHours;
          } else {
            const res = calculateModernHybrid({
              distanceKm,
              gainM,
              lossM,
              baseKph,
              terrain,
              fitnessFactor,
              breaksMin,
              safetyPct,
            });
            if (!res) return;
            movingTime = res.movingTime;
            movingWithMargin = res.movingWithMargin;
            breaksHours = res.breaksHours;
            totalTime = res.totalTime;
          }

          totalTimeEl.textContent = formatHours(totalTime);
          movingTimeEl.textContent = formatHours(movingTime);
          breaksTimeEl.textContent = formatHours(
            totalTime - movingWithMargin + breaksHours
          );
          avgPaceEl.textContent = formatPace(distanceKm, totalTime);
          updateMethodLabel(method);

          // Provide a small time window (±10%) for realism
          const low = totalTime * 0.9;
          const high = totalTime * 1.1;
          timeWindowEl.textContent =
            "Likely range: " +
            formatHours(low) +
            " – " +
            formatHours(high) +
            " depending on conditions.";
        }

        calcBtn.addEventListener("click", calculate);

        resetBtn.addEventListener("click", () => {
          distanceEl.value = 10;
          distanceUnitEl.value = "km";
          elevGainEl.value = 600;
          elevLossEl.value = 600;
          elevUnitEl.value = "m";
          syncElevLossUnit();
          terrainEl.value = "moderate";
          paceEl.value = 4;
          paceUnitEl.value = "kph";
          fitnessEl.value = "1";
          breaksEl.value = 30;
          safetyEl.value = 10;
          safetyLabel.textContent = "+10%";
          methodRadios.forEach((r) => {
            r.checked = r.value === "modern";
          });
          totalTimeEl.textContent = "—";
          movingTimeEl.textContent = "—";
          breaksTimeEl.textContent = "—";
          avgPaceEl.textContent = "—";
          methodLabelEl.textContent = "Modern hybrid";
          timeWindowEl.textContent =
            "Enter your hike details and click “Calculate”.";
        });

        // Initial calculation with defaults
        calculate();
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
