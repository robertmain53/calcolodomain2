    <script>
      (function () {
        const m3Input = document.getElementById("value-m3s");
        const lInput = document.getElementById("value-ls");
        const swapBtn = document.getElementById("swap-direction");
        const clearBtn = document.getElementById("btn-clear");

        // direction: true = m3/s -> L/s (default), false = L/s -> m3/s
        let m3ToL = true;

        function formatNumber(value) {
          if (!isFinite(value)) return "";
          const abs = Math.abs(value);
          if (abs !== 0 && (abs < 0.001 || abs >= 1e6)) {
            return value
              .toExponential(6)
              .replace(/0+e/, "e")
              .replace(/\.e/, "e");
          }
          return Number(value.toFixed(6)).toString();
        }

        function convertFromM3() {
          const v = parseFloat(m3Input.value);
          if (isNaN(v)) {
            lInput.value = "";
            return;
          }
          const result = v * 1000;
          lInput.value = formatNumber(result);
        }

        function convertFromL() {
          const v = parseFloat(lInput.value);
          if (isNaN(v)) {
            m3Input.value = "";
            return;
          }
          const result = v / 1000;
          m3Input.value = formatNumber(result);
        }

        m3Input.addEventListener("input", function () {
          if (m3ToL) convertFromM3();
        });

        lInput.addEventListener("input", function () {
          if (!m3ToL) convertFromL();
        });

        swapBtn.addEventListener("click", function () {
          m3ToL = !m3ToL;
          // Swap placeholders to reflect direction
          if (m3ToL) {
            m3Input.placeholder = "Enter value in m³/s";
            lInput.placeholder = "Result in L/s";
            // Recalculate based on m3 input
            convertFromM3();
          } else {
            m3Input.placeholder = "Result in m³/s";
            lInput.placeholder = "Enter value in L/s";
            convertFromL();
          }
        });

        clearBtn.addEventListener("click", function () {
          m3Input.value = "";
          lInput.value = "";
          m3Input.focus();
        });

        // Initialize placeholders
        m3Input.placeholder = "Enter value in m³/s";
        lInput.placeholder = "Result in L/s";
      })();
    </script>

