    <script>
      // Utility helpers
      function toNumber(value) {
        const n = parseFloat(value);
        return isNaN(n) ? 0 : n;
      }

      function formatMoney(n) {
        if (!isFinite(n)) return "$0";
        return (
          "$" +
          n.toLocaleString(undefined, {
            maximumFractionDigits: 2,
            minimumFractionDigits: 2,
          })
        );
      }

      function formatPercent(p, digits = 1) {
        if (!isFinite(p)) return "0%";
        return (
          p.toLocaleString(undefined, {
            maximumFractionDigits: digits,
            minimumFractionDigits: digits,
          }) + "%"
        );
      }

      // DOM elements
      const vehicleValueEl = document.getElementById("vehicleValue");
      const ltvEl = document.getElementById("ltv");
      const ltvValueEl = document.getElementById("ltvValue");
      const ltvDisplayEl = document.getElementById("ltvDisplay");
      const maxLoanHintEl = document.getElementById("maxLoanHint");
      const loanAmountEl = document.getElementById("loanAmount");
      const termEl = document.getElementById("term");
      const termUnitEl = document.getElementById("termUnit");
      const rateEl = document.getElementById("rate");
      const rateTypeEl = document.getElementById("rateType");
      const originationFeeEl = document.getElementById("originationFee");
      const otherFeesEl = document.getElementById("otherFees");
      const paymentFrequencyEl = document.getElementById("paymentFrequency");
      const monthlyIncomeEl = document.getElementById("monthlyIncome");
      const balloonContainerEl = document.getElementById("balloonContainer");
      const balloonPercentEl = document.getElementById("balloonPercent");
      const balloonValueEl = document.getElementById("balloonValue");
      const balloonDisplayEl = document.getElementById("balloonDisplay");

      const modeStandardBtn = document.getElementById("mode-standard");
      const modeInterestOnlyBtn = document.getElementById("mode-interest-only");

      const calculateBtn = document.getElementById("calculateBtn");
      const resetBtn = document.getElementById("resetBtn");

      const resultsSectionEl = document.getElementById("resultsSection");
      const paymentAmountEl = document.getElementById("paymentAmount");
      const paymentFrequencyLabelEl = document.getElementById(
        "paymentFrequencyLabel"
      );
      const totalCostEl = document.getElementById("totalCost");
      const totalInterestEl = document.getElementById("totalInterest");
      const aprDisplayEl = document.getElementById("aprDisplay");
      const aprNoteEl = document.getElementById("aprNote");
      const riskBadgeEl = document.getElementById("riskBadge");
      const affordBadgeEl = document.getElementById("affordBadge");
      const ltvBadgeEl = document.getElementById("ltvBadge");
      const summaryTextEl = document.getElementById("summaryText");
      const scheduleBodyEl = document.getElementById("scheduleBody");

      let currentMode = "standard"; // 'standard' or 'interest-only'

      function updateLtvDerived() {
        const vehicle = toNumber(vehicleValueEl.value);
        const ltv = toNumber(ltvEl.value);
        const maxLoan = vehicle * (ltv / 100);
        ltvValueEl.textContent = ltv.toFixed(0) + "%";
        ltvDisplayEl.textContent = ltv.toFixed(0) + "%";
        maxLoanHintEl.textContent = "Max based on LTV: " + formatMoney(maxLoan);
      }

      function updateBalloonDerived() {
        const b = toNumber(balloonPercentEl.value);
        balloonValueEl.textContent = b.toFixed(0) + "%";
        balloonDisplayEl.textContent = b.toFixed(0) + "%";
      }

      function setMode(mode) {
        currentMode = mode;
        if (mode === "standard") {
          modeStandardBtn.classList.remove("bg-gray-100", "text-gray-700");
          modeStandardBtn.classList.add("bg-blue-600", "text-white");
          modeInterestOnlyBtn.classList.remove("bg-blue-600", "text-white");
          modeInterestOnlyBtn.classList.add("bg-gray-100", "text-gray-700");
          balloonContainerEl.classList.add("hidden");
        } else {
          modeInterestOnlyBtn.classList.remove("bg-gray-100", "text-gray-700");
          modeInterestOnlyBtn.classList.add("bg-blue-600", "text-white");
          modeStandardBtn.classList.remove("bg-blue-600", "text-white");
          modeStandardBtn.classList.add("bg-gray-100", "text-gray-700");
          balloonContainerEl.classList.remove("hidden");
        }
      }

      modeStandardBtn.addEventListener("click", () => setMode("standard"));
      modeInterestOnlyBtn.addEventListener("click", () =>
        setMode("interest-only")
      );

      ltvEl.addEventListener("input", updateLtvDerived);
      balloonPercentEl.addEventListener("input", updateBalloonDerived);
      vehicleValueEl.addEventListener("input", updateLtvDerived);

      function getPeriodicRate() {
        const rate = toNumber(rateEl.value) / 100;
        const type = rateTypeEl.value;
        let monthlyRate;

        if (type === "annual") {
          monthlyRate = rate / 12;
        } else if (type === "monthly") {
          monthlyRate = rate;
        } else if (type === "biweekly") {
          const annualApprox = Math.pow(1 + rate, 26) - 1;
          monthlyRate = Math.pow(1 + annualApprox, 1 / 12) - 1;
        } else {
          monthlyRate = rate;
        }

        return { monthlyRate, inputRate: rate, type };
      }

      function getPaymentsPerYear(freq) {
        if (freq === "weekly") return 52;
        if (freq === "biweekly") return 26;
        return 12;
      }

      function buildScheduleStandard(P, periodicRate, nPayments) {
        const schedule = [];
        if (periodicRate === 0) {
          const payment = nPayments > 0 ? P / nPayments : 0;
          let balance = P;
          for (let i = 1; i <= nPayments; i++) {
            const principal = Math.min(payment, balance);
            const interest = 0;
            balance = Math.max(0, balance - principal);
            schedule.push({ i, payment, interest, principal, balance });
          }
          return schedule;
        }

        const payment =
          (P * (periodicRate * Math.pow(1 + periodicRate, nPayments))) /
          (Math.pow(1 + periodicRate, nPayments) - 1);
        let balance = P;
        for (let i = 1; i <= nPayments; i++) {
          const interest = balance * periodicRate;
          let principal = payment - interest;
          if (i === nPayments) {
            principal = balance;
          }
          balance = Math.max(0, balance - principal);
          schedule.push({ i, payment, interest, principal, balance });
          if (balance <= 0) break;
        }
        return schedule;
      }

      function buildScheduleInterestOnly(
        P,
        periodicRate,
        nPayments,
        balloonPercent
      ) {
        const schedule = [];
        const balloonPrincipal = P * (balloonPercent / 100);
        const interestOnlyPrincipal = P - balloonPrincipal;
        const interestOnlyPayment = periodicRate * P; // interest on full principal
        let balance = P;

        for (let i = 1; i <= nPayments; i++) {
          let payment, interest, principal;
          if (i < nPayments) {
            interest = balance * periodicRate;
            payment = interestOnlyPayment;
            principal = payment - interest;
            if (principal < 0) principal = 0;
          } else {
            interest = balance * periodicRate;
            payment = interest + balance;
            principal = balance;
          }
          balance = Math.max(0, balance - principal);
          schedule.push({ i, payment, interest, principal, balance });
          if (balance <= 0) break;
        }
        return schedule;
      }

      function calculateAprFromPeriodic(periodicRate, paymentsPerYear) {
        if (periodicRate <= 0) return 0;
        return Math.pow(1 + periodicRate, paymentsPerYear) - 1;
      }

      function updateBadgesAndSummary({
        payment,
        freqLabel,
        totalInterest,
        totalCost,
        ltv,
        monthlyIncome,
        apr,
      }) {
        // LTV badge
        ltvBadgeEl.textContent = "LTV: " + formatPercent(ltv, 0);
        ltvBadgeEl.className = "badge";
        if (ltv <= 50) {
          ltvBadgeEl.classList.add("badge-safe");
        } else if (ltv <= 80) {
          ltvBadgeEl.classList.add("badge-warning");
        } else {
          ltvBadgeEl.classList.add("badge-risk");
        }

        // Risk badge based on APR
        riskBadgeEl.className = "badge";
        if (apr < 36) {
          riskBadgeEl.classList.add("badge-safe");
          riskBadgeEl.textContent = "Risk level: lower (APR < 36%)";
        } else if (apr < 100) {
          riskBadgeEl.classList.add("badge-warning");
          riskBadgeEl.textContent = "Risk level: high (APR 36–100%)";
        } else {
          riskBadgeEl.classList.add("badge-risk");
          riskBadgeEl.textContent = "Risk level: very high (APR ≥ 100%)";
        }

        // Affordability badge
        affordBadgeEl.className = "badge";
        if (monthlyIncome <= 0) {
          affordBadgeEl.classList.add("badge-warning");
          affordBadgeEl.textContent = "Affordability: income not provided";
        } else {
          const monthlyPayment =
            payment *
            (freqLabel === "per month"
              ? 1
              : freqLabel === "every 2 weeks"
              ? 26 / 12
              : 52 / 12);
          const pct = (monthlyPayment / monthlyIncome) * 100;
          if (pct < 10) {
            affordBadgeEl.classList.add("badge-safe");
            affordBadgeEl.textContent =
              "Affordability: payment ≈ " + pct.toFixed(1) + "% of income";
          } else if (pct < 20) {
            affordBadgeEl.classList.add("badge-warning");
            affordBadgeEl.textContent =
              "Affordability: payment ≈ " + pct.toFixed(1) + "% of income";
          } else {
            affordBadgeEl.classList.add("badge-risk");
            affordBadgeEl.textContent =
              "Affordability: payment ≈ " + pct.toFixed(1) + "% of income";
          }
        }

        // Summary text
        const lines = [];
        lines.push(
          `You would pay ${formatMoney(
            payment
          )} ${freqLabel}, for a total cost of ${formatMoney(
            totalCost
          )} including about ${formatMoney(totalInterest)} in interest.`
        );
        if (apr > 0) {
          lines.push(
            `The effective APR is approximately ${formatPercent(
              apr * 100,
              1
            )}. Compare this with alternatives such as personal loans or credit union financing.`
          );
        }
        if (monthlyIncome > 0) {
          const monthlyPayment =
            payment *
            (freqLabel === "per month"
              ? 1
              : freqLabel === "every 2 weeks"
              ? 26 / 12
              : 52 / 12);
          const pct = (monthlyPayment / monthlyIncome) * 100;
          lines.push(
            `Based on your income, the estimated monthly payment would use about ${pct.toFixed(
              1
            )}% of your take‑home pay.`
          );
        }
        summaryTextEl.textContent = lines.join(" ");
      }

      function renderSchedule(schedule) {
        scheduleBodyEl.innerHTML = "";
        schedule.forEach((row) => {
          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="px-2 py-1 border-b text-left">${row.i}</td>
        <td class="px-2 py-1 border-b text-right">${formatMoney(
          row.payment
        )}</td>
        <td class="px-2 py-1 border-b text-right">${formatMoney(
          row.interest
        )}</td>
        <td class="px-2 py-1 border-b text-right">${formatMoney(
          row.principal
        )}</td>
        <td class="px-2 py-1 border-b text-right">${formatMoney(
          row.balance
        )}</td>
      `;
          scheduleBodyEl.appendChild(tr);
        });
      }

      function calculate() {
        const vehicleValue = toNumber(vehicleValueEl.value);
        const ltv = toNumber(ltvEl.value);
        const maxLoan = vehicleValue * (ltv / 100);

        let loanAmount = toNumber(loanAmountEl.value);
        if (loanAmount > maxLoan && maxLoan > 0) {
          loanAmount = maxLoan;
          loanAmountEl.value = loanAmount.toFixed(2);
        }

        const termValue = Math.max(1, toNumber(termEl.value));
        const termUnit = termUnitEl.value;
        const freq = paymentFrequencyEl.value;
        const { monthlyRate } = getPeriodicRate();
        const origFee = toNumber(originationFeeEl.value);
        const otherFees = toNumber(otherFeesEl.value);
        const monthlyIncome = toNumber(monthlyIncomeEl.value);

        const financedAmount = loanAmount + origFee + otherFees;

        const paymentsPerYear = getPaymentsPerYear(freq);
        const totalMonths =
          termUnit === "months" ? termValue : termValue / 4.345; // approx weeks->months
        const totalYears = totalMonths / 12;
        const nPayments = Math.round(totalYears * paymentsPerYear);

        if (financedAmount <= 0 || nPayments <= 0) {
          resultsSectionEl.classList.add("hidden");
          return;
        }

        const periodicRate =
          Math.pow(1 + monthlyRate, 12 / paymentsPerYear) - 1;

        let schedule;
        if (currentMode === "standard") {
          schedule = buildScheduleStandard(
            financedAmount,
            periodicRate,
            nPayments
          );
        } else {
          const balloonPercent = toNumber(balloonPercentEl.value);
          schedule = buildScheduleInterestOnly(
            financedAmount,
            periodicRate,
            nPayments,
            balloonPercent
          );
        }

        const payment = schedule.length > 0 ? schedule[0].payment : 0;
        const totalPaid = schedule.reduce((sum, r) => sum + r.payment, 0);
        const totalInterest = totalPaid - financedAmount;
        const totalCost = loanAmount + totalInterest + origFee + otherFees;

        const apr = calculateAprFromPeriodic(periodicRate, paymentsPerYear);

        const freqLabel =
          freq === "monthly"
            ? "per month"
            : freq === "biweekly"
            ? "every 2 weeks"
            : "per week";

        paymentAmountEl.textContent = formatMoney(payment);
        paymentFrequencyLabelEl.textContent = freqLabel;
        totalCostEl.textContent = formatMoney(totalCost);
        totalInterestEl.textContent = `Including ${formatMoney(
          totalInterest
        )} interest & ${formatMoney(origFee + otherFees)} fees`;
        aprDisplayEl.textContent = formatPercent(apr * 100, 1);

        updateBadgesAndSummary({
          payment,
          freqLabel,
          totalInterest,
          totalCost,
          ltv:
            loanAmount && vehicleValue ? (loanAmount / vehicleValue) * 100 : 0,
          monthlyIncome,
          apr,
        });

        renderSchedule(schedule);
        resultsSectionEl.classList.remove("hidden");
      }

      calculateBtn.addEventListener("click", calculate);

      resetBtn.addEventListener("click", () => {
        vehicleValueEl.value = 8000;
        ltvEl.value = 80;
        loanAmountEl.value = 3000;
        termEl.value = 12;
        termUnitEl.value = "months";
        rateEl.value = 15;
        rateTypeEl.value = "monthly";
        originationFeeEl.value = 150;
        otherFeesEl.value = 0;
        paymentFrequencyEl.value = "monthly";
        monthlyIncomeEl.value = 3500;
        balloonPercentEl.value = 50;
        setMode("standard");
        updateLtvDerived();
        updateBalloonDerived();
        resultsSectionEl.classList.add("hidden");
      });

      // Initialize UI
      updateLtvDerived();
      updateBalloonDerived();
      setMode("standard");
    </script>

