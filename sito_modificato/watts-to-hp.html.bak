    <script>
      // Core conversion factors
      const FACTORS = {
        mechanical: 745.6999,
        metric: 735.49875,
        electrical: 746,
      };

      const modeWtoHPBtn = document.getElementById("mode-w-to-hp");
      const modeHPtoWBtn = document.getElementById("mode-hp-to-w");
      const hpTypeSelect = document.getElementById("hp-type");
      const hpTypeNote = document.getElementById("hp-type-note");
      const inputValueEl = document.getElementById("input-value");
      const outputValueEl = document.getElementById("output-value");
      const inputLabel = document.getElementById("input-label");
      const outputLabel = document.getElementById("output-label");
      const inputHelper = document.getElementById("input-helper");
      const outputHelper = document.getElementById("output-helper");
      const decimalsRange = document.getElementById("decimals");
      const decimalsDisplay = document.getElementById("decimals-display");
      const summaryLine = document.getElementById("summary-line");
      const quickTableBody = document.getElementById("quick-table-body");

      let currentMode = "w-to-hp"; // or 'hp-to-w'

      function getFactor() {
        const type = hpTypeSelect.value;
        return FACTORS[type] || FACTORS.mechanical;
      }

      function getHpLabel() {
        const type = hpTypeSelect.value;
        if (type === "metric") return "metric horsepower (HP, PS, CV)";
        if (type === "electrical") return "electrical horsepower (HP)";
        return "mechanical horsepower (HP)";
      }

      function updateHpNote() {
        const type = hpTypeSelect.value;
        if (type === "metric") {
          hpTypeNote.textContent =
            "Metric horsepower (PS, CV, hk) is common in European and some Asian car specs.";
        } else if (type === "electrical") {
          hpTypeNote.textContent =
            "Electrical horsepower (746 W) is mainly used for electric motor ratings.";
        } else {
          hpTypeNote.textContent =
            "Mechanical HP is standard for engines and motors in the US.";
        }
      }

      function formatNumber(value, decimals) {
        if (!isFinite(value)) return "";
        return Number(value).toFixed(decimals);
      }

      function updateLabels() {
        const hpLabel = getHpLabel();
        if (currentMode === "w-to-hp") {
          inputLabel.textContent = "Power in watts (W)";
          outputLabel.textContent = "Power in " + hpLabel;
          inputHelper.textContent =
            "Enter the power in watts to convert to horsepower.";
          outputHelper.textContent = "Result in " + hpLabel + ".";
        } else {
          inputLabel.textContent = "Power in " + hpLabel;
          outputLabel.textContent = "Power in watts (W)";
          inputHelper.textContent =
            "Enter the power in horsepower to convert to watts.";
          outputHelper.textContent = "Result in watts (W).";
        }
      }

      function updateSummary(input, output) {
        if (!isFinite(input) || !isFinite(output)) {
          summaryLine.classList.add("hidden");
          summaryLine.textContent = "";
          return;
        }
        const type = hpTypeSelect.value;
        let hpTypeText = "mechanical horsepower";
        if (type === "metric") hpTypeText = "metric horsepower";
        if (type === "electrical") hpTypeText = "electrical horsepower";

        if (currentMode === "w-to-hp") {
          summaryLine.textContent = `${input} W ≈ ${output} ${hpTypeText}`;
        } else {
          summaryLine.textContent = `${input} ${hpTypeText} ≈ ${output} W`;
        }
        summaryLine.classList.remove("hidden");
      }

      function convert() {
        const raw = inputValueEl.value.trim();
        const decimals = parseInt(decimalsRange.value, 10);
        if (raw === "") {
          outputValueEl.value = "";
          updateSummary(NaN, NaN);
          return;
        }
        const val = Number(raw);
        if (!isFinite(val)) {
          outputValueEl.value = "";
          updateSummary(NaN, NaN);
          return;
        }

        const factor = getFactor();
        let result;
        if (currentMode === "w-to-hp") {
          result = val / factor;
        } else {
          result = val * factor;
        }
        const formatted = formatNumber(result, decimals);
        outputValueEl.value = formatted;
        updateSummary(formatNumber(val, decimals), formatted);
      }

      function setMode(mode) {
        currentMode = mode;
        if (mode === "w-to-hp") {
          modeWtoHPBtn.classList.add("toggle-btn-active", "text-white");
          modeWtoHPBtn.classList.remove("text-blue-600");
          modeWtoHPBtn.setAttribute("aria-pressed", "true");

          modeHPtoWBtn.classList.remove("toggle-btn-active", "text-white");
          modeHPtoWBtn.classList.add("text-blue-600");
          modeHPtoWBtn.setAttribute("aria-pressed", "false");
        } else {
          modeHPtoWBtn.classList.add("toggle-btn-active", "text-white");
          modeHPtoWBtn.classList.remove("text-blue-600");
          modeHPtoWBtn.setAttribute("aria-pressed", "true");

          modeWtoHPBtn.classList.remove("toggle-btn-active", "text-white");
          modeWtoHPBtn.classList.add("text-blue-600");
          modeWtoHPBtn.setAttribute("aria-pressed", "false");
        }
        updateLabels();
        convert();
      }

      function updateDecimals() {
        const d = parseInt(decimalsRange.value, 10);
        decimalsDisplay.textContent = d;
        convert();
      }

      function initPresets() {
        const presetButtons = document.querySelectorAll("button[data-preset]");
        presetButtons.forEach((btn) => {
          btn.addEventListener("click", () => {
            const val = btn.getAttribute("data-preset");
            if (currentMode === "w-to-hp") {
              inputValueEl.value = val;
            } else {
              // If in HP→W mode, interpret presets as HP equivalents of common wattages
              // Use mechanical HP for mapping
              const w = Number(val);
              const hpApprox = w / FACTORS.mechanical;
              inputValueEl.value = hpApprox.toFixed(2);
            }
            convert();
          });
        });
      }

      function buildQuickTable() {
        const rowsWatts = [100, 250, 500, 750, 1000, 1500, 2000, 2500, 3000];
        quickTableBody.innerHTML = "";
        rowsWatts.forEach((w) => {
          const mech = w / FACTORS.mechanical;
          const metric = w / FACTORS.metric;
          const elec = w / FACTORS.electrical;
          const tr = document.createElement("tr");
          tr.className = "odd:bg-white even:bg-gray-50";
          tr.innerHTML = `
        <td class="px-3 py-2 border-b text-gray-800">${w}</td>
        <td class="px-3 py-2 border-b text-gray-800">${mech.toFixed(3)}</td>
        <td class="px-3 py-2 border-b text-gray-800">${metric.toFixed(3)}</td>
        <td class="px-3 py-2 border-b text-gray-800">${elec.toFixed(3)}</td>
      `;
          quickTableBody.appendChild(tr);
        });
      }

      // Event listeners
      modeWtoHPBtn.addEventListener("click", () => setMode("w-to-hp"));
      modeHPtoWBtn.addEventListener("click", () => setMode("hp-to-w"));
      hpTypeSelect.addEventListener("change", () => {
        updateHpNote();
        updateLabels();
        convert();
      });
      inputValueEl.addEventListener("input", convert);
      decimalsRange.addEventListener("input", updateDecimals);

      // Initialize
      updateHpNote();
      updateLabels();
      updateDecimals();
      initPresets();
      buildQuickTable();
      convert();
    </script>

