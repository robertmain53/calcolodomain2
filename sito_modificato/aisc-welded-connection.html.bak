    <script>
      // Mobile menu toggle
      document
        .getElementById("mobile-menu-toggle")
        ?.addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          if (!menu) return;
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          this.setAttribute("aria-expanded", String(isHidden));
        });

      function formatNumber(val, decimals = 3) {
        if (!isFinite(val)) return "-";
        return Number(val).toFixed(decimals);
      }

      function getMinFilletSize(t) {
        if (!isFinite(t) || t <= 0) return null;
        if (t <= 0.25) return 3 / 16;
        if (t <= 0.5) return 0.25;
        return 5 / 16;
      }

      function updateWeldSizeHelp() {
        const weldType = document.getElementById("weldType").value;
        const help = document.getElementById("weldSizeHelp");
        if (weldType === "fillet") {
          help.textContent = 'Fillet weld leg size (e.g., 1/4" = 0.25 in).';
        } else {
          help.textContent =
            "Groove weld size (approximate depth or leg; use effective throat for precise design).";
        }
      }

      document
        .getElementById("weldType")
        .addEventListener("change", updateWeldSizeHelp);
      updateWeldSizeHelp();

      document.getElementById("resetBtn").addEventListener("click", () => {
        document.getElementById("designMethod").value = "LRFD";
        document.getElementById("weldType").value = "fillet";
        document.getElementById("fexx").value = 70;
        document.getElementById("plateThk").value = 0.5;
        document.getElementById("weldSize").value = 0.25;
        document.getElementById("weldLength").value = 6;
        document.getElementById("numLines").value = 2;
        document.getElementById("effectiveThroat").value = "";
        document.getElementById("shearLoad").value = 20;
        document.getElementById("tensionLoad").value = 10;
        document.getElementById("errorMsg").textContent = "";
        document.getElementById("resultsContainer").classList.add("hidden");
        updateWeldSizeHelp();
      });

      document.getElementById("calcBtn").addEventListener("click", () => {
        const errEl = document.getElementById("errorMsg");
        errEl.textContent = "";

        const designMethod = document.getElementById("designMethod").value;
        const weldType = document.getElementById("weldType").value;

        const Fexx = parseFloat(document.getElementById("fexx").value);
        const tPlate = parseFloat(document.getElementById("plateThk").value);
        const wSize = parseFloat(document.getElementById("weldSize").value);
        const L = parseFloat(document.getElementById("weldLength").value);
        const nLines = parseFloat(document.getElementById("numLines").value);
        const throatInput = parseFloat(
          document.getElementById("effectiveThroat").value
        );
        const Vu = parseFloat(document.getElementById("shearLoad").value);
        const Tu = parseFloat(document.getElementById("tensionLoad").value);

        if (
          !isFinite(Fexx) ||
          Fexx <= 0 ||
          !isFinite(wSize) ||
          wSize <= 0 ||
          !isFinite(L) ||
          L <= 0 ||
          !isFinite(nLines) ||
          nLines <= 0 ||
          !isFinite(Vu) ||
          Vu < 0 ||
          !isFinite(Tu) ||
          Tu < 0
        ) {
          errEl.textContent =
            "Please enter valid positive values for weld size, length, number of lines, FEXX, and loads.";
          return;
        }

        // Effective throat
        let te;
        let throatNote = "";
        if (isFinite(throatInput) && throatInput > 0) {
          te = throatInput;
          throatNote = "User-specified effective throat.";
        } else {
          if (weldType === "fillet") {
            te = 0.707 * wSize;
            throatNote = "Fillet weld: t_e = 0.707 × w.";
          } else {
            // Groove weld without explicit throat: assume t_e ≈ 0.8 × size as a rough default
            te = 0.8 * wSize;
            throatNote =
              "Groove weld: assumed t_e = 0.8 × size (approximation).";
          }
        }

        const Ltot = L * nLines;
        const Aw = te * Ltot; // in^2

        // Nominal strengths (kips)
        const Vn_nom = (0.6 * Fexx * Aw) / 1000.0;
        const Tn_nom = (0.75 * Fexx * Aw) / 1000.0;

        let phi = 0.75;
        let omega = 2.0;
        let V_design, T_design;
        let Vu_used = Vu;
        let Tu_used = Tu;
        let methodLabel;

        if (designMethod === "LRFD") {
          methodLabel = "LRFD (φ design)";
          V_design = phi * Vn_nom;
          T_design = phi * Tn_nom;
        } else {
          methodLabel = "ASD (Ω allowable)";
          // For ASD we treat V_design as allowable = Vn/Ω, same for T
          V_design = Vn_nom / omega;
          T_design = Tn_nom / omega;
        }

        const shearRatio = V_design > 0 ? Vu_used / V_design : Infinity;
        const tensionRatio = T_design > 0 ? Tu_used / T_design : Infinity;
        const interactionRatio = Math.sqrt(
          Math.pow(shearRatio, 2) + Math.pow(tensionRatio, 2)
        );

        const pass = interactionRatio <= 1.0 + 1e-6;

        // Populate results
        document.getElementById("resDesignMethod").textContent = methodLabel;
        document.getElementById("resWeldType").textContent =
          weldType === "fillet" ? "Fillet weld" : "Groove weld";
        document.getElementById("resThroat").textContent = formatNumber(te, 3);
        document.getElementById("resTotalLength").textContent = formatNumber(
          Ltot,
          2
        );
        document.getElementById("resArea").textContent = formatNumber(Aw, 3);
        document.getElementById("resFexx").textContent = formatNumber(Fexx, 0);

        const strengthLabel = designMethod === "LRFD" ? "φVn" : "V_allow";
        const strengthLabelT = designMethod === "LRFD" ? "φTn" : "T_allow";

        document.getElementById(
          "resVn"
        ).textContent = `${strengthLabel} = ${formatNumber(V_design, 3)} kips`;
        document.getElementById("resVu").textContent = `V = ${formatNumber(
          Vu_used,
          3
        )} kips`;
        document.getElementById("resShearRatio").textContent = formatNumber(
          shearRatio,
          3
        );

        document.getElementById(
          "resTn"
        ).textContent = `${strengthLabelT} = ${formatNumber(T_design, 3)} kips`;
        document.getElementById("resTu").textContent = `T = ${formatNumber(
          Tu_used,
          3
        )} kips`;
        document.getElementById("resTensionRatio").textContent = formatNumber(
          tensionRatio,
          3
        );

        document.getElementById("resInteractionRatio").textContent =
          formatNumber(interactionRatio, 3);

        const summary = document.getElementById("summaryText");
        summary.innerHTML = pass
          ? `<span class="result-good font-semibold">OK:</span> Weld is adequate. Maximum demand-to-capacity ratio = ${formatNumber(
              interactionRatio,
              3
            )} ≤ 1.0.`
          : `<span class="result-bad font-semibold">NG:</span> Weld is overstressed. Maximum demand-to-capacity ratio = ${formatNumber(
              interactionRatio,
              3
            )} &gt; 1.0. Increase weld size or length.`;

        // Details
        const details = [];
        details.push(
          `<strong>1. Effective throat</strong><br>${throatNote}<br>t<sub>e</sub> = ${formatNumber(
            te,
            3
          )} in.`
        );
        details.push(
          `<strong>2. Effective area</strong><br>L<sub>tot</sub> = L × n = ${formatNumber(
            L,
            2
          )} × ${formatNumber(nLines, 2)} = ${formatNumber(
            Ltot,
            2
          )} in.<br>A<sub>w</sub> = t<sub>e</sub> × L<sub>tot</sub> = ${formatNumber(
            te,
            3
          )} × ${formatNumber(Ltot, 2)} = ${formatNumber(Aw, 3)} in².`
        );
        details.push(
          `<strong>3. Nominal strengths (AISC 360)</strong><br>V<sub>n</sub> = 0.6 F<sub>EXX</sub> A<sub>w</sub> = 0.6 × ${formatNumber(
            Fexx,
            0
          )} × ${formatNumber(Aw, 3)} = ${formatNumber(
            Vn_nom,
            3
          )} kips.<br>T<sub>n</sub> = 0.75 F<sub>EXX</sub> A<sub>w</sub> = 0.75 × ${formatNumber(
            Fexx,
            0
          )} × ${formatNumber(Aw, 3)} = ${formatNumber(Tn_nom, 3)} kips.`
        );
        if (designMethod === "LRFD") {
          details.push(
            `<strong>4. Design strengths (LRFD)</strong><br>φ = 0.75.<br>φV<sub>n</sub> = 0.75 × ${formatNumber(
              Vn_nom,
              3
            )} = ${formatNumber(
              V_design,
              3
            )} kips.<br>φT<sub>n</sub> = 0.75 × ${formatNumber(
              Tn_nom,
              3
            )} = ${formatNumber(T_design, 3)} kips.`
          );
        } else {
          details.push(
            `<strong>4. Allowable strengths (ASD)</strong><br>Ω ≈ 2.0.<br>V<sub>allow</sub> = V<sub>n</sub> / Ω = ${formatNumber(
              Vn_nom,
              3
            )} / 2.0 = ${formatNumber(
              V_design,
              3
            )} kips.<br>T<sub>allow</sub> = T<sub>n</sub> / Ω = ${formatNumber(
              Tn_nom,
              3
            )} / 2.0 = ${formatNumber(T_design, 3)} kips.`
          );
        }
        details.push(
          `<strong>5. Demand-to-capacity ratios</strong><br>Shear ratio = V / ${
            designMethod === "LRFD" ? "φV<sub>n</sub>" : "V<sub>allow</sub>"
          } = ${formatNumber(Vu_used, 3)} / ${formatNumber(
            V_design,
            3
          )} = ${formatNumber(shearRatio, 3)}.<br>Tension ratio = T / ${
            designMethod === "LRFD" ? "φT<sub>n</sub>" : "T<sub>allow</sub>"
          } = ${formatNumber(Tu_used, 3)} / ${formatNumber(
            T_design,
            3
          )} = ${formatNumber(
            tensionRatio,
            3
          )}.<br>Interaction ratio = (Shear ratio)² + (Tension ratio)² = ${formatNumber(
            interactionRatio,
            3
          )}.`
        );

        // Minimum fillet weld size note
        if (weldType === "fillet" && isFinite(tPlate) && tPlate > 0) {
          const wMin = getMinFilletSize(tPlate);
          if (wMin) {
            const warn = wSize + 1e-6 < wMin;
            details.push(
              `<strong>6. Minimum fillet weld size (informational)</strong><br>Based on t = ${formatNumber(
                tPlate,
                3
              )} in, a common minimum fillet weld size is about ${formatNumber(
                wMin,
                3
              )} in.<br>` +
                (warn
                  ? `<span class="text-red-700 font-medium">Note:</span> Provided weld size w = ${formatNumber(
                      wSize,
                      3
                    )} in is less than this typical minimum. Check AISC minimum weld size requirements.`
                  : `Provided weld size w = ${formatNumber(
                      wSize,
                      3
                    )} in is not less than this typical minimum.`)
            );
          }
        }

        document.getElementById("resDetails").innerHTML =
          details.join("<br><br>");
        document.getElementById("resultsContainer").classList.remove("hidden");
      });
    </script>

