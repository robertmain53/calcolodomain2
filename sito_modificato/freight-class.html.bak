    <script>
      // Freight class calculator logic
      (function () {
        const unitButtons = document.querySelectorAll(".unit-toggle");
        const lengthInput = document.getElementById("length");
        const widthInput = document.getElementById("width");
        const heightInput = document.getElementById("height");
        const weightInput = document.getElementById("weight");
        const lengthUnitSpan = document.getElementById("length-unit");
        const widthUnitSpan = document.getElementById("width-unit");
        const heightUnitSpan = document.getElementById("height-unit");
        const weightUnitSpan = document.getElementById("weight-unit");
        const handlingSelect = document.getElementById("handling");
        const liabilitySelect = document.getElementById("liability");
        const calcBtn = document.getElementById("calculate-btn");
        const resetBtn = document.getElementById("reset-btn");

        const volumeResult = document.getElementById("volume-result");
        const densityResult = document.getElementById("density-result");
        const baseClassResult = document.getElementById("base-class-result");
        const estimatedClassResult = document.getElementById(
          "estimated-class-result"
        );
        const classNote = document.getElementById("class-note");
        const tipText = document.getElementById("tip-text");

        let currentUnit = "imperial"; // 'imperial' or 'metric'

        function setUnits(unit) {
          currentUnit = unit;
          if (unit === "imperial") {
            lengthUnitSpan.textContent = "in";
            widthUnitSpan.textContent = "in";
            heightUnitSpan.textContent = "in";
            weightUnitSpan.textContent = "lb";
          } else {
            lengthUnitSpan.textContent = "cm";
            widthUnitSpan.textContent = "cm";
            heightUnitSpan.textContent = "cm";
            weightUnitSpan.textContent = "kg";
          }
          unitButtons.forEach((btn) => {
            if (btn.dataset.unit === unit) {
              btn.classList.add("bg-blue-600", "text-white", "border-blue-600");
              btn.classList.remove(
                "bg-white",
                "text-gray-700",
                "border-gray-300"
              );
            } else {
              btn.classList.remove(
                "bg-blue-600",
                "text-white",
                "border-blue-600"
              );
              btn.classList.add("bg-white", "text-gray-700", "border-gray-300");
            }
          });
          clearResults();
        }

        function clearResults() {
          volumeResult.textContent = "–";
          densityResult.textContent = "–";
          baseClassResult.textContent = "–";
          estimatedClassResult.textContent = "–";
          classNote.textContent = "";
          tipText.textContent =
            "Enter your shipment dimensions and weight to see how density affects your LTL freight class.";
        }

        unitButtons.forEach((btn) => {
          btn.addEventListener("click", () => setUnits(btn.dataset.unit));
        });

        function parsePositive(value) {
          const num = parseFloat(value);
          return isFinite(num) && num > 0 ? num : null;
        }

        function mapDensityToBaseClass(density) {
          // Returns {range: string, baseClass: number}
          if (!isFinite(density) || density <= 0) {
            return { range: "N/A", baseClass: NaN };
          }
          if (density > 50) return { range: "Class 50", baseClass: 50 };
          if (density > 35) return { range: "Class 55", baseClass: 55 };
          if (density > 30) return { range: "Class 60", baseClass: 60 };
          if (density > 22.5) return { range: "Class 65", baseClass: 65 };
          if (density > 15) return { range: "Class 70–85", baseClass: 77.5 };
          if (density > 10) return { range: "Class 85–100", baseClass: 92.5 };
          if (density > 8) return { range: "Class 100–125", baseClass: 112.5 };
          if (density > 6) return { range: "Class 125–150", baseClass: 137.5 };
          if (density > 4) return { range: "Class 150–250", baseClass: 200 };
          return { range: "Class 250–500", baseClass: 350 };
        }

        function adjustClass(baseClass, handling, liability) {
          if (!isFinite(baseClass)) return NaN;
          let cls = baseClass;

          // Handling adjustment
          if (handling === "difficult") {
            cls += 5;
          } else if (handling === "very-fragile") {
            cls += 10;
          }

          // Liability adjustment
          if (liability === "medium") {
            cls += 5;
          } else if (liability === "high") {
            cls += 10;
          }

          // Round to nearest standard class step (5)
          cls = Math.round(cls / 5) * 5;

          // Clamp to 50–500
          if (cls < 50) cls = 50;
          if (cls > 500) cls = 500;

          return cls;
        }

        function getTip(density, estClass) {
          if (!isFinite(density) || !isFinite(estClass)) {
            return "Accurate dimensions and weight are the best way to avoid reclass and rebill charges.";
          }
          if (density < 4) {
            return "Your freight is very low density. Consider reducing empty space or using smaller packaging to lower your class.";
          }
          if (density < 8) {
            return "Your freight is relatively light for its size. Consolidating items or improving packaging may reduce your class.";
          }
          if (density > 20 && estClass <= 85) {
            return "You have a fairly dense shipment. Verify if your commodity has a fixed NMFC class that could be even lower.";
          }
          return "Use this estimated class as a guide and confirm the final NMFC item and class with your carrier or 3PL.";
        }

        calcBtn.addEventListener("click", () => {
          const L = parsePositive(lengthInput.value);
          const W = parsePositive(widthInput.value);
          const H = parsePositive(heightInput.value);
          const Wt = parsePositive(weightInput.value);

          if (!L || !W || !H || !Wt) {
            alert(
              "Please enter positive values for length, width, height, and weight."
            );
            return;
          }

          let lengthIn = L;
          let widthIn = W;
          let heightIn = H;
          let weightLb = Wt;

          if (currentUnit === "metric") {
            // Convert cm to inches, kg to lb
            lengthIn = L / 2.54;
            widthIn = W / 2.54;
            heightIn = H / 2.54;
            weightLb = Wt * 2.2046226218;
          }

          const cubicFeet = (lengthIn * widthIn * heightIn) / 1728;
          const density = weightLb / cubicFeet;

          const volumeDisplay = cubicFeet.toFixed(2) + " ft³";
          const densityDisplay = density.toFixed(2) + " lb/ft³";

          const { range, baseClass } = mapDensityToBaseClass(density);
          const handling = handlingSelect.value;
          const liability = liabilitySelect.value;
          const estClass = adjustClass(baseClass, handling, liability);

          volumeResult.textContent = volumeDisplay;
          densityResult.textContent = densityDisplay;
          baseClassResult.textContent = range;
          estimatedClassResult.textContent = isFinite(estClass)
            ? "Class " + estClass
            : "–";

          classNote.textContent =
            "This is an estimated freight class based on density, handling and liability. The carrier or NMFTA may assign a different official class.";
          tipText.textContent = getTip(density, estClass);
        });

        resetBtn.addEventListener("click", () => {
          lengthInput.value = "";
          widthInput.value = "";
          heightInput.value = "";
          weightInput.value = "";
          handlingSelect.value = "normal";
          liabilitySelect.value = "low";
          clearResults();
        });

        // Initialize
        setUnits("imperial");
      })();
    </script>

