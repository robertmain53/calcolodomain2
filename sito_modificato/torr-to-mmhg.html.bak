    <script>
      // State
      let currentMode = "torr-to-mmhg";

      function setMode(mode) {
        currentMode = mode;

        const torrBtn = document.getElementById("mode-torr-to-mmhg");
        const mmhgBtn = document.getElementById("mode-mmhg-to-torr");
        const inputUnitLabel = document.getElementById("input-unit-label");
        const outputUnitLabel = document.getElementById("output-unit-label");

        if (mode === "torr-to-mmhg") {
          torrBtn.classList.add("toggle-btn-active");
          torrBtn.classList.remove("toggle-btn-inactive");
          torrBtn.setAttribute("aria-selected", "true");

          mmhgBtn.classList.add("toggle-btn-inactive");
          mmhgBtn.classList.remove("toggle-btn-active");
          mmhgBtn.setAttribute("aria-selected", "false");

          inputUnitLabel.textContent = "Torr";
          outputUnitLabel.textContent = "mmHg";
        } else {
          mmhgBtn.classList.add("toggle-btn-active");
          mmhgBtn.classList.remove("toggle-btn-inactive");
          mmhgBtn.setAttribute("aria-selected", "true");

          torrBtn.classList.add("toggle-btn-inactive");
          torrBtn.classList.remove("toggle-btn-active");
          torrBtn.setAttribute("aria-selected", "false");

          inputUnitLabel.textContent = "mmHg";
          outputUnitLabel.textContent = "Torr";
        }

        handleInputChange();
      }

      function getPrecision() {
        const slider = document.getElementById("precision");
        const value = parseInt(slider.value, 10);
        return isNaN(value) ? 4 : value;
      }

      function updatePrecisionLabel() {
        const precision = getPrecision();
        const label = document.getElementById("precision-label");
        label.textContent =
          precision + (precision === 1 ? " decimal place" : " decimal places");
      }

      function handleInputChange() {
        const inputEl = document.getElementById("input-value");
        const outputEl = document.getElementById("output-value");
        const errorEl = document.getElementById("input-error");
        const copyFeedback = document.getElementById("copy-feedback");

        copyFeedback.classList.add("hidden");
        errorEl.classList.add("hidden");
        errorEl.textContent = "";

        const raw = inputEl.value.trim();
        if (raw === "") {
          outputEl.textContent = "—";
          return;
        }

        let value = Number(raw);
        if (!isFinite(value)) {
          errorEl.textContent =
            "Please enter a valid numeric value (you can use scientific notation like 1e-3).";
          errorEl.classList.remove("hidden");
          outputEl.textContent = "—";
          return;
        }

        // For Torr <-> mmHg we treat them as numerically equal (factor = 1)
        let result = value;

        const precision = getPrecision();
        if (isFinite(result)) {
          outputEl.textContent = result.toFixed(precision);
        } else {
          outputEl.textContent = "—";
        }
      }

      async function copyResult() {
        const outputEl = document.getElementById("output-value");
        const unitEl = document.getElementById("output-unit-label");
        const feedback = document.getElementById("copy-feedback");

        const text = outputEl.textContent;
        if (!text || text === "—") return;

        const fullText = text + " " + unitEl.textContent;

        try {
          await navigator.clipboard.writeText(fullText);
          feedback.textContent = "Copied to clipboard";
          feedback.classList.remove("hidden");
          setTimeout(() => feedback.classList.add("hidden"), 2000);
        } catch (e) {
          feedback.textContent = "Press Ctrl+C to copy";
          feedback.classList.remove("hidden");
          setTimeout(() => feedback.classList.add("hidden"), 3000);
        }
      }

      // Initialize labels on load
      document.addEventListener("DOMContentLoaded", () => {
        updatePrecisionLabel();
        handleInputChange();
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
