    <script>
      // Slider labels
      function bindSlider(id, labelId) {
        const el = document.getElementById(id);
        const label = document.getElementById(labelId);
        if (!el || !label) return;
        const update = () => {
          label.textContent = el.value + " / 10";
        };
        el.addEventListener("input", update);
        update();
      }

      bindSlider("values", "values-label");
      bindSlider("habits", "habits-label");
      bindSlider("communication", "communication-label");
      bindSlider("trust", "trust-label");
      bindSlider("effort", "effort-label");

      function calculateFriendshipScore() {
        const v = Number(document.getElementById("values").value || 0);
        const h = Number(document.getElementById("habits").value || 0);
        const c = Number(document.getElementById("communication").value || 0);
        const t = Number(document.getElementById("trust").value || 0);
        const e = Number(document.getElementById("effort").value || 0);
        const freq = Number(document.getElementById("frequency").value || 0);
        const len = Number(document.getElementById("length").value || 0);

        // Base weighted score
        let score = v * 2.5 + h * 2.0 + c * 2.0 + t * 2.5 + e * 1.0;

        // Small bonus for frequent contact and long history (max +6)
        const contactBonus = (freq - 3) * 1.0; // -2..+2
        const lengthBonus = (len - 3) * 1.0; // -2..+2
        score += contactBonus + lengthBonus;

        score = Math.max(0, Math.min(100, score));

        return {
          score,
          v,
          h,
          c,
          t,
          e,
          freq,
          len,
        };
      }

      function buildInsights(data) {
        const strengths = [];
        const weaknesses = [];

        if (data.v >= 8)
          strengths.push("You share strong core values and life goals.");
        if (data.h >= 8)
          strengths.push("Your daily habits and lifestyles fit together well.");
        if (data.c >= 8)
          strengths.push(
            "You communicate clearly and handle conflict constructively."
          );
        if (data.t >= 8)
          strengths.push(
            "There is a high level of trust and emotional safety."
          );
        if (data.e >= 8)
          strengths.push("The friendship feels balanced in terms of effort.");

        if (data.v <= 5)
          weaknesses.push(
            "You may have different values or priorities in life."
          );
        if (data.h <= 5)
          weaknesses.push(
            "Differences in habits or schedules could cause friction."
          );
        if (data.c <= 5)
          weaknesses.push(
            "Communication or conflict resolution might feel difficult at times."
          );
        if (data.t <= 5)
          weaknesses.push(
            "Trust or emotional safety may not feel fully secure."
          );
        if (data.e <= 5)
          weaknesses.push(
            "The friendship may feel one-sided in terms of effort."
          );

        if (data.freq <= 2)
          weaknesses.push(
            "You don’t talk very often, which can make it harder to stay close."
          );
        if (data.len <= 2 && data.score >= 70)
          strengths.push(
            "For a relatively new friendship, your connection looks promising."
          );

        if (!strengths.length)
          strengths.push(
            "Every friendship has unique strengths; yours may simply not be captured by these sliders."
          );
        if (!weaknesses.length)
          weaknesses.push(
            "No major friction points detected based on your answers."
          );

        let advice;
        if (data.score >= 80) {
          advice =
            "You have the foundations of a strong, resilient friendship. Keep nurturing it with regular check-ins, shared experiences, and honest conversations when something feels off.";
        } else if (data.score >= 60) {
          advice =
            "Your friendship looks generally healthy with a few areas to watch. Choose one friction point and talk about it gently with your friend, focusing on how you feel rather than what they do wrong.";
        } else if (data.score >= 40) {
          advice =
            "There are meaningful strengths here, but also some unmet needs or misalignments. Consider whether the friendship can realistically meet your expectations, and what boundaries or changes might help.";
        } else {
          advice =
            "This friendship may be important to you, but it currently looks unbalanced or unsafe in key areas. It might help to lower expectations, set firmer boundaries, or seek support from other relationships as well.";
        }

        return { strengths, weaknesses, advice };
      }

      document.getElementById("calculate-btn").addEventListener("click", () => {
        const data = calculateFriendshipScore();
        const { strengths, weaknesses, advice } = buildInsights(data);

        const resultsSection = document.getElementById("results-section");
        const scoreBar = document.getElementById("score-bar");
        const scoreNumber = document.getElementById("score-number");
        const summary = document.getElementById("friendship-summary");
        const label = document.getElementById("friendship-score-label");
        const strengthsList = document.getElementById("strengths-list");
        const weaknessesList = document.getElementById("weaknesses-list");
        const adviceText = document.getElementById("advice-text");

        const yourName = (
          document.getElementById("your-name").value || "You"
        ).trim();
        const friendNameRaw = document
          .getElementById("friend-name")
          .value.trim();
        const friendName = friendNameRaw || "your friend";

        let descriptor;
        if (data.score >= 80) descriptor = "a very strong friendship";
        else if (data.score >= 60)
          descriptor = "a generally healthy friendship";
        else if (data.score >= 40)
          descriptor = "a mixed but meaningful friendship";
        else descriptor = "a challenging or unbalanced friendship";

        summary.textContent = `${yourName} and ${friendName} currently have ${descriptor}.`;
        label.textContent = `Overall friendship score based on values, habits, communication, trust, and effort.`;
        scoreNumber.textContent = `${Math.round(data.score)} / 100`;
        scoreBar.style.width = `${data.score}%`;

        strengthsList.innerHTML = "";
        strengths.forEach((s) => {
          const li = document.createElement("li");
          li.textContent = s;
          strengthsList.appendChild(li);
        });

        weaknessesList.innerHTML = "";
        weaknesses.forEach((w) => {
          const li = document.createElement("li");
          li.textContent = w;
          weaknessesList.appendChild(li);
        });

        adviceText.textContent = advice;

        resultsSection.classList.remove("hidden");
        resultsSection.scrollIntoView({ behavior: "smooth", block: "start" });
      });

      document.getElementById("reset-btn").addEventListener("click", () => {
        document.getElementById("friendship-form").reset();
        // Reset sliders to defaults
        document.getElementById("values").value = 7;
        document.getElementById("habits").value = 7;
        document.getElementById("communication").value = 7;
        document.getElementById("trust").value = 8;
        document.getElementById("effort").value = 7;
        bindSlider("values", "values-label");
        bindSlider("habits", "habits-label");
        bindSlider("communication", "communication-label");
        bindSlider("trust", "trust-label");
        bindSlider("effort", "effort-label");

        document.getElementById("frequency").value = 4;
        document.getElementById("length").value = 3;

        document.getElementById("results-section").classList.add("hidden");
      });

      document
        .querySelectorAll("#faq-container .faq-question")
        .forEach((btn) => {
          btn.addEventListener("click", () => {
            const answer = btn.parentElement.querySelector(".faq-answer");
            const sign = btn.querySelector("span:last-child");
            const isHidden = answer.classList.contains("hidden");
            document
              .querySelectorAll("#faq-container .faq-answer")
              .forEach((a) => a.classList.add("hidden"));
            document
              .querySelectorAll("#faq-container .faq-question span:last-child")
              .forEach((s) => (s.textContent = "+"));
            if (isHidden) {
              answer.classList.remove("hidden");
              sign.textContent = "−";
            }
          });
        });
    </script>

