    <script>
      // Toggle fin type specific inputs
      const finTypeEl = document.getElementById("finType");
      const rectInputs = document.getElementById("rectangularInputs");
      const circInputs = document.getElementById("circularInputs");

      finTypeEl.addEventListener("change", () => {
        if (finTypeEl.value === "rectangular") {
          rectInputs.classList.remove("hidden");
          circInputs.classList.add("hidden");
        } else {
          rectInputs.classList.add("hidden");
          circInputs.classList.remove("hidden");
        }
      });

      function formatValue(val, unit) {
        if (!isFinite(val)) return "–";
        let v = val;
        let suffix = "";
        if (Math.abs(v) >= 1e6) {
          v = v / 1e6;
          suffix = "e6";
        } else if (Math.abs(v) >= 1e3) {
          v = v / 1e3;
          suffix = "e3";
        } else if (Math.abs(v) > 0 && Math.abs(v) < 1e-3) {
          v = v * 1e6;
          suffix = "e-6";
        }
        return (
          v.toFixed(4) + (suffix ? " " + suffix : "") + (unit ? " " + unit : "")
        );
      }

      document.getElementById("computeBtn").addEventListener("click", () => {
        const finType = finTypeEl.value;
        const tipCondition = document.getElementById("tipCondition").value;

        const L = parseFloat(document.getElementById("finLength").value);
        const k = parseFloat(document.getElementById("k").value);
        const h = parseFloat(document.getElementById("h").value);
        const wb = parseFloat(document.getElementById("baseWidth").value);
        const Tb = parseFloat(document.getElementById("Tbase").value);
        const Tinf = parseFloat(document.getElementById("Tinf").value);
        const x = parseFloat(document.getElementById("xPosition").value);

        if ([L, k, h, wb, Tb, Tinf, x].some((v) => !isFinite(v))) {
          alert("Please enter valid numeric values for all fields.");
          return;
        }
        if (L <= 0 || k <= 0 || h <= 0 || wb <= 0) {
          alert(
            "Length, conductivity, convection coefficient, and base width must be positive."
          );
          return;
        }
        if (x < 0 || x > L) {
          alert("Position x must be between 0 and L.");
          return;
        }

        let Ac, P;
        if (finType === "rectangular") {
          const b = parseFloat(document.getElementById("rectWidth").value);
          const t = parseFloat(document.getElementById("rectThickness").value);
          if (!isFinite(b) || !isFinite(t) || b <= 0 || t <= 0) {
            alert(
              "Please enter positive width and thickness for the rectangular fin."
            );
            return;
          }
          Ac = b * t;
          P = 2 * (b + t);
        } else {
          const D = parseFloat(document.getElementById("pinDiameter").value);
          if (!isFinite(D) || D <= 0) {
            alert("Please enter a positive diameter for the pin fin.");
            return;
          }
          Ac = (Math.PI * D * D) / 4;
          P = Math.PI * D;
        }

        const m = Math.sqrt((h * P) / (k * Ac));
        const theta_b = Tb - Tinf;
        const mL = m * L;

        // Fin area (sides + tip)
        let Afin;
        if (finType === "rectangular") {
          const b = parseFloat(document.getElementById("rectWidth").value);
          const t = parseFloat(document.getElementById("rectThickness").value);
          const sideArea = 2 * b * L + 2 * t * L;
          const tipArea = b * t;
          Afin = sideArea + tipArea;
        } else {
          const D = parseFloat(document.getElementById("pinDiameter").value);
          const sideArea = Math.PI * D * L;
          const tipArea = (Math.PI * D * D) / 4;
          Afin = sideArea + tipArea;
        }

        // Heat transfer Qfin
        let Qfin;
        if (tipCondition === "adiabatic") {
          Qfin = Math.sqrt(h * P * k * Ac) * theta_b * Math.tanh(mL);
        } else {
          const h_over_mk = h / (m * k);
          const num = Math.sinh(mL) + h_over_mk * Math.cosh(mL);
          const den = Math.cosh(mL) + h_over_mk * Math.sinh(mL);
          Qfin = Math.sqrt(h * P * k * Ac) * theta_b * (num / den);
        }

        const Qmax = h * Afin * theta_b;
        const eta = Qmax > 0 ? Qfin / Qmax : NaN;

        const Ab = wb * 1.0; // base area per meter depth
        const Qbare = h * Ab * theta_b;
        const epsilon = Qbare > 0 ? Qfin / Qbare : NaN;

        // Temperature at x
        let theta_x;
        if (tipCondition === "adiabatic") {
          theta_x = Math.cosh(m * (L - x)) / Math.cosh(mL);
        } else {
          const h_over_mk = h / (m * k);
          const num =
            Math.cosh(m * (L - x)) + h_over_mk * Math.sinh(m * (L - x));
          const den = Math.cosh(mL) + h_over_mk * Math.sinh(mL);
          theta_x = num / den;
        }
        const Tx = Tinf + theta_x * theta_b;

        // Update outputs
        document.getElementById("AcOut").textContent = formatValue(Ac, "m²");
        document.getElementById("POut").textContent = formatValue(P, "m");
        document.getElementById("mOut").textContent = formatValue(m, "1/m");
        document.getElementById("AfinOut").textContent = formatValue(
          Afin,
          "m²"
        );
        document.getElementById("QfinOut").textContent = formatValue(Qfin, "W");
        document.getElementById("etaOut").textContent = isFinite(eta)
          ? eta.toFixed(4) + " (–)"
          : "–";
        document.getElementById("epsilonOut").textContent = isFinite(epsilon)
          ? epsilon.toFixed(2) + " (–)"
          : "–";
        document.getElementById("TxOut").textContent = isFinite(Tx)
          ? Tx.toFixed(2) + " °C"
          : "–";

        // Interpretation
        let interp = [];
        if (isFinite(eta)) {
          if (eta > 0.8)
            interp.push(
              "The fin efficiency is high; the fin material is being used effectively."
            );
          else if (eta > 0.5)
            interp.push(
              "The fin has moderate efficiency; additional length may not be fully utilized."
            );
          else
            interp.push(
              "The fin efficiency is low; the fin may be too long or made of a low‑conductivity material."
            );
        }
        if (isFinite(epsilon)) {
          if (epsilon > 2)
            interp.push(
              "Fin effectiveness is greater than 2, so the fin significantly enhances heat transfer compared to the bare surface."
            );
          else if (epsilon > 1)
            interp.push(
              "Fin effectiveness is between 1 and 2; the fin provides some benefit but may not be optimal."
            );
          else
            interp.push(
              "Fin effectiveness is less than 1; this fin is not worthwhile compared to leaving the surface bare."
            );
        }
        if (interp.length === 0) {
          interp.push(
            "Unable to interpret results due to invalid inputs. Please check your parameters."
          );
        }
        document.getElementById("interpretation").textContent =
          interp.join(" ");
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
