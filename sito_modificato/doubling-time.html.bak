<script>
        function calculateDoublingTime() {
            const rateOfReturn = parseFloat(document.getElementById('rateOfReturn').value);
            let doublingTimeResult = document.getElementById('doublingTimeResult');

            if (isNaN(rateOfReturn) || rateOfReturn <= 0) {
                doublingTimeResult.textContent = 'Please enter a valid positive number for the rate of return.';
                return;
            }

            const doublingTime = (72 / rateOfReturn).toFixed(2);
            doublingTimeResult.textContent = `${doublingTime} years`;
        }
    </script>


