    <script>
      // Tab switching
      const tabMinToDec = document.getElementById("tab-min-to-dec");
      const tabDecToMin = document.getElementById("tab-dec-to-min");
      const panelMinToDec = document.getElementById("panel-min-to-dec");
      const panelDecToMin = document.getElementById("panel-dec-to-min");

      function activateTab(tab) {
        const isMinToDec = tab === "min";
        tabMinToDec.classList.toggle("bg-blue-600", isMinToDec);
        tabMinToDec.classList.toggle("text-white", isMinToDec);
        tabMinToDec.classList.toggle("bg-white", !isMinToDec);
        tabMinToDec.classList.toggle("text-gray-700", !isMinToDec);
        tabMinToDec.setAttribute(
          "aria-selected",
          isMinToDec ? "true" : "false"
        );

        tabDecToMin.classList.toggle("bg-blue-600", !isMinToDec);
        tabDecToMin.classList.toggle("text-white", !isMinToDec);
        tabDecToMin.classList.toggle("bg-white", isMinToDec);
        tabDecToMin.classList.toggle("text-gray-700", isMinToDec);
        tabDecToMin.setAttribute(
          "aria-selected",
          !isMinToDec ? "true" : "false"
        );

        panelMinToDec.classList.toggle("hidden", !isMinToDec);
        panelDecToMin.classList.toggle("hidden", isMinToDec);
      }

      tabMinToDec.addEventListener("click", () => activateTab("min"));
      tabDecToMin.addEventListener("click", () => activateTab("dec"));

      // Minutes to decimal conversion
      const hoursInput = document.getElementById("hoursInput");
      const minutesInput = document.getElementById("minutesInput");
      const roundSelect = document.getElementById("roundSelect");
      const incrementSelect = document.getElementById("incrementSelect");
      const convertBtn = document.getElementById("convertBtn");
      const decimalResult = document.getElementById("decimalResult");
      const roundedTimeLabel = document.getElementById("roundedTimeLabel");
      const copyDecimalBtn = document.getElementById("copyDecimalBtn");
      const resetBtn = document.getElementById("resetBtn");

      function roundToIncrement(minutes, increment) {
        if (!increment || increment <= 0) return minutes;
        return Math.round(minutes / increment) * increment;
      }

      function convertMinutesToDecimal() {
        let h = parseFloat(hoursInput.value);
        let m = parseFloat(minutesInput.value);

        if (isNaN(h)) h = 0;
        if (isNaN(m)) m = 0;

        // If hours is 0 and minutes > 60, treat minutes as total minutes
        if ((h === 0 || isNaN(h)) && m > 60) {
          h = Math.floor(m / 60);
          m = m % 60;
        }

        const increment = parseInt(incrementSelect.value, 10) || 0;
        const roundedMinutes = roundToIncrement(m, increment);
        const totalHours = h + roundedMinutes / 60;

        let display = totalHours;
        const rounding = roundSelect.value;
        if (rounding !== "none") {
          const dp = parseInt(rounding, 10);
          const factor = Math.pow(10, dp);
          display = Math.round(totalHours * factor) / factor;
        }

        decimalResult.textContent = isFinite(display)
          ? display.toFixed(rounding === "none" ? 4 : parseInt(rounding, 10))
          : "0";
        roundedTimeLabel.textContent = `${h} h ${roundedMinutes} min`;
      }

      convertBtn.addEventListener("click", convertMinutesToDecimal);
      hoursInput.addEventListener("input", convertMinutesToDecimal);
      minutesInput.addEventListener("input", convertMinutesToDecimal);
      roundSelect.addEventListener("change", convertMinutesToDecimal);
      incrementSelect.addEventListener("change", convertMinutesToDecimal);

      copyDecimalBtn.addEventListener("click", () => {
        const val = decimalResult.textContent.trim();
        if (!val) return;
        navigator.clipboard.writeText(val).catch(() => {});
      });

      resetBtn.addEventListener("click", () => {
        hoursInput.value = "";
        minutesInput.value = "";
        roundSelect.value = "2";
        incrementSelect.value = "0";
        decimalResult.textContent = "0";
        roundedTimeLabel.textContent = "0 h 0 min";
      });

      // Decimal to h:mm conversion
      const decimalInput = document.getElementById("decimalInput");
      const convertBackBtn = document.getElementById("convertBackBtn");
      const hmResult = document.getElementById("hmResult");
      const totalMinutesResult = document.getElementById("totalMinutesResult");

      function convertDecimalToHM() {
        let dec = parseFloat(decimalInput.value);
        if (isNaN(dec) || dec < 0) {
          hmResult.textContent = "0:00";
          totalMinutesResult.textContent = "0 minutes";
          return;
        }
        const totalMinutes = Math.round(dec * 60);
        const hours = Math.floor(totalMinutes / 60);
        const minutes = totalMinutes % 60;
        hmResult.textContent = `${hours}:${minutes
          .toString()
          .padStart(2, "0")}`;
        totalMinutesResult.textContent = `${totalMinutes} minute${
          totalMinutes === 1 ? "" : "s"
        }`;
      }

      convertBackBtn.addEventListener("click", convertDecimalToHM);
      decimalInput.addEventListener("input", convertDecimalToHM);

      // Quick reference table generation (0–120 minutes, step 5)
      const quickTableBody = document.getElementById("quickTableBody");
      (function buildQuickTable() {
        const rows = [];
        for (let m = 0; m <= 55; m += 5) {
          const leftMin = m;
          const rightMin = m + 60; // second column: +60 minutes
          const leftDec = (leftMin / 60).toFixed(2);
          const rightDec = (rightMin / 60).toFixed(2);
          rows.push(`
        <tr class="odd:bg-white even:bg-gray-50">
          <td class="px-3 py-1 border-b">${leftMin}</td>
          <td class="px-3 py-1 border-b font-mono">${leftDec}</td>
          <td class="px-3 py-1 border-b">${rightMin}</td>
          <td class="px-3 py-1 border-b font-mono">${rightDec}</td>
        </tr>
      `);
        }
        quickTableBody.innerHTML = rows.join("");
      })();
    </script>

