    <script>
      // Business Health Snapshot logic
      function formatCurrency(value) {
        if (!isFinite(value)) return "–";
        return (
          "$" + value.toLocaleString(undefined, { maximumFractionDigits: 2 })
        );
      }

      function formatPercent(value) {
        if (!isFinite(value)) return "–";
        return (
          value.toLocaleString(undefined, { maximumFractionDigits: 1 }) + "%"
        );
      }

      function calculateSnapshot() {
        const price =
          parseFloat(document.getElementById("avg-price").value) || 0;
        const variableCost =
          parseFloat(document.getElementById("variable-cost").value) || 0;
        const fixedCosts =
          parseFloat(document.getElementById("fixed-costs").value) || 0;
        const expectedUnits =
          parseFloat(document.getElementById("expected-units").value) || 0;
        const startingCash =
          parseFloat(document.getElementById("starting-cash").value) || 0;

        const cm = price - variableCost;
        let breakevenUnits = Infinity;
        let breakevenRevenue = Infinity;

        if (cm > 0 && fixedCosts > 0) {
          breakevenUnits = fixedCosts / cm;
          breakevenRevenue = breakevenUnits * price;
        }

        const revenue = price * expectedUnits;
        const totalVariable = variableCost * expectedUnits;
        const profit = revenue - totalVariable - fixedCosts;
        const margin = revenue > 0 ? (profit / revenue) * 100 : NaN;

        let runwayText = "–";
        if (profit > 0) {
          runwayText = "Profitable (cash grows each month)";
        } else if (profit < 0) {
          const months = startingCash > 0 ? startingCash / Math.abs(profit) : 0;
          if (months === 0) {
            runwayText = "No runway (cash already exhausted at this burn rate)";
          } else {
            runwayText =
              months.toLocaleString(undefined, { maximumFractionDigits: 1 }) +
              " months";
          }
        } else if (profit === 0 && startingCash > 0) {
          runwayText = "Indefinite (break-even with current assumptions)";
        }

        document.getElementById("breakeven-units").textContent = isFinite(
          breakevenUnits
        )
          ? breakevenUnits.toLocaleString(undefined, {
              maximumFractionDigits: 1,
            })
          : "Not reachable (price ≤ variable cost)";
        document.getElementById("breakeven-revenue").textContent = isFinite(
          breakevenRevenue
        )
          ? formatCurrency(breakevenRevenue)
          : "–";

        document.getElementById("expected-revenue").textContent =
          formatCurrency(revenue);
        document.getElementById("expected-profit").textContent =
          formatCurrency(profit);
        document.getElementById("expected-margin").textContent = isNaN(margin)
          ? "–"
          : formatPercent(margin);

        document.getElementById("cash-runway").textContent = runwayText;
      }

      document
        .getElementById("snapshot-calc-btn")
        .addEventListener("click", calculateSnapshot);

      // Scenario presets
      const scenarios = {
        lean: { price: 40, variable: 5, fixed: 1500, units: 60, cash: 8000 },
        agency: {
          price: 1200,
          variable: 200,
          fixed: 12000,
          units: 8,
          cash: 25000,
        },
        ecommerce: {
          price: 35,
          variable: 18,
          fixed: 6000,
          units: 400,
          cash: 15000,
        },
        saas: { price: 25, variable: 3, fixed: 20000, units: 900, cash: 50000 },
      };

      document.querySelectorAll(".scenario-btn").forEach((btn) => {
        btn.addEventListener("click", () => {
          const key = btn.getAttribute("data-scenario");
          const s = scenarios[key];
          if (!s) return;
          document.getElementById("avg-price").value = s.price;
          document.getElementById("variable-cost").value = s.variable;
          document.getElementById("fixed-costs").value = s.fixed;
          document.getElementById("expected-units").value = s.units;
          document.getElementById("starting-cash").value = s.cash;
          calculateSnapshot();
        });
      });

      // Auto-calc on load
      document.addEventListener("DOMContentLoaded", calculateSnapshot);
    </script>

