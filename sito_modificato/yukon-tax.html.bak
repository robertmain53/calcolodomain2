    <script>
      // Simple tax bracket engine
      const FED_BRACKETS = [
        { upTo: 55000, rate: 0.15 },
        { upTo: 110000, rate: 0.205 },
        { upTo: 170000, rate: 0.26 },
        { upTo: 270000, rate: 0.29 },
        { upTo: Infinity, rate: 0.33 },
      ];

      const YT_BRACKETS = [
        { upTo: 55000, rate: 0.064 },
        { upTo: 110000, rate: 0.09 },
        { upTo: 170000, rate: 0.109 },
        { upTo: 270000, rate: 0.128 },
        { upTo: Infinity, rate: 0.15 },
      ];

      // Approximate CPP/EI parameters (representative only)
      const CPP_RATE = 0.0595;
      const CPP_BASIC_EXEMPTION = 3500;
      const CPP_MAX_PENSIONABLE = 75000;

      const EI_RATE = 0.0166;
      const EI_MAX_INSURABLE = 63000;

      function calcBracketTax(income, brackets) {
        let remaining = income;
        let lastLimit = 0;
        let tax = 0;
        for (const b of brackets) {
          if (remaining <= 0) break;
          const slice = Math.min(remaining, b.upTo - lastLimit);
          tax += slice * b.rate;
          remaining -= slice;
          lastLimit = b.upTo;
        }
        return Math.max(0, tax);
      }

      function calcCpp(income) {
        const pensionable = Math.min(
          Math.max(0, income - CPP_BASIC_EXEMPTION),
          CPP_MAX_PENSIONABLE - CPP_BASIC_EXEMPTION
        );
        return pensionable * CPP_RATE;
      }

      function calcEi(income) {
        const insurable = Math.min(income, EI_MAX_INSURABLE);
        return insurable * EI_RATE;
      }

      function formatMoney(v) {
        if (!isFinite(v)) return "$0";
        return "$" + v.toFixed(2).replace(/\B(?=(\d{3})+(?!\d))/g, ",");
      }

      function formatPercent(v) {
        if (!isFinite(v)) return "0%";
        return (v * 100).toFixed(1) + "%";
      }

      document.addEventListener("DOMContentLoaded", () => {
        const grossEl = document.getElementById("yt-gross");
        const freqEl = document.getElementById("yt-frequency");
        const typeEl = document.getElementById("yt-income-type");
        const calcBtn = document.getElementById("yt-calc-btn");
        const resetBtn = document.getElementById("yt-reset-btn");
        const errorEl = document.getElementById("yt-error");
        const resultsEl = document.getElementById("yt-results");

        const netPayEl = document.getElementById("yt-net-pay");
        const netAnnualEl = document.getElementById("yt-net-annual");
        const totalDedEl = document.getElementById("yt-total-ded");
        const fedTaxEl = document.getElementById("yt-fed-tax");
        const ytTaxEl = document.getElementById("yt-yt-tax");
        const cppEl = document.getElementById("yt-cpp");
        const eiEl = document.getElementById("yt-ei");
        const fedRateEl = document.getElementById("yt-fed-rate");
        const ytRateEl = document.getElementById("yt-yt-rate");
        const totalRateEl = document.getElementById("yt-total-rate");

        function periodsPerYear(freq) {
          switch (freq) {
            case "monthly":
              return 12;
            case "biweekly":
              return 26;
            case "weekly":
              return 52;
            default:
              return 1;
          }
        }

        calcBtn.addEventListener("click", () => {
          errorEl.classList.add("hidden");
          resultsEl.classList.add("hidden");

          const gross = parseFloat(grossEl.value);
          if (isNaN(gross) || gross <= 0) {
            errorEl.textContent =
              "Please enter a valid gross income greater than 0.";
            errorEl.classList.remove("hidden");
            return;
          }

          const freq = freqEl.value;
          const incomeType = typeEl.value;
          const perYear = periodsPerYear(freq);
          const annualIncome = gross * perYear;

          const fedTaxAnnual = calcBracketTax(annualIncome, FED_BRACKETS);
          const ytTaxAnnual = calcBracketTax(annualIncome, YT_BRACKETS);

          let cppAnnual = 0;
          let eiAnnual = 0;
          if (incomeType === "employment") {
            cppAnnual = calcCpp(annualIncome);
            eiAnnual = calcEi(annualIncome);
          }

          const totalDedAnnual =
            fedTaxAnnual + ytTaxAnnual + cppAnnual + eiAnnual;
          const netAnnual = Math.max(0, annualIncome - totalDedAnnual);
          const netPerPeriod = netAnnual / perYear;

          // Update UI
          netPayEl.textContent = formatMoney(netPerPeriod);
          netAnnualEl.textContent = `≈ ${formatMoney(netAnnual)} per year`;

          totalDedEl.textContent = formatMoney(totalDedAnnual / perYear);
          fedTaxEl.textContent = formatMoney(fedTaxAnnual / perYear);
          ytTaxEl.textContent = formatMoney(ytTaxAnnual / perYear);
          cppEl.textContent = formatMoney(cppAnnual / perYear);
          eiEl.textContent = formatMoney(eiAnnual / perYear);

          fedRateEl.textContent = formatPercent(fedTaxAnnual / annualIncome);
          ytRateEl.textContent = formatPercent(ytTaxAnnual / annualIncome);
          totalRateEl.textContent = formatPercent(
            totalDedAnnual / annualIncome
          );

          resultsEl.classList.remove("hidden");
        });

        resetBtn.addEventListener("click", () => {
          document.getElementById("yt-year").value = "2025";
          freqEl.value = "annual";
          typeEl.value = "employment";
          grossEl.value = 60000;
          errorEl.classList.add("hidden");
          document.getElementById("yt-results").classList.add("hidden");
        });
      });
    </script>

