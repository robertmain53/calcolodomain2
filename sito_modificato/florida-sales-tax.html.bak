    <script>
      // --- Data: illustrative Florida county surtax rates ---
      const FL_STATE_RATE = 0.06;
      const flCountyRates = [
        { county: "Alachua", local: 0.015 },
        { county: "Brevard", local: 0.01 },
        { county: "Broward", local: 0.01 },
        { county: "Duval", local: 0.015 },
        { county: "Hillsborough", local: 0.015 },
        { county: "Lee", local: 0.0075 },
        { county: "Leon", local: 0.015 },
        { county: "Miami-Dade", local: 0.01 },
        { county: "Orange", local: 0.0125 },
        { county: "Osceola", local: 0.015 },
        { county: "Palm Beach", local: 0.01 },
        { county: "Pinellas", local: 0.01 },
        { county: "Polk", local: 0.01 },
        { county: "Sarasota", local: 0.01 },
        { county: "Seminole", local: 0.01 },
        { county: "Volusia", local: 0.005 },
      ];

      // Simple ZIP -> county mapping (very limited, illustrative only)
      const flZipToCounty = {
        32003: "Clay",
        32034: "Nassau",
        32202: "Duval",
        32301: "Leon",
        32701: "Seminole",
        32789: "Orange",
        32801: "Orange",
        33010: "Miami-Dade",
        33101: "Miami-Dade",
        33301: "Broward",
        33401: "Palm Beach",
        33511: "Hillsborough",
        33602: "Hillsborough",
        33701: "Pinellas",
        33901: "Lee",
        34102: "Collier",
        34236: "Sarasota",
        34741: "Osceola",
        34950: "St. Lucie",
      };

      function formatPercent(rate) {
        return (rate * 100).toFixed(2) + "%";
      }

      function formatMoney(value, round) {
        if (isNaN(value)) return "$0.00";
        const v = round ? Math.round(value * 100) / 100 : value;
        return "$" + v.toFixed(2);
      }

      function populateCountySelect() {
        const select = document.getElementById("fl-county");
        select.innerHTML = "";
        const sorted = [...flCountyRates].sort((a, b) =>
          a.county.localeCompare(b.county)
        );
        sorted.forEach((item) => {
          const opt = document.createElement("option");
          const combined = FL_STATE_RATE + item.local;
          opt.value = item.county;
          opt.textContent = `${item.county} (${formatPercent(combined)})`;
          select.appendChild(opt);
        });
      }

      function populateCountyTable() {
        const tbody = document.getElementById("fl-county-table-body");
        tbody.innerHTML = "";
        const sorted = [...flCountyRates].sort((a, b) =>
          a.county.localeCompare(b.county)
        );
        sorted.forEach((item) => {
          const combined = FL_STATE_RATE + item.local;
          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="px-3 py-2">${item.county}</td>
        <td class="px-3 py-2 text-right">${formatPercent(FL_STATE_RATE)}</td>
        <td class="px-3 py-2 text-right">${formatPercent(item.local)}</td>
        <td class="px-3 py-2 text-right font-medium">${formatPercent(
          combined
        )}</td>
      `;
          tbody.appendChild(tr);
        });
      }

      function getRateFromInputs() {
        const mode = document.getElementById("fl-location-mode").value;
        if (mode === "custom") {
          const custom = parseFloat(
            document.getElementById("fl-custom-rate").value
          );
          if (isNaN(custom) || custom < 0) return null;
          return custom / 100;
        }
        if (mode === "county") {
          const countyName = document.getElementById("fl-county").value;
          const entry = flCountyRates.find((c) => c.county === countyName);
          if (!entry) return null;
          return FL_STATE_RATE + entry.local;
        }
        if (mode === "zip") {
          const zip = document.getElementById("fl-zip").value.trim();
          const countyName = flZipToCounty[zip];
          if (!countyName) return null;
          const entry = flCountyRates.find((c) => c.county === countyName);
          if (!entry) return FL_STATE_RATE; // fallback to state-only
          return FL_STATE_RATE + entry.local;
        }
        return null;
      }

      function getLocationDescription() {
        const mode = document.getElementById("fl-location-mode").value;
        if (mode === "custom") {
          return "Custom rate entered by user.";
        }
        if (mode === "county") {
          const countyName = document.getElementById("fl-county").value;
          return countyName
            ? `County selected: ${countyName} (illustrative surtax).`
            : "";
        }
        if (mode === "zip") {
          const zip = document.getElementById("fl-zip").value.trim();
          const countyName = flZipToCounty[zip];
          if (!zip) return "";
          if (!countyName)
            return `ZIP ${zip} not in sample mapping. Using state rate only if calculated.`;
          return `ZIP ${zip} mapped to ${countyName} County (illustrative surtax).`;
        }
        return "";
      }

      function updateAmountLabel() {
        const mode = document.getElementById("fl-calc-mode").value;
        const label = document.getElementById("fl-amount-label");
        if (mode === "before") {
          label.textContent = "Price before tax ($)";
        } else {
          label.textContent = "Total price with tax ($)";
        }
      }

      function updateRateDetailDisplay(rate) {
        const detailEl = document.getElementById("fl-rate-detail");
        const mode = document.getElementById("fl-location-mode").value;
        if (mode === "custom") {
          detailEl.textContent =
            "Custom combined rate (state + local) entered by user.";
          return;
        }
        let local = 0;
        if (mode === "county") {
          const countyName = document.getElementById("fl-county").value;
          const entry = flCountyRates.find((c) => c.county === countyName);
          if (entry) local = entry.local;
        } else if (mode === "zip") {
          const zip = document.getElementById("fl-zip").value.trim();
          const countyName = flZipToCounty[zip];
          const entry = flCountyRates.find((c) => c.county === countyName);
          if (entry) local = entry.local;
        }
        detailEl.textContent = `State ${formatPercent(
          FL_STATE_RATE
        )} + Local ${formatPercent(local)} (illustrative).`;
      }

      function calculateFloridaSalesTax() {
        const amountInput = parseFloat(
          document.getElementById("fl-amount").value
        );
        const mode = document.getElementById("fl-calc-mode").value;
        const round = document.getElementById("fl-round").checked;
        const rate = getRateFromInputs();

        if (isNaN(amountInput) || amountInput < 0 || rate === null) {
          alert(
            "Please enter a valid amount and select a valid location or rate."
          );
          return;
        }

        let taxable, tax, total;
        if (mode === "before") {
          taxable = amountInput;
          tax = taxable * rate;
          total = taxable + tax;
        } else {
          total = amountInput;
          taxable = total / (1 + rate);
          tax = total - taxable;
        }

        const resultsSection = document.getElementById("fl-results-section");
        resultsSection.classList.remove("hidden");

        document.getElementById("fl-rate-display").textContent =
          formatPercent(rate);
        updateRateDetailDisplay(rate);
        document.getElementById("fl-taxable-display").textContent = formatMoney(
          taxable,
          round
        );
        document.getElementById("fl-tax-display").textContent = formatMoney(
          tax,
          round
        );
        document.getElementById("fl-total-display").textContent = formatMoney(
          total,
          round
        );
        document.getElementById("fl-location-summary").textContent =
          getLocationDescription();
      }

      function handleLocationModeChange() {
        const mode = document.getElementById("fl-location-mode").value;
        document
          .getElementById("fl-county-wrapper")
          .classList.toggle("hidden", mode !== "county");
        document
          .getElementById("fl-zip-wrapper")
          .classList.toggle("hidden", mode !== "zip");
        document
          .getElementById("fl-custom-wrapper")
          .classList.toggle("hidden", mode !== "custom");
      }

      document.addEventListener("DOMContentLoaded", () => {
        populateCountySelect();
        populateCountyTable();
        updateAmountLabel();

        document
          .getElementById("fl-location-mode")
          .addEventListener("change", handleLocationModeChange);
        document
          .getElementById("fl-calc-mode")
          .addEventListener("change", updateAmountLabel);
        document
          .getElementById("fl-calc-btn")
          .addEventListener("click", calculateFloridaSalesTax);

        document
          .getElementById("fl-amount")
          .addEventListener("keydown", (e) => {
            if (e.key === "Enter") {
              e.preventDefault();
              calculateFloridaSalesTax();
            }
          });
      });

      // MathJax config
      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
