    <script>
      // Simple responsive line chart without external libraries
      function drawValueChart(points) {
        const canvas = document.getElementById("valueChart");
        const placeholder = document.getElementById("chartPlaceholder");
        if (!canvas) return;

        const ctx = canvas.getContext("2d");
        const width = canvas.parentElement.clientWidth - 32;
        const height = canvas.parentElement.clientHeight - 32;
        canvas.width = width;
        canvas.height = height;

        ctx.clearRect(0, 0, width, height);

        if (!points || points.length < 2) {
          placeholder.classList.remove("hidden");
          return;
        }
        placeholder.classList.add("hidden");

        const padding = { left: 40, right: 10, top: 10, bottom: 25 };
        const xs = points.map((p) => p.yearIndex);
        const ys = points.map((p) => p.value);

        const minX = Math.min.apply(null, xs);
        const maxX = Math.max.apply(null, xs);
        const minY = 0;
        const maxY = Math.max.apply(null, ys) * 1.05;

        function xScale(x) {
          if (maxX === minX) return padding.left;
          return (
            padding.left +
            ((x - minX) * (width - padding.left - padding.right)) /
              (maxX - minX)
          );
        }
        function yScale(y) {
          if (maxY === minY) return height - padding.bottom;
          return (
            height -
            padding.bottom -
            ((y - minY) * (height - padding.top - padding.bottom)) /
              (maxY - minY)
          );
        }

        // Axes
        ctx.strokeStyle = "#d1d5db";
        ctx.lineWidth = 1;
        ctx.beginPath();
        ctx.moveTo(padding.left, padding.top);
        ctx.lineTo(padding.left, height - padding.bottom);
        ctx.lineTo(width - padding.right, height - padding.bottom);
        ctx.stroke();

        // Y labels (0, mid, max)
        ctx.fillStyle = "#6b7280";
        ctx.font = "10px Inter, system-ui, sans-serif";
        const yTicks = [0, maxY / 2, maxY];
        yTicks.forEach((v) => {
          const y = yScale(v);
          ctx.beginPath();
          ctx.moveTo(padding.left - 3, y);
          ctx.lineTo(padding.left, y);
          ctx.stroke();
          ctx.fillText("$" + v.toFixed(0), 2, y + 3);
        });

        // X labels (first, last)
        const first = points[0];
        const last = points[points.length - 1];
        [first, last].forEach((p) => {
          const x = xScale(p.yearIndex);
          ctx.beginPath();
          ctx.moveTo(x, height - padding.bottom);
          ctx.lineTo(x, height - padding.bottom + 3);
          ctx.stroke();
          ctx.fillText(String(p.year), x - 8, height - 5);
        });

        // Line
        ctx.strokeStyle = "#2563eb";
        ctx.lineWidth = 2;
        ctx.beginPath();
        points.forEach((p, i) => {
          const x = xScale(p.yearIndex);
          const y = yScale(p.value);
          if (i === 0) ctx.moveTo(x, y);
          else ctx.lineTo(x, y);
        });
        ctx.stroke();

        // Points
        ctx.fillStyle = "#1d4ed8";
        points.forEach((p) => {
          const x = xScale(p.yearIndex);
          const y = yScale(p.value);
          ctx.beginPath();
          ctx.arc(x, y, 3, 0, 2 * Math.PI);
          ctx.fill();
        });
      }

      function formatCurrency(value) {
        if (!isFinite(value)) return "$0";
        return (
          "$" + value.toLocaleString(undefined, { maximumFractionDigits: 0 })
        );
      }

      function formatMileage(value) {
        if (!isFinite(value)) return "0";
        return (
          value.toLocaleString(undefined, { maximumFractionDigits: 0 }) + " mi"
        );
      }

      document.addEventListener("DOMContentLoaded", function () {
        const now = new Date().getFullYear();
        const purchaseYearInput = document.getElementById("purchaseYear");
        const currentYearInput = document.getElementById("currentYear");
        const annualRateInput = document.getElementById("annualRate");
        const projectionYearsInput = document.getElementById("projectionYears");
        const straightYearsInput = document.getElementById("straightYears");

        if (purchaseYearInput && !purchaseYearInput.value)
          purchaseYearInput.value = now;
        if (currentYearInput && !currentYearInput.value)
          currentYearInput.value = now;
        if (annualRateInput && !annualRateInput.value)
          annualRateInput.value = 18;
        if (projectionYearsInput && !projectionYearsInput.value)
          projectionYearsInput.value = 5;
        if (straightYearsInput && !straightYearsInput.value)
          straightYearsInput.value = 8;

        const methodRadios = document.querySelectorAll('input[name="method"]');
        const decliningOptions = document.getElementById("decliningOptions");
        const straightOptions = document.getElementById("straightOptions");

        methodRadios.forEach((radio) => {
          radio.addEventListener("change", () => {
            if (radio.checked && radio.value === "declining") {
              decliningOptions.classList.remove("hidden");
              straightOptions.classList.add("hidden");
            } else if (radio.checked && radio.value === "straight") {
              straightOptions.classList.remove("hidden");
              decliningOptions.classList.add("hidden");
            }
          });
        });

        document
          .getElementById("calculateBtn")
          .addEventListener("click", function () {
            const purchasePrice = parseFloat(
              document.getElementById("purchasePrice").value
            );
            const purchaseYear = parseInt(purchaseYearInput.value, 10);
            const currentYear = parseInt(currentYearInput.value, 10);
            const currentValueInput = parseFloat(
              document.getElementById("currentValue").value
            );
            const currentMileage =
              parseFloat(document.getElementById("currentMileage").value) || 0;
            const annualMileage =
              parseFloat(document.getElementById("annualMileage").value) ||
              12000;
            const projectionYears =
              parseInt(projectionYearsInput.value, 10) || 5;

            if (
              !purchasePrice ||
              !isFinite(purchasePrice) ||
              purchasePrice <= 0
            ) {
              alert("Please enter a valid purchase price.");
              return;
            }
            if (!purchaseYear || !currentYear || currentYear < purchaseYear) {
              alert(
                "Please enter valid purchase and current years (current year must be >= purchase year)."
              );
              return;
            }

            const method =
              Array.from(methodRadios).find((r) => r.checked)?.value ||
              "declining";

            let schedule = [];
            const ageNow = currentYear - purchaseYear;
            const startYear = currentYear;
            const endYear = currentYear + projectionYears;

            let startingValue;

            if (isFinite(currentValueInput) && currentValueInput > 0) {
              startingValue = currentValueInput;
            } else {
              // Estimate current value from purchase price and age using declining balance with annualRate
              const estRate = parseFloat(annualRateInput.value) / 100 || 0.18;
              startingValue =
                purchasePrice * Math.pow(1 - estRate, Math.max(ageNow, 0));
            }

            if (method === "declining") {
              const r = parseFloat(annualRateInput.value) / 100;
              if (!isFinite(r) || r < 0 || r >= 1) {
                alert(
                  "Please enter an annual depreciation rate between 0 and 100%."
                );
                return;
              }

              let value = startingValue;
              for (let i = 0; i <= projectionYears; i++) {
                const year = startYear + i;
                const age = ageNow + i;
                const mileage = currentMileage + annualMileage * i;
                const prevValue = i === 0 ? value : schedule[i - 1].value;
                if (i > 0) {
                  value = prevValue * (1 - r);
                }
                const annualDep = i === 0 ? 0 : prevValue - value;
                const totalLoss = purchasePrice - value;
                schedule.push({
                  yearIndex: i,
                  year,
                  age,
                  mileage,
                  value: Math.max(value, 0),
                  annualDep: Math.max(annualDep, 0),
                  totalLoss: Math.max(totalLoss, 0),
                });
              }
            } else {
              const usefulLife =
                parseInt(document.getElementById("straightYears").value, 10) ||
                8;
              let salvageValue = parseFloat(
                document.getElementById("salvageValue").value
              );
              if (!isFinite(salvageValue) || salvageValue < 0)
                salvageValue = purchasePrice * 0.15;
              if (salvageValue > purchasePrice) salvageValue = purchasePrice;

              const totalDep = purchasePrice - salvageValue;
              const annualAmount = totalDep / usefulLife;

              // Compute current value based on straight-line schedule
              const ageForStraight = Math.min(ageNow, usefulLife);
              const straightCurrentValue = Math.max(
                purchasePrice - annualAmount * ageForStraight,
                salvageValue
              );
              let value = straightCurrentValue;

              for (let i = 0; i <= projectionYears; i++) {
                const year = startYear + i;
                const age = ageNow + i;
                const mileage = currentMileage + annualMileage * i;
                const prevValue = i === 0 ? value : schedule[i - 1].value;
                if (i > 0) {
                  const theoretical =
                    purchasePrice - annualAmount * (ageForStraight + i);
                  value = Math.max(theoretical, salvageValue);
                }
                const annualDep = i === 0 ? 0 : prevValue - value;
                const totalLoss = purchasePrice - value;
                schedule.push({
                  yearIndex: i,
                  year,
                  age,
                  mileage,
                  value: Math.max(value, 0),
                  annualDep: Math.max(annualDep, 0),
                  totalLoss: Math.max(totalLoss, 0),
                });
              }
            }

            // Render table
            const tbody = document.getElementById("resultsBody");
            tbody.innerHTML = "";
            schedule.forEach((row, idx) => {
              const tr = document.createElement("tr");
              if (idx === 0) tr.classList.add("bg-blue-50");
              tr.innerHTML = `
          <td class="whitespace-nowrap text-xs text-gray-700">${row.year}</td>
          <td class="whitespace-nowrap text-xs text-gray-700">${row.age}</td>
          <td class="whitespace-nowrap text-xs text-gray-700">${formatMileage(
            row.mileage
          )}</td>
          <td class="whitespace-nowrap text-xs font-semibold text-gray-800">${formatCurrency(
            row.value
          )}</td>
          <td class="whitespace-nowrap text-xs text-gray-700">
            ${idx === 0 ? "-" : formatCurrency(row.annualDep)}${
                idx === 0
                  ? ""
                  : ` (${
                      (row.annualDep / (schedule[idx - 1].value || 1)) * 100 <
                      0.01
                        ? "0.0"
                        : (
                            (row.annualDep / (schedule[idx - 1].value || 1)) *
                            100
                          ).toFixed(1)
                    }%)`
              }
          </td>
          <td class="whitespace-nowrap text-xs text-gray-700">
            ${formatCurrency(row.totalLoss)} (${(
                (row.totalLoss / purchasePrice) *
                100
              ).toFixed(1)}%)
          </td>
        `;
              tbody.appendChild(tr);
            });

            // Summary
            const summaryCard = document.getElementById("summaryCard");
            const summaryCurrentValue = document.getElementById(
              "summaryCurrentValue"
            );
            const summaryFutureValue =
              document.getElementById("summaryFutureValue");
            const summaryTotalLoss =
              document.getElementById("summaryTotalLoss");
            const summaryTotalLossPct = document.getElementById(
              "summaryTotalLossPct"
            );
            const summaryYears = document.getElementById("summaryYears");

            const currentRow = schedule[0];
            const futureRow = schedule[schedule.length - 1];

            summaryCurrentValue.textContent = formatCurrency(currentRow.value);
            summaryFutureValue.textContent = formatCurrency(futureRow.value);
            summaryTotalLoss.textContent = formatCurrency(futureRow.totalLoss);
            summaryTotalLossPct.textContent =
              ((futureRow.totalLoss / purchasePrice) * 100).toFixed(1) + "%";
            summaryYears.textContent = projectionYears;
            summaryCard.classList.remove("hidden");

            const badge = document.getElementById("resultsBadge");
            badge.classList.remove("hidden");
            badge.textContent = "Results updated";

            // Chart
            const chartPoints = schedule.map((r) => ({
              yearIndex: r.yearIndex,
              year: r.year,
              value: r.value,
            }));
            drawValueChart(chartPoints);
          });

        // Redraw chart on resize
        window.addEventListener("resize", () => {
          const rows = document.querySelectorAll("#resultsBody tr");
          if (!rows.length || rows[0].children.length < 4) return;
          const schedule = [];
          rows.forEach((row, idx) => {
            const year = parseInt(row.children[0].textContent, 10);
            const valueText = row.children[3].textContent.replace(
              /[^0-9.]/g,
              ""
            );
            const value = parseFloat(valueText) || 0;
            schedule.push({ yearIndex: idx, year, value });
          });
          if (schedule.length > 1) drawValueChart(schedule);
        });
      });
    </script>

