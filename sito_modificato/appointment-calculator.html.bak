    <script>
      // Utility functions
      function parseDateTime(dateStr, timeStr) {
        if (!dateStr) return null;
        const [year, month, day] = dateStr.split("-").map(Number);
        let hours = 0,
          minutes = 0;
        if (timeStr) {
          const parts = timeStr.split(":").map(Number);
          hours = parts[0] ?? 0;
          minutes = parts[1] ?? 0;
        }
        return new Date(year, month - 1, day, hours, minutes, 0, 0);
      }

      function formatDate(d) {
        return d.toLocaleDateString(undefined, {
          year: "numeric",
          month: "short",
          day: "numeric",
          weekday: "short",
        });
      }

      function formatTime(d) {
        return d.toLocaleTimeString(undefined, {
          hour: "2-digit",
          minute: "2-digit",
        });
      }

      function roundToMinutes(date, step) {
        if (!step || step <= 1) return date;
        const ms = date.getTime();
        const stepMs = step * 60 * 1000;
        const rounded = Math.round(ms / stepMs) * stepMs;
        return new Date(rounded);
      }

      function addBusinessDays(date, n) {
        const dir = n >= 0 ? 1 : -1;
        let remaining = Math.abs(n);
        let current = new Date(
          date.getFullYear(),
          date.getMonth(),
          date.getDate(),
          date.getHours(),
          date.getMinutes(),
          date.getSeconds(),
          date.getMilliseconds()
        );
        while (remaining > 0) {
          current.setDate(current.getDate() + dir);
          const day = current.getDay();
          if (day !== 0 && day !== 6) {
            remaining--;
          }
        }
        return current;
      }

      function addMonths(date, n) {
        const d = new Date(date.getTime());
        const day = d.getDate();
        d.setMonth(d.getMonth() + n);
        // handle month overflow (e.g., Feb 30)
        while (d.getDate() < day) {
          d.setDate(d.getDate() - 1);
        }
        return d;
      }

      // Tab switching
      const tabSingle = document.getElementById("tab-single");
      const tabRecurring = document.getElementById("tab-recurring");
      const panelSingle = document.getElementById("panel-single");
      const panelRecurring = document.getElementById("panel-recurring");

      tabSingle.addEventListener("click", () => {
        tabSingle.classList.add("bg-blue-600", "text-white");
        tabSingle.classList.remove("bg-gray-100", "text-gray-700");
        tabRecurring.classList.remove("bg-blue-600", "text-white");
        tabRecurring.classList.add("bg-gray-100", "text-gray-700");
        panelSingle.classList.remove("hidden");
        panelRecurring.classList.add("hidden");
      });

      tabRecurring.addEventListener("click", () => {
        tabRecurring.classList.add("bg-blue-600", "text-white");
        tabRecurring.classList.remove("bg-gray-100", "text-gray-700");
        tabSingle.classList.remove("bg-blue-600", "text-white");
        tabSingle.classList.add("bg-gray-100", "text-gray-700");
        panelRecurring.classList.remove("hidden");
        panelSingle.classList.add("hidden");
      });

      // Single appointment calculation
      document
        .getElementById("btn-calc-single")
        .addEventListener("click", () => {
          const dateStr = document.getElementById("start-date").value;
          const timeStr = document.getElementById("start-time").value;
          const base = parseDateTime(dateStr, timeStr);
          if (!base) {
            alert("Please enter a valid start date (and optionally time).");
            return;
          }

          let offset = parseInt(
            document.getElementById("offset-value").value || "0",
            10
          );
          const unit = document.getElementById("offset-unit").value;
          const direction = document.getElementById("direction").value;
          if (direction === "subtract") offset = -offset;

          let adjusted = new Date(base.getTime());
          if (offset !== 0) {
            switch (unit) {
              case "days":
                adjusted.setDate(adjusted.getDate() + offset);
                break;
              case "business":
                adjusted = addBusinessDays(adjusted, offset);
                break;
              case "weeks":
                adjusted.setDate(adjusted.getDate() + offset * 7);
                break;
              case "months":
                adjusted = addMonths(adjusted, offset);
                break;
              case "years":
                adjusted.setFullYear(adjusted.getFullYear() + offset);
                break;
            }
          }

          const duration = parseInt(
            document.getElementById("duration-min").value || "0",
            10
          );
          const travelBefore = parseInt(
            document.getElementById("travel-before").value || "0",
            10
          );
          const travelAfter = parseInt(
            document.getElementById("travel-after").value || "0",
            10
          );
          const roundStep = parseInt(
            document.getElementById("round-to").value || "1",
            10
          );

          let apptStart = new Date(adjusted.getTime());
          let apptEnd = new Date(apptStart.getTime() + duration * 60000);
          let leaveBy = new Date(apptStart.getTime() - travelBefore * 60000);
          let backFree = new Date(apptEnd.getTime() + travelAfter * 60000);

          apptStart = roundToMinutes(apptStart, roundStep);
          apptEnd = roundToMinutes(apptEnd, roundStep);
          leaveBy = roundToMinutes(leaveBy, roundStep);
          backFree = roundToMinutes(backFree, roundStep);

          const totalBlocked = travelBefore + duration + travelAfter;

          document.getElementById(
            "res-base-datetime"
          ).textContent = `${formatDate(base)} at ${formatTime(base)}`;
          document.getElementById(
            "res-adjusted-datetime"
          ).textContent = `${formatDate(apptStart)} at ${formatTime(
            apptStart
          )}`;
          document.getElementById("res-leave-by").textContent = `${formatDate(
            leaveBy
          )} at ${formatTime(leaveBy)}`;
          document.getElementById("res-back-free").textContent = `${formatDate(
            backFree
          )} at ${formatTime(backFree)}`;
          document.getElementById(
            "res-total-block"
          ).textContent = `${totalBlocked} minutes (${(
            totalBlocked / 60
          ).toFixed(2)} hours) blocked including travel and appointment.`;

          document.getElementById("single-results").classList.remove("hidden");
        });

      document
        .getElementById("btn-reset-single")
        .addEventListener("click", () => {
          document.getElementById("start-date").value = "";
          document.getElementById("start-time").value = "";
          document.getElementById("offset-value").value = "0";
          document.getElementById("offset-unit").value = "days";
          document.getElementById("direction").value = "add";
          document.getElementById("duration-min").value = "60";
          document.getElementById("travel-before").value = "0";
          document.getElementById("travel-after").value = "0";
          document.getElementById("round-to").value = "1";
          document.getElementById("single-results").classList.add("hidden");
        });

      // Recurring appointments
      const recFreq = document.getElementById("rec-frequency");
      const recCustomWrapper = document.getElementById(
        "rec-custom-days-wrapper"
      );

      recFreq.addEventListener("change", () => {
        if (recFreq.value === "custom") {
          recCustomWrapper.classList.remove("hidden");
        } else {
          recCustomWrapper.classList.add("hidden");
        }
      });

      document
        .getElementById("btn-calc-recurring")
        .addEventListener("click", () => {
          const dateStr = document.getElementById("rec-start-date").value;
          const timeStr = document.getElementById("rec-start-time").value;
          const first = parseDateTime(dateStr, timeStr);
          if (!first) {
            alert(
              "Please enter a valid first appointment date (and optionally time)."
            );
            return;
          }

          const freq = recFreq.value;
          const count = Math.min(
            Math.max(
              parseInt(document.getElementById("rec-count").value || "1", 10),
              1
            ),
            365
          );
          const customDays = parseInt(
            document.getElementById("rec-custom-days").value || "1",
            10
          );

          const duration = parseInt(
            document.getElementById("rec-duration-min").value || "0",
            10
          );
          const travelBefore = parseInt(
            document.getElementById("rec-travel-before").value || "0",
            10
          );
          const travelAfter = parseInt(
            document.getElementById("rec-travel-after").value || "0",
            10
          );
          const roundStep = parseInt(
            document.getElementById("rec-round-to").value || "1",
            10
          );

          const tbody = document.getElementById("recurring-results");
          tbody.innerHTML = "";

          let current = new Date(first.getTime());
          let totalMinutes = 0;

          for (let i = 0; i < count; i++) {
            let apptStart = new Date(current.getTime());
            let apptEnd = new Date(apptStart.getTime() + duration * 60000);
            let leaveBy = new Date(apptStart.getTime() - travelBefore * 60000);
            let backFree = new Date(apptEnd.getTime() + travelAfter * 60000);

            apptStart = roundToMinutes(apptStart, roundStep);
            apptEnd = roundToMinutes(apptEnd, roundStep);
            leaveBy = roundToMinutes(leaveBy, roundStep);
            backFree = roundToMinutes(backFree, roundStep);

            totalMinutes += travelBefore + duration + travelAfter;

            const tr = document.createElement("tr");
            tr.innerHTML = `
        <td class="px-2 py-1 text-gray-700">${i + 1}</td>
        <td class="px-2 py-1 text-gray-700 whitespace-nowrap">${formatDate(
          apptStart
        )}</td>
        <td class="px-2 py-1 text-gray-700 whitespace-nowrap">${formatTime(
          apptStart
        )}</td>
        <td class="px-2 py-1 text-gray-700 whitespace-nowrap">${formatTime(
          apptEnd
        )}</td>
        <td class="px-2 py-1 text-gray-700 whitespace-nowrap">${formatTime(
          leaveBy
        )}</td>
        <td class="px-2 py-1 text-gray-700 whitespace-nowrap">${formatTime(
          backFree
        )}</td>
      `;
            tbody.appendChild(tr);

            // advance to next occurrence
            switch (freq) {
              case "daily":
                current.setDate(current.getDate() + 1);
                break;
              case "weekly":
                current.setDate(current.getDate() + 7);
                break;
              case "monthly":
                current = addMonths(current, 1);
                break;
              case "custom":
                current.setDate(current.getDate() + customDays);
                break;
            }
          }

          const totalHours = (totalMinutes / 60).toFixed(2);
          document.getElementById(
            "rec-summary-text"
          ).textContent = `${count} appointments scheduled. Total time blocked including travel: ${totalMinutes} minutes (${totalHours} hours).`;

          document
            .getElementById("recurring-summary")
            .classList.remove("hidden");
          document
            .getElementById("recurring-results-wrapper")
            .classList.remove("hidden");
        });

      document
        .getElementById("btn-reset-recurring")
        .addEventListener("click", () => {
          document.getElementById("rec-start-date").value = "";
          document.getElementById("rec-start-time").value = "";
          document.getElementById("rec-frequency").value = "daily";
          recCustomWrapper.classList.add("hidden");
          document.getElementById("rec-custom-days").value = "10";
          document.getElementById("rec-count").value = "10";
          document.getElementById("rec-duration-min").value = "60";
          document.getElementById("rec-travel-before").value = "0";
          document.getElementById("rec-travel-after").value = "0";
          document.getElementById("rec-round-to").value = "1";
          document.getElementById("recurring-results").innerHTML = "";
          document.getElementById("recurring-summary").classList.add("hidden");
          document
            .getElementById("recurring-results-wrapper")
            .classList.add("hidden");
        });

      // Copy recurring table as text
      document
        .getElementById("btn-copy-recurring")
        .addEventListener("click", () => {
          const tbody = document.getElementById("recurring-results");
          if (!tbody || !tbody.children.length) {
            alert("No schedule to copy yet.");
            return;
          }
          const lines = ["#\tDate\tStart\tEnd\tLeave by\tBack & free"];
          Array.from(tbody.children).forEach((row) => {
            const cells = Array.from(row.children).map((td) =>
              td.textContent.trim()
            );
            lines.push(cells.join("\t"));
          });
          const text = lines.join("\n");
          navigator.clipboard
            .writeText(text)
            .then(() => {
              alert("Schedule copied to clipboard as tab-separated text.");
            })
            .catch(() => {
              alert("Unable to copy to clipboard in this browser.");
            });
        });

      // Prefill today and a reasonable time
      (function initDefaults() {
        const today = new Date();
        const yyyy = today.getFullYear();
        const mm = String(today.getMonth() + 1).padStart(2, "0");
        const dd = String(today.getDate()).padStart(2, "0");
        const dateStr = `${yyyy}-${mm}-${dd}`;
        document.getElementById("start-date").value = dateStr;
        document.getElementById("rec-start-date").value = dateStr;

        const defaultHour = String(
          Math.min(9 + (today.getHours() > 16 ? 0 : 1), 17)
        ).padStart(2, "0");
        const timeStr = `${defaultHour}:00`;
        document.getElementById("start-time").value = timeStr;
        document.getElementById("rec-start-time").value = timeStr;
      })();
    </script>

