    <script>
      // Nature modifiers map: stat -> {natureName: multiplier}
      const natureModifiers = {
        Atk: {
          Lonely: 1.1,
          Brave: 1.1,
          Adamant: 1.1,
          Naughty: 1.1,
          Bold: 0.9,
          Timid: 0.9,
          Modest: 0.9,
          Calm: 0.9,
        },
        Def: {
          Bold: 1.1,
          Relaxed: 1.1,
          Impish: 1.1,
          Lax: 1.1,
          Lonely: 0.9,
          Hasty: 0.9,
          Mild: 0.9,
          Gentle: 0.9,
        },
        SpA: {
          Modest: 1.1,
          Mild: 1.1,
          Quiet: 1.1,
          Rash: 1.1,
          Adamant: 0.9,
          Impish: 0.9,
          Jolly: 0.9,
          Careful: 0.9,
        },
        SpD: {
          Calm: 1.1,
          Gentle: 1.1,
          Sassy: 1.1,
          Careful: 1.1,
          Naughty: 0.9,
          Lax: 0.9,
          Rash: 0.9,
          Naive: 0.9,
        },
        Spe: {
          Timid: 1.1,
          Hasty: 1.1,
          Jolly: 1.1,
          Naive: 1.1,
          Brave: 0.9,
          Relaxed: 0.9,
          Quiet: 0.9,
          Sassy: 0.9,
        },
      };

      function getNatureMultiplier(statKey, nature) {
        if (!nature || nature === "neutral") return 1.0;
        const map = natureModifiers[statKey];
        if (!map) return 1.0;
        return map[nature] || 1.0;
      }

      function calcStatFromIV(statKey, base, iv, ev, level, nature) {
        ev = Math.max(0, Math.min(252, ev || 0));
        base = base || 1;
        level = Math.max(1, Math.min(100, level || 1));
        if (statKey === "HP") {
          // Ignore Shedinja special case; user can handle manually
          const val =
            Math.floor(((2 * base + iv + Math.floor(ev / 4)) * level) / 100) +
            level +
            10;
          return val;
        } else {
          const natureMult = getNatureMultiplier(statKey, nature);
          const baseStat =
            Math.floor(((2 * base + iv + Math.floor(ev / 4)) * level) / 100) +
            5;
          return Math.floor(baseStat * natureMult + 1e-6); // avoid float issues
        }
      }

      function parseCharacteristic(value) {
        if (!value) return null;
        const [stat, modStr] = value.split("-");
        return { stat, mod: parseInt(modStr, 10) };
      }

      function rateIVRange(minIV, maxIV) {
        if (minIV === 31 && maxIV === 31)
          return { label: "Perfect", class: "stat-good" };
        if (minIV >= 28) return { label: "Excellent", class: "stat-good" };
        if (maxIV <= 9) return { label: "Poor", class: "stat-bad" };
        if (minIV >= 20) return { label: "Good", class: "stat-neutral" };
        if (maxIV <= 19) return { label: "Average", class: "stat-neutral" };
        return { label: "Mixed", class: "stat-neutral" };
      }

      function overallSummary(ranges) {
        const ivs = Object.values(ranges).filter((r) => r);
        if (!ivs.length)
          return "No valid IVs could be determined with the current inputs.";
        const avgMin = ivs.reduce((s, r) => s + r.min, 0) / ivs.length;
        const avgMax = ivs.reduce((s, r) => s + r.max, 0) / ivs.length;
        if (avgMin >= 28)
          return "This Pokémon has overall excellent IVs and is suitable for competitive play.";
        if (avgMax <= 10)
          return "This Pokémon has generally low IVs; consider breeding or catching a better one for competitive use.";
        if (avgMin >= 20)
          return "This Pokémon has good overall IVs and is strong for in‑game and casual battles.";
        return "This Pokémon has mixed IVs: some stats are strong, others are average or weak.";
      }

      document.getElementById("calc-btn").addEventListener("click", () => {
        const level = parseInt(document.getElementById("level").value, 10);
        const nature = document.getElementById("nature").value;
        const characteristic = parseCharacteristic(
          document.getElementById("characteristic").value
        );
        const assumeZeroEV = document.getElementById("assume-zero-ev").checked;

        const stats = ["HP", "Atk", "Def", "SpA", "SpD", "Spe"];
        const observed = {};
        const evs = {};
        const bases = {};

        stats.forEach((s) => {
          const statVal = parseInt(
            document.getElementById("stat-" + s).value,
            10
          );
          observed[s] = isNaN(statVal) ? null : statVal;

          const evVal = parseInt(document.getElementById("ev-" + s).value, 10);
          if (isNaN(evVal)) {
            evs[s] = assumeZeroEV ? 0 : 0;
          } else {
            evs[s] = Math.max(0, Math.min(252, evVal));
          }

          const baseVal = parseInt(
            document.getElementById("base-" + s).value,
            10
          );
          bases[s] = isNaN(baseVal) ? null : baseVal;
        });

        const errorEl = document.getElementById("error-msg");
        errorEl.classList.add("hidden");
        errorEl.textContent = "";

        if (
          !observed.HP &&
          !observed.Atk &&
          !observed.Def &&
          !observed.SpA &&
          !observed.SpD &&
          !observed.Spe
        ) {
          errorEl.textContent =
            "Enter at least HP or one other stat to calculate IVs.";
          errorEl.classList.remove("hidden");
          document.getElementById("results-section").classList.add("hidden");
          return;
        }

        const possibleIVs = {};
        stats.forEach((s) => {
          if (!observed[s] || !bases[s]) {
            possibleIVs[s] = null;
            return;
          }
          const list = [];
          for (let iv = 0; iv <= 31; iv++) {
            const val = calcStatFromIV(s, bases[s], iv, evs[s], level, nature);
            if (val === observed[s]) list.push(iv);
          }
          possibleIVs[s] = list.length ? list : null;
        });

        // Apply characteristic constraint if available
        if (characteristic) {
          const { stat: charStat, mod } = characteristic;
          // Characteristic stat must have highest IV (or tied) and IV % 5 == mod
          let maxIV = -1;
          stats.forEach((s) => {
            if (possibleIVs[s]) {
              const localMax = Math.max(...possibleIVs[s]);
              if (localMax > maxIV) maxIV = localMax;
            }
          });
          if (maxIV >= 0) {
            stats.forEach((s) => {
              if (!possibleIVs[s]) return;
              if (s === charStat) {
                possibleIVs[s] = possibleIVs[s].filter(
                  (iv) => iv % 5 === mod && iv >= maxIV - 4
                ); // allow ties near top
              } else {
                // cannot exceed characteristic stat
                possibleIVs[s] = possibleIVs[s].filter((iv) => iv <= maxIV);
              }
              if (!possibleIVs[s].length) possibleIVs[s] = null;
            });
          }
        }

        const ranges = {};
        let anyValid = false;
        stats.forEach((s) => {
          if (possibleIVs[s] && possibleIVs[s].length) {
            const min = Math.min(...possibleIVs[s]);
            const max = Math.max(...possibleIVs[s]);
            ranges[s] = { min, max };
            anyValid = true;
          } else {
            ranges[s] = null;
          }
        });

        if (!anyValid) {
          errorEl.textContent =
            "No IVs are consistent with the current inputs. Double‑check level, stats, EVs and base stats.";
          errorEl.classList.remove("hidden");
          document.getElementById("results-section").classList.add("hidden");
          return;
        }

        const tbody = document.getElementById("iv-table-body");
        tbody.innerHTML = "";
        stats.forEach((s) => {
          const row = document.createElement("tr");
          const labelCell = document.createElement("td");
          labelCell.className = "px-2 py-1 font-medium";
          labelCell.textContent =
            s === "SpA" ? "Sp. Atk" : s === "SpD" ? "Sp. Def" : s;
          row.appendChild(labelCell);

          const rangeCell = document.createElement("td");
          rangeCell.className = "px-2 py-1";
          if (ranges[s]) {
            const { min, max } = ranges[s];
            rangeCell.textContent = min === max ? `${min}` : `${min} – ${max}`;
          } else {
            rangeCell.textContent = "Not enough data";
            rangeCell.classList.add("text-gray-400");
          }
          row.appendChild(rangeCell);

          const ratingCell = document.createElement("td");
          ratingCell.className = "px-2 py-1";
          if (ranges[s]) {
            const { min, max } = ranges[s];
            const rating = rateIVRange(min, max);
            const span = document.createElement("span");
            span.textContent = rating.label;
            span.className = `badge ${rating.class}`;
            ratingCell.appendChild(span);
          } else {
            ratingCell.textContent = "-";
          }
          row.appendChild(ratingCell);

          tbody.appendChild(row);
        });

        document.getElementById("results-summary").textContent =
          overallSummary(ranges);
        document.getElementById("results-section").classList.remove("hidden");

        // Advanced combinations: cartesian product of possible IVs (limited)
        const combosContainer = document.getElementById("combos-container");
        const lists = stats.map((s) => possibleIVs[s] || []);
        const totalCombos = lists.reduce(
          (acc, arr) => acc * (arr.length || 1),
          1
        );
        if (!totalCombos || totalCombos > 5000) {
          combosContainer.textContent =
            totalCombos > 5000
              ? "Too many possible IV combinations to list (>" +
                totalCombos +
                "). Refine inputs (level, EVs, base stats, characteristic) to narrow results."
              : "Not enough information to list specific IV combinations.";
          return;
        }
        const header = "HP / Atk / Def / SpA / SpD / Spe\n";
        const lines = [];
        function buildCombos(idx, current) {
          if (idx === stats.length) {
            lines.push(current.join(" / "));
            return;
          }
          const arr = lists[idx];
          if (!arr.length) {
            current.push("–");
            buildCombos(idx + 1, current);
            current.pop();
          } else {
            for (const v of arr) {
              current.push(v);
              buildCombos(idx + 1, current);
              current.pop();
            }
          }
        }
        buildCombos(0, []);
        combosContainer.textContent = header + lines.join("\n");
      });

      document.getElementById("reset-btn").addEventListener("click", () => {
        ["pokemon-name", "level", "nature", "characteristic"].forEach((id) => {
          const el = document.getElementById(id);
          if (!el) return;
          if (id === "level") el.value = 50;
          else if (id === "nature") el.value = "neutral";
          else if (id === "characteristic") el.value = "";
          else el.value = "";
        });
        ["HP", "Atk", "Def", "SpA", "SpD", "Spe"].forEach((s) => {
          document.getElementById("stat-" + s).value = "";
          document.getElementById("ev-" + s).value = "";
          document.getElementById("base-" + s).value = "";
        });
        document.getElementById("assume-zero-ev").checked = true;
        document.getElementById("results-section").classList.add("hidden");
        const errorEl = document.getElementById("error-msg");
        errorEl.classList.add("hidden");
        errorEl.textContent = "";
      });

      // Mobile menu
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", () => {
          const menu = document.getElementById("mobile-menu");
          menu.classList.toggle("hidden");
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
