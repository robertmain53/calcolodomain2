      (function () {
        const modeTabs = document.querySelectorAll(".mode-tab");
        const beamInputs = document.getElementById("beamInputs");
        const axialInputs = document.getElementById("axialInputs");
        const combinedInputs = document.getElementById("combinedInputs");
        const modeLabel = document.getElementById("calcModeLabel");
        const sectionTypeEl = document.getElementById("sectionType");
        const customPanel = document.getElementById("customSectionPanel");
        const alloyPresetEl = document.getElementById("alloyPreset");

        let currentMode = "beam";

        modeTabs.forEach((btn) => {
          btn.addEventListener("click", () => {
            modeTabs.forEach((b) => {
              b.classList.remove(
                "bg-white",
                "border-gray-200",
                "text-blue-600"
              );
              b.classList.add("border-transparent", "text-gray-600");
            });
            btn.classList.add("bg-white", "border-gray-200", "text-blue-600");
            btn.classList.remove("border-transparent", "text-gray-600");
            currentMode = btn.dataset.mode;
            updateModeVisibility();
          });
        });

        function updateModeVisibility() {
          beamInputs.classList.toggle("hidden", currentMode !== "beam");
          axialInputs.classList.toggle("hidden", currentMode !== "axial");
          combinedInputs.classList.toggle("hidden", currentMode !== "combined");
          if (currentMode === "beam") {
            modeLabel.textContent = "Mode: Beam (bending + shear)";
          } else if (currentMode === "axial") {
            modeLabel.textContent = "Mode: Axial member (tension/compression)";
          } else {
            modeLabel.textContent = "Mode: Combined axial + bending";
          }
        }

        sectionTypeEl.addEventListener("change", () => {
          const v = sectionTypeEl.value;
          if (v === "custom") {
            customPanel.classList.remove("hidden");
          } else {
            customPanel.classList.add("hidden");
          }
        });

        alloyPresetEl.addEventListener("change", () => {
          const f0_2k = document.getElementById("f0_2k");
          const fuk = document.getElementById("fuk");
          if (alloyPresetEl.value === "6082T6") {
            f0_2k.value = 260;
            fuk.value = 300;
          } else if (alloyPresetEl.value === "6061T6") {
            f0_2k.value = 240;
            fuk.value = 290;
          }
        });

        function mmToM(x) {
          return x / 1000.0;
        }
        function kNmToNmm(x) {
          return x * 1e6;
        }
        function kNToN(x) {
          return x * 1e3;
        }

        function computeSectionProps() {
          const type = sectionTypeEl.value;
          const h = parseFloat(document.getElementById("h").value) || 0;
          const b = parseFloat(document.getElementById("b").value) || 0;
          const tw = parseFloat(document.getElementById("tw").value) || 0;
          const tf = parseFloat(document.getElementById("tf").value) || 0;

          let A = 0,
            Wpl_y = 0,
            Av = 0,
            Iy = 0;

          if (type === "iprofile") {
            const Aflange = b * tf * 2;
            const Aweb = tw * (h - 2 * tf);
            A = Aflange + Aweb; // mm2

            const z = (h - tf) / 2;
            const Wflange = b * tf * z;
            const Wweb = (tw * Math.pow(h - 2 * tf, 2)) / 4;
            Wpl_y = 2 * Wflange + Wweb; // mm3

            Av = tw * (h - 2 * tf);

            Iy =
              (b * Math.pow(h, 3)) / 12 -
              ((b - tw) * Math.pow(h - 2 * tf, 3)) / 12;
          } else if (type === "rect") {
            const t = tw;
            A = 2 * (b + h - 2 * t) * t;
            const I_outer = (b * Math.pow(h, 3)) / 12;
            const I_inner = ((b - 2 * t) * Math.pow(h - 2 * t, 3)) / 12;
            Iy = I_outer - I_inner;
            const Z_el = Iy / (h / 2);
            Wpl_y = 1.1 * Z_el;
            Av = 2 * t * (h - 2 * t);
          } else if (type === "custom") {
            A = parseFloat(document.getElementById("A_custom").value) || 0;
            Wpl_y =
              parseFloat(document.getElementById("Wpl_custom").value) || 0;
            Av = parseFloat(document.getElementById("Av_custom").value) || 0;
            Iy = null;
          }

          return { A, Wpl_y, Av, Iy, h, b, tw, tf };
        }

        function classifySection(props, f0_2k) {
          const { h, b, tw, tf } = props;
          if (!h || !b || !tw || !tf) {
            return {
              cls: 3,
              note: "Insufficient data for detailed classification. Assuming Class 3.",
              chiM: 0.9,
            };
          }
          const lambdaWeb = (h - 2 * tf) / tw;
          const lambdaFlange = (b - tw) / (2 * tf);

          let cls = 1;
          if (lambdaWeb > 80 || lambdaFlange > 15) cls = 4;
          else if (lambdaWeb > 60 || lambdaFlange > 12) cls = 3;
          else if (lambdaWeb > 40 || lambdaFlange > 10) cls = 2;
          else cls = 1;

          let chiM = 1.0;
          if (cls === 3) chiM = 0.9;
          if (cls === 4) chiM = 0.75;

          let note = `Estimated web slenderness λ_web ≈ ${lambdaWeb.toFixed(
            1
          )}, flange slenderness λ_flange ≈ ${lambdaFlange.toFixed(1)}. `;
          if (cls === 1) note += "Section behaves plastically (Class 1).";
          else if (cls === 2) note += "Section is compact (Class 2).";
          else if (cls === 3)
            note +=
              "Section is semi‑compact (Class 3) – elastic resistance assumed.";
          else
            note +=
              "Section is slender (Class 4) – effective widths required for precise design.";

          return { cls, note, chiM };
        }

        function runCalculation() {
          const resultsSummary = document.getElementById("resultsSummary");
          const resultsDetail = document.getElementById("resultsDetail");
          const classBadge = document.getElementById("classBadge");
          const classText = document.getElementById("classText");
          const bendingText = document.getElementById("bendingText");
          const shearText = document.getElementById("shearText");
          const axialCard = document.getElementById("axialResultCard");
          const axialText = document.getElementById("axialText");
          const interactionCard = document.getElementById(
            "interactionResultCard"
          );
          const interactionText = document.getElementById("interactionText");
          const intermediateText = document.getElementById("intermediateText");

          const f0_2k = parseFloat(document.getElementById("f0_2k").value) || 0;
          const gammaM =
            parseFloat(document.getElementById("gammaM").value) || 1.1;

          const props = computeSectionProps();
          if (!props.A || !props.Wpl_y || !props.Av) {
            resultsSummary.textContent =
              "Please provide valid section dimensions or custom section properties.";
            resultsDetail.classList.add("hidden");
            return;
          }

          const classInfo = classifySection(props, f0_2k);
          classBadge.textContent = `Class ${classInfo.cls}`;
          classText.textContent = classInfo.note;

          const f0_2d = f0_2k / gammaM;
          const M_Rd_Nmm = classInfo.chiM * f0_2d * props.Wpl_y;
          const M_Rd_kNm = M_Rd_Nmm / 1e6;
          const V_Rd_N = (props.Av * f0_2d) / Math.sqrt(3);
          const V_Rd_kN = V_Rd_N / 1e3;

          let bendingUtil = null;
          let shearUtil = null;
          let axialUtil = null;
          let interactionUtil = null;

          if (currentMode === "beam") {
            const MEd_kNm =
              parseFloat(document.getElementById("MEd").value) || 0;
            const VEd_kN =
              parseFloat(document.getElementById("VEd").value) || 0;
            bendingUtil = M_Rd_kNm > 0 ? MEd_kNm / M_Rd_kNm : Infinity;
            shearUtil = V_Rd_kN > 0 ? VEd_kN / V_Rd_kN : Infinity;

            bendingText.innerHTML = `M<sub>Ed</sub> = ${MEd_kNm.toFixed(
              2
            )} kNm, M<sub>Rd</sub> ≈ ${M_Rd_kNm.toFixed(
              2
            )} kNm → utilization η<sub>M</sub> = ${bendingUtil.toFixed(2)} (${
              bendingUtil <= 1
                ? '<span class="result-pass">OK</span>'
                : '<span class="result-fail">Not OK</span>'
            }).`;
            shearText.innerHTML = `V<sub>Ed</sub> = ${VEd_kN.toFixed(
              2
            )} kN, V<sub>Rd</sub> ≈ ${V_Rd_kN.toFixed(
              2
            )} kN → utilization η<sub>V</sub> = ${shearUtil.toFixed(2)} (${
              shearUtil <= 1
                ? '<span class="result-pass">OK</span>'
                : '<span class="result-fail">Not OK</span>'
            }).`;
            axialCard.classList.add("hidden");
            interactionCard.classList.add("hidden");
          } else if (currentMode === "axial") {
            const NEd_kN =
              parseFloat(document.getElementById("NEd").value) || 0;
            const Lcr_m = parseFloat(document.getElementById("Lcr").value) || 0;
            const A_mm2 = props.A;
            const Npl_Rk_N = A_mm2 * f0_2k;
            const Npl_Rd_kN = Npl_Rk_N / gammaM / 1e3;

            let N_Rd_kN = Npl_Rd_kN;
            let chi = 1.0;
            let note =
              "Tension or low‑slenderness member – plastic resistance used.";

            if (Lcr_m > 0 && props.Iy) {
              const Lcr_mm = Lcr_m * 1000;
              const Ncr_N =
                (Math.PI * Math.PI * 70000 * props.Iy) / (Lcr_mm * Lcr_mm);
              chi = Math.min(1, Ncr_N / Npl_Rk_N);
              N_Rd_kN = chi * Npl_Rd_kN;
              note = `Approximate column buckling: χ ≈ ${chi.toFixed(
                2
              )} based on Euler load.`;
            }

            axialUtil = N_Rd_kN > 0 ? NEd_kN / N_Rd_kN : Infinity;
            axialText.innerHTML = `N<sub>Ed</sub> = ${NEd_kN.toFixed(
              2
            )} kN, N<sub>Rd</sub> ≈ ${N_Rd_kN.toFixed(2)} kN (χ ≈ ${chi.toFixed(
              2
            )}). Utilization η<sub>N</sub> = ${axialUtil.toFixed(2)} (${
              axialUtil <= 1
                ? '<span class="result-pass">OK</span>'
                : '<span class="result-fail">Not OK</span>'
            }).<br><span class="text-xs text-gray-500">${note}</span>`;
            bendingText.textContent = "Not evaluated in axial‑only mode.";
            shearText.textContent = "Not evaluated in axial‑only mode.";
            axialCard.classList.remove("hidden");
            interactionCard.classList.add("hidden");
          } else if (currentMode === "combined") {
            const NEd_kN =
              parseFloat(document.getElementById("NEd_comb").value) || 0;
            const MyEd_kNm =
              parseFloat(document.getElementById("MyEd_comb").value) || 0;
            const MzEd_kNm =
              parseFloat(document.getElementById("MzEd_comb").value) || 0;

            const A_mm2 = props.A;
            const Npl_Rk_N = A_mm2 * f0_2k;
            const Npl_Rd_kN = Npl_Rk_N / gammaM / 1e3;
            const N_Rd_kN = Npl_Rd_kN;

            const My_Rd_kNm = M_Rd_kNm;
            const Mz_Rd_kNm = M_Rd_kNm;

            const etaN = N_Rd_kN > 0 ? NEd_kN / N_Rd_kN : 0;
            const etaMy = My_Rd_kNm > 0 ? MyEd_kNm / My_Rd_kNm : 0;
            const etaMz = Mz_Rd_kNm > 0 ? MzEd_kNm / Mz_Rd_kNm : 0;
            interactionUtil = etaN + etaMy + etaMz;

            interactionText.innerHTML = `η<sub>N</sub> = ${etaN.toFixed(
              2
            )}, η<sub>My</sub> = ${etaMy.toFixed(
              2
            )}, η<sub>Mz</sub> = ${etaMz.toFixed(
              2
            )} → η<sub>int</sub> = ${interactionUtil.toFixed(2)} (${
              interactionUtil <= 1
                ? '<span class="result-pass">OK</span>'
                : '<span class="result-fail">Not OK</span>'
            }).<br><span class="text-xs text-gray-500">Linear interaction used. For final design, apply the exact interaction expressions from EN 1999‑1‑1.</span>`;
            bendingText.textContent = "See interaction check above.";
            shearText.textContent =
              "Shear not explicitly checked in combined mode.";
            axialCard.classList.add("hidden");
            interactionCard.classList.remove("hidden");
          }

          let overallStatus = "Check completed.";
          let worstUtil = 0;
          [bendingUtil, shearUtil, axialUtil, interactionUtil].forEach((v) => {
            if (typeof v === "number" && isFinite(v))
              worstUtil = Math.max(worstUtil, v);
          });
          if (worstUtil > 0) {
            overallStatus =
              `Worst utilization η ≈ ${worstUtil.toFixed(2)} → ` +
              (worstUtil <= 1
                ? "member passes simplified Eurocode 9 checks."
                : "member does NOT pass simplified Eurocode 9 checks.");
          }

          resultsSummary.innerHTML = overallStatus;
          resultsDetail.classList.remove("hidden");

          intermediateText.innerHTML = `
      <div>Gross area A ≈ ${props.A.toFixed(0)} mm²</div>
      <div>Plastic section modulus W<sub>pl,y</sub> ≈ ${props.Wpl_y.toExponential(
        3
      )} mm³</div>
      <div>Shear area A<sub>v</sub> ≈ ${props.Av.toFixed(0)} mm²</div>
      <div>Design strength f<sub>0.2,d</sub> = f<sub>0.2,k</sub> / γ<sub>M</sub> ≈ ${f0_2d.toFixed(
        1
      )} MPa</div>
      <div>M<sub>Rd</sub> ≈ ${M_Rd_kNm.toFixed(2)} kNm</div>
      <div>V<sub>Rd</sub> ≈ ${V_Rd_kN.toFixed(2)} kN</div>
      <div>Section class ≈ ${
        classInfo.cls
      }, reduction factor χ<sub>M</sub> ≈ ${classInfo.chiM.toFixed(2)}</div>
    `;
        }

        document
          .getElementById("runCalc")
          .addEventListener("click", runCalculation);

        document.getElementById("resetCalc").addEventListener("click", () => {
          document.getElementById("L").value = 6;
          document.getElementById("L_unit").value = "m";
          document.getElementById("h").value = 300;
          document.getElementById("b").value = 150;
          document.getElementById("tw").value = 8;
          document.getElementById("tf").value = 12;
          alloyPresetEl.value = "6082T6";
          document.getElementById("f0_2k").value = 260;
          document.getElementById("fuk").value = 300;
          document.getElementById("gammaM").value = 1.1;
          document.getElementById("gammaG").value = 1.35;
          document.getElementById("gammaQ").value = 1.5;
          document.getElementById("MEd").value = 60;
          document.getElementById("VEd").value = 80;
          document.getElementById("NEd").value = 300;
          document.getElementById("Lcr").value = 6;
          document.getElementById("NEd_comb").value = 200;
          document.getElementById("MyEd_comb").value = 40;
          document.getElementById("MzEd_comb").value = 0;
          sectionTypeEl.value = "iprofile";
          customPanel.classList.add("hidden");
          document.getElementById("resultsSummary").textContent =
            "Enter geometry, material and actions, then click “Run Eurocode 9 Check”.";
          document.getElementById("resultsDetail").classList.add("hidden");
        });

        updateModeVisibility();
      })();
    </script>
  
