    <script>
      // Utility: unit factors
      const freqFactors = { Hz: 1, kHz: 1e3, MHz: 1e6 };
      const inductanceFactors = { H: 1, mH: 1e-3, µH: 1e-6, nH: 1e-9 };
      const capacitanceFactors = {
        F: 1,
        mF: 1e-3,
        µF: 1e-6,
        nF: 1e-9,
        pF: 1e-12,
      };

      function formatOhms(value) {
        if (!isFinite(value)) return "—";
        const abs = Math.abs(value);
        let unit = "Ω";
        let scaled = value;
        if (abs >= 1e6) {
          scaled = value / 1e6;
          unit = "MΩ";
        } else if (abs >= 1e3) {
          scaled = value / 1e3;
          unit = "kΩ";
        } else if (abs < 1 && abs >= 1e-3) {
          scaled = value * 1e3;
          unit = "mΩ";
        } else if (abs < 1e-3 && abs > 0) {
          scaled = value * 1e6;
          unit = "µΩ";
        }
        return scaled.toFixed(2) + " " + unit;
      }

      function formatAngle(deg) {
        return deg.toFixed(1) + "°";
      }

      // Tabs
      const tabs = {
        "tab-xl": "panel-xl",
        "tab-xc": "panel-xc",
        "tab-rlc": "panel-rlc",
      };
      Object.keys(tabs).forEach((tabId) => {
        document.getElementById(tabId).addEventListener("click", () => {
          // activate tab
          Object.keys(tabs).forEach((id) => {
            const btn = document.getElementById(id);
            const panel = document.getElementById(tabs[id]);
            if (id === tabId) {
              btn.classList.add("tab-active");
              btn.classList.remove("text-gray-500");
              panel.classList.remove("hidden");
            } else {
              btn.classList.remove("tab-active");
              btn.classList.add("text-gray-500");
              panel.classList.add("hidden");
            }
          });
        });
      });

      // Inductive reactance calculations
      function updateXL(fromSlider = false) {
        const freqInput = document.getElementById("freq-xl");
        const freqUnit = document.getElementById("freq-xl-unit").value;
        const LInput = document.getElementById("inductance");
        const LUnit = document.getElementById("inductance-unit").value;
        const slider = document.getElementById("freq-xl-slider");
        const sliderLabel = document.getElementById("freq-xl-slider-label");

        let f;
        if (fromSlider) {
          f = parseFloat(slider.value);
          freqInput.value = f.toFixed(0);
        } else {
          f = parseFloat(freqInput.value) || 0;
          slider.value = Math.min(Math.max(f, slider.min), slider.max);
        }
        const fHz = f * (freqFactors[freqUnit] || 1);
        sliderLabel.textContent = f.toFixed(0) + " Hz";

        const L =
          (parseFloat(LInput.value) || 0) * (inductanceFactors[LUnit] || 1);
        const omega = 2 * Math.PI * fHz;
        const XL = omega * L;

        document.getElementById("xl-result").textContent = formatOhms(XL);
        document.getElementById("zl-result").textContent = `0 + j${
          formatOhms(XL).split(" ")[0]
        } Ω`;
      }

      document
        .getElementById("freq-xl")
        .addEventListener("input", () => updateXL(false));
      document
        .getElementById("freq-xl-unit")
        .addEventListener("change", () => updateXL(false));
      document
        .getElementById("inductance")
        .addEventListener("input", () => updateXL(false));
      document
        .getElementById("inductance-unit")
        .addEventListener("change", () => updateXL(false));
      document
        .getElementById("freq-xl-slider")
        .addEventListener("input", () => updateXL(true));

      // Capacitive reactance calculations
      function updateXC(fromSlider = false) {
        const freqInput = document.getElementById("freq-xc");
        const freqUnit = document.getElementById("freq-xc-unit").value;
        const CInput = document.getElementById("capacitance");
        const CUnit = document.getElementById("capacitance-unit").value;
        const slider = document.getElementById("freq-xc-slider");
        const sliderLabel = document.getElementById("freq-xc-slider-label");

        let f;
        if (fromSlider) {
          f = parseFloat(slider.value);
          freqInput.value = f.toFixed(0);
        } else {
          f = parseFloat(freqInput.value) || 0;
          slider.value = Math.min(Math.max(f, slider.min), slider.max);
        }
        const fHz = f * (freqFactors[freqUnit] || 1);
        sliderLabel.textContent = f.toFixed(0) + " Hz";

        const C =
          (parseFloat(CInput.value) || 0) * (capacitanceFactors[CUnit] || 1);
        let XC = Infinity;
        if (fHz > 0 && C > 0) {
          XC = 1 / (2 * Math.PI * fHz * C);
        }

        document.getElementById("xc-result").textContent = isFinite(XC)
          ? formatOhms(XC)
          : "∞ Ω";
        const mag = isFinite(XC) ? formatOhms(XC).split(" ")[0] : "∞";
        document.getElementById("zc-result").textContent = `0 − j${mag} Ω`;
      }

      document
        .getElementById("freq-xc")
        .addEventListener("input", () => updateXC(false));
      document
        .getElementById("freq-xc-unit")
        .addEventListener("change", () => updateXC(false));
      document
        .getElementById("capacitance")
        .addEventListener("input", () => updateXC(false));
      document
        .getElementById("capacitance-unit")
        .addEventListener("change", () => updateXC(false));
      document
        .getElementById("freq-xc-slider")
        .addEventListener("input", () => updateXC(true));

      // RLC / impedance calculations
      function updateRLC(fromSlider = false) {
        const freqInput = document.getElementById("freq-rlc");
        const freqUnit = document.getElementById("freq-rlc-unit").value;
        const slider = document.getElementById("freq-rlc-slider");
        const sliderLabel = document.getElementById("freq-rlc-slider-label");

        let f;
        if (fromSlider) {
          f = parseFloat(slider.value);
          freqInput.value = (f / (freqFactors[freqUnit] || 1)).toFixed(2);
        } else {
          const fDisplay = parseFloat(freqInput.value) || 0;
          f = fDisplay * (freqFactors[freqUnit] || 1);
          slider.value = Math.min(Math.max(f, slider.min), slider.max);
        }
        const fHz = f;
        let displayFreq = fHz;
        let displayUnit = "Hz";
        if (fHz >= 1e6) {
          displayFreq = fHz / 1e6;
          displayUnit = "MHz";
        } else if (fHz >= 1e3) {
          displayFreq = fHz / 1e3;
          displayUnit = "kHz";
        }
        sliderLabel.textContent = displayFreq.toFixed(2) + " " + displayUnit;

        const R = parseFloat(document.getElementById("resistance").value) || 0;
        const L =
          (parseFloat(document.getElementById("inductance-rlc").value) || 0) *
          (inductanceFactors[
            document.getElementById("inductance-rlc-unit").value
          ] || 1);
        const C =
          (parseFloat(document.getElementById("capacitance-rlc").value) || 0) *
          (capacitanceFactors[
            document.getElementById("capacitance-rlc-unit").value
          ] || 1);

        const omega = 2 * Math.PI * fHz;
        const XL = omega * L;
        let XC = Infinity;
        if (fHz > 0 && C > 0) {
          XC = 1 / (omega * C);
        }

        const Xnet = isFinite(XC) ? XL - XC : XL;
        const Zmag = Math.sqrt(R * R + Xnet * Xnet);
        const phase = (Math.atan2(Xnet, R) * 180) / Math.PI;

        document.getElementById("xl-rlc").textContent = formatOhms(XL);
        document.getElementById("xc-rlc").textContent = isFinite(XC)
          ? formatOhms(XC)
          : "∞ Ω";

        let nature = "inductive";
        if (Math.abs(Xnet) < 1e-6) nature = "resonant (X ≈ 0)";
        else if (Xnet < 0) nature = "capacitive";

        document.getElementById("xnet-rlc").textContent = `X = ${formatOhms(
          Xnet
        )} (${nature})`;
        const sign = Xnet >= 0 ? "+" : "−";
        const imagMag = formatOhms(Math.abs(Xnet)).split(" ")[0];
        document.getElementById("z-rlc").textContent = `${R.toFixed(
          2
        )} ${sign} j${imagMag} Ω`;
        document.getElementById("zmag-rlc").textContent = `|Z| = ${formatOhms(
          Zmag
        )}`;
        document.getElementById(
          "phase-rlc"
        ).textContent = `Phase φ = ${formatAngle(phase)} (${
          Xnet >= 0 ? "current lags voltage" : "current leads voltage"
        })`;

        // Resonant frequency hint
        let resonanceText = "";
        if (L > 0 && C > 0) {
          const f0 = 1 / (2 * Math.PI * Math.sqrt(L * C));
          const diff = Math.abs(fHz - f0) / f0;
          resonanceText = `Resonant frequency f₀ ≈ ${(f0 / 1000).toFixed(
            2
          )} kHz.`;
          if (diff < 0.05) {
            resonanceText += " You are near resonance (|X| ≈ 0).";
          }
        }
        document.getElementById("resonance-rlc").textContent = resonanceText;
      }

      document
        .getElementById("freq-rlc")
        .addEventListener("input", () => updateRLC(false));
      document
        .getElementById("freq-rlc-unit")
        .addEventListener("change", () => updateRLC(false));
      document
        .getElementById("resistance")
        .addEventListener("input", () => updateRLC(false));
      document
        .getElementById("inductance-rlc")
        .addEventListener("input", () => updateRLC(false));
      document
        .getElementById("inductance-rlc-unit")
        .addEventListener("change", () => updateRLC(false));
      document
        .getElementById("capacitance-rlc")
        .addEventListener("input", () => updateRLC(false));
      document
        .getElementById("capacitance-rlc-unit")
        .addEventListener("change", () => updateRLC(false));
      document
        .getElementById("freq-rlc-slider")
        .addEventListener("input", () => updateRLC(true));

      // Initial calculations
      updateXL(false);
      updateXC(false);
      updateRLC(false);
    </script>

