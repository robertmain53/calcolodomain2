    <script>
      function formatCurrency(value) {
        if (isNaN(value)) return "$0";
        return (
          "$" + value.toLocaleString(undefined, { maximumFractionDigits: 0 })
        );
      }

      function formatPercent(value) {
        if (isNaN(value)) return "–";
        return value.toFixed(2) + "%";
      }

      function formatCurrencyNPV(value) {
        if (isNaN(value)) return "$0";
        return (
          "$" + value.toLocaleString(undefined, { maximumFractionDigits: 0 })
        );
      }

      function calculateNPV(annualCost, ratePct, years) {
        const r = ratePct / 100;
        if (r === 0) return annualCost * years;
        return (annualCost * (1 - Math.pow(1 + r, -years))) / r;
      }

      document.addEventListener("DOMContentLoaded", function () {
        const monitoringInput = document.getElementById("monitoringCost");
        const bondingInput = document.getElementById("bondingCost");
        const residualInput = document.getElementById("residualLoss");
        const firmValueInput = document.getElementById("firmValue");
        const revenueInput = document.getElementById("revenue");
        const yearsInput = document.getElementById("years");
        const discountInput = document.getElementById("discountRate");

        const outMonitoring = document.getElementById("outMonitoringCost");
        const outBonding = document.getElementById("outBondingCost");
        const outResidual = document.getElementById("outResidualLoss");
        const outTotal = document.getElementById("outTotalCost");
        const outPctFirm = document.getElementById("outPctFirm");
        const outPctRevenue = document.getElementById("outPctRevenue");
        const outNPV = document.getElementById("outNPV");
        const interpretationText =
          document.getElementById("interpretationText");
        const resultsBox = document.getElementById("results");

        const sliders = [
          { id: "ownershipDispersion", labelId: "ownershipDispersionVal" },
          { id: "infoAsymmetry", labelId: "infoAsymmetryVal" },
          { id: "managerOwnership", labelId: "managerOwnershipVal" },
        ];
        sliders.forEach((s) => {
          const slider = document.getElementById(s.id);
          const label = document.getElementById(s.labelId);
          slider.addEventListener("input", () => {
            label.textContent = slider.value;
          });
        });

        document
          .getElementById("calculateBtn")
          .addEventListener("click", function () {
            const M = parseFloat(monitoringInput.value) || 0;
            const B = parseFloat(bondingInput.value) || 0;
            const L = parseFloat(residualInput.value) || 0;
            const firmVal = parseFloat(firmValueInput.value) || 0;
            const rev = parseFloat(revenueInput.value) || 0;
            const years = Math.max(1, parseInt(yearsInput.value, 10) || 1);
            const disc = Math.max(0, parseFloat(discountInput.value) || 0);

            const total = M + B + L;
            const pctFirm = firmVal > 0 ? (total / firmVal) * 100 : NaN;
            const pctRevenue = rev > 0 ? (total / rev) * 100 : NaN;
            const npv = calculateNPV(total, disc, years);

            outMonitoring.textContent = formatCurrency(M);
            outBonding.textContent = formatCurrency(B);
            outResidual.textContent = formatCurrency(L);
            outTotal.textContent = formatCurrency(total);
            outPctFirm.textContent = isNaN(pctFirm)
              ? "–"
              : formatPercent(pctFirm);
            outPctRevenue.textContent = isNaN(pctRevenue)
              ? "–"
              : formatPercent(pctRevenue);
            outNPV.textContent = formatCurrencyNPV(npv);

            let interp = "";
            if (!isNaN(pctFirm)) {
              if (pctFirm < 0.5) {
                interp =
                  "Agency costs appear modest relative to firm value. Further reductions may offer limited incremental value unless there are clear governance issues.";
              } else if (pctFirm < 2) {
                interp =
                  "Agency costs are meaningful but not extreme. Review whether monitoring and incentive structures are efficient and targeted.";
              } else {
                interp =
                  "Agency costs are high relative to firm value. Consider whether governance reforms, ownership changes, or restructuring could unlock value.";
              }
            } else {
              interp =
                "Enter firm value and revenue to see agency cost ratios and a more precise interpretation.";
            }
            interpretationText.textContent = interp;

            resultsBox.classList.remove("hidden");
            resultsBox.scrollIntoView({ behavior: "smooth", block: "start" });
          });
      });
    </script>

