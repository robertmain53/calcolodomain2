<script="">
        document.addEventListener('DOMContentLoaded', () => {
            const radInput = document.getElementById('radInput');
            const errorRadInput = document.getElementById('errorRadInput');
            const convertButton = document.getElementById('convertButton');
            const resultOutput = document.getElementById('resultOutput');

            const convertRadToGray = (rads) => {
                return rads * 0.01;
            };

            radInput.addEventListener('blur', () => {
                if (!radInput.value || radInput.value <= 0) {
                    errorRadInput.textContent = 'Please enter a valid positive number for rads.';
                    radInput.setAttribute('aria-invalid', 'true');
                } else {
                    errorRadInput.textContent = '';
                    radInput.setAttribute('aria-invalid', 'false');
                }
            });

            convertButton.addEventListener('click', () => {
                const rads = parseFloat(radInput.value);
                if (rads > 0) {
                    const grays = convertRadToGray(rads);
                    resultOutput.textContent = `Result: ${grays.toFixed(4)} Grays`;
                }
            });
        });
    </script>
