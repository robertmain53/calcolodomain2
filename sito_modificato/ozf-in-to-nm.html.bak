<script>
  (function() {
    const FACTOR_OZFIN_TO_NM = 0.0070615518;
    const FACTOR_NM_TO_OZFIN = 1 / FACTOR_OZFIN_TO_NM; // ~141.61193

    const ozfInInput = document.getElementById('ozfInInput');
    const nmFromOzfIn = document.getElementById('nmFromOzfIn');
    const nmFromOzfInRounded = document.getElementById('nmFromOzfInRounded');

    const nmInput = document.getElementById('nmInput');
    const ozfInFromNm = document.getElementById('ozfInFromNm');
    const ozfInFromNmRounded = document.getElementById('ozfInFromNmRounded');

    function formatNumber(value, decimals) {
      if (!isFinite(value)) return '0';
      return value.toFixed(decimals);
    }

    function updateFromOzfIn() {
      const v = parseFloat(ozfInInput.value);
      if (isNaN(v)) {
        nmFromOzfIn.textContent = '0';
        nmFromOzfInRounded.textContent = 'Rounded: 0 N·m';
        return;
      }
      const nm = v * FACTOR_OZFIN_TO_NM;
      nmFromOzfIn.textContent = nm.toPrecision(8);
      nmFromOzfInRounded.textContent = 'Rounded: ' + formatNumber(nm, 4) + ' N·m';
    }

    function updateFromNm() {
      const v = parseFloat(nmInput.value);
      if (isNaN(v)) {
        ozfInFromNm.textContent = '0';
        ozfInFromNmRounded.textContent = 'Rounded: 0 ozf·in';
        return;
      }
      const ozfin = v * FACTOR_NM_TO_OZFIN;
      ozfInFromNm.textContent = ozfin.toPrecision(8);
      ozfInFromNmRounded.textContent = 'Rounded: ' + formatNumber(ozfin, 2) + ' ozf·in';
    }

    ozfInInput.addEventListener('input', function() {
      updateFromOzfIn();
    });

    nmInput.addEventListener('input', function() {
      updateFromNm();
    });

    // Presets
    document.querySelectorAll('[data-preset-ozfin]').forEach(btn => {
      btn.addEventListener('click', () => {
        const v = btn.getAttribute('data-preset-ozfin');
        ozfInInput.value = v;
        updateFromOzfIn();
        ozfInInput.focus();
      });
    });

    document.querySelectorAll('[data-preset-nm]').forEach(btn => {
      btn.addEventListener('click', () => {
        const v = btn.getAttribute('data-preset-nm');
        nmInput.value = v;
        updateFromNm();
        nmInput.focus();
      });
    });

    // Initialize defaults
    updateFromOzfIn();
    updateFromNm();
  })();
</script>

