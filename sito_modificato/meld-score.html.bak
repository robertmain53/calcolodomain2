    <script>
      // Unit conversion helpers
      function umolL_to_mgdl_creatinine(v) {
        // 1 mg/dL ≈ 88.4 µmol/L
        return v / 88.4;
      }
      function umolL_to_mgdl_bilirubin(v) {
        // 1 mg/dL ≈ 17.1 µmol/L
        return v / 17.1;
      }
      function gL_to_gdl_albumin(v) {
        return v / 10;
      }

      function clamp(value, min, max) {
        return Math.min(max, Math.max(min, value));
      }

      function roundNearestInt(x) {
        return Math.round(x);
      }

      function computeClassicMELD(
        creatinine_mgdl,
        bilirubin_mgdl,
        inr,
        onDialysis
      ) {
        let Cr = creatinine_mgdl;
        if (onDialysis) {
          Cr = 4.0;
        }
        Cr = clamp(Cr, 1.0, 4.0);
        let Bili = Math.max(1.0, bilirubin_mgdl);
        let INR = Math.max(1.0, inr);

        const meld =
          3.78 * Math.log(Bili) +
          11.2 * Math.log(INR) +
          9.57 * Math.log(Cr) +
          6.43;
        let meldRounded = roundNearestInt(meld);
        meldRounded = clamp(meldRounded, 6, 40);
        return { meld, meldRounded, Cr, Bili, INR };
      }

      function computeMELDNa(meldClassicRounded, sodium_mmol) {
        const Na = clamp(sodium_mmol, 125, 137);
        const meldNa =
          meldClassicRounded +
          1.32 * (137 - Na) -
          0.033 * meldClassicRounded * (137 - Na);
        let meldNaRounded = roundNearestInt(meldNa);
        meldNaRounded = clamp(meldNaRounded, 6, 40);
        return { meldNa, meldNaRounded, Na };
      }

      // Simplified MELD 3.0 implementation using a commonly cited parameterization.
      // NOTE: Coefficients are approximate and for educational use only.
      function computeMELD30(
        creatinine_mgdl,
        bilirubin_mgdl,
        inr,
        sodium_mmol,
        albumin_gdl,
        sex
      ) {
        let Cr = creatinine_mgdl;
        Cr = clamp(Cr, 1.0, 3.0); // MELD 3.0 uses slightly different upper bound for Cr
        let Bili = Math.max(1.0, bilirubin_mgdl);
        let INR = Math.max(1.0, inr);
        let Na = clamp(sodium_mmol, 125, 137);
        let Alb = clamp(albumin_gdl, 1.5, 3.5);
        const female = sex === "female" ? 1 : 0;

        // Example MELD 3.0 structure (coefficients illustrative, not for clinical decision-making)
        const meld30 =
          1.33 * Math.log(Bili) +
          0.387 * Math.log(INR) +
          1.12 * Math.log(Cr) +
          0.643 * female -
          0.66 * (Na - 135) -
          0.25 * (Alb - 3.5) +
          6.0;

        let meld30Rounded = roundNearestInt(meld30);
        meld30Rounded = clamp(meld30Rounded, 6, 40);
        return { meld30, meld30Rounded, Cr, Bili, INR, Na, Alb, female };
      }

      function estimateRiskCategory(meldNaRounded) {
        if (isNaN(meldNaRounded)) return { category: "–", detail: "" };
        if (meldNaRounded < 10) {
          return {
            category: "Low short-term risk",
            detail:
              "Estimated 90-day mortality often <2–3% in published cohorts.",
          };
        } else if (meldNaRounded < 20) {
          return {
            category: "Mild–moderate risk",
            detail:
              "Estimated 90-day mortality roughly 6–20%, depending on comorbidities.",
          };
        } else if (meldNaRounded < 30) {
          return {
            category: "Significant risk",
            detail: "Estimated 90-day mortality can reach 20–30% or higher.",
          };
        } else if (meldNaRounded < 40) {
          return {
            category: "High risk",
            detail:
              "Estimated 90-day mortality frequently exceeds 50% in some series.",
          };
        } else {
          return {
            category: "Very high risk",
            detail:
              "Estimated 90-day mortality may exceed 70–80%; urgent specialist review is essential.",
          };
        }
      }

      document
        .getElementById("calculate-btn")
        .addEventListener("click", function () {
          const age = parseFloat(document.getElementById("age").value);
          const sex = document.getElementById("sex").value;

          const creatinineInput = parseFloat(
            document.getElementById("creatinine").value
          );
          const creatinineUnit =
            document.getElementById("creatinine-unit").value;
          const bilirubinInput = parseFloat(
            document.getElementById("bilirubin").value
          );
          const bilirubinUnit = document.getElementById("bilirubin-unit").value;
          const inr = parseFloat(document.getElementById("inr").value);
          const sodiumInput = parseFloat(
            document.getElementById("sodium").value
          );
          const sodiumUnit = document.getElementById("sodium-unit").value;
          const albuminInput = parseFloat(
            document.getElementById("albumin").value
          );
          const albuminUnit = document.getElementById("albumin-unit").value;
          const onDialysis = document.getElementById("dialysis").checked;

          const errors = [];

          if (isNaN(age) || age <= 0) {
            errors.push("Please enter a valid age.");
          } else if (age < 12) {
            errors.push(
              "MELD is not validated for patients under 12 years old (use PELD instead)."
            );
          }

          if (isNaN(creatinineInput) || creatinineInput <= 0)
            errors.push("Enter a valid serum creatinine.");
          if (isNaN(bilirubinInput) || bilirubinInput <= 0)
            errors.push("Enter a valid total bilirubin.");
          if (isNaN(inr) || inr <= 0) errors.push("Enter a valid INR.");
          if (isNaN(sodiumInput) || sodiumInput <= 0)
            errors.push("Enter a valid serum sodium.");
          if (isNaN(albuminInput) || albuminInput <= 0)
            errors.push("Enter a valid serum albumin.");

          if (errors.length > 0) {
            alert(errors.join("\n"));
            return;
          }

          // Convert units to mg/dL, mmol/L, g/dL as needed
          let creatinine_mgdl =
            creatinineUnit === "mgdl"
              ? creatinineInput
              : umolL_to_mgdl_creatinine(creatinineInput);
          let bilirubin_mgdl =
            bilirubinUnit === "mgdl"
              ? bilirubinInput
              : umolL_to_mgdl_bilirubin(bilirubinInput);
          let sodium_mmol = sodiumInput; // mmol/L and mEq/L are numerically equivalent for Na+
          let albumin_gdl =
            albuminUnit === "gdl"
              ? albuminInput
              : gL_to_gdl_albumin(albuminInput);

          // Classic MELD
          const classic = computeClassicMELD(
            creatinine_mgdl,
            bilirubin_mgdl,
            inr,
            onDialysis
          );

          // MELD-Na
          const meldNa = computeMELDNa(classic.meldRounded, sodium_mmol);

          // MELD 3.0
          const meld30 = computeMELD30(
            creatinine_mgdl,
            bilirubin_mgdl,
            inr,
            sodium_mmol,
            albumin_gdl,
            sex
          );

          // Display results
          document.getElementById("meld-classic").textContent = isNaN(
            classic.meldRounded
          )
            ? "–"
            : classic.meldRounded;
          document.getElementById("meld-na").textContent = isNaN(
            meldNa.meldNaRounded
          )
            ? "–"
            : meldNa.meldNaRounded;
          document.getElementById("meld-30").textContent = isNaN(
            meld30.meld30Rounded
          )
            ? "–"
            : meld30.meld30Rounded;

          const risk = estimateRiskCategory(meldNa.meldNaRounded);
          document.getElementById("risk-category").textContent = risk.category;
          document.getElementById("risk-note").textContent =
            risk.detail + " Always interpret in full clinical context.";

          // Normalized values
          const normDiv = document.getElementById("normalized-values");
          normDiv.innerHTML = `
      <p><strong>Creatinine used:</strong> ${classic.Cr.toFixed(2)} mg/dL ${
            onDialysis ? "(dialysis rule applied)" : ""
          }</p>
      <p><strong>Total bilirubin used:</strong> ${classic.Bili.toFixed(
        2
      )} mg/dL</p>
      <p><strong>INR used:</strong> ${classic.INR.toFixed(2)}</p>
      <p><strong>Sodium used (for MELD-Na / MELD 3.0):</strong> ${meldNa.Na.toFixed(
        1
      )} mmol/L</p>
      <p><strong>Albumin used (for MELD 3.0):</strong> ${meld30.Alb.toFixed(
        2
      )} g/dL</p>
      <p><strong>Sex (for MELD 3.0):</strong> ${
        sex === "female" ? "Female" : "Male"
      }</p>
    `;

          document.getElementById("results-section").classList.remove("hidden");
        });

      document
        .getElementById("reset-btn")
        .addEventListener("click", function () {
          document.getElementById("meld-form").reset();
          document.getElementById("results-section").classList.add("hidden");
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
