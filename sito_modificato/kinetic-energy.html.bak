    <script>
      // Kinetic energy calculator logic
      (function () {
        const modeEl = document.getElementById("mode");
        const massEl = document.getElementById("mass");
        const massUnitEl = document.getElementById("massUnit");
        const velocityEl = document.getElementById("velocity");
        const velocityUnitEl = document.getElementById("velocityUnit");
        const energyEl = document.getElementById("energy");
        const energyUnitEl = document.getElementById("energyUnit");
        const calcBtn = document.getElementById("calculateBtn");
        const resetBtn = document.getElementById("resetBtn");
        const swapBtn = document.getElementById("swapBtn");
        const errorMsg = document.getElementById("errorMsg");
        const resultsPanel = document.getElementById("resultsPanel");
        const massSIEl = document.getElementById("massSI");
        const velocitySIEl = document.getElementById("velocitySI");
        const energySIEl = document.getElementById("energySI");
        const energyKJEl = document.getElementById("energyKJ");
        const energyKCalEl = document.getElementById("energyKCal");
        const energyKWhEl = document.getElementById("energyKWh");

        function toKg(value, unit) {
          const v = Number(value);
          switch (unit) {
            case "kg":
              return v;
            case "g":
              return v / 1000;
            case "tonne":
              return v * 1000;
            case "lb":
              return v * 0.45359237;
            case "slug":
              return v * 14.59390294;
            default:
              return NaN;
          }
        }

        function toMS(value, unit) {
          const v = Number(value);
          switch (unit) {
            case "m/s":
              return v;
            case "km/h":
              return v / 3.6;
            case "mph":
              return v * 0.44704;
            case "ft/s":
              return v * 0.3048;
            case "knots":
              return v * 0.514444;
            default:
              return NaN;
          }
        }

        function fromJoules(J, unit) {
          switch (unit) {
            case "J":
              return J;
            case "kJ":
              return J / 1e3;
            case "MJ":
              return J / 1e6;
            case "cal":
              return J / 4.184;
            case "kcal":
              return J / 4184;
            case "kWh":
              return J / 3.6e6;
            case "eV":
              return J / 1.602176634e-19;
            default:
              return J;
          }
        }

        function toJoules(value, unit) {
          const v = Number(value);
          switch (unit) {
            case "J":
              return v;
            case "kJ":
              return v * 1e3;
            case "MJ":
              return v * 1e6;
            case "cal":
              return v * 4.184;
            case "kcal":
              return v * 4184;
            case "kWh":
              return v * 3.6e6;
            case "eV":
              return v * 1.602176634e-19;
            default:
              return v;
          }
        }

        function formatNumber(x) {
          if (!isFinite(x)) return "";
          if (Math.abs(x) >= 1e4 || (Math.abs(x) < 1e-2 && x !== 0)) {
            return x.toExponential(4);
          }
          return x.toLocaleString(undefined, { maximumFractionDigits: 6 });
        }

        function updateModeState() {
          const mode = modeEl.value;
          // Enable/disable fields based on mode
          massEl.readOnly = mode === "m";
          velocityEl.readOnly = mode === "v";
          energyEl.readOnly = mode === "ke";

          massEl.classList.toggle("bg-gray-50", mode === "m");
          velocityEl.classList.toggle("bg-gray-50", mode === "v");
          energyEl.classList.toggle("bg-gray-50", mode === "ke");
        }

        modeEl.addEventListener("change", updateModeState);
        updateModeState();

        calcBtn.addEventListener("click", function () {
          errorMsg.classList.add("hidden");
          resultsPanel.classList.add("hidden");

          const mode = modeEl.value;
          let mVal = massEl.value.trim();
          let vVal = velocityEl.value.trim();
          let eVal = energyEl.value.trim();

          if (mode === "ke") {
            if (!mVal || !vVal) {
              errorMsg.textContent =
                "Enter both mass and velocity to compute kinetic energy.";
              errorMsg.classList.remove("hidden");
              return;
            }
          } else if (mode === "v") {
            if (!mVal || !eVal) {
              errorMsg.textContent =
                "Enter mass and kinetic energy to compute velocity.";
              errorMsg.classList.remove("hidden");
              return;
            }
          } else if (mode === "m") {
            if (!vVal || !eVal) {
              errorMsg.textContent =
                "Enter velocity and kinetic energy to compute mass.";
              errorMsg.classList.remove("hidden");
              return;
            }
          }

          let mSI = null;
          let vSI = null;
          let keJ = null;

          try {
            if (mode === "ke") {
              mSI = toKg(mVal, massUnitEl.value);
              vSI = toMS(vVal, velocityUnitEl.value);
              if (!(mSI > 0) || !(vSI >= 0)) throw new Error();
              keJ = 0.5 * mSI * vSI * vSI;
              const keOut = fromJoules(keJ, energyUnitEl.value);
              energyEl.value = isFinite(keOut) ? keOut : "";
            } else if (mode === "v") {
              mSI = toKg(mVal, massUnitEl.value);
              keJ = toJoules(eVal, energyUnitEl.value);
              if (!(mSI > 0) || !(keJ >= 0)) throw new Error();
              vSI = Math.sqrt((2 * keJ) / mSI);
              const vOut = (function (v, unit) {
                switch (unit) {
                  case "m/s":
                    return v;
                  case "km/h":
                    return v * 3.6;
                  case "mph":
                    return v / 0.44704;
                  case "ft/s":
                    return v / 0.3048;
                  case "knots":
                    return v / 0.514444;
                  default:
                    return v;
                }
              })(vSI, velocityUnitEl.value);
              velocityEl.value = isFinite(vOut) ? vOut : "";
            } else if (mode === "m") {
              vSI = toMS(vVal, velocityUnitEl.value);
              keJ = toJoules(eVal, energyUnitEl.value);
              if (!(vSI > 0) || !(keJ >= 0)) throw new Error();
              mSI = (2 * keJ) / (vSI * vSI);
              const mOut = (function (m, unit) {
                switch (unit) {
                  case "kg":
                    return m;
                  case "g":
                    return m * 1000;
                  case "tonne":
                    return m / 1000;
                  case "lb":
                    return m / 0.45359237;
                  case "slug":
                    return m / 14.59390294;
                  default:
                    return m;
                }
              })(mSI, massUnitEl.value);
              massEl.value = isFinite(mOut) ? mOut : "";
            }
          } catch (e) {
            errorMsg.textContent =
              "Please check your inputs. Mass must be positive and velocity and energy must be non‑negative.";
            errorMsg.classList.remove("hidden");
            return;
          }

          if (!(mSI >= 0) || !(vSI >= 0) || !(keJ >= 0)) {
            errorMsg.textContent =
              "Calculation failed due to invalid values. Please verify your inputs.";
            errorMsg.classList.remove("hidden");
            return;
          }

          // Populate detailed results
          massSIEl.textContent = formatNumber(mSI);
          velocitySIEl.textContent = formatNumber(vSI);
          energySIEl.textContent = formatNumber(keJ);
          energyKJEl.textContent = formatNumber(keJ / 1e3);
          energyKCalEl.textContent = formatNumber(keJ / 4184);
          energyKWhEl.textContent = formatNumber(keJ / 3.6e6);
          resultsPanel.classList.remove("hidden");
        });

        resetBtn.addEventListener("click", function () {
          massEl.value = "";
          velocityEl.value = "";
          energyEl.value = "";
          errorMsg.classList.add("hidden");
          resultsPanel.classList.add("hidden");
        });

        swapBtn.addEventListener("click", function () {
          const massUnit = massUnitEl.value;
          const velUnit = velocityUnitEl.value;
          massUnitEl.value = velUnit === "m/s" ? "kg" : massUnit;
          velocityUnitEl.value = massUnit === "kg" ? "m/s" : velUnit;
        });
      })();

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
