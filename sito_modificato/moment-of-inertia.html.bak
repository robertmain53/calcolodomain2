            Calculators for Everyone
          </p>
        </div>
      </div>
    </footer>

    <script src="/assets/js/mobile-menu.js"></script>
    <script src="/search.js"></script>

    <script>
      // Utility: unit factors
      const lengthFactors = {
        m: 1,
        cm: 0.01,
        mm: 0.001,
        in: 0.0254,
        ft: 0.3048,
      };
      const massFactors = {
        kg: 1,
        g: 0.001,
        lb: 0.45359237,
        slug: 14.59390294,
      };

      const modeTabs = document.querySelectorAll(".mode-tab");
      const shapeSelect = document.getElementById("shape-select");
      const axisSelect = document.getElementById("axis-select");
      const inputsContainer = document.getElementById("inputs-container");
      const parallelContainer = document.getElementById("parallel-container");
      const distanceInput = document.getElementById("distance-d");
      const distanceUnit = document.getElementById("distance-unit");
      const lengthUnit = document.getElementById("length-unit");
      const massUnitWrapper = document.getElementById("mass-unit-wrapper");
      const massUnit = document.getElementById("mass-unit");
      const calcBtn = document.getElementById("calc-btn");
      const resetBtn = document.getElementById("reset-btn");
      const resultsCard = document.getElementById("results-card");
      const resultMain = document.getElementById("result-main");
      const resultSecondary = document.getElementById("result-secondary");
      const resultFormula = document.getElementById("result-formula");
      const shapeDiagram = document.getElementById("shape-diagram");

      let currentMode = "area"; // 'area' or 'mass'

      function setMode(mode) {
        currentMode = mode;
        modeTabs.forEach((btn) => {
          if (btn.dataset.mode === mode) {
            btn.classList.remove("bg-gray-100", "text-gray-700");
            btn.classList.add("bg-blue-600", "text-white");
          } else {
            btn.classList.remove("bg-blue-600", "text-white");
            btn.classList.add("bg-gray-100", "text-gray-700");
          }
        });
        massUnitWrapper.style.display = mode === "mass" ? "block" : "none";
        updateInputs();
      }

      modeTabs.forEach((btn) => {
        btn.addEventListener("click", () => setMode(btn.dataset.mode));
      });

      axisSelect.addEventListener("change", () => {
        parallelContainer.classList.toggle(
          "hidden",
          axisSelect.value !== "parallel"
        );
      });

      shapeSelect.addEventListener("change", updateInputs);
      lengthUnit.addEventListener("change", updateInputs);

      function createNumberInput(id, label, placeholder) {
        const wrapper = document.createElement("div");
        wrapper.innerHTML = `
      <label for="${id}" class="block text-sm font-medium text-gray-700 mb-1">${label}</label>
      <input type="number" id="${id}" class="w-full border border-gray-300 rounded-md px-3 py-2 text-sm focus:outline-none focus:ring-2 focus:ring-blue-500" placeholder="${placeholder}">
    `;
        return wrapper;
      }

      function updateInputs() {
        inputsContainer.innerHTML = "";
        shapeDiagram.classList.add("hidden");
        const shape = shapeSelect.value;
        const Lunit = lengthUnit.value;

        if (shape === "rect") {
          inputsContainer.appendChild(
            createNumberInput("b", `Width b (${Lunit})`, "e.g., 0.2")
          );
          inputsContainer.appendChild(
            createNumberInput("h", `Height h (${Lunit})`, "e.g., 0.4")
          );
          shapeDiagram.classList.remove("hidden");
          shapeDiagram.innerHTML = `<span>Rectangle:</span> width b, height h. Ix about horizontal centroidal axis, Iy about vertical centroidal axis.`;
        } else if (shape === "circle") {
          inputsContainer.appendChild(
            createNumberInput("R", `Radius R (${Lunit})`, "e.g., 0.1")
          );
          shapeDiagram.classList.remove("hidden");
          shapeDiagram.innerHTML = `<span>Solid circle:</span> radius R, axis through center in plane of circle (diameter).`;
        } else if (shape === "hollow-circle") {
          inputsContainer.appendChild(
            createNumberInput(
              "Ro",
              `Outer radius R\u2092 (${Lunit})`,
              "e.g., 0.1"
            )
          );
          inputsContainer.appendChild(
            createNumberInput(
              "Ri",
              `Inner radius R\u1d62 (${Lunit})`,
              "e.g., 0.08"
            )
          );
          shapeDiagram.classList.remove("hidden");
          shapeDiagram.innerHTML = `<span>Hollow circle / tube:</span> outer radius R\u2092, inner radius R\u1d62, axis through center.`;
        } else if (shape === "rect-axial") {
          inputsContainer.appendChild(
            createNumberInput("b", `Width b (${Lunit})`, "e.g., 0.2")
          );
          inputsContainer.appendChild(
            createNumberInput("h", `Height h (${Lunit})`, "e.g., 0.4")
          );
          shapeDiagram.classList.remove("hidden");
          shapeDiagram.innerHTML = `<span>Rectangular plate:</span> axis along one edge, parallel to width b (I = b·h³/3).`;
        } else if (shape === "rod-center") {
          inputsContainer.appendChild(
            createNumberInput("m", `Mass m (${massUnit.value})`, "e.g., 5")
          );
          inputsContainer.appendChild(
            createNumberInput("L", `Length L (${Lunit})`, "e.g., 1.0")
          );
          shapeDiagram.classList.remove("hidden");
          shapeDiagram.innerHTML = `<span>Thin rod (center):</span> axis through center, perpendicular to rod (I = 1/12·m·L²).`;
        } else if (shape === "rod-end") {
          inputsContainer.appendChild(
            createNumberInput("m", `Mass m (${massUnit.value})`, "e.g., 5")
          );
          inputsContainer.appendChild(
            createNumberInput("L", `Length L (${Lunit})`, "e.g., 1.0")
          );
          shapeDiagram.classList.remove("hidden");
          shapeDiagram.innerHTML = `<span>Thin rod (end):</span> axis through one end, perpendicular to rod (I = 1/3·m·L²).`;
        } else if (shape === "disc") {
          inputsContainer.appendChild(
            createNumberInput("m", `Mass m (${massUnit.value})`, "e.g., 2")
          );
          inputsContainer.appendChild(
            createNumberInput("R", `Radius R (${Lunit})`, "e.g., 0.1")
          );
          shapeDiagram.classList.remove("hidden");
          shapeDiagram.innerHTML = `<span>Disc / cylinder:</span> axis through center, perpendicular to face (I = 1/2·m·R²).`;
        } else if (shape === "sphere") {
          inputsContainer.appendChild(
            createNumberInput("m", `Mass m (${massUnit.value})`, "e.g., 2")
          );
          inputsContainer.appendChild(
            createNumberInput("R", `Radius R (${Lunit})`, "e.g., 0.1")
          );
          shapeDiagram.classList.remove("hidden");
          shapeDiagram.innerHTML = `<span>Solid sphere:</span> axis through center (any diameter) (I = 2/5·m·R²).`;
        } else if (shape === "custom") {
          inputsContainer.appendChild(
            createNumberInput("k", "Dimensionless constant k", "e.g., 0.5")
          );
          if (currentMode === "mass") {
            inputsContainer.appendChild(
              createNumberInput("m", `Mass m (${massUnit.value})`, "e.g., 2")
            );
          } else {
            inputsContainer.appendChild(
              createNumberInput("A", `Area A (${Lunit}²)`, "e.g., 0.01")
            );
          }
          inputsContainer.appendChild(
            createNumberInput(
              "L",
              `Characteristic length L (${Lunit})`,
              "e.g., 0.1"
            )
          );
          shapeDiagram.classList.remove("hidden");
          shapeDiagram.innerHTML = `<span>Custom:</span> I = k·m·L² (mass) or I = k·A·L² (area).`;
        }
      }

      function parseInput(id) {
        const el = document.getElementById(id);
        if (!el) return NaN;
        const v = parseFloat(el.value);
        return isNaN(v) ? NaN : v;
      }

      function formatNumber(val) {
        if (!isFinite(val)) return "NaN";
        const abs = Math.abs(val);
        if (abs !== 0 && (abs < 1e-3 || abs >= 1e5)) {
          return val.toExponential(4);
        }
        return val.toLocaleString(undefined, { maximumFractionDigits: 6 });
      }

      calcBtn.addEventListener("click", () => {
        const shape = shapeSelect.value;
        const Lunit = lengthUnit.value;
        const mUnit = massUnit.value;
        const Lfactor = lengthFactors[Lunit] || 1;
        const mfactor = massFactors[mUnit] || 1;

        let I_SI = NaN; // in base SI: m^4 for area, kg·m² for mass
        let formulaText = "";
        let extraText = "";

        if (shape === "rect") {
          const b = parseInput("b");
          const h = parseInput("h");
          if (isNaN(b) || isNaN(h)) {
            alert("Please enter width and height.");
            return;
          }
          const b_m = b * Lfactor;
          const h_m = h * Lfactor;
          const Ix = (b_m * Math.pow(h_m, 3)) / 12;
          const Iy = (h_m * Math.pow(b_m, 3)) / 12;
          I_SI = Ix; // default to Ix
          formulaText = `Rectangle: Ix = b·h³/12, Iy = h·b³/12. Using Ix with b = ${b} ${Lunit}, h = ${h} ${Lunit}.`;
          extraText = `Ix = ${formatNumber(Ix)} m⁴, Iy = ${formatNumber(
            Iy
          )} m⁴.`;
        } else if (shape === "circle") {
          const R = parseInput("R");
          if (isNaN(R)) {
            alert("Please enter radius.");
            return;
          }
          const R_m = R * Lfactor;
          I_SI = (Math.PI * Math.pow(R_m, 4)) / 4;
          formulaText = `Solid circle: I = π·R⁴/4 with R = ${R} ${Lunit}.`;
        } else if (shape === "hollow-circle") {
          const Ro = parseInput("Ro");
          const Ri = parseInput("Ri");
          if (isNaN(Ro) || isNaN(Ri)) {
            alert("Please enter both inner and outer radii.");
            return;
          }
          if (Ri >= Ro) {
            alert("Inner radius must be smaller than outer radius.");
            return;
          }
          const Ro_m = Ro * Lfactor;
          const Ri_m = Ri * Lfactor;
          I_SI = (Math.PI / 4) * (Math.pow(Ro_m, 4) - Math.pow(Ri_m, 4));
          formulaText = `Hollow circle: I = (π/4)·(Rₒ⁴ − Rᵢ⁴) with Rₒ = ${Ro} ${Lunit}, Rᵢ = ${Ri} ${Lunit}.`;
        } else if (shape === "rect-axial") {
          const b = parseInput("b");
          const h = parseInput("h");
          if (isNaN(b) || isNaN(h)) {
            alert("Please enter width and height.");
            return;
          }
          const b_m = b * Lfactor;
          const h_m = h * Lfactor;
          I_SI = (b_m * Math.pow(h_m, 3)) / 3;
          formulaText = `Rectangular plate about edge: I = b·h³/3 with b = ${b} ${Lunit}, h = ${h} ${Lunit}.`;
        } else if (shape === "rod-center") {
          const m = parseInput("m");
          const L = parseInput("L");
          if (isNaN(m) || isNaN(L)) {
            alert("Please enter mass and length.");
            return;
          }
          const m_kg = m * mfactor;
          const L_m = L * Lfactor;
          I_SI = (m_kg * Math.pow(L_m, 2)) / 12;
          formulaText = `Thin rod (center): I = (1/12)·m·L² with m = ${m} ${mUnit}, L = ${L} ${Lunit}.`;
        } else if (shape === "rod-end") {
          const m = parseInput("m");
          const L = parseInput("L");
          if (isNaN(m) || isNaN(L)) {
            alert("Please enter mass and length.");
            return;
          }
          const m_kg = m * mfactor;
          const L_m = L * Lfactor;
          I_SI = (m_kg * Math.pow(L_m, 2)) / 3;
          formulaText = `Thin rod (end): I = (1/3)·m·L² with m = ${m} ${mUnit}, L = ${L} ${Lunit}.`;
        } else if (shape === "disc") {
          const m = parseInput("m");
          const R = parseInput("R");
          if (isNaN(m) || isNaN(R)) {
            alert("Please enter mass and radius.");
            return;
          }
          const m_kg = m * mfactor;
          const R_m = R * Lfactor;
          I_SI = 0.5 * m_kg * Math.pow(R_m, 2);
          formulaText = `Disc: I = (1/2)·m·R² with m = ${m} ${mUnit}, R = ${R} ${Lunit}.`;
        } else if (shape === "sphere") {
          const m = parseInput("m");
          const R = parseInput("R");
          if (isNaN(m) || isNaN(R)) {
            alert("Please enter mass and radius.");
            return;
          }
          const m_kg = m * mfactor;
          const R_m = R * Lfactor;
          I_SI = (2 / 5) * m_kg * Math.pow(R_m, 2);
          formulaText = `Solid sphere: I = (2/5)·m·R² with m = ${m} ${mUnit}, R = ${R} ${Lunit}.`;
        } else if (shape === "custom") {
          const k = parseInput("k");
          const L = parseInput("L");
          if (isNaN(k) || isNaN(L)) {
            alert("Please enter k and length.");
            return;
          }
          const L_m = L * Lfactor;
          if (currentMode === "mass") {
            const m = parseInput("m");
            if (isNaN(m)) {
              alert("Please enter mass.");
              return;
            }
            const m_kg = m * mfactor;
            I_SI = k * m_kg * Math.pow(L_m, 2);
            formulaText = `Custom mass moment: I = k·m·L² with k = ${k}, m = ${m} ${mUnit}, L = ${L} ${Lunit}.`;
          } else {
            const A = parseInput("A");
            if (isNaN(A)) {
              alert("Please enter area.");
              return;
            }
            // Area is given in Lunit^2; convert to m²
            const A_m2 = A * Math.pow(Lfactor, 2);
            I_SI = k * A_m2 * Math.pow(L_m, 2);
            formulaText = `Custom area moment: I = k·A·L² with k = ${k}, A = ${A} ${Lunit}², L = ${L} ${Lunit}.`;
          }
        }

        if (axisSelect.value === "parallel") {
          const d = parseInput("distance-d");
          if (isNaN(d)) {
            alert("Please enter distance d for the parallel axis.");
            return;
          }
          const d_m = d * (lengthFactors[distanceUnit.value] || 1);
          if (currentMode === "mass") {
            // Need total mass; approximate from inputs where possible
            let m_kg = NaN;
            if (
              shape === "rod-center" ||
              shape === "rod-end" ||
              shape === "disc" ||
              shape === "sphere" ||
              shape === "custom"
            ) {
              const m = parseInput("m");
              if (!isNaN(m)) m_kg = m * mfactor;
            }
            if (!isNaN(m_kg)) {
              I_SI = I_SI + m_kg * d_m * d_m;
              extraText +=
                (extraText ? " " : "") +
                `Parallel‑axis: I = Ic + m·d² with d = ${d} ${distanceUnit.value}.`;
            } else {
              extraText +=
                (extraText ? " " : "") +
                `Parallel‑axis adjustment requires total mass m.`;
            }
          } else {
            // area moment: approximate area where possible
            let A_m2 = NaN;
            if (shape === "rect" || shape === "rect-axial") {
              const b = parseInput("b");
              const h = parseInput("h");
              if (!isNaN(b) && !isNaN(h)) {
                A_m2 = b * Lfactor * (h * Lfactor);
              }
            } else if (shape === "circle") {
              const R = parseInput("R");
              if (!isNaN(R)) {
                const R_m = R * Lfactor;
                A_m2 = Math.PI * R_m * R_m;
              }
            } else if (shape === "hollow-circle") {
              const Ro = parseInput("Ro");
              const Ri = parseInput("Ri");
              if (!isNaN(Ro) && !isNaN(Ri)) {
                const Ro_m = Ro * Lfactor;
                const Ri_m = Ri * Lfactor;
                A_m2 = Math.PI * (Ro_m * Ro_m - Ri_m * Ri_m);
              }
            } else if (shape === "custom") {
              const A = parseInput("A");
              if (!isNaN(A)) {
                A_m2 = A * Math.pow(Lfactor, 2);
              }
            }
            if (!isNaN(A_m2)) {
              I_SI = I_SI + A_m2 * d_m * d_m;
              extraText +=
                (extraText ? " " : "") +
                `Parallel‑axis: I = Ic + A·d² with d = ${d} ${distanceUnit.value}.`;
            } else {
              extraText +=
                (extraText ? " " : "") +
                `Parallel‑axis adjustment requires total area A.`;
            }
          }
        }

        if (!isFinite(I_SI)) {
          alert(
            "Unable to compute moment of inertia. Please check your inputs."
          );
          return;
        }

        // Prepare output units
        let mainUnit, secondaryUnit, I_secondary;
        if (currentMode === "area") {
          // base is m^4; convert to chosen length unit^4
          const factorToUser = Math.pow(1 / Lfactor, 4);
          const I_user = I_SI * factorToUser;
          mainUnit = `${Lunit}⁴`;
          resultMain.textContent = `I = ${formatNumber(I_user)} ${mainUnit}`;
          secondaryUnit = "m⁴";
          I_secondary = I_SI;
          resultSecondary.textContent = `In SI base units: I = ${formatNumber(
            I_secondary
          )} ${secondaryUnit}`;
        } else {
          // base is kg·m²; convert to chosen mass and length units
          const factorToUser = (1 / mfactor) * Math.pow(1 / Lfactor, 2);
          const I_user = I_SI * factorToUser;
          mainUnit = `${mUnit}·${Lunit}²`;
          resultMain.textContent = `I = ${formatNumber(I_user)} ${mainUnit}`;
          secondaryUnit = "kg·m²";
          I_secondary = I_SI;
          resultSecondary.textContent = `In SI base units: I = ${formatNumber(
            I_secondary
          )} ${secondaryUnit}`;
        }

        resultFormula.textContent =
          formulaText + (extraText ? " " + extraText : "");
        resultsCard.classList.remove("hidden");
      });

      resetBtn.addEventListener("click", () => {
        document
          .querySelectorAll("#inputs-container input")
          .forEach((inp) => (inp.value = ""));
        distanceInput.value = "";
        resultsCard.classList.add("hidden");
      });

      // Initialize
      document.addEventListener("DOMContentLoaded", () => {
        setMode("area");
        axisSelect.value = "default";
        parallelContainer.classList.add("hidden");
        updateInputs();
      });
    </script>

    <script>
      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
