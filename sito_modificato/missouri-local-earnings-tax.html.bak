    <script>
      // Utility functions
      function formatCurrency(value) {
        if (isNaN(value)) return "$0.00";
        return value.toLocaleString("en-US", {
          style: "currency",
          currency: "USD",
        });
      }
      function formatPercent(value) {
        if (isNaN(value)) return "0%";
        return value.toFixed(2) + "%";
      }

      function createIncomeRow(
        description = "",
        amount = "",
        percentInCity = ""
      ) {
        const row = document.createElement("div");
        row.className = "px-3 py-2 input-grid text-sm";

        row.innerHTML = `
      <div>
        <label class="md:hidden block text-xs text-gray-500 mb-0.5">Description</label>
        <input type="text" class="income-desc w-full border border-gray-300 rounded-md px-2 py-1" placeholder="e.g. Main job" value="${description}">
      </div>
      <div>
        <label class="md:hidden block text-xs text-gray-500 mb-0.5">Annual amount ($)</label>
        <input type="number" min="0" step="0.01" class="income-amount w-full border border-gray-300 rounded-md px-2 py-1 text-right" placeholder="0.00" value="${amount}">
      </div>
      <div>
        <label class="md:hidden block text-xs text-gray-500 mb-0.5">% of work in city</label>
        <input type="number" min="0" max="100" step="0.1" class="income-percent-city w-full border border-gray-300 rounded-md px-2 py-1 text-right" placeholder="e.g. 50" value="${percentInCity}">
      </div>
      <div class="flex justify-center">
        <button type="button" class="remove-income-row text-red-600 hover:text-red-800 text-xs md:text-sm">Remove</button>
      </div>
    `;
        return row;
      }

      document.addEventListener("DOMContentLoaded", function () {
        const incomeRowsContainer = document.getElementById("income-rows");
        const addIncomeRowBtn = document.getElementById("add-income-row");
        const calculateBtn = document.getElementById("calculate-btn");
        const resetBtn = document.getElementById("reset-btn");
        const errorEl = document.getElementById("calc-error");
        const resultsContent = document.getElementById("results-content");

        function initRows() {
          incomeRowsContainer.innerHTML = "";
          incomeRowsContainer.appendChild(
            createIncomeRow("Main job", "", "100")
          );
        }

        addIncomeRowBtn.addEventListener("click", () => {
          incomeRowsContainer.appendChild(createIncomeRow());
        });

        incomeRowsContainer.addEventListener("click", (e) => {
          if (e.target.classList.contains("remove-income-row")) {
            const rows = incomeRowsContainer.querySelectorAll(".input-grid");
            if (rows.length > 1) {
              e.target.closest(".input-grid").remove();
            }
          }
        });

        resetBtn.addEventListener("click", () => {
          document.getElementById("city").value = "kc";
          document.getElementById("resident-status").value = "resident";
          document.getElementById("tax-year").value = "2025";
          document.getElementById("work-days-total").value = "";
          document.getElementById("work-days-city").value = "";
          initRows();
          errorEl.classList.add("hidden");
          resultsContent.classList.add("hidden");
        });

        calculateBtn.addEventListener("click", () => {
          errorEl.classList.add("hidden");
          errorEl.textContent = "";

          const city = document.getElementById("city").value;
          const residentStatus =
            document.getElementById("resident-status").value;
          const workDaysTotal = parseFloat(
            document.getElementById("work-days-total").value
          );
          const workDaysCity = parseFloat(
            document.getElementById("work-days-city").value
          );

          const rows = incomeRowsContainer.querySelectorAll(".input-grid");
          let totalIncome = 0;
          let taxableIncome = 0;
          let breakdownLines = [];

          if (rows.length === 0) {
            errorEl.textContent = "Please add at least one income source.";
            errorEl.classList.remove("hidden");
            return;
          }

          // Determine days-based apportionment if valid
          let daysFactor = null;
          if (
            !isNaN(workDaysTotal) &&
            workDaysTotal > 0 &&
            !isNaN(workDaysCity) &&
            workDaysCity >= 0
          ) {
            daysFactor = Math.min(Math.max(workDaysCity / workDaysTotal, 0), 1);
          }

          rows.forEach((row, index) => {
            const amountInput = row.querySelector(".income-amount");
            const percentInput = row.querySelector(".income-percent-city");
            const descInput = row.querySelector(".income-desc");

            const amount = parseFloat(amountInput.value);
            const percentCity = parseFloat(percentInput.value);
            const desc = descInput.value.trim() || `Income ${index + 1}`;

            if (!isNaN(amount) && amount > 0) {
              totalIncome += amount;

              let factor;
              if (residentStatus === "resident") {
                factor = 1; // residents generally taxed on all compensation
              } else {
                // non-resident: use blend of daysFactor and manual percent if available
                let manualFactor = null;
                if (!isNaN(percentCity) && percentCity >= 0) {
                  manualFactor = Math.min(Math.max(percentCity / 100, 0), 1);
                }

                if (daysFactor !== null && manualFactor !== null) {
                  // simple average of the two approaches
                  factor = (daysFactor + manualFactor) / 2;
                } else if (daysFactor !== null) {
                  factor = daysFactor;
                } else if (manualFactor !== null) {
                  factor = manualFactor;
                } else {
                  factor = 1; // fallback: assume all in city
                }
              }

              const taxableForRow = amount * factor;
              taxableIncome += taxableForRow;

              breakdownLines.push(
                `${desc}: ${formatCurrency(amount)} × ${(factor * 100).toFixed(
                  1
                )}% in city = ${formatCurrency(taxableForRow)} taxable`
              );
            }
          });

          if (totalIncome <= 0) {
            errorEl.textContent =
              "Please enter at least one positive income amount.";
            errorEl.classList.remove("hidden");
            return;
          }

          const rate = 0.01; // 1%
          const tax = taxableIncome * rate;
          const effectiveRate = (tax / totalIncome) * 100;

          // Populate results
          const cityName =
            city === "kc" ? "Kansas City, Missouri" : "St. Louis, Missouri";
          const residencyLabel =
            residentStatus === "resident" ? "Resident" : "Non-resident";

          document.getElementById("result-city").textContent = cityName;
          document.getElementById("result-residency").textContent =
            residencyLabel;
          document.getElementById("result-total-income").textContent =
            formatCurrency(totalIncome);
          document.getElementById("result-taxable-income").textContent =
            formatCurrency(taxableIncome);
          document.getElementById("result-rate").textContent = "1.00%";
          document.getElementById("result-tax").textContent =
            formatCurrency(tax);
          document.getElementById("result-effective-rate").textContent =
            formatPercent(effectiveRate);

          const breakdownEl = document.getElementById("result-breakdown");
          let breakdownHtml =
            '<h4 class="font-semibold mb-1">Breakdown</h4><ul class="list-disc ml-5">';
          breakdownLines.forEach((line) => {
            breakdownHtml += `<li>${line}</li>`;
          });
          breakdownHtml += "</ul>";

          if (residentStatus === "nonresident") {
            breakdownHtml +=
              '<p class="mt-2 text-xs text-gray-600">For non-residents, taxable income is estimated using your days worked in the city and/or the percentage of work performed in the city. Actual refund calculations may require official city forms and documentation.</p>';
          }

          breakdownEl.innerHTML = breakdownHtml;

          resultsContent.classList.remove("hidden");
        });

        // Initialize default row
        initRows();
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
