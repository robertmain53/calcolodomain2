    <script>
      (function () {
        const currencySelect = document.getElementById("currency-select");
        const prefixes = [
          "cash-prefix",
          "securities-prefix",
          "receivables-prefix",
          "inventory-prefix",
          "liabilities-prefix",
        ].map((id) => document.getElementById(id));

        const cashInput = document.getElementById("cash-input");
        const securitiesInput = document.getElementById("securities-input");
        const receivablesInput = document.getElementById("receivables-input");
        const inventoryToggle = document.getElementById(
          "include-inventory-toggle"
        );
        const inventoryWrapper = document.getElementById(
          "inventory-input-wrapper"
        );
        const inventoryInput = document.getElementById("inventory-input");
        const liabilitiesInput = document.getElementById(
          "current-liabilities-input"
        );

        const quickAssetsOutput = document.getElementById(
          "quick-assets-output"
        );
        const quickAssetsWithInventoryRow = document.getElementById(
          "quick-assets-with-inventory-row"
        );
        const quickAssetsWithInventoryOutput = document.getElementById(
          "quick-assets-with-inventory-output"
        );
        const liabilitiesOutput = document.getElementById("liabilities-output");
        const quickRatioOutput = document.getElementById("quick-ratio-output");
        const quickRatioBadge = document.getElementById("quick-ratio-badge");
        const quickRatioInterpretation = document.getElementById(
          "quick-ratio-interpretation"
        );
        const quickRatioWithInventorySection = document.getElementById(
          "quick-ratio-with-inventory-section"
        );
        const quickRatioWithInventoryOutput = document.getElementById(
          "quick-ratio-with-inventory-output"
        );
        const quickRatioWithInventoryBadge = document.getElementById(
          "quick-ratio-with-inventory-badge"
        );

        const calculateBtn = document.getElementById("calculate-btn");
        const resetBtn = document.getElementById("reset-btn");

        function fmtCurrency(value, symbol) {
          if (isNaN(value)) return "—";
          const abs = Math.abs(value);
          let formatted;
          if (abs >= 1e9) {
            formatted = (value / 1e9).toFixed(2) + "B";
          } else if (abs >= 1e6) {
            formatted = (value / 1e6).toFixed(2) + "M";
          } else if (abs >= 1e3) {
            formatted = (value / 1e3).toFixed(2) + "K";
          } else {
            formatted = value.toFixed(2);
          }
          return (symbol || "") + formatted;
        }

        function fmtRatio(value) {
          if (!isFinite(value)) return "—";
          if (Math.abs(value) >= 1000) return value.toFixed(0);
          if (Math.abs(value) >= 100) return value.toFixed(1);
          return value.toFixed(2);
        }

        function classifyRatio(ratio) {
          if (!isFinite(ratio) || isNaN(ratio))
            return {
              label: "N/A",
              color: "bg-gray-200 text-gray-700",
              text: "",
            };

          if (ratio < 0.75) {
            return {
              label: "Weak liquidity",
              color: "bg-red-100 text-red-800",
              text: "Your quick ratio is below 0.75. This can indicate liquidity risk – consider strengthening your cash position or reducing short‑term obligations.",
            };
          } else if (ratio < 1.0) {
            return {
              label: "Below 1.0",
              color: "bg-amber-100 text-amber-800",
              text: "Your quick ratio is below 1.0. You may need to rely on inventory sales, new borrowing, or equity to cover near‑term liabilities.",
            };
          } else if (ratio <= 2.0) {
            return {
              label: "Healthy",
              color: "bg-green-100 text-green-800",
              text: "Your quick ratio is in a range often considered healthy, though the ideal level depends on your industry and business model.",
            };
          } else {
            return {
              label: "Very strong",
              color: "bg-blue-100 text-blue-800",
              text: "Your quick ratio is very strong. Ensure excess cash is not sitting idle and that capital is being used efficiently.",
            };
          }
        }

        function updatePrefixes() {
          const symbol = currencySelect.value;
          prefixes.forEach((el) => {
            if (el) el.textContent = symbol || "";
          });
        }

        function calculate() {
          const symbol = currencySelect.value;

          const cash = parseFloat(cashInput.value) || 0;
          const securities = parseFloat(securitiesInput.value) || 0;
          const receivables = parseFloat(receivablesInput.value) || 0;
          const inventory = inventoryToggle.checked
            ? parseFloat(inventoryInput.value) || 0
            : 0;
          const liabilities = parseFloat(liabilitiesInput.value);

          if (!liabilities || liabilities <= 0) {
            quickAssetsOutput.textContent = "—";
            quickAssetsWithInventoryOutput.textContent = "—";
            liabilitiesOutput.textContent = "—";
            quickRatioOutput.textContent = "—";
            quickRatioWithInventoryOutput.textContent = "—";
            quickRatioBadge.classList.add("hidden");
            quickRatioWithInventoryBadge.classList.add("hidden");
            quickRatioInterpretation.textContent =
              "Please enter a positive value for current liabilities to calculate the quick ratio.";
            quickRatioWithInventorySection.classList.add("hidden");
            quickAssetsWithInventoryRow.classList.toggle(
              "hidden",
              !inventoryToggle.checked
            );
            return;
          }

          const quickAssets = cash + securities + receivables;
          const quickAssetsWithInventory =
            quickAssets + (inventoryToggle.checked ? inventory : 0);

          const quickRatio = quickAssets / liabilities;
          const quickRatioWithInventory =
            quickAssetsWithInventory / liabilities;

          quickAssetsOutput.textContent = fmtCurrency(quickAssets, symbol);
          liabilitiesOutput.textContent = fmtCurrency(liabilities, symbol);
          quickRatioOutput.textContent = fmtRatio(quickRatio);

          const classification = classifyRatio(quickRatio);
          quickRatioBadge.textContent = classification.label;
          quickRatioBadge.className = "badge " + classification.color;
          quickRatioBadge.classList.remove("hidden");
          quickRatioInterpretation.textContent = classification.text;

          if (inventoryToggle.checked) {
            quickAssetsWithInventoryRow.classList.remove("hidden");
            quickAssetsWithInventoryOutput.textContent = fmtCurrency(
              quickAssetsWithInventory,
              symbol
            );

            quickRatioWithInventorySection.classList.remove("hidden");
            quickRatioWithInventoryOutput.textContent = fmtRatio(
              quickRatioWithInventory
            );

            const classificationInv = classifyRatio(quickRatioWithInventory);
            quickRatioWithInventoryBadge.textContent = classificationInv.label;
            quickRatioWithInventoryBadge.className =
              "badge " + classificationInv.color;
            quickRatioWithInventoryBadge.classList.remove("hidden");
          } else {
            quickAssetsWithInventoryRow.classList.add("hidden");
            quickRatioWithInventorySection.classList.add("hidden");
          }
        }

        function reset() {
          cashInput.value = "";
          securitiesInput.value = "";
          receivablesInput.value = "";
          inventoryInput.value = "";
          liabilitiesInput.value = "";
          inventoryToggle.checked = false;
          inventoryWrapper.classList.add("hidden");
          quickAssetsOutput.textContent = "—";
          quickAssetsWithInventoryOutput.textContent = "—";
          liabilitiesOutput.textContent = "—";
          quickRatioOutput.textContent = "—";
          quickRatioWithInventoryOutput.textContent = "—";
          quickRatioBadge.classList.add("hidden");
          quickRatioWithInventoryBadge.classList.add("hidden");
          quickAssetsWithInventoryRow.classList.add("hidden");
          quickRatioWithInventorySection.classList.add("hidden");
          quickRatioInterpretation.textContent =
            "Enter values and click “Calculate” to see your quick ratio.";
        }

        currencySelect.addEventListener("change", updatePrefixes);
        inventoryToggle.addEventListener("change", () => {
          if (inventoryToggle.checked) {
            inventoryWrapper.classList.remove("hidden");
          } else {
            inventoryWrapper.classList.add("hidden");
          }
        });

        calculateBtn.addEventListener("click", (e) => {
          e.preventDefault();
          calculate();
        });

        resetBtn.addEventListener("click", (e) => {
          e.preventDefault();
          reset();
        });

        // Initialize
        updatePrefixes();
      })();
    </script>

    <script>
      window.MathJax = {
        tex: {
          inlineMath: [
