    <script>
      // Core calculation logic
      function roundTo(value, decimals) {
        const factor = Math.pow(10, decimals);
        return Math.round(value * factor) / factor;
      }

      function updateFromDpiSens(setup) {
        const dpiEl = document.getElementById("dpi" + setup);
        const sensEl = document.getElementById("sens" + setup);
        const edpiEl = document.getElementById("edpi" + setup);

        const dpi = parseFloat(dpiEl.value);
        const sens = parseFloat(sensEl.value);

        if (!isFinite(dpi) || dpi <= 0 || !isFinite(sens) || sens < 0) {
          edpiEl.value = "";
          updateSummary();
          return;
        }
        const edpi = dpi * sens;
        edpiEl.value = roundTo(edpi, 2);
        updateSummary();
      }

      function updateFromEdpi(setup) {
        const dpiEl = document.getElementById("dpi" + setup);
        const sensEl = document.getElementById("sens" + setup);
        const edpiEl = document.getElementById("edpi" + setup);

        const dpi = parseFloat(dpiEl.value);
        const edpi = parseFloat(edpiEl.value);

        if (!isFinite(dpi) || dpi <= 0 || !isFinite(edpi) || edpi < 0) {
          sensEl.value = "";
          updateSummary();
          return;
        }
        const sens = edpi / dpi;
        sensEl.value = roundTo(sens, 4);
        updateSummary();
      }

      function updateSummary() {
        const summary = document.getElementById("summaryText");
        const edpiA = parseFloat(document.getElementById("edpiA").value);
        const edpiB = parseFloat(document.getElementById("edpiB").value);

        if (!isFinite(edpiA) && !isFinite(edpiB)) {
          summary.textContent =
            "Enter your DPI and sensitivity above to see your eDPI and compare setups.";
          return;
        }

        if (isFinite(edpiA) && !isFinite(edpiB)) {
          summary.textContent =
            "Setup A eDPI: " +
            roundTo(edpiA, 2) +
            ". Enter Setup B to compare or match this sensitivity.";
          return;
        }

        if (!isFinite(edpiA) && isFinite(edpiB)) {
          summary.textContent =
            "Setup B eDPI: " +
            roundTo(edpiB, 2) +
            ". Enter Setup A to compare or match this sensitivity.";
          return;
        }

        const diff = edpiB - edpiA;
        const ratio = edpiA === 0 ? null : edpiB / edpiA;

        if (!ratio || !isFinite(ratio)) {
          summary.textContent =
            "Setup A eDPI: " +
            roundTo(edpiA, 2) +
            ", Setup B eDPI: " +
            roundTo(edpiB, 2) +
            ".";
          return;
        }

        const faster = ratio > 1;
        const percent = Math.abs((ratio - 1) * 100);

        summary.textContent =
          "Setup A eDPI: " +
          roundTo(edpiA, 2) +
          ", Setup B eDPI: " +
          roundTo(edpiB, 2) +
          ". Setup B is " +
          roundTo(percent, 1) +
          "% " +
          (faster ? "faster" : "slower") +
          " than Setup A.";
      }

      function matchEdpiFromAtoB() {
        const edpiA = parseFloat(document.getElementById("edpiA").value);
        const dpiBEl = document.getElementById("dpiB");
        const sensBEl = document.getElementById("sensB");
        const edpiBEl = document.getElementById("edpiB");

        const dpiB = parseFloat(dpiBEl.value);

        if (!isFinite(edpiA) || edpiA <= 0 || !isFinite(dpiB) || dpiB <= 0) {
          alert(
            "Please enter valid DPI and sensitivity for Setup A and DPI for Setup B first."
          );
          return;
        }

        const sensB = edpiA / dpiB;
        sensBEl.value = roundTo(sensB, 4);
        edpiBEl.value = roundTo(edpiA, 2);
        updateSummary();
      }

      function swapSetups() {
        const fields = ["dpiA", "sensA", "edpiA", "dpiB", "sensB", "edpiB"];
        const values = {};
        fields.forEach(
          (id) => (values[id] = document.getElementById(id).value)
        );

        document.getElementById("dpiA").value = values["dpiB"];
        document.getElementById("sensA").value = values["sensB"];
        document.getElementById("edpiA").value = values["edpiB"];

        document.getElementById("dpiB").value = values["dpiA"];
        document.getElementById("sensB").value = values["sensA"];
        document.getElementById("edpiB").value = values["edpiA"];

        updateSummary();
      }

      function copyEdpi(setup) {
        const edpiEl = document.getElementById("edpi" + setup);
        if (!edpiEl.value) {
          alert("No eDPI value to copy.");
          return;
        }
        navigator.clipboard
          .writeText(edpiEl.value)
          .then(() => {
            // Optional: small visual feedback could be added here
          })
          .catch(() => {
            alert("Unable to copy to clipboard in this browser.");
          });
      }

      function applyPreset(game) {
        // These presets just set reasonable starting points for Setup A
        const dpiAEl = document.getElementById("dpiA");
        const sensAEl = document.getElementById("sensA");

        if (game === "valorant") {
          dpiAEl.value = 800;
          sensAEl.value = 0.4;
        } else if (game === "cs2") {
          dpiAEl.value = 400;
          sensAEl.value = 2.0;
        } else if (game === "overwatch") {
          dpiAEl.value = 800;
          sensAEl.value = 5.0;
        } else if (game === "apex") {
          dpiAEl.value = 800;
          sensAEl.value = 1.2;
        }
        updateFromDpiSens("A");
      }

      function resetDefaults() {
        document.getElementById("dpiA").value = 800;
        document.getElementById("sensA").value = 0.5;
        document.getElementById("dpiB").value = 1600;
        document.getElementById("sensB").value = "";
        document.getElementById("edpiA").value = "";
        document.getElementById("edpiB").value = "";
        updateFromDpiSens("A");
        updateSummary();
      }

      document.addEventListener("DOMContentLoaded", function () {
        // Bind inputs
        ["A", "B"].forEach((setup) => {
          document
            .getElementById("dpi" + setup)
            .addEventListener("input", () => updateFromDpiSens(setup));
          document
            .getElementById("sens" + setup)
            .addEventListener("input", () => updateFromDpiSens(setup));
          document
            .getElementById("edpi" + setup)
            .addEventListener("input", () => updateFromEdpi(setup));
        });

        document
          .getElementById("matchEdpiBtn")
          .addEventListener("click", matchEdpiFromAtoB);
        document
          .getElementById("swapBtn")
          .addEventListener("click", swapSetups);
        document
          .getElementById("copyA")
          .addEventListener("click", () => copyEdpi("A"));
        document
          .getElementById("resetBtn")
          .addEventListener("click", resetDefaults);

        document.querySelectorAll(".preset-btn").forEach((btn) => {
          btn.addEventListener("click", () => {
            const game = btn.getAttribute("data-game-preset");
            applyPreset(game);
          });
        });

        // Initialize defaults
        resetDefaults();
      });

      // Mobile menu script (simple toggle)
      document.addEventListener("DOMContentLoaded", function () {
        const toggle = document.getElementById("mobile-menu-toggle");
        const menu = document.getElementById("mobile-menu");
        if (toggle && menu) {
          toggle.addEventListener("click", () => {
            const isHidden = menu.classList.contains("hidden");
            menu.classList.toggle("hidden", !isHidden);
            toggle.setAttribute("aria-expanded", isHidden ? "true" : "false");
          });
        }
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
