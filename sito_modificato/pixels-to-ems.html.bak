      "32": { em: document.querySelector('[data-row="32-em"]'), rem: document.querySelector('[data-row="32-rem"]') }
    };

    function updateAllFromPx() {
      const px = parseFloat(pxInput.value || 0);
      const root = parseFloat(rootSizeInput.value || 16);
      const context = parseFloat(contextSizeInput.value || 16);

      const emVal = context ? px / context : 0;
      const remVal = root ? px / root : 0;

      emOutput.value = +emVal.toFixed(6);
      remOutput.value = +remVal.toFixed(6);

      labelCurrent.textContent = px + 'px → ' + emVal.toFixed(6).replace(/\.?0+$/,'') + 'em (context ' + context + 'px) → ' + remVal.toFixed(6).replace(/\.?0+$/,'') + 'rem (root ' + root + 'px)';

      // update quick table
      Object.keys(rows).forEach(pxKey => {
        const k = parseFloat(pxKey);
        rows[pxKey].em.textContent = (context ? k / context : 0).toFixed(4).replace(/\.?0+$/,'');
        rows[pxKey].rem.textContent = (root ? k / root : 0).toFixed(4).replace(/\.?0+$/,'');
      });
    }

    function updateFromEm() {
      const emVal = parseFloat(emOutput.value || 0);
      const context = parseFloat(contextSizeInput.value || 16);
      const px = emVal * context;
      pxInput.value = +px.toFixed(6);
      updateAllFromPx();
    }

    function updateFromRem() {
      const remVal = parseFloat(remOutput.value || 0);
      const root = parseFloat(rootSizeInput.value || 16);
      const px = remVal * root;
      pxInput.value = +px.toFixed(6);
      updateAllFromPx();
    }

    pxInput.addEventListener('input', updateAllFromPx);
    rootSizeInput.addEventListener('input', updateAllFromPx);
    contextSizeInput.addEventListener('input', updateAllFromPx);
    emOutput.addEventListener('input', updateFromEm);
    remOutput.addEventListener('input', updateFromRem);

    updateAllFromPx();
  </script>

