      cm: 0.01,
      mm: 0.001,
      um: 1e-6,
      nm: 1e-9,
      in: 0.0254,
      ft: 0.3048,
      yd: 0.9144,
      mi: 1609.344,
      nmi: 1852,
      rod: 5.0292
    };

    const fromValEl = document.getElementById('from-value');
    const fromUnitEl = document.getElementById('from-unit');
    const toValEl = document.getElementById('to-value');
    const toUnitEl = document.getElementById('to-unit');
    const filterBtns = document.querySelectorAll('.filter-btn');

    const resM = document.getElementById('res-m');
    const resKm = document.getElementById('res-km');
    const resCm = document.getElementById('res-cm');
    const resMm = document.getElementById('res-mm');
    const resIn = document.getElementById('res-in');
    const resFt = document.getElementById('res-ft');
    const resYd = document.getElementById('res-yd');
    const resMi = document.getElementById('res-mi');
    const resNmi = document.getElementById('res-nmi');

    function updateFromSource() {
      const val = parseFloat(fromValEl.value || 0);
      const fromUnit = fromUnitEl.value;
      const toUnit = toUnitEl.value;
      const meters = val * (TO_M[fromUnit] || 1);
      const converted = meters / (TO_M[toUnit] || 1);
      toValEl.value = +converted.toFixed(6);

      resM.textContent = meters.toFixed(6).replace(/\.?0+$/,'');
      resKm.textContent = (meters / TO_M['km']).toFixed(6).replace(/\.?0+$/,'');
      resCm.textContent = (meters / TO_M['cm']).toFixed(6).replace(/\.?0+$/,'');
      resMm.textContent = (meters / TO_M['mm']).toFixed(6).replace(/\.?0+$/,'');
      resIn.textContent = (meters / TO_M['in']).toFixed(6).replace(/\.?0+$/,'');
      resFt.textContent = (meters / TO_M['ft']).toFixed(6).replace(/\.?0+$/,'');
      resYd.textContent = (meters / TO_M['yd']).toFixed(6).replace(/\.?0+$/,'');
      resMi.textContent = (meters / TO_M['mi']).toFixed(6).replace(/\.?0+$/,'');
      resNmi.textContent = (meters / TO_M['nmi']).toFixed(6).replace(/\.?0+$/,'');
    }

    function updateFromTarget() {
      const val = parseFloat(toValEl.value || 0);
      const fromUnit = fromUnitEl.value;
      const toUnit = toUnitEl.value;
      const meters = val * (TO_M[toUnit] || 1);
      const converted = meters / (TO_M[fromUnit] || 1);
      fromValEl.value = +converted.toFixed(6);
      updateFromSource();
    }

    [fromValEl, fromUnitEl, toUnitEl].forEach(el => el.addEventListener('input', updateFromSource));
    toValEl.addEventListener('input', updateFromTarget);

    // filters
    filterBtns.forEach(btn => {
      btn.addEventListener('click', () => {
        const filter = btn.dataset.filter;
        filterBtns.forEach(b => b.classList.remove('filter-btn-active'));
        btn.classList.add('filter-btn-active');

        [fromUnitEl, toUnitEl].forEach(sel => {
          Array.from(sel.options).forEach(opt => {
            const cat = opt.dataset.cat || 'all';
            if (filter === 'all') {
              opt.hidden = false;
            } else {
              opt.hidden = cat !== filter;
            }
          });
          // safeguard
          if (sel.selectedOptions[0].hidden) {
            const firstVisible = Array.from(sel.options).find(o => !o.hidden);
            if (firstVisible) sel.value = firstVisible.value;
          }
        });
        updateFromSource();
      });
    });

    // quick table init
    document.getElementById('tbl-ft-1').textContent = (1 * TO_M['ft']).toFixed(4).replace(/\.?0+$/,'');
    document.getElementById('tbl-ft-5').textContent = (5 * TO_M['ft']).toFixed(4).replace(/\.?0+$/,'');
    document.getElementById('tbl-ft-10').textContent = (10 * TO_M['ft']).toFixed(4).replace(/\.?0+$/,'');
    document.getElementById('tbl-ft-50').textContent = (50 * TO_M['ft']).toFixed(4).replace(/\.?0+$/,'');

    document.getElementById('tbl-m-1').textContent = (1 / TO_M['ft']).toFixed(5).replace(/\.?0+$/,'');
    document.getElementById('tbl-m-2').textContent = (2 / TO_M['ft']).toFixed(5).replace(/\.?0+$/,'');
    document.getElementById('tbl-m-5').textContent = (5 / TO_M['ft']).toFixed(4).replace(/\.?0+$/,'');
    document.getElementById('tbl-m-10').textContent = (10 / TO_M['ft']).toFixed(4).replace(/\.?0+$/,'');

    // init converter
    updateFromSource();
  </script>

