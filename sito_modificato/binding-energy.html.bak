    <script>
      (function () {
        // Constants
        const c = 2.99792458e8; // m/s
        const uToKg = 1.6605390666e-27; // kg
        const uToMeV = 931.494; // MeV per u
        const m_p_u = 1.007276466621;
        const m_n_u = 1.00866491595;
        const m_e_u = 0.000548579909065;

        const modeMassDefectBtn = document.getElementById("mode-mass-defect");
        const modeAtomicMassBtn = document.getElementById("mode-atomic-mass");
        const massDefectBlock = document.getElementById("mass-defect-block");
        const atomicMassBlock = document.getElementById("atomic-mass-block");

        const unitMeVBtn = document.getElementById("unit-MeV");
        const unitJBtn = document.getElementById("unit-J");
        const unitBothBtn = document.getElementById("unit-both");

        const presetSelect = document.getElementById("preset-select");
        const ZInput = document.getElementById("protons");
        const NInput = document.getElementById("neutrons");
        const dmInput = document.getElementById("mass-defect");
        const dmUnitSelect = document.getElementById("mass-defect-unit");
        const mAtomInput = document.getElementById("atomic-mass");
        const mAtomUnitSelect = document.getElementById("atomic-mass-unit");

        const calcBtn = document.getElementById("calculate-btn");
        const resetBtn = document.getElementById("reset-btn");
        const errorMsg = document.getElementById("error-msg");

        const resultsCard = document.getElementById("results-card");
        const resA = document.getElementById("res-A");
        const resDm = document.getElementById("res-dm");
        const resB = document.getElementById("res-B");
        const resBperA = document.getElementById("res-BperA");
        const stabilityHint = document.getElementById("stability-hint");

        let mode = "mass-defect"; // or 'atomic-mass'
        let outUnit = "MeV"; // 'MeV', 'J', 'both'

        function setMode(newMode) {
          mode = newMode;
          if (newMode === "mass-defect") {
            massDefectBlock.classList.remove("hidden");
            atomicMassBlock.classList.add("hidden");
            modeMassDefectBtn.classList.add("bg-blue-600", "text-white");
            modeMassDefectBtn.classList.remove(
              "bg-white",
              "text-gray-700",
              "border",
              "border-gray-300"
            );
            modeAtomicMassBtn.classList.remove("bg-blue-600", "text-white");
            modeAtomicMassBtn.classList.add(
              "bg-white",
              "text-gray-700",
              "border",
              "border-gray-300"
            );
          } else {
            massDefectBlock.classList.add("hidden");
            atomicMassBlock.classList.remove("hidden");
            modeAtomicMassBtn.classList.add("bg-blue-600", "text-white");
            modeAtomicMassBtn.classList.remove(
              "bg-white",
              "text-gray-700",
              "border",
              "border-gray-300"
            );
            modeMassDefectBtn.classList.remove("bg-blue-600", "text-white");
            modeMassDefectBtn.classList.add(
              "bg-white",
              "text-gray-700",
              "border",
              "border-gray-300"
            );
          }
          clearError();
        }

        function setOutUnit(unit) {
          outUnit = unit;
          [unitMeVBtn, unitJBtn, unitBothBtn].forEach((btn) => {
            btn.classList.remove("bg-blue-600", "text-white");
            btn.classList.add(
              "bg-white",
              "text-gray-700",
              "border",
              "border-gray-300"
            );
          });
          if (unit === "MeV") {
            unitMeVBtn.classList.add("bg-blue-600", "text-white");
            unitMeVBtn.classList.remove(
              "bg-white",
              "text-gray-700",
              "border",
              "border-gray-300"
            );
          } else if (unit === "J") {
            unitJBtn.classList.add("bg-blue-600", "text-white");
            unitJBtn.classList.remove(
              "bg-white",
              "text-gray-700",
              "border",
              "border-gray-300"
            );
          } else {
            unitBothBtn.classList.add("bg-blue-600", "text-white");
            unitBothBtn.classList.remove(
              "bg-white",
              "text-gray-700",
              "border",
              "border-gray-300"
            );
          }
          clearError();
        }

        function showError(msg) {
          errorMsg.textContent = msg;
          resultsCard.classList.add("hidden");
        }

        function clearError() {
          errorMsg.textContent = "";
        }

        function applyPreset(id) {
          const presets = {
            "H-1": { Z: 1, N: 0, mAtom_u: 1.00782503223 },
            "He-4": { Z: 2, N: 2, mAtom_u: 4.002602 },
            "C-12": { Z: 6, N: 6, mAtom_u: 12.0 },
            "Fe-56": { Z: 26, N: 30, mAtom_u: 55.9349375 },
            "U-235": { Z: 92, N: 143, mAtom_u: 235.0439299 },
          };
          const p = presets[id];
          if (!p) return;
          ZInput.value = p.Z;
          NInput.value = p.N;
          mAtomInput.value = p.mAtom_u;
          mAtomUnitSelect.value = "u";
          if (mode === "mass-defect") {
            // encourage using atomic-mass mode with presets
            setMode("atomic-mass");
          }
        }

        function compute() {
          clearError();
          const Z = parseInt(ZInput.value, 10);
          const N = parseInt(NInput.value, 10);

          if (!Number.isFinite(Z) || Z < 0 || !Number.isFinite(N) || N < 0) {
            showError("Please enter non-negative integer values for Z and N.");
            return;
          }
          const A = Z + N;
          if (A === 0) {
            showError(
              "Total nucleon number A = Z + N must be greater than zero."
            );
            return;
          }

          let dm_u = null; // mass defect in atomic mass units

          if (mode === "mass-defect") {
            const dm = parseFloat(dmInput.value);
            if (!Number.isFinite(dm) || dm <= 0) {
              showError("Please enter a positive mass defect Δm.");
              return;
            }
            const unit = dmUnitSelect.value;
            if (unit === "u") {
              dm_u = dm;
            } else {
              // kg to u
              dm_u = dm / uToKg;
            }
          } else {
            const mAtom = parseFloat(mAtomInput.value);
            if (!Number.isFinite(mAtom) || mAtom <= 0) {
              showError("Please enter a positive atomic mass.");
              return;
            }
            const unit = mAtomUnitSelect.value;
            let mAtom_u;
            if (unit === "u") {
              mAtom_u = mAtom;
            } else {
              mAtom_u = mAtom / uToKg;
            }
            const mNucleons_u = Z * m_p_u + N * m_n_u;
            const mNucleus_u = mAtom_u - Z * m_e_u;
            dm_u = mNucleons_u - mNucleus_u;
            if (dm_u <= 0) {
              showError(
                "Computed mass defect is non-positive. Check your inputs."
              );
              return;
            }
          }

          // Binding energy in MeV
          const B_MeV = dm_u * uToMeV;
          const B_per_A_MeV = B_MeV / A;

          // Convert to Joules if needed
          const dm_kg = dm_u * uToKg;
          const B_J = dm_kg * c * c;
          const B_per_A_J = B_J / A;

          // Format outputs
          resA.textContent = A.toString();
          resDm.textContent = dm_u.toExponential(6) + " u";

          let BText = "";
          let BperAText = "";

          if (outUnit === "MeV" || outUnit === "both") {
            BText += B_MeV.toFixed(3) + " MeV";
            BperAText += B_per_A_MeV.toFixed(3) + " MeV/nucleon";
          }
          if (outUnit === "J" || outUnit === "both") {
            if (BText) BText += "  (";
            if (BperAText) BperAText += "  (";
            BText += B_J.toExponential(6) + " J";
            BperAText += B_per_A_J.toExponential(6) + " J/nucleon";
            if (outUnit === "both") {
              BText += ")";
              BperAText += ")";
            }
          }

          resB.textContent = BText;
          resBperA.textContent = BperAText;

          // Stability hint based on B/A in MeV
          let hint =
            "Binding energy per nucleon around 8–9 MeV typically indicates a very stable nucleus.";
          if (B_per_A_MeV < 5) {
            hint =
              "This binding energy per nucleon is relatively low; very light or very heavy nuclei tend to be less tightly bound.";
          } else if (B_per_A_MeV >= 7.5 && B_per_A_MeV <= 9.0) {
            hint =
              "This value is close to the maximum of the binding energy curve (around iron), indicating a very stable nucleus.";
          } else if (B_per_A_MeV > 9.5) {
            hint =
              "This binding energy per nucleon is unusually high; double-check the input values for consistency.";
          }
          stabilityHint.textContent = hint;

          resultsCard.classList.remove("hidden");
        }

        function resetAll() {
          ZInput.value = 2;
          NInput.value = 2;
          dmInput.value = "";
          mAtomInput.value = "";
          dmUnitSelect.value = "u";
          mAtomUnitSelect.value = "u";
          presetSelect.value = "";
          setMode("mass-defect");
          setOutUnit("MeV");
          clearError();
          resultsCard.classList.add("hidden");
        }

        // Event listeners
        modeMassDefectBtn.addEventListener("click", () =>
          setMode("mass-defect")
        );
        modeAtomicMassBtn.addEventListener("click", () =>
          setMode("atomic-mass")
        );

        unitMeVBtn.addEventListener("click", () => setOutUnit("MeV"));
        unitJBtn.addEventListener("click", () => setOutUnit("J"));
        unitBothBtn.addEventListener("click", () => setOutUnit("both"));

        presetSelect.addEventListener("change", (e) =>
          applyPreset(e.target.value)
        );

        calcBtn.addEventListener("click", compute);
        resetBtn.addEventListener("click", resetAll);
      })();
    </script>
  
