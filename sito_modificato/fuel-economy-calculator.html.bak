    <script>
      // Utility conversions
      const L_PER_US_GAL = 3.785411784;
      const L_PER_UK_GAL = 4.54609;
      const MI_PER_KM = 0.621371192;
      const KM_PER_MI = 1.609344;

      function toNumber(value) {
        const n = parseFloat(value);
        return isNaN(n) ? null : n;
      }

      function mpgToLPer100(mpg, isUK) {
        if (!mpg || mpg <= 0) return null;
        return (isUK ? 282.480936 : 235.214583) / mpg;
      }

      function lPer100ToMpg(lPer100, isUK) {
        if (!lPer100 || lPer100 <= 0) return null;
        return (isUK ? 282.480936 : 235.214583) / lPer100;
      }

      function kmPerLToLPer100(kmPerL) {
        if (!kmPerL || kmPerL <= 0) return null;
        return 100 / kmPerL;
      }

      function lPer100ToKmPerL(lPer100) {
        if (!lPer100 || lPer100 <= 0) return null;
        return 100 / lPer100;
      }

      function distanceFuelToLPer100(distance, fuel, distanceUnit, volumeUnit) {
        if (!distance || distance <= 0 || !fuel || fuel <= 0) return null;
        let km = distance;
        if (distanceUnit === "mi") km = distance * KM_PER_MI;

        let liters = fuel;
        if (volumeUnit === "gal_us") liters = fuel * L_PER_US_GAL;
        else if (volumeUnit === "gal_uk") liters = fuel * L_PER_UK_GAL;

        return (liters / km) * 100;
      }

      function computeTrip(
        distance,
        distanceUnit,
        lPer100km,
        price,
        priceUnit,
        volumeUnit,
        co2FactorKgPerL
      ) {
        if (!distance || distance <= 0 || !lPer100km || lPer100km <= 0)
          return null;

        // Convert distance to km
        const dKm = distanceUnit === "mi" ? distance * KM_PER_MI : distance;

        // Fuel used in liters
        const fuelLiters = (lPer100km / 100) * dKm;

        // Convert liters to chosen volume unit for display
        let fuelDisplay = fuelLiters;
        let fuelUnitLabel = "L";
        let litersPerUnit = 1;
        if (volumeUnit === "gal_us") {
          fuelDisplay = fuelLiters / L_PER_US_GAL;
          fuelUnitLabel = "US gal";
          litersPerUnit = L_PER_US_GAL;
        } else if (volumeUnit === "gal_uk") {
          fuelDisplay = fuelLiters / L_PER_UK_GAL;
          fuelUnitLabel = "UK gal";
          litersPerUnit = L_PER_UK_GAL;
        }

        // Cost
        let cost = 0;
        if (price && price > 0) {
          if (priceUnit === "per_L") {
            cost = fuelLiters * price;
          } else {
            // per gallon (US or UK depending on volumeUnit)
            const fuelGallons = fuelLiters / litersPerUnit;
            cost = fuelGallons * price;
          }
        }

        // Cost per distance
        const distanceLabel = distanceUnit === "mi" ? "mi" : "km";
        const costPerDistance = distance > 0 && cost > 0 ? cost / distance : 0;

        // CO2
        const co2Kg =
          co2FactorKgPerL && co2FactorKgPerL > 0
            ? fuelLiters * co2FactorKgPerL
            : 0;

        // Derived MPG and km/L
        const mpgUS = lPer100ToMpg(lPer100km, false);
        const mpgUK = lPer100ToMpg(lPer100km, true);
        const kmPerL = lPer100ToKmPerL(lPer100km);

        return {
          fuelLiters,
          fuelDisplay,
          fuelUnitLabel,
          cost,
          costPerDistance,
          distanceLabel,
          co2Kg,
          mpgUS,
          mpgUK,
          kmPerL,
          lPer100km,
        };
      }

      function formatNumber(value, decimals) {
        if (value === null || value === undefined || isNaN(value)) return "—";
        return value.toFixed(decimals);
      }

      function formatCurrency(value) {
        if (!value || isNaN(value)) return "$0.00";
        return "$" + value.toFixed(2);
      }

      function formatKgCO2(value) {
        if (!value || isNaN(value)) return "—";
        if (value < 1) return (value * 1000).toFixed(0) + " g";
        if (value < 1000) return value.toFixed(1) + " kg";
        return (value / 1000).toFixed(2) + " t";
      }

      document.addEventListener("DOMContentLoaded", function () {
        const distanceUnitEl = document.getElementById("distance-unit");
        const volumeUnitEl = document.getElementById("volume-unit");
        const co2FactorEl = document.getElementById("co2-factor");
        const customCo2Wrapper = document.getElementById("custom-co2-wrapper");
        const customCo2El = document.getElementById("custom-co2");

        const veh1DistanceUnitLabel = document.getElementById(
          "veh1-distance-unit-label"
        );
        const veh2DistanceUnitLabel = document.getElementById(
          "veh2-distance-unit-label"
        );
        const veh1ActualDistanceUnitLabel = document.getElementById(
          "veh1-actual-distance-unit-label"
        );
        const veh2ActualDistanceUnitLabel = document.getElementById(
          "veh2-actual-distance-unit-label"
        );
        const veh1FuelUsedUnitLabel = document.getElementById(
          "veh1-fuel-used-unit-label"
        );
        const veh2FuelUsedUnitLabel = document.getElementById(
          "veh2-fuel-used-unit-label"
        );
        const veh1MpgUnitLabel = document.getElementById("veh1-mpg-unit-label");
        const veh2MpgUnitLabel = document.getElementById("veh2-mpg-unit-label");

        const tabVeh1 = document.getElementById("tab-veh1");
        const tabVeh2 = document.getElementById("tab-veh2");
        const veh1Panel = document.getElementById("veh1-panel");
        const veh2Panel = document.getElementById("veh2-panel");

        const calcBtn = document.getElementById("calculate-btn");
        const resetBtn = document.getElementById("reset-btn");
        const errorEl = document.getElementById("calc-error");
        const resultsSection = document.getElementById("results-section");
        const comparisonSection = document.getElementById("comparison-section");
        const veh2ResultCard = document.getElementById("veh2-result-card");

        // Mode-specific containers
        const veh1Inputs = {
          mpg: document.getElementById("veh1-inputs-mpg"),
          l_per_100km: document.getElementById("veh1-inputs-lper100"),
          km_per_l: document.getElementById("veh1-inputs-kmperl"),
          distance_fuel: document.getElementById("veh1-inputs-distancefuel"),
        };
        const veh2Inputs = {
          mpg: document.getElementById("veh2-inputs-mpg"),
          l_per_100km: document.getElementById("veh2-inputs-lper100"),
          km_per_l: document.getElementById("veh2-inputs-kmperl"),
          distance_fuel: document.getElementById("veh2-inputs-distancefuel"),
        };

        function updateUnitLabels() {
          const distUnit = distanceUnitEl.value;
          const volUnit = volumeUnitEl.value;

          const distLabel = distUnit === "mi" ? "mi" : "km";
          veh1DistanceUnitLabel.textContent = distLabel;
          veh2DistanceUnitLabel.textContent = distLabel;
          veh1ActualDistanceUnitLabel.textContent = distLabel;
          veh2ActualDistanceUnitLabel.textContent = distLabel;

          let fuelLabel = "L";
          if (volUnit === "gal_us") fuelLabel = "gal";
          else if (volUnit === "gal_uk") fuelLabel = "gal";

          veh1FuelUsedUnitLabel.textContent = fuelLabel;
          veh2FuelUsedUnitLabel.textContent = fuelLabel;

          // MPG label depends on gallon type
          const mpgLabel = volUnit === "gal_uk" ? "MPG (UK)" : "MPG (US)";
          veh1MpgUnitLabel.textContent = mpgLabel;
          veh2MpgUnitLabel.textContent = mpgLabel;
        }

        function updateCo2Visibility() {
          if (co2FactorEl.value === "custom") {
            customCo2Wrapper.classList.remove("hidden");
          } else {
            customCo2Wrapper.classList.add("hidden");
          }
        }

        function switchTab(tab) {
          if (tab === 1) {
            tabVeh1.classList.add("tab-active");
            tabVeh1.classList.remove("text-gray-500");
            tabVeh2.classList.remove("tab-active");
            tabVeh2.classList.add("text-gray-500");
            veh1Panel.classList.remove("hidden");
            veh2Panel.classList.add("hidden");
          } else {
            tabVeh2.classList.add("tab-active");
            tabVeh2.classList.remove("text-gray-500");
            tabVeh1.classList.remove("tab-active");
            tabVeh1.classList.add("text-gray-500");
            veh2Panel.classList.remove("hidden");
            veh1Panel.classList.add("hidden");
          }
        }

        function updateModeInputs(prefix, inputs) {
          const mode = document.querySelector(
            `input[name="${prefix}-mode"]:checked`
          ).value;
          Object.keys(inputs).forEach((key) => {
            if (key === mode) inputs[key].classList.remove("hidden");
            else inputs[key].classList.add("hidden");
          });
        }

        distanceUnitEl.addEventListener("change", updateUnitLabels);
        volumeUnitEl.addEventListener("change", updateUnitLabels);
        co2FactorEl.addEventListener("change", updateCo2Visibility);

        tabVeh1.addEventListener("click", () => switchTab(1));
        tabVeh2.addEventListener("click", () => switchTab(2));

        document
          .querySelectorAll('input[name="veh1-mode"]')
          .forEach((r) =>
            r.addEventListener("change", () =>
              updateModeInputs("veh1", veh1Inputs)
            )
          );
        document
          .querySelectorAll('input[name="veh2-mode"]')
          .forEach((r) =>
            r.addEventListener("change", () =>
              updateModeInputs("veh2", veh2Inputs)
            )
          );

        function getCo2Factor() {
          const sel = co2FactorEl.value;
          if (sel === "gasoline") return 2.31;
          if (sel === "diesel") return 2.68;
          const custom = toNumber(customCo2El.value);
          return custom && custom > 0 ? custom : 0;
        }

        function calculate() {
          errorEl.classList.add("hidden");
          errorEl.textContent = "";

          const distUnit = distanceUnitEl.value;
          const volUnit = volumeUnitEl.value;
          const co2Factor = getCo2Factor();

          // Vehicle 1 inputs
          const veh1Distance = toNumber(
            document.getElementById("veh1-distance").value
          );
          const veh1Price = toNumber(
            document.getElementById("veh1-price").value
          );
          const veh1PriceUnit =
            document.getElementById("veh1-price-unit").value;
          const veh1Mode = document.querySelector(
            'input[name="veh1-mode"]:checked'
          ).value;

          let veh1LPer100 = null;
          if (veh1Mode === "mpg") {
            const mpg = toNumber(document.getElementById("veh1-mpg").value);
            const isUK = volUnit === "gal_uk";
            veh1LPer100 = mpgToLPer100(mpg, isUK);
          } else if (veh1Mode === "l_per_100km") {
            veh1LPer100 = toNumber(
              document.getElementById("veh1-lper100").value
            );
          } else if (veh1Mode === "km_per_l") {
            const kmPerL = toNumber(
              document.getElementById("veh1-kmperl").value
            );
            veh1LPer100 = kmPerLToLPer100(kmPerL);
          } else if (veh1Mode === "distance_fuel") {
            const d = toNumber(
              document.getElementById("veh1-actual-distance").value
            );
            const f = toNumber(document.getElementById("veh1-fuel-used").value);
            veh1LPer100 = distanceFuelToLPer100(d, f, distUnit, volUnit);
          }

          if (
            !veh1Distance ||
            veh1Distance <= 0 ||
            !veh1LPer100 ||
            veh1LPer100 <= 0
          ) {
            errorEl.textContent =
              "Please enter at least distance and a valid fuel economy for Vehicle 1.";
            errorEl.classList.remove("hidden");
            resultsSection.classList.add("hidden");
            comparisonSection.classList.add("hidden");
            return;
          }

          const veh1Trip = computeTrip(
            veh1Distance,
            distUnit,
            veh1LPer100,
            veh1Price,
            veh1PriceUnit,
            volUnit,
            co2Factor
          );

          // Vehicle 2 (optional)
          const veh2DistanceVal = document
            .getElementById("veh2-distance")
            .value.trim();
          const veh2HasData = veh2DistanceVal !== "";
          let veh2Trip = null;

          if (veh2HasData) {
            const veh2Distance = toNumber(veh2DistanceVal);
            const veh2Price = toNumber(
              document.getElementById("veh2-price").value
            );
            const veh2PriceUnit =
              document.getElementById("veh2-price-unit").value;
            const veh2Mode = document.querySelector(
              'input[name="veh2-mode"]:checked'
            ).value;

            let veh2LPer100 = null;
            if (veh2Mode === "mpg") {
              const mpg2 = toNumber(document.getElementById("veh2-mpg").value);
              const isUK2 = volUnit === "gal_uk";
              veh2LPer100 = mpgToLPer100(mpg2, isUK2);
            } else if (veh2Mode === "l_per_100km") {
              veh2LPer100 = toNumber(
                document.getElementById("veh2-lper100").value
              );
            } else if (veh2Mode === "km_per_l") {
              const kmPerL2 = toNumber(
                document.getElementById("veh2-kmperl").value
              );
              veh2LPer100 = kmPerLToLPer100(kmPerL2);
            } else if (veh2Mode === "distance_fuel") {
              const d2 = toNumber(
                document.getElementById("veh2-actual-distance").value
              );
              const f2 = toNumber(
                document.getElementById("veh2-fuel-used").value
              );
              veh2LPer100 = distanceFuelToLPer100(d2, f2, distUnit, volUnit);
            }

            if (
              veh2Distance &&
              veh2Distance > 0 &&
              veh2LPer100 &&
              veh2LPer100 > 0
            ) {
              veh2Trip = computeTrip(
                veh2Distance,
                distUnit,
                veh2LPer100,
                veh2Price,
                veh2PriceUnit,
                volUnit,
                co2Factor
              );
            }
          }

          // Populate Vehicle 1 results
          document.getElementById("veh1-res-mpg").textContent = veh1Trip.mpgUS
            ? formatNumber(veh1Trip.mpgUS, 1)
            : "—";
          document.getElementById("veh1-res-lper100").textContent =
            formatNumber(veh1Trip.lPer100km, 1);
          document.getElementById("veh1-res-kmperl").textContent =
            veh1Trip.kmPerL ? formatNumber(veh1Trip.kmPerL, 2) : "—";
          document.getElementById("veh1-res-fuel").textContent =
            formatNumber(veh1Trip.fuelDisplay, 2) +
            " " +
            veh1Trip.fuelUnitLabel;
          document.getElementById("veh1-res-cost").textContent = formatCurrency(
            veh1Trip.cost
          );
          document.getElementById("veh1-res-cost-per-distance").textContent =
            veh1Trip.costPerDistance
              ? formatCurrency(veh1Trip.costPerDistance) +
                " per " +
                veh1Trip.distanceLabel
              : "—";
          document.getElementById("veh1-res-co2").textContent = formatKgCO2(
            veh1Trip.co2Kg
          );

          // Vehicle 2 results
          if (veh2Trip) {
            veh2ResultCard.classList.remove("hidden");
            document.getElementById("veh2-res-mpg").textContent = veh2Trip.mpgUS
              ? formatNumber(veh2Trip.mpgUS, 1)
              : "—";
            document.getElementById("veh2-res-lper100").textContent =
              formatNumber(veh2Trip.lPer100km, 1);
            document.getElementById("veh2-res-kmperl").textContent =
              veh2Trip.kmPerL ? formatNumber(veh2Trip.kmPerL, 2) : "—";
            document.getElementById("veh2-res-fuel").textContent =
              formatNumber(veh2Trip.fuelDisplay, 2) +
              " " +
              veh2Trip.fuelUnitLabel;
            document.getElementById("veh2-res-cost").textContent =
              formatCurrency(veh2Trip.cost);
            document.getElementById("veh2-res-cost-per-distance").textContent =
              veh2Trip.costPerDistance
                ? formatCurrency(veh2Trip.costPerDistance) +
                  " per " +
                  veh2Trip.distanceLabel
                : "—";
            document.getElementById("veh2-res-co2").textContent = formatKgCO2(
              veh2Trip.co2Kg
            );

            // Comparison
            comparisonSection.classList.remove("hidden");
            document.getElementById("cmp-fuel1").textContent =
              formatNumber(veh1Trip.fuelDisplay, 2) +
              " " +
              veh1Trip.fuelUnitLabel;
            document.getElementById("cmp-fuel2").textContent =
              formatNumber(veh2Trip.fuelDisplay, 2) +
              " " +
              veh2Trip.fuelUnitLabel;
            const fuelDiff = veh1Trip.fuelLiters - veh2Trip.fuelLiters;
            const fuelDiffDisplay =
              Math.abs(fuelDiff) /
              (volUnit === "L"
                ? 1
                : volUnit === "gal_us"
                ? L_PER_US_GAL
                : L_PER_UK_GAL);
            const fuelDiffUnit = veh1Trip.fuelUnitLabel;
            document.getElementById("cmp-fuel-diff").textContent =
              (fuelDiff > 0 ? "+" : fuelDiff < 0 ? "−" : "") +
              formatNumber(Math.abs(fuelDiffDisplay), 2) +
              " " +
              fuelDiffUnit;

            document.getElementById("cmp-cost1").textContent = formatCurrency(
              veh1Trip.cost
            );
            document.getElementById("cmp-cost2").textContent = formatCurrency(
              veh2Trip.cost
            );
            const costDiff = veh1Trip.cost - veh2Trip.cost;
            document.getElementById("cmp-cost-diff").textContent =
              (costDiff > 0 ? "+" : costDiff < 0 ? "−" : "") +
              formatCurrency(Math.abs(costDiff));

            document.getElementById("cmp-co21").textContent = formatKgCO2(
              veh1Trip.co2Kg
            );
            document.getElementById("cmp-co22").textContent = formatKgCO2(
              veh2Trip.co2Kg
            );
            const co2Diff = veh1Trip.co2Kg - veh2Trip.co2Kg;
            document.getElementById("cmp-co2-diff").textContent =
              (co2Diff > 0 ? "+" : co2Diff < 0 ? "−" : "") +
              formatKgCO2(Math.abs(co2Diff));

            const better =
              costDiff > 0
                ? "Vehicle 2"
                : costDiff < 0
                ? "Vehicle 1"
                : "Both vehicles";
            const summary =
              costDiff === 0
                ? "Both vehicles have the same estimated trip cost."
                : `${better} is cheaper for this trip by about ${formatCurrency(
                    Math.abs(costDiff)
                  )} and saves approximately ${formatKgCO2(
                    Math.abs(co2Diff)
                  )} of CO₂.`;
            document.getElementById("comparison-summary").textContent = summary;
          } else {
            veh2ResultCard.classList.add("hidden");
            comparisonSection.classList.add("hidden");
          }

          resultsSection.classList.remove("hidden");
        }

        function resetAll() {
          document
            .querySelectorAll('input[type="number"]')
            .forEach((el) => (el.value = ""));
          document.getElementById("veh1-price-unit").value = "per_gal";
          document.getElementById("veh2-price-unit").value = "per_gal";
          distanceUnitEl.value = "mi";
          volumeUnitEl.value = "gal_us";
          co2FactorEl.value = "gasoline";
          customCo2El.value = "";
          document.querySelector(
            'input[name="veh1-mode"][value="mpg"]'
          ).checked = true;
          document.querySelector(
            'input[name="veh2-mode"][value="mpg"]'
          ).checked = true;
          updateModeInputs("veh1", veh1Inputs);
          updateModeInputs("veh2", veh2Inputs);
          updateUnitLabels();
          updateCo2Visibility();
          resultsSection.classList.add("hidden");
          comparisonSection.classList.add("hidden");
          errorEl.classList.add("hidden");
          errorEl.textContent = "";
          switchTab(1);
        }

        calcBtn.addEventListener("click", calculate);
        resetBtn.addEventListener("click", resetAll);

        // Initial setup
        updateUnitLabels();
        updateCo2Visibility();
        updateModeInputs("veh1", veh1Inputs);
        updateModeInputs("veh2", veh2Inputs);
      });
    </script>

