    <script>
      // --- Simplified tax parameters (approximate, for educational use only) ---
      const STANDARD_DEDUCTION = {
        single: 14600,
        married_joint: 29200,
        married_separate: 14600,
        hoh: 21900,
      };

      // 2024-like federal brackets (approximate) for demonstration
      const FEDERAL_BRACKETS = {
        single: [
          { upTo: 11600, rate: 0.1 },
          { upTo: 47150, rate: 0.12 },
          { upTo: 100525, rate: 0.22 },
          { upTo: 191950, rate: 0.24 },
          { upTo: 243725, rate: 0.32 },
          { upTo: 609350, rate: 0.35 },
          { upTo: Infinity, rate: 0.37 },
        ],
        married_joint: [
          { upTo: 23200, rate: 0.1 },
          { upTo: 94300, rate: 0.12 },
          { upTo: 201050, rate: 0.22 },
          { upTo: 383900, rate: 0.24 },
          { upTo: 487450, rate: 0.32 },
          { upTo: 731200, rate: 0.35 },
          { upTo: Infinity, rate: 0.37 },
        ],
        married_separate: [
          { upTo: 11600, rate: 0.1 },
          { upTo: 47150, rate: 0.12 },
          { upTo: 100525, rate: 0.22 },
          { upTo: 191950, rate: 0.24 },
          { upTo: 243725, rate: 0.32 },
          { upTo: 365600, rate: 0.35 },
          { upTo: Infinity, rate: 0.37 },
        ],
        hoh: [
          { upTo: 16550, rate: 0.1 },
          { upTo: 63100, rate: 0.12 },
          { upTo: 100500, rate: 0.22 },
          { upTo: 191950, rate: 0.24 },
          { upTo: 243700, rate: 0.32 },
          { upTo: 609350, rate: 0.35 },
          { upTo: Infinity, rate: 0.37 },
        ],
      };

      const CHILD_CREDIT_PER_DEP = 1500; // simplified flat credit
      const SS_RATE = 0.062;
      const MEDICARE_RATE = 0.0145;
      const SS_WAGE_BASE = 168600; // approximate

      function formatMoney(value) {
        if (!isFinite(value)) return "$0.00";
        return value.toLocaleString("en-US", {
          style: "currency",
          currency: "USD",
        });
      }

      function calcFederalTax(taxableIncome, filingStatus) {
        if (taxableIncome <= 0) return { tax: 0, marginalRate: 0 };
        const brackets =
          FEDERAL_BRACKETS[filingStatus] || FEDERAL_BRACKETS.single;
        let remaining = taxableIncome;
        let lastLimit = 0;
        let tax = 0;
        let marginalRate = brackets[0].rate;

        for (const bracket of brackets) {
          const bracketSize = bracket.upTo - lastLimit;
          const amountInBracket = Math.min(remaining, bracketSize);
          if (amountInBracket <= 0) break;
          tax += amountInBracket * bracket.rate;
          remaining -= amountInBracket;
          marginalRate = bracket.rate;
          lastLimit = bracket.upTo;
          if (remaining <= 0) break;
        }

        return { tax, marginalRate };
      }

      function calculate() {
        const grossPay = parseFloat(document.getElementById("grossPay").value);
        const payFreq = parseInt(
          document.getElementById("payFrequency").value,
          10
        );
        const filingStatus = document.getElementById("filingStatus").value;
        const pretax =
          parseFloat(document.getElementById("pretaxDeductions").value) || 0;
        const dependents =
          parseInt(document.getElementById("dependents").value, 10) || 0;
        const extraWithholding =
          parseFloat(document.getElementById("extraWithholding").value) || 0;
        const otherIncome =
          parseFloat(document.getElementById("otherAnnualIncome").value) || 0;
        const stateRatePct =
          parseFloat(document.getElementById("stateRate").value) || 0;

        const errorEl = document.getElementById("calcError");
        if (!grossPay || grossPay <= 0 || !payFreq) {
          errorEl.classList.remove("hidden");
          document.getElementById("resultsSection").classList.add("hidden");
          return;
        }
        errorEl.classList.add("hidden");

        const annualGrossMain = grossPay * payFreq;
        const annualGrossTotal = annualGrossMain + otherIncome;

        const taxableWagesPerPeriod = Math.max(0, grossPay - pretax);
        const annualTaxableWages =
          taxableWagesPerPeriod * payFreq + otherIncome;

        const stdDed =
          STANDARD_DEDUCTION[filingStatus] || STANDARD_DEDUCTION.single;
        const approxTaxableIncome = Math.max(0, annualTaxableWages - stdDed);

        const fedResult = calcFederalTax(approxTaxableIncome, filingStatus);
        let federalTaxAnnual = fedResult.tax;

        const credit = dependents * CHILD_CREDIT_PER_DEP;
        federalTaxAnnual = Math.max(0, federalTaxAnnual - credit);

        const federalTaxPerPeriod =
          federalTaxAnnual / payFreq + extraWithholding;

        const ssWages = Math.min(annualTaxableWages, SS_WAGE_BASE);
        const ssTaxAnnual = ssWages * SS_RATE;
        const medicareTaxAnnual = annualTaxableWages * MEDICARE_RATE;
        const ficaAnnual = ssTaxAnnual + medicareTaxAnnual;
        const ficaPerPeriod = ficaAnnual / payFreq;

        const stateTaxAnnual = annualTaxableWages * (stateRatePct / 100);
        const stateTaxPerPeriod = stateTaxAnnual / payFreq;

        const totalTaxAnnual =
          federalTaxAnnual +
          stateTaxAnnual +
          ficaAnnual +
          extraWithholding * payFreq;
        const netAnnual = annualGrossMain - totalTaxAnnual;
        const totalTaxPerPeriod = totalTaxAnnual / payFreq;
        const netPerPeriod = netAnnual / payFreq;

        // Update UI
        document.getElementById("grossAnnual").textContent =
          formatMoney(annualGrossMain);
        document.getElementById("taxableAnnual").textContent =
          formatMoney(approxTaxableIncome);
        document.getElementById("federalTaxAnnual").textContent =
          formatMoney(federalTaxAnnual);
        document.getElementById("stateTaxAnnual").textContent =
          formatMoney(stateTaxAnnual);
        document.getElementById("ficaAnnual").textContent =
          formatMoney(ficaAnnual);
        document.getElementById("netAnnual").textContent =
          formatMoney(netAnnual);

        document.getElementById("netPayPerPeriod").textContent =
          formatMoney(netPerPeriod);
        document.getElementById("totalTaxPerPeriod").textContent =
          formatMoney(totalTaxPerPeriod);
        document.getElementById("federalTaxPerPeriod").textContent =
          formatMoney(federalTaxPerPeriod);
        document.getElementById("stateTaxPerPeriod").textContent =
          formatMoney(stateTaxPerPeriod);
        document.getElementById("ficaPerPeriod").textContent =
          formatMoney(ficaPerPeriod);

        const effectiveRate =
          annualGrossMain > 0 ? totalTaxAnnual / annualGrossMain : 0;
        document.getElementById("detailEffectiveRate").textContent =
          "Estimated effective total tax rate: " +
          (effectiveRate * 100).toFixed(1) +
          "% of your main job income.";
        document.getElementById("detailMarginalRate").textContent =
          "Approximate federal marginal tax rate: " +
          (fedResult.marginalRate * 100).toFixed(1) +
          "%.";

        document.getElementById("resultsSection").classList.remove("hidden");
      }

      document.getElementById("calcBtn").addEventListener("click", calculate);
      document.getElementById("resetBtn").addEventListener("click", () => {
        [
          "grossPay",
          "pretaxDeductions",
          "dependents",
          "extraWithholding",
          "otherAnnualIncome",
          "stateRate",
        ].forEach((id) => {
          document.getElementById(id).value = "";
        });
        document.getElementById("filingStatus").value = "single";
        document.getElementById("payFrequency").value = "26";
        document.getElementById("resultsSection").classList.add("hidden");
        document.getElementById("calcError").classList.add("hidden");
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
