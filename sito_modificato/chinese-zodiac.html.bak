        { element: "Wood", yinYang: "Yin" },
        { element: "Fire", yinYang: "Yang" },
        { element: "Fire", yinYang: "Yin" },
        { element: "Earth", yinYang: "Yang" },
        { element: "Earth", yinYang: "Yin" },
        { element: "Metal", yinYang: "Yang" },
        { element: "Metal", yinYang: "Yin" },
        { element: "Water", yinYang: "Yang" },
        { element: "Water", yinYang: "Yin" },
      ];

      // Hour animal mapping (local time, approximate)
      const hourAnimals = [
        { start: 23, end: 1, name: "Rat" },
        { start: 1, end: 3, name: "Ox" },
        { start: 3, end: 5, name: "Tiger" },
        { start: 5, end: 7, name: "Rabbit" },
        { start: 7, end: 9, name: "Dragon" },
        { start: 9, end: 11, name: "Snake" },
        { start: 11, end: 13, name: "Horse" },
        { start: 13, end: 15, name: "Goat" },
        { start: 15, end: 17, name: "Monkey" },
        { start: 17, end: 19, name: "Rooster" },
        { start: 19, end: 21, name: "Dog" },
        { start: 21, end: 23, name: "Pig" },
      ];

      function parseISODate(str) {
        const [y, m, d] = str.split("-").map(Number);
        return new Date(y, m - 1, d);
      }

      function getAdjustedYear(date) {
        const year = date.getFullYear();
        const cny = chineseNewYearDates[year];
        if (!cny) return year; // fallback
        const cnyDate = parseISODate(cny);
        return date < cnyDate ? year - 1 : year;
      }

      function getZodiacProfile(date) {
        const adjustedYear = getAdjustedYear(date);
        const cycleIndex = (adjustedYear - 4) % 60;
        const animalIndex = (adjustedYear - 4) % 12;
        const stemIndex = (adjustedYear - 4) % 10;

        const animal = animals[(animalIndex + 12) % 12];
        const stem = stems[(stemIndex + 10) % 10];

        return {
          adjustedYear,
          animal,
          element: stem.element,
          yinYang: stem.yinYang,
        };
      }

      function getHourAnimal(hour) {
        if (isNaN(hour)) return null;
        for (const h of hourAnimals) {
          if (h.start === 23) {
            if (hour >= 23 || hour < 1) return h;
          } else if (hour >= h.start && hour < h.end) {
            return h;
          }
        }
        return null;
      }

      function buildYearsTable() {
        const tbody = document.getElementById("years-table-body");
        if (!tbody) return;
        const start = 1924;
        const end = 2043;
        let rows = "";
        for (let y = start; y <= end; y++) {
          const dummyDate = new Date(y, 6, 1); // mid‑year, safely after CNY
          const profile = getZodiacProfile(dummyDate);
          rows += `
        <tr>
          <td class="px-3 py-1 whitespace-nowrap">${y}</td>
          <td class="px-3 py-1 whitespace-nowrap">${profile.animal.name}</td>
          <td class="px-3 py-1 whitespace-nowrap">${profile.element}</td>
          <td class="px-3 py-1 whitespace-nowrap">${profile.yinYang}</td>
        </tr>`;
        }
        tbody.innerHTML = rows;
      }

      function formatDateHuman(d) {
        return d.toLocaleDateString(undefined, {
          year: "numeric",
          month: "long",
          day: "numeric",
        });
      }

      document.addEventListener("DOMContentLoaded", () => {
        buildYearsTable();

        const calcBtn = document.getElementById("calc-btn");
        const resetBtn = document.getElementById("reset-btn");
        const birthInput = document.getElementById("birthdate");
        const timeInput = document.getElementById("birthtime");
        const resultCard = document.getElementById("result-card");

        calcBtn.addEventListener("click", () => {
          const val = birthInput.value;
          if (!val) {
            alert("Please select your birthdate first.");
            return;
          }
          const [y, m, d] = val.split("-").map(Number);
          const birthDate = new Date(y, m - 1, d);
          if (isNaN(birthDate.getTime())) {
            alert("Invalid date. Please check your input.");
            return;
          }

          const profile = getZodiacProfile(birthDate);
          const { adjustedYear, animal, element, yinYang } = profile;

          const birthText = `Born on ${formatDateHuman(
            birthDate
          )} (Chinese zodiac year: ${adjustedYear})`;
          document.getElementById("result-birth").textContent = birthText;
          document.getElementById(
            "result-animal"
          ).textContent = `${animal.name} (${adjustedYear})`;
          document.getElementById(
            "result-chinese-name"
          ).textContent = `${animal.hanzi} · ${animal.pinyin}`;
          document.getElementById(
            "result-element"
          ).textContent = `${element} element`;
          document.getElementById("result-yin-yang").textContent = yinYang;

          const yearsInCycle = [];
          for (
            let year = adjustedYear - 60;
            year <= adjustedYear + 60;
            year += 12
          ) {
            if (year >= 1900 && year <= 2100) yearsInCycle.push(year);
          }
          document.getElementById("result-years").textContent = `Typical ${
            animal.name
          } years include: ${yearsInCycle.join(
            ", "
          )} (check Chinese New Year if born in Jan/Feb).`;

          document.getElementById("result-summary").textContent =
            animal.summary;
          document.getElementById("result-directions").textContent =
            animal.directions;
          document.getElementById("result-compatible").textContent =
            animal.compatible.join(", ");
          document.getElementById("result-challenging").textContent =
            animal.challenging.join(", ");

          const timeVal = timeInput.value;
          let hourText = "";
          if (timeVal) {
            const [hh] = timeVal.split(":").map(Number);
            const hourAnimal = getHourAnimal(hh);
            if (hourAnimal) {
              hourText = `Hour animal (approx.): ${hourAnimal.name}.`;
            }
          }
          document.getElementById("result-hour").textContent = hourText;

          resultCard.classList.remove("hidden");
        });

        resetBtn.addEventListener("click", () => {
          birthInput.value = "";
          timeInput.value = "";
          resultCard.classList.add("hidden");
        });

        // Simple FAQ toggle (in addition to global script_faq if any)
        document
          .querySelectorAll("#faq-container .faq-question")
          .forEach((btn) => {
            btn.addEventListener("click", () => {
              const answer = btn.parentElement.querySelector(".faq-answer");
              const symbol = btn.querySelector("span:last-child");
              const isHidden = answer.classList.contains("hidden");
              document
                .querySelectorAll("#faq-container .faq-answer")
                .forEach((a) => a.classList.add("hidden"));
              document
                .querySelectorAll(
                  "#faq-container .faq-question span:last-child"
                )
                .forEach((s) => (s.textContent = "+"));
              if (isHidden) {
                answer.classList.remove("hidden");
                symbol.textContent = "−";
              }
            });
          });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
