    <script>
      (function () {
        const BTU_IT_TO_W = 0.29307107;
        const BTU_TH_TO_W = 1054.35 / 3600; // thermochemical
        const TON_TO_BTU_HR = 12000;

        const btuInput = document.getElementById("btuInput");
        const wattInput = document.getElementById("wattInput");
        const btuStandard = document.getElementById("btuStandard");
        const btuStandard2 = document.getElementById("btuStandard2");

        const wattsFromBtuEl = document.getElementById("wattsFromBtu");
        const kwFromBtuEl = document.getElementById("kwFromBtu");
        const tonsFromBtuEl = document.getElementById("tonsFromBtu");

        const btuFromWattsEl = document.getElementById("btuFromWatts");
        const kwFromWattsEl = document.getElementById("kwFromWatts");
        const tonsFromWattsEl = document.getElementById("tonsFromWatts");

        function getFactorToW(std) {
          return std === "th" ? BTU_TH_TO_W : BTU_IT_TO_W;
        }

        function formatNumber(val) {
          if (!isFinite(val)) return "0";
          if (Math.abs(val) >= 1000)
            return val.toLocaleString(undefined, { maximumFractionDigits: 0 });
          if (Math.abs(val) >= 10)
            return val.toLocaleString(undefined, { maximumFractionDigits: 1 });
          return val.toLocaleString(undefined, { maximumFractionDigits: 3 });
        }

        function updateFromBtu() {
          const raw = parseFloat(btuInput.value);
          if (isNaN(raw)) {
            wattsFromBtuEl.textContent = "0";
            kwFromBtuEl.textContent = "0";
            tonsFromBtuEl.textContent = "0";
            return;
          }
          const factor = getFactorToW(btuStandard.value);
          const watts = raw * factor;
          const kw = watts / 1000;
          const tons = raw / TON_TO_BTU_HR;

          wattsFromBtuEl.textContent = formatNumber(watts);
          kwFromBtuEl.textContent = formatNumber(kw);
          tonsFromBtuEl.textContent = formatNumber(tons);
        }

        function updateFromWatts() {
          const raw = parseFloat(wattInput.value);
          if (isNaN(raw)) {
            btuFromWattsEl.textContent = "0";
            kwFromWattsEl.textContent = "0";
            tonsFromWattsEl.textContent = "0";
            return;
          }
          const factor = getFactorToW(btuStandard2.value);
          const btuHr = raw / factor;
          const kw = raw / 1000;
          const tons = btuHr / TON_TO_BTU_HR;

          btuFromWattsEl.textContent = formatNumber(btuHr);
          kwFromWattsEl.textContent = formatNumber(kw);
          tonsFromWattsEl.textContent = formatNumber(tons);
        }

        btuInput.addEventListener("input", updateFromBtu);
        btuStandard.addEventListener("change", updateFromBtu);

        wattInput.addEventListener("input", updateFromWatts);
        btuStandard2.addEventListener("change", updateFromWatts);

        document.querySelectorAll(".preset-btn").forEach((btn) => {
          btn.addEventListener("click", () => {
            const val = btn.getAttribute("data-preset-btu");
            btuInput.value = val;
            updateFromBtu();
          });
        });

        // Initialize with empty values
        updateFromBtu();
        updateFromWatts();
      })();
    </script>

