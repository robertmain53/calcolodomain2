<script>
  (function() {
    const FACTOR = 282.481; // exact factor for L/100km <-> MPG(UK)

    const lInput = document.getElementById('lPer100km');
    const mpgInput = document.getElementById('mpgUk');
    const swapBtn = document.getElementById('swapDirection');
    const resetBtn = document.getElementById('resetBtn');
    const directionHint = document.getElementById('directionHint');
    const resultSummary = document.getElementById('resultSummary');
    const resultText = document.getElementById('resultText');

    let mode = 'L_TO_MPG'; // or 'MPG_TO_L'
    let isUpdating = false;

    function formatNumber(value, decimals) {
      if (!isFinite(value)) return '';
      return Number(value).toFixed(decimals).replace(/\.0+$/,'').replace(/(\.\d*[1-9])0+$/,'$1');
    }

    function updateDirectionHint() {
      if (mode === 'L_TO_MPG') {
        directionHint.innerHTML = 'Currently converting from <span class="font-semibold">L/100km → MPG(UK)</span>.';
      } else {
        directionHint.innerHTML = 'Currently converting from <span class="font-semibold">MPG(UK) → L/100km</span>.';
      }
    }

    function updateResultSummary(fromValue, fromUnit, toValue, toUnit) {
      if (fromValue == null || toValue == null || fromValue === '' || toValue === '') {
        resultSummary.classList.add('hidden');
        return;
      }
      resultText.textContent = `${fromValue} ${fromUnit} is equivalent to ${toValue} ${toUnit}.`;
      resultSummary.classList.remove('hidden');
    }

    function convertFromL() {
      const val = parseFloat(lInput.value);
      if (!isFinite(val) || val <= 0) {
        mpgInput.value = '';
        resultSummary.classList.add('hidden');
        return;
      }
      const mpg = FACTOR / val;
      const mpgStr = formatNumber(mpg, 2);
      mpgInput.value = mpgStr;
      updateResultSummary(formatNumber(val, 3), 'L/100km', mpgStr, 'MPG(UK)');
    }

    function convertFromMpg() {
      const val = parseFloat(mpgInput.value);
      if (!isFinite(val) || val <= 0) {
        lInput.value = '';
        resultSummary.classList.add('hidden');
        return;
      }
      const l = FACTOR / val;
      const lStr = formatNumber(l, 3);
      lInput.value = lStr;
      updateResultSummary(formatNumber(val, 2), 'MPG(UK)', lStr, 'L/100km');
    }

    lInput.addEventListener('input', function() {
      if (isUpdating) return;
      isUpdating = true;
      mode = 'L_TO_MPG';
      updateDirectionHint();
      convertFromL();
      isUpdating = false;
    });

    mpgInput.addEventListener('input', function() {
      if (isUpdating) return;
      isUpdating = true;
      mode = 'MPG_TO_L';
      updateDirectionHint();
      convertFromMpg();
      isUpdating = false;
    });

    swapBtn.addEventListener('click', function() {
      mode = (mode === 'L_TO_MPG') ? 'MPG_TO_L' : 'L_TO_MPG';
      updateDirectionHint();
      // Recalculate based on the currently filled field
      if (mode === 'L_TO_MPG') {
        convertFromL();
      } else {
        convertFromMpg();
      }
    });

    resetBtn.addEventListener('click', function() {
      lInput.value = '';
      mpgInput.value = '';
      mode = 'L_TO_MPG';
      updateDirectionHint();
      resultSummary.classList.add('hidden');
    });

    // Initialize
    updateDirectionHint();
  })();
</script>

