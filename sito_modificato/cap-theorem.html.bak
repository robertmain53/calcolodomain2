        function calculate() {
            // Placeholder logic for CAP theorem visualization
            const consistency = document.getElementById('consistency').value;
            const availability = document.getElementById('availability').value;
            const partitionTolerance = document.getElementById('partitionTolerance').checked;
            let characteristic = 'Unknown';

            if (consistency === 'strong' && availability === 'high' && !partitionTolerance) {
                characteristic = 'Highly Consistent and Available';
            } else if (consistency === 'eventual' && partitionTolerance) {
                characteristic = 'Eventual Consistency with Partition Tolerance';
            }

            document.getElementById('systemCharacteristic').innerText = characteristic;
        }
    </script>


