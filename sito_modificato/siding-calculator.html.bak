    <script src="/assets/js/mobile-menu.js"></script>
    <script src="/search.js"></script>

    <script>
      // Mobile menu toggle
      document
        .getElementById("mobile-menu-toggle")
        ?.addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          if (!menu) return;
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          this.setAttribute("aria-expanded", String(isHidden));
        });

      const unitSystemEl = document.getElementById("unit-system");
      const sidingTypeEl = document.getElementById("siding-type");
      const panelLengthEl = document.getElementById("panel-length");
      const panelExposureEl = document.getElementById("panel-exposure");
      const panelLengthUnitEl = document.getElementById("panel-length-unit");
      const panelExposureUnitEl = document.getElementById(
        "panel-exposure-unit"
      );
      const wastePercentEl = document.getElementById("waste-percent");
      const costModeEl = document.getElementById("cost-mode");
      const costValueContainer = document.getElementById(
        "cost-value-container"
      );
      const costValueEl = document.getElementById("cost-value");
      const costHintEl = document.getElementById("cost-hint");
      const wallsContainer = document.getElementById("walls-container");
      const openingsContainer = document.getElementById("openings-container");
      const addWallBtn = document.getElementById("add-wall");
      const addOpeningBtn = document.getElementById("add-opening");
      const calcBtn = document.getElementById("calculate-btn");

      const resultsEl = document.getElementById("results");
      const grossAreaEl = document.getElementById("gross-area");
      const openingsAreaEl = document.getElementById("openings-area");
      const netAreaEl = document.getElementById("net-area");
      const wasteAreaEl = document.getElementById("waste-area");
      const squaresEl = document.getElementById("squares");
      const panelCoverageEl = document.getElementById("panel-coverage");
      const panelsNeededEl = document.getElementById("panels-needed");
      const wasteDisplayEl = document.getElementById("waste-display");
      const costTotalEl = document.getElementById("cost-total");

      function getUnitLabels() {
        const system = unitSystemEl.value;
        if (system === "metric") {
          return { length: "m", small: "cm", area: "m²", squareArea: 9.290304 }; // 100 ft² in m²
        }
        return { length: "ft", small: "in", area: "ft²", squareArea: 100 };
      }

      function updateUnitLabels() {
        const labels = getUnitLabels();
        panelLengthUnitEl.textContent = labels.length;
        panelExposureUnitEl.textContent = labels.small;

        // Update placeholders on existing wall/opening rows
        document
          .querySelectorAll('[data-role="width-unit"]')
          .forEach((el) => (el.textContent = labels.length));
        document
          .querySelectorAll('[data-role="height-unit"]')
          .forEach((el) => (el.textContent = labels.length));
        document
          .querySelectorAll('[data-role="small-unit"]')
          .forEach((el) => (el.textContent = labels.small));
      }

      function applySidingPreset() {
        const type = sidingTypeEl.value;
        const system = unitSystemEl.value;

        if (type === "vinyl") {
          if (system === "imperial") {
            panelLengthEl.value = 12;
            panelExposureEl.value = 8;
          } else {
            panelLengthEl.value = 3.66;
            panelExposureEl.value = 20;
          }
        } else if (type === "fiber") {
          if (system === "imperial") {
            panelLengthEl.value = 12;
            panelExposureEl.value = 7;
          } else {
            panelLengthEl.value = 3.66;
            panelExposureEl.value = 18;
          }
        } else if (type === "shingle") {
          if (system === "imperial") {
            panelLengthEl.value = 4;
            panelExposureEl.value = 7;
          } else {
            panelLengthEl.value = 1.22;
            panelExposureEl.value = 18;
          }
        } else if (type === "panel") {
          if (system === "imperial") {
            panelLengthEl.value = 8;
            panelExposureEl.value = 48;
          } else {
            panelLengthEl.value = 2.44;
            panelExposureEl.value = 122;
          }
        }
      }

      unitSystemEl.addEventListener("change", () => {
        updateUnitLabels();
        applySidingPreset();
      });

      sidingTypeEl.addEventListener("change", () => {
        if (sidingTypeEl.value !== "custom") {
          applySidingPreset();
        }
      });

      costModeEl.addEventListener("change", () => {
        const mode = costModeEl.value;
        if (mode === "none") {
          costValueContainer.classList.add("hidden");
          costHintEl.textContent = "";
          costValueEl.value = "";
          return;
        }
        costValueContainer.classList.remove("hidden");
        if (mode === "sqft") {
          costHintEl.textContent =
            "Enter material cost per square foot (or per square meter in metric).";
        } else if (mode === "square") {
          costHintEl.textContent =
            "Enter material cost per square (100 ft² of coverage).";
        } else if (mode === "panel") {
          costHintEl.textContent =
            "Enter material cost per siding panel or board.";
        }
      });

      function createWallRow(index) {
        const labels = getUnitLabels();
        const div = document.createElement("div");
        div.className = "border rounded-md p-4 bg-gray-50";
        div.dataset.type = "wall";

        div.innerHTML = `
      <div class="flex justify-between items-center mb-3">
        <h3 class="font-semibold text-gray-800">Wall #${index}</h3>
        <button type="button" class="text-xs text-red-600 hover:underline" data-action="remove-wall">Remove</button>
      </div>
      <div class="grid md:grid-cols-4 gap-3">
        <div>
          <label class="block text-xs font-medium text-gray-700 mb-1">Wall type</label>
          <select class="w-full border rounded-md px-2 py-1 text-sm" data-field="wall-type">
            <option value="rect">Rectangle</option>
            <option value="gable">Gable (triangle on top)</option>
          </select>
        </div>
        <div>
          <label class="block text-xs font-medium text-gray-700 mb-1">Width</label>
          <div class="flex gap-1">
            <input type="number" min="0" step="0.01" class="input-small w-full border rounded-md px-2 py-1 text-sm" data-field="width">
            <span class="self-center text-xs text-gray-600" data-role="width-unit">${labels.length}</span>
          </div>
        </div>
        <div>
          <label class="block text-xs font-medium text-gray-700 mb-1">Wall height</label>
          <div class="flex gap-1">
            <input type="number" min="0" step="0.01" class="input-small w-full border rounded-md px-2 py-1 text-sm" data-field="height">
            <span class="self-center text-xs text-gray-600" data-role="height-unit">${labels.length}</span>
          </div>
        </div>
        <div>
          <label class="block text-xs font-medium text-gray-700 mb-1">Gable height</label>
          <div class="flex gap-1">
            <input type="number" min="0" step="0.01" class="input-small w-full border rounded-md px-2 py-1 text-sm" data-field="gable-height" disabled>
            <span class="self-center text-xs text-gray-600" data-role="height-unit">${labels.length}</span>
          </div>
        </div>
      </div>
    `;
        return div;
      }

      function createOpeningRow(index) {
        const labels = getUnitLabels();
        const div = document.createElement("div");
        div.className = "border rounded-md p-4 bg-gray-50";
        div.dataset.type = "opening";

        div.innerHTML = `
      <div class="flex justify-between items-center mb-3">
        <h3 class="font-semibold text-gray-800">Opening #${index}</h3>
        <button type="button" class="text-xs text-red-600 hover:underline" data-action="remove-opening">Remove</button>
      </div>
      <div class="grid md:grid-cols-3 gap-3">
        <div>
          <label class="block text-xs font-medium text-gray-700 mb-1">Label (optional)</label>
          <input type="text" class="w-full border rounded-md px-2 py-1 text-sm" data-field="label" placeholder="Window, door, etc.">
        </div>
        <div>
          <label class="block text-xs font-medium text-gray-700 mb-1">Width</label>
          <div class="flex gap-1">
            <input type="number" min="0" step="0.01" class="input-small w-full border rounded-md px-2 py-1 text-sm" data-field="width">
            <span class="self-center text-xs text-gray-600" data-role="width-unit">${labels.length}</span>
          </div>
        </div>
        <div>
          <label class="block text-xs font-medium text-gray-700 mb-1">Height</label>
          <div class="flex gap-1">
            <input type="number" min="0" step="0.01" class="input-small w-full border rounded-md px-2 py-1 text-sm" data-field="height">
            <span class="self-center text-xs text-gray-600" data-role="height-unit">${labels.length}</span>
          </div>
        </div>
      </div>
    `;
        return div;
      }

      addWallBtn.addEventListener("click", () => {
        const index =
          wallsContainer.querySelectorAll('[data-type="wall"]').length + 1;
        const row = createWallRow(index);
        wallsContainer.appendChild(row);
      });

      addOpeningBtn.addEventListener("click", () => {
        const index =
          openingsContainer.querySelectorAll('[data-type="opening"]').length +
          1;
        const row = createOpeningRow(index);
        openingsContainer.appendChild(row);
      });

      wallsContainer.addEventListener("change", (e) => {
        const target = e.target;
        if (target.dataset.field === "wall-type") {
          const row = target.closest('[data-type="wall"]');
          if (!row) return;
          const gableInput = row.querySelector('[data-field="gable-height"]');
          if (!gableInput) return;
          if (target.value === "gable") {
            gableInput.disabled = false;
          } else {
            gableInput.disabled = true;
            gableInput.value = "";
          }
        }
      });

      wallsContainer.addEventListener("click", (e) => {
        const btn = e.target.closest('button[data-action="remove-wall"]');
        if (!btn) return;
        const row = btn.closest('[data-type="wall"]');
        if (row) row.remove();
        // Re-number walls
        wallsContainer
          .querySelectorAll('[data-type="wall"]')
          .forEach((row, idx) => {
            const title = row.querySelector("h3");
            if (title) title.textContent = `Wall #${idx + 1}`;
          });
      });

      openingsContainer.addEventListener("click", (e) => {
        const btn = e.target.closest('button[data-action="remove-opening"]');
        if (!btn) return;
        const row = btn.closest('[data-type="opening"]');
        if (row) row.remove();
        // Re-number openings
        openingsContainer
          .querySelectorAll('[data-type="opening"]')
          .forEach((row, idx) => {
            const title = row.querySelector("h3");
            if (title) title.textContent = `Opening #${idx + 1}`;
          });
      });

      function parseNumber(input) {
        const v = parseFloat(input);
        return isNaN(v) || v < 0 ? 0 : v;
      }

      function calculateArea() {
        const system = unitSystemEl.value;
        const labels = getUnitLabels();

        let grossArea = 0;
        wallsContainer.querySelectorAll('[data-type="wall"]').forEach((row) => {
          const type = row.querySelector('[data-field="wall-type"]').value;
          const width = parseNumber(
            row.querySelector('[data-field="width"]').value
          );
          const height = parseNumber(
            row.querySelector('[data-field="height"]').value
          );
          const gableHeight = parseNumber(
            row.querySelector('[data-field="gable-height"]').value
          );

          if (width <= 0 || height <= 0) return;

          let area = width * height;
          if (type === "gable" && gableHeight > 0) {
            area += 0.5 * width * gableHeight;
          }
          grossArea += area;
        });

        let openingsArea = 0;
        openingsContainer
          .querySelectorAll('[data-type="opening"]')
          .forEach((row) => {
            const width = parseNumber(
              row.querySelector('[data-field="width"]').value
            );
            const height = parseNumber(
              row.querySelector('[data-field="height"]').value
            );
            if (width <= 0 || height <= 0) return;
            openingsArea += width * height;
          });

        let netArea = Math.max(grossArea - openingsArea, 0);
        const wastePercent = parseNumber(wastePercentEl.value);
        const wasteFactor = 1 + wastePercent / 100;
        const areaWithWaste = netArea * wasteFactor;

        // Convert to ft² if metric for squares and cost
        let areaForSquares = areaWithWaste;
        let areaForDisplay = areaWithWaste;
        let grossDisplay = grossArea;
        let openingsDisplay = openingsArea;
        let netDisplay = netArea;
        let areaUnit = labels.area;

        if (system === "metric") {
          // Inputs are in meters, so area is m². Convert to ft² for squares.
          const m2ToFt2 = 10.76391041671;
          areaForSquares = areaWithWaste * m2ToFt2;
          grossDisplay = grossArea;
          openingsDisplay = openingsArea;
          netDisplay = netArea;
          areaUnit = "m²";
        }

        const squares = areaForSquares / 100;

        return {
          system,
          labels,
          grossArea: grossDisplay,
          openingsArea: openingsDisplay,
          netArea: netDisplay,
          areaWithWaste: areaWithWaste,
          areaUnit,
          squares,
          wastePercent,
        };
      }

      function calculatePanels(areaWithWaste) {
        const system = unitSystemEl.value;
        let panelLength = parseNumber(panelLengthEl.value);
        let exposure = parseNumber(panelExposureEl.value);

        if (panelLength <= 0 || exposure <= 0) {
          return {
            coveragePerPanel: 0,
            panelsNeeded: 0,
            coverageUnit: system === "metric" ? "m²" : "ft²",
          };
        }

        let coveragePerPanel;
        let coverageUnit;

        if (system === "imperial") {
          const exposureFt = exposure / 12;
          coveragePerPanel = panelLength * exposureFt;
          coverageUnit = "ft²";
        } else {
          const exposureM = exposure / 100;
          coveragePerPanel = panelLength * exposureM;
          coverageUnit = "m²";
        }

        const panelsNeeded =
          coveragePerPanel > 0 ? areaWithWaste / coveragePerPanel : 0;
        return { coveragePerPanel, panelsNeeded, coverageUnit };
      }

      function calculateCost(areaWithWaste, squares, panelsNeeded) {
        const mode = costModeEl.value;
        const costPer = parseNumber(costValueEl.value);
        if (mode === "none" || costPer <= 0) return null;

        let total = 0;
        if (mode === "sqft") {
          // If metric, areaWithWaste is m²; treat cost as per m² in that case.
          total = areaWithWaste * costPer;
        } else if (mode === "square") {
          total = squares * costPer;
        } else if (mode === "panel") {
          total = panelsNeeded * costPer;
        }
        return total;
      }

      function formatNumber(value, decimals = 2) {
        return value.toLocaleString(undefined, {
          maximumFractionDigits: decimals,
          minimumFractionDigits: decimals,
        });
      }

      calcBtn.addEventListener("click", () => {
        const areaData = calculateArea();
        const {
          system,
          labels,
          grossArea,
          openingsArea,
          netArea,
          areaWithWaste,
          areaUnit,
          squares,
          wastePercent,
        } = areaData;

        if (grossArea <= 0) {
          alert("Please add at least one wall with non-zero width and height.");
          return;
        }

        const panelData = calculatePanels(areaWithWaste);
        const { coveragePerPanel, panelsNeeded, coverageUnit } = panelData;
        const costTotal = calculateCost(
          system === "metric" && costModeEl.value === "square"
            ? areaWithWaste * 10.76391041671
            : areaWithWaste,
          squares,
          panelsNeeded
        );

        grossAreaEl.textContent = `${formatNumber(grossArea)} ${areaUnit}`;
        openingsAreaEl.textContent = `${formatNumber(
          openingsArea
        )} ${areaUnit}`;
        netAreaEl.textContent = `${formatNumber(netArea)} ${areaUnit}`;
        wasteAreaEl.textContent = `${formatNumber(areaWithWaste)} ${areaUnit}`;
        squaresEl.textContent = `${formatNumber(squares)} squares`;

        if (coveragePerPanel > 0) {
          panelCoverageEl.textContent = `${formatNumber(
            coveragePerPanel
          )} ${coverageUnit} per panel`;
          panelsNeededEl.textContent = `${Math.ceil(
            panelsNeeded
          ).toLocaleString()} panels (calculated ${formatNumber(
            panelsNeeded
          )} exact)`;
        } else {
          panelCoverageEl.textContent =
            "Enter panel length and exposure to see panel count.";
          panelsNeededEl.textContent = "–";
        }

        wasteDisplayEl.textContent = `${formatNumber(wastePercent, 1)}%`;

        if (costTotal != null) {
          costTotalEl.textContent = `$${formatNumber(costTotal, 2)}`;
        } else {
          costTotalEl.textContent = "Not calculated";
        }

        resultsEl.classList.remove("hidden");
      });

      // Initialize defaults
      updateUnitLabels();
      applySidingPreset();
      // Add one default wall
      addWallBtn.click();
    </script>

