    <script src="https://cdn.jsdelivr.net/npm/chart.js@4.4.0/dist/chart.umd.min.js"></script>
    <script>
      // Utility: Antoine Psat in kPa
      function psat_kPa(A, B, C, T_C) {
        const log10PmmHg = A - B / (T_C + C);
        const PmmHg = Math.pow(10, log10PmmHg);
        return PmmHg * 0.133322; // mmHg to kPa
      }

      function solveBubbleT(x1, P, A1, B1, C1, A2, B2, C2) {
        let T = 60; // initial guess
        for (let i = 0; i < 50; i++) {
          const P1 = psat_kPa(A1, B1, C1, T);
          const P2 = psat_kPa(A2, B2, C2, T);
          const f = x1 * P1 + (1 - x1) * P2 - P;
          const dT = 0.01;
          const P1d = psat_kPa(A1, B1, C1, T + dT);
          const P2d = psat_kPa(A2, B2, C2, T + dT);
          const fd = x1 * P1d + (1 - x1) * P2d - P;
          const df = (fd - f) / dT;
          if (Math.abs(df) < 1e-6) break;
          T -= f / df;
          if (T < -50) T = -50;
          if (T > 300) T = 300;
          if (Math.abs(f) < 1e-4) break;
        }
        return T;
      }

      function solveDewT(y1, P, A1, B1, C1, A2, B2, C2) {
        let T = 60;
        for (let i = 0; i < 50; i++) {
          const P1 = psat_kPa(A1, B1, C1, T);
          const P2 = psat_kPa(A2, B2, C2, T);
          const f = (y1 * P) / P1 + ((1 - y1) * P) / P2 - 1;
          const dT = 0.01;
          const P1d = psat_kPa(A1, B1, C1, T + dT);
          const P2d = psat_kPa(A2, B2, C2, T + dT);
          const fd = (y1 * P) / P1d + ((1 - y1) * P) / P2d - 1;
          const df = (fd - f) / dT;
          if (Math.abs(df) < 1e-6) break;
          T -= f / df;
          if (T < -50) T = -50;
          if (T > 300) T = 300;
          if (Math.abs(f) < 1e-4) break;
        }
        return T;
      }

      function rachfordRice(z1, K1, K2) {
        const z2 = 1 - z1;
        let betaLow = 0,
          betaHigh = 1;
        function f(beta) {
          return (
            (z1 * (K1 - 1)) / (1 + beta * (K1 - 1)) +
            (z2 * (K2 - 1)) / (1 + beta * (K2 - 1))
          );
        }
        let fL = f(betaLow),
          fH = f(betaHigh);
        if (fL * fH > 0) {
          return null;
        }
        for (let i = 0; i < 60; i++) {
          const betaMid = 0.5 * (betaLow + betaHigh);
          const fM = f(betaMid);
          if (Math.abs(fM) < 1e-8) return betaMid;
          if (fL * fM < 0) {
            betaHigh = betaMid;
            fH = fM;
          } else {
            betaLow = betaMid;
            fL = fM;
          }
        }
        return 0.5 * (betaLow + betaHigh);
      }

      const modeEl = document.getElementById("mode");
      const presetEl = document.getElementById("preset");
      const pConstWrapper = document.getElementById("p-const-wrapper");
      const tConstWrapper = document.getElementById("t-const-wrapper");
      const flashZWrapper = document.getElementById("flash-z-wrapper");
      const runBtn = document.getElementById("runBtn");
      const scalarResultsEl = document.getElementById("scalar-results");
      const downloadCsvBtn = document.getElementById("downloadCsv");
      let chart;

      function applyPreset() {
        const val = presetEl.value;
        const name1 = document.getElementById("name1");
        const name2 = document.getElementById("name2");
        const A1 = document.getElementById("A1");
        const B1 = document.getElementById("B1");
        const C1 = document.getElementById("C1");
        const A2 = document.getElementById("A2");
        const B2 = document.getElementById("B2");
        const C2 = document.getElementById("C2");

        if (val === "ethanol-water") {
          name1.value = "Ethanol";
          A1.value = 8.20417;
          B1.value = 1642.89;
          C1.value = 230.3;
          name2.value = "Water";
          A2.value = 8.07131;
          B2.value = 1730.63;
          C2.value = 233.426;
        } else if (val === "benzene-toluene") {
          name1.value = "Benzene";
          A1.value = 6.90565;
          B1.value = 1211.033;
          C1.value = 220.79;
          name2.value = "Toluene";
          A2.value = 6.95464;
          B2.value = 1344.8;
          C2.value = 219.48;
        }
      }

      presetEl.addEventListener("change", () => {
        if (presetEl.value !== "custom") applyPreset();
      });

      function updateModeUI() {
        const mode = modeEl.value;
        if (mode === "txy") {
          pConstWrapper.classList.remove("hidden");
          tConstWrapper.classList.add("hidden");
          flashZWrapper.classList.add("hidden");
        } else if (mode === "pxy") {
          pConstWrapper.classList.add("hidden");
          tConstWrapper.classList.remove("hidden");
          flashZWrapper.classList.add("hidden");
        } else {
          pConstWrapper.classList.remove("hidden");
          tConstWrapper.classList.remove("hidden");
          flashZWrapper.classList.remove("hidden");
        }
        scalarResultsEl.classList.add("hidden");
      }

      modeEl.addEventListener("change", updateModeUI);
      updateModeUI();
      applyPreset();

      let lastData = null;

      function runCalculation() {
        const mode = modeEl.value;
        const A1 = parseFloat(document.getElementById("A1").value);
        const B1 = parseFloat(document.getElementById("B1").value);
        const C1 = parseFloat(document.getElementById("C1").value);
        const A2 = parseFloat(document.getElementById("A2").value);
        const B2 = parseFloat(document.getElementById("B2").value);
        const C2 = parseFloat(document.getElementById("C2").value);
        const name1 = document.getElementById("name1").value || "Component 1";
        const name2 = document.getElementById("name2").value || "Component 2";

        const P = parseFloat(document.getElementById("P").value);
        const T = parseFloat(document.getElementById("T").value);
        const z1 = parseFloat(document.getElementById("z1").value);

        if (
          !isFinite(A1) ||
          !isFinite(B1) ||
          !isFinite(C1) ||
          !isFinite(A2) ||
          !isFinite(B2) ||
          !isFinite(C2)
        ) {
          alert("Please enter valid Antoine constants for both components.");
          return;
        }

        if (mode === "txy") {
          if (!isFinite(P) || P <= 0) {
            alert("Please enter a valid pressure P > 0.");
            return;
          }
          const x = [],
            y = [],
            Tb = [];
          const n = 41;
          for (let i = 0; i < n; i++) {
            const x1 = i / (n - 1);
            const Tbi = solveBubbleT(x1, P, A1, B1, C1, A2, B2, C2);
            const P1 = psat_kPa(A1, B1, C1, Tbi);
            const y1 = (x1 * P1) / P;
            x.push(x1);
            y.push(Math.max(0, Math.min(1, y1)));
            Tb.push(Tbi);
          }
          lastData = { mode, x, y, Tb, P, name1, name2 };
          scalarResultsEl.classList.add("hidden");
          drawChartTxy(lastData);
        } else if (mode === "pxy") {
          if (!isFinite(T)) {
            alert("Please enter a valid temperature.");
            return;
          }
          const x = [],
            y = [],
            Pb = [],
            Pd = [];
          const n = 41;
          const P1sat = psat_kPa(A1, B1, C1, T);
          const P2sat = psat_kPa(A2, B2, C2, T);
          for (let i = 0; i < n; i++) {
            const x1 = i / (n - 1);
            const y1 = (x1 * P1sat) / (x1 * P1sat + (1 - x1) * P2sat);
            const Pb_i = x1 * P1sat + (1 - x1) * P2sat;
            const denom = y1 / P1sat + (1 - y1) / P2sat;
            const Pd_i = 1 / denom;
            x.push(x1);
            y.push(y1);
            Pb.push(Pb_i);
            Pd.push(Pd_i);
          }
          lastData = { mode, x, y, Pb, Pd, T, name1, name2 };
          scalarResultsEl.classList.add("hidden");
          drawChartPxy(lastData);
        } else {
          if (
            !isFinite(T) ||
            !isFinite(P) ||
            !isFinite(z1) ||
            z1 < 0 ||
            z1 > 1
          ) {
            alert(
              "Please enter valid T, P and overall composition z1 between 0 and 1."
            );
            return;
          }
          const P1sat = psat_kPa(A1, B1, C1, T);
          const P2sat = psat_kPa(A2, B2, C2, T);
          const K1 = P1sat / P;
          const K2 = P2sat / P;
          const beta = rachfordRice(z1, K1, K2);
          if (beta === null) {
            alert(
              "Flash calculation failed: no feasible vapor fraction. Check T, P and composition."
            );
            return;
          }
          const z2 = 1 - z1;
          const x1 = z1 / (1 + beta * (K1 - 1));
          const x2 = 1 - x1;
          const y1 = K1 * x1;
          const y2 = 1 - y1;
          const Vfrac = beta;
          const Lfrac = 1 - beta;

          scalarResultsEl.innerHTML = `<p><strong>Flash results (ideal Raoult’s law):</strong></p>
         <ul class="list-disc ml-5">
           <li>Vapor fraction β ≈ <strong>${Vfrac.toFixed(
             3
           )}</strong>, Liquid fraction ≈ <strong>${Lfrac.toFixed(
            3
          )}</strong></li>
           <li>Liquid composition: x₁ = <strong>${x1.toFixed(
             3
           )}</strong>, x₂ = <strong>${x2.toFixed(3)}</strong></li>
           <li>Vapor composition: y₁ = <strong>${y1.toFixed(
             3
           )}</strong>, y₂ = <strong>${y2.toFixed(3)}</strong></li>
         </ul>`;
          scalarResultsEl.classList.remove("hidden");

          lastData = {
            mode,
            T,
            P,
            z1,
            Vfrac,
            Lfrac,
            x1,
            x2,
            y1,
            y2,
            name1,
            name2,
          };
          drawFlashChart(lastData);
        }
      }

      function drawChartTxy(data) {
        const ctx = document.getElementById("vleChart").getContext("2d");
        if (chart) chart.destroy();
        chart = new Chart(ctx, {
          type: "line",
          data: {
            labels: data.x.map((v) => v.toFixed(2)),
            datasets: [
              {
                label: `Bubble curve T(x) – liquid ${data.name1} mole fraction`,
                data: data.Tb,
                borderColor: "#2563eb",
                backgroundColor: "rgba(37,99,235,0.1)",
                tension: 0.15,
                fill: false,
              },
              {
                label: `Dew curve T(y) – vapor ${data.name1} mole fraction`,
                data: data.Tb.map((T, i) => T), // same T, different x-axis (y1)
                borderColor: "#16a34a",
                backgroundColor: "rgba(22,163,74,0.1)",
                tension: 0.15,
                fill: false,
              },
            ],
          },
          options: {
            responsive: true,
            scales: {
              x: {
                title: {
                  display: true,
                  text: `x₁ (liquid) or y₁ (vapor) mole fraction of ${data.name1}`,
                },
              },
              y: {
                title: { display: true, text: "Temperature (°C)" },
              },
            },
          },
        });
      }

      function drawChartPxy(data) {
        const ctx = document.getElementById("vleChart").getContext("2d");
        if (chart) chart.destroy();
        chart = new Chart(ctx, {
          type: "line",
          data: {
            labels: data.x.map((v) => v.toFixed(2)),
            datasets: [
              {
                label: `Bubble curve P(x) – liquid ${data.name1} mole fraction`,
                data: data.Pb,
                borderColor: "#2563eb",
                tension: 0.15,
                fill: false,
              },
              {
                label: `Dew curve P(y) – vapor ${data.name1} mole fraction`,
                data: data.Pd,
                borderColor: "#16a34a",
                tension: 0.15,
                fill: false,
              },
            ],
          },
          options: {
            responsive: true,
            scales: {
              x: {
                title: {
                  display: true,
                  text: `x₁ (liquid) or y₁ (vapor) mole fraction of ${data.name1}`,
                },
              },
              y: {
                title: { display: true, text: "Pressure (kPa)" },
              },
            },
          },
        });
      }

      function drawFlashChart(data) {
        const ctx = document.getElementById("vleChart").getContext("2d");
        if (chart) chart.destroy();
        chart = new Chart(ctx, {
          type: "bar",
          data: {
            labels: [data.name1, data.name2],
            datasets: [
              {
                label: "Liquid phase xᵢ",
                data: [data.x1, data.x2],
                backgroundColor: "rgba(37,99,235,0.6)",
              },
              {
                label: "Vapor phase yᵢ",
                data: [data.y1, data.y2],
                backgroundColor: "rgba(22,163,74,0.6)",
              },
            ],
          },
          options: {
            responsive: true,
            scales: {
              y: {
                min: 0,
                max: 1,
                title: { display: true, text: "Mole fraction" },
              },
            },
          },
        });
      }

      runBtn.addEventListener("click", runCalculation);

      downloadCsvBtn.addEventListener("click", () => {
        if (!lastData) {
          alert("Run a calculation first.");
          return;
        }
        let csv = "";
        if (lastData.mode === "txy") {
          csv += "x1,y1,T_C,P_kPa\n";
          for (let i = 0; i < lastData.x.length; i++) {
            csv += `${lastData.x[i]},${lastData.y[i]},${lastData.Tb[i]},${lastData.P}\n`;
          }
        } else if (lastData.mode === "pxy") {
          csv += "x1,y1,Pb_kPa,Pd_kPa,T_C\n";
          for (let i = 0; i < lastData.x.length; i++) {
            csv += `${lastData.x[i]},${lastData.y[i]},${lastData.Pb[i]},${lastData.Pd[i]},${lastData.T}\n`;
          }
        } else {
          csv += "T_C,P_kPa,z1,Vfrac,Lfrac,x1,x2,y1,y2\n";
          csv += `${lastData.T},${lastData.P},${lastData.z1},${lastData.Vfrac},${lastData.Lfrac},${lastData.x1},${lastData.x2},${lastData.y1},${lastData.y2}\n`;
        }
        const blob = new Blob([csv], { type: "text/csv;charset=utf-8;" });
        const url = URL.createObjectURL(blob);
        const a = document.createElement("a");
        a.href = url;
        a.download = "vle-data.csv";
        document.body.appendChild(a);
        a.click();
        document.body.removeChild(a);
        URL.revokeObjectURL(url);
      });
    </script>

