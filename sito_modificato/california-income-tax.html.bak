      function formatMoney(value) {
        if (!isFinite(value)) return "$0";
        return value.toLocaleString("en-US", {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 0,
        });
      }

      function formatPercent(value) {
        if (!isFinite(value)) return "0%";
        return (value * 100).toFixed(1).replace(/\.0$/, "") + "%";
      }

      function calcCaliforniaTax(taxableIncome, year, status) {
        const brackets = CA_DATA.brackets[year][status];
        let remaining = Math.max(0, taxableIncome);
        let lastLimit = 0;
        let tax = 0;
        const breakdown = [];

        for (const bracket of brackets) {
          if (remaining <= 0) break;
          const span = Math.min(remaining, bracket.upTo - lastLimit);
          if (span > 0) {
            const bracketTax = span * bracket.rate;
            breakdown.push({
              from: lastLimit,
              to: lastLimit + span,
              rate: bracket.rate,
              amount: bracketTax,
            });
            tax += bracketTax;
            remaining -= span;
            lastLimit = bracket.upTo;
          }
        }

        const marginalRate =
          taxableIncome <= 0
            ? 0
            : breakdown.length
            ? breakdown[breakdown.length - 1].rate
            : 0;

        // Mental Health Services Tax
        const mhBase = Math.max(
          0,
          taxableIncome - CA_DATA.mentalHealthThreshold
        );
        const mhTax = mhBase * CA_DATA.mentalHealthRate;

        return {
          baseTax: tax,
          mentalHealthTax: mhTax,
          totalTax: tax + mhTax,
          marginalRate,
          breakdown,
        };
      }

      function updateStandardToggleUI(checked) {
        const thumb = document.getElementById("ca-standard-thumb");
        if (!thumb) return;
        if (checked) {
          thumb.classList.add("translate-x-5");
          thumb.classList.remove("translate-x-0");
        } else {
          thumb.classList.add("translate-x-0");
          thumb.classList.remove("translate-x-5");
        }
      }

      function runCaCalculator() {
        const status = document.getElementById("ca-filing-status").value;
        const year = parseInt(document.getElementById("ca-tax-year").value, 10);

        const wages =
          parseFloat(document.getElementById("ca-wages").value) || 0;
        const otherIncome =
          parseFloat(document.getElementById("ca-other-income").value) || 0;
        const pretax =
          parseFloat(document.getElementById("ca-pretax").value) || 0;
        const itemized =
          parseFloat(document.getElementById("ca-itemized").value) || 0;
        const dependents = Math.max(
          0,
          parseInt(document.getElementById("ca-allowances").value, 10) || 0
        );
        const useStandard = document.getElementById("ca-use-standard").checked;

        const totalIncome = Math.max(0, wages + otherIncome);
        const adjustedIncome = Math.max(0, totalIncome - pretax);

        const stdDed = CA_DATA.standardDeduction[year][status] || 0;
        const deductions = useStandard ? stdDed : itemized;
        const depReduction = dependents * (CA_DATA.perDependent[year] || 0);

        const taxableIncome = Math.max(
          0,
          adjustedIncome - deductions - depReduction
        );

        const taxResult = calcCaliforniaTax(taxableIncome, year, status);

        // Update summary
        document.getElementById("ca-taxable").textContent =
          formatMoney(taxableIncome);
        document.getElementById("ca-tax").textContent = formatMoney(
          taxResult.baseTax
        );
        document.getElementById("ca-mh-tax").textContent = formatMoney(
          taxResult.mentalHealthTax
        );
        document.getElementById("ca-total-tax").textContent = formatMoney(
          taxResult.totalTax
        );

        const effectiveRate =
          totalIncome > 0 ? taxResult.totalTax / totalIncome : 0;
        document.getElementById("ca-effective-rate").textContent =
          formatPercent(effectiveRate);
        document.getElementById("ca-marginal-rate").textContent = formatPercent(
          taxResult.marginalRate
        );

        document.getElementById("ca-total-income").textContent =
          formatMoney(totalIncome);
        const afterTax = totalIncome - taxResult.totalTax;
        document.getElementById("ca-after-tax").textContent =
          formatMoney(afterTax);
        document.getElementById("ca-share").textContent =
          formatPercent(effectiveRate);

        // Bracket breakdown UI
        const bb = document.getElementById("ca-bracket-breakdown");
        bb.innerHTML = "";
        if (!taxResult.breakdown.length) {
          bb.innerHTML =
            '<p class="text-gray-500">No California income tax is due at this income level.</p>';
        } else {
          taxResult.breakdown.forEach((b, idx) => {
            const row = document.createElement("div");
            row.className = "flex justify-between items-baseline";
            const left = document.createElement("div");
            left.className = "text-gray-700";
            left.textContent = `Bracket ${idx + 1}: ${formatMoney(
              b.from
            )} – ${formatMoney(b.to)} at ${formatPercent(b.rate)}`;
            const right = document.createElement("div");
            right.className = "font-medium text-gray-900";
            right.textContent = formatMoney(b.amount);
            row.appendChild(left);
            row.appendChild(right);
            bb.appendChild(row);
          });

          if (taxResult.mentalHealthTax > 0) {
            const mhRow = document.createElement("div");
            mhRow.className =
              "flex justify-between items-baseline border-t border-gray-200 pt-1 mt-1";
            const left = document.createElement("div");
            left.className = "text-gray-700";
            left.textContent =
              "Mental Health Services Tax (1% over $1,000,000)";
            const right = document.createElement("div");
            right.className = "font-semibold text-gray-900";
            right.textContent = formatMoney(taxResult.mentalHealthTax);
            mhRow.appendChild(left);
            mhRow.appendChild(right);
            bb.appendChild(mhRow);
          }
        }
      }

      document.addEventListener("DOMContentLoaded", () => {
        const useStd = document.getElementById("ca-use-standard");
        if (useStd) {
          updateStandardToggleUI(useStd.checked);
          useStd.addEventListener("change", () => {
            updateStandardToggleUI(useStd.checked);
          });
        }

        document
          .getElementById("ca-calc-btn")
          .addEventListener("click", (e) => {
            e.preventDefault();
            runCaCalculator();
          });

        // Recalculate on key inputs blur/change for responsiveness
        [
          "ca-filing-status",
          "ca-tax-year",
          "ca-wages",
          "ca-other-income",
          "ca-pretax",
          "ca-itemized",
          "ca-allowances",
        ].forEach((id) => {
          const el = document.getElementById(id);
          if (el) {
            el.addEventListener("change", () => runCaCalculator());
            el.addEventListener("blur", () => runCaCalculator());
          }
        });
        if (useStd) {
          useStd.addEventListener("change", runCaCalculator);
        }

        // Initial calculation
        runCaCalculator();
      });

