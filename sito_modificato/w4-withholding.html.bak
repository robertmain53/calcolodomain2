    <script>
      // Simple US federal tax brackets for illustration (approximate, single/mfj/hoh)
      const TAX_BRACKETS = {
        single: [
          { upTo: 11600, rate: 0.1 },
          { upTo: 47150, rate: 0.12 },
          { upTo: 100525, rate: 0.22 },
          { upTo: 191950, rate: 0.24 },
          { upTo: 243725, rate: 0.32 },
          { upTo: 609350, rate: 0.35 },
          { upTo: Infinity, rate: 0.37 },
        ],
        mfj: [
          { upTo: 23200, rate: 0.1 },
          { upTo: 94300, rate: 0.12 },
          { upTo: 201050, rate: 0.22 },
          { upTo: 383900, rate: 0.24 },
          { upTo: 487450, rate: 0.32 },
          { upTo: 731200, rate: 0.35 },
          { upTo: Infinity, rate: 0.37 },
        ],
        hoh: [
          { upTo: 16550, rate: 0.1 },
          { upTo: 63100, rate: 0.12 },
          { upTo: 100500, rate: 0.22 },
          { upTo: 191950, rate: 0.24 },
          { upTo: 243700, rate: 0.32 },
          { upTo: 609350, rate: 0.35 },
          { upTo: Infinity, rate: 0.37 },
        ],
      };

      const STANDARD_DEDUCTION = {
        single: 14600,
        mfj: 29200,
        hoh: 21900,
      };

      function calcTax(brackets, taxable) {
        let tax = 0;
        let prev = 0;
        for (const b of brackets) {
          if (taxable <= prev) break;
          const amount = Math.min(taxable, b.upTo) - prev;
          tax += amount * b.rate;
          prev = b.upTo;
          if (taxable <= b.upTo) break;
        }
        return Math.max(0, tax);
      }

      function fmt(n) {
        if (!isFinite(n)) return "$0";
        return "$" + n.toFixed(0).replace(/\B(?=(\d{3})+(?!\d))/g, ",");
      }

      function fmt2(n) {
        if (!isFinite(n)) return "$0.00";
        return "$" + n.toFixed(2).replace(/\B(?=(\d{3})+(?!\d))/g, ",");
      }

      document.getElementById("calculate-btn").addEventListener("click", () => {
        const errorEl = document.getElementById("calc-error");
        errorEl.classList.add("hidden");
        errorEl.textContent = "";

        const status = document.getElementById("filing-status").value;
        const payFreq =
          parseFloat(document.getElementById("pay-frequency").value) || 0;
        const grossPerPay =
          parseFloat(document.getElementById("gross-pay").value) || 0;
        const pretaxPerPay =
          parseFloat(document.getElementById("pretax-per-pay").value) || 0;
        const otherIncome =
          parseFloat(document.getElementById("other-income").value) || 0;
        const itemized =
          parseFloat(document.getElementById("itemized-deductions").value) || 0;
        const childCredits =
          parseFloat(document.getElementById("child-credits").value) || 0;
        const otherCredits =
          parseFloat(document.getElementById("other-credits").value) || 0;
        const extraPerPay =
          parseFloat(document.getElementById("extra-withholding").value) || 0;

        if (payFreq <= 0 || grossPerPay < 0 || pretaxPerPay < 0) {
          errorEl.textContent =
            "Please enter valid pay frequency and pay amounts.";
          errorEl.classList.remove("hidden");
          return;
        }

        const annualWages = Math.max(0, (grossPerPay - pretaxPerPay) * payFreq);
        const stdDed = STANDARD_DEDUCTION[status] || STANDARD_DEDUCTION.single;
        const deductionsUsed = Math.max(stdDed, itemized);
        const taxableIncome = Math.max(
          0,
          annualWages + otherIncome - deductionsUsed
        );

        const taxBeforeCredits = calcTax(
          TAX_BRACKETS[status] || TAX_BRACKETS.single,
          taxableIncome
        );
        const totalCredits = childCredits + otherCredits;
        const estTax = Math.max(0, taxBeforeCredits - totalCredits);

        const taxPerPay = estTax / payFreq;
        const withholdingPerPay = Math.max(0, taxPerPay + extraPerPay);
        const annualWithholding = withholdingPerPay * payFreq;

        const diff = annualWithholding - estTax;

        // Suggested W-4 mapping
        const step3 = totalCredits;
        const step4a = otherIncome;
        const step4b = Math.max(0, itemized - stdDed);
        const step4c = extraPerPay;

        // Per-pay net (federal only)
        const netPay = Math.max(
          0,
          grossPerPay - pretaxPerPay - withholdingPerPay
        );

        // Update UI
        document.getElementById("res-taxable-income").textContent =
          fmt(taxableIncome);
        document.getElementById("res-tax").textContent = fmt(estTax);
        document.getElementById("res-withholding").textContent =
          fmt(annualWithholding);

        const refundEl = document.getElementById("res-refund");
        refundEl.textContent = fmt(Math.abs(diff));
        refundEl.classList.remove("text-green-600", "text-red-600");
        if (diff > 0) {
          refundEl.textContent += " refund";
          refundEl.classList.add("text-green-600");
        } else if (diff < 0) {
          refundEl.textContent += " owed";
          refundEl.classList.add("text-red-600");
        }

        document.getElementById("res-step3").textContent = fmt(step3);
        document.getElementById("res-step4a").textContent = fmt(step4a);
        document.getElementById("res-step4b").textContent = fmt(step4b);
        document.getElementById("res-step4c").textContent = fmt2(step4c);

        document.getElementById("res-tax-per-pay").textContent =
          fmt2(withholdingPerPay);
        document.getElementById("res-net-pay").textContent = fmt2(netPay);
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
