    <script>
      // Cat age conversion model
      function catYearsToHuman(catYears) {
        if (catYears <= 0) return 0;
        if (catYears <= 1) {
          return catYears * 15;
        } else if (catYears <= 2) {
          return 15 + (catYears - 1) * 9;
        } else {
          return 24 + (catYears - 2) * 4;
        }
      }

      function getLifeStage(catYears) {
        if (catYears < 0.5)
          return {
            stage: "Kitten",
            desc: "Very young kitten, rapid growth and socialisation.",
          };
        if (catYears < 1)
          return {
            stage: "Kitten",
            desc: "Older kitten, playful and still growing fast.",
          };
        if (catYears < 2)
          return {
            stage: "Junior",
            desc: "Young cat reaching full size and maturity.",
          };
        if (catYears < 7)
          return { stage: "Prime adult", desc: "Healthy, active adult cat." };
        if (catYears < 11)
          return {
            stage: "Mature",
            desc: "Middle‑aged cat; monitor weight and subtle health changes.",
          };
        if (catYears < 15)
          return {
            stage: "Senior",
            desc: "Older cat; consider more frequent vet checks and senior diet.",
          };
        return {
          stage: "Geriatric",
          desc: "Very old cat; focus on comfort, monitoring, and regular veterinary care.",
        };
      }

      function updateFromInputs() {
        const yearsInput = document.getElementById("cat-years");
        const monthsInput = document.getElementById("cat-months");
        const slider = document.getElementById("age-slider");

        let years = parseFloat(yearsInput.value) || 0;
        let months = parseFloat(monthsInput.value) || 0;

        if (months > 11) months = 11;
        if (months < 0) months = 0;
        monthsInput.value = months;

        let catYears = years + months / 12;
        if (catYears < 0) catYears = 0;
        if (catYears > 30) catYears = 30;

        // Sync slider (clamp to slider max)
        const sliderVal = Math.max(
          parseFloat(slider.min),
          Math.min(parseFloat(slider.max), catYears)
        );
        slider.value = sliderVal;

        renderResults(catYears);
      }

      function updateFromSlider() {
        const slider = document.getElementById("age-slider");
        const yearsInput = document.getElementById("cat-years");
        const monthsInput = document.getElementById("cat-months");

        let catYears = parseFloat(slider.value) || 0;
        if (catYears < 0) catYears = 0;

        const wholeYears = Math.floor(catYears);
        const months = Math.round((catYears - wholeYears) * 12);

        yearsInput.value = wholeYears;
        monthsInput.value = months;

        renderResults(catYears);
      }

      function renderResults(catYears) {
        const human = catYearsToHuman(catYears);
        const stageInfo = getLifeStage(catYears);

        const catAgeEl = document.getElementById("result-cat-age");
        const catAgeDetailEl = document.getElementById("result-cat-age-detail");
        const humanAgeEl = document.getElementById("result-human-age");
        const stageEl = document.getElementById("result-life-stage");
        const stageDescEl = document.getElementById("result-life-desc");
        const adviceEl = document.getElementById("stage-advice");

        const years = Math.floor(catYears);
        const months = Math.round((catYears - years) * 12);
        const humanRounded = Math.round(human);

        catAgeEl.textContent = catYears.toFixed(1) + " years";
        catAgeDetailEl.textContent = years + " years " + months + " months";
        humanAgeEl.textContent = humanRounded + " years";
        stageEl.textContent = stageInfo.stage;
        stageDescEl.textContent = stageInfo.desc;

        let advice = "";
        switch (stageInfo.stage) {
          case "Kitten":
            advice =
              "Kittens need frequent small meals, safe exploration, socialisation, and their initial vaccine series. Schedule regular vet visits during the first year.";
            break;
          case "Junior":
            advice =
              "Junior cats are energetic and curious. Reinforce good habits, consider spaying/neutering if not already done, and transition to adult food around 12 months as advised by your vet.";
            break;
          case "Prime adult":
            advice =
              "Prime adults benefit from measured meals, daily play, scratching posts, and annual vet check‑ups including dental exams and parasite prevention.";
            break;
          case "Mature":
            advice =
              "Mature cats may start to slow down. Monitor weight, appetite, and litter box habits, and discuss senior screening bloodwork with your veterinarian.";
            break;
          case "Senior":
            advice =
              "Senior cats often need more frequent vet visits, possible diet adjustments, and environmental changes like ramps and low‑sided litter boxes to stay comfortable.";
            break;
          case "Geriatric":
            advice =
              "Geriatric cats are fragile and prone to chronic disease. Work closely with your vet to manage pain, mobility, kidney or thyroid issues, and maintain quality of life.";
            break;
          default:
            advice = stageInfo.desc;
        }
        adviceEl.textContent = advice;
      }

      function parseRange(value) {
        const parts = (value || "").split("-");
        if (parts.length !== 2) return [0, 0];
        const min = parseFloat(parts[0]) || 0;
        const max = parseFloat(parts[1]) || min;
        return [min, max];
      }

      function estimateAgeFromQuestions() {
        const teeth = document.getElementById("q-teeth").value;
        const eyes = document.getElementById("q-eyes").value;
        const activity = document.getElementById("q-activity").value;
        const body = document.getElementById("q-body").value;

        const ranges = [teeth, eyes, activity, body].map(parseRange);
        let minSum = 0;
        let maxSum = 0;
        ranges.forEach((r) => {
          minSum += r[0];
          maxSum += r[1];
        });

        const minAge = minSum / ranges.length;
        const maxAge = maxSum / ranges.length;
        const estAge = (minAge + maxAge) / 2;

        // Update known-age inputs and slider
        const yearsInput = document.getElementById("cat-years");
        const monthsInput = document.getElementById("cat-months");
        const slider = document.getElementById("age-slider");

        const years = Math.floor(estAge);
        const months = Math.round((estAge - years) * 12);

        yearsInput.value = years;
        monthsInput.value = months;
        slider.value = Math.max(
          parseFloat(slider.min),
          Math.min(parseFloat(slider.max), estAge)
        );

        renderResults(estAge);

        const note = document.getElementById("estimate-note");
        note.textContent =
          "Estimated age range: about " +
          minAge.toFixed(1) +
          "–" +
          maxAge.toFixed(1) +
          " years. Shown results use the midpoint.";
      }

      function setupModeToggle() {
        const knownBtn = document.getElementById("mode-known-age");
        const estimateBtn = document.getElementById("mode-estimate-age");
        const knownPanel = document.getElementById("known-age-panel");
        const estimatePanel = document.getElementById("estimate-age-panel");

        knownBtn.addEventListener("click", () => {
          knownPanel.classList.remove("hidden");
          estimatePanel.classList.add("hidden");
          knownBtn.classList.add("bg-blue-600", "text-white");
          knownBtn.classList.remove("bg-gray-100", "text-gray-700");
          estimateBtn.classList.remove("bg-blue-600", "text-white");
          estimateBtn.classList.add("bg-gray-100", "text-gray-700");
        });

        estimateBtn.addEventListener("click", () => {
          knownPanel.classList.remove("hidden");
          estimatePanel.classList.remove("hidden");
          estimateBtn.classList.add("bg-blue-600", "text-white");
          estimateBtn.classList.remove("bg-gray-100", "text-gray-700");
          knownBtn.classList.remove("bg-blue-600", "text-white");
          knownBtn.classList.add("bg-gray-100", "text-gray-700");
        });
      }

      document.addEventListener("DOMContentLoaded", () => {
        const yearsInput = document.getElementById("cat-years");
        const monthsInput = document.getElementById("cat-months");
        const slider = document.getElementById("age-slider");
        const estimateBtn = document.getElementById("estimate-btn");

        yearsInput.addEventListener("input", updateFromInputs);
        monthsInput.addEventListener("input", updateFromInputs);
        slider.addEventListener("input", updateFromSlider);
        estimateBtn.addEventListener("click", estimateAgeFromQuestions);

        setupModeToggle();
        updateFromInputs();
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
