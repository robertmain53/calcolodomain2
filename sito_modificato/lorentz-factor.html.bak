    <!-- Chart.js for gamma plot -->
    <script
      src="https://cdn.jsdelivr.net/npm/chart.js@4.4.0/dist/chart.umd.min.js"
      integrity="sha256-4Hq9X8yC3uR2tZk9K5v3Y4x0FJr7v1W8u4qZ8dC2kqE="
      crossorigin="anonymous"
    ></script>

    <script>
      (function () {
        const c = 299792458; // speed of light in m/s

        const inputModeEl = document.getElementById("input-mode");
        const velocityGroup = document.getElementById("velocity-group");
        const betaGroup = document.getElementById("beta-group");
        const gammaInputGroup = document.getElementById("gamma-input-group");

        const velocityInput = document.getElementById("velocity-input");
        const velocityUnit = document.getElementById("velocity-unit");
        const betaInput = document.getElementById("beta-input");
        const gammaInput = document.getElementById("gamma-input");

        const properTime = document.getElementById("proper-time");
        const properTimeUnit = document.getElementById("proper-time-unit");
        const properLength = document.getElementById("proper-length");
        const properLengthUnit = document.getElementById("proper-length-unit");

        const gammaOutput = document.getElementById("gamma-output");
        const betaOutput = document.getElementById("beta-output");
        const vOutput = document.getElementById("v-output");
        const dilatedTimeOutput = document.getElementById(
          "dilated-time-output"
        );
        const contractedLengthOutput = document.getElementById(
          "contracted-length-output"
        );
        const timeDilationFactor = document.getElementById(
          "time-dilation-factor"
        );
        const lengthContractionFactor = document.getElementById(
          "length-contraction-factor"
        );

        const betaSlider = document.getElementById("beta-slider");
        const betaSliderLabel = document.getElementById("beta-slider-label");
        const gammaSliderLabel = document.getElementById("gamma-slider-label");

        const computeBtn = document.getElementById("compute-btn");
        const errorMsg = document.getElementById("error-msg");

        function showError(msg) {
          errorMsg.textContent = msg;
          errorMsg.classList.remove("hidden");
        }

        function clearError() {
          errorMsg.textContent = "";
          errorMsg.classList.add("hidden");
        }

        function formatNumber(x, digits) {
          if (!isFinite(x)) return "—";
          return x.toFixed(digits);
        }

        function timeToSeconds(value, unit) {
          const v = Number(value);
          if (!isFinite(v) || v < 0) return 0;
          switch (unit) {
            case "s":
              return v;
            case "min":
              return v * 60;
            case "h":
              return v * 3600;
            case "y":
              return v * 365.25 * 24 * 3600;
            default:
              return v;
          }
        }

        function secondsToUnit(seconds, unit) {
          switch (unit) {
            case "s":
              return seconds;
            case "min":
              return seconds / 60;
            case "h":
              return seconds / 3600;
            case "y":
              return seconds / (365.25 * 24 * 3600);
            default:
              return seconds;
          }
        }

        function lengthToMeters(value, unit) {
          const v = Number(value);
          if (!isFinite(v) || v < 0) return 0;
          switch (unit) {
            case "m":
              return v;
            case "km":
              return v * 1000;
            case "ly":
              return v * 9.4607e15; // 1 light-year in meters
            default:
              return v;
          }
        }

        function metersToUnit(meters, unit) {
          switch (unit) {
            case "m":
              return meters;
            case "km":
              return meters / 1000;
            case "ly":
              return meters / 9.4607e15;
            default:
              return meters;
          }
        }

        function computeFromInputs() {
          clearError();
          let beta, gamma;

          const mode = inputModeEl.value;

          try {
            if (mode === "v") {
              const vRaw = Number(velocityInput.value);
              if (!isFinite(vRaw)) {
                showError("Please enter a valid velocity.");
                return;
              }
              let vMS;
              switch (velocityUnit.value) {
                case "c":
                  if (vRaw < 0 || vRaw >= 1) {
                    showError(
                      "For v in units of c, value must satisfy 0 ≤ v < 1."
                    );
                    return;
                  }
                  beta = vRaw;
                  vMS = beta * c;
                  break;
                case "ms":
                  if (vRaw < 0 || vRaw >= c) {
                    showError("Velocity in m/s must be between 0 and c.");
                    return;
                  }
                  vMS = vRaw;
                  beta = vMS / c;
                  break;
                case "kmh":
                  const vMSConv = vRaw / 3.6;
                  if (vRaw < 0 || vMSConv >= c) {
                    showError("Velocity in km/h must be between 0 and c.");
                    return;
                  }
                  vMS = vMSConv;
                  beta = vMS / c;
                  break;
                default:
                  showError("Unknown velocity unit.");
                  return;
              }
              gamma = 1 / Math.sqrt(1 - beta * beta);
            } else if (mode === "beta") {
              const b = Number(betaInput.value);
              if (!isFinite(b) || b < 0 || b >= 1) {
                showError("β must satisfy 0 ≤ β < 1.");
                return;
              }
              beta = b;
              gamma = 1 / Math.sqrt(1 - beta * beta);
            } else if (mode === "gamma") {
              const g = Number(gammaInput.value);
              if (!isFinite(g) || g < 1) {
                showError("γ must be ≥ 1.");
                return;
              }
              gamma = g;
              const betaSq = 1 - 1 / (gamma * gamma);
              if (betaSq < 0) {
                showError("Invalid γ value.");
                return;
              }
              beta = Math.sqrt(betaSq);
            } else {
              showError("Unknown input mode.");
              return;
            }
          } catch (e) {
            showError("Could not compute with the given inputs.");
            return;
          }

          if (
            !isFinite(beta) ||
            beta < 0 ||
            beta >= 1 ||
            !isFinite(gamma) ||
            gamma < 1
          ) {
            showError("Resulting speed must be less than the speed of light.");
            return;
          }

          const vMS = beta * c;
          const vKMH = vMS * 3.6;

          // Update outputs
          gammaOutput.textContent = formatNumber(gamma, 6);
          betaOutput.textContent = formatNumber(beta, 6);
          vOutput.textContent =
            formatNumber(vMS, 3) + " m/s (" + formatNumber(vKMH, 3) + " km/h)";

          // Time dilation
          const tauSeconds = timeToSeconds(
            properTime.value,
            properTimeUnit.value
          );
          const tSeconds = gamma * tauSeconds;
          const tInUnit = secondsToUnit(tSeconds, properTimeUnit.value);
          dilatedTimeOutput.textContent =
            formatNumber(tInUnit, 6) + " " + properTimeUnit.value;

          // Length contraction
          const L0Meters = lengthToMeters(
            properLength.value,
            properLengthUnit.value
          );
          const LMeters = L0Meters / gamma;
          const LInUnit = metersToUnit(LMeters, properLengthUnit.value);
          contractedLengthOutput.textContent =
            formatNumber(LInUnit, 6) + " " + properLengthUnit.value;

          timeDilationFactor.textContent = formatNumber(gamma, 6) + "× slower";
          lengthContractionFactor.textContent =
            formatNumber(gamma, 6) + "× shorter";

          // Sync slider
          betaSlider.value = beta.toFixed(3);
          betaSliderLabel.textContent = formatNumber(beta, 3);
          gammaSliderLabel.textContent = formatNumber(gamma, 3);
        }

        inputModeEl.addEventListener("change", () => {
          const mode = inputModeEl.value;
          velocityGroup.classList.toggle("hidden", mode !== "v");
          betaGroup.classList.toggle("hidden", mode !== "beta");
          gammaInputGroup.classList.toggle("hidden", mode !== "gamma");
          clearError();
        });

        computeBtn.addEventListener("click", computeFromInputs);

        [
          velocityInput,
          velocityUnit,
          betaInput,
          gammaInput,
          properTime,
          properTimeUnit,
          properLength,
          properLengthUnit,
        ].forEach((el) => {
          el.addEventListener("change", () => {
            // Do not auto-compute on every keystroke, only on change
          });
        });

        betaSlider.addEventListener("input", () => {
          const beta = Number(betaSlider.value);
          if (!isFinite(beta) || beta < 0 || beta >= 1) return;
          const gamma = 1 / Math.sqrt(1 - beta * beta);
          betaSliderLabel.textContent = formatNumber(beta, 3);
          gammaSliderLabel.textContent = formatNumber(gamma, 3);

          // Also update main outputs for quick exploration
          clearError();
          const vMS = beta * c;
          const vKMH = vMS * 3.6;

          gammaOutput.textContent = formatNumber(gamma, 6);
          betaOutput.textContent = formatNumber(beta, 6);
          vOutput.textContent =
            formatNumber(vMS, 3) + " m/s (" + formatNumber(vKMH, 3) + " km/h)";

          const tauSeconds = timeToSeconds(
            properTime.value,
            properTimeUnit.value
          );
          const tSeconds = gamma * tauSeconds;
          const tInUnit = secondsToUnit(tSeconds, properTimeUnit.value);
          dilatedTimeOutput.textContent =
            formatNumber(tInUnit, 6) + " " + properTimeUnit.value;

          const L0Meters = lengthToMeters(
            properLength.value,
            properLengthUnit.value
          );
          const LMeters = L0Meters / gamma;
          const LInUnit = metersToUnit(LMeters, properLengthUnit.value);
          contractedLengthOutput.textContent =
            formatNumber(LInUnit, 6) + " " + properLengthUnit.value;

          timeDilationFactor.textContent = formatNumber(gamma, 6) + "× slower";
          lengthContractionFactor.textContent =
            formatNumber(gamma, 6) + "× shorter";
        });

        // Initialize chart
        let gammaChart;
        function initChart() {
          const ctx = document.getElementById("gamma-chart");
          if (!ctx) return;

          const betaValues = [];
          const gammaValues = [];
          const maxBeta = 0.999;
          const steps = 300;
          for (let i = 0; i <= steps; i++) {
            const beta = (maxBeta * i) / steps;
            const gamma = 1 / Math.sqrt(1 - beta * beta);
            betaValues.push(beta);
            gammaValues.push(gamma);
          }

          gammaChart = new Chart(ctx, {
            type: "line",
            data: {
              labels: betaValues,
              datasets: [
                {
                  label: "γ(β)",
                  data: gammaValues,
                  borderColor: "#2563eb",
                  borderWidth: 2,
                  pointRadius: 0,
                  tension: 0.1,
                },
              ],
            },
            options: {
              responsive: true,
              maintainAspectRatio: false,
              scales: {
                x: {
                  title: { display: true, text: "β = v/c" },
                  ticks: {
                    callback: function (value, index) {
                      const beta = this.getLabelForValue(value);
                      return Number(beta).toFixed(1);
                    },
                  },
                },
                y: {
                  title: { display: true, text: "γ" },
                  min: 1,
                  max: 10,
                  ticks: {
                    callback: function (value) {
                      return value.toFixed(0);
                    },
                  },
                },
              },
              plugins: {
                legend: { display: false },
                tooltip: {
                  callbacks: {
                    label: function (ctx) {
                      const beta = ctx.label;
                      const gamma = ctx.parsed.y;
                      return (
                        "β = " +
                        Number(beta).toFixed(3) +
                        ", γ = " +
                        gamma.toFixed(3)
                      );
                    },
                  },
                },
              },
            },
          });
        }

        document.addEventListener("DOMContentLoaded", () => {
          initChart();
          // Set default velocity to 0 in units of c
          velocityInput.value = "0";
          computeFromInputs();
        });
      })();
    </script>
  
