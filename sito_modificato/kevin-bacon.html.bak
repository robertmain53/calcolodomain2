    <script>
      // Toggle logic
      document.getElementById("mobile-menu-toggle")?.addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          if (menu) menu.classList.toggle("hidden");
      });

      // Mini movie graph logic (Unchanged logic, kept clean)
      const MOVIES = [
        { title: "Apollo 13", year: 1995, cast: ["Kevin Bacon", "Tom Hanks", "Bill Paxton", "Gary Sinise", "Ed Harris"] },
        { title: "Footloose", year: 1984, cast: ["Kevin Bacon", "John Lithgow", "Dianne Wiest"] },
        { title: "A Few Good Men", year: 1992, cast: ["Tom Cruise", "Jack Nicholson", "Demi Moore", "Kevin Bacon", "Kiefer Sutherland"] },
        { title: "Mystic River", year: 2003, cast: ["Kevin Bacon", "Sean Penn", "Tim Robbins", "Laurence Fishburne"] },
        { title: "Sleepless in Seattle", year: 1993, cast: ["Tom Hanks", "Meg Ryan", "Bill Pullman"] },
        { title: "You've Got Mail", year: 1998, cast: ["Tom Hanks", "Meg Ryan", "Parker Posey"] },
        { title: "The Devil Wears Prada", year: 2006, cast: ["Meryl Streep", "Anne Hathaway", "Emily Blunt", "Stanley Tucci"] },
        { title: "Julie & Julia", year: 2009, cast: ["Meryl Streep", "Amy Adams", "Stanley Tucci"] },
        { title: "The Pelican Brief", year: 1993, cast: ["Julia Roberts", "Denzel Washington", "Sam Shepard"] },
        { title: "Philadelphia", year: 1993, cast: ["Tom Hanks", "Denzel Washington", "Jason Robards"] },
        { title: "Catch Me If You Can", year: 2002, cast: ["Leonardo DiCaprio", "Tom Hanks", "Christopher Walken", "Amy Adams"] },
        { title: "Crazy, Stupid, Love.", year: 2011, cast: ["Steve Carell", "Ryan Gosling", "Julianne Moore", "Emma Stone", "Kevin Bacon"] },
      ];

      const graph = {};
      function buildGraph() {
        MOVIES.forEach((movie) => {
          const cast = movie.cast;
          for (let i = 0; i < cast.length; i++) {
            for (let j = i + 1; j < cast.length; j++) {
              const a = cast[i];
              const b = cast[j];
              if (!graph[a]) graph[a] = [];
              if (!graph[b]) graph[b] = [];
              graph[a].push({ actor: b, movie: movie.title, year: movie.year });
              graph[b].push({ actor: a, movie: movie.title, year: movie.year });
            }
          }
        });
      }
      buildGraph();

      function normalizeName(name) { return name.trim(); }

      function bfsShortestPath(start, target) {
        if (start === target) return { distance: 0, path: [start], edges: [] };
        const queue = [start];
        const visited = new Set([start]);
        const parent = {};
        const edgeInfo = {};

        while (queue.length > 0) {
          const current = queue.shift();
          const neighbors = graph[current] || [];
          for (const { actor: nextActor, movie, year } of neighbors) {
            if (!visited.has(nextActor)) {
              visited.add(nextActor);
              parent[nextActor] = current;
              edgeInfo[`${current}→${nextActor}`] = { movie, year };
              if (nextActor === target) {
                const path = [target];
                const edges = [];
                let node = target;
                while (node !== start) {
                  const p = parent[node];
                  const info = edgeInfo[`${p}→${node}`];
                  edges.unshift({ from: p, to: node, movie: info.movie, year: info.year });
                  path.unshift(p);
                  node = p;
                }
                return { distance: path.length - 1, path, edges };
              }
              queue.push(nextActor);
            }
          }
        }
        return null;
      }

      function showStatus(msg, type = "info") {
        const el = document.getElementById("status-message");
        el.textContent = msg;
        el.className = "text-sm mb-3 " + (type === "error" ? "text-red-600" : "text-gray-600");
      }

      function renderResult(result, from, to) {
        const card = document.getElementById("result-card");
        const numEl = document.getElementById("bacon-number");
        const fromEl = document.getElementById("from-label");
        const toEl = document.getElementById("to-label");
        const pathList = document.getElementById("path-steps");

        fromEl.textContent = from;
        toEl.textContent = to;

        if (!result) {
          card.classList.remove("hidden");
          numEl.textContent = "∞";
          pathList.innerHTML = "<li class='text-sm text-red-600'>No connection found in this demo dataset.</li>";
          return;
        }

        numEl.textContent = result.distance.toString();
        pathList.innerHTML = "";

        result.edges.forEach((edge, index) => {
          const li = document.createElement("li");
          li.className = "flex-shrink-0 bg-white border border-gray-200 rounded-md px-3 py-2 shadow-sm";
          li.innerHTML = `
            <div class="text-xs text-gray-500 mb-1">Step ${index + 1}</div>
            <div class="text-sm font-medium">${edge.from} <span class="text-gray-400">→</span> ${edge.to}</div>
            <div class="text-xs text-gray-600 mt-1">
              <span class="badge mr-1">Movie</span>
              <span>${edge.movie}</span>
              ${edge.year ? `<span class="text-gray-400 ml-1">(${edge.year})</span>` : ""}
            </div>`;
          pathList.appendChild(li);
        });
        card.classList.remove("hidden");
      }

      document.addEventListener("DOMContentLoaded", () => {
        const actorAInput = document.getElementById("actor-a");
        const actorBInput = document.getElementById("actor-b");
        
        // Defaults
        actorAInput.value = "Tom Hanks";
        actorBInput.value = "Kevin Bacon";

        document.getElementById("compute-btn").addEventListener("click", () => {
          let from = normalizeName(actorAInput.value);
          let to = normalizeName(actorBInput.value || "Kevin Bacon");

          if (!from) { showStatus("Please enter at least the first actor.", "error"); return; }
          
          // Simple validation against graph keys for better UX in demo
          if (!graph[from]) {
             showStatus(`"${from}" is not in this demo dataset.`, "error");
             return;
          }

          showStatus("Searching...");
          const result = bfsShortestPath(from, to);
          if (!result) {
            showStatus("No connection found in this demo dataset.", "error");
          } else {
            showStatus(`Found a connection in ${result.distance} steps.`);
          }
          renderResult(result, from, to);
        });

        document.getElementById("reset-btn").addEventListener("click", () => {
          actorAInput.value = "";
          actorBInput.value = "";
          document.getElementById("status-message").textContent = "";
          document.getElementById("result-card").classList.add("hidden");
        });
      });
      
      // MathJax config
      window.MathJax = {
        tex: { inlineMath: [["\\(", "\\)"], ["$", "$"]], displayMath: [["$", "$"], ["\\[", "\\]"]] },
        svg: { fontCache: "global" }
      };
    </script>
    <script>
      (function () {
        var loadMathJax = function () {
          if (window.__mathjaxLoading) return;
          window.__mathjaxLoading = true;
          var script = document.createElement("script");
          script.id = "MathJax-script";
