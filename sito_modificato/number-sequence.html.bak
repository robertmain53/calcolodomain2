    <script>
      // Sync number input and range
      const extraTermsInput = document.getElementById("extra-terms");
      const extraTermsRange = document.getElementById("extra-terms-range");
      extraTermsInput.addEventListener("input", () => {
        let v = parseInt(extraTermsInput.value, 10);
        if (isNaN(v)) v = 1;
        v = Math.max(1, Math.min(50, v));
        extraTermsInput.value = v;
        extraTermsRange.value = Math.min(v, parseInt(extraTermsRange.max, 10));
      });
      extraTermsRange.addEventListener("input", () => {
        extraTermsInput.value = extraTermsRange.value;
      });

      function parseSequence(str) {
        if (!str) return [];
        const parts = str.replace(/,/g, " ").split(/\s+/).filter(Boolean);
        const nums = [];
        for (const p of parts) {
          const v = Number(p);
          if (!isNaN(v) && isFinite(v)) nums.push(v);
        }
        return nums;
      }

      function computeDifferences(arr) {
        const diffs = [];
        for (let i = 0; i < arr.length - 1; i++) {
          diffs.push(arr[i + 1] - arr[i]);
        }
        return diffs;
      }

      function isConstant(arr, tol = 1e-9) {
        if (arr.length === 0) return false;
        const first = arr[0];
        return arr.every((x) => Math.abs(x - first) <= tol);
      }

      function isGeometric(arr, tol = 1e-9) {
        if (arr.length < 2) return { ok: false };
        if (arr.some((x) => x === 0)) return { ok: false };
        const ratios = [];
        for (let i = 0; i < arr.length - 1; i++) {
          ratios.push(arr[i + 1] / arr[i]);
        }
        if (!isConstant(ratios, tol)) return { ok: false };
        return { ok: true, r: ratios[0] };
      }

      function detectFibonacciLike(arr) {
        if (arr.length < 4) return { ok: false };
        // Try small integer coefficients c1, c2 in range [-5,5]
        for (let c1 = -5; c1 <= 5; c1++) {
          for (let c2 = -5; c2 <= 5; c2++) {
            if (c1 === 0 && c2 === 0) continue;
            let ok = true;
            for (let n = 2; n < arr.length; n++) {
              const pred = c1 * arr[n - 1] + c2 * arr[n - 2];
              if (Math.abs(pred - arr[n]) > 1e-9) {
                ok = false;
                break;
              }
            }
            if (ok) return { ok: true, c1, c2 };
          }
        }
        return { ok: false };
      }

      function buildDifferenceTable(arr, maxOrder = 3) {
        const table = [arr.slice()];
        for (let k = 1; k <= maxOrder; k++) {
          const prev = table[k - 1];
          if (prev.length < 2) break;
          table.push(computeDifferences(prev));
        }
        return table;
      }

      function detectPolynomial(arr) {
        const table = buildDifferenceTable(arr, 3);
        for (let order = 1; order < table.length; order++) {
          if (isConstant(table[order])) {
            return { ok: true, degree: order, table };
          }
        }
        return { ok: false, table };
      }

      function newtonForwardCoefficients(arr) {
        const n = arr.length;
        const table = [arr.slice()];
        for (let k = 1; k < n; k++) {
          const prev = table[k - 1];
          const row = [];
          for (let i = 0; i < prev.length - 1; i++) {
            row.push(prev[i + 1] - prev[i]);
          }
          table.push(row);
        }
        const coeffs = table.map((row) => row[0]);
        return coeffs;
      }

      function factorial(n) {
        let r = 1;
        for (let i = 2; i <= n; i++) r *= i;
        return r;
      }

      function formatPolynomialFormula(coeffs) {
        // Newton forward form: a1 + Δa1 * C(n-1,1) + Δ²a1 * C(n-1,2) + ...
        const terms = [];
        for (let k = 0; k < coeffs.length; k++) {
          if (Math.abs(coeffs[k]) < 1e-12) continue;
          let part = "";
          if (k === 0) {
            part = coeffs[k].toString();
          } else {
            const c = coeffs[k] / factorial(k);
            const cStr = Math.abs(c - 1) < 1e-12 ? "" : c.toString();
            part = `${cStr}·(n−1)`;
            for (let j = 1; j < k; j++) {
              part += `·(n−1−${j})`;
            }
          }
          terms.push(part);
        }
        if (!terms.length) return "a_n = 0";
        return "a_n = " + terms.join(" + ");
      }

      function generateSequenceByRule(type, baseArr, extraCount, params) {
        const result = baseArr.slice();
        if (type === "arithmetic") {
          const d = params.d;
          let last = result[result.length - 1];
          for (let i = 0; i < extraCount; i++) {
            last = last + d;
            result.push(last);
          }
        } else if (type === "geometric") {
          const r = params.r;
          let last = result[result.length - 1];
          for (let i = 0; i < extraCount; i++) {
            last = last * r;
            result.push(last);
          }
        } else if (type === "fibonacci") {
          const { c1, c2 } = params;
          let a = result[result.length - 2];
          let b = result[result.length - 1];
          for (let i = 0; i < extraCount; i++) {
            const next = c1 * b + c2 * a;
            result.push(next);
            a = b;
            b = next;
          }
        } else if (type === "polynomial") {
          const coeffs = params.coeffs;
          const startN = 1;
          for (let k = 0; k < extraCount; k++) {
            const n = startN + result.length;
            let value = 0;
            for (let i = 0; i < coeffs.length; i++) {
              // Newton forward evaluation
              let term = coeffs[i];
              for (let j = 0; j < i; j++) {
                term *= n - 1 - j;
              }
              term / factorial(i);
              value += term;
            }
            result.push(value);
          }
        } else if (type === "linear-last-diff") {
          const diffs = computeDifferences(baseArr);
          const d = diffs[diffs.length - 1];
          let last = result[result.length - 1];
          for (let i = 0; i < extraCount; i++) {
            last = last + d;
            result.push(last);
          }
        }
        return result;
      }

      function renderDifferenceTable(table) {
        const headerRow = document.getElementById("diff-header-row");
        const bodyRow = document.getElementById("diff-body-row");
        headerRow.innerHTML = "";
        bodyRow.innerHTML = "";
        table.forEach((row, idx) => {
          const th = document.createElement("th");
          th.className = "px-2 py-1 border-b text-left font-semibold";
          th.textContent = idx === 0 ? "Terms" : `${idx}ᵗʰ diff`;
          headerRow.appendChild(th);

          const td = document.createElement("td");
          td.className = "px-2 py-1 border-b align-top";
          td.textContent = row.join(", ");
          bodyRow.appendChild(td);
        });
      }

      function renderSequenceTable(fullSeq, baseLength) {
        const tbody = document.getElementById("sequence-table-body");
        tbody.innerHTML = "";
        for (let i = 0; i < fullSeq.length; i++) {
          const tr = document.createElement("tr");
          const nCell = document.createElement("td");
          nCell.className = "px-2 py-1 border-b";
          nCell.textContent = i + 1;

          const vCell = document.createElement("td");
          vCell.className = "px-2 py-1 border-b font-mono";
          const val = fullSeq[i];
          vCell.textContent =
            Math.abs(val) > 1e9 || (Math.abs(val) < 1e-6 && val !== 0)
              ? val.toExponential(6)
              : Number.isInteger(val)
              ? val
              : val.toFixed(6).replace(/\.?0+$/, "");

          const tCell = document.createElement("td");
          tCell.className = "px-2 py-1 border-b";
          tCell.textContent = i < baseLength ? "Given" : "Generated";

          tr.appendChild(nCell);
          tr.appendChild(vCell);
          tr.appendChild(tCell);
          tbody.appendChild(tr);
        }
      }

      function setStatus(text, colorClass) {
        const badge = document.getElementById("status-badge");
        badge.textContent = text;
        badge.className = "badge " + colorClass;
        badge.classList.remove("hidden");
      }

      document.getElementById("analyze-btn").addEventListener("click", () => {
        const inputStr = document.getElementById("sequence-input").value.trim();
        const patternMode = document.getElementById("pattern-select").value;
        const extraCount = Math.max(
          1,
          Math.min(
            50,
            parseInt(document.getElementById("extra-terms").value, 10) || 1
          )
        );
        const errorEl = document.getElementById("error-message");
        const resultsEl = document.getElementById("results-container");
        const formulaContainer = document.getElementById("formula-container");
        const differencesContainer = document.getElementById(
          "differences-container"
        );
        const patternSummary = document.getElementById("pattern-summary");
        const formulaText = document.getElementById("formula-text");
        const statusBadge = document.getElementById("status-badge");

        errorEl.classList.add("hidden");
        errorEl.textContent = "";
        resultsEl.classList.add("hidden");
        formulaContainer.classList.add("hidden");
        differencesContainer.classList.add("hidden");
        statusBadge.classList.add("hidden");

        const arr = parseSequence(inputStr);
        if (arr.length < 3) {
          errorEl.textContent =
            "Please enter at least 3 valid numbers to analyze a sequence.";
          errorEl.classList.remove("hidden");
          return;
        }

        let chosenType = null;
        let params = {};
        let explanation = "";
        let ambiguity = false;

        const firstDiff = computeDifferences(arr);
        const isArith = isConstant(firstDiff);
        const geomInfo = isGeometric(arr);
        const fibInfo = detectFibonacciLike(arr);
        const polyInfo = detectPolynomial(arr);

        if (
          patternMode === "arithmetic" ||
          (patternMode === "auto" && isArith)
        ) {
          chosenType = "arithmetic";
          params.d = firstDiff[0];
          explanation = `Arithmetic sequence with common difference d = ${params.d}.`;
          if (geomInfo.ok && Math.abs(params.d) < 1e-9) {
            ambiguity = true;
          }
          formulaText.textContent = `a₁ = ${arr[0]}\nCommon difference d = ${params.d}\n\nnth term:\n  aₙ = a₁ + (n − 1)·d\n     = ${arr[0]} + (n − 1)·(${params.d})`;
          formulaContainer.classList.remove("hidden");
        } else if (
          patternMode === "geometric" ||
          (patternMode === "auto" && geomInfo.ok)
        ) {
          chosenType = "geometric";
          params.r = geomInfo.r;
          explanation = `Geometric sequence with common ratio r = ${params.r}.`;
          formulaText.textContent = `a₁ = ${arr[0]}\nCommon ratio r = ${params.r}\n\nnth term:\n  aₙ = a₁ · r^(n − 1)\n     = ${arr[0]} · (${params.r})^(n − 1)`;
          formulaContainer.classList.remove("hidden");
        } else if (
          patternMode === "fibonacci" ||
          (patternMode === "auto" && fibInfo.ok)
        ) {
          chosenType = "fibonacci";
          params.c1 = fibInfo.c1;
          params.c2 = fibInfo.c2;
          explanation = `Fibonacci-like sequence with recurrence aₙ = ${params.c1}·aₙ₋₁ + ${params.c2}·aₙ₋₂.`;
          formulaText.textContent = `Recurrence relation:\n  a₁ = ${arr[0]}, a₂ = ${arr[1]}\n  aₙ = ${params.c1}·aₙ₋₁ + ${params.c2}·aₙ₋₂  (for n ≥ 3)`;
          formulaContainer.classList.remove("hidden");
        } else if (
          patternMode === "polynomial" ||
          (patternMode === "auto" && polyInfo.ok)
        ) {
          chosenType = "polynomial";
          const coeffs = newtonForwardCoefficients(arr);
          params.coeffs = coeffs;
          explanation = `Polynomial sequence of degree ${polyInfo.degree} (constant ${polyInfo.degree}ᵗʰ differences).`;
          const formula = formatPolynomialFormula(coeffs);
          formulaText.textContent = formula;
          formulaContainer.classList.remove("hidden");
          differencesContainer.classList.remove("hidden");
          renderDifferenceTable(polyInfo.table);
        } else {
          chosenType = "linear-last-diff";
          const lastDiff = firstDiff[firstDiff.length - 1];
          explanation = `No clean arithmetic, geometric, Fibonacci-like or low-degree polynomial pattern detected. Extending using the last observed difference (${lastDiff}) as a best guess.`;
        }

        const fullSeq = generateSequenceByRule(
          chosenType,
          arr,
          extraCount,
          params
        );
        renderSequenceTable(fullSeq, arr.length);
        resultsEl.classList.remove("hidden");
        patternSummary.textContent = explanation;

        if (chosenType === "polynomial" && !polyInfo.ok) {
          differencesContainer.classList.add("hidden");
        } else if (chosenType !== "polynomial" && polyInfo.table) {
          differencesContainer.classList.remove("hidden");
          renderDifferenceTable(polyInfo.table);
        }

        if (ambiguity) {
          setStatus(
            "Pattern may not be unique",
            "bg-yellow-100 text-yellow-800"
          );
        } else if (chosenType === "linear-last-diff") {
          setStatus("Heuristic guess only", "bg-yellow-100 text-yellow-800");
        } else {
          setStatus("Pattern detected", "bg-green-100 text-green-800");
        }
      });

      document.getElementById("clear-btn").addEventListener("click", () => {
        document.getElementById("sequence-input").value = "";
        document.getElementById("results-container").classList.add("hidden");
        document.getElementById("error-message").classList.add("hidden");
        document.getElementById("status-badge").classList.add("hidden");
      });

      // Simple FAQ toggle (in addition to global script_faq if present)
      document
        .querySelectorAll("#faq-container .faq-question")
        .forEach((btn) => {
          btn.addEventListener("click", () => {
            const answer = btn.nextElementSibling;
            const sign = btn.querySelector("span:last-child");
            const isHidden = answer.classList.contains("hidden");
            document
              .querySelectorAll("#faq-container .faq-question + div")
              .forEach((div) => div.classList.add("hidden"));
            document
              .querySelectorAll("#faq-container .faq-question span:last-child")
              .forEach((s) => (s.textContent = "+"));
            if (isHidden) {
              answer.classList.remove("hidden");
              sign.textContent = "−";
            }
          });
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
