    <script>
      (function () {
        const currencySelect = document.getElementById("currency");
        const currencySymbolSpan = document.getElementById("currency-symbol");
        const netBonusInput = document.getElementById("net-bonus");
        const payFrequencySelect = document.getElementById("pay-frequency");
        const resetBtn = document.getElementById("reset-btn");
        const calculateBtn = document.getElementById("calculate-btn");
        const errorMessage = document.getElementById("error-message");

        const flatTaxWrapper = document.getElementById("flat-tax-wrapper");
        const detailedTaxWrapper = document.getElementById(
          "detailed-tax-wrapper"
        );
        const flatTaxRateInput = document.getElementById("flat-tax-rate");
        const federalRateInput = document.getElementById("federal-rate");
        const stateRateInput = document.getElementById("state-rate");
        const localRateInput = document.getElementById("local-rate");
        const ssRateInput = document.getElementById("ss-rate");
        const medicareRateInput = document.getElementById("medicare-rate");
        const otherTaxRateInput = document.getElementById("other-tax-rate");

        const pretaxToggle = document.getElementById("pretax-toggle");
        const pretaxWrapper = document.getElementById("pretax-wrapper");
        const pretaxRateInput = document.getElementById("pretax-rate");
        const employerExtraRateInput = document.getElementById(
          "employer-extra-rate"
        );

        const resultGrossBonus = document.getElementById("result-gross-bonus");
        const resultTotalTax = document.getElementById("result-total-tax");
        const resultEffectiveRate = document.getElementById(
          "result-effective-rate"
        );
        const resultNetBonus = document.getElementById("result-net-bonus");
        const resultEmployerCost = document.getElementById(
          "result-employer-cost"
        );

        const taxFederalAmount = document.getElementById("tax-federal-amount");
        const taxStateAmount = document.getElementById("tax-state-amount");
        const taxLocalAmount = document.getElementById("tax-local-amount");
        const taxSSAmount = document.getElementById("tax-ss-amount");
        const taxMedicareAmount = document.getElementById(
          "tax-medicare-amount"
        );
        const taxOtherAmount = document.getElementById("tax-other-amount");
        const pretaxAmount = document.getElementById("pretax-amount");

        function getSelectedTaxMode() {
          const radios = document.querySelectorAll('input[name="tax-mode"]');
          for (const r of radios) {
            if (r.checked) return r.value;
          }
          return "flat";
        }

        function formatCurrency(value) {
          const currency = currencySelect.value;
          const symbolMap = {
            USD: "$",
            EUR: "€",
            GBP: "£",
            CAD: "$",
            AUD: "$",
            INR: "₹",
          };
          const symbol = symbolMap[currency] || "";
          if (isNaN(value)) return "—";
          return (
            symbol +
            value.toLocaleString(undefined, {
              minimumFractionDigits: 2,
              maximumFractionDigits: 2,
            })
          );
        }

        function formatPercent(value) {
          if (isNaN(value)) return "—";
          return value.toFixed(2) + "%";
        }

        function clearResults() {
          resultGrossBonus.textContent = "—";
          resultTotalTax.textContent = "—";
          resultEffectiveRate.textContent = "—";
          resultNetBonus.textContent = "—";
          resultEmployerCost.textContent = "—";
          taxFederalAmount.textContent = "—";
          taxStateAmount.textContent = "—";
          taxLocalAmount.textContent = "—";
          taxSSAmount.textContent = "—";
          taxMedicareAmount.textContent = "—";
          taxOtherAmount.textContent = "—";
          pretaxAmount.textContent = "—";
        }

        function calculate() {
          errorMessage.classList.add("hidden");
          errorMessage.textContent = "";
          clearResults();

          const net = parseFloat(netBonusInput.value);
          if (isNaN(net) || net <= 0) {
            errorMessage.textContent =
              "Please enter a positive desired net bonus amount.";
            errorMessage.classList.remove("hidden");
            return;
          }

          const taxMode = getSelectedTaxMode();
          let combinedTaxRate = 0;
          let federalRate = 0,
            stateRate = 0,
            localRate = 0,
            ssRate = 0,
            medicareRate = 0,
            otherRate = 0;

          if (taxMode === "flat") {
            const flatRate = parseFloat(flatTaxRateInput.value);
            if (isNaN(flatRate) || flatRate < 0 || flatRate >= 100) {
              errorMessage.textContent =
                "Enter a flat tax rate between 0% and 99.99%.";
              errorMessage.classList.remove("hidden");
              return;
            }
            combinedTaxRate = flatRate / 100;
          } else {
            federalRate = Math.max(0, parseFloat(federalRateInput.value) || 0);
            stateRate = Math.max(0, parseFloat(stateRateInput.value) || 0);
            localRate = Math.max(0, parseFloat(localRateInput.value) || 0);
            ssRate = Math.max(0, parseFloat(ssRateInput.value) || 0);
            medicareRate = Math.max(
              0,
              parseFloat(medicareRateInput.value) || 0
            );
            otherRate = Math.max(0, parseFloat(otherTaxRateInput.value) || 0);

            const totalPercent =
              federalRate +
              stateRate +
              localRate +
              ssRate +
              medicareRate +
              otherRate;
            if (totalPercent <= 0 || totalPercent >= 100) {
              errorMessage.textContent =
                "Combined detailed tax rate must be between 0% and 99.99%.";
              errorMessage.classList.remove("hidden");
              return;
            }
            combinedTaxRate = totalPercent / 100;
          }

          const pretaxOn = pretaxToggle.checked;
          let pretaxRate = 0;
          let employerExtraRate = 0;
          if (pretaxOn) {
            pretaxRate =
              Math.max(0, parseFloat(pretaxRateInput.value) || 0) / 100;
            employerExtraRate =
              Math.max(0, parseFloat(employerExtraRateInput.value) || 0) / 100;
            if (pretaxRate >= 1) {
              errorMessage.textContent =
                "Pre-tax deduction rate must be less than 100%.";
              errorMessage.classList.remove("hidden");
              return;
            }
          }

          let gross;
          if (!pretaxOn) {
            if (combinedTaxRate >= 1) {
              errorMessage.textContent =
                "Combined tax rate must be less than 100%.";
              errorMessage.classList.remove("hidden");
              return;
            }
            gross = net / (1 - combinedTaxRate);
          } else {
            const denom = 1 - pretaxRate - (1 - pretaxRate) * combinedTaxRate;
            if (denom <= 0) {
              errorMessage.textContent =
                "The combination of tax and pre-tax rates leaves no net pay. Reduce the rates.";
              errorMessage.classList.remove("hidden");
              return;
            }
            gross = net / denom;
          }

          if (!isFinite(gross) || gross <= 0) {
            errorMessage.textContent =
              "Unable to compute a valid gross bonus with the given inputs.";
            errorMessage.classList.remove("hidden");
            return;
          }

          const pretaxDeduction = pretaxOn ? gross * pretaxRate : 0;
          const taxable = gross - pretaxDeduction;
          const totalTax = taxable * combinedTaxRate;
          const netCheck = gross - pretaxDeduction - totalTax;
          const effectiveRate = (totalTax / gross) * 100;
          const employerExtra = pretaxOn ? gross * employerExtraRate : 0;
          const employerCost = gross + employerExtra;

          resultGrossBonus.textContent = formatCurrency(gross);
          resultTotalTax.textContent = formatCurrency(totalTax);
          resultEffectiveRate.textContent = formatPercent(effectiveRate);
          resultNetBonus.textContent = formatCurrency(netCheck);
          resultEmployerCost.textContent = formatCurrency(employerCost);

          if (taxMode === "flat") {
            taxFederalAmount.textContent = "—";
            taxStateAmount.textContent = "—";
            taxLocalAmount.textContent = "—";
            taxSSAmount.textContent = "—";
            taxMedicareAmount.textContent = "—";
            taxOtherAmount.textContent = formatCurrency(totalTax);
          } else {
            const totalPercent =
              federalRate +
                stateRate +
                localRate +
                ssRate +
                medicareRate +
                otherRate || 1;
            const factor = taxable / 100;
            taxFederalAmount.textContent = formatCurrency(federalRate * factor);
            taxStateAmount.textContent = formatCurrency(stateRate * factor);
            taxLocalAmount.textContent = formatCurrency(localRate * factor);
            taxSSAmount.textContent = formatCurrency(ssRate * factor);
            taxMedicareAmount.textContent = formatCurrency(
              medicareRate * factor
            );
            taxOtherAmount.textContent = formatCurrency(otherRate * factor);
          }

          pretaxAmount.textContent = pretaxOn
            ? formatCurrency(pretaxDeduction)
            : formatCurrency(0);
        }

        currencySelect.addEventListener("change", () => {
          const map = {
            USD: "$",
            EUR: "€",
            GBP: "£",
            CAD: "$",
            AUD: "$",
            INR: "₹",
          };
          currencySymbolSpan.textContent = map[currencySelect.value] || "";
          if (resultGrossBonus.textContent !== "—") {
            calculate();
          }
        });

        document.querySelectorAll('input[name="tax-mode"]').forEach((radio) => {
          radio.addEventListener("change", () => {
            const mode = getSelectedTaxMode();
            if (mode === "flat") {
              flatTaxWrapper.classList.remove("hidden");
              detailedTaxWrapper.classList.add("hidden");
            } else {
              flatTaxWrapper.classList.add("hidden");
              detailedTaxWrapper.classList.remove("hidden");
            }
            clearResults();
          });
        });

        pretaxToggle.addEventListener("change", () => {
          if (pretaxToggle.checked) {
            pretaxWrapper.classList.remove("hidden");
          } else {
            pretaxWrapper.classList.add("hidden");
          }
          clearResults();
        });

        resetBtn.addEventListener("click", () => {
          currencySelect.value = "USD";
          currencySymbolSpan.textContent = "$";
          payFrequencySelect.value = "one-time";
          netBonusInput.value = 1000;
          document.querySelector(
            'input[name="tax-mode"][value="flat"]'
          ).checked = true;
          flatTaxWrapper.classList.remove("hidden");
          detailedTaxWrapper.classList.add("hidden");
          flatTaxRateInput.value = 30;
          federalRateInput.value = 22;
          stateRateInput.value = 5;
          localRateInput.value = 0;
          ssRateInput.value = 6.2;
          medicareRateInput.value = 1.45;
          otherTaxRateInput.value = 0;
          pretaxToggle.checked = false;
          pretaxWrapper.classList.add("hidden");
          pretaxRateInput.value = 5;
          employerExtraRateInput.value = 0;
          errorMessage.classList.add("hidden");
          errorMessage.textContent = "";
          clearResults();
        });

        calculateBtn.addEventListener("click", calculate);
        netBonusInput.addEventListener("keydown", (e) => {
          if (e.key === "Enter") calculate();
        });

        resetBtn.click();
      })();
    </script>

