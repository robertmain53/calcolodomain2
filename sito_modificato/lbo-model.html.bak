    <script>
      // Utility: format numbers
      function fmtMoney(val) {
        if (isNaN(val)) return "–";
        const abs = Math.abs(val);
        const sign = val < 0 ? "-" : "";
        return sign + "$" + abs.toFixed(1);
      }
      function fmtPct(val) {
        if (isNaN(val)) return "–";
        return val.toFixed(1) + "%";
      }
      function fmtX(val) {
        if (isNaN(val)) return "–";
        return val.toFixed(2) + "x";
      }

      // IRR solver for a single outflow at t=0 and single inflow at t=N
      function solveSimpleIrr(equityOut, equityIn, years) {
        if (equityOut <= 0 || equityIn <= 0 || years <= 0) return NaN;
        // Closed form: equityIn / equityOut = (1 + r)^years
        const ratio = equityIn / equityOut;
        return Math.pow(ratio, 1 / years) - 1;
      }

      function runLboModel() {
        const purchasePrice =
          parseFloat(document.getElementById("purchasePrice").value) || 0;
        const entryEbitda =
          parseFloat(document.getElementById("entryEbitda").value) || 0;
        const debtPct =
          (parseFloat(document.getElementById("debtPct").value) || 0) / 100;
        const interestRate =
          (parseFloat(document.getElementById("interestRate").value) || 0) /
          100;
        const amortizationPct =
          (parseFloat(document.getElementById("amortizationPct").value) || 0) /
          100;

        const holdingPeriod = Math.max(
          1,
          Math.min(
            10,
            parseInt(document.getElementById("holdingPeriod").value) || 5
          )
        );
        const revenueStart =
          parseFloat(document.getElementById("revenueStart").value) || 0;
        const revenueGrowth =
          (parseFloat(document.getElementById("revenueGrowth").value) || 0) /
          100;
        const ebitdaMarginStart =
          (parseFloat(document.getElementById("ebitdaMarginStart").value) ||
            0) / 100;
        const ebitdaMarginEnd =
          (parseFloat(document.getElementById("ebitdaMarginEnd").value) || 0) /
          100;
        const capexPct =
          (parseFloat(document.getElementById("capexPct").value) || 0) / 100;
        const wcPct =
          (parseFloat(document.getElementById("wcPct").value) || 0) / 100;
        const taxRate =
          (parseFloat(document.getElementById("taxRate").value) || 0) / 100;
        const exitMultiple =
          parseFloat(document.getElementById("exitMultiple").value) || 0;

        const debtAtClose = purchasePrice * debtPct;
        const equityCheck = purchasePrice - debtAtClose;
        const leverage = entryEbitda > 0 ? debtAtClose / entryEbitda : NaN;

        // Projections
        let revenue = revenueStart;
        let openingDebt = debtAtClose;

        const rows = [];
        for (let year = 1; year <= holdingPeriod; year++) {
          if (year > 1) {
            revenue = revenue * (1 + revenueGrowth);
          }

          let margin;
          if (holdingPeriod === 1) {
            margin = ebitdaMarginEnd;
          } else {
            const t = (year - 1) / (holdingPeriod - 1);
            margin =
              ebitdaMarginStart + (ebitdaMarginEnd - ebitdaMarginStart) * t;
          }
          const ebitda = revenue * margin;
          const interest = openingDebt * interestRate;
          const taxes = Math.max(0, (ebitda - interest) * taxRate);
          const capex = revenue * capexPct;
          const deltaWc = revenue * wcPct;
          const fcf = ebitda - taxes - capex - deltaWc;

          const mandatoryAmort = openingDebt * amortizationPct;
          const cashAfterMandatory = fcf - interest - mandatoryAmort;
          const optionalPaydown = Math.max(0, cashAfterMandatory);
          const totalPrincipal = Math.min(
            openingDebt,
            mandatoryAmort + optionalPaydown
          );
          const endingDebt = Math.max(0, openingDebt - totalPrincipal);

          rows.push({
            year,
            revenue,
            ebitda,
            margin,
            interest,
            principal: totalPrincipal,
            endingDebt,
            fcf,
          });

          openingDebt = endingDebt;
        }

        // Exit
        const final = rows[rows.length - 1];
        const exitEbitda = final ? final.ebitda : 0;
        const exitEv = exitEbitda * exitMultiple;
        const netDebtExit = final ? final.endingDebt : 0;
        const equityExit = exitEv - netDebtExit;

        const irr = solveSimpleIrr(equityCheck, equityExit, holdingPeriod);
        const moic = equityCheck > 0 ? equityExit / equityCheck : NaN;

        // Update summary
        document.getElementById("equityIrrDisplay").textContent = isFinite(irr)
          ? fmtPct(irr * 100)
          : "–";
        document.getElementById("moicDisplay").textContent = isFinite(moic)
          ? fmtX(moic)
          : "–";
        document.getElementById("leverageDisplay").textContent = isFinite(
          leverage
        )
          ? fmtX(leverage)
          : "–";

        document.getElementById("equityCheckDisplay").textContent =
          fmtMoney(equityCheck);
        document.getElementById("debtAtCloseDisplay").textContent =
          fmtMoney(debtAtClose);
        document.getElementById("debtAtExitDisplay").textContent =
          fmtMoney(netDebtExit);

        // Update table
        const tbody = document.getElementById("lboTableBody");
        tbody.innerHTML = "";
        rows.forEach((r) => {
          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="px-3 py-2 text-left text-gray-800">Year ${r.year}</td>
        <td class="px-3 py-2 text-right text-gray-800">${fmtMoney(
          r.revenue
        )}</td>
        <td class="px-3 py-2 text-right text-gray-800">${fmtMoney(
          r.ebitda
        )}</td>
        <td class="px-3 py-2 text-right text-gray-800">${fmtPct(
          r.margin * 100
        )}</td>
        <td class="px-3 py-2 text-right text-gray-800">${fmtMoney(
          r.interest
        )}</td>
        <td class="px-3 py-2 text-right text-gray-800">${fmtMoney(
          r.principal
        )}</td>
        <td class="px-3 py-2 text-right text-gray-800">${fmtMoney(
          r.endingDebt
        )}</td>
        <td class="px-3 py-2 text-right text-gray-800">${fmtMoney(r.fcf)}</td>
      `;
          tbody.appendChild(tr);
        });

        // Add exit row
        const exitRow = document.createElement("tr");
        exitRow.className = "bg-yellow-50";
        exitRow.innerHTML = `
      <td class="px-3 py-2 text-left font-semibold text-yellow-900">Exit</td>
      <td class="px-3 py-2 text-right text-yellow-900">EV: ${fmtMoney(
        exitEv
      )}</td>
      <td class="px-3 py-2 text-right text-yellow-900">Exit EBITDA: ${fmtMoney(
        exitEbitda
      )}</td>
      <td class="px-3 py-2 text-right text-yellow-900">Multiple: ${fmtX(
        exitMultiple
      )}</td>
      <td class="px-3 py-2 text-right text-yellow-900">Net Debt: ${fmtMoney(
        netDebtExit
      )}</td>
      <td class="px-3 py-2 text-right text-yellow-900">Equity Exit: ${fmtMoney(
        equityExit
      )}</td>
      <td class="px-3 py-2 text-right text-yellow-900" colspan="2">IRR: ${
        isFinite(irr) ? fmtPct(irr * 100) : "–"
      }, MOIC: ${isFinite(moic) ? fmtX(moic) : "–"}</td>
    `;
        tbody.appendChild(exitRow);
      }

      function resetLboDefaults() {
        document.getElementById("purchasePrice").value = 500;
        document.getElementById("entryEbitda").value = 50;
        document.getElementById("debtPct").value = 60;
        document.getElementById("interestRate").value = 7;
        document.getElementById("amortizationPct").value = 10;

        document.getElementById("holdingPeriod").value = 5;
        document.getElementById("revenueStart").value = 300;
        document.getElementById("revenueGrowth").value = 5;
        document.getElementById("ebitdaMarginStart").value = 16.7;
        document.getElementById("ebitdaMarginEnd").value = 20;
        document.getElementById("capexPct").value = 3;
        document.getElementById("wcPct").value = 1;
        document.getElementById("taxRate").value = 25;
        document.getElementById("exitMultiple").value = 10;

        runLboModel();
      }

      document.addEventListener("DOMContentLoaded", function () {
        document
          .getElementById("runLboBtn")
          .addEventListener("click", runLboModel);
        document
          .getElementById("resetLboBtn")
          .addEventListener("click", resetLboDefaults);
        resetLboDefaults();
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
