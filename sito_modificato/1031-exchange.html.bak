    <script>
      function fmt(n) {
        if (!isFinite(n)) return "$0";
        const sign = n < 0 ? "-" : "";
        const v = Math.abs(n);
        return (
          sign + "$" + v.toLocaleString(undefined, { maximumFractionDigits: 0 })
        );
      }
      function fmt2(n) {
        if (!isFinite(n)) return "$0";
        const sign = n < 0 ? "-" : "";
        const v = Math.abs(n);
        return (
          sign +
          "$" +
          v.toLocaleString(undefined, {
            minimumFractionDigits: 2,
            maximumFractionDigits: 2,
          })
        );
      }

      function calc1031() {
        const get = (id) => parseFloat(document.getElementById(id).value) || 0;

        const origPurchasePrice = get("origPurchasePrice");
        const origAcqCosts = get("origAcqCosts");
        const improvements = get("improvements");
        const depreciation = get("depreciation");
        const salePrice = get("salePrice");
        const sellingCosts = get("sellingCosts");
        const oldLoanBalance = get("oldLoanBalance");

        const newPurchasePrice = get("newPurchasePrice");
        const newAcqCosts = get("newAcqCosts");
        const newLoanAmount = get("newLoanAmount");

        const cgRate = get("cgRate") / 100;
        const recapRate = get("recapRate") / 100;
        const stateRate = get("stateRate") / 100;
        const niitRate = get("niitRate") / 100;

        const errorEl = document.getElementById("calcError");
        if (salePrice <= 0 || newPurchasePrice <= 0) {
          errorEl.classList.remove("hidden");
          return;
        } else {
          errorEl.classList.add("hidden");
        }

        // Basis and gain
        const adjBasis =
          origPurchasePrice + origAcqCosts + improvements - depreciation;
        const amountRealized = salePrice - sellingCosts;
        const realizedGain = amountRealized - adjBasis;

        const recapGain = Math.max(Math.min(depreciation, realizedGain), 0);
        const cgGain = Math.max(realizedGain - recapGain, 0);

        const recapTax = recapGain * recapRate;
        const cgTax = cgGain * cgRate;
        const stateTax = realizedGain > 0 ? realizedGain * stateRate : 0;
        const niitTax = realizedGain > 0 ? realizedGain * niitRate : 0;
        const totalTax = Math.max(recapTax + cgTax + stateTax + niitTax, 0);

        const netSaleProceeds = amountRealized - oldLoanBalance;
        const netCashAfterTax = netSaleProceeds - totalTax;

        // 1031 exchange calculations
        const newTotalCost = newPurchasePrice + newAcqCosts;
        const equityNeeded = newTotalCost - newLoanAmount;
        const cashBoot = Math.max(netSaleProceeds - equityNeeded, 0);
        const debtRelief = Math.max(oldLoanBalance - newLoanAmount, 0);
        const totalBoot = cashBoot + debtRelief;

        const recognizedGain = Math.max(Math.min(realizedGain, totalBoot), 0);
        constredGain = Math.max(realizedGain - recognizedGain, 0);

        // Allocate recognized gain proportionally between recap and capital gain (simplified)
        let recogRecap = 0,
          recogCg = 0;
        if (realizedGain > 0 && recognizedGain > 0) {
          const recapShare = recapGain / realizedGain;
          recogRecap = Math.min(recognizedGain * recapShare, recapGain);
          recogCg = Math.max(recognizedGain - recogRecap, 0);
        }

        const exRecapTax = recogRecap * recapRate;
        const exCgTax = recogCg * cgRate;
        const exStateTax = recognizedGain * stateRate;
        const exNiitTax = recognizedGain * niitRate;
        const exTaxNow = Math.max(
          exRecapTax + exCgTax + exStateTax + exNiitTax,
          0
        );

        const taxDeferred = Math.max(totalTax - exTaxNow, 0);

        const newBasis = newTotalCost -redGain;

        // Populate "no 1031" results
        document.getElementById("noAdjBasis").textContent = fmt2(adjBasis);
        document.getElementById("noAmountRealized").textContent =
          fmt2(amountRealized);
        document.getElementById("noRealizedGain").textContent =
          fmt2(realizedGain);
        document.getElementById("noRecapGain").textContent = fmt2(recapGain);
        document.getElementById("noCgGain").textContent = fmt2(cgGain);
        document.getElementById("noRecapTax").textContent = fmt2(recapTax);
        document.getElementById("noCgTax").textContent = fmt2(cgTax);
        document.getElementById("noStateTax").textContent = fmt2(stateTax);
        document.getElementById("noNiitTax").textContent = fmt2(niitTax);
        document.getElementById("noTotalTax").textContent = fmt2(totalTax);
        document.getElementById("noNetCash").textContent =
          fmt2(netCashAfterTax);

        // Populate 1031 results
        document.getElementById("exNetEquity").textContent =
          fmt2(netSaleProceeds);
        document.getElementById("exCashBoot").textContent = fmt2(cashBoot);
        document.getElementById("exDebtBoot").textContent = fmt2(debtRelief);
        document.getElementById("exTotalBoot").textContent = fmt2(totalBoot);
        document.getElementById("exRealizedGain").textContent =
          fmt2(realizedGain);
        document.getElementById("exRecognizedGain").textContent =
          fmt2(recognizedGain);
        document.getElementById("exTaxNow").textContent = fmt2(exTaxNow);
        document.getElementById("exTaxDeferred").textContent =
          fmt2(taxDeferred);
        document.getElementById("exNewBasis").textContent = fmt2(newBasis);
        document.getElementById("exDeferredGain").textContent =
          fmt2(deferredGain);
      }

      document.getElementById("calcBtn").addEventListener("click", calc1031);
      document.getElementById("resetBtn").addEventListener("click", () => {
        document.getElementById("origPurchasePrice").value = 300000;
        document.getElementById("origAcqCosts").value = 6000;
        document.getElementById("improvements").value = 40000;
        document.getElementById("depreciation").value = 70000;
        document.getElementById("salePrice").value = 550000;
        document.getElementById("sellingCosts").value = 33000;
        document.getElementById("oldLoanBalance").value = 250000;
        document.getElementById("newPurchasePrice").value = 600000;
        document.getElementById("newAcqCosts").value = 8000;
        document.getElementById("newLoanAmount").value = 300000;
        document.getElementById("cgRate").value = 15;
        document.getElementById("recapRate").value = 25;
        document.getElementById("stateRate").value = 5;
        document.getElementById("niitRate").value = 3.8;
        calc1031();
      });

      // Initial calculation on load
      document.addEventListener("DOMContentLoaded", calc1031);
    </script>

    <script>
      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
