<script="">
        function calculateDPMO() {
            const unitsProduced = document.getElementById('unitsProduced').value;
            const defects = document.getElementById('defects').value;
            const opportunities = document.getElementById('opportunities').value;

            if (unitsProduced && defects && opportunities) {
                const dpmo = (defects / (unitsProduced * opportunities)) * 1000000;
                document.getElementById('dpmoResult').textContent = dpmo.toFixed(2);
            } else {
                alert('Please fill in all fields.');
            }
        }
    </script>


