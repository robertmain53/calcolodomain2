    <script>
      (function () {
        const unitToggle = document.getElementById("unit-toggle");
        const deckLength = document.getElementById("deck-length");
        const deckWidth = document.getElementById("deck-width");
        const deckLengthUnit = document.getElementById("deck-length-unit");
        const deckWidthUnit = document.getElementById("deck-width-unit");

        const boardProfile = document.getElementById("board-profile");
        const boardWidth = document.getElementById("board-width");
        const boardWidthUnit = document.getElementById("board-width-unit");
        const boardGap = document.getElementById("board-gap");
        const boardGapUnit = document.getElementById("board-gap-unit");

        const joistSpacing = document.getElementById("joist-spacing");
        const beamSpacing = document.getElementById("beam-spacing");
        const postSpacing = document.getElementById("post-spacing");

        const wasteFactor = document.getElementById("waste-factor");
        const currencySymbolInput = document.getElementById("currency-symbol");
        const currencyEls = [
          document.getElementById("currency-symbol-board"),
          document.getElementById("currency-symbol-joist"),
          document.getElementById("currency-symbol-post"),
          document.getElementById("currency-symbol-footing"),
        ];

        const pricePerBoard = document.getElementById("price-per-board");
        const pricePerJoist = document.getElementById("price-per-joist");
        const pricePerPost = document.getElementById("price-per-post");
        const pricePerFooting = document.getElementById("price-per-footing");

        const calculateBtn = document.getElementById("calculate-btn");
        const resetBtn = document.getElementById("reset-btn");
        const errorMessage = document.getElementById("error-message");

        const summaryArea = document.getElementById("summary-area");
        const summaryAreaAlt = document.getElementById("summary-area-alt");
        const summaryBoards = document.getElementById("summary-boards");
        const summaryBoardLength = document.getElementById(
          "summary-board-length"
        );
        const summaryCost = document.getElementById("summary-cost");
        const resultsBody = document.getElementById("results-body");

        function getCurrency() {
          return currencySymbolInput.value || "$";
        }

        currencySymbolInput.addEventListener("input", () => {
          const sym = getCurrency();
          currencyEls.forEach((el) => (el.textContent = sym));
        });

        // Handle board profile presets
        boardProfile.addEventListener("change", () => {
          const val = boardProfile.value;
          if (val === "custom") {
            boardWidth.removeAttribute("readonly");
          } else {
            boardWidth.setAttribute("readonly", "readonly");
            if (val === "5.5" || val === "5.5-2x6") {
              boardWidth.value = 5.5;
            } else if (val === "3.5") {
              boardWidth.value = 3.5;
            }
          }
        });

        // Unit toggle handler
        unitToggle.addEventListener("change", () => {
          const isMetric = unitToggle.checked;

          if (isMetric) {
            // Convert ft to m (approx)
            if (deckLength.value)
              deckLength.value = (
                parseFloat(deckLength.value) * 0.3048
              ).toFixed(2);
            if (deckWidth.value)
              deckWidth.value = (parseFloat(deckWidth.value) * 0.3048).toFixed(
                2
              );
            deckLengthUnit.textContent = "m";
            deckWidthUnit.textContent = "m";

            // Convert in to mm
            if (boardWidth.value)
              boardWidth.value = (parseFloat(boardWidth.value) * 25.4).toFixed(
                1
              );
            if (boardGap.value)
              boardGap.value = (parseFloat(boardGap.value) * 25.4).toFixed(1);
            boardWidthUnit.textContent = "mm";
            boardGapUnit.textContent = "mm";

            // Convert joist spacing and beam/post spacing to metric labels only (values remain imperial inches/feet for logic)
            joistSpacing.options[0].textContent = '400 mm OC (approx 16")';
            joistSpacing.options[1].textContent = '300 mm OC (approx 12")';
            joistSpacing.options[2].textContent = '600 mm OC (approx 24")';

            beamSpacing.options[0].textContent = "2.4 m (approx 8 ft)";
            beamSpacing.options[1].textContent = "3.0 m (approx 10 ft)";
            beamSpacing.options[2].textContent = "3.6 m (approx 12 ft)";

            postSpacing.options[0].textContent = "1.8 m (approx 6 ft)";
            postSpacing.options[1].textContent = "2.4 m (approx 8 ft)";
            postSpacing.options[2].textContent = "3.0 m (approx 10 ft)";
          } else {
            // Convert m to ft
            if (deckLength.value)
              deckLength.value = (
                parseFloat(deckLength.value) / 0.3048
              ).toFixed(2);
            if (deckWidth.value)
              deckWidth.value = (parseFloat(deckWidth.value) / 0.3048).toFixed(
                2
              );
            deckLengthUnit.textContent = "ft";
            deckWidthUnit.textContent = "ft";

            // Convert mm to in
            if (boardWidth.value)
              boardWidth.value = (parseFloat(boardWidth.value) / 25.4).toFixed(
                2
              );
            if (boardGap.value)
              boardGap.value = (parseFloat(boardGap.value) / 25.4).toFixed(2);
            boardWidthUnit.textContent = "in";
            boardGapUnit.textContent = "in";

            joistSpacing.options[0].textContent = '16" OC (standard)';
            joistSpacing.options[1].textContent =
              '12" OC (stiffer / composite)';
            joistSpacing.options[2].textContent = '24" OC (light loads)';

            beamSpacing.options[0].textContent = "8 ft (2.4 m typical)";
            beamSpacing.options[1].textContent = "10 ft (3.0 m)";
            beamSpacing.options[2].textContent = "12 ft (3.6 m)";

            postSpacing.options[0].textContent = "6 ft (1.8 m)";
            postSpacing.options[1].textContent = "8 ft (2.4 m)";
            postSpacing.options[2].textContent = "10 ft (3.0 m)";
          }
        });

        function resetForm() {
          unitToggle.checked = false;
          deckLength.value = 16;
          deckWidth.value = 12;
          deckLengthUnit.textContent = "ft";
          deckWidthUnit.textContent = "ft";

          boardProfile.value = "5.5";
          boardWidth.value = 5.5;
          boardWidth.removeAttribute("readonly");
          boardWidth.setAttribute("readonly", "readonly");
          boardWidthUnit.textContent = "in";
          boardGap.value = 0.25;
          boardGapUnit.textContent = "in";

          joistSpacing.value = "16";
          beamSpacing.value = "8";
          postSpacing.value = "6";

          wasteFactor.value = 10;
          currencySymbolInput.value = "$";
          currencyEls.forEach((el) => (el.textContent = "$"));

          pricePerBoard.value = 15;
          pricePerJoist.value = 12;
          pricePerPost.value = 25;
          pricePerFooting.value = 15;

          errorMessage.classList.add("hidden");
          summaryArea.textContent = "–";
          summaryAreaAlt.textContent = "";
          summaryBoards.textContent = "–";
          summaryCost.textContent = "–";
          resultsBody.innerHTML =
            '<tr><td class="px-3 py-2 text-gray-600" colspan="4">Run the calculation to see a full material breakdown.</td></tr>';
        }

        resetBtn.addEventListener("click", resetForm);

        function formatNumber(n, decimals = 0) {
          if (!isFinite(n)) return "–";
          return n.toLocaleString(undefined, {
            maximumFractionDigits: decimals,
            minimumFractionDigits: decimals,
          });
        }

        function calculate() {
          errorMessage.classList.add("hidden");

          const isMetric = unitToggle.checked;
          const L = parseFloat(deckLength.value);
          const W = parseFloat(deckWidth.value);
          const bw = parseFloat(boardWidth.value);
          const gap = parseFloat(boardGap.value);
          const waste = parseFloat(wasteFactor.value);

          if (!(L > 0 && W > 0 && bw > 0)) {
            errorMessage.textContent =
              "Please enter valid positive values for deck size and board width.";
            errorMessage.classList.remove("hidden");
            return;
          }

          // Area
          let areaPrimary, areaAlt, areaPrimaryUnit, areaAltUnit;
          if (!isMetric) {
            areaPrimary = L * W; // ft²
            areaAlt = areaPrimary * 0.092903; // m²
            areaPrimaryUnit = "ft²";
            areaAltUnit = "m²";
          } else {
            areaPrimary = L * W; // m²
            areaAlt = areaPrimary * 10.7639; // ft²
            areaPrimaryUnit = "m²";
            areaAltUnit = "ft²";
          }

          summaryArea.textContent = `${formatNumber(
            areaPrimary,
            1
          )} ${areaPrimaryUnit}`;
          summaryAreaAlt.textContent = `${formatNumber(
            areaAlt,
            1
          )} ${areaAltUnit}`;

          // Board count
          let deckWidthIn, effBoardWidthIn;
          if (!isMetric) {
            deckWidthIn = W * 12;
            effBoardWidthIn = bw + gap;
          } else {
            // convert m to in
            deckWidthIn = W * 39.3701;
            // bw and gap are in mm -> in
            effBoardWidthIn = (bw + gap) / 25.4;
          }
          const boardsRaw = deckWidthIn / effBoardWidthIn;
          let boards = Math.ceil(boardsRaw);

          // Apply waste
          if (waste > 0) {
            boards = Math.ceil(boards * (1 + waste / 100));
          }

          summaryBoards.textContent = formatNumber(boards, 0);

          // Assume 8 ft boards for summary text
          summaryBoardLength.textContent =
            "Assuming 8 ft boards (adjust manually if using other lengths)";

          // Joists
          const js = parseFloat(joistSpacing.value); // inches
          let deckLengthIn;
          if (!isMetric) {
            deckLengthIn = L * 12;
          } else {
            deckLengthIn = L * 39.3701;
          }
          const joistsRaw = deckLengthIn / js;
          const joists = Math.floor(joistsRaw) + 1;

          // Beams: assume 2 beams
          const beams = 2;

          // Posts
          const ps = parseFloat(postSpacing.value); // ft
          let deckWidthFt;
          if (!isMetric) {
            deckWidthFt = W;
          } else {
            deckWidthFt = W * 3.28084;
          }
          const postsPerBeam = Math.floor(deckWidthFt / ps) + 1;
          const posts = postsPerBeam * beams;
          const footings = posts;

          // Costs
          const cur = getCurrency();
          const boardCost = boards * (parseFloat(pricePerBoard.value) || 0);
          const joistCost = joists * (parseFloat(pricePerJoist.value) || 0);
          const postCost = posts * (parseFloat(pricePerPost.value) || 0);
          const footingCost =
            footings * (parseFloat(pricePerFooting.value) || 0);
          const totalCost = boardCost + joistCost + postCost + footingCost;

          summaryCost.textContent =
            totalCost > 0 ? `${cur}${formatNumber(totalCost, 0)}` : "–";

          // Populate table
          const rows = [];

          rows.push(`
      <tr class="border-t">
        <td class="px-3 py-2 text-gray-800">Decking boards</td>
        <td class="px-3 py-2 text-right">${formatNumber(boards, 0)}</td>
        <td class="px-3 py-2 text-right">boards</td>
        <td class="px-3 py-2 text-right">${
          boardCost > 0 ? cur + formatNumber(boardCost, 0) : "–"
        }</td>
      </tr>
    `);

          rows.push(`
      <tr class="border-t">
        <td class="px-3 py-2 text-gray-800">Joists</td>
        <td class="px-3 py-2 text-right">${formatNumber(joists, 0)}</td>
        <td class="px-3 py-2 text-right">pieces</td>
        <td class="px-3 py-2 text-right">${
          joistCost > 0 ? cur + formatNumber(joistCost, 0) : "–"
        }</td>
      </tr>
    `);

          rows.push(`
      <tr class="border-t">
        <td class="px-3 py-2 text-gray-800">Beams (assumed)</td>
        <td class="px-3 py-2 text-right">${formatNumber(beams, 0)}</td>
        <td class="px-3 py-2 text-right">pieces</td>
        <td class="px-3 py-2 text-right text-gray-400 text-xs">Not costed (varies by design)</td>
      </tr>
    `);

          rows.push(`
      <tr class="border-t">
        <td class="px-3 py-2 text-gray-800">Posts</td>
        <td class="px-3 py-2 text-right">${formatNumber(posts, 0)}</td>
        <td class="px-3 py-2 text-right">posts</td>
        <td class="px-3 py-2 text-right">${
          postCost > 0 ? cur + formatNumber(postCost, 0) : "–"
        }</td>
      </tr>
    `);

          rows.push(`
      <tr class="border-t">
        <td class="px-3 py-2 text-gray-800">Concrete footings</td>
        <td class="px-3 py-2 text-right">${formatNumber(footings, 0)}</td>
        <td class="px-3 py-2 text-right">footings</td>
        <td class="px-3 py-2 text-right">${
          footingCost > 0 ? cur + formatNumber(footingCost, 0) : "–"
        }</td>
      </tr>
    `);

          rows.push(`
      <tr class="border-t bg-gray-50 font-semibold">
        <td class="px-3 py-2 text-gray-900">Total (materials only)</td>
        <td class="px-3 py-2 text-right">–</td>
        <td class="px-3 py-2 text-right">–</td>
        <td class="px-3 py-2 text-right">${
          totalCost > 0 ? cur + formatNumber(totalCost, 0) : "–"
        }</td>
      </tr>
    `);

          resultsBody.innerHTML = rows.join("");
        }

        calculateBtn.addEventListener("click", calculate);
      })();
    </script>
  
