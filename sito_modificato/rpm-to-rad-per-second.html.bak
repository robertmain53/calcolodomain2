    <script>
      // RPM <-> rad/s converter logic
      (function () {
        const MODE_RPM_TO_RAD = "rpm-to-rad";
        const MODE_RAD_TO_RPM = "rad-to-rpm";

        let mode = MODE_RPM_TO_RAD;

        const modeRpmBtn = document.getElementById("mode-rpm-to-rad");
        const modeRadBtn = document.getElementById("mode-rad-to-rpm");
        const inputValueEl = document.getElementById("input-value");
        const inputUnitLabel = document.getElementById("input-unit-label");
        const decimalsEl = document.getElementById("decimals");
        const resultValueEl = document.getElementById("result-value");
        const resultUnitEl = document.getElementById("result-unit");
        const exactFactorLabelEl =
          document.getElementById("exact-factor-label");
        const exactFactorValueEl =
          document.getElementById("exact-factor-value");
        const errorMessageEl = document.getElementById("error-message");
        const swapBtn = document.getElementById("swap-btn");
        const copyBtn = document.getElementById("copy-result");

        function setMode(newMode) {
          mode = newMode;
          if (mode === MODE_RPM_TO_RAD) {
            modeRpmBtn.classList.remove(
              "border-gray-300",
              "bg-white",
              "text-gray-700"
            );
            modeRpmBtn.classList.add(
              "border-blue-600",
              "bg-blue-50",
              "text-blue-700"
            );
            modeRadBtn.classList.remove(
              "border-blue-600",
              "bg-blue-50",
              "text-blue-700"
            );
            modeRadBtn.classList.add(
              "border-gray-300",
              "bg-white",
              "text-gray-700"
            );
            inputUnitLabel.textContent = "RPM";
            resultUnitEl.textContent = "rad/s";
          } else {
            modeRadBtn.classList.remove(
              "border-gray-300",
              "bg-white",
              "text-gray-700"
            );
            modeRadBtn.classList.add(
              "border-blue-600",
              "bg-blue-50",
              "text-blue-700"
            );
            modeRpmBtn.classList.remove(
              "border-blue-600",
              "bg-blue-50",
              "text-blue-700"
            );
            modeRpmBtn.classList.add(
              "border-gray-300",
              "bg-white",
              "text-gray-700"
            );
            inputUnitLabel.textContent = "rad/s";
            resultUnitEl.textContent = "RPM";
          }
          updateResult();
        }

        function formatNumber(value, decimals) {
          if (!isFinite(value)) return "—";
          return Number(value).toFixed(decimals);
        }

        function updateResult() {
          errorMessageEl.classList.add("hidden");
          const raw = inputValueEl.value.trim();
          if (raw === "") {
            resultValueEl.textContent = "0";
            if (mode === MODE_RPM_TO_RAD) {
              exactFactorLabelEl.textContent = "1 RPM × 2π / 60";
              exactFactorValueEl.textContent = "= 0.1047197551 rad/s";
            } else {
              exactFactorLabelEl.textContent = "1 rad/s × 60 / (2π)";
              exactFactorValueEl.textContent = "= 9.549296586 RPM";
            }
            return;
          }

          const x = Number(raw);
          if (!isFinite(x)) {
            errorMessageEl.textContent = "Please enter a valid numeric value.";
            errorMessageEl.classList.remove("hidden");
            return;
          }

          const decimals = parseInt(decimalsEl.value, 10) || 4;
          let result;
          if (mode === MODE_RPM_TO_RAD) {
            // rad/s = RPM * 2π / 60
            result = (x * 2 * Math.PI) / 60;
            exactFactorLabelEl.textContent = x + " RPM × 2π / 60";
            exactFactorValueEl.textContent =
              "= " + formatNumber(result, Math.max(decimals, 6)) + " rad/s";
          } else {
            // RPM = rad/s * 60 / (2π)
            result = (x * 60) / (2 * Math.PI);
            exactFactorLabelEl.textContent = x + " rad/s × 60 / (2π)";
            exactFactorValueEl.textContent =
              "= " + formatNumber(result, Math.max(decimals, 6)) + " RPM";
          }

          resultValueEl.textContent = formatNumber(result, decimals);
        }

        modeRpmBtn.addEventListener("click", () => setMode(MODE_RPM_TO_RAD));
        modeRadBtn.addEventListener("click", () => setMode(MODE_RAD_TO_RPM));
        swapBtn.addEventListener("click", () => {
          setMode(mode === MODE_RPM_TO_RAD ? MODE_RAD_TO_RPM : MODE_RPM_TO_RAD);
        });

        inputValueEl.addEventListener("input", updateResult);
        decimalsEl.addEventListener("change", updateResult);

        copyBtn.addEventListener("click", () => {
          const text =
            resultValueEl.textContent + " " + resultUnitEl.textContent;
          if (!navigator.clipboard) {
            // Fallback
            const temp = document.createElement("textarea");
            temp.value = text;
            document.body.appendChild(temp);
            temp.select();
            try {
              document.execCommand("copy");
            } catch (e) {}
            document.body.removeChild(temp);
          } else {
            navigator.clipboard.writeText(text).catch(() => {});
          }
          copyBtn.textContent = "Copied!";
          setTimeout(() => {
            copyBtn.textContent = "Copy";
          }, 1200);
        });

        // Initialize with 1 RPM example
        inputValueEl.value = "1";
        setMode(MODE_RPM_TO_RAD);
      })();
    </script>

