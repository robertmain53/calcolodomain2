    <script>
      function safeNumber(id, fallback = 0) {
        const el = document.getElementById(id);
        if (!el) return fallback;
        const v = parseFloat(el.value);
        return isNaN(v) ? fallback : v;
      }

      function formatMoney(value) {
        if (!isFinite(value)) return "$0.00";
        return "$" + value.toFixed(2);
      }

      function formatPercent(value) {
        if (!isFinite(value)) return "0.00%";
        return (value * 100).toFixed(2) + "%";
      }

      function calculateFees() {
        const volume = safeNumber("monthly-volume", 0);
        const avgTicket = Math.max(safeNumber("avg-ticket", 0), 0.01);
        const txns = volume > 0 ? volume / avgTicket : 0;

        const percentRate = safeNumber("percent-rate", 0) / 100;
        const perTxn = safeNumber("per-txn-fee", 0);
        const monthlyFees = safeNumber("monthly-fees", 0);
        const cbCount = safeNumber("chargeback-count", 0);
        const cbFee = safeNumber("chargeback-fee", 0);

        const variableFees = volume * percentRate + txns * perTxn;
        const fixedFees = monthlyFees + cbCount * cbFee;
        const totalFees = variableFees + fixedFees;
        const effectiveRate = volume > 0 ? totalFees / volume : 0;
        const netRevenue = volume - totalFees;

        document.getElementById("res-txns").textContent = txns.toFixed(0);
        document.getElementById("res-total-fees").textContent =
          formatMoney(totalFees);
        document.getElementById("res-net").textContent =
          formatMoney(netRevenue);
        document.getElementById("res-effective-rate").textContent =
          formatPercent(effectiveRate);

        // Comparison
        const compareSection = document.getElementById("compare-section");
        if (!compareSection.classList.contains("hidden")) {
          const altPercent = safeNumber("alt-percent-rate", 0) / 100;
          const altPerTxn = safeNumber("alt-per-txn-fee", 0);
          const altMonthly = safeNumber("alt-monthly-fees", 0);
          const altCbFee = safeNumber("alt-chargeback-fee", 0);

          const altVariable = volume * altPercent + txns * altPerTxn;
          const altFixed = altMonthly + cbCount * altCbFee;
          const altTotal = altVariable + altFixed;
          const altEff = volume > 0 ? altTotal / volume : 0;

          const savingsMonthly = totalFees - altTotal;
          const savingsAnnual = savingsMonthly * 12;

          document.getElementById("alt-total-fees").textContent =
            formatMoney(altTotal);
          document.getElementById("alt-effective-rate").textContent =
            formatPercent(altEff);
          document.getElementById("alt-savings-monthly").textContent =
            formatMoney(savingsMonthly);
          document.getElementById("alt-savings-annual").textContent =
            formatMoney(savingsAnnual);
        }
      }

      document.addEventListener("DOMContentLoaded", function () {
        const btn = document.getElementById("calc-btn");
        if (btn) btn.addEventListener("click", calculateFees);

        const toggleCompare = document.getElementById("toggle-compare");
        const compareSection = document.getElementById("compare-section");
        if (toggleCompare && compareSection) {
          toggleCompare.addEventListener("click", function () {
            const hidden = compareSection.classList.toggle("hidden");
            if (hidden) {
              toggleCompare.textContent = "Enable";
              toggleCompare.classList.remove("toggle-btn-active");
              toggleCompare.classList.add("toggle-btn-inactive");
            } else {
              toggleCompare.textContent = "Disable";
              toggleCompare.classList.remove("toggle-btn-inactive");
              toggleCompare.classList.add("toggle-btn-active");
              calculateFees();
            }
          });
        }

        // Auto-calc on load with defaults
        calculateFees();
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
