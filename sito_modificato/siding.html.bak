<script>
  // Utility helpers
  function createWallRow(index, unitSystem) {
    const isImperial = unitSystem === 'imperial';
    const lengthLabel = isImperial ? 'ft' : 'm';
    const heightLabel = isImperial ? 'ft' : 'm';

    const div = document.createElement('div');
    div.className = "border border-gray-200 rounded-md p-3 flex flex-col gap-2 md:flex-row md:items-end md:gap-4";
    div.dataset.wallIndex = index;

    div.innerHTML = `
      <div class="flex-1">
        <label class="block text-xs font-medium text-gray-700 mb-1">Wall type</label>
        <select class="wall-type w-full border-gray-300 rounded-md shadow-sm focus:ring-blue-500 focus:border-blue-500">
          <option value="rect">Rectangular</option>
          <option value="gable">Gable (triangle)</option>
        </select>
      </div>
      <div class="flex-1">
        <label class="block text-xs font-medium text-gray-700 mb-1">Width / base</label>
        <div class="flex">
          <input type="number" min="0" step="0.01" class="wall-width flex-1 border-gray-300 rounded-l-md shadow-sm focus:ring-blue-500 focus:border-blue-500" value="10">
          <span class="inline-flex items-center px-2 border border-l-0 border-gray-300 rounded-r-md bg-gray-50 text-xs text-gray-600">${lengthLabel}</span>
        </div>
      </div>
      <div class="flex-1">
        <label class="block text-xs font-medium text-gray-700 mb-1">Height${isImperial ? '' : ''}</label>
        <div class="flex">
          <input type="number" min="0" step="0.01" class="wall-height flex-1 border-gray-300 rounded-l-md shadow-sm focus:ring-blue-500 focus:border-blue-500" value="8">
          <span class="inline-flex items-center px-2 border border-l-0 border-gray-300 rounded-r-md bg-gray-50 text-xs text-gray-600">${heightLabel}</span>
        </div>
      </div>
      <button type="button" class="remove-wall text-xs text-red-600 hover:text-red-800 mt-2 md:mt-0">Remove</button>
    `;
    return div;
  }

  function createOpeningRow(index, unitSystem) {
    const isImperial = unitSystem === 'imperial';
    const lengthLabel = isImperial ? 'ft' : 'm';
    const heightLabel = isImperial ? 'ft' : 'm';

    const div = document.createElement('div');
    div.className = "border border-gray-200 rounded-md p-3 flex flex-col gap-2 md:flex-row md:items-end md:gap-4";
    div.dataset.openingIndex = index;

    div.innerHTML = `
      <div class="flex-1">
        <label class="block text-xs font-medium text-gray-700 mb-1">Opening label (optional)</label>
        <input type="text" class="opening-label w-full border-gray-300 rounded-md shadow-sm focus:ring-blue-500 focus:border-blue-500" placeholder="Window, door, garage...">
      </div>
      <div class="flex-1">
        <label class="block text-xs font-medium text-gray-700 mb-1">Width</label>
        <div class="flex">
          <input type="number" min="0" step="0.01" class="opening-width flex-1 border-gray-300 rounded-l-md shadow-sm focus:ring-blue-500 focus:border-blue-500" value="3">
          <span class="inline-flex items-center px-2 border border-l-0 border-gray-300 rounded-r-md bg-gray-50 text-xs text-gray-600">${lengthLabel}</span>
        </div>
      </div>
      <div class="flex-1">
        <label class="block text-xs font-medium text-gray-700 mb-1">Height</label>
        <div class="flex">
          <input type="number" min="0" step="0.01" class="opening-height flex-1 border-gray-300 rounded-l-md shadow-sm focus:ring-blue-500 focus:border-blue-500" value="4">
          <span class="inline-flex items-center px-2 border border-l-0 border-gray-300 rounded-r-md bg-gray-50 text-xs text-gray-600">${heightLabel}</span>
        </div>
      </div>
      <button type="button" class="remove-opening text-xs text-red-600 hover:text-red-800 mt-2 md:mt-0">Remove</button>
    `;
    return div;
  }

  function getUnitSystem() {
    const radios = document.querySelectorAll('input[name="unitSystem"]');
    for (const r of radios) {
      if (r.checked) return r.value;
    }
    return 'imperial';
  }

  function formatArea(value, unitSystem) {
    if (isNaN(value)) return '–';
    const unit = unitSystem === 'imperial' ? 'ft²' : 'm²';
    return value.toFixed(2) + ' ' + unit;
  }

  function formatLength(value, unitSystem) {
    if (isNaN(value)) return '–';
    const unit = unitSystem === 'imperial' ? 'ft' : 'm';
    return value.toFixed(2) + ' ' + unit;
  }

  function formatCurrency(value) {
    if (isNaN(value) || value <= 0) return '–';
    return '$' + value.toFixed(2);
  }

  document.addEventListener('DOMContentLoaded', function () {
    const wallsContainer = document.getElementById('walls-container');
    const openingsContainer = document.getElementById('openings-container');
    const addWallBtn = document.getElementById('add-wall-btn');
    const addOpeningBtn = document.getElementById('add-opening-btn');
    const calculateBtn = document.getElementById('calculate-btn');
    const resetBtn = document.getElementById('reset-btn');
    const errorMsg = document.getElementById('error-msg');

    const sidingTypeSelect = document.getElementById('siding-type');
    const sidingTypeNote = document.getElementById('siding-type-note');
    const panelCoverageInput = document.getElementById('panel-coverage');
    const panelCoverageUnit = document.getElementById('panel-coverage-unit');
    const wasteInput = document.getElementById('waste');
    const panelPriceInput = document.getElementById('panel-price');
    const laborRateInput = document.getElementById('labor-rate');
    const panelPerBoxInput = document.getElementById('panel-per-box');
    const ignoreOpeningsCheckbox = document.getElementById('ignore-openings');

    const cornerCountInput = document.getElementById('corner-count');
    const cornerLengthInput = document.getElementById('corner-length');
    const cornerLengthUnit = document.getElementById('corner-length-unit');
    const starterLengthInput = document.getElementById('starter-length');
    const starterLengthUnit = document.getElementById('starter-length-unit');
    const trimPriceInput = document.getElementById('trim-price');

    // Results
    const resWallArea = document.getElementById('res-wall-area');
    const resOpeningsArea = document.getElementById('res-openings-area');
    const resNetArea = document.getElementById('res-net-area');
    const resAdjustedArea = document.getElementById('res-adjusted-area');
    const resSquares = document.getElementById('res-squares');
    const resPanels = document.getElementById('res-panels');
    const resBoxes = document.getElementById('res-boxes');
    const resMaterialCost = document.getElementById('res-material-cost');
    const resLaborCost = document.getElementById('res-labor-cost');
    const resTrimCost = document.getElementById('res-trim-cost');
    const resTotalCost = document.getElementById('res-total-cost');
    const resCornerLength = document.getElementById('res-corner-length');
    const resStarterLength = document.getElementById('res-starter-length');

    let wallIndex = 0;
    let openingIndex = 0;

    function updateSidingTypeDefaults() {
      const type = sidingTypeSelect.value;
      const unitSystem = getUnitSystem();
      const isImperial = unitSystem === 'imperial';

      if (type === 'vinyl') {
        panelCoverageInput.value = isImperial ? 8 : 0.74; // approx 8 ft² or 0.74 m²
        sidingTypeNote.textContent = isImperial
          ? 'Typical 12 ft × 8 in vinyl panel (~8 ft² coverage).'
          : 'Typical vinyl panel (~0.74 m² coverage).';
      } else if (type === 'fiber') {
        panelCoverageInput.value = isImperial ? 7 : 0.65;
        sidingTypeNote.textContent = 'Fiber cement lap board (approximate coverage per board).';
      } else if (type === 'panel') {
        panelCoverageInput.value = isImperial ? 32 : 2.97; // 4x8 panel
        sidingTypeNote.textContent = '4 ft × 8 ft fiber cement panel (~32 ft² or 2.97 m²).';
      } else if (type === 'wood') {
        panelCoverageInput.value = isImperial ? 6.5 : 0.6;
        sidingTypeNote.textContent = 'Wood lap siding (coverage depends on exposure).';
      } else if (type === 'metal') {
        panelCoverageInput.value = isImperial ? 10 : 0.93;
        sidingTypeNote.textContent = 'Metal siding panel (approximate coverage).';
      } else {
        sidingTypeNote.textContent = 'Enter the net exposed area per panel or board.';
      }

      panelCoverageUnit.textContent = isImperial ? 'ft²' : 'm²';
    }

    function addInitialRows() {
      const unitSystem = getUnitSystem();
      wallsContainer.appendChild(createWallRow(wallIndex++, unitSystem));
      wallsContainer.appendChild(createWallRow(wallIndex++, unitSystem));
      openingsContainer.appendChild(createOpeningRow(openingIndex++, unitSystem));
    }

    function updateUnitsDisplay() {
      const unitSystem = getUnitSystem();
      const isImperial = unitSystem === 'imperial';

      // Update existing wall and opening rows
      wallsContainer.querySelectorAll('[data-wall-index]').forEach(row => {
        const spans = row.querySelectorAll('span.inline-flex');
        if (spans[0]) spans[0].textContent = isImperial ? 'ft' : 'm';
        if (spans[1]) spans[1].textContent = isImperial ? 'ft' : 'm';
      });

      openingsContainer.querySelectorAll('[data-opening-index]').forEach(row => {
        const spans = row.querySelectorAll('span.inline-flex');
        if (spans[0]) spans[0].textContent = isImperial ? 'ft' : 'm';
        if (spans[1]) spans[1].textContent = isImperial ? 'ft' : 'm';
      });

      cornerLengthUnit.textContent = isImperial ? 'ft' : 'm';
      starterLengthUnit.textContent = isImperial ? 'ft' : 'm';

      updateSidingTypeDefaults();
    }

    addWallBtn.addEventListener('click', () => {
      const unitSystem = getUnitSystem();
      wallsContainer.appendChild(createWallRow(wallIndex++, unitSystem));
    });

    addOpeningBtn.addEventListener('click', () => {
      const unitSystem = getUnitSystem();
      openingsContainer.appendChild(createOpeningRow(openingIndex++, unitSystem));
    });

    wallsContainer.addEventListener('click', (e) => {
      if (e.target.classList.contains('remove-wall')) {
        const row = e.target.closest('[data-wall-index]');
        if (row) row.remove();
      }
    });

    openingsContainer.addEventListener('click', (e) => {
      if (e.target.classList.contains('remove-opening')) {
        const row = e.target.closest('[data-opening-index]');
        if (row) row.remove();
      }
    });

    document.querySelectorAll('input[name="unitSystem"]').forEach(radio => {
      radio.addEventListener('change', () => {
        updateUnitsDisplay();
      });
    });

    sidingTypeSelect.addEventListener('change', updateSidingTypeDefaults);

    function resetResults() {
      const unitSystem = getUnitSystem();
      resWallArea.textContent = '–';
      resOpeningsArea.textContent = '–';
      resNetArea.textContent = '–';
      resAdjustedArea.textContent = '–';
      resSquares.textContent = '–';
      resPanels.textContent = '–';
      resBoxes.textContent = '–';
      resMaterialCost.textContent = '–';
      resLaborCost.textContent = '–';
      resTrimCost.textContent = '–';
      resTotalCost.textContent = '–';
      resCornerLength.textContent = '–';
      resStarterLength.textContent = '–';
    }

    function calculate() {
      errorMsg.textContent = '';
      resetResults();

      const unitSystem = getUnitSystem();
      const isImperial = unitSystem === 'imperial';

      // Collect walls
      const wallRows = wallsContainer.querySelectorAll('[data-wall-index]');
      if (wallRows.length === 0) {
        errorMsg.textContent = 'Please add at least one wall.';
        return;
      }

      let wallArea = 0;
      for (const row of wallRows) {
        const type = row.querySelector('.wall-type').value;
        const width = parseFloat(row.querySelector('.wall-width').value);
        const height = parseFloat(row.querySelector('.wall-height').value);
        if (isNaN(width) || isNaN(height) || width <= 0 || height <= 0) {
          errorMsg.textContent = 'Please enter valid dimensions for all walls.';
          return;
        }
        let area = 0;
        if (type === 'rect') {
          area = width * height;
        } else {
          area = width * height / 2;
        }
        wallArea += area;
      }

      // Collect openings
      let openingsArea = 0;
      if (!ignoreOpeningsCheckbox.checked) {
        const openingRows = openingsContainer.querySelectorAll('[data-opening-index]');
        for (const row of openingRows) {
          const w = parseFloat(row.querySelector('.opening-width').value);
          const h = parseFloat(row.querySelector('.opening-height').value);
          if (!isNaN(w) && !isNaN(h) && w > 0 && h > 0) {
            openingsArea += w * h;
          }
        }
      }

      let netArea = wallArea - openingsArea;
      if (netArea <= 0) {
        errorMsg.textContent = 'Net siding area is zero or negative. Check wall and opening dimensions.';
        return;
      }

      const waste = parseFloat(wasteInput.value) || 0;
      const wasteFactor = 1 + waste / 100;
      const adjustedArea = netArea * wasteFactor;

      // Convert to ft² if metric for squares and coverage
      let areaForSquares = adjustedArea;
      let panelCoverage = parseFloat(panelCoverageInput.value);
      if (isNaN(panelCoverage) || panelCoverage <= 0) {
        errorMsg.textContent = 'Please enter a valid coverage per panel.';
        return;
      }

      if (!isImperial) {
        // m² to ft²
        areaForSquares = adjustedArea * 10.7639;
        // panel coverage is in m²; convert to ft² for panel count
        panelCoverage = panelCoverage * 10.7639;
      }

      const squares = areaForSquares / 100;
      const panelsNeeded = adjustedArea / (isImperial ? parseFloat(panelCoverageInput.value) : parseFloat(panelCoverageInput.value)); // keep in native units for display
      const panelsForCalc = areaForSquares / panelCoverage; // consistent ft² basis
      const panelsRounded = Math.ceil(panelsForCalc);

      const panelsPerBox = parseInt(panelPerBoxInput.value, 10);
      const boxes = panelsPerBox > 0 ? Math.ceil(panelsRounded / panelsPerBox) : NaN;

      // Costs
      const pricePerPanel = parseFloat(panelPriceInput.value) || 0;
      const laborRate = parseFloat(laborRateInput.value) || 0;
      const trimCost = parseFloat(trimPriceInput.value) || 0;

      const materialCost = pricePerPanel > 0 ? panelsRounded * pricePerPanel : NaN;
      const laborCost = laborRate > 0 ? squares * laborRate : NaN;
      const totalCost = (isNaN(materialCost) ? 0 : materialCost) +
                        (isNaN(laborCost) ? 0 : laborCost) +
                        (trimCost > 0 ? trimCost : 0);

      // Trim lengths
      const cornerCount = parseInt(cornerCountInput.value, 10) || 0;
      const cornerHeight = parseFloat(cornerLengthInput.value) || 0;
      const totalCornerLength = cornerCount * cornerHeight;
      const starterLength = parseFloat(starterLengthInput.value) || 0;

      // Update results
      resWallArea.textContent = formatArea(wallArea, unitSystem);
      resOpeningsArea.textContent = ignoreOpeningsCheckbox.checked ? '0' : formatArea(openingsArea, unitSystem);
      resNetArea.textContent = formatArea(netArea, unitSystem);
      resAdjustedArea.textContent = formatArea(adjustedArea, unitSystem);
      resSquares.textContent = isNaN(squares) ? '–' : squares.toFixed(2) + ' squares';
      resPanels.textContent = isNaN(panelsRounded) ? '–' : panelsRounded + ' panels (approx.)';
      resBoxes.textContent = isNaN(boxes) ? '–' : boxes + ' boxes (approx.)';

      resMaterialCost.textContent = formatCurrency(materialCost);
      resLaborCost.textContent = formatCurrency(laborCost);
      resTrimCost.textContent = trimCost > 0 ? formatCurrency(trimCost) : '–';
      resTotalCost.textContent = totalCost > 0 ? formatCurrency(totalCost) : '–';

      resCornerLength.textContent = totalCornerLength > 0 ? formatLength(totalCornerLength, unitSystem) : '–';
      resStarterLength.textContent = starterLength > 0 ? formatLength(starterLength, unitSystem) : '–';
    }

    calculateBtn.addEventListener('click', calculate);

    resetBtn.addEventListener('click', () => {
      // Clear containers
      wallsContainer.innerHTML = '';
      openingsContainer.innerHTML = '';
      wallIndex = 0;
      openingIndex = 0;
      addInitialRows();

      // Reset inputs
      document.querySelector('input[name="unitSystem"][value="imperial"]').checked = true;
      sidingTypeSelect.value = 'vinyl';
      updateSidingTypeDefaults();
      wasteInput.value = 10;
      panelPriceInput.value = 0;
      laborRateInput.value = 0;
      panelPerBoxInput.value = 10;
      ignoreOpeningsCheckbox.checked = false;

      cornerCountInput.value = 4;
      cornerLengthInput.value = 8;
      starterLengthInput.value = 0;
      trimPriceInput.value = 0;

      errorMsg.textContent = '';
      resetResults();
      updateUnitsDisplay();
    });

    // Initialize
    addInitialRows();
    updateSidingTypeDefaults();
  });
</script>

