    <script>
      // Weight & Balance calculator logic
      (function () {
        const rowsBody = document.getElementById("wb-rows-body");
        const unitSelect = document.getElementById("wb-unit-select");

        const totalWeightEl = document.getElementById("wb-total-weight");
        const totalMomentEl = document.getElementById("wb-total-moment");
        const totalCgEl = document.getElementById("wb-total-cg");

        const summaryWeightEl = document.getElementById("wb-summary-weight");
        const summaryWeightUnitEl = document.getElementById(
          "wb-summary-weight-unit"
        );
        const summaryWeightUnit2El = document.getElementById(
          "wb-summary-weight-unit-2"
        );
        const summaryCgEl = document.getElementById("wb-summary-cg");
        const summaryArmUnitEl = document.getElementById("wb-summary-arm-unit");
        const summaryFuelWeightEl = document.getElementById(
          "wb-summary-fuel-weight"
        );

        const weightUnitLabel = document.getElementById("wb-weight-unit-label");
        const armUnitLabel = document.getElementById("wb-arm-unit-label");

        const statusSummaryEl = document.getElementById("wb-status-summary");
        const weightStatusEl = document.getElementById(
          "wb-summary-weight-status"
        );
        const cgStatusEl = document.getElementById("wb-summary-cg-status");

        const maxRampEl = document.getElementById("wb-max-ramp");
        const maxTakeoffEl = document.getElementById("wb-max-takeoff");
        const maxLandingEl = document.getElementById("wb-max-landing");
        const cgFwdEl = document.getElementById("wb-cg-fwd");
        const cgAftEl = document.getElementById("wb-cg-aft");

        const envelopeRangeEl = document.getElementById("wb-envelope-range");
        const envelopeCgMarkerEl = document.getElementById(
          "wb-envelope-cg-marker"
        );
        const envelopeFwdLabelEl = document.getElementById(
          "wb-envelope-fwd-label"
        );
        const envelopeAftLabelEl = document.getElementById(
          "wb-envelope-aft-label"
        );
        const envelopeCgLabelEl = document.getElementById(
          "wb-envelope-cg-label"
        );

        const addRowBtn = document.getElementById("wb-add-row-btn");
        const loadC172Btn = document.getElementById("wb-load-c172-btn");
        const resetBtn = document.getElementById("wb-reset-btn");

        const FUEL_DENSITY = {
          imperial: 6.0, // lb/gal (Avgas approx)
          metric: 0.72, // kg/L (approx)
        };

        function createRow(data = {}) {
          const tr = document.createElement("tr");

          tr.innerHTML = `
        <td class="px-2 py-1">
          <input type="text" class="w-full border border-gray-200 rounded px-2 py-1 text-xs" value="${
            data.name || ""
          }" placeholder="Station name">
        </td>
        <td class="px-2 py-1 text-right">
          <input type="number" step="0.1" class="w-24 border border-gray-200 rounded px-2 py-1 text-right text-xs wb-weight-input" value="${
            data.weight != null ? data.weight : ""
          }">
        </td>
        <td class="px-2 py-1 text-right">
          <input type="number" step="0.01" class="w-24 border border-gray-200 rounded px-2 py-1 text-right text-xs wb-arm-input" value="${
            data.arm != null ? data.arm : ""
          }">
        </td>
        <td class="px-2 py-1 text-right">
          <span class="wb-moment">0</span>
        </td>
        <td class="px-2 py-1 text-center">
          <select class="border border-gray-200 rounded px-1 py-1 text-xs wb-type-select">
            <option value="payload"${
              data.type === "payload" ? " selected" : ""
            }>Payload</option>
            <option value="fuel"${
              data.type === "fuel" ? " selected" : ""
            }>Fuel</option>
            <option value="empty"${
              data.type === "empty" ? " selected" : ""
            }>Empty</option>
          </select>
        </td>
        <td class="px-2 py-1 text-center">
          <button type="button" class="text-red-600 hover:text-red-800 text-xs wb-remove-row">&times;</button>
        </td>
      `;

          rowsBody.appendChild(tr);
        }

        function recalc() {
          let totalWeight = 0;
          let totalMoment = 0;
          let fuelWeight = 0;

          const rows = rowsBody.querySelectorAll("tr");
          rows.forEach((row) => {
            const weightInput = row.querySelector(".wb-weight-input");
            const armInput = row.querySelector(".wb-arm-input");
            const momentSpan = row.querySelector(".wb-moment");
            const typeSelect = row.querySelector(".wb-type-select");

            const weight = parseFloat(weightInput.value) || 0;
            const arm = parseFloat(armInput.value) || 0;
            const moment = weight * arm;

            momentSpan.textContent = moment.toFixed(1);

            totalWeight += weight;
            totalMoment += moment;

            if (typeSelect.value === "fuel") {
              fuelWeight += weight;
            }
          });

          totalWeightEl.textContent = totalWeight.toFixed(1);
          totalMomentEl.textContent = totalMoment.toFixed(1);

          let cg = null;
          if (totalWeight > 0) {
            cg = totalMoment / totalWeight;
            totalCgEl.textContent = cg.toFixed(2);
            summaryCgEl.textContent = cg.toFixed(2);
            envelopeCgLabelEl.textContent = "CG: " + cg.toFixed(2);
          } else {
            totalCgEl.textContent = "—";
            summaryCgEl.textContent = "—";
            envelopeCgLabelEl.textContent = "CG: —";
          }

          summaryWeightEl.textContent = totalWeight.toFixed(1);
          summaryFuelWeightEl.textContent = fuelWeight.toFixed(1);

          // Fuel volume estimate
          const unit = unitSelect.value;
          const density = FUEL_DENSITY[unit];
          let volume = 0;
          if (density > 0) {
            volume = fuelWeight / density;
          }
          const fuelVolumeEl = document.getElementById(
            "wb-summary-fuel-volume"
          );
          if (unit === "imperial") {
            fuelVolumeEl.textContent = volume.toFixed(1) + " gal";
          } else {
            fuelVolumeEl.textContent = volume.toFixed(1) + " L";
          }

          updateStatuses(totalWeight, cg);
          updateEnvelope(cg);
        }

        function updateStatuses(totalWeight, cg) {
          const maxRamp = parseFloat(maxRampEl.value) || null;
          const maxTO = parseFloat(maxTakeoffEl.value) || null;
          const maxLDG = parseFloat(maxLandingEl.value) || null;
          const cgFwd = parseFloat(cgFwdEl.value) || null;
          const cgAft = parseFloat(cgAftEl.value) || null;

          // Weight status
          if (!maxRamp && !maxTO && !maxLDG) {
            weightStatusEl.textContent = "No limits set";
            weightStatusEl.className =
              "mt-1 inline-flex items-center px-2 py-0.5 rounded-full text-xs wb-badge-ok";
          } else {
            let within = true;
            let msg = "Within limits";

            if (maxRamp && totalWeight > maxRamp + 1e-6) {
              within = false;
              msg = "Above max ramp";
            }
            if (maxTO && totalWeight > maxTO + 1e-6) {
              within = false;
              msg = "Above max takeoff";
            }
            if (maxLDG && totalWeight > maxLDG + 1e-6) {
              within = false;
              msg = "Above max landing";
            }

            weightStatusEl.textContent = msg;
            weightStatusEl.className =
              "mt-1 inline-flex items-center px-2 py-0.5 rounded-full text-xs " +
              (within ? "wb-badge-ok" : "wb-badge-bad");
          }

          // CG status
          if (cg == null || (!cgFwd && !cgAft)) {
            cgStatusEl.textContent = "Awaiting data";
            cgStatusEl.className =
              "mt-1 inline-flex items-center px-2 py-0.5 rounded-full text-xs wb-badge-warn";
          } else {
            let within = true;
            let msg = "Within CG limits";

            if (cgFwd && cg < cgFwd - 1e-6) {
              within = false;
              msg = "Forward of limit";
            }
            if (cgAft && cg > cgAft + 1e-6) {
              within = false;
              msg = "Aft of limit";
            }

            cgStatusEl.textContent = msg;
            cgStatusEl.className =
              "mt-1 inline-flex items-center px-2 py-0.5 rounded-full text-xs " +
              (within ? "wb-badge-ok" : "wb-badge-bad");
          }

          // Summary text
          let summary = "";
          if (totalWeight <= 0) {
            summary =
              "Enter weights and arms to see total weight, CG, and limit checks.";
          } else {
            summary = `Total weight ${totalWeight.toFixed(1)} ${
              summaryWeightUnitEl.textContent
            }`;
            if (cg != null) {
              summary += `, CG ${cg.toFixed(2)} ${
                summaryArmUnitEl.textContent
              }.`;
            } else {
              summary += ".";
            }
            if (
              weightStatusEl.textContent &&
              weightStatusEl.textContent !== "No limits set"
            ) {
              summary += " Weight status: " + weightStatusEl.textContent + ".";
            }
            if (
              cgStatusEl.textContent &&
              cgStatusEl.textContent !== "Awaiting data"
            ) {
              summary += " CG status: " + cgStatusEl.textContent + ".";
            }
          }
          statusSummaryEl.textContent = summary;
        }

        function updateEnvelope(cg) {
          const cgFwd = parseFloat(cgFwdEl.value);
          const cgAft = parseFloat(cgAftEl.value);

          if (isNaN(cgFwd) || isNaN(cgAft) || cgAft <= cgFwd) {
            envelopeRangeEl.style.left = "0%";
            envelopeRangeEl.style.right = "0%";
            envelopeCgMarkerEl.style.left = "50%";
            envelopeFwdLabelEl.textContent = "—";
            envelopeAftLabelEl.textContent = "—";
            return;
          }

          envelopeFwdLabelEl.textContent = cgFwd.toFixed(2);
          envelopeAftLabelEl.textContent = cgAft.toFixed(2);

          // Range bar
          const span = cgAft - cgFwd;
          const left = 0;
          const right = 0;
          envelopeRangeEl.style.left = left + "%";
          envelopeRangeEl.style.right = right + "%";

          // Marker
          if (cg == null) {
            envelopeCgMarkerEl.style.left = "50%";
          } else {
            let pos = (cg - cgFwd) / span;
            pos = Math.max(0, Math.min(1, pos));
            envelopeCgMarkerEl.style.left = pos * 100 + "%";
          }
        }

        function clearRows() {
          rowsBody.innerHTML = "";
        }

        function loadSampleC172() {
          clearRows();
          unitSelect.value = "imperial";
          handleUnitChange();

          // Example only – pilots must use their own aircraft data
          maxRampEl.value = 2558;
          maxTakeoffEl.value = 2550;
          maxLandingEl.value = 2550;
          cgFwdEl.value = 35.0;
          cgAftEl.value = 47.3;

          const sampleRows = [
            { name: "Basic Empty", weight: 1663, arm: 39.5, type: "empty" },
            {
              name: "Pilot / Front Left",
              weight: 170,
              arm: 37.0,
              type: "payload",
            },
            { name: "Front Right", weight: 170, arm: 37.0, type: "payload" },
            { name: "Rear Seats", weight: 0, arm: 73.0, type: "payload" },
            { name: "Baggage Area 1", weight: 20, arm: 95.0, type: "payload" },
            { name: "Fuel (usable)", weight: 240, arm: 48.0, type: "fuel" },
          ];
          sampleRows.forEach((r) => createRow(r));
          recalc();
        }

        function handleUnitChange() {
          const unit = unitSelect.value;
          if (unit === "imperial") {
            weightUnitLabel.textContent = "lb";
            armUnitLabel.textContent = "in";
            summaryWeightUnitEl.textContent = "lb";
            summaryWeightUnit2El.textContent = "lb";
            summaryArmUnitEl.textContent = "in";
          } else {
            weightUnitLabel.textContent = "kg";
            armUnitLabel.textContent = "cm";
            summaryWeightUnitEl.textContent = "kg";
            summaryWeightUnit2El.textContent = "kg";
            summaryArmUnitEl.textContent = "cm";
          }
          recalc();
        }

        // Event listeners
        rowsBody.addEventListener("input", function (e) {
          if (
            e.target.classList.contains("wb-weight-input") ||
            e.target.classList.contains("wb-arm-input") ||
            e.target.classList.contains("wb-type-select")
          ) {
            recalc();
          }
        });

        rowsBody.addEventListener("click", function (e) {
          if (e.target.classList.contains("wb-remove-row")) {
            const row = e.target.closest("tr");
            if (row) {
              row.remove();
              recalc();
            }
          }
        });

        [maxRampEl, maxTakeoffEl, maxLandingEl, cgFwdEl, cgAftEl].forEach(
          (el) => {
            el.addEventListener("input", recalc);
          }
        );

        unitSelect.addEventListener("change", handleUnitChange);

        addRowBtn.addEventListener("click", function () {
          createRow({ type: "payload" });
        });

        loadC172Btn.addEventListener("click", loadSampleC172);

        resetBtn.addEventListener("click", function () {
          clearRows();
          maxRampEl.value = "";
          maxTakeoffEl.value = "";
          maxLandingEl.value = "";
          cgFwdEl.value = "";
          cgAftEl.value = "";
          recalc();
        });

        // Initialize with a few blank rows
        createRow({ name: "Basic Empty", type: "empty" });
        createRow({ name: "Pilot / Front Left", type: "payload" });
        createRow({ name: "Fuel", type: "fuel" });
        recalc();
      })();

      // Mobile menu script (from template)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const expanded = this.getAttribute("aria-expanded") === "true";
          this.setAttribute("aria-expanded", String(!expanded));
          menu.classList.toggle("hidden");
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
