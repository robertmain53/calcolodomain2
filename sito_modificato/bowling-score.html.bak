    <script>
      // Bowling score calculator logic
      const FRAMES = 10;
      const MAX_PINS = 10;

      const playersBody = document.getElementById("players-body");
      const playerCountSelect = document.getElementById("player-count");
      const validationMessage = document.getElementById("validation-message");
      const summaryContainer = document.getElementById("summary-container");
      const maxScoreLabel = document.getElementById("max-score-label");

      function createPlayerRow(index) {
        const tr = document.createElement("tr");
        tr.dataset.playerIndex = index;
        tr.className = index % 2 === 0 ? "bg-white" : "bg-gray-50";

        const nameCell = document.createElement("td");
        nameCell.className = "px-2 py-2 border-t border-gray-200 align-top";
        const nameInput = document.createElement("input");
        nameInput.type = "text";
        nameInput.value = `Player ${index + 1}`;
        nameInput.className =
          "w-24 md:w-28 border border-gray-300 rounded px-1 py-0.5 text-xs";
        nameInput.dataset.role = "player-name";
        nameCell.appendChild(nameInput);
        tr.appendChild(nameCell);

        // Frames 1-9: 2 rolls each
        for (let f = 0; f < FRAMES - 1; f++) {
          for (let r = 0; r < 2; r++) {
            const td = document.createElement("td");
            td.className =
              "px-1 py-1 border-t border-l border-gray-200 text-center frame-cell";
            const input = document.createElement("input");
            input.type = "text";
            input.maxLength = 1;
            input.className =
              "roll-input border border-gray-300 rounded text-xs py-0.5";
            input.dataset.player = index;
            input.dataset.frame = f;
            input.dataset.roll = r;
            input.addEventListener("input", onRollInput);
            td.appendChild(input);
            tr.appendChild(td);
          }
        }

        // Frame 10: 3 rolls
        for (let r = 0; r < 3; r++) {
          const td = document.createElement("td");
          td.className =
            "px-1 py-1 border-t border-l border-gray-200 text-center frame-cell";
          const input = document.createElement("input");
          input.type = "text";
          input.maxLength = 1;
          input.className =
            "roll-input border border-gray-300 rounded text-xs py-0.5";
          input.dataset.player = index;
          input.dataset.frame = FRAMES - 1;
          input.dataset.roll = r;
          input.addEventListener("input", onRollInput);
          td.appendChild(input);
          tr.appendChild(td);
        }

        // Total cell
        const totalTd = document.createElement("td");
        totalTd.className =
          "px-2 py-1 border-t border-l border-gray-200 text-center";
        const totalSpan = document.createElement("span");
        totalSpan.dataset.role = "total-score";
        totalSpan.className = "font-semibold";
        totalSpan.textContent = "0";
        totalTd.appendChild(totalSpan);
        tr.appendChild(totalTd);

        return tr;
      }

      function buildTable() {
        playersBody.innerHTML = "";
        const count = parseInt(playerCountSelect.value, 10);
        for (let i = 0; i < count; i++) {
          playersBody.appendChild(createPlayerRow(i));
        }
        updateScores();
      }

      function parseRollChar(ch) {
        if (!ch) return null;
        ch = ch.toUpperCase();
        if (ch === "X") return MAX_PINS;
        if (ch === "-") return 0;
        if (ch === "/") return "/";
        const n = parseInt(ch, 10);
        if (isNaN(n) || n < 0 || n > 9) return null;
        return n;
      }

      function getPlayerRollInputs(playerIndex) {
        return Array.from(
          playersBody.querySelectorAll(`input[data-player="${playerIndex}"]`)
        );
      }

      function getRollsForPlayer(playerIndex) {
        const rolls = [];
        for (let f = 0; f < FRAMES; f++) {
          rolls[f] = [];
        }
        const inputs = getPlayerRollInputs(playerIndex);
        inputs.forEach((input) => {
          const f = parseInt(input.dataset.frame, 10);
          const r = parseInt(input.dataset.roll, 10);
          const val = input.value.trim();
          rolls[f][r] = val === "" ? null : val;
        });
        return rolls;
      }

      function validateAndConvert(rolls) {
        // Returns { frames: [{rolls:[n1,n2,n3], strike, spare}], error }
        const frames = [];
        for (let f = 0; f < FRAMES; f++) {
          const raw = rolls[f] || [];
          const frame = { rolls: [], strike: false, spare: false };
          if (f < FRAMES - 1) {
            const r1 = parseRollChar(raw[0] || "");
            const r2Raw = raw[1] || "";
            if (r1 === null && raw[0])
              return { error: `Invalid entry in frame ${f + 1}, roll 1.` };
            if (r1 === MAX_PINS) {
              frame.rolls = [MAX_PINS];
              frame.strike = true;
            } else {
              const r2 = parseRollChar(r2Raw);
              if (r2Raw === "/" && (r1 === null || r1 < 0 || r1 >= MAX_PINS)) {
                return {
                  error: `Spare in frame ${
                    f + 1
                  } requires a valid first roll less than 10.`,
                };
              }
              if (r2 === null && r2Raw)
                return { error: `Invalid entry in frame ${f + 1}, roll 2.` };
              if (r2 === "/") {
                frame.rolls = [r1 ?? 0, MAX_PINS - (r1 ?? 0)];
                frame.spare = true;
              } else {
                const v1 = r1 ?? 0;
                const v2 = r2 ?? 0;
                if (v1 + v2 > MAX_PINS) {
                  return {
                    error: `Total pins in frame ${f + 1} cannot exceed 10.`,
                  };
                }
                frame.rolls = [v1, v2];
              }
            }
          } else {
            // 10th frame
            const r1 = parseRollChar(raw[0] || "");
            if (r1 === null && raw[0])
              return { error: "Invalid entry in 10th frame, roll 1." };
            const r2Raw = raw[1] || "";
            const r3Raw = raw[2] || "";
            let r2 = parseRollChar(r2Raw);
            let r3 = parseRollChar(r3Raw);
            if (r2 === null && r2Raw)
              return { error: "Invalid entry in 10th frame, roll 2." };
            if (r3 === null && r3Raw)
              return { error: "Invalid entry in 10th frame, roll 3." };

            if (r1 === MAX_PINS) {
              // Strike in 10th
              frame.rolls.push(MAX_PINS);
              // r2
              if (r2Raw === "/") {
                return {
                  error:
                    'Cannot use "/" as second roll after a strike in the 10th frame.',
                };
              }
              if (r2 != null) {
                frame.rolls.push(r2);
              }
              // r3
              if (r3Raw === "/") {
                if (r2 == null || r2 >= MAX_PINS) {
                  return {
                    error:
                      "Spare in 10th frame requires a valid second roll less than 10.",
                  };
                }
                frame.rolls.push(MAX_PINS - r2);
                frame.spare = true;
              } else if (r3 != null) {
                frame.rolls.push(r3);
              }
              frame.strike = true;
            } else {
              const v1 = r1 ?? 0;
              if (r2Raw === "/") {
                if (r1 == null || r1 >= MAX_PINS) {
                  return {
                    error:
                      "Spare in 10th frame requires a valid first roll less than 10.",
                  };
                }
                frame.rolls = [v1, MAX_PINS - v1];
                frame.spare = true;
                // third roll allowed
                if (r3Raw === "/") {
                  return {
                    error: 'Cannot use "/" as third roll in the 10th frame.',
                  };
                }
                if (r3 != null) frame.rolls.push(r3);
              } else {
                const v2 = r2 ?? 0;
                if (v1 + v2 > MAX_PINS) {
                  return {
                    error:
                      "Total pins in 10th frame (first two rolls) cannot exceed 10 unless first roll is a strike.",
                  };
                }
                frame.rolls = [v1, v2];
                // no bonus roll unless spare or strike
                if (r3Raw) {
                  return {
                    error:
                      "Third roll in 10th frame is only allowed after a strike or spare.",
                  };
                }
              }
            }
          }
          frames.push(frame);
        }
        return { frames, error: null };
      }

      function flattenRolls(frames) {
        const arr = [];
        frames.forEach((frame, idx) => {
          frame.rolls.forEach((r, i) => {
            arr.push({ pins: r, frameIndex: idx, rollIndex: i });
          });
        });
        return arr;
      }

      function calculateScores(frames) {
        const flat = flattenRolls(frames);
        const frameScores = [];
        let total = 0;

        function getNextRolls(startFlatIndex, count) {
          const res = [];
          for (
            let i = startFlatIndex;
            i < flat.length && res.length < count;
            i++
          ) {
            res.push(flat[i].pins);
          }
          return res;
        }

        let flatIndex = 0;
        for (let f = 0; f < FRAMES; f++) {
          const frame = frames[f];
          let frameScore = 0;

          if (f < FRAMES - 1) {
            if (frame.strike) {
              const bonus = getNextRolls(flatIndex + 1, 2);
              if (bonus.length < 2) {
                frameScore = null; // incomplete
              } else {
                frameScore = MAX_PINS + bonus[0] + bonus[1];
              }
              flatIndex += 1;
            } else if (frame.spare) {
              const bonus = getNextRolls(flatIndex + 2, 1);
              if (bonus.length < 1) {
                frameScore = null;
              } else {
                frameScore = MAX_PINS + bonus[0];
              }
              flatIndex += 2;
            } else {
              frameScore = frame.rolls[0] + frame.rolls[1];
              flatIndex += 2;
            }
          } else {
            // 10th frame: just sum its rolls
            frameScore = frame.rolls.reduce((a, b) => a + b, 0);
          }

          if (frameScore != null) {
            total += frameScore;
            frameScores[f] = total;
          } else {
            frameScores[f] = null;
          }
        }

        return { frameScores, total };
      }

      function updateScores() {
        validationMessage.textContent = "";
        summaryContainer.innerHTML = "";
        const playerRows = Array.from(playersBody.querySelectorAll("tr"));
        let globalMax = 0;

        playerRows.forEach((row, idx) => {
          const nameInput = row.querySelector('input[data-role="player-name"]');
          const totalSpan = row.querySelector('span[data-role="total-score"]');
          const rolls = getRollsForPlayer(idx);
          const { frames, error } = validateAndConvert(rolls);
          if (error) {
            validationMessage.textContent = error;
            row.classList.add("bg-red-50");
            totalSpan.textContent = "—";
            return;
          } else {
            row.classList.remove("bg-red-50");
          }

          const { frameScores, total } = calculateScores(frames);
          totalSpan.textContent = isNaN(total) ? "0" : total;
          if (total > globalMax) globalMax = total;

          // Build summary card
          const completedFrames = frameScores.filter((s) => s != null).length;
          const avgPerFrame = completedFrames
            ? (total / completedFrames).toFixed(1)
            : "0.0";

          const card = document.createElement("div");
          card.className =
            "border border-gray-200 rounded-lg p-3 text-sm bg-white";
          card.innerHTML = `
        <div class="flex justify-between items-center mb-1">
          <div class="font-semibold">${
            nameInput.value || "Player " + (idx + 1)
          }</div>
          <div class="text-gray-700">Total: <span class="font-bold">${
            isNaN(total) ? 0 : total
          }</span></div>
        </div>
        <div class="text-xs text-gray-600 mb-1">
          Completed frames: ${completedFrames}/10 · Avg per completed frame: ${avgPerFrame}
        </div>
        <div class="text-xs text-gray-500">
          Best frame so far: ${
            frameScores.reduce(
              (max, s, i) =>
                s != null && (max.score == null || s > max.score)
                  ? { score: s, frame: i + 1 }
                  : max,
              { score: null, frame: null }
            ).frame || "—"
          }
        </div>
      `;
          summaryContainer.appendChild(card);
        });

        maxScoreLabel.textContent = globalMax || 300;
      }

      function onRollInput(e) {
        const input = e.target;
        let v = input.value.toUpperCase();
        if (v.length > 1) v = v.slice(0, 1);
        // Allow digits, X, /, -
        if (!v.match(/^[0-9X\/-]?$/)) {
          v = "";
        }
        input.value = v;
        updateScores();
      }

      playerCountSelect.addEventListener("change", buildTable);

      document.getElementById("reset-btn").addEventListener("click", () => {
        Array.from(playersBody.querySelectorAll('input[type="text"]')).forEach(
          (inp) => {
            if (inp.dataset.role !== "player-name") inp.value = "";
          }
        );
        updateScores();
      });

      document
        .getElementById("perfect-game-btn")
        .addEventListener("click", () => {
          const rows = Array.from(playersBody.querySelectorAll("tr"));
          rows.forEach((row, idx) => {
            const inputs = getPlayerRollInputs(idx);
            inputs.forEach((inp) => (inp.value = ""));
            // Frames 1-9: first roll X
            for (let f = 0; f < FRAMES - 1; f++) {
              const r1 = playersBody.querySelector(
                `input[data-player="${idx}"][data-frame="${f}"][data-roll="0"]`
              );
              if (r1) r1.value = "X";
            }
            // Frame 10: X X X
            ["0", "1", "2"].forEach((r) => {
              const inp = playersBody.querySelector(
                `input[data-player="${idx}"][data-frame="9"][data-roll="${r}"]`
              );
              if (inp) inp.value = "X";
            });
          });
          updateScores();
        });

      document.getElementById("all-nines-btn").addEventListener("click", () => {
        const rows = Array.from(playersBody.querySelectorAll("tr"));
        rows.forEach((row, idx) => {
          const inputs = getPlayerRollInputs(idx);
          inputs.forEach((inp) => (inp.value = ""));
          // Frames 1-9: 9 and gutter
          for (let f = 0; f < FRAMES - 1; f++) {
            const r1 = playersBody.querySelector(
              `input[data-player="${idx}"][data-frame="${f}"][data-roll="0"]`
            );
            const r2 = playersBody.querySelector(
              `input[data-player="${idx}"][data-frame="${f}"][data-roll="1"]`
            );
            if (r1) r1.value = "9";
            if (r2) r2.value = "-";
          }
          // Frame 10: 9 and gutter
          const r1 = playersBody.querySelector(
            `input[data-player="${idx}"][data-frame="9"][data-roll="0"]`
          );
          const r2 = playersBody.querySelector(
            `input[data-player="${idx}"][data-frame="9"][data-roll="1"]`
          );
          const r3 = playersBody.querySelector(
            `input[data-player="${idx}"][data-frame="9"][data-roll="2"]`
          );
          if (r1) r1.value = "9";
          if (r2) r2.value = "-";
          if (r3) r3.value = "";
        });
        updateScores();
      });

      // Initialize
      buildTable();
    </script>

