    <script>
      // MODELED Hawaii tax parameters (approximate, for illustration only)
      const HI_PARAMS = {
        2024: {
          standardDeduction: {
            single: 2200,
            mfs: 2200,
            hoh: 3200,
            mfj: 4400,
          },
          exemptionAmount: 1144,
          brackets: {
            // thresholds are per filing status; values are illustrative
            single: [
              { upTo: 2400, rate: 0.014 },
              { upTo: 4800, rate: 0.032 },
              { upTo: 9600, rate: 0.055 },
              { upTo: 14400, rate: 0.064 },
              { upTo: 19200, rate: 0.068 },
              { upTo: 24000, rate: 0.072 },
              { upTo: 36000, rate: 0.076 },
              { upTo: 48000, rate: 0.079 },
              { upTo: 150000, rate: 0.0825 },
              { upTo: 175000, rate: 0.09 },
              { upTo: 200000, rate: 0.1 },
              { upTo: Infinity, rate: 0.11 },
            ],
            mfj: [
              { upTo: 4800, rate: 0.014 },
              { upTo: 9600, rate: 0.032 },
              { upTo: 19200, rate: 0.055 },
              { upTo: 28800, rate: 0.064 },
              { upTo: 38400, rate: 0.068 },
              { upTo: 48000, rate: 0.072 },
              { upTo: 72000, rate: 0.076 },
              { upTo: 96000, rate: 0.079 },
              { upTo: 300000, rate: 0.0825 },
              { upTo: 350000, rate: 0.09 },
              { upTo: 400000, rate: 0.1 },
              { upTo: Infinity, rate: 0.11 },
            ],
            mfs: [
              { upTo: 2400, rate: 0.014 },
              { upTo: 4800, rate: 0.032 },
              { upTo: 9600, rate: 0.055 },
              { upTo: 14400, rate: 0.064 },
              { upTo: 19200, rate: 0.068 },
              { upTo: 24000, rate: 0.072 },
              { upTo: 36000, rate: 0.076 },
              { upTo: 48000, rate: 0.079 },
              { upTo: 150000, rate: 0.0825 },
              { upTo: 175000, rate: 0.09 },
              { upTo: 200000, rate: 0.1 },
              { upTo: Infinity, rate: 0.11 },
            ],
            hoh: [
              { upTo: 3600, rate: 0.014 },
              { upTo: 7200, rate: 0.032 },
              { upTo: 14400, rate: 0.055 },
              { upTo: 21600, rate: 0.064 },
              { upTo: 28800, rate: 0.068 },
              { upTo: 36000, rate: 0.072 },
              { upTo: 54000, rate: 0.076 },
              { upTo: 72000, rate: 0.079 },
              { upTo: 225000, rate: 0.0825 },
              { upTo: 262500, rate: 0.09 },
              { upTo: 300000, rate: 0.1 },
              { upTo: Infinity, rate: 0.11 },
            ],
          },
        },
        2023: {
          // For now, reuse 2024 parameters as an approximation
          standardDeduction: {
            single: 2200,
            mfs: 2200,
            hoh: 3200,
            mfj: 4400,
          },
          exemptionAmount: 1144,
          brackets: null, // will be filled from 2024
        },
      };
      HI_PARAMS[2023].brackets = HI_PARAMS[2024].brackets;

      function formatCurrency(value) {
        if (!isFinite(value)) return "$0";
        return value.toLocaleString("en-US", {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 0,
        });
      }

      function formatPercent(value) {
        if (!isFinite(value)) return "0%";
        return (value * 100).toFixed(2).replace(/\.00$/, "") + "%";
      }

      function calculateHawaiiTax(taxableIncome, year, status) {
        const params = HI_PARAMS[year];
        const brackets = params.brackets[status];
        let remaining = Math.max(0, taxableIncome);
        let lastLimit = 0;
        let totalTax = 0;
        const breakdown = [];

        for (let i = 0; i < brackets.length; i++) {
          const bracket = brackets[i];
          const upper = bracket.upTo;
          const bracketRange = upper - lastLimit;
          const taxableHere = Math.max(0, Math.min(remaining, bracketRange));
          const taxHere = taxableHere * bracket.rate;

          if (taxableHere > 0) {
            breakdown.push({
              index: i + 1,
              from: lastLimit,
              to: upper,
              rate: bracket.rate,
              amount: taxableHere,
              tax: taxHere,
            });
          }

          totalTax += taxHere;
          remaining -= taxableHere;
          lastLimit = upper;

          if (remaining <= 0) break;
        }

        const marginalRate = breakdown.length
          ? breakdown[breakdown.length - 1].rate
          : 0;

        return { totalTax, breakdown, marginalRate };
      }

      function recalc() {
        const status = document.getElementById("filing-status").value;
        const year = parseInt(document.getElementById("tax-year").value, 10);
        const incomeInput =
          parseFloat(document.getElementById("gross-income").value) || 0;
        const deductionType = document.querySelector(
          "input.deduction-type:checked"
        ).value;
        const dependents = Math.max(
          0,
          parseInt(document.getElementById("dependents").value, 10) || 0
        );

        const params = HI_PARAMS[year];
        let deduction = 0;
        if (deductionType === "standard") {
          deduction = params.standardDeduction[status] || 0;
        } else {
          deduction = Math.max(
            0,
            parseFloat(document.getElementById("itemized-amount").value) || 0
          );
        }

        const exemptions = dependents * params.exemptionAmount;
        const taxableIncome = Math.max(0, incomeInput - deduction - exemptions);

        const { totalTax, breakdown, marginalRate } = calculateHawaiiTax(
          taxableIncome,
          year,
          status
        );
        const effectiveRate = taxableIncome > 0 ? totalTax / taxableIncome : 0;
        const takeHome = incomeInput - totalTax;

        // Update results
        document.getElementById("result-year").textContent = year.toString();
        document.getElementById("result-taxable").textContent =
          formatCurrency(taxableIncome);
        document.getElementById("result-tax").textContent =
          formatCurrency(totalTax);
        document.getElementById("result-effective").textContent =
          formatPercent(effectiveRate);
        document.getElementById("result-marginal").textContent = marginalRate
          ? formatPercent(marginalRate)
          : "0%";
        document.getElementById("result-takehome").textContent =
          formatCurrency(takeHome);

        // Update bracket table
        const tbody = document.getElementById("bracket-body");
        tbody.innerHTML = "";
        if (!breakdown.length) {
          const tr = document.createElement("tr");
          tr.innerHTML =
            '<td colspan="4" class="px-3 py-2 text-sm text-gray-500">No taxable income in any bracket.</td>';
          tbody.appendChild(tr);
          return;
        }

        breakdown.forEach((row) => {
          const tr = document.createElement("tr");
          tr.className = "border-t border-gray-100";
          const rangeLabel =
            row.to === Infinity
              ? formatCurrency(row.from) + " and above"
              : `${formatCurrency(row.from)} – ${formatCurrency(row.to)}`;
          tr.innerHTML = `
        <td class="px-3 py-2">Bracket ${row.index}</td>
        <td class="px-3 py-2">${rangeLabel}</td>
        <td class="px-3 py-2">${formatPercent(row.rate)}</td>
        <td class="px-3 py-2">${formatCurrency(row.tax)}</td>
      `;
          tbody.appendChild(tr);
        });
      }

      document.addEventListener("DOMContentLoaded", () => {
        const itemizedWrapper = document.getElementById("itemized-wrapper");
        document.querySelectorAll("input.deduction-type").forEach((radio) => {
          radio.addEventListener("change", () => {
            if (radio.value === "itemized" && radio.checked) {
              itemizedWrapper.classList.remove("hidden");
            } else if (radio.value === "standard" && radio.checked) {
              itemizedWrapper.classList.add("hidden");
            }
            recalc();
          });
        });

        [
          "filing-status",
          "tax-year",
          "gross-income",
          "itemized-amount",
          "dependents",
        ].forEach((id) => {
          const el = document.getElementById(id);
          if (el) el.addEventListener("input", () => recalc());
          if (el) el.addEventListener("change", () => recalc());
        });

        document
          .getElementById("calculate-btn")
          .addEventListener("click", (e) => {
            e.preventDefault();
            recalc();
            document.getElementById("calculate-btn").blur();
          });

        recalc();
      });
    </script>

    <script>
      window.MathJax = {
