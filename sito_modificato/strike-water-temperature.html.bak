    <script>
      // Strike water calculator logic
      (function () {
        const unitsEl = document.getElementById("units");
        const mashMethodEl = document.getElementById("mashMethod");
        const grainWeightEl = document.getElementById("grainWeight");
        const grainTempEl = document.getElementById("grainTemp");
        const targetMashTempEl = document.getElementById("targetMashTemp");
        const mashThicknessEl = document.getElementById("mashThickness");
        const tunMassEl = document.getElementById("tunMass");
        const tunTempEl = document.getElementById("tunTemp");
        const absorptionEl = document.getElementById("absorption");
        const strikeVolumeOverrideEl = document.getElementById(
          "strikeVolumeOverride"
        );

        const grainWeightUnitEl = document.getElementById("grainWeightUnit");
        const grainTempUnitEl = document.getElementById("grainTempUnit");
        const targetMashTempUnitEl =
          document.getElementById("targetMashTempUnit");
        const mashThicknessUnitEl =
          document.getElementById("mashThicknessUnit");
        const tunMassUnitEl = document.getElementById("tunMassUnit");
        const absorptionUnitEl = document.getElementById("absorptionUnit");
        const strikeVolumeOverrideUnitEl = document.getElementById(
          "strikeVolumeOverrideUnit"
        );

        const calculateBtn = document.getElementById("calculateBtn");
        const resultsEl = document.getElementById("results");
        const strikeTempResultEl = document.getElementById("strikeTempResult");
        const strikeVolumeResultEl =
          document.getElementById("strikeVolumeResult");
        const mashThicknessResultEl = document.getElementById(
          "mashThicknessResult"
        );
        const postAbsorptionVolumeResultEl = document.getElementById(
          "postAbsorptionVolumeResult"
        );
        const summaryResultEl = document.getElementById("summaryResult");
        const warningResultEl = document.getElementById("warningResult");

        function updateUnits() {
          const isUS = unitsEl.value === "us";
          if (isUS) {
            grainWeightUnitEl.textContent = "lb";
            grainTempUnitEl.textContent = "°F";
            targetMashTempUnitEl.textContent = "°F";
            mashThicknessUnitEl.textContent = "qt/lb";
            tunMassUnitEl.textContent = "lb";
            absorptionUnitEl.textContent = "gal/lb";
            strikeVolumeOverrideUnitEl.textContent = "gal";

            if (!strikeVolumeOverrideEl.value) {
              mashThicknessEl.placeholder = "1.25–1.75";
            }
          } else {
            grainWeightUnitEl.textContent = "kg";
            grainTempUnitEl.textContent = "°C";
            targetMashTempUnitEl.textContent = "°C";
            mashThicknessUnitEl.textContent = "L/kg";
            tunMassUnitEl.textContent = "kg";
            absorptionUnitEl.textContent = "L/kg";
            strikeVolumeOverrideUnitEl.textContent = "L";

            if (!strikeVolumeOverrideEl.value) {
              mashThicknessEl.placeholder = "2.6–3.7";
            }
          }
        }

        unitsEl.addEventListener("change", updateUnits);
        updateUnits();

        function toNumber(el) {
          const v = parseFloat(el.value);
          return isNaN(v) ? null : v;
        }

        calculateBtn.addEventListener("click", function () {
          const isUS = unitsEl.value === "us";

          const G = toNumber(grainWeightEl);
          const Tg = toNumber(grainTempEl);
          const Tm = toNumber(targetMashTempEl);
          let R = toNumber(mashThicknessEl);
          let Mtun = toNumber(tunMassEl) || 0;
          const Ttun = toNumber(tunTempEl);
          let absorption = toNumber(absorptionEl);
          const Voverride = toNumber(strikeVolumeOverrideEl);

          warningResultEl.classList.add("hidden");
          warningResultEl.textContent = "";

          if (G === null || G <= 0 || Tg === null || Tm === null) {
            alert("Please enter valid grain weight and temperatures.");
            return;
          }

          // Specific heats (relative, unitless scaling)
          const cw = 1.0; // water
          const cg = 0.38; // grain (approx)
          const ctun = 0.3; // tun (approx, plastic/metal mix)

          // Convert volumes and ratios to consistent mass basis
          // For US: 1 qt water ≈ 2.086 lb, 1 gal ≈ 8.34 lb
          // For metric: 1 L water ≈ 1 kg
          let Mw; // mass of water
          let Vw; // volume of water
          let effectiveR; // water-to-grain ratio in qt/lb or L/kg

          if (Voverride && Voverride > 0) {
            // Use override volume
            if (isUS) {
              Vw = Voverride; // gal
              Mw = Vw * 8.34; // lb
              effectiveR = (Vw * 4) / G; // qt/lb
            } else {
              Vw = Voverride; // L
              Mw = Vw * 1.0; // kg
              effectiveR = Vw / G; // L/kg
            }
            R = effectiveR;
          } else {
            if (R === null || R <= 0) {
              alert("Please enter a valid mash thickness or strike volume.");
              return;
            }
            effectiveR = R;
            if (isUS) {
              const Vqt = effectiveR * G; // qt
              Vw = Vqt / 4.0; // gal
              Mw = Vw * 8.34; // lb
            } else {
              Vw = effectiveR * G; // L
              Mw = Vw * 1.0; // kg
            }
          }

          // If absorption not set, choose default
          if (absorption === null || absorption <= 0) {
            absorption = isUS ? 0.12 : 1.0;
          }

          // Convert tun mass to same mass units as grain
          // Already in lb or kg depending on units; treat as-is
          const Mt = Mtun;

          // Heat balance: Mw*cw*(Tw - Tm) = G*cg*(Tm - Tg) + Mt*ctun*(Tm - Ttun)
          // Solve for Tw:
          let numerator = G * cg * (Tm - Tg);
          if (Mt > 0 && Ttun !== null) {
            numerator += Mt * ctun * (Tm - Ttun);
          }
          const denominator = Mw * cw;
          const Tw = Tm + numerator / denominator;

          // Estimated volume after grain absorption
          let Vpost;
          if (isUS) {
            const absorbed = absorption * G; // gal
            Vpost = Math.max(Vw - absorbed, 0);
          } else {
            const absorbed = absorption * G; // L
            Vpost = Math.max(Vw - absorbed, 0);
          }

          // Output formatting
          const tempUnit = isUS ? "°F" : "°C";
          const volUnit = isUS ? "gal" : "L";
          const ratioUnit = isUS ? "qt/lb" : "L/kg";

          strikeTempResultEl.textContent = Tw.toFixed(1) + " " + tempUnit;
          strikeVolumeResultEl.textContent = Vw.toFixed(2) + " " + volUnit;
          mashThicknessResultEl.textContent =
            "Mash thickness: " + effectiveR.toFixed(2) + " " + ratioUnit;
          postAbsorptionVolumeResultEl.textContent =
            Vpost.toFixed(2) + " " + volUnit;

          const methodLabel =
            mashMethodEl.value === "biab" ? "BIAB" : "standard all-grain";
          summaryResultEl.textContent =
            "Method: " +
            methodLabel +
            ". Grain: " +
            G +
            (isUS ? " lb" : " kg") +
            " at " +
            Tg +
            " " +
            tempUnit +
            ". Target mash: " +
            Tm +
            " " +
            tempUnit +
            ". Tun mass: " +
            Mt.toFixed(1) +
            (isUS ? " lb" : " kg") +
            ".";

          if (Tw > (isUS ? 212 : 100)) {
            warningResultEl.textContent =
              "Warning: Calculated strike temperature is above boiling. Consider preheating your mash tun or using a thinner mash.";
            warningResultEl.classList.remove("hidden");
          }

          resultsEl.classList.remove("hidden");
        });
      })();
    </script>

