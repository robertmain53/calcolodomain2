    <script>
      // Utility conversions
      function cToF(c) {
        return (c * 9) / 5 + 32;
      }
      function fToC(f) {
        return ((f - 32) * 5) / 9;
      }
      function kToC(k) {
        return k - 273.15;
      }
      function cToK(c) {
        return c + 273.15;
      }

      // Magnus formula for dew point (over water)
      function dewPointFromTempRH(Tc, rh) {
        const a = 17.62;
        const b = 243.12; // °C
        const gamma = Math.log(rh / 100) + (a * Tc) / (b + Tc);
        return (b * gamma) / (a - gamma);
      }

      // Saturation vapor pressure (hPa)
      function saturationVaporPressure(Tc) {
        const a = 17.62;
        const b = 243.12;
        return 6.112 * Math.exp((a * Tc) / (b + Tc));
      }

      // Approximate heat index (°C) from T (°C) and RH (%)
      function heatIndex(Tc, rh) {
        const Tf = cToF(Tc);
        if (Tf < 80 || rh < 40) {
          return Tc; // little difference
        }
        const R = rh;
        const HI_F =
          -42.379 +
          2.04901523 * Tf +
          10.14333127 * R -
          0.22475541 * Tf * R -
          0.00683783 * Tf * Tf -
          0.05481717 * R * R +
          0.00122874 * Tf * Tf * R +
          0.00085282 * Tf * R * R -
          0.00000199 * Tf * Tf * R * R;
        return fToC(HI_F);
      }

      function classifyComfort(dewC) {
        const dewF = cToF(dewC);
        if (dewF < 40) {
          return {
            label: "Very dry",
            cls: "comfort-dry",
            text: "Air is very dry and crisp. Comfortable for many, but can dry out skin, eyes, and sinuses indoors.",
          };
        } else if (dewF < 55) {
          return {
            label: "Comfortable",
            cls: "comfort-good",
            text: "Most people find this range pleasant. Air does not feel humid or sticky.",
          };
        } else if (dewF < 65) {
          return {
            label: "Humid",
            cls: "comfort-muggy",
            text: "Humidity is noticeable. It may feel muggy, especially during physical activity or in warm weather.",
          };
        } else if (dewF < 70) {
          return {
            label: "Very humid",
            cls: "comfort-oppressive",
            text: "Air feels heavy and sticky. Sweat evaporates slowly and heat can feel oppressive.",
          };
        } else {
          return {
            label: "Oppressive",
            cls: "comfort-oppressive",
            text: "Extremely humid, tropical conditions. High risk of heat stress during exertion.",
          };
        }
      }

      function formatNumber(val, decimals) {
        if (!isFinite(val)) return "—";
        return val.toFixed(decimals);
      }

      function calculateDewPoint() {
        const tempInput = document.getElementById("tempValue");
        const tempUnit = document.getElementById("tempUnit").value;
        const rhInput = document.getElementById("rhValue");
        const showF = document.getElementById("useFahrenheitOutput").checked;
        const showHI = document.getElementById("showHeatIndex").checked;

        const dewOut = document.getElementById("dewPointOut");
        const dewFOut = document.getElementById("dewPointFOut");
        const absOut = document.getElementById("absHumidityOut");
        const vapOut = document.getElementById("vaporPressureOut");
        const hiOut = document.getElementById("heatIndexOut");

        const comfortBox = document.getElementById("comfortBox");
        const comfortLabel = document.getElementById("comfortLabel");
        const comfortText = document.getElementById("comfortText");

        let T = parseFloat(tempInput.value);
        let RH = parseFloat(rhInput.value);

        if (isNaN(T) || isNaN(RH)) {
          dewOut.textContent = "Enter temperature and RH";
          dewFOut.textContent = "—";
          absOut.textContent = "—";
          vapOut.textContent = "—";
          hiOut.textContent = "—";
          comfortBox.classList.add("hidden");
          return;
        }

        if (RH < 0) RH = 0;
        if (RH > 100) RH = 100;
        rhInput.value = RH.toString();

        // Convert input temperature to °C
        let Tc;
        if (tempUnit === "C") {
          Tc = T;
        } else if (tempUnit === "F") {
          Tc = fToC(T);
        } else {
          Tc = kToC(T);
        }

        // Dew point
        const TdC = dewPointFromTempRH(Tc, RH);
        const TdF = cToF(TdC);

        // Vapor pressure and absolute humidity
        const es = saturationVaporPressure(Tc); // hPa
        const e = (RH / 100) * es; // hPa
        const TK = cToK(Tc);
        const AH = (2.1674 * e) / TK; // g/m³ approx

        // Heat index
        const HIc = showHI ? heatIndex(Tc, RH) : NaN;

        dewOut.textContent = formatNumber(TdC, 1) + " °C";
        dewFOut.textContent = showF ? formatNumber(TdF, 1) + " °F" : "—";
        absOut.textContent = formatNumber(AH, 1) + " g/m³";
        vapOut.textContent = formatNumber(e, 2) + " hPa";
        hiOut.textContent = showHI ? formatNumber(HIc, 1) + " °C" : "—";

        // Comfort classification
        if (isFinite(TdC)) {
          const comfort = classifyComfort(TdC);
          comfortBox.classList.remove(
            "hidden",
            "comfort-dry",
            "comfort-good",
            "comfort-muggy",
            "comfort-oppressive"
          );
          comfortBox.classList.add(comfort.cls);
          comfortLabel.textContent = comfort.label;
          comfortText.textContent = comfort.text;
        } else {
          comfortBox.classList.add("hidden");
        }
      }

      function resetForm() {
        document.getElementById("tempValue").value = 30;
        document.getElementById("tempUnit").value = "C";
        document.getElementById("rhValue").value = 60;
        document.getElementById("useFahrenheitOutput").checked = false;
        document.getElementById("showHeatIndex").checked = true;
        document.getElementById("comfortBox").classList.add("hidden");
        document.getElementById("dewPointOut").textContent = "—";
        document.getElementById("dewPointFOut").textContent = "—";
        document.getElementById("absHumidityOut").textContent = "—";
        document.getElementById("vaporPressureOut").textContent = "—";
        document.getElementById("heatIndexOut").textContent = "—";
      }

      document.addEventListener("DOMContentLoaded", function () {
        document
          .getElementById("calcBtn")
          .addEventListener("click", function (e) {
            e.preventDefault();
            calculateDewPoint();
          });
        document
          .getElementById("resetBtn")
          .addEventListener("click", function (e) {
            e.preventDefault();
            resetForm();
          });
        document
          .getElementById("tempValue")
          .addEventListener("keyup", function (e) {
            if (e.key === "Enter") calculateDewPoint();
          });
        document
          .getElementById("rhValue")
          .addEventListener("keyup", function (e) {
            if (e.key === "Enter") calculateDewPoint();
          });

        // Initial calculation
        calculateDewPoint();
      });

      // Mobile menu script (simple toggle)
      document.addEventListener("DOMContentLoaded", function () {
        const toggle = document.getElementById("mobile-menu-toggle");
        const menu = document.getElementById("mobile-menu");
        if (toggle && menu) {
          toggle.addEventListener("click", () => {
            const isHidden = menu.classList.contains("hidden");
            menu.classList.toggle("hidden", !isHidden);
            toggle.setAttribute("aria-expanded", isHidden ? "true" : "false");
          });
        }
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
