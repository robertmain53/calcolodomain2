        54: ["S14", "D20"],
        53: ["S13", "D20"],
        52: ["S12", "D20"],
        51: ["S11", "D20"],
        50: ["D25"],
        40: ["D20"],
        32: ["D16"],
      };

      const NO_FINISH_3_DARTS = new Set([169, 168, 166, 165, 163, 162, 159]);

      function formatRoute(route) {
        if (!route || route.length === 0) return "—";
        return route.join(", ");
      }

      function describeRoute(route) {
        if (!route || route.length === 0) return "";
        const darts = route.length;
        return `${darts} dart${darts > 1 ? "s" : ""}: ${formatRoute(route)}`;
      }

      // Simple generator for all possible dart scores (0–60 plus 25, 50)
      const SINGLE = Array.from({ length: 20 }, (_, i) => i + 1);
      const DOUBLE = SINGLE.map((v) => 2 * v);
      const TRIPLE = SINGLE.map((v) => 3 * v);
      const ALL_SCORES = new Set([...SINGLE, ...DOUBLE, ...TRIPLE, 25, 50, 0]);
      const FINISHING_DOUBLES = new Set([...DOUBLE, 50]);

      function generateAllRoutes(maxDarts) {
        const darts = [];
        for (let v of ALL_SCORES) darts.push(v);
        const routes = [];

        function backtrack(remaining, dartsLeft, current) {
          if (dartsLeft === 0) {
            if (
              remaining === 0 &&
              current.length > 0 &&
              FINISHING_DOUBLES.has(current[current.length - 1])
            ) {
              routes.push([...current]);
            }
            return;
          }
          if (remaining < 0) return;
          if (remaining === 0) {
            // must already end on double
            if (
              current.length > 0 &&
              FINISHING_DOUBLES.has(current[current.length - 1])
            ) {
              routes.push([...current]);
            }
            return;
          }
          for (let score of darts) {
            if (score > remaining) continue;
            current.push(score);
            backtrack(remaining - score, dartsLeft - 1, current);
            current.pop();
          }
        }

        return function findRoutes(target, dartsLeft) {
          routes.length = 0;
          backtrack(target, dartsLeft, []);
          // sort by length then by preference (higher first dart)
          routes.sort((a, b) => {
            if (a.length !== b.length) return a.length - b.length;
            return (b[0] || 0) - (a[0] || 0);
          });
          return routes;
        };
      }

      const findRoutes = generateAllRoutes(3);

      function scoreToNotation(score) {
        if (score === 25) return "25";
        if (score === 50) return "D25";
        if (score === 0) return "0";
        for (let v of SINGLE) {
          if (score === v) return "S" + v;
          if (score === 2 * v) return "D" + v;
          if (score === 3 * v) return "T" + v;
        }
        return String(score);
      }

      function routeScoresToNotation(route) {
        return route.map(scoreToNotation);
      }

      function getCheckout(score, dartsLeft) {
        const notes = [];
        if (score < 2) {
          return {
            main: null,
            alts: [],
            notes: ["Score must be at least 2 to finish on a double."],
          };
        }
        if (score > 170) {
          return {
            main: null,
            alts: [],
            notes: ["No checkout: maximum possible finish is 170 in 3 darts."],
          };
        }

        if (dartsLeft === 3 && NO_FINISH_3_DARTS.has(score)) {
          notes.push(
            "No checkout in 3 darts under double-out rules. Play a strong set-up shot."
          );
          return { main: null, alts: [], notes };
        }

        // Use curated preset if available and dartsLeft >= required darts
        const preset = PRESET_CHECKOUTS[score];
        if (preset && preset.length <= dartsLeft) {
          const main = preset;
          const alts = [];
          if (dartsLeft > preset.length) {
            notes.push(
              `You have ${dartsLeft} darts, but this finish only needs ${preset.length}.`
            );
          }
          return { main, alts, notes };
        }

        // Algorithmic search
        const allRoutes = findRoutes(score, dartsLeft).map(
          routeScoresToNotation
        );
        if (allRoutes.length === 0) {
          notes.push(
            "No checkout found with the given score and darts in hand under double-out rules."
          );
          return { main: null, alts: [], notes };
        }

        const main = allRoutes[0];
        const alts = allRoutes.slice(1, 4); // up to 3 alternatives
        return { main, alts, notes };
      }

      function renderCheckout(score, dartsLeft) {
        const outEl = document.getElementById("checkout-output");
        const altEl = document.getElementById("checkout-alternatives");
        const notesEl = document.getElementById("checkout-notes");

        outEl.innerHTML = "";
        altEl.innerHTML = "";
        notesEl.innerHTML = "";

        const { main, alts, notes } = getCheckout(score, dartsLeft);

        if (!main) {
          outEl.innerHTML = `<p class="text-red-600 text-sm font-medium">No checkout available.</p>`;
        } else {
          const mainHtml = `
        <div class="flex flex-wrap gap-2 items-center">
          <span class="inline-flex items-center px-3 py-1 rounded-full text-sm font-semibold bg-blue-600 text-white">
            ${formatRoute(main)}
          </span>
          <span class="text-xs text-gray-500">(${describeRoute(main)})</span>
        </div>
      `;
          outEl.innerHTML = mainHtml;
        }

        if (alts && alts.length > 0) {
          const altHtml = alts
            .map(
              (route) => `
        <div class="mb-1">
          <span class="inline-flex items-center px-3 py-1 rounded-full text-xs font-medium bg-gray-100 text-gray-800">
            ${formatRoute(route)}
          </span>
          <span class="text-[11px] text-gray-500 ml-1">${describeRoute(
            route
          )}</span>
        </div>
      `
            )
            .join("");
          altEl.innerHTML = altHtml;
        } else {
          altEl.innerHTML = `<p class="text-xs text-gray-400">No alternative routes found for this score and darts in hand.</p>`;
        }

        const extraNotes = [];
        if (dartsLeft === 3 && NO_FINISH_3_DARTS.has(score)) {
          extraNotes.push(
            "This is a classic “no finish” score. Aim to leave a good double next turn."
          );
        }
        if (score === 170) {
          extraNotes.push(
            "170 is the highest possible checkout in standard 501 darts."
          );
        }
        if (score === 32) {
          extraNotes.push(
            "D16 is one of the most popular doubles because missing inside leaves D8, then D4, etc."
          );
        }

        const allNotes = [...notes, ...extraNotes];
        if (allNotes.length === 0) {
          notesEl.innerHTML = `<p class="text-sm text-gray-600">Professional-style routes prioritize good doubles and safe misses.</p>`;
        } else {
          notesEl.innerHTML = `<ul class="list-disc list-inside text-sm text-gray-700">${allNotes
            .map((n) => `<li>${n}</li>`)
            .join("")}</ul>`;
        }
      }

      function initOutChart() {
        const body = document.getElementById("out-chart-body");
        const rows = [];
        for (let s = 170; s >= 60; s--) {
          const preset = PRESET_CHECKOUTS[s];
          if (!preset) continue;
          rows.push(`
        <tr class="hover:bg-gray-50 cursor-pointer" data-chart-score="${s}">
          <td class="px-2 py-1 border-t text-blue-600 hover:underline">${s}</td>
          <td class="px-2 py-1 border-t">${formatRoute(preset)}</td>
        </tr>
      `);
        }
        body.innerHTML = rows.join("");
      }

      document.addEventListener("DOMContentLoaded", () => {
        const scoreInput = document.getElementById("score-input");
        const dartsLeftSelect = document.getElementById("darts-left");
        const btnSuggest = document.getElementById("btn-suggest");
        const btnClear = document.getElementById("btn-clear-score");

        btnSuggest.addEventListener("click", () => {
          const score = parseInt(scoreInput.value, 10);
          const dartsLeft = parseInt(dartsLeftSelect.value, 10);
          if (isNaN(score)) {
            alert("Please enter a valid remaining score.");
            return;
          }
          renderCheckout(score, dartsLeft);
        });

        btnClear.addEventListener("click", () => {
          scoreInput.value = "";
          document.getElementById("checkout-output").innerHTML =
            '<p class="text-gray-500 text-sm">Enter a score and click “Show checkout” to see the best route.</p>';
          document.getElementById("checkout-alternatives").innerHTML =
            '<p class="text-gray-400 text-xs">If there are safer or popular alternative finishes, they’ll appear here.</p>';
          document.getElementById("checkout-notes").innerHTML =
            '<p class="text-sm text-gray-700">Professional-style recommendations prioritize leaving a good double if you miss.</p>';
        });

        document.querySelectorAll("[data-quick-score]").forEach((btn) => {
          btn.addEventListener("click", () => {
            const val = parseInt(btn.getAttribute("data-quick-score"), 10);
            scoreInput.value = val;
            renderCheckout(val, parseInt(dartsLeftSelect.value, 10));
          });
        });

        initOutChart();

        document
          .getElementById("out-chart-body")
          .addEventListener("click", (e) => {
            const row = e.target.closest("tr[data-chart-score]");
            if (!row) return;
            const score = parseInt(row.getAttribute("data-chart-score"), 10);
            scoreInput.value = score;
            renderCheckout(score, parseInt(dartsLeftSelect.value, 10));
          });

        // Initial render for default 170 / 3 darts
        renderCheckout(170, 3);
      });

      // Mobile menu script (simple)
      document.addEventListener("DOMContentLoaded", () => {
        const toggle = document.getElementById("mobile-menu-toggle");
        const menu = document.getElementById("mobile-menu");
        if (toggle && menu) {
          toggle.addEventListener("click", () => {
            const isHidden = menu.classList.contains("hidden");
            menu.classList.toggle("hidden", !isHidden);
            toggle.setAttribute("aria-expanded", String(isHidden));
          });
        }
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
