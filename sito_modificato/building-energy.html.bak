    <script>
      (function () {
        const buildingTypeEl = document.getElementById("buildingType");
        const floorAreaEl = document.getElementById("floorArea");
        const areaUnitEl = document.getElementById("areaUnit");
        const climateEl = document.getElementById("climate");
        const hoursPerWeekEl = document.getElementById("hoursPerWeek");
        const hoursPerWeekLabel = document.getElementById("hoursPerWeekLabel");
        const envelopeQualityEl = document.getElementById("envelopeQuality");
        const envelopeQualityLabel = document.getElementById(
          "envelopeQualityLabel"
        );
        const hvacEfficiencyEl = document.getElementById("hvacEfficiency");
        const hvacEfficiencyLabel = document.getElementById(
          "hvacEfficiencyLabel"
        );
        const internalLoadEfficiencyEl = document.getElementById(
          "internalLoadEfficiency"
        );
        const internalLoadEfficiencyLabel = document.getElementById(
          "internalLoadEfficiencyLabel"
        );
        const elecPriceEl = document.getElementById("elecPrice");
        const gasPriceEl = document.getElementById("gasPrice");

        const totalEnergyKWhEl = document.getElementById("totalEnergyKWh");
        const elecUseKWhEl = document.getElementById("elecUseKWh");
        const gasUseThermsEl = document.getElementById("gasUseTherms");
        const euiValueEl = document.getElementById("euiValue");
        const euiRatingEl = document.getElementById("euiRating");
        const annualCostEl = document.getElementById("annualCost");
        const costPerAreaEl = document.getElementById("costPerArea");
        const co2EmissionsEl = document.getElementById("co2Emissions");
        const upgradeEnergyEl = document.getElementById("upgradeEnergy");
        const upgradeCostEl = document.getElementById("upgradeCost");
        const upgradeSavingsEl = document.getElementById("upgradeSavings");
        const calcStatusEl = document.getElementById("calcStatus");

        const calculateBtn = document.getElementById("calculateBtn");
        const resetBtn = document.getElementById("resetBtn");

        // Reference data: base EUI (kBtu/ft²·yr) for temperate climate
        const baseEUI = {
          office: 80,
          residential: 40,
          singlefamily: 35,
          retail: 70,
          school: 60,
          hospital: 180,
          warehouse: 30,
        };

        // Reference operating hours per week by type
        const refHours = {
          office: 60,
          residential: 112, // 16h/day
          singlefamily: 112,
          retail: 72,
          school: 50,
          hospital: 168,
          warehouse: 60,
        };

        // Climate multipliers (approximate)
        const climateFactor = {
          cool: 1.15,
          temperate: 1.0,
          warm: 1.05,
          hot: 1.15,
          dryhot: 1.1,
        };

        function formatNumber(value, decimals) {
          if (!isFinite(value)) return "–";
          return value.toLocaleString(undefined, {
            minimumFractionDigits: decimals,
            maximumFractionDigits: decimals,
          });
        }

        function describeEfficiencyFactor(f) {
          if (f <= 0.6) return "High-performance";
          if (f <= 0.85) return "Efficient";
          if (f <= 1.15) return "Typical";
          if (f <= 1.35) return "Inefficient";
          return "Very inefficient";
        }

        function classifyEUI(type, eui) {
          if (!isFinite(eui)) return "";
          const ref = baseEUI[type] || 70;
          const ratio = eui / ref;
          if (ratio <= 0.5)
            return "Very efficient compared to typical buildings of this type.";
          if (ratio <= 0.8)
            return "Efficient – better than typical for this building type.";
          if (ratio <= 1.1)
            return "Typical energy performance for this building type.";
          if (ratio <= 1.4)
            return "Inefficient – there is clear room for improvement.";
          return "Very inefficient – consider a detailed energy audit and retrofit plan.";
        }

        function updateLabels() {
          hoursPerWeekLabel.textContent = hoursPerWeekEl.value + " h";

          const env = parseFloat(envelopeQualityEl.value);
          const hvac = parseFloat(hvacEfficiencyEl.value);
          const internal = parseFloat(internalLoadEfficiencyEl.value);

          envelopeQualityLabel.textContent = describeEfficiencyFactor(env);
          hvacEfficiencyLabel.textContent = describeEfficiencyFactor(hvac);
          internalLoadEfficiencyLabel.textContent =
            describeEfficiencyFactor(internal);
        }

        function resetDefaults() {
          buildingTypeEl.value = "office";
          floorAreaEl.value = 5000;
          areaUnitEl.value = "ft2";
          climateEl.value = "temperate";
          hoursPerWeekEl.value = 60;
          envelopeQualityEl.value = 1.0;
          hvacEfficiencyEl.value = 1.0;
          internalLoadEfficiencyEl.value = 1.0;
          elecPriceEl.value = 0.15;
          gasPriceEl.value = 1.2;
          updateLabels();
          clearResults();
          calcStatusEl.textContent = "";
        }

        function clearResults() {
          [
            totalEnergyKWhEl,
            elecUseKWhEl,
            gasUseThermsEl,
            euiValueEl,
            euiRatingEl,
            annualCostEl,
            costPerAreaEl,
            co2EmissionsEl,
            upgradeEnergyEl,
            upgradeCostEl,
            upgradeSavingsEl,
          ].forEach((el) => (el.textContent = "–"));
        }

        function calculate() {
          const type = buildingTypeEl.value;
          let area = parseFloat(floorAreaEl.value);
          const areaUnit = areaUnitEl.value;
          const climate = climateEl.value;
          const hours = parseFloat(hoursPerWeekEl.value);
          const env = parseFloat(envelopeQualityEl.value);
          const hvac = parseFloat(hvacEfficiencyEl.value);
          const internal = parseFloat(internalLoadEfficiencyEl.value);
          const elecPrice = parseFloat(elecPriceEl.value);
          const gasPrice = parseFloat(gasPriceEl.value);

          if (!isFinite(area) || area <= 0) {
            alert("Please enter a valid floor area.");
            return;
          }

          // Convert area to ft² for EUI in kBtu/ft²·yr
          let areaFt2 = area;
          let areaM2 = area;
          if (areaUnit === "m2") {
            areaFt2 = area * 10.7639;
          } else {
            areaM2 = area / 10.7639;
          }

          const base = baseEUI[type] || 70;
          const refH = refHours[type] || 60;
          const hFactor = hours / refH;
          const cFactor = climateFactor[climate] || 1.0;

          // Split base EUI into components: 40% heating/cooling, 60% internal/electric
          const baseHC = base * 0.4;
          const baseInt = base * 0.6;

          const adjHC = baseHC * env * hvac * cFactor;
          const adjInt = baseInt * internal * hFactor;

          const totalEUI_kBtu_ft2 = adjHC + adjInt;

          // Convert to total annual energy (kBtu and kWh)
          const total_kBtu = totalEUI_kBtu_ft2 * areaFt2;
          const total_kWh = total_kBtu / 3.412;

          // Assume 60% of site energy is electricity, 40% gas (rough split)
          const elec_kWh = total_kWh * 0.6;
          const gas_kWh_equiv = total_kWh * 0.4;
          const gas_therms = gas_kWh_equiv / 29.3;

          // Cost
          const annualCost = elec_kWh * elecPrice + gas_therms * gasPrice;
          const costPerFt2 = annualCost / areaFt2;

          // CO2 emissions
          const co2_elec = elec_kWh * 0.4; // kg
          const co2_gas = gas_therms * 5.3; // kg
          const co2_total = co2_elec + co2_gas;

          // Metric EUI in kWh/m²·yr
          const eui_kWh_m2 = total_kWh / areaM2;

          // Update UI
          totalEnergyKWhEl.textContent =
            formatNumber(total_kWh, 0) + " kWh/year";
          elecUseKWhEl.textContent = formatNumber(elec_kWh, 0) + " kWh/year";
          gasUseThermsEl.textContent =
            formatNumber(gas_therms, 0) + " therms/year";

          euiValueEl.textContent =
            formatNumber(totalEUI_kBtu_ft2, 1) +
            " kBtu/ft²·yr" +
            " (" +
            formatNumber(eui_kWh_m2, 1) +
            " kWh/m²·yr)";

          euiRatingEl.textContent = classifyEUI(type, totalEUI_kBtu_ft2);

          annualCostEl.textContent =
            "$" + formatNumber(annualCost, 0) + "/year";
          costPerAreaEl.textContent =
            "$" + formatNumber(costPerFt2, 2) + "/ft²·yr";

          co2EmissionsEl.textContent =
            formatNumber(co2_total / 1000, 2) + " metric tons CO₂/year";

          // Upgrade scenario: 20% reduction in total energy
          const upgrade_kWh = total_kWh * 0.8;
          const upgrade_elec_kWh = elec_kWh * 0.8;
          const upgrade_gas_therms = gas_therms * 0.8;
          const upgradeCost =
            upgrade_elec_kWh * elecPrice + upgrade_gas_therms * gasPrice;
          const savings = annualCost - upgradeCost;

          upgradeEnergyEl.textContent =
            formatNumber(upgrade_kWh, 0) + " kWh/year";
          upgradeCostEl.textContent =
            "$" + formatNumber(upgradeCost, 0) + "/year";
          upgradeSavingsEl.textContent =
            "$" + formatNumber(savings, 0) + "/year";

          const now = new Date();
          calcStatusEl.textContent =
            "Last calculated: " + now.toLocaleTimeString();
        }

        // Event listeners
        [
          hoursPerWeekEl,
          envelopeQualityEl,
          hvacEfficiencyEl,
          internalLoadEfficiencyEl,
        ].forEach((el) => {
          el.addEventListener("input", updateLabels);
        });

        calculateBtn.addEventListener("click", calculate);
        resetBtn.addEventListener("click", resetDefaults);

        // Initialize
        updateLabels();
        clearResults();
      })();
    </script>
  
