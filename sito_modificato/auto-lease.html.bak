    <script>
      function toNumber(id) {
        const v = parseFloat(document.getElementById(id).value);
        return isNaN(v) ? 0 : v;
      }

      function formatMoney(v) {
        return "$" + v.toFixed(0).replace(/\B(?=(\d{3})+(?!\d))/g, ",");
      }

      function formatMoney2(v) {
        return "$" + v.toFixed(2).replace(/\B(?=(\d{3})+(?!\d))/g, ",");
      }

      function formatMiles(v) {
        return v.toFixed(0).replace(/\B(?=(\d{3})+(?!\d))/g, ",") + " mi";
      }

      function recalcFromResidualPercent() {
        const price = toNumber("vehiclePrice");
        const residualPercent = toNumber("residualPercent");
        const residualValue = (price * residualPercent) / 100;
        document.getElementById("residualValue").value =
          residualValue.toFixed(0);
      }

      function recalcFromResidualValue() {
        const price = toNumber("vehiclePrice");
        const residualValue = toNumber("residualValue");
        const residualPercent = price > 0 ? (residualValue / price) * 100 : 0;
        document.getElementById("residualPercent").value =
          residualPercent.toFixed(1);
      }

      function recalcFromMoneyFactor() {
        const mf = toNumber("moneyFactor");
        const apr = mf * 2400;
        document.getElementById("interestRate").value = apr.toFixed(2);
      }

      function recalcFromAPR() {
        const apr = toNumber("interestRate");
        const mf = apr / 2400;
        document.getElementById("moneyFactor").value = mf.toFixed(4);
      }

      function calculateLease() {
        const price = toNumber("vehiclePrice");
        const down = toNumber("downPayment");
        const tradeIn = toNumber("tradeInValue");
        const term = Math.max(1, toNumber("leaseTerm"));
        const residual = toNumber("residualValue");
        const mf = toNumber("moneyFactor");
        const taxRate = toNumber("salesTax") / 100;
        const acq = toNumber("acquisitionFee");
        const doc = toNumber("docFees");
        const taxOn = document.getElementById("taxOn").value;

        const annualMiles = toNumber("annualMiles");
        const expectedAnnualMiles = toNumber("expectedAnnualMiles");
        const excessFee = toNumber("excessMileFee");
        const purchaseFee = toNumber("purchaseOptionFee");

        // Cap cost
        const capCost = Math.max(0, price + acq - down - tradeIn);

        // Depreciation and finance
        const depreciationTotal = Math.max(0, capCost - residual);
        const depreciationPerMonth = depreciationTotal / term;
        const financePerMonth = (capCost + residual) * mf;
        const basePayment = depreciationPerMonth + financePerMonth;

        // Tax handling
        let monthlyTax = 0;
        let totalTax = 0;
        let totalPaymentsBeforeTax = basePayment * term;

        if (taxOn === "payment") {
          monthlyTax = basePayment * taxRate;
          totalTax = monthlyTax * term;
        } else if (taxOn === "totalLease") {
          totalTax = totalPaymentsBeforeTax * taxRate;
          monthlyTax = totalTax / term;
        } else if (taxOn === "capCost") {
          totalTax = capCost * taxRate;
          monthlyTax = 0;
        }

        const monthlyPayment = basePayment + monthlyTax;

        // Upfront costs: down + acq + doc + first payment + upfront tax (if capCost taxed)
        let upfrontCost = down + acq + doc + monthlyPayment;
        if (taxOn === "capCost") {
          upfrontCost += totalTax;
        }

        const totalLeaseCost = upfrontCost + monthlyPayment * (term - 1); // first payment already in upfront

        // Mileage
        const allowedMiles = annualMiles * (term / 12);
        const expectedMiles = expectedAnnualMiles * (term / 12);
        const excessMiles = Math.max(0, expectedMiles - allowedMiles);
        const mileageCharges = excessMiles * excessFee;

        // Buyout cost
        const buyoutCost = residual + purchaseFee;

        // Loan comparison (simple amortization)
        const apr = toNumber("interestRate") / 100;
        const monthlyRate = apr / 12;
        let loanPayment = 0;
        if (monthlyRate === 0) {
          loanPayment = price / term;
        } else {
          const factor = Math.pow(1 + monthlyRate, term);
          loanPayment = (price * monthlyRate * factor) / (factor - 1);
        }

        // Update UI
        document.getElementById("capCost").textContent = formatMoney(capCost);
        document.getElementById("residualOut").textContent =
          formatMoney(residual);
        document.getElementById("depreciationCharge").textContent =
          formatMoney2(depreciationPerMonth) + " /mo";
        document.getElementById("financeCharge").textContent =
          formatMoney2(financePerMonth) + " /mo";
        document.getElementById("monthlyPayment").textContent =
          formatMoney2(monthlyPayment);
        document.getElementById("monthlyPaymentTaxNote").textContent =
          taxOn === "capCost"
            ? "tax paid upfront on cap cost"
            : "incl. estimated tax on payment";

        document.getElementById("totalPayments").textContent = formatMoney2(
          totalPaymentsBeforeTax
        );
        document.getElementById("totalTax").textContent =
          formatMoney2(totalTax);
        document.getElementById("upfrontCost").textContent =
          formatMoney2(upfrontCost);
        document.getElementById("totalLeaseCost").textContent =
          formatMoney2(totalLeaseCost);

        document.getElementById("allowedMiles").textContent =
          formatMiles(allowedMiles);
        document.getElementById("expectedMiles").textContent =
          formatMiles(expectedMiles);
        document.getElementById("excessMiles").textContent =
          formatMiles(excessMiles);
        document.getElementById("mileageCharges").textContent =
          formatMoney2(mileageCharges);
        document.getElementById("buyoutCost").textContent =
          formatMoney2(buyoutCost);

        document.getElementById("loanPayment").textContent =
          formatMoney2(loanPayment) + " /mo";
      }

      function attachEvents() {
        const ids = [
          "vehiclePrice",
          "downPayment",
          "tradeInValue",
          "leaseTerm",
          "residualPercent",
          "residualValue",
          "moneyFactor",
          "interestRate",
          "salesTax",
          "acquisitionFee",
          "docFees",
          "taxOn",
          "annualMiles",
          "expectedAnnualMiles",
          "excessMileFee",
          "purchaseOptionFee",
        ];
        ids.forEach((id) => {
          const el = document.getElementById(id);
          if (!el) return;
          el.addEventListener("input", () => {
            if (id === "residualPercent") recalcFromResidualPercent();
            if (id === "residualValue") recalcFromResidualValue();
            if (id === "moneyFactor") recalcFromMoneyFactor();
            if (id === "interestRate") recalcFromAPR();
            calculateLease();
          });
          if (el.tagName === "SELECT") {
            el.addEventListener("change", calculateLease);
          }
        });

        document.getElementById("resetBtn").addEventListener("click", () => {
          document.getElementById("vehiclePrice").value = 35000;
          document.getElementById("downPayment").value = 3000;
          document.getElementById("tradeInValue").value = 0;
          document.getElementById("leaseTerm").value = 36;
          document.getElementById("residualPercent").value = 55;
          recalcFromResidualPercent();
          document.getElementById("moneyFactor").value = 0.0015;
          recalcFromMoneyFactor();
          document.getElementById("salesTax").value = 8.5;
          document.getElementById("acquisitionFee").value = 895;
          document.getElementById("docFees").value = 400;
          document.getElementById("taxOn").value = "payment";
          document.getElementById("annualMiles").value = 12000;
          document.getElementById("expectedAnnualMiles").value = 12000;
          document.getElementById("excessMileFee").value = 0.25;
          document.getElementById("purchaseOptionFee").value = 0;
          calculateLease();
        });
      }

      document.addEventListener("DOMContentLoaded", () => {
        recalcFromResidualPercent();
        recalcFromMoneyFactor();
        attachEvents();
        calculateLease();
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
