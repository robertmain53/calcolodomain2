        document.addEventListener('DOMContentLoaded', function() {
            // JavaScript for handling calculator logic
            const calculatorForm = document.getElementById('calculator-form');
            calculatorForm.addEventListener('submit', function(event) {
                event.preventDefault();
                // Perform calculation and update results
                const inputExpression = document.getElementById('expression').value;
                try {
                    const result = eval(inputExpression);
                    document.getElementById('resultValue').textContent = result;
                } catch (e) {
                    document.getElementById('resultValue').textContent = 'Error';
                }
            });
        });
    </script>
