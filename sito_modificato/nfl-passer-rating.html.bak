    <script>
      // Utility: clamp component between 0 and 2.375
      function clampComponent(x) {
        if (isNaN(x) || !isFinite(x)) return 0;
        if (x < 0) return 0;
        if (x > 2.375) return 2.375;
        return x;
      }

      function computeFromRaw(att, comp, yards, td, intc) {
        att = Number(att);
        comp = Number(comp);
        yards = Number(yards);
        td = Number(td);
        intc = Number(intc);
        if (att <= 0) {
          return {
            rating: 0,
            a: 0,
            b: 0,
            c: 0,
            d: 0,
            compPct: 0,
            ypa: 0,
            tdpa: 0,
            intpa: 0,
          };
        }
        const compPct = (comp / att) * 100;
        const ypa = yards / att;
        const tdpa = td / att;
        const intpa = intc / att;

        let a = (comp / att - 0.3) * 5;
        let b = (yards / att - 3) * 0.25;
        let c = (td / att) * 20;
        let d = 2.375 - (intc / att) * 25;

        a = clampComponent(a);
        b = clampComponent(b);
        c = clampComponent(c);
        d = clampComponent(d);

        const rating = ((a + b + c + d) / 6) * 100;

        return { rating, a, b, c, d, compPct, ypa, tdpa, intpa };
      }

      function computeFromRates(compPct, ypa, tdpa, intpa) {
        compPct = Number(compPct);
        ypa = Number(ypa);
        tdpa = Number(tdpa);
        intpa = Number(intpa);

        const compRate = compPct / 100;

        let a = (compRate - 0.3) * 5;
        let b = (ypa - 3) * 0.25;
        let c = tdpa * 20;
        let d = 2.375 - intpa * 25;

        a = clampComponent(a);
        b = clampComponent(b);
        c = clampComponent(c);
        d = clampComponent(d);

        const rating = ((a + b + c + d) / 6) * 100;

        return { rating, a, b, c, d, compPct, ypa, tdpa, intpa };
      }

      function updateMainFromRaw() {
        const att = document.getElementById("att").value;
        const comp = document.getElementById("comp").value;
        const yards = document.getElementById("yards").value;
        const td = document.getElementById("td").value;
        const intc = document.getElementById("int").value;

        const res = computeFromRaw(att, comp, yards, td, intc);

        document.getElementById("rating-output").textContent =
          res.rating.toFixed(1);
        document.getElementById("a-value").textContent = res.a.toFixed(3);
        document.getElementById("b-value").textContent = res.b.toFixed(3);
        document.getElementById("c-value").textContent = res.c.toFixed(3);
        document.getElementById("d-value").textContent = res.d.toFixed(3);

        document.getElementById("summary-att").textContent = Number(att) || 0;

        document.getElementById("stat-comp-pct").textContent =
          res.compPct.toFixed(1);
        document.getElementById("stat-ypa").textContent = res.ypa.toFixed(1);
        document.getElementById("stat-tdpa").textContent = res.tdpa.toFixed(3);
        document.getElementById("stat-intpa").textContent =
          res.intpa.toFixed(3);

        // Sync rate inputs
        document.getElementById("compPct").value = res.compPct
          ? res.compPct.toFixed(1)
          : "";
        document.getElementById("ypa").value = res.ypa
          ? res.ypa.toFixed(2)
          : "";
        document.getElementById("tdpa").value = res.tdpa
          ? res.tdpa.toFixed(4)
          : "";
        document.getElementById("intpa").value = res.intpa
          ? res.intpa.toFixed(4)
          : "";

        // Label
        const r = res.rating;
        let label = "No attempts – rating is undefined";
        if (Number(att) <= 0) {
          label = "No attempts – rating is undefined";
        } else if (r < 75) {
          label = "Poor efficiency";
        } else if (r < 90) {
          label = "Below-average efficiency";
        } else if (r < 100) {
          label = "Solid / slightly above average";
        } else if (r < 110) {
          label = "Very good performance";
        } else {
          label = "Elite passing efficiency";
        }
        document.getElementById("rating-label").textContent = label;
      }

      function updateMainFromRates() {
        const compPct = document.getElementById("compPct").value;
        const ypa = document.getElementById("ypa").value;
        const tdpa = document.getElementById("tdpa").value;
        const intpa = document.getElementById("intpa").value;

        const res = computeFromRates(compPct, ypa, tdpa, intpa);

        document.getElementById("rating-output").textContent =
          res.rating.toFixed(1);
        document.getElementById("a-value").textContent = res.a.toFixed(3);
        document.getElementById("b-value").textContent = res.b.toFixed(3);
        document.getElementById("c-value").textContent = res.c.toFixed(3);
        document.getElementById("d-value").textContent = res.d.toFixed(3);

        // Attempts unknown in rate mode
        document.getElementById("summary-att").textContent = "—";

        document.getElementById("stat-comp-pct").textContent = isNaN(
          res.compPct
        )
          ? "0.0"
          : res.compPct.toFixed(1);
        document.getElementById("stat-ypa").textContent = isNaN(res.ypa)
          ? "0.0"
          : res.ypa.toFixed(1);
        document.getElementById("stat-tdpa").textContent = isNaN(res.tdpa)
          ? "0.000"
          : res.tdpa.toFixed(3);
        document.getElementById("stat-intpa").textContent = isNaN(res.intpa)
          ? "0.000"
          : res.intpa.toFixed(3);

        let label = "Rating based on efficiency inputs";
        const r = res.rating;
        if (!isNaN(r)) {
          if (r < 75) label = "Poor efficiency";
          else if (r < 90) label = "Below-average efficiency";
          else if (r < 100) label = "Solid / slightly above average";
          else if (r < 110) label = "Very good performance";
          else label = "Elite passing efficiency";
        }
        document.getElementById("rating-label").textContent = label;
      }

      document.addEventListener("DOMContentLoaded", function () {
        const modeRawBtn = document.getElementById("mode-raw-btn");
        const modeRateBtn = document.getElementById("mode-rate-btn");
        const rawInputs = document.querySelectorAll(
          "#raw-stats-panel .stat-input"
        );
        const perAttemptToggle = document.getElementById("per-attempt-toggle");
        const perAttemptStats = document.getElementById("per-attempt-stats");

        let mode = "raw"; // 'raw' or 'rate'

        function setMode(newMode) {
          mode = newMode;
          if (mode === "raw") {
            modeRawBtn.classList.add("bg-blue-600", "text-white");
            modeRawBtn.classList.remove("bg-white", "text-gray-700");
            modeRateBtn.classList.remove("bg-blue-600", "text-white");
            modeRateBtn.classList.add("bg-white", "text-gray-700");
            rawInputs.forEach((el) => (el.disabled = false));
          } else {
            modeRateBtn.classList.add("bg-blue-600", "text-white");
            modeRateBtn.classList.remove("bg-white", "text-gray-700");
            modeRawBtn.classList.remove("bg-blue-600", "text-white");
            modeRawBtn.classList.add("bg-white", "text-gray-700");
            rawInputs.forEach((el) => (el.disabled = true));
          }
        }

        modeRawBtn.addEventListener("click", function () {
          setMode("raw");
          updateMainFromRaw();
        });
        modeRateBtn.addEventListener("click", function () {
          setMode("rate");
          updateMainFromRates();
        });

        document
          .getElementById("calc-btn")
          .addEventListener("click", function () {
            if (mode === "raw") {
              updateMainFromRaw();
            } else {
              updateMainFromRates();
            }
          });

        document
          .getElementById("reset-btn")
          .addEventListener("click", function () {
            document.getElementById("att").value = 35;
            document.getElementById("comp").value = 24;
            document.getElementById("yards").value = 275;
            document.getElementById("td").value = 2;
            document.getElementById("int").value = 1;

            document.getElementById("compPct").value = "";
            document.getElementById("ypa").value = "";
            document.getElementById("tdpa").value = "";
            document.getElementById("intpa").value = "";

            setMode("raw");
            updateMainFromRaw();
          });

        // Live update on raw input change
        rawInputs.forEach((el) => {
          el.addEventListener("input", function () {
            if (mode === "raw") updateMainFromRaw();
          });
        });

        // Live update on rate input change
        ["compPct", "ypa", "tdpa", "intpa"].forEach((id) => {
          const el = document.getElementById(id);
          el.addEventListener("input", function () {
            if (mode === "rate") updateMainFromRates();
          });
        });

        perAttemptToggle.addEventListener("change", function () {
          perAttemptStats.classList.toggle("hidden", !perAttemptToggle.checked);
        });

        // Comparison calculator
        document
          .getElementById("cmp-calc-btn")
          .addEventListener("click", function () {
            const A = {
              att: document.getElementById("cmpA-att").value,
              comp: document.getElementById("cmpA-comp").value,
              yards: document.getElementById("cmpA-yards").value,
              td: document.getElementById("cmpA-td").value,
              intc: document.getElementById("cmpA-int").value,
            };
            const B = {
              att: document.getElementById("cmpB-att").value,
              comp: document.getElementById("cmpB-comp").value,
              yards: document.getElementById("cmpB-yards").value,
              td: document.getElementById("cmpB-td").value,
              intc: document.getElementById("cmpB-int").value,
            };

            const resA = computeFromRaw(A.att, A.comp, A.yards, A.td, A.intc);
            const resB = computeFromRaw(B.att, B.comp, B.yards, B.td, B.intc);

            let text = `Player A: ${resA.rating.toFixed(
              1
            )} | Player B: ${resB.rating.toFixed(1)}. `;
            if (resA.rating > resB.rating) {
              text += "Player A has the higher passer rating.";
            } else if (resB.rating > resA.rating) {
              text += "Player B has the higher passer rating.";
            } else {
              text += "Both players have the same passer rating.";
            }
            document.getElementById("cmp-result").textContent = text;
          });

        // Initial calculation
        updateMainFromRaw();
      });
    </script>

