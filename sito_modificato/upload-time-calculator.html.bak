<script>
  // Helper: unit multipliers (binary for storage, decimal for speeds)
  const sizeUnitToBytes = {
    KB: 1024,
    MB: 1024 * 1024,
    GB: 1024 * 1024 * 1024,
    TB: 1024 * 1024 * 1024 * 1024
  };

  const speedUnitToBps = {
    kbps: 1000,
    Mbps: 1000 * 1000,
    Gbps: 1000 * 1000 * 1000,
    kBps: 1000 * 8,
    MBps: 1000 * 1000 * 8,
    GBps: 1000 * 1000 * 1000 * 8
  };

  function formatDuration(seconds) {
    if (!isFinite(seconds) || seconds < 0) return '—';
    const s = Math.round(seconds);
    const days = Math.floor(s / 86400);
    const hours = Math.floor((s % 86400) / 3600);
    const minutes = Math.floor((s % 3600) / 60);
    const secs = s % 60;

    const parts = [];
    if (days) parts.push(days + ' day' + (days !== 1 ? 's' : ''));
    if (hours) parts.push(hours + ' hour' + (hours !== 1 ? 's' : ''));
    if (minutes) parts.push(minutes + ' min');
    if (!parts.length || secs) parts.push(secs + ' s');

    return parts.join(' ');
  }

  function formatBitsPerSecond(bps) {
    if (!isFinite(bps) || bps <= 0) return '—';
    const units = ['bps', 'kbps', 'Mbps', 'Gbps', 'Tbps'];
    let i = 0;
    let value = bps;
    while (value >= 1000 && i < units.length - 1) {
      value / 1000;
      i++;
    }
    return value.toFixed(2) + ' ' + units[i];
  }

  function formatBytes(bytes) {
    if (!isFinite(bytes) || bytes < 0) return '—';
    const units = ['B', 'KB', 'MB', 'GB', 'TB'];
    let i = 0;
    let value = bytes;
    while (value >= 1024 && i < units.length - 1) {
      value / 1024;
      i++;
    }
    return value.toFixed(2) + ' ' + units[i];
  }

  function recalcMultiTotal() {
    const rows = document.querySelectorAll('#file-rows tr');
    let totalBytes = 0;
    rows.forEach(row => {
      const sizeInput = row.querySelector('.file-size-input');
      const unitSelect = row.querySelector('.file-size-unit');
      const val = parseFloat(sizeInput.value);
      if (!isNaN(val) && val > 0) {
        const unit = unitSelect.value;
        totalBytes += val * (sizeUnitToBytes[unit] || 0);
      }
    });

    const totalDisplay = document.getElementById('multi-total-size');
    const unitDisplay = document.getElementById('multi-total-unit');

    if (totalBytes === 0) {
      totalDisplay.textContent = '0';
      unitDisplay.textContent = 'MB';
      return 0;
    }

    // Choose a nice unit for display only
    let value = totalBytes;
    let unitIndex = 0;
    const units = ['B', 'KB', 'MB', 'GB', 'TB'];
    while (value >= 1024 && unitIndex < units.length - 1) {
      value / 1024;
      unitIndex++;
    }
    totalDisplay.textContent = value.toFixed(2);
    unitDisplay.textContent = units[unitIndex];

    return totalBytes;
  }

  document.addEventListener('DOMContentLoaded', () => {
    const efficiencyInput = document.getElementById('efficiency');
    const efficiencyLabel = document.getElementById('efficiency-label');
    const calculateBtn = document.getElementById('calculate-btn');
    const resetBtn = document.getElementById('reset-btn');
    const errorMessage = document.getElementById('error-message');
    const resultsCard = document.getElementById('results-card');

    const fileSizeInput = document.getElementById('file-size');
    const fileSizeUnit = document.getElementById('file-size-unit');
    const uploadSpeedInput = document.getElementById('upload-speed');
    const uploadSpeedUnit = document.getElementById('upload-speed-unit');

    const resultMain = document.getElementById('result-main');
    const resultSecondary = document.getElementById('result-secondary');
    const resultEffectiveSpeed = document.getElementById('result-effective-speed');
    const resultTotalData = document.getElementById('result-total-data');
    const resultSeconds = document.getElementById('result-seconds');
    const resultMinutes = document.getElementById('result-minutes');

    // Sync efficiency label
    efficiencyInput.addEventListener('input', () => {
      efficiencyLabel.textContent = efficiencyInput.value + '%';
    });

    // Multi-file table logic
    const fileRowsBody = document.getElementById('file-rows');
    const addFileRowBtn = document.getElementById('add-file-row');

    function renumberRows() {
      fileRowsBody.querySelectorAll('tr').forEach((row, index) => {
        const cell = row.querySelector('td:first-child');
        if (cell) cell.textContent = index + 1;
      });
    }

    addFileRowBtn.addEventListener('click', () => {
      const row = document.createElement('tr');
      row.innerHTML = `
        <td class="px-2 py-1 border-b"></td>
        <td class="px-2 py-1 border-b">
          <input type="number" min="0" step="0.0001" class="file-size-input w-24 border border-gray-300 rounded px-2 py-1 text-sm">
        </td>
        <td class="px-2 py-1 border-b">
          <select class="file-size-unit border border-gray-300 rounded px-1 py-1 text-sm bg-white">
            <option value="KB">KB</option>
            <option value="MB" selected>MB</option>
            <option value="GB">GB</option>
            <option value="TB">TB</option>
          </select>
        </td>
        <td class="px-2 py-1 border-b text-center">
          <button type="button" class="remove-row text-xs text-red-600 hover:underline">Delete</button>
        </td>
      `;
      fileRowsBody.appendChild(row);
      renumberRows();
    });

    fileRowsBody.addEventListener('click', (e) => {
      if (e.target.classList.contains('remove-row')) {
        const row = e.target.closest('tr');
        if (row && fileRowsBody.rows.length > 1) {
          row.remove();
          renumberRows();
          recalcMultiTotal();
        }
      }
    });

    fileRowsBody.addEventListener('input', (e) => {
      if (e.target.classList.contains('file-size-input') || e.target.classList.contains('file-size-unit')) {
        recalcMultiTotal();
      }
    });

    // Initial multi total
    recalcMultiTotal();

    function calculate() {
      errorMessage.textContent = '';
      resultsCard.classList.add('hidden');

      // 1) Determine total bytes: prefer multi-file table if > 0
      let totalBytesFromTable = recalcMultiTotal();
      let totalBytes;

      if (totalBytesFromTable > 0) {
        totalBytes = totalBytesFromTable;
      } else {
        const sizeVal = parseFloat(fileSizeInput.value);
        if (isNaN(sizeVal) || sizeVal <= 0) {
          errorMessage.textContent = 'Please enter a valid file size (or use the multi-file table).';
          return;
        }
        const sizeUnit = fileSizeUnit.value;
        const sizeMultiplier = sizeUnitToBytes[sizeUnit];
        if (!sizeMultiplier) {
          errorMessage.textContent = 'Unknown file size unit.';
          return;
        }
        totalBytes = sizeVal * sizeMultiplier;
      }

      // 2) Upload speed
      const speedVal = parseFloat(uploadSpeedInput.value);
      if (isNaN(speedVal) || speedVal <= 0) {
        errorMessage.textContent = 'Please enter a valid upload speed.';
        return;
      }
      const speedUnit = uploadSpeedUnit.value;
      const speedMultiplier = speedUnitToBps[speedUnit];
      if (!speedMultiplier) {
        errorMessage.textContent = 'Unknown upload speed unit.';
        return;
      }
      const rawBps = speedVal * speedMultiplier;

      // 3) Efficiency
      const effPercent = parseFloat(efficiencyInput.value);
      const eff = effPercent / 100;
      if (eff <= 0 || eff > 1.2) {
        errorMessage.textContent = 'Please choose a realistic efficiency between 40% and 100%.';
        return;
      }
      const effectiveBps = rawBps * eff;

      if (effectiveBps <= 0) {
        errorMessage.textContent = 'Effective upload speed must be greater than zero.';
        return;
      }

      // 4) Compute time
      const totalBits = totalBytes * 8;
      const seconds = totalBits / effectiveBps;

      // 5) Populate results
      resultMain.textContent = formatDuration(seconds);
      resultSecondary.textContent = `≈ ${Math.round(seconds)} seconds (${(seconds / 60).toFixed(2)} minutes)`;
      resultEffectiveSpeed.textContent = formatBitsPerSecond(effectiveBps);
      resultTotalData.textContent = formatBytes(totalBytes) + ' (' + totalBits.toLocaleString() + ' bits)';
      resultSeconds.textContent = Math.round(seconds).toLocaleString() + ' s';
      resultMinutes.textContent = (seconds / 60).toFixed(2) + ' min';

      resultsCard.classList.remove('hidden');
    }

    calculateBtn.addEventListener('click', calculate);

    resetBtn.addEventListener('click', () => {
      fileSizeInput.value = '';
      fileSizeUnit.value = 'MB';
      uploadSpeedInput.value = '';
      uploadSpeedUnit.value = 'Mbps';
      efficiencyInput.value = 85;
      efficiencyLabel.textContent = '85%';
      errorMessage.textContent = '';
      resultsCard.classList.add('hidden');

      // Reset multi-file table to a single empty row
      fileRowsBody.innerHTML = `
        <tr>
          <td class="px-2 py-1 border-b">1</td>
          <td class="px-2 py-1 border-b">
            <input type="number" min="0" step="0.0001" class="file-size-input w-24 border border-gray-300 rounded px-2 py-1 text-sm">
          </td>
          <td class="px-2 py-1 border-b">
            <select class="file-size-unit border border-gray-300 rounded px-1 py-1 text-sm bg-white">
              <option value="KB">KB</option>
              <option value="MB" selected>MB</option>
              <option value="GB">GB</option>
              <option value="TB">TB</option>
            </select>
          </td>
          <td class="px-2 py-1 border-b text-center">
            <button type="button" class="remove-row text-xs text-red-600 hover:underline">Delete</button>
          </td>
        </tr>
      `;
      recalcMultiTotal();
    });
  });

  window.MathJax = {
    tex: { inlineMath: [['\\(','\\)'], ['$', '$']], displayMath: [['$','$'], ['\\[','\\]']] },
    svg: { fontCache: 'global' }
  };
</script>
<script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>

