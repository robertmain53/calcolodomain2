    <script>
      // PPP calculator logic
      function parsePositive(id) {
        const v = parseFloat(
          document.getElementById(id).value.replace(",", ".")
        );
        return isFinite(v) && v > 0 ? v : null;
      }

      function formatNumber(x, decimals = 4) {
        if (!isFinite(x)) return "";
        return x.toLocaleString(undefined, {
          minimumFractionDigits: decimals,
          maximumFractionDigits: decimals,
        });
      }

      function formatMoney(x, decimals = 2) {
        if (!isFinite(x)) return "";
        return x.toLocaleString(undefined, {
          minimumFractionDigits: decimals,
          maximumFractionDigits: decimals,
        });
      }

      document
        .getElementById("ppp-calc-btn")
        .addEventListener("click", function () {
          const errEl = document.getElementById("ppp-error");
          errEl.textContent = "";

          const homeCountry =
            document.getElementById("home-country").value.trim() || "Home";
          const foreignCountry =
            document.getElementById("foreign-country").value.trim() ||
            "Foreign";

          const PH = parsePositive("home-price-level");
          const PF = parsePositive("foreign-price-level");
          let PPP = parsePositive("ppp-rate");
          const E = parsePositive("market-rate");

          let Ahome = parsePositive("amount-home");
          let Afor = parsePositive("amount-foreign");

          if (!PPP) {
            if (!PH || !PF) {
              errEl.textContent =
                "Enter either both price levels or a PPP exchange rate.";
              document.getElementById("ppp-results").classList.add("hidden");
              return;
            }
            PPP = PF / PH;
          }

          if (!E) {
            errEl.textContent =
              "Please enter the current market exchange rate.";
            document.getElementById("ppp-results").classList.add("hidden");
            return;
          }

          // Derive missing amount using market rate
          if (Ahome && !Afor) {
            Afor = Ahome * E;
          } else if (!Ahome && Afor) {
            Ahome = Afor / E;
          }

          // Misalignment
          const misalignment = ((E - PPP) / PPP) * 100;
          const direction =
            misalignment > 0
              ? " (home currency overvalued vs PPP)"
              : misalignment < 0
              ? " (home currency undervalued vs PPP)"
              : " (aligned with PPP)";

          // Real exchange rate (if price levels available)
          let RER = null;
          if (PH && PF) {
            RER = E * (PH / PF);
          }

          // Conversions
          let marketConv = null;
          let pppConv = null;
          let diff = null;
          let convLabel = "";

          if (Ahome) {
            marketConv = Ahome * E;
            pppConv = Ahome * PPP;
            diff = pppConv - marketConv;
            convLabel = `${formatMoney(
              Ahome
            )} in ${homeCountry} converted to ${foreignCountry}`;
          } else if (Afor) {
            marketConv = Afor / E;
            pppConv = Afor / PPP;
            diff = pppConv - marketConv;
            convLabel = `${formatMoney(
              Afor
            )} in ${foreignCountry} converted to ${homeCountry}`;
          }

          // Populate outputs
          document.getElementById("ppp-rate-out").textContent = formatNumber(
            PPP,
            4
          );
          document.getElementById(
            "ppp-rate-label"
          ).textContent = `Implied PPP: 1 unit of ${homeCountry} should buy ${formatNumber(
            PPP,
            4
          )} units of ${foreignCountry}.`;

          document.getElementById("market-rate-out").textContent = formatNumber(
            E,
            4
          );
          document.getElementById("misalignment-out").textContent =
            (misalignment >= 0 ? "+" : "") +
            formatNumber(misalignment, 2) +
            "%";
          document.getElementById("misalignment-direction").textContent =
            direction;

          if (RER !== null) {
            document.getElementById("rer-out").textContent = formatNumber(
              RER,
              4
            );
          } else {
            document.getElementById("rer-out").textContent = "—";
          }

          if (marketConv !== null && pppConv !== null) {
            const sign = diff >= 0 ? "+" : "−";
            const absDiff = Math.abs(diff);
            document.getElementById(
              "market-conversion-out"
            ).textContent = `${convLabel} at market rate: ${formatMoney(
              marketConv
            )}`;
            document.getElementById(
              "ppp-conversion-out"
            ).textContent = `${convLabel} at PPP rate: ${formatMoney(pppConv)}`;
            document.getElementById(
              "ppp-diff-out"
            ).textContent = `${sign}${formatMoney(
              absDiff
            )} (${sign}${formatNumber(
              (absDiff / Math.abs(marketConv || pppConv)) * 100,
              2
            )}%) vs market conversion`;
          } else {
            document.getElementById("market-conversion-out").textContent =
              "Enter an amount to see conversions.";
            document.getElementById("ppp-conversion-out").textContent = "";
            document.getElementById("ppp-diff-out").textContent = "";
          }

          // Interpretation
          let interp = "";
          if (misalignment > 5) {
            interp += `The home currency appears significantly overvalued (about ${formatNumber(
              misalignment,
              1
            )}%) relative to PPP. `;
            interp += `Goods and services in ${homeCountry} are relatively expensive compared with ${foreignCountry} when measured in a common purchasing power unit. `;
          } else if (misalignment < -5) {
            interp += `The home currency appears significantly undervalued (about ${formatNumber(
              Math.abs(misalignment),
              1
            )}%) relative to PPP. `;
            interp += `On a PPP basis, incomes in ${homeCountry} buy more goods and services than the market FX rate alone would suggest. `;
          } else {
            interp += `The market exchange rate is close to the PPP benchmark (misalignment around ${formatNumber(
              misalignment,
              1
            )}%). `;
            interp += `There is no strong signal of overvaluation or undervaluation based on these inputs. `;
          }

          if (RER !== null) {
            if (RER > 1.05) {
              interp += `The real exchange rate (RER ≈ ${formatNumber(
                RER,
                2
              )}) indicates that home‑country goods are relatively expensive.`;
            } else if (RER < 0.95) {
              interp += `The real exchange rate (RER ≈ ${formatNumber(
                RER,
                2
              )}) indicates that home‑country goods are relatively cheap.`;
            } else {
              interp += `The real exchange rate (RER ≈ ${formatNumber(
                RER,
                2
              )}) is close to 1, suggesting broadly similar price levels.`;
            }
          }

          document.getElementById("ppp-interpretation").textContent = interp;

          document.getElementById("ppp-results").classList.remove("hidden");
        });

      document
        .getElementById("ppp-reset-btn")
        .addEventListener("click", function () {
          [
            "home-country",
            "foreign-country",
            "home-price-level",
            "foreign-price-level",
            "ppp-rate",
            "market-rate",
            "amount-home",
            "amount-foreign",
          ].forEach((id) => (document.getElementById(id).value = ""));
          document.getElementById("ppp-error").textContent = "";
          document.getElementById("ppp-results").classList.add("hidden");
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
