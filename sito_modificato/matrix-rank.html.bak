  <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>

  <script>
    // Mobile menu toggle
    document.getElementById('mobile-menu-toggle')?.addEventListener('click', function () {
      const menu = document.getElementById('mobile-menu');
      if (!menu) return;
      const isHidden = menu.classList.contains('hidden');
      menu.classList.toggle('hidden', !isHidden);
      this.setAttribute('aria-expanded', String(isHidden));
    });

    document.addEventListener('DOMContentLoaded', function () {
      const rowsInput = document.getElementById('mr-rows');
      const colsInput = document.getElementById('mr-cols');
      const updateGridBtn = document.getElementById('mr-update-grid');
      const matrixContainer = document.getElementById('mr-matrix-container');
      const calcBtn = document.getElementById('mr-calc');
      const clearBtn = document.getElementById('mr-clear');
      const errorEl = document.getElementById('mr-error');

      const rankBox = document.getElementById('mr-res-rank');
      const nullityBox = document.getElementById('mr-res-nullity');
      const rowcolBox = document.getElementById('mr-res-rowcol');
      const squareBox = document.getElementById('mr-res-square');

      const rrefWrapper = document.getElementById('mr-rref-wrapper');
      const rrefLabel = document.getElementById('mr-rref-label');
      const rrefContainer = document.getElementById('mr-rref-container');

      const exampleButtons = document.querySelectorAll('[data-example]');

      const TOL = 1e-10;
      let currentRows = 3;
      let currentCols = 3;

      function setBoxValue(box, text) {
        if (!box) return;
        const span = box.querySelector('.result-value');
        if (span) span.textContent = text;
      }

      function clearResults() {
        setBoxValue(rankBox, '—');
        setBoxValue(nullityBox, '—');
        setBoxValue(rowcolBox, '—');
        setBoxValue(squareBox, '—');
        if (rrefLabel) rrefLabel.textContent = 'RREF(A) will appear here after computation.';
        if (rrefContainer) rrefContainer.innerHTML = '';
        if (errorEl) errorEl.textContent = '';
      }

      function clampInt(value, min, max, fallback) {
        const n = Number(value);
        if (!Number.isFinite(n)) return fallback;
        const i = Math.round(n);
        if (i < min) return min;
        if (i > max) return max;
        return i;
      }

      function createMatrixGrid(rows, cols) {
        currentRows = rows;
        currentCols = cols;
        matrixContainer.innerHTML = '';

        const table = document.createElement('table');
        table.className = 'matrix-grid';

        for (let i = 0; i < rows; i++) {
          const tr = document.createElement('tr');
          for (let j = 0; j < cols; j++) {
            const td = document.createElement('td');
            const input = document.createElement('input');
            input.type = 'number';
            input.step = 'any';
            input.className = 'matrix-input rounded border border-gray-300 px-1 py-0.5 text-xs focus:ring-blue-500 focus:border-blue-500';
            input.setAttribute('data-row', String(i));
            input.setAttribute('data-col', String(j));
            td.appendChild(input);
            tr.appendChild(td);
          }
          table.appendChild(tr);
        }

        matrixContainer.appendChild(table);
      }

      function getMatrix() {
        const A = [];
        for (let i = 0; i < currentRows; i++) {
          const row = [];
          for (let j = 0; j < currentCols; j++) {
            const input = matrixContainer.querySelector('input[data-row="' + i + '"][data-col="' + j + '"]');
            let val = 0;
            if (input && input.value.trim() !== '') {
              const num = Number(input.value);
              val = Number.isFinite(num) ? num : 0;
            }
            row.push(val);
          }
          A.push(row);
        }
        return A;
      }

      function gaussianRankAndRREF(A) {
        const m = A.length;
        const n = m ? A[0].length : 0;
        const R = [];
        for (let i = 0; i < m; i++) {
          R[i] = A[i].slice();
        }

        let rank = 0;
        let row = 0;
        const pivots = [];

        for (let col = 0; col < n && row < m; col++) {
          // Find pivot row
          let pivotRow = -1;
          let maxAbs = TOL;
          for (let i = row; i < m; i++) {
            const val = Math.abs(R[i][col]);
            if (val > maxAbs) {
              maxAbs = val;
              pivotRow = i;
            }
          }

          if (pivotRow === -1) {
            // no pivot in this column
            continue;
          }

          // Swap rows if needed
          if (pivotRow !== row) {
            const tmp = R[row];
            R[row] = R[pivotRow];
            R[pivotRow] = tmp;
          }

          // Normalize pivot row
          const pivot = R[row][col];
          if (Math.abs(pivot) > TOL) {
            for (let j = col; j < n; j++) {
              R[row][j] / pivot;
            }
          }

          // Eliminate below
          for (let i = row + 1; i < m; i++) {
            const factor = R[i][col];
            if (Math.abs(factor) > TOL) {
              for (let j = col; j < n; j++) {
                R[i][j] -= factor * R[row][j];
              }
            }
          }

          pivots.push({ row: row, col: col });
          rank++;
          row++;
        }

        // Backward elimination to get RREF
        for (let p = pivots.length - 1; p >= 0; p--) {
          const pivotInfo = pivots[p];
          const r = pivotInfo.row;
          const c = pivotInfo.col;
          for (let i = 0; i < r; i++) {
            const factor = R[i][c];
            if (Math.abs(factor) > TOL) {
              for (let j = c; j < n; j++) {
                R[i][j] -= factor * R[r][j];
              }
            }
          }
        }

        // Clean very small numbers
        for (let i = 0; i < m; i++) {
          for (let j = 0; j < n; j++) {
            if (Math.abs(R[i][j]) < TOL) {
              R[i][j] = 0;
            }
          }
        }

        return { rank: rank, rref: R };
      }

      function renderRREF(R) {
        const m = R.length;
        const n = m ? R[0].length : 0;
        rrefContainer.innerHTML = '';

        if (!m || !n) {
          rrefLabel.textContent = 'RREF(A) is empty.';
          return;
        }

        const table = document.createElement('table');
        table.className = 'ref-table';

        for (let i = 0; i < m; i++) {
          const tr = document.createElement('tr');
          for (let j = 0; j < n; j++) {
            const td = document.createElement('td');
            const val = R[i][j];
            const display = Math.abs(val) < TOL ? '0' : val.toFixed(4).replace(/\.?0+$/, '');
            td.textContent = display;
            td.className = 'text-right text-gray-800';
            tr.appendChild(td);
          }
          table.appendChild(tr);
        }

        rrefContainer.appendChild(table);
        rrefLabel.textContent = 'RREF(A) computed: each non zero row corresponds to an independent row.';
      }

      function computeRank() {
        clearResults();

        const A = getMatrix();
        const m = A.length;
        const n = m ? A[0].length : 0;

        let allZero = true;
        outer: for (let i = 0; i < m; i++) {
          for (let j = 0; j < n; j++) {
            if (Math.abs(A[i][j]) > TOL) {
              allZero = false;
              break outer;
            }
          }
        }
        if (allZero) {
          setBoxValue(rankBox, '0');
          setBoxValue(nullityBox, String(n));
          setBoxValue(rowcolBox, 'All zero matrix');
          if (m === n) {
            setBoxValue(squareBox, 'Singular (rank 0)');
          } else {
            setBoxValue(squareBox, 'Non square');
          }
          rrefLabel.textContent = 'RREF(A) is the zero matrix.';
          renderRREF(A);
          return;
        }

        try {
          const { rank, rref } = gaussianRankAndRREF(A);
          const nullity = n - rank;

          setBoxValue(rankBox, String(rank));
          setBoxValue(nullityBox, String(nullity));

          let rowcolDesc = [];
          if (rank === m) rowcolDesc.push('full row rank');
          if (rank === n) rowcolDesc.push('full column rank');
          if (!rowcolDesc.length) rowcolDesc.push('not full rank');
          setBoxValue(rowcolBox, rowcolDesc.join(', '));

          if (m === n) {
            if (rank === n) {
              setBoxValue(squareBox, 'Full rank, invertible (det ≠ 0)');
            } else {
              setBoxValue(squareBox, 'Rank < n, singular (det = 0)');
            }
          } else {
            setBoxValue(squareBox, 'Matrix is not square');
          }

          renderRREF(rref);
        } catch (e) {
          errorEl.textContent = 'Error while computing rank. Please check matrix entries.';
        }
      }

      function clearMatrix() {
        const inputs = matrixContainer.querySelectorAll('input[type="number"]');
        inputs.forEach(inp => { inp.value = ''; });
        clearResults();
      }

      function loadExample(name) {
        let rows = 3;
        let cols = 3;
        let data = [];

        if (name === 'full-rank-3x3') {
          rows = 3; cols = 3;
          data = [
            [1, 2, 3],
            [0, 1, 4],
            [2, -1, 1]
          ];
        } else if (name === 'rank-2-3x3') {
          rows = 3; cols = 3;
          data = [
            [1, 2, 3],
            [2, 4, 6],
            [1, 0, 1]
          ];
        } else if (name === 'wide-rank') {
          rows = 2; cols = 4;
          data = [
            [1, 0, 1, 0],
            [0, 1, 0, 1]
          ];
        }

        rowsInput.value = String(rows);
        colsInput.value = String(cols);
        createMatrixGrid(rows, cols);

        for (let i = 0; i < rows; i++) {
          for (let j = 0; j < cols; j++) {
            const input = matrixContainer.querySelector('input[data-row="' + i + '"][data-col="' + j + '"]');
            if (input) {
              const val = data[i] && data[i][j] != null ? data[i][j] : 0;
              input.value = String(val);
            }
          }
        }
        clearResults();
      }

      // Initialise grid
      createMatrixGrid(currentRows, currentCols);

      if (updateGridBtn) {
        updateGridBtn.addEventListener('click', function () {
          const r = clampInt(rowsInput.value, 1, 8, 3);
          const c = clampInt(colsInput.value, 1, 8, 3);
          rowsInput.value = String(r);
          colsInput.value = String(c);
          createMatrixGrid(r, c);
          clearResults();
        });
      }

      if (calcBtn) calcBtn.addEventListener('click', computeRank);
      if (clearBtn) clearBtn.addEventListener('click', clearMatrix);

      exampleButtons.forEach(btn => {
        btn.addEventListener('click', function () {
          const name = btn.getAttribute('data-example');
          if (name) loadExample(name);
        });
      });
    });
  </script>

