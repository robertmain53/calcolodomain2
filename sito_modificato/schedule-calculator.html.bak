    <script>
      // Utility functions
      function pad2(n) {
        return n.toString().padStart(2, "0");
      }

      function parseTimeToMinutes(str, format12) {
        if (!str) return null;
        str = str.trim().toLowerCase();
        let ampm = null;
        if (format12) {
          if (str.endsWith("am") || str.endsWith("pm")) {
            ampm = str.slice(-2);
            str = str.slice(0, -2).trim();
          }
        }
        const parts = str.split(":");
        if (parts.length !== 2) return null;
        let h = parseInt(parts[0], 10);
        let m = parseInt(parts[1], 10);
        if (isNaN(h) || isNaN(m) || m < 0 || m >= 60) return null;
        if (format12) {
          if (h === 12) h = 0;
          if (ampm === "pm") h += 12;
        }
        if (h < 0 || h >= 24) return null;
        return h * 60 + m;
      }

      function minutesToDisplay(mins) {
        const h = Math.floor(mins / 60);
        const m = Math.round(mins % 60);
        return h + ":" + pad2(m);
      }

      function roundMinutes(mins, step) {
        if (step === "none") return mins;
        const s = parseInt(step, 10);
        return Math.round(mins / s) * s;
      }

      // State
      const daysOfWeek = [
        "Sunday",
        "Monday",
        "Tuesday",
        "Wednesday",
        "Thursday",
        "Friday",
        "Saturday",
      ];
      let scheduleData = [];
      let selectedDayIndex = 0;

      function initSchedule() {
        scheduleData = [];
        const weekStart = parseInt(
          document.getElementById("weekStart").value,
          10
        );
        for (let i = 0; i < 7; i++) {
          const dayIndex = (weekStart + i) % 7;
          scheduleData.push({
            label: daysOfWeek[dayIndex],
            shifts: [{ start: "09:00", end: "17:00", breakMinutes: 60 }],
          });
        }
        selectedDayIndex = 0;
        renderScheduleTable();
        recalcTotals();
      }

      function renderScheduleTable() {
        const tbody = document.getElementById("scheduleBody");
        tbody.innerHTML = "";
        const timeFormat = document.querySelector(
          'input[name="timeFormat"]:checked'
        ).value;

        scheduleData.forEach((day, dIdx) => {
          if (day.shifts.length === 0) {
            const tr = document.createElement("tr");
            tr.className = "border-t";
            tr.innerHTML = `
          <td class="px-3 py-2 align-top">
            <button type="button" data-day="${dIdx}" class="day-select text-left font-medium ${
              dIdx === selectedDayIndex ? "text-blue-600" : "text-gray-800"
            }">${day.label}</button>
          </td>
          <td class="px-3 py-2 text-gray-400" colspan="5">No shifts</td>
          <td class="px-3 py-2 text-right">
            <button type="button" data-day="${dIdx}" class="text-xs text-blue-600 hover:underline add-shift-inline">Add shift</button>
          </td>
        `;
            tbody.appendChild(tr);
            return;
          }

          day.shifts.forEach((shift, sIdx) => {
            const tr = document.createElement("tr");
            tr.className = "border-t";
            const shiftLabel = "Shift " + (sIdx + 1);
            const shiftMinutes = computeShiftMinutes(shift, timeFormat);
            tr.innerHTML = `
          <td class="px-3 py-2 align-top ${sIdx === 0 ? "" : "border-t-0"}">
            ${
              sIdx === 0
                ? `<button type="button" data-day="${dIdx}" class="day-select text-left font-medium ${
                    dIdx === selectedDayIndex
                      ? "text-blue-600"
                      : "text-gray-800"
                  }">${day.label}</button>`
                : ""
            }
          </td>
          <td class="px-3 py-2 align-top">${shiftLabel}</td>
          <td class="px-3 py-2 align-top">
            <input type="text" data-day="${dIdx}" data-shift="${sIdx}" data-field="start" value="${
              shift.start
            }" class="time-input border-gray-300 rounded-md shadow-sm focus:ring-blue-500 focus:border-blue-500 px-2 py-1 text-sm" placeholder="${
              timeFormat === "12" ? "9:00 AM" : "09:00"
            }">
          </td>
          <td class="px-3 py-2 align-top">
            <input type="text" data-day="${dIdx}" data-shift="${sIdx}" data-field="end" value="${
              shift.end
            }" class="time-input border-gray-300 rounded-md shadow-sm focus:ring-blue-500 focus:border-blue-500 px-2 py-1 text-sm" placeholder="${
              timeFormat === "12" ? "5:00 PM" : "17:00"
            }">
          </td>
          <td class="px-3 py-2 align-top">
            <input type="number" min="0" step="1" data-day="${dIdx}" data-shift="${sIdx}" data-field="breakMinutes" value="${
              shift.breakMinutes
            }" class="w-20 border-gray-300 rounded-md shadow-sm focus:ring-blue-500 focus:border-blue-500 px-2 py-1 text-sm">
          </td>
          <td class="px-3 py-2 align-top">
            <span class="shift-hours text-gray-800">${
              shiftMinutes !== null ? minutesToDisplay(shiftMinutes) : "–"
            }</span>
          </td>
          <td class="px-3 py-2 align-top text-right">
            <button type="button" data-day="${dIdx}" data-shift="${sIdx}" class="text-xs text-red-600 hover:underline remove-shift">Remove</button>
          </td>
        `;
            tbody.appendChild(tr);
          });
        });

        attachTableEvents();
      }

      function computeShiftMinutes(shift, timeFormat) {
        const format12 = timeFormat === "12";
        const startM = parseTimeToMinutes(shift.start, format12);
        const endM = parseTimeToMinutes(shift.end, format12);
        if (startM === null || endM === null) return null;
        let diff = endM - startM;
        if (diff < 0) diff += 24 * 60; // overnight
        const rounding = document.getElementById("rounding").value;
        diff = roundMinutes(diff, rounding === "none" ? "none" : rounding);
        const breakMin = parseInt(shift.breakMinutes, 10) || 0;
        const net = Math.max(0, diff - breakMin);
        return net;
      }

      function recalcTotals() {
        const timeFormat = document.querySelector(
          'input[name="timeFormat"]:checked'
        ).value;
        const regularPerDay =
          parseFloat(document.getElementById("regularPerDay").value) || 0;
        let totalMinutes = 0;
        let totalRegular = 0;
        let totalOvertime = 0;
        let daysWithHours = 0;

        scheduleData.forEach((day) => {
          let dayMinutes = 0;
          day.shifts.forEach((shift) => {
            const m = computeShiftMinutes(shift, timeFormat);
            if (m !== null) dayMinutes += m;
          });
          if (dayMinutes > 0) {
            daysWithHours++;
            const regularMinutes = Math.min(dayMinutes, regularPerDay * 60);
            const overtimeMinutes = Math.max(0, dayMinutes - regularMinutes);
            totalMinutes += dayMinutes;
            totalRegular += regularMinutes;
            totalOvertime += overtimeMinutes;
          }
        });

        document.getElementById("totalHours").textContent =
          minutesToDisplay(totalMinutes);
        document.getElementById("regularHours").textContent =
          minutesToDisplay(totalRegular);
        document.getElementById("overtimeHours").textContent =
          minutesToDisplay(totalOvertime);
        document.getElementById("daysCount").textContent = daysWithHours;
      }

      function attachTableEvents() {
        const tbody = document.getElementById("scheduleBody");

        tbody.querySelectorAll(".day-select").forEach((btn) => {
          btn.addEventListener("click", (e) => {
            selectedDayIndex = parseInt(
              e.currentTarget.getAttribute("data-day"),
              10
            );
            renderScheduleTable();
            recalcTotals();
          });
        });

        tbody.querySelectorAll("input[data-field]").forEach((input) => {
          input.addEventListener("change", (e) => {
            const el = e.currentTarget;
            const dayIdx = parseInt(el.getAttribute("data-day"), 10);
            const shiftIdx = parseInt(el.getAttribute("data-shift"), 10);
            const field = el.getAttribute("data-field");
            if (field === "breakMinutes") {
              let v = parseInt(el.value, 10);
              if (isNaN(v) || v < 0) v = 0;
              scheduleData[dayIdx].shifts[shiftIdx][field] = v;
              el.value = v;
            } else {
              scheduleData[dayIdx].shifts[shiftIdx][field] = el.value;
            }
            renderScheduleTable();
            recalcTotals();
          });
        });

        tbody.querySelectorAll(".remove-shift").forEach((btn) => {
          btn.addEventListener("click", (e) => {
            const dayIdx = parseInt(
              e.currentTarget.getAttribute("data-day"),
              10
            );
            const shiftIdx = parseInt(
              e.currentTarget.getAttribute("data-shift"),
              10
            );
            scheduleData[dayIdx].shifts.splice(shiftIdx, 1);
            renderScheduleTable();
            recalcTotals();
          });
        });

        tbody.querySelectorAll(".add-shift-inline").forEach((btn) => {
          btn.addEventListener("click", (e) => {
            const dayIdx = parseInt(
              e.currentTarget.getAttribute("data-day"),
              10
            );
            addShiftToDay(dayIdx);
          });
        });
      }

      function addShiftToDay(dayIdx) {
        const day = scheduleData[dayIdx];
        const lastShift = day.shifts[day.shifts.length - 1];
        let newStart = lastShift ? lastShift.end : "09:00";
        let newEnd = lastShift ? lastShift.end : "17:00";
        day.shifts.push({ start: newStart, end: newEnd, breakMinutes: 0 });
        selectedDayIndex = dayIdx;
        renderScheduleTable();
        recalcTotals();
      }

      document.addEventListener("DOMContentLoaded", () => {
        initSchedule();

        document
          .getElementById("weekStart")
          .addEventListener("change", initSchedule);
        document.querySelectorAll('input[name="timeFormat"]').forEach((r) => {
          r.addEventListener("change", () => {
            renderScheduleTable();
            recalcTotals();
          });
        });
        document.getElementById("rounding").addEventListener("change", () => {
          renderScheduleTable();
          recalcTotals();
        });
        document
          .getElementById("regularPerDay")
          .addEventListener("change", recalcTotals);

        document.getElementById("addDayBtn").addEventListener("click", () => {
          const newDayLabel = prompt(
            "Custom day label (e.g., Day 8 or Holiday):",
            "Extra Day"
          );
          if (!newDayLabel) return;
          scheduleData.push({
            label: newDayLabel,
            shifts: [{ start: "09:00", end: "17:00", breakMinutes: 60 }],
          });
          selectedDayIndex = scheduleData.length - 1;
          renderScheduleTable();
          recalcTotals();
        });

        document.getElementById("addShiftBtn").addEventListener("click", () => {
          addShiftToDay(selectedDayIndex);
        });

        document
          .getElementById("resetWeekBtn")
          .addEventListener("click", () => {
            if (confirm("Clear all shifts and reset the week?")) {
              initSchedule();
            }
          });

        document
          .getElementById("copySummaryBtn")
          .addEventListener("click", () => {
            const text = buildSummaryText();
            navigator.clipboard
              .writeText(text)
              .then(() => {
                showExportStatus("Summary copied to clipboard.");
              })
              .catch(() => {
                showExportStatus("Unable to copy. Please copy manually.", true);
              });
          });

        document
          .getElementById("downloadCsvBtn")
          .addEventListener("click", () => {
            const csv = buildCsv();
            const blob = new Blob([csv], { type: "text/csv;charset=utf-8;" });
            const url = URL.createObjectURL(blob);
            const a = document.createElement("a");
            a.href = url;
            a.download = "schedule.csv";
            document.body.appendChild(a);
            a.click();
            document.body.removeChild(a);
            URL.revokeObjectURL(url);
          });

        document.getElementById("printBtn").addEventListener("click", () => {
          window.print();
        });

        // Simple FAQ toggle (in addition to global script_faq if present)
        document.querySelectorAll("#faq .faq-question").forEach((btn) => {
          btn.addEventListener("click", () => {
            const answer = btn.parentElement.querySelector(".faq-answer");
            const icon = btn.querySelector("span:last-child");
            const isHidden = answer.classList.contains("hidden");
            document
              .querySelectorAll("#faq .faq-answer")
              .forEach((a) => a.classList.add("hidden"));
            document
              .querySelectorAll("#faq .faq-question span:last-child")
              .forEach((i) => (i.textContent = "▼"));
            if (isHidden) {
              answer.classList.remove("hidden");
              icon.textContent = "▲";
            }
          });
        });
      });

      function buildSummaryText() {
        const timeFormat = document.querySelector(
          'input[name="timeFormat"]:checked'
        ).value;
        const regularPerDay =
          parseFloat(document.getElementById("regularPerDay").value) || 0;
        let lines = [];
        lines.push("Schedule summary");
        lines.push("================");
        scheduleData.forEach((day) => {
          let dayMinutes = 0;
          day.shifts.forEach((shift) => {
            const m = computeShiftMinutes(shift, timeFormat);
            if (m !== null) dayMinutes += m;
          });
          if (dayMinutes === 0) return;
          const regularMinutes = Math.min(dayMinutes, regularPerDay * 60);
          const overtimeMinutes = Math.max(0, dayMinutes - regularMinutes);
          lines.push(
            `${day.label}: total ${minutesToDisplay(
              dayMinutes
            )} (regular ${minutesToDisplay(
              regularMinutes
            )}, overtime ${minutesToDisplay(overtimeMinutes)})`
          );
        });
        lines.push("");
        lines.push(`Generated at: ${new Date().toLocaleString()}`);
        return lines.join("\n");
      }

      function buildCsv() {
        const timeFormat = document.querySelector(
          'input[name="timeFormat"]:checked'
        ).value;
        const regularPerDay =
          parseFloat(document.getElementById("regularPerDay").value) || 0;
        let rows = [];
        rows.push(
          ["Day", "Shift #", "Start", "End", "Break (min)", "Hours"].join(",")
        );
        scheduleData.forEach((day) => {
          day.shifts.forEach((shift, idx) => {
            const m = computeShiftMinutes(shift, timeFormat);
            rows.push(
              [
                `"${day.label}"`,
                idx + 1,
                `"${shift.start}"`,
                `"${shift.end}"`,
                shift.breakMinutes,
                m !== null ? (m / 60).toFixed(2) : "",
              ].join(",")
            );
          });
        });
        // Totals
        let totalMinutes = 0;
        let totalRegular = 0;
        let totalOvertime = 0;
        scheduleData.forEach((day) => {
          let dayMinutes = 0;
          day.shifts.forEach((shift) => {
            const m = computeShiftMinutes(shift, timeFormat);
            if (m !== null) dayMinutes += m;
          });
          if (dayMinutes > 0) {
            const regularMinutes = Math.min(dayMinutes, regularPerDay * 60);
            const overtimeMinutes = Math.max(0, dayMinutes - regularMinutes);
            totalMinutes += dayMinutes;
            totalRegular += regularMinutes;
            totalOvertime += overtimeMinutes;
          }
        });
        rows.push("");
        rows.push(["Totals", "", "", "", "", ""].join(","));
        rows.push(
          ["Total hours", "", "", "", "", (totalMinutes / 60).toFixed(2)].join(
            ","
          )
        );
        rows.push(
          [
            "Regular hours",
            "",
            "",
            "",
            "",
            (totalRegular / 60).toFixed(2),
          ].join(",")
        );
        rows.push(
          [
            "Overtime hours",
            "",
            "",
            "",
            "",
            (totalOvertime / 60).toFixed(2),
          ].join(",")
        );
        return rows.join("\n");
      }

      function showExportStatus(msg, isError) {
        const el = document.getElementById("exportStatus");
        el.textContent = msg;
        el.className =
          "text-xs " + (isError ? "text-red-600" : "text-gray-500");
        setTimeout(() => {
          el.textContent = "";
        }, 4000);
      }

      // Mobile menu script (fallback if mobile-menu.js not yet applied)
      document.addEventListener("DOMContentLoaded", () => {
        const toggle = document.getElementById("mobile-menu-toggle");
        const menu = document.getElementById("mobile-menu");
        if (toggle && menu) {
          toggle.addEventListener("click", () => {
            const isHidden = menu.classList.contains("hidden");
            menu.classList.toggle("hidden");
            toggle.setAttribute("aria-expanded", isHidden ? "true" : "false");
          });
        }
      });

