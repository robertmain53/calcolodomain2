    <script>
      // Physical constants
      const C = 299792458; // m/s
      const G = 6.6743e-11; // m^3 kg^-1 s^-2
      const MSUN = 1.98847e30; // kg
      const MPC_IN_M = 3.0856775814913673e22; // meters
      const RAD2ARCSEC = 206264.80624709636;

      function E_z(z, OmegaM) {
        const Ol = 1 - OmegaM;
        return Math.sqrt(OmegaM * Math.pow(1 + z, 3) + Ol);
      }

      function comovingDistance(z, H0, OmegaM) {
        // H0 in km/s/Mpc -> convert to 1/s
        const H0_SI = (H0 * 1000) / MPC_IN_M;
        const n = 512;
        const dz = z / n;
        let sum = 0;
        for (let i = 0; i <= n; i++) {
          const zi = i * dz;
          const weight = i === 0 || i === n ? 1 : i % 2 === 0 ? 2 : 4;
          sum += weight / E_z(zi, OmegaM);
        }
        const integral = (dz / 3) * sum;
        return (C / H0_SI) * integral; // meters
      }

      function angularDiameterDistance(z, H0, OmegaM) {
        if (z <= 0) return 0;
        const Dc = comovingDistance(z, H0, OmegaM);
        return Dc / (1 + z);
      }

      function formatNumber(x, digits = 3) {
        if (!isFinite(x)) return "–";
        if (x === 0) return "0";
        const absx = Math.abs(x);
        if (absx >= 1e4 || absx < 1e-3) {
          return x.toExponential(digits - 1);
        }
        return x.toFixed(digits);
      }

      function computeLensing() {
        const massVal = parseFloat(document.getElementById("mass").value);
        const massUnit = document.getElementById("massUnit").value;
        const zL = parseFloat(document.getElementById("zL").value);
        const zS = parseFloat(document.getElementById("zS").value);
        const betaVal = parseFloat(document.getElementById("beta").value);
        const betaUnit = document.getElementById("betaUnit").value;
        const H0 = parseFloat(document.getElementById("H0").value);
        const OmegaM = parseFloat(document.getElementById("OmegaM").value);
        const errorMsg = document.getElementById("errorMsg");

        errorMsg.classList.add("hidden");
        errorMsg.textContent = "";

        if (!isFinite(massVal) || massVal <= 0) {
          errorMsg.textContent = "Please enter a positive lens mass.";
          errorMsg.classList.remove("hidden");
          return;
        }
        if (!isFinite(zL) || !isFinite(zS) || zL < 0 || zS <= 0 || zS <= zL) {
          errorMsg.textContent =
            "Source redshift must be greater than lens redshift, and both must be non‑negative.";
          errorMsg.classList.remove("hidden");
          return;
        }
        if (!isFinite(betaVal) || betaVal < 0) {
          errorMsg.textContent =
            "Source offset β must be a non‑negative number.";
          errorMsg.classList.remove("hidden");
          return;
        }
        if (
          !isFinite(H0) ||
          H0 <= 0 ||
          !isFinite(OmegaM) ||
          OmegaM <= 0 ||
          OmegaM >= 1
        ) {
          errorMsg.textContent =
            "Please enter a reasonable cosmology (0 < Ωm < 1, H0 > 0).";
          errorMsg.classList.remove("hidden");
          return;
        }

        let M_kg = massVal;
        if (massUnit === "Msun") {
          M_kg = massVal * MSUN;
        }

        // Distances
        const DL = angularDiameterDistance(zL, H0, OmegaM);
        const DS = angularDiameterDistance(zS, H0, OmegaM);
        const DcL = comovingDistance(zL, H0, OmegaM);
        const DcS = comovingDistance(zS, H0, OmegaM);
        const DLS = (DcS - DcL) / (1 + zS);

        // Einstein radius (radians)
        const thetaE2 = ((4 * G * M_kg) / (C * C)) * (DLS / (DL * DS));
        const thetaE = thetaE2 > 0 ? Math.sqrt(thetaE2) : 0;

        // Physical Einstein radius in lens plane
        const RE = thetaE * DL; // meters

        // Deflection angle at Einstein radius equals thetaE for point mass
        const alphaE = thetaE;

        // Source offset in radians
        let betaRad;
        if (betaUnit === "arcsec") {
          betaRad = betaVal / RAD2ARCSEC;
        } else {
          betaRad = betaVal * thetaE;
        }

        // Image positions and magnifications (if thetaE > 0 and beta > 0)
        let thetaPlus = NaN,
          thetaMinus = NaN;
        let muPlus = NaN,
          muMinus = NaN,
          muTot = NaN,
          imageSep = NaN;

        if (thetaE > 0 && betaRad >= 0) {
          const y = betaRad / thetaE;
          if (y === 0) {
            // Perfect alignment: Einstein ring
            thetaPlus = thetaE;
            thetaMinus = -thetaE;
            muPlus = Infinity;
            muMinus = Infinity;
            muTot = Infinity;
            imageSep = 2 * thetaE;
          } else {
            const sqrtTerm = Math.sqrt(y * y + 4);
            const xPlus = 0.5 * (y + sqrtTerm);
            const xMinus = 0.5 * (y - sqrtTerm);
            thetaPlus = xPlus * thetaE;
            thetaMinus = xMinus * thetaE;

            const mu_p = 0.5 * (1 + (y * y + 2) / (y * sqrtTerm));
            const mu_m = 0.5 * (1 - (y * y + 2) / (y * sqrtTerm));
            muPlus = mu_p;
            muMinus = mu_m;
            muTot = Math.abs(mu_p) + Math.abs(mu_m);
            imageSep = Math.abs(thetaPlus - thetaMinus);
          }
        }

        // Update outputs
        document.getElementById("thetaEArcsec").textContent =
          thetaE > 0 ? formatNumber(thetaE * RAD2ARCSEC, 4) + " arcsec" : "–";
        document.getElementById("thetaERad").textContent =
          thetaE > 0 ? formatNumber(thetaE, 4) + " rad" : "–";

        if (RE > 0) {
          const RE_kpc = RE / (MPC_IN_M * 1e-3);
          document.getElementById("REPhys").textContent =
            formatNumber(RE_kpc, 4) + " kpc";
        } else {
          document.getElementById("REPhys").textContent = "–";
        }

        document.getElementById("alphaE").textContent =
          thetaE > 0 ? formatNumber(alphaE * RAD2ARCSEC, 4) + " arcsec" : "–";

        // Distances in Gpc
        document.getElementById("DL").textContent =
          DL > 0 ? formatNumber(DL / (MPC_IN_M * 1e3), 4) + " Gpc" : "–";
        document.getElementById("DS").textContent =
          DS > 0 ? formatNumber(DS / (MPC_IN_M * 1e3), 4) + " Gpc" : "–";
        document.getElementById("DLS").textContent =
          DLS > 0 ? formatNumber(DLS / (MPC_IN_M * 1e3), 4) + " Gpc" : "–";

        if (isFinite(thetaPlus) && isFinite(thetaMinus)) {
          const tPlusArc = thetaPlus * RAD2ARCSEC;
          const tMinusArc = thetaMinus * RAD2ARCSEC;
          document.getElementById("thetaImages").textContent =
            formatNumber(tPlusArc, 4) +
            '", ' +
            formatNumber(tMinusArc, 4) +
            '"';
        } else if (thetaE > 0 && betaRad === 0) {
          document.getElementById("thetaImages").textContent =
            "Einstein ring at " + formatNumber(thetaE * RAD2ARCSEC, 4) + '"';
        } else {
          document.getElementById("thetaImages").textContent = "–";
        }

        document.getElementById("imageSep").textContent = isFinite(imageSep)
          ? formatNumber(imageSep * RAD2ARCSEC, 4) + " arcsec"
          : "–";

        document.getElementById("muTot").textContent = isFinite(muTot)
          ? formatNumber(muTot, 4)
          : thetaE > 0 && betaRad === 0
          ? "→ ∞ (Einstein ring)"
          : "–";

        if (isFinite(muPlus) && isFinite(muMinus)) {
          document.getElementById("muEach").textContent =
            formatNumber(muPlus, 4) + ", " + formatNumber(muMinus, 4);
        } else if (thetaE > 0 && betaRad === 0) {
          document.getElementById("muEach").textContent =
            "formally infinite (idealized model)";
        } else {
          document.getElementById("muEach").textContent = "–";
        }
      }

      document
        .getElementById("computeBtn")
        .addEventListener("click", computeLensing);
      window.addEventListener("load", computeLensing);
    </script>

