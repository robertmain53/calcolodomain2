    <script>
      // Tab handling
      const tabs = document.querySelectorAll(".force-tab");
      const sections = document.querySelectorAll(".mode-section");
      let currentMode = "net";

      function setActiveMode(mode) {
        currentMode = mode;
        tabs.forEach((btn) => {
          if (btn.dataset.mode === mode) {
            btn.classList.add("tab-active");
          } else {
            btn.classList.remove("tab-active");
          }
        });
        sections.forEach((sec) => {
          sec.classList.toggle("hidden", sec.id !== "mode-" + mode);
        });
        document.getElementById("force-results").classList.add("hidden");
      }

      tabs.forEach((btn) => {
        btn.addEventListener("click", () => setActiveMode(btn.dataset.mode));
      });
      // default
      setActiveMode("net");

      // Unit helpers
      function massToKg(value, unit) {
        const v = Number(value);
        if (!isFinite(v)) return NaN;
        switch (unit) {
          case "kg":
            return v;
          case "g":
            return v / 1000;
          case "lb":
            return v * 0.45359237;
          case "tonne":
            return v * 1000;
          default:
            return NaN;
        }
      }

      function accToMs2(value, unit) {
        const v = Number(value);
        if (!isFinite(v)) return NaN;
        switch (unit) {
          case "m/s2":
            return v;
          case "ft/s2":
            return v * 0.3048;
          case "km/h2":
            return v * (1000 / 3600 / 3600);
          case "g":
            return v * 9.80665;
          default:
            return NaN;
        }
      }

      function forceFromUnit(value, unit) {
        const v = Number(value);
        if (!isFinite(v)) return NaN;
        switch (unit) {
          case "N":
            return v;
          case "kN":
            return v * 1000;
          case "lbf":
            return v * 4.4482216152605;
          default:
            return NaN;
        }
      }

      function pressureToPa(value, unit) {
        const v = Number(value);
        if (!isFinite(v)) return NaN;
        switch (unit) {
          case "Pa":
            return v;
          case "kPa":
            return v * 1000;
          case "bar":
            return v * 100000;
          case "psi":
            return v * 6894.757293168;
          default:
            return NaN;
        }
      }

      function areaToM2(value, unit) {
        const v = Number(value);
        if (!isFinite(v)) return NaN;
        switch (unit) {
          case "m2":
            return v;
          case "cm2":
            return v / 10000;
          case "mm2":
            return v / 1e6;
          case "in2":
            return v * 0.00064516;
          case "ft2":
            return v * 0.09290304;
          default:
            return NaN;
        }
      }

      function speedToMs(value, unit) {
        const v = Number(value);
        if (!isFinite(v)) return NaN;
        switch (unit) {
          case "m/s":
            return v;
          case "km/h":
            return v * (1000 / 3600);
          case "ft/s":
            return v * 0.3048;
          default:
            return NaN;
        }
      }

      function lengthToM(value, unit) {
        const v = Number(value);
        if (!isFinite(v)) return NaN;
        switch (unit) {
          case "m":
            return v;
          case "cm":
            return v / 100;
          case "ft":
            return v * 0.3048;
          default:
            return NaN;
        }
      }

      function formatNumber(x, decimals = 3) {
        if (!isFinite(x)) return "";
        if (Math.abs(x) >= 1e4 || (Math.abs(x) < 1e-2 && x !== 0)) {
          return x.toExponential(decimals);
        }
        return x.toFixed(decimals).replace(/\.?0+$/, "");
      }

      // Calculation
      document
        .getElementById("force-calc-btn")
        .addEventListener("click", () => {
          let F_N = NaN;
          let stepsHtml = "";
          if (currentMode === "net") {
            const m = massToKg(
              document.getElementById("mass-net").value,
              document.getElementById("mass-net-unit").value
            );
            const a = accToMs2(
              document.getElementById("acc-net").value,
              document.getElementById("acc-net-unit").value
            );
            if (!isFinite(m) || !isFinite(a)) {
              alert("Please enter valid mass and acceleration.");
              return;
            }
            F_N = m * a;
            stepsHtml = `
        <p><strong>Mode:</strong> Net force (F = m·a)</p>
        <p>Mass: m = ${formatNumber(m)} kg</p>
        <p>Acceleration: a = ${formatNumber(a)} m/s²</p>
        <p>Formula: F = m · a</p>
        <p>Calculation: F = ${formatNumber(m)} × ${formatNumber(
              a
            )} = ${formatNumber(F_N)} N</p>
      `;
          } else if (currentMode === "weight") {
            const m = massToKg(
              document.getElementById("mass-weight").value,
              document.getElementById("mass-weight-unit").value
            );
            const g = accToMs2(
              document.getElementById("gravity").value,
              document.getElementById("gravity-unit").value
            );
            if (!isFinite(m) || !isFinite(g)) {
              alert("Please enter valid mass and gravitational acceleration.");
              return;
            }
            F_N = m * g;
            stepsHtml = `
        <p><strong>Mode:</strong> Weight (F = m·g)</p>
        <p>Mass: m = ${formatNumber(m)} kg</p>
        <p>Gravity: g = ${formatNumber(g)} m/s²</p>
        <p>Formula: W = m · g</p>
        <p>Calculation: W = ${formatNumber(m)} × ${formatNumber(
              g
            )} = ${formatNumber(F_N)} N</p>
      `;
          } else if (currentMode === "friction") {
            const mu = Number(document.getElementById("mu").value);
            const N = forceFromUnit(
              document.getElementById("normal-force").value,
              document.getElementById("normal-force-unit").value
            );
            if (!isFinite(mu) || !isFinite(N)) {
              alert("Please enter valid μ and normal force.");
              return;
            }
            F_N = mu * N;
            stepsHtml = `
        <p><strong>Mode:</strong> Friction (F = μ·N)</p>
        <p>Coefficient of friction: μ = ${formatNumber(mu)}</p>
        <p>Normal force: N = ${formatNumber(N)} N</p>
        <p>Formula: F<sub>friction</sub> = μ · N</p>
        <p>Calculation: F = ${formatNumber(mu)} × ${formatNumber(
              N
            )} = ${formatNumber(F_N)} N</p>
      `;
          } else if (currentMode === "centripetal") {
            const m = massToKg(
              document.getElementById("mass-cent").value,
              document.getElementById("mass-cent-unit").value
            );
            const v = speedToMs(
              document.getElementById("velocity").value,
              document.getElementById("velocity-unit").value
            );
            const r = lengthToM(
              document.getElementById("radius").value,
              document.getElementById("radius-unit").value
            );
            if (!isFinite(m) || !isFinite(v) || !isFinite(r) || r === 0) {
              alert(
                "Please enter valid mass, speed, and radius (radius must be non-zero)."
              );
              return;
            }
            F_N = (m * v * v) / r;
            stepsHtml = `
        <p><strong>Mode:</strong> Centripetal force (F = m·v²/r)</p>
        <p>Mass: m = ${formatNumber(m)} kg</p>
        <p>Speed: v = ${formatNumber(v)} m/s</p>
        <p>Radius: r = ${formatNumber(r)} m</p>
        <p>Formula: F<sub>c</sub> = m · v² / r</p>
        <p>Calculation: F = ${formatNumber(m)} × ${formatNumber(
              v
            )}² / ${formatNumber(r)} = ${formatNumber(F_N)} N</p>
      `;
          } else if (currentMode === "pressure") {
            const p = pressureToPa(
              document.getElementById("pressure").value,
              document.getElementById("pressure-unit").value
            );
            const A = areaToM2(
              document.getElementById("area").value,
              document.getElementById("area-unit").value
            );
            if (!isFinite(p) || !isFinite(A)) {
              alert("Please enter valid pressure and area.");
              return;
            }
            F_N = p * A;
            stepsHtml = `
        <p><strong>Mode:</strong> Force from pressure (F = p·A)</p>
        <p>Pressure: p = ${formatNumber(p)} Pa</p>
        <p>Area: A = ${formatNumber(A)} m²</p>
        <p>Formula: F = p · A</p>
        <p>Calculation: F = ${formatNumber(p)} × ${formatNumber(
              A
            )} = ${formatNumber(F_N)} N</p>
      `;
          }

          if (!isFinite(F_N)) {
            alert("Unable to compute force. Please check your inputs.");
            return;
          }

          const F_kN = F_N / 1000;
          const F_lbf = F_N / 4.4482216152605;

          document.getElementById(
            "force-main-value"
          ).textContent = `${formatNumber(F_N)} N`;
          document.getElementById(
            "force-alt-units"
          ).textContent = `≈ ${formatNumber(F_kN)} kN, ≈ ${formatNumber(
            F_lbf
          )} lbf`;
          document.getElementById("force-steps").innerHTML = stepsHtml;
          document.getElementById("force-results").classList.remove("hidden");
        });

      document
        .getElementById("force-reset-btn")
        .addEventListener("click", () => {
          document.getElementById("force-form").reset();
          document.getElementById("force-results").classList.add("hidden");
          // restore some defaults
          document.getElementById("gravity").value = 9.81;
          setActiveMode("net");
        });

      // Mobile menu script (simple)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", () => {
          const menu = document.getElementById("mobile-menu");
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          document
            .getElementById("mobile-menu-toggle")
            .setAttribute("aria-expanded", String(isHidden));
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
