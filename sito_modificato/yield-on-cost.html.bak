    <script>
      // Mode toggle
      const modeStockBtn = document.getElementById("mode-stock");
      const modeReBtn = document.getElementById("mode-realestate");
      const stockInputs = document.getElementById("stock-inputs");
      const reInputs = document.getElementById("realestate-inputs");

      let currentMode = "stock";

      function setMode(mode) {
        currentMode = mode;
        if (mode === "stock") {
          stockInputs.classList.remove("hidden");
          reInputs.classList.add("hidden");
          modeStockBtn.classList.add("bg-blue-600", "text-white");
          modeStockBtn.classList.remove("bg-white", "text-gray-700");
          modeReBtn.classList.remove("bg-blue-600", "text-white");
          modeReBtn.classList.add("bg-white", "text-gray-700");
        } else {
          reInputs.classList.remove("hidden");
          stockInputs.classList.add("hidden");
          modeReBtn.classList.add("bg-blue-600", "text-white");
          modeReBtn.classList.remove("bg-white", "text-gray-700");
          modeStockBtn.classList.remove("bg-blue-600", "text-white");
          modeStockBtn.classList.add("bg-white", "text-gray-700");
        }
        clearResults();
        document.getElementById("error-msg").textContent = "";
      }

      modeStockBtn.addEventListener("click", () => setMode("stock"));
      modeReBtn.addEventListener("click", () => setMode("realestate"));

      // Helpers
      function fmtPct(value) {
        if (!isFinite(value)) return "–";
        return value.toFixed(2) + "%";
      }
      function fmtMoney(value) {
        if (!isFinite(value)) return "–";
        return (
          "$" + value.toLocaleString(undefined, { maximumFractionDigits: 2 })
        );
      }

      const yocEl = document.getElementById("yoc-result");
      const currentYieldEl = document.getElementById("current-yield-result");
      const forwardYocEl = document.getElementById("forward-yoc-result");
      const incomeTodayEl = document.getElementById("income-today-result");
      const incomeFutureEl = document.getElementById("income-future-result");
      const incomeTotalEl = document.getElementById("income-total-result");
      const extraMetricsEl = document.getElementById("extra-metrics");
      const comparisonNoteEl = document.getElementById("comparison-note");
      const scenarioTbody = document.getElementById("scenario-tbody");

      function clearResults() {
        yocEl.textContent = "–";
        currentYieldEl.textContent = "–";
        forwardYocEl.textContent = "–";
        incomeTodayEl.textContent = "–";
        incomeFutureEl.textContent = "–";
        incomeTotalEl.textContent = "–";
        extraMetricsEl.classList.add("hidden");
        comparisonNoteEl.classList.add("hidden");
        comparisonNoteEl.textContent = "";
        scenarioTbody.innerHTML = "";
      }

      // Scenario table for dividend growth
      function buildScenarioTable(baseYoc) {
        scenarioTbody.innerHTML = "";
        if (!isFinite(baseYoc) || baseYoc <= 0) {
          return;
        }
        const growthRates = [0, 3, 5, 8, 10];
        growthRates.forEach((g) => {
          const y5 = baseYoc * Math.pow(1 + g / 100, 5);
          const y10 = baseYoc * Math.pow(1 + g / 100, 10);
          const tr = document.createElement("tr");
          tr.className = "border-t border-gray-100";
          tr.innerHTML = `
        <td class="px-3 py-2">${g.toFixed(1)}%</td>
        <td class="px-3 py-2 text-right">${fmtPct(y5)}</td>
        <td class="px-3 py-2 text-right">${fmtPct(y10)}</td>
      `;
          scenarioTbody.appendChild(tr);
        });
      }

      // Main calculation
      document.getElementById("calc-btn").addEventListener("click", () => {
        document.getElementById("error-msg").textContent = "";
        clearResults();

        if (currentMode === "stock") {
          const cost = parseFloat(
            document.getElementById("stock-cost-basis").value
          );
          const annualDiv = parseFloat(
            document.getElementById("stock-annual-dividend").value
          );
          const currentVal = parseFloat(
            document.getElementById("stock-current-value").value
          );
          const g = parseFloat(
            document.getElementById("stock-div-growth").value
          );
          const years = parseFloat(
            document.getElementById("stock-years").value
          );

          if (!cost || !annualDiv || cost <= 0 || annualDiv < 0) {
            document.getElementById("error-msg").textContent =
              "Please enter at least cost basis and annual dividend income.";
            return;
          }

          const yoc = (annualDiv / cost) * 100;
          yocEl.textContent = fmtPct(yoc);
          incomeTodayEl.textContent = fmtMoney(annualDiv);

          if (currentVal && currentVal > 0) {
            const cy = (annualDiv / currentVal) * 100;
            currentYieldEl.textContent = fmtPct(cy);
            comparisonNoteEl.classList.remove("hidden");
            if (yoc > cy) {
              comparisonNoteEl.textContent = `Your yield on cost (${fmtPct(
                yoc
              )}) is higher than the current yield (${fmtPct(
                cy
              )}). This often reflects successful dividend growth since you bought.`;
            } else if (yoc < cy) {
              comparisonNoteEl.textContent = `Your yield on cost (${fmtPct(
                yoc
              )}) is lower than the current yield (${fmtPct(
                cy
              )}). The stock may now offer a higher income yield to new investors, possibly due to a price drop or dividend increase.`;
            } else {
              comparisonNoteEl.textContent = `Your yield on cost matches the current yield. This usually happens when the dividend and price have grown at similar rates.`;
            }
          }

          if (isFinite(g) && isFinite(years) && years > 0) {
            const futureIncome = annualDiv * Math.pow(1 + g / 100, years);
            const forwardYoc = (futureIncome / cost) * 100;
            forwardYocEl.textContent = fmtPct(forwardYoc);
            incomeFutureEl.textContent = fmtMoney(futureIncome);

            // total income over period assuming geometric growth
            const totalIncome =
              annualDiv * ((Math.pow(1 + g / 100, years) - 1) / (g / 100 || 1));
            incomeTotalEl.textContent = fmtMoney(totalIncome);
            extraMetricsEl.classList.remove("hidden");
          }

          buildScenarioTable(yoc);
        } else {
          // Real estate mode
          const cost = parseFloat(
            document.getElementById("re-total-cost").value
          );
          const noi = parseFloat(document.getElementById("re-noi").value);
          const value = parseFloat(
            document.getElementById("re-stabilized-value").value
          );
          const g = parseFloat(document.getElementById("re-noi-growth").value);
          const years = parseFloat(document.getElementById("re-years").value);
          const capRate = parseFloat(
            document.getElementById("re-cap-rate").value
          );

          if (!cost || !noi || cost <= 0 || noi < 0) {
            document.getElementById("error-msg").textContent =
              "Please enter at least total cost and annual NOI.";
            return;
          }

          const yoc = (noi / cost) * 100;
          yocEl.textContent = fmtPct(yoc);
          incomeTodayEl.textContent = fmtMoney(noi);

          if (value && value > 0) {
            const impliedCap = (noi / value) * 100;
            currentYieldEl.textContent = fmtPct(impliedCap);
            comparisonNoteEl.classList.remove("hidden");
            comparisonNoteEl.textContent = `Your yield on cost is ${fmtPct(
              yoc
            )}, while the implied market cap rate at the stabilized value is ${fmtPct(
              impliedCap
            )}. A higher YOC than market cap rate suggests value creation.`;
          } else if (capRate && capRate > 0) {
            currentYieldEl.textContent = fmtPct(capRate);
            comparisonNoteEl.classList.remove("hidden");
            comparisonNoteEl.textContent = `Your yield on cost is ${fmtPct(
              yoc
            )} versus a market cap rate of ${fmtPct(
              capRate
            )}. Developers often target a YOC at least 1–2% above market cap rates.`;
          }

          if (isFinite(g) && isFinite(years) && years > 0) {
            const futureNoi = noi * Math.pow(1 + g / 100, years);
            const forwardYoc = (futureNoi / cost) * 100;
            forwardYocEl.textContent = fmtPct(forwardYoc);
            incomeFutureEl.textContent = fmtMoney(futureNoi);
            const totalIncome =
              noi * ((Math.pow(1 + g / 100, years) - 1) / (g / 100 || 1));
            incomeTotalEl.textContent = fmtMoney(totalIncome);
            extraMetricsEl.classList.remove("hidden");
          }

          // Scenario table still useful: treat YOC as base
          buildScenarioTable(yoc);
        }
      });

      document.getElementById("reset-btn").addEventListener("click", () => {
        document
          .querySelectorAll('input[type="number"]')
          .forEach((i) => (i.value = ""));
        document.getElementById("error-msg").textContent = "";
        clearResults();
      });

      // Initialize scenario table with placeholder
