    <script>
      // Utility: convert any time value to hours
      function toHours(value, unit) {
        if (!isFinite(value) || value <= 0) return null;
        switch (unit) {
          case "min":
            return value / 60;
          case "h":
            return value;
          case "d":
            return value * 24;
          default:
            return null;
        }
      }

      function formatTimeHours(hours) {
        if (!isFinite(hours)) return "-";
        if (hours < 1) {
          const minutes = hours * 60;
          return minutes.toFixed(1) + " min";
        } else if (hours < 48) {
          return hours.toFixed(2) + " h";
        } else {
          const days = hours / 24;
          return days.toFixed(2) + " days";
        }
      }

      function calculate() {
        const errorEl = document.getElementById("calc-error");
        errorEl.textContent = "";

        const halfLifeVal = parseFloat(
          document.getElementById("half-life").value
        );
        const halfLifeUnit = document.getElementById("half-life-unit").value;
        const doseVal = parseFloat(document.getElementById("dose").value);
        const vdVal = parseFloat(document.getElementById("vd").value);
        const vdUnit = document.getElementById("vd-unit").value;
        const bwVal = parseFloat(document.getElementById("body-weight").value);
        const bwUnit = document.getElementById("body-weight-unit").value;
        const dosingIntervalVal = parseFloat(
          document.getElementById("dosing-interval").value
        );
        const dosingIntervalUnit = document.getElementById(
          "dosing-interval-unit"
        ).value;
        const numDoses = parseInt(
          document.getElementById("num-doses").value,
          10
        );
        const timeStepVal = parseFloat(
          document.getElementById("time-step").value
        );
        const timeStepUnit = document.getElementById("time-step-unit").value;

        if (!isFinite(halfLifeVal) || halfLifeVal <= 0) {
          errorEl.textContent = "Please enter a valid positive half-life.";
          return;
        }

        const tHalfHours = toHours(halfLifeVal, halfLifeUnit);
        if (!tHalfHours) {
          errorEl.textContent = "Invalid half-life unit.";
          return;
        }

        const k = Math.log(2) / tHalfHours; // per hour

        // Elimination milestones
        const elimList = document.getElementById("elimination-list");
        elimList.innerHTML = "";
        const milestones = [
          { label: "50% eliminated (1 half-life)", halfLives: 1 },
          { label: "75% eliminated", halfLives: 2 },
          { label: "90% eliminated", halfLives: Math.log(10) / Math.log(2) }, // ~3.32
          { label: "95% eliminated", halfLives: Math.log(20) / Math.log(2) }, // ~4.32
          { label: "97.5% eliminated", halfLives: Math.log(40) / Math.log(2) }, // ~5.32
        ];
        milestones.forEach((m) => {
          const hours = m.halfLives * tHalfHours;
          const li = document.createElement("li");
          li.textContent = `${m.label}: about ${formatTimeHours(hours)}`;
          elimList.appendChild(li);
        });

        // Steady state & washout
        const steadyList = document.getElementById("steady-state-list");
        steadyList.innerHTML = "";
        const dosingIntervalHours =
          dosingIntervalUnit === "d"
            ? dosingIntervalVal * 24
            : dosingIntervalVal;
        if (isFinite(dosingIntervalHours) && dosingIntervalHours > 0) {
          const approxSteadyHours = 4.5 * tHalfHours;
          const washoutHours = 4.5 * tHalfHours;
          const li1 = document.createElement("li");
          li1.textContent = `Approximate time to steady state: ${formatTimeHours(
            approxSteadyHours
          )} (assuming regular dosing).`;
          const li2 = document.createElement("li");
          li2.textContent = `Approximate washout (≥95% eliminated) after stopping: ${formatTimeHours(
            washoutHours
          )}.`;
          const li3 = document.createElement("li");
          li3.textContent = `Dosing interval: every ${formatTimeHours(
            dosingIntervalHours
          )}.`;
          steadyList.appendChild(li1);
          steadyList.appendChild(li2);
          steadyList.appendChild(li3);
        } else {
          const li = document.createElement("li");
          li.textContent =
            "Enter a valid dosing interval to estimate steady state and washout.";
          steadyList.appendChild(li);
        }

        // Initial concentration C0
        let C0 = 1; // relative
        let hasAbsolute = false;
        let concUnit = "relative units";
        const doseProvided = isFinite(doseVal) && doseVal > 0;
        const vdProvided = isFinite(vdVal) && vdVal > 0;

        if (doseProvided && vdProvided) {
          let vdLiters = null;
          if (vdUnit === "L") {
            vdLiters = vdVal;
          } else if (vdUnit === "Lkg") {
            if (!isFinite(bwVal) || bwVal <= 0) {
              errorEl.textContent =
                "Enter a valid body weight to use Vd in L/kg.";
              return;
            }
            const bwKg = bwUnit === "lb" ? bwVal * 0.45359237 : bwVal;
            vdLiters = vdVal * bwKg;
          }
          if (vdLiters && vdLiters > 0) {
            C0 = doseVal / vdLiters;
            hasAbsolute = true;
            const doseUnit = document.getElementById("dose-unit").value || "mg";
            concUnit = `${doseUnit}/L (approximate)`;
          }
        }

        // Time step and total time
        const timeStepHours =
          timeStepUnit === "d" ? timeStepVal * 24 : timeStepVal;
        if (!isFinite(timeStepHours) || timeStepHours <= 0) {
          errorEl.textContent = "Please enter a valid positive time step.";
          return;
        }

        const maxTimeHours = Math.max(
          5 * tHalfHours,
          dosingIntervalHours * Math.max(1, numDoses)
        );
        const tableBody = document.getElementById("results-table-body");
        tableBody.innerHTML = "";

        const rows = [];
        for (let t = 0; t <= maxTimeHours + 1e-6; t += timeStepHours) {
          // Single-dose concentration
          let conc = C0 * Math.exp(-k * t);

          // Repeated dosing accumulation (superposition)
          if (
            isFinite(dosingIntervalHours) &&
            dosingIntervalHours > 0 &&
            isFinite(numDoses) &&
            numDoses > 1
          ) {
            conc = 0;
            for (let n = 0; n < numDoses; n++) {
              const doseTime = n * dosingIntervalHours;
              if (t >= doseTime) {
                const dt = t - doseTime;
                conc += C0 * Math.exp(-k * dt);
              }
            }
          }

          const percent = (conc / C0) * 100;
          rows.push({
            timeHours: t,
            conc: conc,
            percent: percent,
          });
        }

        rows.forEach((r) => {
          const tr = document.createElement("tr");
          const td1 = document.createElement("td");
          const td2 = document.createElement("td");
          const td3 = document.createElement("td");
          const td4 = document.createElement("td");

          td1.className = "px-3 py-1.5 border-b whitespace-nowrap";
          td2.className = "px-3 py-1.5 border-b whitespace-nowrap";
          td3.className = "px-3 py-1.5 border-b whitespace-nowrap";
          td4.className = "px-3 py-1.5 border-b whitespace-nowrap";

          td1.textContent = formatTimeHours(r.timeHours);
          td2.textContent = r.timeHours.toFixed(2);
          td3.textContent = hasAbsolute
            ? r.conc.toFixed(4) + " " + concUnit
            : r.conc.toFixed(4) + " (relative)";
          td4.textContent = r.percent.toFixed(2) + " % of C0";

          tr.appendChild(td1);
          tr.appendChild(td2);
          tr.appendChild(td3);
          tr.appendChild(td4);
          tableBody.appendChild(tr);
        });

        // Enable CSV download
        const downloadBtn = document.getElementById("download-csv");
        downloadBtn.disabled = false;
        downloadBtn.onclick = function () {
          if (!rows.length) return;
          let csv =
            "Time (formatted),Time (hours),Concentration," +
            (hasAbsolute ? concUnit : "relative") +
            ",Percent of C0 (%)\n";
          rows.forEach((r) => {
            csv += `"${formatTimeHours(r.timeHours)}",${r.timeHours.toFixed(
              4
            )},${r.conc.toFixed(6)},${r.percent.toFixed(4)}\n`;
          });
          const blob = new Blob([csv], { type: "text/csv;charset=utf-8;" });
          const url = URL.createObjectURL(blob);
          const a = document.createElement("a");
          const drugName = (
            document.getElementById("drug-name").value || "drug"
          )
            .replace(/[^a-z0-9]+/gi, "-")
            .toLowerCase();
          a.href = url;
          a.download = `${drugName || "drug"}-half-life-simulation.csv`;
          document.body.appendChild(a);
          a.click();
          document.body.removeChild(a);
          URL.revokeObjectURL(url);
        };
      }

      document
        .getElementById("calculate-btn")
        .addEventListener("click", calculate);

      document.getElementById("reset-btn").addEventListener("click", () => {
        document.getElementById("drug-name").value = "";
        document.getElementById("half-life").value = 8;
        document.getElementById("half-life-unit").value = "h";
        document.getElementById("dose").value = "";
        document.getElementById("dose-unit").value = "mg";
        document.getElementById("vd").value = "";
        document.getElementById("vd-unit").value = "L";
        document.getElementById("body-weight").value = "";
        document.getElementById("body-weight-unit").value = "kg";
        document.getElementById("dosing-interval").value = 12;
        document.getElementById("dosing-interval-unit").value = "h";
        document.getElementById("num-doses").value = 10;
        document.getElementById("time-step").value = 1;
        document.getElementById("time-step-unit").value = "h";
        document.getElementById("calc-error").textContent = "";

        document.getElementById("elimination-list").innerHTML =
          "<li>Enter a valid half-life to see elimination times.</li>";
        document.getElementById("steady-state-list").innerHTML =
          "<li>Enter half-life and dosing interval to estimate steady state.</li>";
        document.getElementById("results-table-body").innerHTML =
          '<tr><td class="px-3 py-2 border-b text-gray-500" colspan="4">Run a calculation to generate the concentration table.</td></tr>';
        document.getElementById("download-csv").disabled = true;
      });

      // Run an initial calculation with defaults
      window.addEventListener("DOMContentLoaded", () => {
        calculate();
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
