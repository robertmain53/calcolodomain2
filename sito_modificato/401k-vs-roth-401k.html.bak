<script>
        function validateInput(inputId, errorId, errorMessage) {
            const inputElement = document.getElementById(inputId);
            const errorElement = document.getElementById(errorId);
            if (!inputElement.value) {
                errorElement.textContent = errorMessage;
                return false;
            } else {
                errorElement.textContent = '';
                return true;
            }
        }

        function calculateRetirement() {
            const contributionValid = validateInput('annualContribution', 'annualContributionError', 'Please enter a valid annual contribution.');
            const yearsValid = validateInput('yearsToRetirement', 'yearsToRetirementError', 'Please enter the years to retirement.');
            const rateValid = validateInput('expectedRateOfReturn', 'expectedRateOfReturnError', 'Please enter the expected rate of return.');

            if (contributionValid && yearsValid && rateValid) {
                // Perform calculation
                const annualContribution = parseFloat(document.getElementById('annualContribution').value);
                const yearsToRetirement = parseFloat(document.getElementById('yearsToRetirement').value);
                const rateOfReturn = parseFloat(document.getElementById('expectedRateOfReturn').value) / 100;

                const futureValue = annualContribution * Math.pow((1 + rateOfReturn), yearsToRetirement);

                document.getElementById('traditionalValue').textContent = `$${futureValue.toFixed(2)}`;
                document.getElementById('rothValue').textContent = `$${futureValue.toFixed(2)}`;
            }
        }
    </script>


