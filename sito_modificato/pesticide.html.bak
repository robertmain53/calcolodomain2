    <script>
      // Utility: unit labels update
      function updateUnitLabels() {
        const areaUnit = document.getElementById("areaUnit").value;
        const volumeUnit = document.getElementById("volumeUnit").value;

        document.getElementById("labelAreaUnitDisplay").textContent =
          areaUnit === "acre" ? "acre" : "ha";
        document.getElementById("sprayVolumeUnitDisplay").textContent =
          volumeUnit + "/" + (areaUnit === "acre" ? "acre" : "ha");
        document.getElementById("tankVolumeUnitDisplay").textContent =
          volumeUnit;
        document.getElementById("fieldAreaUnitDisplay").textContent =
          areaUnit === "acre" ? "acres" : "hectares";
      }

      document
        .getElementById("areaUnit")
        .addEventListener("change", updateUnitLabels);
      document
        .getElementById("volumeUnit")
        .addEventListener("change", updateUnitLabels);
      updateUnitLabels();

      function formatNumber(value, decimals) {
        if (!isFinite(value)) return "-";
        const abs = Math.abs(value);
        let d = decimals;
        if (abs > 0 && abs < 0.01) d = 4;
        return value.toLocaleString(undefined, {
          maximumFractionDigits: d,
          minimumFractionDigits: 0,
        });
      }

      document.getElementById("calcBtn").addEventListener("click", function () {
        const errorEl = document.getElementById("errorMsg");
        errorEl.textContent = "";

        const areaUnit = document.getElementById("areaUnit").value; // acre or ha
        const volumeUnit = document.getElementById("volumeUnit").value; // gal or L

        const labelRate = parseFloat(
          document.getElementById("labelRate").value
        );
        const labelRateUnit = document.getElementById("labelRateUnit").value;
        const sprayVolume = parseFloat(
          document.getElementById("sprayVolume").value
        );
        const tankVolume = parseFloat(
          document.getElementById("tankVolume").value
        );
        const fieldArea = parseFloat(
          document.getElementById("fieldArea").value
        );

        if (!(labelRate > 0) || !(sprayVolume > 0) || !(tankVolume > 0)) {
          errorEl.textContent =
            "Please enter positive values for label rate, spray volume, and tank volume.";
          document.getElementById("resultsSection").classList.add("hidden");
          return;
        }

        // Area per tank
        const areaPerTank = tankVolume / sprayVolume; // in chosen area unit

        // Product per tank (in label product unit)
        const productPerTank = labelRate * areaPerTank;

        // Water per tank (approx. tank volume minus product volume if same type of unit, else just show tank volume)
        let waterPerTankText;
        if (
          (labelRateUnit === "L" ||
            labelRateUnit === "mL" ||
            labelRateUnit === "gal" ||
            labelRateUnit === "fl_oz") &&
          (volumeUnit === "L" || volumeUnit === "gal")
        ) {
          // Convert productPerTank to tank volume unit if possible
          let productVolInTankUnit = null;

          // Convert productPerTank to liters
          let productLiters = null;
          if (labelRateUnit === "L") productLiters = productPerTank;
          else if (labelRateUnit === "mL")
            productLiters = productPerTank / 1000;
          else if (labelRateUnit === "gal")
            productLiters = productPerTank * 3.78541;
          else if (labelRateUnit === "fl_oz")
            productLiters = productPerTank * 0.0295735;

          if (productLiters !== null) {
            if (volumeUnit === "L") {
              productVolInTankUnit = productLiters;
            } else if (volumeUnit === "gal") {
              productVolInTankUnit = productLiters / 3.78541;
            }
          }

          if (
            productVolInTankUnit !== null &&
            productVolInTankUnit < tankVolume
          ) {
            const waterVol = tankVolume - productVolInTankUnit;
            waterPerTankText =
              formatNumber(waterVol, 2) + " " + volumeUnit + " (approx.)";
          } else {
            waterPerTankText =
              "≈ " + formatNumber(tankVolume, 2) + " " + volumeUnit;
          }
        } else {
          waterPerTankText =
            "≈ " + formatNumber(tankVolume, 2) + " " + volumeUnit;
        }

        // Field-level calculations if fieldArea provided
        let numTanks = null;
        let totalProduct = null;
        let totalSprayVolume = null;

        if (fieldArea > 0) {
          numTanks = fieldArea / areaPerTank;
          totalProduct = labelRate * fieldArea;
          totalSprayVolume = sprayVolume * fieldArea;
        }

        // Populate results
        const areaUnitLabel = areaUnit === "acre" ? "acres" : "ha";
        document.getElementById("areaPerTank").textContent =
          formatNumber(areaPerTank, 3) + " " + areaUnitLabel + " per tank";

        document.getElementById("productPerTank").textContent =
          formatNumber(productPerTank, 3) +
          " " +
          labelRateUnit +
          " product per tank";

        document.getElementById("waterPerTank").textContent = waterPerTankText;

        if (fieldArea > 0) {
          document.getElementById("numTanks").textContent =
            formatNumber(numTanks, 2) + " tanks (full equivalents)";
          document.getElementById("totalProduct").textContent =
            formatNumber(totalProduct, 3) +
            " " +
            labelRateUnit +
            " product for field";
          document.getElementById("totalSprayVolume").textContent =
            formatNumber(totalSprayVolume, 1) +
            " " +
            volumeUnit +
            " total spray volume";
        } else {
          document.getElementById("numTanks").textContent = "—";
          document.getElementById("totalProduct").textContent = "—";
          document.getElementById("totalSprayVolume").textContent = "—";
        }

        document.getElementById("resultsSection").classList.remove("hidden");
      });

      document
        .getElementById("resetBtn")
        .addEventListener("click", function () {
          ["labelRate", "sprayVolume", "tankVolume", "fieldArea"].forEach(
            (id) => {
              document.getElementById(id).value = "";
            }
          );
          document.getElementById("areaUnit").value = "acre";
          document.getElementById("volumeUnit").value = "gal";
          document.getElementById("labelRateUnit").value = "L";
          updateUnitLabels();
          document.getElementById("errorMsg").textContent = "";
          document.getElementById("resultsSection").classList.add("hidden");
        });

      // Mobile menu toggle
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          this.setAttribute("aria-expanded", isHidden ? "true" : "false");
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
