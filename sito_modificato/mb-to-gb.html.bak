  <script>
    (function() {
      const mbInput = document.getElementById('mbInput');
      const mode = document.getElementById('mode');
      const outGb = document.getElementById('outGb');
      const outGib = document.getElementById('outGib');
      const outMbDec = document.getElementById('outMbDec');
      const outBytesDec = document.getElementById('outBytesDec');
      const outBytesBin = document.getElementById('outBytesBin');
      const tableBody = document.getElementById('tableBody');

      function fmt(val) {
        if (!isFinite(val)) return '—';
        if (Math.abs(val) >= 1) return Number(val.toFixed(6)).toString();
        return Number(val.toExponential(6)).toString();
      }

      function update() {
        const mbVal = parseFloat(mbInput.value);
        if (isNaN(mbVal)) {
          outGb.textContent = '—';
          outGib.textContent = '—';
          outMbDec.textContent = '—';
          outBytesDec.textContent = '—';
          outBytesBin.textContent = '—';
          return;
        }

        // decimal
        const gbDec = mbVal / 1000;
        const bytesDec = mbVal * 1_000_000; // MB (10^6)

        // binary
        const gibBin = mbVal / 1024;
        const bytesBin = mbVal * 1_048_576; // MiB if you interpret as binary

        outGb.textContent = fmt(gbDec);
        outGib.textContent = fmt(gibBin);
        outMbDec.textContent = fmt(mbVal);
        // show raw bytes as maybe big int
        outBytesDec.textContent = bytesDec.toLocaleString('en-US');
        outBytesBin.textContent = bytesBin.toLocaleString('en-US');
      }

      mbInput.addEventListener('input', update);
      mode.addEventListener('change', update);

      // fill table with some common values
      const samples = [256, 512, 1024, 2048, 4096, 8192];
      samples.forEach(v => {
        const gb = v / 1000;
        const gib = v / 1024;
        const tr = document.createElement('tr');
        tr.innerHTML = `
          <td>${v}</td>
          <td>${gb.toFixed(6)}</td>
          <td>${gib.toFixed(6)}</td>
        `;
        tableBody.appendChild(tr);
      });

      // init
      mbInput.value = 2048;
      update();
    })();
  </script>

