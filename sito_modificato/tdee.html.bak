    <script>
      // Helper functions
      function getSelectedSex() {
        const btn = document.querySelector(".sex-btn.border-blue-500");
        return btn ? btn.dataset.sex : "male";
      }

      function getNumber(id) {
        const el = document.getElementById(id);
        const v = parseFloat(el && el.value);
        return isNaN(v) ? null : v;
      }

      function round(val, decimals = 0) {
        const f = Math.pow(10, decimals);
        return Math.round(val * f) / f;
      }

      function calcBMR({ sex, age, heightCm, weightKg, bodyFat, formula }) {
        if (!age || !heightCm || !weightKg) return null;
        const hasBodyFat = bodyFat && bodyFat > 0 && bodyFat < 70;

        if (formula === "katch" && hasBodyFat) {
          const lbm = weightKg * (1 - bodyFat / 100);
          return 370 + 21.6 * lbm;
        }

        if (formula === "harris") {
          if (sex === "male") {
            return 13.397 * weightKg + 4.799 * heightCm - 5.677 * age + 88.362;
          } else {
            return 9.247 * weightKg + 3.098 * heightCm - 4.33 * age + 447.593;
          }
        }

        // default: Mifflin–St Jeor
        if (sex === "male") {
          return 10 * weightKg + 6.25 * heightCm - 5 * age + 5;
        } else {
          return 10 * weightKg + 6.25 * heightCm - 5 * age - 161;
        }
      }

      function updateGoalButtons(activeGoal) {
        document.querySelectorAll(".goal-btn").forEach((btn) => {
          if (btn.dataset.goal === activeGoal) {
            btn.classList.remove(
              "border-gray-300",
              "text-gray-700",
              "bg-white"
            );
            btn.classList.add("border-blue-500", "text-blue-600", "bg-blue-50");
          } else {
            btn.classList.remove(
              "border-blue-500",
              "text-blue-600",
              "bg-blue-50"
            );
            btn.classList.add("border-gray-300", "text-gray-700", "bg-white");
          }
        });
      }

      function calculateTDEE() {
        const sex = getSelectedSex();
        const age = getNumber("age");
        const unitSystem = document.getElementById("unit-system").value;

        let heightCm, weightKg;
        if (unitSystem === "metric") {
          heightCm = getNumber("height-cm");
          weightKg = getNumber("weight-kg");
        } else {
          const ft = getNumber("height-ft");
          const inch = getNumber("height-in") || 0;
          const lb = getNumber("weight-lb");
          if (ft != null && inch != null) {
            heightCm = (ft * 12 + inch) * 2.54;
          }
          if (lb != null) {
            weightKg = lb * 0.45359237;
          }
        }

        const bodyFat = getNumber("bodyfat");
        const formula = document.getElementById("formula").value;
        const activity = parseFloat(
          document.getElementById("activity").value || "1.2"
        );

        const bmr = calcBMR({ sex, age, heightCm, weightKg, bodyFat, formula });
        if (!bmr) {
          alert(
            "Please check that age, height, and weight are entered correctly."
          );
          return;
        }

        const tdee = bmr * activity;

        // Calorie targets
        const cut10 = tdee * 0.9;
        const cut20 = tdee * 0.8;
        const gain10 = tdee * 1.1;
        const gain20 = tdee * 1.2;

        // Update outputs
        document.getElementById("bmr-output").textContent = `${round(
          bmr
        )} kcal/day`;
        document.getElementById("tdee-output").textContent = `${round(
          tdee
        )} kcal/day`;
        document.getElementById("cut10-output").textContent = `${round(
          cut10
        )} kcal`;
        document.getElementById("cut20-output").textContent = `${round(
          cut20
        )} kcal`;
        document.getElementById("maintain-output").textContent = `${round(
          tdee
        )} kcal`;
        document.getElementById("gain10-output").textContent = `${round(
          gain10
        )} kcal`;
        document.getElementById("gain20-output").textContent = `${round(
          gain20
        )} kcal`;

        // Store for macro calculation
        window._tdeeState = {
          bmr,
          tdee,
          cut10,
          cut20,
          gain10,
          gain20,
          weightKg,
        };

        // Default goal if none selected
        const activeGoalBtn = document.querySelector(
          ".goal-btn.border-blue-500"
        );
        const goal = activeGoalBtn ? activeGoalBtn.dataset.goal : "maintain";
        updateMacros(goal);
      }

      function getGoalCalories(goal) {
        const s = window._tdeeState;
        if (!s) return null;
        switch (goal) {
          case "loss":
            return s.cut20; // use ~20% deficit as default
          case "gain":
            return s.gain10; // use ~10% surplus as default
          case "maintain":
          default:
            return s.tdee;
        }
      }

      function updateMacros(goal) {
        const s = window._tdeeState;
        if (!s || !s.weightKg) return;

        const calories = getGoalCalories(goal);
        if (!calories) return;

        const proteinPerKg = getNumber("protein-g-per-kg") || 1.8;
        const fatPercent = getNumber("fat-percent") || 30;

        const proteinG = proteinPerKg * s.weightKg;
        const proteinKcal = proteinG * 4;

        const fatKcal = calories * (fatPercent / 100);
        const fatG = fatKcal / 9;

        let remainingKcal = calories - proteinKcal - fatKcal;
        if (remainingKcal < 0) remainingKcal = 0;
        const carbsG = remainingKcal / 4;

        document.getElementById("macro-calories").textContent = `${round(
          calories
        )} kcal`;
        document.getElementById("macro-protein").textContent = `${round(
          proteinG
        )} g`;
        document.getElementById("macro-fat").textContent = `${round(fatG)} g`;
        document.getElementById("macro-carbs").textContent = `${round(
          carbsG
        )} g`;
      }

      document.addEventListener("DOMContentLoaded", function () {
        // Sex buttons
        document.querySelectorAll(".sex-btn").forEach((btn) => {
          btn.addEventListener("click", () => {
            document.querySelectorAll(".sex-btn").forEach((b) => {
              b.classList.remove(
                "border-blue-500",
                "text-blue-600",
                "bg-blue-50"
              );
              b.classList.add("border-gray-300", "text-gray-700", "bg-white");
            });
            btn.classList.remove(
              "border-gray-300",
              "text-gray-700",
              "bg-white"
            );
            btn.classList.add("border-blue-500", "text-blue-600", "bg-blue-50");
          });
        });

        // Unit system toggle
        document
          .getElementById("unit-system")
          .addEventListener("change", function () {
            const metric = document.getElementById("metric-inputs");
            const imperial = document.getElementById("imperial-inputs");
            if (this.value === "metric") {
              metric.classList.remove("hidden");
              imperial.classList.add("hidden");
            } else {
              metric.classList.add("hidden");
              imperial.classList.remove("hidden");
            }
          });

        // Formula selection: warn if Katch selected without body fat
        document
          .getElementById("formula")
          .addEventListener("change", function () {
            if (this.value === "katch") {
              const bf = getNumber("bodyfat");
              if (!bf) {
                alert(
                  "Katch–McArdle requires a valid body fat percentage. Please enter your body fat % or choose another formula."
                );
              }
            }
          });

        // Goal buttons
        document.querySelectorAll(".goal-btn").forEach((btn) => {
          btn.addEventListener("click", () => {
            const goal = btn.dataset.goal;
            updateGoalButtons(goal);
            updateMacros(goal);
          });
        });

        // Reset macros
        document
          .getElementById("reset-macros")
          .addEventListener("click", () => {
            document.getElementById("protein-g-per-kg").value = 1.8;
            document.getElementById("fat-percent").value = 30;
            const activeGoalBtn = document.querySelector(
              ".goal-btn.border-blue-500"
            );
            const goal = activeGoalBtn
              ? activeGoalBtn.dataset.goal
              : "maintain";
            updateMacros(goal);
          });

        // Recalculate macros when macro settings change
        ["protein-g-per-kg", "fat-percent"].forEach((id) => {
          document.getElementById(id).addEventListener("input", () => {
            const activeGoalBtn = document.querySelector(
              ".goal-btn.border-blue-500"
            );
            const goal = activeGoalBtn
              ? activeGoalBtn.dataset.goal
              : "maintain";
            updateMacros(goal);
          });
        });

        // Main calculate button
        document
          .getElementById("calc-btn")
          .addEventListener("click", calculateTDEE);

        // Initial calculation with defaults
        calculateTDEE();
      });
    </script>

