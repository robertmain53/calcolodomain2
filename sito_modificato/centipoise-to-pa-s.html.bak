    <script>
      (function () {
        const cpInput = document.getElementById("cpInput");
        const pasInput = document.getElementById("pasInput");
        const precisionSelect = document.getElementById("precisionSelect");
        const swapButton = document.getElementById("swapButton");
        const resultSummary = document.getElementById("resultSummary");

        let lastEdited = "cp"; // 'cp' or 'pas'
        let swapped = false; // for UX only; math is symmetric

        function formatNumber(value) {
          if (value === "" || value === null || isNaN(value)) return "";
          const decimals = parseInt(precisionSelect.value, 10);
          return Number(value).toFixed(decimals);
        }

        function updateSummary(fromUnit, fromValue, toUnit, toValue) {
          if (
            fromValue === "" ||
            isNaN(fromValue) ||
            toValue === "" ||
            isNaN(toValue)
          ) {
            resultSummary.classList.add("hidden");
            return;
          }
          const decimals = parseInt(precisionSelect.value, 10);
          const fromStr = Number(fromValue).toFixed(decimals);
          const toStr = Number(toValue).toFixed(decimals);
          resultSummary.textContent = `${fromStr} ${fromUnit} = ${toStr} ${toUnit}`;
          resultSummary.classList.remove("hidden");
        }

        function convertFromCp() {
          const cpVal = parseFloat(cpInput.value);
          if (isNaN(cpVal)) {
            pasInput.value = "";
            resultSummary.classList.add("hidden");
            return;
          }
          const pasVal = cpVal * 0.001;
          pasInput.value = formatNumber(pasVal);
          updateSummary("cP", cpVal, "Pa·s", pasVal);
        }

        function convertFromPas() {
          const pasVal = parseFloat(pasInput.value);
          if (isNaN(pasVal)) {
            cpInput.value = "";
            resultSummary.classList.add("hidden");
            return;
          }
          const cpVal = pasVal * 1000;
          cpInput.value = formatNumber(cpVal);
          updateSummary("Pa·s", pasVal, "cP", cpVal);
        }

        cpInput.addEventListener("input", function () {
          lastEdited = "cp";
          convertFromCp();
        });

        pasInput.addEventListener("input", function () {
          lastEdited = "pas";
          convertFromPas();
        });

        precisionSelect.addEventListener("change", function () {
          if (lastEdited === "cp") {
            convertFromCp();
          } else {
            convertFromPas();
          }
        });

        swapButton.addEventListener("click", function () {
          swapped = !swapped;
          // purely cosmetic: swap labels/placeholders
          const cpLabel = document.querySelector('label[for="cpInput"]');
          const pasLabel = document.querySelector('label[for="pasInput"]');
          if (swapped) {
            cpLabel.innerHTML =
              'Pascal-second <span class="text-gray-500 font-normal">(Pa·s)</span>';
            pasLabel.innerHTML =
              'Centipoise <span class="text-gray-500 font-normal">(cP)</span>';
          } else {
            cpLabel.innerHTML =
              'Centipoise <span class="text-gray-500 font-normal">(cP)</span>';
            pasLabel.innerHTML =
              'Pascal-second <span class="text-gray-500 font-normal">(Pa·s)</span>';
          }
        });
      })();
    </script>

