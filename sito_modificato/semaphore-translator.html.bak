    <script>
      // Semaphore mapping: character -> {lPos, rPos, phonetic}
      // Positions 0-7 as described in the content.
      const SEMAPHORE_MAP = {
        A: { l: 6, r: 1, p: "ALFA" },
        B: { l: 6, r: 2, p: "BRAVO" },
        C: { l: 6, r: 3, p: "CHARLIE" },
        D: { l: 6, r: 4, p: "DELTA" },
        E: { l: 6, r: 5, p: "ECHO" },
        F: { l: 6, r: 7, p: "FOXTROT" },
        G: { l: 7, r: 1, p: "GOLF" },
        H: { l: 7, r: 2, p: "HOTEL" },
        I: { l: 7, r: 3, p: "INDIA" },
        J: { l: 7, r: 4, p: "JULIETT" },
        K: { l: 7, r: 5, p: "KILO" },
        L: { l: 7, r: 6, p: "LIMA" },
        M: { l: 0, r: 1, p: "MIKE" },
        N: { l: 0, r: 2, p: "NOVEMBER" },
        O: { l: 0, r: 3, p: "OSCAR" },
        P: { l: 0, r: 4, p: "PAPA" },
        Q: { l: 0, r: 5, p: "QUEBEC" },
        R: { l: 0, r: 6, p: "ROMEO" },
        S: { l: 1, r: 2, p: "SIERRA" },
        T: { l: 1, r: 3, p: "TANGO" },
        U: { l: 1, r: 4, p: "UNIFORM" },
        V: { l: 1, r: 5, p: "VICTOR" },
        W: { l: 1, r: 6, p: "WHISKEY" },
        X: { l: 1, r: 7, p: "XRAY" },
        Y: { l: 2, r: 3, p: "YANKEE" },
        Z: { l: 2, r: 4, p: "ZULU" },
        0: { l: 2, r: 5, p: "ZERO" },
        1: { l: 2, r: 6, p: "ONE" },
        2: { l: 2, r: 7, p: "TWO" },
        3: { l: 3, r: 4, p: "THREE" },
        4: { l: 3, r: 5, p: "FOUR" },
        5: { l: 3, r: 6, p: "FIVE" },
        6: { l: 3, r: 7, p: "SIX" },
        7: { l: 4, r: 5, p: "SEVEN" },
        8: { l: 4, r: 6, p: "EIGHT" },
        9: { l: 4, r: 7, p: "NINE" },
        ".": { l: 5, r: 6, p: "PERIOD" },
        ",": { l: 5, r: 7, p: "COMMA" },
        "?": { l: 5, r: 0, p: "QUESTION" },
        "!": { l: 5, r: 1, p: "EXCLAMATION" },
        "/": { l: 5, r: 2, p: "SLASH" },
      };

      const POS_TO_ANGLE = {
        0: 90,
        1: 45,
        2: 0,
        3: -45,
        4: -90,
        5: -135,
        6: 180,
        7: 135,
      };

      function setArms(leftEl, rightEl, lPos, rPos) {
        const lAngle = POS_TO_ANGLE[lPos];
        const rAngle = POS_TO_ANGLE[rPos];
        if (typeof lAngle === "number") {
          leftEl.style.transform = `rotate(${lAngle}deg)`;
        }
        if (typeof rAngle === "number") {
          rightEl.style.transform = `rotate(${rAngle}deg)`;
        }
      }

      // Encode logic
      const encodeInput = document.getElementById("encode-input");
      const encodeGenerateBtn = document.getElementById("encode-generate");
      const encodeSequenceEl = document.getElementById("encode-sequence");
      const encodeStepsEl = document.getElementById("encode-steps");
      const encodeStatusEl = document.getElementById("encode-status");
      const encodeArmLeft = document.getElementById("encode-arm-left");
      const encodeArmRight = document.getElementById("encode-arm-right");
      const encodePosLabel = document.getElementById("encode-position-label");
      const encodeShowPhonetic = document.getElementById(
        "encode-show-phonetic"
      );
      const encodeAutoStep = document.getElementById("encode-auto-step");
      const encodeSpeed = document.getElementById("encode-speed");
      const encodePlayBtn = document.getElementById("encode-play");
      const encodePauseBtn = document.getElementById("encode-pause");
      const encodePrevBtn = document.getElementById("encode-step-prev");
      const encodeNextBtn = document.getElementById("encode-step-next");

      let encodeSteps = [];
      let encodeIndex = -1;
      let encodeTimer = null;

      function buildEncodeSteps() {
        const raw = encodeInput.value || "";
        const upper = raw.toUpperCase();
        encodeSteps = [];
        for (const ch of upper) {
          if (ch === " ") {
            encodeSteps.push({ char: " ", type: "space" });
            continue;
          }
          if (SEMAPHORE_MAP[ch]) {
            const m = SEMAPHORE_MAP[ch];
            encodeSteps.push({
              char: ch,
              l: m.l,
              r: m.r,
              phonetic: m.p || "",
            });
          }
        }
        encodeIndex = encodeSteps.length ? 0 : -1;
      }

      function renderEncodeSequence() {
        if (!encodeSteps.length) {
          encodeSequenceEl.textContent = "No valid characters to encode.";
          return;
        }
        const parts = encodeSteps.map((step) => {
          if (step.type === "space") return "[space]";
          return `${step.char}(${step.l}-${step.r})`;
        });
        encodeSequenceEl.textContent = parts.join("  ");
      }

      function renderEncodeStepsList() {
        encodeStepsEl.innerHTML = "";
        encodeSteps.forEach((step, idx) => {
          const btn = document.createElement("button");
          btn.type = "button";
          btn.className =
            "border border-gray-200 rounded-md px-2 py-1 text-xs text-left hover:border-blue-400";
          btn.dataset.index = idx;
          if (step.type === "space") {
            btn.innerHTML = `<span class="font-semibold">[space]</span>`;
          } else {
            const phonetic =
              encodeShowPhonetic.checked && step.phonetic
                ? `<div class="text-[0.65rem] text-gray-500">${step.phonetic}</div>`
                : "";
            btn.innerHTML = `<div class="font-semibold">${step.char}</div><div class="mono">${step.l}-${step.r}</div>${phonetic}`;
          }
          btn.addEventListener("click", () => {
            stopEncodeTimer();
            setEncodeIndex(idx);
          });
          encodeStepsEl.appendChild(btn);
        });
        highlightEncodeStep();
      }

      function highlightEncodeStep() {
        Array.from(encodeStepsEl.children).forEach((el, idx) => {
          el.classList.toggle("flag-step-active", idx === encodeIndex);
        });
      }

      function updateEncodeFigure() {
        if (encodeIndex < 0 || encodeIndex >= encodeSteps.length) {
          encodePosLabel.textContent = "Waiting for input…";
          return;
        }
        const step = encodeSteps[encodeIndex];
        if (step.type === "space") {
          encodeArmLeft.style.transform = "rotate(0deg)";
          encodeArmRight.style.transform = "rotate(0deg)";
          encodePosLabel.textContent = "Space / rest";
          return;
        }
        setArms(encodeArmLeft, encodeArmRight, step.l, step.r);
        const angleText = `${step.l}-${step.r} (${POS_TO_ANGLE[step.l]}° / ${
          POS_TO_ANGLE[step.r]
        }°)`;
        encodePosLabel.textContent = `${step.char} – ${angleText}`;
      }

      function setEncodeIndex(idx) {
        if (!encodeSteps.length) return;
        encodeIndex = Math.max(0, Math.min(encodeSteps.length - 1, idx));
        highlightEncodeStep();
        updateEncodeFigure();
        encodeStatusEl.textContent = `Step ${encodeIndex + 1} of ${
          encodeSteps.length
        }`;
      }

      function nextEncodeStep() {
        if (!encodeSteps.length) return;
        if (encodeIndex < encodeSteps.length - 1) {
          setEncodeIndex(encodeIndex + 1);
        } else if (encodeAutoStep.checked) {
          stopEncodeTimer();
        }
      }

      function prevEncodeStep() {
        if (!encodeSteps.length) return;
        if (encodeIndex > 0) {
          setEncodeIndex(encodeIndex - 1);
        }
      }

      function startEncodeTimer() {
        if (!encodeSteps.length) return;
        stopEncodeTimer();
        const base = 900; // ms
        const factor = parseFloat(encodeSpeed.value) || 1;
        const interval = base / factor;
        encodeTimer = setInterval(() => {
          if (encodeIndex >= encodeSteps.length - 1) {
            stopEncodeTimer();
          } else {
            nextEncodeStep();
          }
        }, interval);
      }

      function stopEncodeTimer() {
        if (encodeTimer) {
          clearInterval(encodeTimer);
          encodeTimer = null;
        }
      }

      encodeGenerateBtn.addEventListener("click", () => {
        stopEncodeTimer();
        buildEncodeSteps();
        if (!encodeSteps.length) {
          encodeSequenceEl.textContent = "No valid characters to encode.";
          encodeStepsEl.innerHTML = "";
          encodeStatusEl.textContent = "";
          encodePosLabel.textContent = "Waiting for input…";
          return;
        }
        renderEncodeSequence();
        renderEncodeStepsList();
        setEncodeIndex(0);
      });

      encodePlayBtn.addEventListener("click", () => {
        if (!encodeSteps.length) {
          buildEncodeSteps();
          if (!encodeSteps.length) return;
          renderEncodeSequence();
          renderEncodeStepsList();
          setEncodeIndex(0);
        }
        startEncodeTimer();
      });

      encodePauseBtn.addEventListener("click", stopEncodeTimer);
      encodeNextBtn.addEventListener("click", () => {
        stopEncodeTimer();
        nextEncodeStep();
      });
      encodePrevBtn.addEventListener("click", () => {
        stopEncodeTimer();
        prevEncodeStep();
      });
      encodeSpeed.addEventListener("input", () => {
        if (encodeTimer) startEncodeTimer();
      });
      encodeShowPhonetic.addEventListener("change", () => {
        if (encodeSteps.length) renderEncodeStepsList();
      });

      // Decode logic
      const decodeInput = document.getElementById("decode-input");
      const decodeRunBtn = document.getElementById("decode-run");
      const decodeStatusEl = document.getElementById("decode-status");
      const decodeOutputEl = document.getElementById("decode-output");
      const decodeArmLeft = document.getElementById("decode-arm-left");
      const decodeArmRight = document.getElementById("decode-arm-right");
      const decodePosLabel = document.getElementById("decode-position-label");
      const decodeHelperEl = document.getElementById("decode-helper");

      // Reverse map: "l-r" -> char
      const REVERSE_MAP = {};
      Object.keys(SEMAPHORE_MAP).forEach((ch) => {
        const m = SEMAPHORE_MAP[ch];
        const key = `${m.l}-${m.r}`;
        if (!REVERSE_MAP[key]) REVERSE_MAP[key] = ch;
      });

      function decodePositions() {
        const raw = decodeInput.value.trim();
        if (!raw) {
          decodeOutputEl.textContent = "Please enter positions to decode.";
          decodeStatusEl.textContent = "";
          return;
        }
        const tokens = raw.split(/\s+/);
        let result = "";
        let lastChar = null;
        tokens.forEach((tok) => {
          if (!tok) return;
          if (tok.toUpperCase() === "X") {
            result += " ";
            lastChar = " ";
            return;
          }
          const m = tok.match(/^([0-7])[-:]([0-7])$/);
          if (!m) {
            result += "?";
            lastChar = "?";
            return;
          }
          const key = `${m[1]}-${m[2]}`;
          const ch = REVERSE_MAP[key] || "?";
          result += ch;
          lastChar = ch;
        });
        decodeOutputEl.textContent = result || "(no decodable characters)";
        decodeStatusEl.textContent = `Decoded ${tokens.length} token(s).`;

        // Show last token on figure if valid
        const lastTok = tokens[tokens.length - 1] || "";
        const m = lastTok.match(/^([0-7])[-:]([0-7])$/);
        if (m) {
          const l = parseInt(m[1], 10);
          const r = parseInt(m[2], 10);
          setArms(decodeArmLeft, decodeArmRight, l, r);
          const angleText = `${l}-${r} (${POS_TO_ANGLE[l]}° / ${POS_TO_ANGLE[r]}°)`;
          const ch = REVERSE_MAP[`${l}-${r}`] || "?";
          decodePosLabel.textContent = `${ch} – ${angleText}`;
        } else {
          decodePosLabel.textContent = "Last token not a valid position.";
        }
      }

      decodeRunBtn.addEventListener("click", decodePositions);

      // Helper grid for decode: clickable letters
      function buildDecodeHelper() {
        const chars = "ABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789";
        for (const ch of chars) {
          const m = SEMAPHORE_MAP[ch];
          if (!m) continue;
          const btn = document.createElement("button");
          btn.type = "button";
          btn.className =
            "border border-gray-200 rounded px-1 py-0.5 hover:border-blue-400";
          btn.textContent = ch;
          btn.title = `${ch} = ${m.l}-${m.r}`;
          btn.addEventListener("click", () => {
            const token = `${m.l}-${m.r}`;
            if (!decodeInput.value.trim()) {
              decodeInput.value = token;
            } else {
              decodeInput.value = decodeInput.value.trim() + " " + token;
            }
            decodeInput.focus();
          });
          decodeHelperEl.appendChild(btn);
        }
        // Add X for space
        const xBtn = document.createElement("button");
        xBtn.type = "button";
        xBtn.className =
          "border border-gray-200 rounded px-1 py-0.5 hover:border-blue-400";
        xBtn.textContent = "␣";
        xBtn.title = "Space (X token)";
        xBtn.addEventListener("click", () => {
          if (!decodeInput.value.trim()) {
            decodeInput.value = "X";
          } else {
            decodeInput.value = decodeInput.value.trim() + " X";
          }
          decodeInput.focus();
        });
        decodeHelperEl.appendChild(xBtn);
      }

      // Build reference table
      function buildSemaphoreTable() {
        const body = document.getElementById("semaphore-table-body");
        const entries = Object.entries(SEMAPHORE_MAP).sort((a, b) => {
          const ca = a[0],
            cb = b[0];
          return ca.localeCompare(cb);
        });
        for (const [ch, m] of entries) {
          const tr = document.createElement("tr");
          tr.className = "border-t";
          const angles = `${POS_TO_ANGLE[m.l]}° / ${POS_TO_ANGLE[m.r]}°`;
          tr.innerHTML = `
        <td class="px-2 py-1 border-r font-semibold mono">${ch}</td>
        <td class="px-2 py-1 border-r mono">${m.l}-${m.r}</td>
        <td class="px-2 py-1 border-r mono">${angles}</td>
        <td class="px-2 py-1">${m.p || ""}</td>
      `;
          body.appendChild(tr);
        }
      }

      // Tabs
      const tabEncode = document.getElementById("tab-encode");
      const tabDecode = document.getElementById("tab-decode");
      const panelEncode = document.getElementById("panel-encode");
      const panelDecode = document.getElementById("panel-decode");

      tabEncode.addEventListener("click", () => {
        tabEncode.classList.add("border-blue-600", "text-blue-600");
        tabEncode.classList.remove("text-gray-600");
        tabDecode.classList.remove("border-blue-600", "text-blue-600");
        tabDecode.classList.add("text-gray-600");
        panelEncode.classList.remove("hidden");
        panelDecode.classList.add("hidden");
      });

      tabDecode.addEventListener("click", () => {
        tabDecode.classList.add("border-blue-600", "text-blue-600");
        tabDecode.classList.remove("text-gray-600");
        tabEncode.classList.remove("border-blue-600", "text-blue-600");
        tabEncode.classList.add("text-gray-600");
        panelDecode.classList.remove("hidden");
        panelEncode.classList.add("hidden");
      });

      // Init
      buildDecodeHelper();
      buildSemaphoreTable();
    </script>

