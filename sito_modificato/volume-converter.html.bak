          <div class="prose">
            <h2>How this volume converter works</h2>
            <p>We use cubic meters (m³) as the internal base unit. Each time you type a value, it is converted to m³ and then to all other units using precise SI factors.</p>
            <div class="formula-box">
              <p><strong>Key relations</strong></p>
              <ul>
                <li>1 L = 0.001 m³</li>
                <li>1 mL = 0.000001 m³</li>
                <li>1 ft³ = 0.028316846592 m³</li>
                <li>1 in³ = 0.000016387064 m³</li>
                <li>1 US gal = 0.003785411784 m³</li>
                <li>1 UK gal = 0.00454609 m³</li>
              </ul>
            </div>
            <h3>FAQ</h3>
            <ul>
              <li><strong>Why do I see many decimals?</strong> For industrial and engineering contexts, exact factors are useful. Round to 2–3 decimals for daily work.</li>
              <li><strong>US vs UK gallons?</strong> Pick the right radio button above to switch.</li>
            </ul>
          </div>

          <script>
            (function(){
              const litersEl = document.getElementById('inp-liters');
              const mlEl = document.getElementById('inp-ml');
              const galEl = document.getElementById('inp-gal');
              const m3El = document.getElementById('inp-m3');
              const ft3El = document.getElementById('inp-ft3');
              const in3El = document.getElementById('inp-in3');
              const btnClear = document.getElementById('btn-clear');
              const btnPresets = document.getElementById('btn-presets');
              const presetPanel = document.getElementById('preset-panel');
              const gallonRadios = document.querySelectorAll('input[name="gallon-type"]');

              let lock = false;

              function getGallonFactor(){
                const checked = document.querySelector('input[name="gallon-type"]:checked');
                return (checked && checked.value === 'uk') ? 0.00454609 : 0.003785411784;
              }

              // ---- conversion helpers to/from m3 ----
              function litersToM3(l){ return l * 0.001; }
              function mlToM3(ml){ return ml * 0.000001; }
              function galToM3(g){ return g * getGallonFactor(); }
              function m3ToLiters(m){ return m / 0.001; }
              function m3ToML(m){ return m / 0.000001; }
              function m3ToGallons(m){ return m / getGallonFactor(); }
              function ft3ToM3(ft){ return ft * 0.028316846592; }
              function in3ToM3(i){ return i * 0.000016387064; }
              function m3ToFT3(m){ return m / 0.028316846592; }
              function m3ToIN3(m){ return m / 0.000016387064; }

              function updateFromM3(m3){
                lock = true;
                litersEl.value = m3ToLiters(m3).toFixed(6);
                mlEl.value = m3ToML(m3).toFixed(2);
                galEl.value = m3ToGallons(m3).toFixed(6);
                m3El.value = m3.toFixed(6);
                ft3El.value = m3ToFT3(m3).toFixed(6);
                in3El.value = m3ToIN3(m3).toFixed(2);
                lock = false;
              }

              litersEl.addEventListener('input', () => {
                if (lock) return;
                const v = parseFloat(litersEl.value);
                if (!isNaN(v)) updateFromM3(litersToM3(v));
              });
              mlEl.addEventListener('input', () => {
                if (lock) return;
                const v = parseFloat(mlEl.value);
                if (!isNaN(v)) updateFromM3(mlToM3(v));
              });
              galEl.addEventListener('input', () => {
                if (lock) return;
                const v = parseFloat(galEl.value);
                if (!isNaN(v)) updateFromM3(galToM3(v));
              });
              m3El.addEventListener('input', () => {
                if (lock) return;
                const v = parseFloat(m3El.value);
                if (!isNaN(v)) updateFromM3(v);
              });
              ft3El.addEventListener('input', () => {
                if (lock) return;
                const v = parseFloat(ft3El.value);
                if (!isNaN(v)) updateFromM3(ft3ToM3(v));
              });
              in3El.addEventListener('input', () => {
                if (lock) return;
                const v = parseFloat(in3El.value);
                if (!isNaN(v)) updateFromM3(in3ToM3(v));
              });

              btnClear.addEventListener('click', () => {
                litersEl.value = "";
                mlEl.value = "";
                galEl.value = "";
                m3El.value = "";
                ft3El.value = "";
                in3El.value = "";
              });

              btnPresets.addEventListener('click', () => {
                presetPanel.classList.toggle('hidden');
              });

              presetPanel.addEventListener('click', (e) => {
                const btn = e.target.closest('.preset-btn');
                if (!btn) return;
                const liters = parseFloat(btn.dataset.liters);
                if (!isNaN(liters)) {
                  updateFromM3(litersToM3(liters));
                }
              });

              gallonRadios.forEach(r => {
                r.addEventListener('change', () => {
                  // recompute everything from current m3 value (from liters)
                  const currentLiters = parseFloat(litersEl.value);
                  if (!isNaN(currentLiters)) {
                    updateFromM3(litersToM3(currentLiters));
                  }
                });
              });

              // initial sync
              const initLiters = parseFloat(litersEl.value);
              updateFromM3(litersToM3(initLiters));
            })();
          </script>

