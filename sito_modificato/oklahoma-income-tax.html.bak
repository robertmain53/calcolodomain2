    <script>
      // --- Oklahoma Income Tax Calculator Logic ---
      // NOTE: Brackets and deductions are approximate and for educational use only.
      const okData = {
        // Simple two-year support so we can adjust later if needed
        2024: {
          brackets: {
            single: [
              { upTo: 1000, rate: 0.0025 },
              { upTo: 2500, rate: 0.0075 },
              { upTo: 3750, rate: 0.0175 },
              { upTo: 4900, rate: 0.0275 },
              { upTo: 7200, rate: 0.0375 },
              { upTo: Infinity, rate: 0.0475 },
            ],
            mfj: [
              { upTo: 2000, rate: 0.0025 },
              { upTo: 5000, rate: 0.0075 },
              { upTo: 7500, rate: 0.0175 },
              { upTo: 9800, rate: 0.0275 },
              { upTo: 12200, rate: 0.0375 },
              { upTo: Infinity, rate: 0.0475 },
            ],
            mfs: [
              { upTo: 1000, rate: 0.0025 },
              { upTo: 2500, rate: 0.0075 },
              { upTo: 3750, rate: 0.0175 },
              { upTo: 4900, rate: 0.0275 },
              { upTo: 7200, rate: 0.0375 },
              { upTo: Infinity, rate: 0.0475 },
            ],
            hoh: [
              { upTo: 1000, rate: 0.0025 },
              { upTo: 2500, rate: 0.0075 },
              { upTo: 3750, rate: 0.0175 },
              { upTo: 4900, rate: 0.0275 },
              { upTo: 7200, rate: 0.0375 },
              { upTo: Infinity, rate: 0.0475 },
            ],
          },
          standardDeduction: {
            single: 6350,
            mfj: 12700,
            mfs: 6350,
            hoh: 9350,
          },
        },
        2025: {
          // Assume same as 2024 for now; update when official 2025 law is final
          brackets: {},
          standardDeduction: {},
        },
      };

      // Copy 2024 data into 2025 as placeholder
      okData["2025"].brackets = JSON.parse(
        JSON.stringify(okData["2024"].brackets)
      );
      okData["2025"].standardDeduction = JSON.parse(
        JSON.stringify(okData["2024"].standardDeduction)
      );

      function annualizeIncome(amount, period) {
        const val = Number(amount) || 0;
        if (val <= 0) return 0;
        switch (period) {
          case "monthly":
            return val * 12;
          case "biweekly":
            return val * 26;
          case "weekly":
            return val * 52;
          default:
            return val;
        }
      }

      function computeOkTax(
        annualIncome,
        filingStatus,
        year,
        deductionOverride,
        creditsInput
      ) {
        const yearData = okData[year] || okData["2025"];
        const brackets =
          yearData.brackets[filingStatus] || yearData.brackets.single;
        const defaultDeduction = yearData.standardDeduction[filingStatus] || 0;

        const deduction = Math.max(
          0,
          deductionOverride !== null ? deductionOverride : defaultDeduction
        );
        const taxable = Math.max(0, annualIncome - deduction);

        let remaining = taxable;
        let lastLimit = 0;
        let tax = 0;
        let marginalRate = 0;

        for (const bracket of brackets) {
          if (remaining <= 0) break;
          const span = Math.min(remaining, bracket.upTo - lastLimit);
          if (span > 0) {
            tax += span * bracket.rate;
            marginalRate = bracket.rate;
            remaining -= span;
          }
          lastLimit = bracket.upTo;
        }

        const credits = Math.max(0, creditsInput || 0);
        const netTax = Math.max(0, tax - credits);

        return {
          taxableIncome: taxable,
          deduction,
          preCreditTax: tax,
          credits,
          netTax,
          marginalRate,
        };
      }

      function formatMoney(value) {
        return value.toLocaleString("en-US", {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 0,
        });
      }

      function formatPercent(value) {
        return (value * 100).toFixed(1) + "%";
      }

      document.addEventListener("DOMContentLoaded", () => {
        const form = document.getElementById("ok-tax-form");
        const incomeInput = document.getElementById("income");
        const incomePeriod = document.getElementById("income-period");
        const filingStatus = document.getElementById("filing-status");
        const taxYear = document.getElementById("tax-year");
        const stdDeduction = document.getElementById("std-deduction");
        const okCredits = document.getElementById("ok-credits");
        const errorEl = document.getElementById("calc-error");

        const resultTax = document.getElementById("result-tax");
        const resultEffectiveRate = document.getElementById(
          "result-effective-rate"
        );
        const resultAfterTax = document.getElementById("result-after-tax");
        const resultMarginalRate = document.getElementById(
          "result-marginal-rate"
        );

        const breakdownGross = document.getElementById("breakdown-gross");
        const breakdownDeduction = document.getElementById(
          "breakdown-deduction"
        );
        const breakdownTaxable = document.getElementById("breakdown-taxable");
        const breakdownPrecredit = document.getElementById(
          "breakdown-precredit"
        );
        const breakdownCredits = document.getElementById("breakdown-credits");
        const breakdownNet = document.getElementById("breakdown-net");

        function runCalculation() {
          const incomeVal = parseFloat(incomeInput.value);
          if (isNaN(incomeVal) || incomeVal < 0) {
            errorEl.classList.remove("hidden");
            return;
          }
          errorEl.classList.add("hidden");

          const annualIncome = annualizeIncome(incomeVal, incomePeriod.value);
          const year = taxYear.value;
          const status = filingStatus.value;

          const deductionOverride =
            stdDeduction.value !== ""
              ? Math.max(0, parseFloat(stdDeduction.value) || 0)
              : null;
          const creditsVal = Math.max(0, parseFloat(okCredits.value) || 0);

          const calc = computeOkTax(
            annualIncome,
            status,
            year,
            deductionOverride,
            creditsVal
          );

          const netTax = calc.netTax;
          const afterTax = Math.max(0, annualIncome - netTax);
          const effectiveRate = annualIncome > 0 ? netTax / annualIncome : 0;

          resultTax.textContent = formatMoney(netTax);
          resultAfterTax.textContent = formatMoney(afterTax);
          resultEffectiveRate.textContent = `Effective state tax rate: ${formatPercent(
            effectiveRate
          )}`;
          resultMarginalRate.textContent = `Top marginal OK rate: ${formatPercent(
            calc.marginalRate
          )}`;

          breakdownGross.textContent = formatMoney(annualIncome);
          breakdownDeduction.textContent = formatMoney(calc.deduction);
          breakdownTaxable.textContent = formatMoney(calc.taxableIncome);
          breakdownPrecredit.textContent = formatMoney(calc.preCreditTax);
          breakdownCredits.textContent = formatMoney(calc.credits);
          breakdownNet.textContent = formatMoney(netTax);
        }

        document
          .getElementById("calculate-btn")
          .addEventListener("click", runCalculation);

        document.getElementById("reset-btn").addEventListener("click", () => {
          filingStatus.value = "single";
          taxYear.value = "2025";
          incomeInput.value = 60000;
          incomePeriod.value = "annual";
          stdDeduction.value = "";
          okCredits.value = 0;
          errorEl.classList.add("hidden");
          runCalculation();
        });

        // Auto-calc on initial load
        runCalculation();

        // Optional: recalc on key changes
        [
          incomeInput,
          incomePeriod,
          filingStatus,
          taxYear,
          stdDeduction,
          okCredits,
        ].forEach((el) => {
          el.addEventListener("change", () => {
            if (!document.hidden) runCalculation();
          });
        });
      });
    </script>

