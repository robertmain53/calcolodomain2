    <script>
      // Basic AWG to kcmil mapping for common sizes used in NEC 250.66
      const awgToKcmil = {
        // Copper or aluminum; NEC table is based on area, not material
        // Values are circular mil area approximations
        14: 4110,
        12: 6530,
        10: 10380,
        8: 16510,
        6: 26240,
        4: 41740,
        3: 52620,
        2: 66360,
        1: 83690,
        0: 105600, // 1/0
        "00": 133100, // 2/0
        "000": 167800, // 3/0
        "0000": 211600, // 4/0
        // kcmil sizes (250, 300, 350, etc.) are already in kcmil
      };

      // NEC 250.66 simplified ranges in kcmil (total area) with resulting GEC sizes
      const gecTable = [
        { maxKcmil: 2, cu: "8 AWG", al: "6 AWG" }, // up to 2 AWG Cu or 1/0 Al (approx)
        { maxKcmil: 105600, cu: "8 AWG", al: "6 AWG" }, // up to 1/0 Cu
        { maxKcmil: 167800, cu: "6 AWG", al: "4 AWG" }, // >1/0 to 3/0 Cu
        { maxKcmil: 350000, cu: "4 AWG", al: "2 AWG" }, // >3/0 to 350 kcmil Cu
        { maxKcmil: 600000, cu: "2 AWG", al: "1/0 AWG" }, // >350 to 600 kcmil Cu
        { maxKcmil: 1100000, cu: "1/0 AWG", al: "3/0 AWG" }, // >600 to 1100 kcmil Cu
        { maxKcmil: Infinity, cu: "3/0 AWG", al: "250 kcmil" }, // >1100 kcmil Cu
      ];

      function findGecSize(totalKcmil, material) {
        for (const row of gecTable) {
          if (totalKcmil <= row.maxKcmil) {
            return material === "copper" ? row.cu : row.al;
          }
        }
        return material === "copper" ? "3/0 AWG" : "250 kcmil";
      }

      function applyElectrodeLimit(baseSize, material, electrodeType) {
        if (electrodeType === "standard") return baseSize;

        const orderCu = [
          "8 AWG",
          "6 AWG",
          "4 AWG",
          "2 AWG",
          "1/0 AWG",
          "2/0 AWG",
          "3/0 AWG",
        ];
        const orderAl = [
          "6 AWG",
          "4 AWG",
          "2 AWG",
          "1/0 AWG",
          "3/0 AWG",
          "4/0 AWG",
          "250 kcmil",
        ];

        if (electrodeType === "concrete") {
          // Max 4 AWG Cu; Al not typically used directly in concrete-encased applications
          if (material === "copper") {
            const idx = orderCu.indexOf(baseSize);
            const limitIdx = orderCu.indexOf("4 AWG");
            if (idx > limitIdx && limitIdx !== -1) return "4 AWG";
          }
          return baseSize;
        }

        if (electrodeType === "rod") {
          if (material === "copper") {
            const idx = orderCu.indexOf(baseSize);
            const limitIdx = orderCu.indexOf("6 AWG");
            if (idx > limitIdx && limitIdx !== -1) return "6 AWG";
          } else {
            const idx = orderAl.indexOf(baseSize);
            const limitIdx = orderAl.indexOf("4 AWG");
            if (idx > limitIdx && limitIdx !== -1) return "4 AWG";
          }
          return baseSize;
        }

        return baseSize;
      }

      function parseAwgToKcmil(value) {
        // Interpret 0, 00, 000, 0000 as 1/0, 2/0, 3/0, 4/0
        if (value === 0) return awgToKcmil["0"];
        if (value === 00 || value === 2) return awgToKcmil["00"];
        if (value === 000 || value === 3) return awgToKcmil["000"];
        if (value === 4 && !awgToKcmil["4"]) return 41740;
        const key = String(value);
        if (awgToKcmil[key]) return awgToKcmil[key];
        // If not in map, approximate: treat as kcmil if >= 250
        if (value >= 250) return value * 1000;
        return NaN;
      }

      document
        .getElementById("gec-calc-btn")
        .addEventListener("click", function () {
          const errorEl = document.getElementById("gec-error");
          const resultCard = document.getElementById("gec-result-card");
          errorEl.textContent = "";
          resultCard.classList.add("hidden");

          const parallelSets = parseInt(
            document.getElementById("gec-parallel-sets").value,
            10
          );
          const sizeVal = parseFloat(
            document.getElementById("gec-ungrounded-size").value
          );
          const unit = document.getElementById("gec-ungrounded-unit").value;
          const gecMaterial = document.getElementById("gec-material").value;
          const electrodeType =
            document.getElementById("gec-electrode-type").value;

          if (!parallelSets || parallelSets < 1) {
            errorEl.textContent =
              "Enter a valid number of parallel sets (1 or more).";
            return;
          }
          if (!sizeVal || sizeVal <= 0) {
            errorEl.textContent = "Enter a valid ungrounded conductor size.";
            return;
          }

          let kcmilPerConductor;
          if (unit === "awg") {
            kcmilPerConductor = parseAwgToKcmil(sizeVal);
          } else {
            // numeric kcmil
            kcmilPerConductor = sizeVal * 1000;
          }

          if (!kcmilPerConductor || !isFinite(kcmilPerConductor)) {
            errorEl.textContent =
              "Unable to interpret conductor size. For AWG, use common sizes (e.g., 3, 2, 1, 0, 00, 000). For kcmil, use the numeric size.";
            return;
          }

          const totalKcmil = kcmilPerConductor * parallelSets;
          const baseSize = findGecSize(totalKcmil, gecMaterial);
          const limitedSize = applyElectrodeLimit(
            baseSize,
            gecMaterial,
            electrodeType
          );

          document.getElementById("gec-result-material").textContent =
            gecMaterial === "copper"
              ? "Copper"
              : "Aluminum / Copper-clad aluminum";
          document.getElementById("gec-result-size").textContent = limitedSize;

          let notes = `Total equivalent ungrounded conductor area ≈ ${Math.round(
            totalKcmil / 1000
          )} kcmil. `;
          notes += `Base GEC size from NEC 250.66: ${baseSize}. `;
          if (limitedSize !== baseSize) {
            notes += `Electrode type limitation (NEC 250.66(A)-(C)) caps the required size at ${limitedSize}.`;
          } else {
            notes += `No additional reduction from electrode type limitations was applied.`;
          }
          document.getElementById("gec-result-notes").textContent = notes;

          resultCard.classList.remove("hidden");
        });

      document
        .getElementById("gec-reset-btn")
        .addEventListener("click", function () {
          document.getElementById("gec-system-type").value = "service";
          document.getElementById("gec-electrode-type").value = "standard";
          document.getElementById("gec-material").value = "copper";
          document.getElementById("gec-parallel-sets").value = "1";
          document.getElementById("gec-ungrounded-size").value = "3";
          document.getElementById("gec-ungrounded-unit").value = "awg";
          document.getElementById("gec-ungrounded-material").value = "copper";
          document.getElementById("gec-error").textContent = "";
          document.getElementById("gec-result-card").classList.add("hidden");
        });
    </script>

