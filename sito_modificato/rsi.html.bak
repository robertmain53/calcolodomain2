    <script src="https://cdn.jsdelivr.net/npm/chart.js@4.4.1/dist/chart.umd.min.js"></script>
    <script>
      // Parse input lines into price series
      function parsePriceInput(sourceType, raw) {
        const lines = raw
          .split(/\r?\n/)
          .map((l) => l.trim())
          .filter((l) => l.length > 0);
        const prices = [];
        for (const line of lines) {
          const parts = line
            .split(/[,\s;]+/)
            .map((p) => p.trim())
            .filter((p) => p.length > 0);
          if (sourceType === "close") {
            const v = parseFloat(parts[0]);
            if (!isNaN(v)) prices.push(v);
          } else {
            if (parts.length < 3) continue;
            const h = parseFloat(parts[0]);
            const l = parseFloat(parts[1]);
            const c = parseFloat(parts[2]);
            if ([h, l, c].some(isNaN)) continue;
            const typical = (h + l + c) / 3;
            prices.push(typical);
          }
        }
        return prices;
      }

      // Compute RSI using Wilder's smoothing
      function computeRSI(prices, period) {
        const n = period;
        if (prices.length < n + 1) {
          throw new Error(
            `Need at least ${n + 1} data points for period ${n}.`
          );
        }

        const changes = [];
        for (let i = 1; i < prices.length; i++) {
          changes.push(prices[i] - prices[i - 1]);
        }

        let gainSum = 0;
        let lossSum = 0;
        for (let i = 0; i < n; i++) {
          const ch = changes[i];
          if (ch > 0) gainSum += ch;
          else lossSum += -ch;
        }

        let avgGain = gainSum / n;
        let avgLoss = lossSum / n;

        const results = [];
        // First RSI corresponds to index n (prices[n])
        let rs = avgLoss === 0 ? Infinity : avgGain / avgLoss;
        let rsi = avgLoss === 0 ? 100 : 100 - 100 / (1 + rs);
        results.push({
          index: n,
          price: prices[n],
          change: changes[n - 1],
          avgGain,
          avgLoss,
          rsi,
        });

        for (let i = n; i < changes.length; i++) {
          const ch = changes[i];
          const gain = ch > 0 ? ch : 0;
          const loss = ch < 0 ? -ch : 0;

          avgGain = (avgGain * (n - 1) + gain) / n;
          avgLoss = (avgLoss * (n - 1) + loss) / n;

          rs = avgLoss === 0 ? Infinity : avgGain / avgLoss;
          rsi = avgLoss === 0 ? 100 : 100 - 100 / (1 + rs);

          results.push({
            index: i + 1,
            price: prices[i + 1],
            change: ch,
            avgGain,
            avgLoss,
            rsi,
          });
        }

        return results;
      }

      function classifyRSI(value) {
        if (value >= 70) return { label: "Overbought", color: "text-red-600" };
        if (value <= 30) return { label: "Oversold", color: "text-green-600" };
        if (value >= 55)
          return { label: "Bullish momentum", color: "text-emerald-600" };
        if (value <= 45)
          return { label: "Bearish momentum", color: "text-amber-600" };
        return { label: "Neutral", color: "text-gray-700" };
      }

      let rsiChart = null;

      function renderRSIChart(ctx, prices, rsiResults, period) {
        const labels = rsiResults.map((r) => r.index);
        const rsiValues = rsiResults.map((r) => r.rsi);
        const priceValues = rsiResults.map((r) => r.price);

        if (rsiChart) {
          rsiChart.destroy();
        }

        rsiChart = new Chart(ctx, {
          type: "line",
          data: {
            labels,
            datasets: [
              {
                label: "Price",
                data: priceValues,
                borderColor: "#4b5563",
                backgroundColor: "rgba(75,85,99,0.1)",
                yAxisID: "y",
                tension: 0.2,
                pointRadius: 0,
              },
              {
                label: `RSI (${period})`,
                data: rsiValues,
                borderColor: "#2563eb",
                backgroundColor: "rgba(37,99,235,0.1)",
                yAxisID: "y1",
                tension: 0.2,
                pointRadius: 0,
              },
              {
                label: "Overbought (70)",
                data: rsiValues.map(() => 70),
                borderColor: "rgba(220,38,38,0.4)",
                borderDash: [4, 4],
                yAxisID: "y1",
                pointRadius: 0,
              },
              {
                label: "Oversold (30)",
                data: rsiValues.map(() => 30),
                borderColor: "rgba(22,163,74,0.4)",
                borderDash: [4, 4],
                yAxisID: "y1",
                pointRadius: 0,
              },
            ],
          },
          options: {
            responsive: true,
            maintainAspectRatio: false,
            interaction: {
              mode: "index",
              intersect: false,
            },
            plugins: {
              legend: {
                display: true,
              },
              tooltip: {
                callbacks: {
                  label: function (context) {
                    const label = context.dataset.label || "";
                    const value = context.parsed.y;
                    return `${label}: ${value.toFixed(2)}`;
                  },
                },
              },
            },
            scales: {
              y: {
                type: "linear",
                position: "left",
                title: {
                  display: true,
                  text: "Price",
                },
                ticks: {
                  callback: (v) => v.toFixed(2),
                },
              },
              y1: {
                type: "linear",
                position: "right",
                min: 0,
                max: 100,
                title: {
                  display: true,
                  text: "RSI",
                },
                grid: {
                  drawOnChartArea: false,
                },
              },
            },
          },
        });
      }

      function renderRSITable(tbody, results) {
        tbody.innerHTML = "";
        const formatter = (v) =>
          v === null || v === undefined || isNaN(v) ? "" : v.toFixed(4);
        results.forEach((row, idx) => {
          const tr = document.createElement("tr");
          tr.className = idx === results.length - 1 ? "bg-blue-50" : "";
          tr.innerHTML = `
        <td class="px-2 py-1 whitespace-nowrap text-gray-700">${row.index}</td>
        <td class="px-2 py-1 whitespace-nowrap text-gray-700">${formatter(
          row.price
        )}</td>
        <td class="px-2 py-1 whitespace-nowrap ${
          row.change >= 0 ? "text-emerald-700" : "text-red-700"
        }">${formatter(row.change)}</td>
        <td class="px-2 py-1 whitespace-nowrap text-gray-700">${formatter(
          row.avgGain
        )}</td>
        <td class="px-2 py-1 whitespace-nowrap text-gray-700">${formatter(
          row.avgLoss
        )}</td>
        <td class="px-2 py-1 whitespace-nowrap font-semibold text-gray-900">${formatter(
          row.rsi
        )}</td>
      `;
          tbody.appendChild(tr);
        });
      }

      function downloadRSICSV(results) {
        if (!results || results.length === 0) return;
        const header = [
          "Index",
          "Price",
          "Change",
          "AvgGain",
          "AvgLoss",
          "RSI",
        ];
        const rows = results.map((r) => [
          r.index,
          r.price,
          r.change,
          r.avgGain,
          r.avgLoss,
          r.rsi,
        ]);
        const csv = [header.join(","), ...rows.map((r) => r.join(","))].join(
          "\n"
        );
        const blob = new Blob([csv], { type: "text/csv;charset=utf-8;" });
        const url = URL.createObjectURL(blob);
        const a = document.createElement("a");
        a.href = url;
        a.download = "rsi-values.csv";
        document.body.appendChild(a);
        a.click();
        document.body.removeChild(a);
        URL.revokeObjectURL(url);
      }

      document.addEventListener("DOMContentLoaded", () => {
        const periodInput = document.getElementById("rsi-period");
        const sourceSelect = document.getElementById("rsi-source");
        const dataInput = document.getElementById("rsi-data-input");
        const calcBtn = document.getElementById("rsi-calc-btn");
        const clearBtn = document.getElementById("rsi-clear-btn");
        const sampleBtn = document.getElementById("rsi-sample-btn");
        const errorEl = document.getElementById("rsi-error");
        const mainResult = document.getElementById("rsi-main-result");
        const latestValueEl = document.getElementById("rsi-latest-value");
        const statusEl = document.getElementById("rsi-status");
        const chartContainer = document.getElementById("rsi-chart-container");
        const chartCanvas = document.getElementById("rsi-chart");
        const tableContainer = document.getElementById("rsi-table-container");
        const tableBody = document.getElementById("rsi-table-body");
        const downloadBtn = document.getElementById("rsi-download-btn");

        let lastResults = null;
        let lastPrices = null;

        sampleBtn.addEventListener("click", () => {
          const sample = [
            100.0, 100.5, 101.2, 100.9, 101.5, 102.3, 101.8, 102.6, 103.1,
            102.7, 103.5, 104.2, 103.9, 104.8, 105.3, 104.7, 105.5, 106.1,
            105.6, 106.4,
          ];
          dataInput.value = sample.join("\n");
          errorEl.textContent = "";
        });

        clearBtn.addEventListener("click", () => {
          dataInput.value = "";
          errorEl.textContent = "";
          mainResult.classList.add("hidden");
          chartContainer.classList.add("hidden");
          tableContainer.classList.add("hidden");
          lastResults = null;
          lastPrices = null;
          if (rsiChart) {
            rsiChart.destroy();
            rsiChart = null;
          }
        });

        calcBtn.addEventListener("click", () => {
          errorEl.textContent = "";
          mainResult.classList.add("hidden");
          chartContainer.classList.add("hidden");
          tableContainer.classList.add("hidden");
          statusEl.className = "text-sm font-medium";

          const period = parseInt(periodInput.value, 10);
          if (isNaN(period) || period < 2 || period > 200) {
            errorEl.textContent =
              "Please enter a valid RSI period between 2 and 200.";
            return;
          }

          const raw = dataInput.value.trim();
          if (!raw) {
            errorEl.textContent = "Please paste price data first.";
            return;
          }

          const prices = parsePriceInput(sourceSelect.value, raw);
          if (prices.length < period + 1) {
            errorEl.textContent = `Not enough valid data. Need at least ${
              period + 1
            } rows for period ${period}.`;
            return;
          }

          try {
            const results = computeRSI(prices, period);
            lastResults = results;
            lastPrices = prices;

            const latest = results[results.length - 1];
            const latestRSI = latest.rsi;
            latestValueEl.textContent = latestRSI.toFixed(2);

            const cls = classifyRSI(latestRSI);
            statusEl.textContent = cls.label;
            statusEl.classList.add(cls.color);

            mainResult.classList.remove("hidden");

            renderRSITable(tableBody, results);
            tableContainer.classList.remove("hidden");

            renderRSIChart(
              chartCanvas.getContext("2d"),
              prices,
              results,
              period
            );
            chartContainer.classList.remove("hidden");
          } catch (e) {
            console.error(e);
            errorEl.textContent =
              e.message ||
              "Error while computing RSI. Please check your input.";
          }
        });

        downloadBtn.addEventListener("click", () => {
          if (!lastResults) return;
          downloadRSICSV(lastResults);
        });
      });
    </script>

