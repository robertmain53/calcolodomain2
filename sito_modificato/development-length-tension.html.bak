    <script>
      // Unit system
      const btnUS = document.getElementById("btn-us");
      const btnMetric = document.getElementById("btn-metric");
      let unitSystem = "us"; // 'us' or 'metric'

      const dbInput = document.getElementById("db");
      const fyInput = document.getElementById("fy");
      const fcInput = document.getElementById("fc");
      const coverInput = document.getElementById("clearCover");
      const dbUnit = document.getElementById("db-unit");
      const fyUnit = document.getElementById("fy-unit");
      const fcUnit = document.getElementById("fc-unit");
      const coverUnit = document.getElementById("cover-unit");

      function setUSUnits() {
        unitSystem = "us";
        dbUnit.textContent = "in";
        fyUnit.textContent = "psi";
        fcUnit.textContent = "psi";
        coverUnit.textContent = "in";
        // Typical defaults
        if (!dbInput.value) dbInput.value = 0.5;
        if (!fyInput.value) fyInput.value = 60000;
        if (!fcInput.value) fcInput.value = 4000;
        if (!coverInput.value) coverInput.value = 2.0;
      }

      function setMetricUnits() {
        unitSystem = "metric";
        dbUnit.textContent = "mm";
        fyUnit.textContent = "MPa";
        fcUnit.textContent = "MPa";
        coverUnit.textContent = "mm";
        // Convert existing US defaults if they look like US values
        const db = parseFloat(dbInput.value);
        const fy = parseFloat(fyInput.value);
        const fc = parseFloat(fcInput.value);
        const cover = parseFloat(coverInput.value);
        if (!isNaN(db) && db < 5) dbInput.value = (db * 25.4).toFixed(1);
        if (!isNaN(fy) && fy > 1000) fyInput.value = (fy / 6.895).toFixed(0);
        if (!isNaN(fc) && fc > 1000) fcInput.value = (fc / 6.895).toFixed(0);
        if (!isNaN(cover) && cover < 10)
          coverInput.value = (cover * 25.4).toFixed(1);
      }

      btnUS.addEventListener("click", () => {
        btnUS.classList.add(
          "bg-white",
          "shadow-sm",
          "border",
          "border-gray-300",
          "text-gray-900"
        );
        btnMetric.classList.remove(
          "bg-white",
          "shadow-sm",
          "border",
          "border-gray-300",
          "text-gray-900"
        );
        btnMetric.classList.add("text-gray-600");
        setUSUnits();
      });

      btnMetric.addEventListener("click", () => {
        btnMetric.classList.add(
          "bg-white",
          "shadow-sm",
          "border",
          "border-gray-300",
          "text-gray-900"
        );
        btnUS.classList.remove(
          "bg-white",
          "shadow-sm",
          "border",
          "border-gray-300",
          "text-gray-900"
        );
        btnUS.classList.add("text-gray-600");
        setMetricUnits();
      });

      // Bar size auto-fill
      const barSizeSelect = document.getElementById("barSize");
      barSizeSelect.addEventListener("change", () => {
        const opt = barSizeSelect.options[barSizeSelect.selectedIndex];
        const dbIn = parseFloat(opt.getAttribute("data-db-in"));
        if (!isNaN(dbIn)) {
          if (unitSystem === "us") {
            dbInput.value = dbIn.toFixed(3);
          } else {
            dbInput.value = (dbIn * 25.4).toFixed(1);
          }
        }
      });

      // Calculator
      const calcBtn = document.getElementById("calcBtn");
      const resetBtn = document.getElementById("resetBtn");
      const resultsDiv = document.getElementById("results");

      const barTypeSelect = document.getElementById("barType");
      const barFormSelect = document.getElementById("barForm");
      const lambdaSelect = document.getElementById("lambda");
      const phiSelect = document.getElementById("phi");
      const epoxySelect = document.getElementById("epoxy");
      const topBarSelect = document.getElementById("topBar");
      const confinementSelect = document.getElementById("confinement");

      const ldMainUS = document.getElementById("ld-main-us");
      const ldMainMetric = document.getElementById("ld-main-metric");
      const ldHookUS = document.getElementById("ld-hook-us");
      const ldHookMetric = document.getElementById("ld-hook-metric");

      const summaryBar = document.getElementById("summary-bar");
      const summaryFc = document.getElementById("summary-fc");
      const summaryFy = document.getElementById("summary-fy");
      const summaryLambda = document.getElementById("summary-lambda");
      const summaryPsiE = document.getElementById("summary-psi-e");
      const summaryPsiT = document.getElementById("summary-psi-t");
      const summaryPsiS = document.getElementById("summary-psi-s");
      const summaryPhi = document.getElementById("summary-phi");
      const summaryEq = document.getElementById("summary-eq");

      function computePsiE(epoxyMode, clearCoverVal) {
        // Simplified: good cover/spacing => 1.2, poor => 1.5
        if (epoxyMode === "none") return 1.0;
        if (epoxyMode === "epoxy-good") return 1.2;
        if (epoxyMode === "epoxy-poor") return 1.5;
        // If user only selected epoxy but not good/poor, infer from cover
        if (clearCoverVal >= (unitSystem === "us" ? 3.0 : 75.0)) return 1.2;
        return 1.5;
      }

      function computePsiT(isTop) {
        return isTop ? 1.3 : 1.0;
      }

      function computePsiS(confinementMode) {
        // Simplified: good confinement => 0.8, else 1.0 (subject to ACI limits)
        return confinementMode === "good" ? 0.8 : 1.0;
      }

      function computeLdStraight(db, fy, fc, lambda, psiE, psiT, psiS) {
        // Implement a unit-consistent simplified ACI-style equation.
        // US: ld (in) ≈ (ψt ψe ψs λ fy db) / (25 * sqrt(fc))
        // Metric: convert to US, compute, then convert back.
        let ldIn;
        if (unitSystem === "us") {
          const C = 25; // approximate coefficient for US units
          ldIn = (psiT * psiE * psiS * lambda * fy * db) / (C * Math.sqrt(fc));
          // Minimum limits: not less than 12 in and not less than some multiple of db
          const minLd = Math.max(12, 0.0); // 12 in basic minimum; db multiple handled below
          ldIn = Math.max(ldIn, minLd, 12 * db); // at least 12db as a rough lower bound
        } else {
          // Convert to US, compute, then convert back
          const dbIn = db / 25.4;
          const fyPsi = fy * 6.895; // MPa to psi
          const fcPsi = fc * 6.895;
          const C = 25;
          ldIn =
            (psiT * psiE * psiS * lambda * fyPsi * dbIn) /
            (C * Math.sqrt(fcPsi));
          ldIn = Math.max(ldIn, 12, 12 * dbIn);
        }
        return ldIn;
      }

      function computeLdHook(db, fy, fc, lambda, psiE) {
        // Very simplified hooked-bar expression:
        // US: ldh (in) ≈ (0.02 * λ * ψe * fy * db) / sqrt(fc), with basic limits
        let ldIn;
        if (unitSystem === "us") {
          const C = 0.02;
          ldIn = (C * lambda * psiE * fy * db) / Math.sqrt(fc);
          // Minimum hook length ~8db, not less than 6 in (rough)
          ldIn = Math.max(ldIn, 8 * db, 6);
        } else {
          const dbIn = db / 25.4;
          const fyPsi = fy * 6.895;
          const fcPsi = fc * 6.895;
          const C = 0.02;
          ldIn = (C * lambda * psiE * fyPsi * dbIn) / Math.sqrt(fcPsi);
          ldIn = Math.max(ldIn, 8 * dbIn, 6);
        }
        return ldIn;
      }

      calcBtn.addEventListener("click", () => {
        const dbVal = parseFloat(dbInput.value);
        const fyVal = parseFloat(fyInput.value);
        const fcVal = parseFloat(fcInput.value);
        const lambdaVal = parseFloat(lambdaSelect.value);
        const phiVal = parseFloat(phiSelect.value);
        const clearCoverVal = parseFloat(coverInput.value);

        if (
          isNaN(dbVal) ||
          isNaN(fyVal) ||
          isNaN(fcVal) ||
          dbVal <= 0 ||
          fyVal <= 0 ||
          fcVal <= 0
        ) {
          alert(
            "Please enter valid positive values for bar diameter, f_y, and f'_c."
          );
          return;
        }

        const psiE = computePsiE(epoxySelect.value, clearCoverVal);
        const psiT = computePsiT(topBarSelect.value === "yes");
        const psiS = computePsiS(confinementSelect.value);

        // Straight bar development length
        const ldIn = computeLdStraight(
          dbVal,
          fyVal,
          fcVal,
          lambdaVal,
          psiE,
          psiT,
          psiS
        );
        const ldMm = ldIn * 25.4;

        // Apply phi if user selected < 1.0 (design length)
        const ldInDesign = ldIn / phiVal;
        const ldMmDesign = ldMm / phiVal;

        ldMainUS.textContent = ldInDesign.toFixed(1);
        ldMainMetric.textContent = ldMmDesign.toFixed(0);

        // Hooked bar (if applicable)
        if (barFormSelect.value === "hooked") {
          const ldHookIn = computeLdHook(dbVal, fyVal, fcVal, lambdaVal, psiE);
          const ldHookMm = ldHookIn * 25.4;
          const ldHookInDesign = ldHookIn / phiVal;
          const ldHookMmDesign = ldHookMm / phiVal;
          ldHookUS.textContent = ldHookInDesign.toFixed(1);
          ldHookMetric.textContent = ldHookMmDesign.toFixed(0);
        } else {
          ldHookUS.textContent = "—";
          ldHookMetric.textContent = "—";
        }

        // Summary
        summaryBar.textContent = `${barSizeSelect.value} (${dbVal.toFixed(3)} ${
          unitSystem === "us" ? "in" : "mm"
        })`;
        summaryFc.textContent = `${fcVal.toFixed(0)} ${
          unitSystem === "us" ? "psi" : "MPa"
        }`;
        summaryFy.textContent = `${fyVal.toFixed(0)} ${
          unitSystem === "us" ? "psi" : "MPa"
        }`;
        summaryLambda.textContent = lambdaVal.toFixed(2);
        summaryPsiE.textContent = psiE.toFixed(2);
        summaryPsiT.textContent = psiT.toFixed(2);
        summaryPsiS.textContent = psiS.toFixed(2);
        summaryPhi.textContent = phiVal.toFixed(2);
        summaryEq.textContent =
          unitSystem === "us"
            ? "ld = (ψt · ψe · ψs · λ · fy · db) / (25 · √fc)  (approx. US units)"
            : "ld = (ψt · ψe · ψs · λ · fy · db) / (C · √fc)  (approx. metric, via US conversion)";

        resultsDiv.classList.remove("hidden");
      });

      resetBtn.addEventListener("click", () => {
        barTypeSelect.value = "deformed";
        barFormSelect.value = "straight";
        barSizeSelect.value = "#4";
        dbInput.value = unitSystem === "us" ? 0.5 : 12.7;
        fyInput.value = unitSystem === "us" ? 60000 : 415;
        fcInput.value = unitSystem === "us" ? 4000 : 28;
        lambdaSelect.value = "1.0";
        phiSelect.value = "1.0";
        epoxySelect.value = "none";
        topBarSelect.value = "no";
        confinementSelect.value = "standard";
        coverInput.value = unitSystem === "us" ? 2.0 : 50.0;
        resultsDiv.classList.add("hidden");
      });

      // Initialize defaults
      setUSUnits();
    </script>

    <script>
      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
