    <script>
      function parseNumber(id) {
        const el = document.getElementById(id);
        if (!el) return null;
        const v = parseFloat(el.value);
        return isNaN(v) ? null : v;
      }

      function formatNumber(value, decimals = 2) {
        if (value === null || value === undefined || isNaN(value)) return "–";
        return Number(value).toFixed(decimals);
      }

      function showWarning() {
        const w = document.getElementById("warning-text");
        if (w) w.classList.remove("hidden");
      }

      document.getElementById("calculate-btn").addEventListener("click", () => {
        const na = parseNumber("na");
        const cl = parseNumber("cl");
        const hco3 = parseNumber("hco3");
        const albumin = parseNumber("albumin");

        const normalAG = parseNumber("normal-ag") ?? 12;
        const normalHCO3 = parseNumber("normal-hco3") ?? 24;
        const normalAlb = parseNumber("normal-alb") ?? 4;

        const agEl = document.getElementById("ag-result");
        const agCorrEl = document.getElementById("ag-corr-result");
        const deltaAgEl = document.getElementById("delta-ag-result");
        const deltaHco3El = document.getElementById("delta-hco3-result");
        const deltaRatioEl = document.getElementById("delta-ratio-result");
        const interpMain = document.getElementById("interpretation-main");
        const interpList = document.getElementById("interpretation-list");

        interpList.innerHTML = "";

        if (na === null || cl === null || hco3 === null) {
          interpMain.textContent =
            "Please enter sodium, chloride, and bicarbonate values to calculate.";
          agEl.textContent =
            agCorrEl.textContent =
            deltaAgEl.textContent =
            deltaHco3El.textContent =
            deltaRatioEl.textContent =
              "–";
          return;
        }

        const ag = na - (cl + hco3);
        let agCorr = ag;
        let usedAlbuminCorrection = false;

        if (albumin !== null && normalAlb !== null) {
          const deltaAlb = normalAlb - albumin;
          if (deltaAlb > 0) {
            agCorr = ag + 2.5 * deltaAlb;
            usedAlbuminCorrection = true;
          }
        }

        const deltaAG = agCorr - normalAG;
        const deltaHCO3 = normalHCO3 - hco3;
        let deltaRatio = null;
        if (deltaHCO3 > 0.1) {
          deltaRatio = deltaAG / deltaHCO3;
        }

        agEl.textContent = formatNumber(ag, 1);
        agCorrEl.textContent = formatNumber(agCorr, 1);
        deltaAgEl.textContent = formatNumber(deltaAG, 1);
        deltaHco3El.textContent = formatNumber(deltaHCO3, 1);
        deltaRatioEl.textContent =
          deltaRatio === null ? "–" : formatNumber(deltaRatio, 2);

        const points = [];

        if (usedAlbuminCorrection) {
          points.push("Anion gap has been corrected for low albumin.");
        }

        if (agCorr > normalAG + 4) {
          points.push(
            "Corrected anion gap is elevated → high-anion-gap metabolic acidosis is likely."
          );
        } else if (agCorr < normalAG - 4) {
          points.push(
            "Corrected anion gap is lower than typical; consider hypoalbuminemia, lab variation, or paraproteinemia."
          );
        } else {
          points.push(
            "Corrected anion gap is within the usual reference range."
          );
        }

        if (deltaRatio !== null) {
          let pattern = "";
          if (deltaRatio < 0.4) {
            pattern =
              "Pattern suggests a pure normal-anion-gap (hyperchloremic) metabolic acidosis.";
          } else if (deltaRatio < 0.8) {
            pattern =
              "Pattern suggests mixed high- and normal-anion-gap metabolic acidosis.";
          } else if (deltaRatio <= 2.0) {
            pattern =
              "Pattern is compatible with an isolated high-anion-gap metabolic acidosis.";
          } else {
            pattern =
              "Pattern suggests concurrent metabolic alkalosis or chronic respiratory acidosis.";
          }
          points.push(pattern);
        } else {
          points.push(
            "Delta ratio could not be calculated reliably (ΔHCO₃⁻ is very small)."
          );
        }

        if (hco3 > normalHCO3 + 2) {
          points.push(
            "Bicarbonate is above normal; consider metabolic alkalosis or chronic respiratory acidosis."
          );
        } else if (hco3 < normalHCO3 - 2) {
          points.push(
            "Bicarbonate is below normal; metabolic acidosis is present."
          );
        }

        interpMain.textContent =
          "Preliminary interpretation (must be correlated with clinical context and blood gas):";
        points.forEach((p) => {
          const li = document.createElement("li");
          li.textContent = p;
          interpList.appendChild(li);
        });

        showWarning();
      });

      document.getElementById("reset-btn").addEventListener("click", () => {
        ["na", "cl", "hco3", "albumin"].forEach((id) => {
          const el = document.getElementById(id);
          if (el) el.value = "";
        });
        document.getElementById("normal-ag").value = 12;
        document.getElementById("normal-hco3").value = 24;
        document.getElementById("normal-alb").value = 4;

        [
          "ag-result",
          "ag-corr-result",
          "delta-ag-result",
          "delta-hco3-result",
          "delta-ratio-result",
        ].forEach((id) => {
          const el = document.getElementById(id);
          if (el) el.textContent = "–";
        });

        const interpMain = document.getElementById("interpretation-main");
        const interpList = document.getElementById("interpretation-list");
        interpMain.textContent =
          "Enter values and click “Calculate” to see interpretation.";
        interpList.innerHTML = "";
        const w = document.getElementById("warning-text");
        if (w) w.classList.add("hidden");
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
