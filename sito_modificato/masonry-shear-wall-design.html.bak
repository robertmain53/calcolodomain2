    <script>
      // Utility functions
      function round(value, decimals) {
        if (!isFinite(value)) return value;
        const factor = Math.pow(10, decimals);
        return Math.round(value * factor) / factor;
      }

      function createStatusBadge(ok) {
        return ok
          ? '<span class="badge bg-green-100 text-green-700 border border-green-200 ml-2">OK</span>'
          : '<span class="badge bg-red-100 text-red-700 border border-red-200 ml-2">NG</span>';
      }

      function runMasonryShearWallDesign() {
        const L = parseFloat(document.getElementById("wallLength").value); // m
        const h = parseFloat(document.getElementById("wallHeight").value); // m
        const t_mm = parseFloat(document.getElementById("wallThickness").value); // mm
        const fm = parseFloat(document.getElementById("fm").value); // MPa
        const fy = parseFloat(document.getElementById("fy").value); // MPa

        const db = parseFloat(document.getElementById("barDiameter").value); // mm
        const sv = parseFloat(document.getElementById("barSpacing").value); // mm
        const cover = parseFloat(document.getElementById("cover").value); // mm

        const Vu = parseFloat(document.getElementById("Vu").value); // kN
        const Mu = parseFloat(document.getElementById("Mu").value); // kN·m
        const Pu = parseFloat(document.getElementById("Pu").value); // kN

        const phiV = parseFloat(document.getElementById("phiV").value);
        const phiM = parseFloat(document.getElementById("phiM").value);
        const driftLimit = parseFloat(
          document.getElementById("driftLimit").value
        );

        const statusEl = document.getElementById("statusMessage");
        const resultsEl = document.getElementById("resultsContainer");

        if (
          [
            L,
            h,
            t_mm,
            fm,
            fy,
            db,
            sv,
            cover,
            Vu,
            Mu,
            phiV,
            phiM,
            driftLimit,
          ].some((v) => !isFinite(v) || v <= 0)
        ) {
          statusEl.textContent =
            "Please enter positive numeric values for all required fields.";
          statusEl.className = "text-sm text-red-600";
          return;
        }

        statusEl.textContent = "Design check completed.";
        statusEl.className = "text-sm text-green-700";

        // Geometry
        const t = t_mm / 1000; // m
        const d_mm = t_mm - cover;
        const d = d_mm / 1000; // m

        // Steel area per bar (mm²)
        const Abar = (Math.PI * Math.pow(db, 2)) / 4;

        // Number of bars along length
        const spacing_m = sv / 1000;
        const nBars = Math.floor(L / spacing_m) + 1;

        const As_mm2 = nBars * Abar;
        const wallArea_mm2 = t_mm * 1000 * L; // thickness * height(1000mm) * length(m)
        const rho_v = As_mm2 / wallArea_mm2; // vertical steel ratio

        // Shear capacity (simplified)
        let vn = 0.17 * Math.sqrt(fm) + (0.8 * rho_v * fy) / 1000; // MPa
        const vnMax = 0.8 * Math.sqrt(fm); // cap nominal shear stress
        if (vn > vnMax) vn = vnMax;

        const b = L; // m (wall length)
        const Vn = vn * b * d * 1000; // kN (MPa * m * m * 1000 = kN)
        const phiVn = phiV * Vn;
        const shearOK = Vu <= phiVn;

        // Flexural capacity (simplified rectangular section)
        const b_mm = L * 1000;
        const As = As_mm2; // mm²
        const a = (As * fy) / (0.85 * fm * b_mm); // mm
        const Mn_Nmm = As * fy * (d_mm - a / 2); // N·mm
        const Mn_kNm = Mn_Nmm / 1e6; // kN·m
        const phiMn = phiM * Mn_kNm;
        const flexureOK = Mu <= phiMn;

        // Drift check (approximate)
        const E = 1000 * fm * 1e6; // Pa (N/m²), fm in MPa
        const I = (t * Math.pow(L, 3)) / 12; // m^4
        const V_N = Vu * 1000; // kN -> N
        const delta = (V_N * Math.pow(h, 3)) / (3 * E * I); // m
        const driftRatio = delta / h;
        const driftOK = driftRatio <= driftLimit;

        // Build results HTML
        const html = `
      <div class="grid md:grid-cols-3 gap-4">
        <div class="border rounded p-3">
          <div class="text-xs uppercase text-gray-500 mb-1">Shear Check</div>
          <div class="text-lg font-semibold ${
            shearOK ? "text-green-700" : "text-red-700"
          }">
            ${shearOK ? "PASS" : "FAIL"}
          </div>
          <div class="text-sm text-gray-700 mt-1">
            V<sub>u</sub> = ${round(Vu, 1)} kN<br>
            φV<sub>n</sub> = ${round(phiVn, 1)} kN
          </div>
        </div>
        <div class="border rounded p-3">
          <div class="text-xs uppercase text-gray-500 mb-1">Flexure Check</div>
          <div class="text-lg font-semibold ${
            flexureOK ? "text-green-700" : "text-red-700"
          }">
            ${flexureOK ? "PASS" : "FAIL"}
          </div>
          <div class="text-sm text-gray-700 mt-1">
            M<sub>u</sub> = ${round(Mu, 1)} kN·m<br>
            φM<sub>n</sub> = ${round(phiMn, 1)} kN·m
          </div>
        </div>
        <div class="border rounded p-3">
          <div class="text-xs uppercase text-gray-500 mb-1">Drift Check</div>
          <div class="text-lg font-semibold ${
            driftOK ? "text-green-700" : "text-red-700"
          }">
            ${driftOK ? "PASS" : "FAIL"}
          </div>
          <div class="text-sm text-gray-700 mt-1">
            θ = ${round(driftRatio, 4)} (≈ h/${
          driftRatio > 0 ? Math.round(1 / driftRatio) : "–"
        })<br>
            θ<sub>allow</sub> = ${driftLimit}
          </div>
        </div>
      </div>

      <div class="mt-5 border rounded p-3 text-sm bg-gray-50">
        <h3 class="font-semibold mb-2 text-gray-800">Key Section Properties</h3>
        <div class="grid md:grid-cols-2 gap-3">
          <div>
            <div>Thickness t = ${round(t, 3)} m</div>
            <div>Effective depth d = ${round(d, 3)} m</div>
            <div>Length L = ${round(L, 3)} m</div>
            <div>Height h = ${round(h, 3)} m</div>
          </div>
          <div>
            <div>Bars along length = ${nBars}</div>
            <div>Total As = ${round(As_mm2, 1)} mm²</div>
            <div>Vertical steel ratio ρ<sub>v</sub> = ${round(
              rho_v * 100,
              3
            )} %</div>
            <div>Axial load P<sub>u</sub> = ${round(Pu, 1)} kN</div>
          </div>
        </div>
      </div>

      <div class="mt-4 border rounded p-3 text-xs text-gray-600 bg-yellow-50 border-yellow-200">
        <strong>Note:</strong> This is a simplified design aid. It does not check minimum/maximum reinforcement,
        bar spacing limits, lap splices, confinement, or detailed seismic requirements. Always verify with the full code.
      </div>
    `;

        resultsEl.innerHTML = html;
      }

      document
        .getElementById("runDesign")
        .addEventListener("click", runMasonryShearWallDesign);

      document.getElementById("resetForm").addEventListener("click", () => {
        document.getElementById("wallLength").value = 4.0;
        document.getElementById("wallHeight").value = 3.0;
        document.getElementById("wallThickness").value = 190;
        document.getElementById("fm").value = 10;
        document.getElementById("fy").value = 420;
        document.getElementById("barDiameter").value = 16;
        document.getElementById("barSpacing").value = 400;
        document.getElementById("cover").value = 50;
        document.getElementById("Vu").value = 150;
        document.getElementById("Mu").value = 300;
        document.getElementById("Pu").value = 400;
        document.getElementById("phiV").value = 0.75;
        document.getElementById("phiM").value = 0.9;
        document.getElementById("driftLimit").value = 0.004;
        document.getElementById("resultsContainer").innerHTML =
          '<p class="text-gray-500 text-sm">Enter parameters and click “Run Design Check” to see shear, flexure, and drift results.</p>';
        document.getElementById("statusMessage").textContent = "";
      });

      // Run once on load with defaults
      document.addEventListener("DOMContentLoaded", runMasonryShearWallDesign);
    </script>

