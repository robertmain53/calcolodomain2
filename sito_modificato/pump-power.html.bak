<script="">
        document.getElementById('calculate').addEventListener('click', function() {
            const flowRate = parseFloat(document.getElementById('flow-rate').value);
            const head = parseFloat(document.getElementById('head').value);
            const density = parseFloat(document.getElementById('density').value);
            const efficiency = parseFloat(document.getElementById('efficiency').value) / 100;
            const gravity = 9.81; // constant

            if (!isNaN(flowRate) && !isNaN(head) && !isNaN(density) && !isNaN(efficiency)) {
                const power = (flowRate * head * density * gravity) / efficiency;
                document.getElementById('pump-power').textContent = power.toFixed(2) + ' W';
            } else {
                alert('Please enter valid numbers in all fields.');
            }
        });
    </script>


