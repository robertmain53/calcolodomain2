    <script>
      (function () {
        const pressureUnitSelect = document.getElementById("pressure-unit");
        const componentsBody = document.getElementById("components-body");
        const componentsError = document.getElementById("components-error");
        const calculateBtn = document.getElementById("calculate-btn");
        const addComponentBtn = document.getElementById("add-component");
        const clearComponentsBtn = document.getElementById("clear-components");
        const exampleAirBtn = document.getElementById("example-air-btn");
        const resultsSection = document.getElementById("results-section");
        const resultsBody = document.getElementById("results-body");
        const resultTotalPressure = document.getElementById(
          "result-total-pressure"
        );
        const resultComponentCount = document.getElementById(
          "result-component-count"
        );
        const resultSumY = document.getElementById("result-sum-y");
        const calcMessage = document.getElementById("calc-message");

        const tabPressures = document.getElementById("tab-pressures");
        const tabMoles = document.getElementById("tab-moles");
        const thModePressure = document.getElementById("th-mode-pressure");
        const thModeMoles = document.getElementById("th-mode-moles");
        const hintPressures = document.getElementById("hint-pressures");
        const hintMoles = document.getElementById("hint-moles");
        const totalPressureWrapper = document.getElementById(
          "total-pressure-wrapper"
        );
        const totalPressureInput = document.getElementById(
          "total-pressure-input"
        );

        let mode = "pressures"; // 'pressures' or 'moles'
        let componentIdCounter = 0;

        const unitToAtm = {
          atm: 1,
          kpa: 1 / 101.325,
          mmhg: 1 / 760,
          psi: 1 / 14.6959,
        };

        const atmToUnit = {
          atm: 1,
          kpa: 101.325,
          mmhg: 760,
          psi: 14.6959,
        };

        function createComponentRow(name = "", value = "") {
          const id = componentIdCounter++;
          const tr = document.createElement("tr");
          tr.dataset.id = id;

          tr.innerHTML = `
      <td class="px-2 py-1 border-t border-gray-200">
        <input type="text" class="w-32 md:w-40 border border-gray-300 rounded-md px-2 py-1 text-sm" placeholder="e.g. N2" value="${name}">
      </td>
      <td class="px-2 py-1 border-t border-gray-200">
        <div class="flex items-center space-x-1">
          <input type="number" step="any" class="component-value border border-gray-300 rounded-md px-2 py-1 text-sm w-28" placeholder="${
            mode === "pressures" ? "Partial P" : "n or y"
          }" value="${value}">
          <span class="text-xs text-gray-500" data-unit-label>${
            mode === "pressures" ? pressureUnitSelect.value : "mol or y"
          }</span>
        </div>
      </td>
      <td class="px-2 py-1 border-t border-gray-200">
        <span class="component-y text-sm text-gray-800">–</span>
      </td>
      <td class="px-2 py-1 border-t border-gray-200 text-center">
        <button type="button" class="remove-component text-xs text-red-600 hover:underline">Remove</button>
      </td>
    `;

          componentsBody.appendChild(tr);
        }

        function updateHeadersAndPlaceholders() {
          const rows = componentsBody.querySelectorAll("tr");
          rows.forEach((row) => {
            const valueInput = row.querySelector(".component-value");
            const unitLabel = row.querySelector("[data-unit-label]");
            if (mode === "pressures") {
              valueInput.placeholder = "Partial P";
              unitLabel.textContent = pressureUnitSelect.value;
            } else {
              valueInput.placeholder = "n or y";
              unitLabel.textContent = "mol or y";
            }
          });

          if (mode === "pressures") {
            thModePressure.classList.remove("hidden");
            thModeMoles.classList.add("hidden");
            hintPressures.classList.remove("hidden");
            hintMoles.classList.add("hidden");
            totalPressureWrapper.classList.add("hidden");
          } else {
            thModePressure.classList.add("hidden");
            thModeMoles.classList.remove("hidden");
            hintPressures.classList.add("hidden");
            hintMoles.classList.remove("hidden");
            totalPressureWrapper.classList.remove("hidden");
          }
        }

        function clearAll() {
          componentsBody.innerHTML = "";
          resultsBody.innerHTML = "";
          resultsSection.classList.add("hidden");
          componentsError.classList.add("hidden");
          componentsError.textContent = "";
          calcMessage.textContent = "";
        }

        function loadDryAirExample() {
          clearAll();
          mode = "pressures";
          setActiveTab();
          updateHeadersAndPlaceholders();
          const totalP = 1; // atm
          const comp = [
            { name: "N2", y: 0.7808 },
            { name: "O2", y: 0.2095 },
            { name: "Ar", y: 0.0093 },
            { name: "CO2", y: 0.0004 },
          ];
          const factor = atmToUnit[pressureUnitSelect.value];
          comp.forEach((c) => {
            createComponentRow(c.name, (c.y * totalP * factor).toPrecision(4));
          });
          calcMessage.textContent = "Loaded dry air example at 1 atm.";
        }

        function setActiveTab() {
          if (mode === "pressures") {
            tabPressures.classList.add("border-blue-600", "text-blue-600");
            tabPressures.classList.remove(
              "border-transparent",
              "text-gray-600"
            );
            tabMoles.classList.remove("border-blue-600", "text-blue-600");
            tabMoles.classList.add("border-transparent", "text-gray-600");
          } else {
            tabMoles.classList.add("border-blue-600", "text-blue-600");
            tabMoles.classList.remove("border-transparent", "text-gray-600");
            tabPressures.classList.remove("border-blue-600", "text-blue-600");
            tabPressures.classList.add("border-transparent", "text-gray-600");
          }
        }

        function readComponents() {
          const rows = Array.from(componentsBody.querySelectorAll("tr"));
          const components = [];
          let hasError = false;

          rows.forEach((row) => {
            const nameInput = row.querySelector('input[type="text"]');
            const valueInput = row.querySelector(".component-value");
            nameInput.classList.remove("input-error");
            valueInput.classList.remove("input-error");

            const name =
              nameInput.value.trim() || "Gas " + (components.length + 1);
            const rawVal = valueInput.value.trim();
            if (!rawVal) {
              hasError = true;
              valueInput.classList.add("input-error");
              return;
            }
            const val = parseFloat(rawVal);
            if (!isFinite(val) || val <= 0) {
              hasError = true;
              valueInput.classList.add("input-error");
              return;
            }

            components.push({ row, name, value: val });
          });

          if (hasError || components.length === 0) {
            return null;
          }
          return components;
        }

        function calculate() {
          componentsError.classList.add("hidden");
          componentsError.textContent = "";
          calcMessage.textContent = "";

          const components = readComponents();
          if (!components) {
            componentsError.textContent =
              "Please enter a positive value for each gas component.";
            componentsError.classList.remove("hidden");
            resultsSection.classList.add("hidden");
            return;
          }

          let totalPressureAtm = null;
          let sumY = 0;
          let results = [];

          if (mode === "pressures") {
            const unit = pressureUnitSelect.value;
            const toAtm = unitToAtm[unit];
            let sumPAtm = 0;
            components.forEach((c) => {
              const pAtm = c.value * toAtm;
              sumPAtm += pAtm;
            });
            if (sumPAtm <= 0) {
              componentsError.textContent =
                "Total pressure must be greater than zero.";
              componentsError.classList.remove("hidden");
              resultsSection.classList.add("hidden");
              return;
            }
            totalPressureAtm = sumPAtm;
            components.forEach((c) => {
              const pAtm = c.value * toAtm;
              const y = pAtm / totalPressureAtm;
              sumY += y;
              results.push({
                name: c.name,
                pAtm,
                y,
              });
            });
          } else {
            // moles / mole fractions mode
            let sumN = 0;
            components.forEach((c) => {
              sumN += c.value;
            });
            if (sumN <= 0) {
              componentsError.textContent =
                "Total amount (sum of n or y) must be greater than zero.";
              componentsError.classList.remove("hidden");
              resultsSection.classList.add("hidden");
              return;
            }
            components.forEach((c) => {
              const y = c.value / sumN;
              sumY += y;
              results.push({
                name: c.name,
                pAtm: null,
                y,
              });
            });

            const totalPVal = parseFloat(totalPressureInput.value);
            if (isFinite(totalPVal) && totalPVal > 0) {
              const unit = pressureUnitSelect.value;
              const toAtm = unitToAtm[unit];
              totalPressureAtm = totalPVal * toAtm;
              results.forEach((r) => {
                r.pAtm = r.y * totalPressureAtm;
              });
            }
          }

          // Update UI
          const unit = pressureUnitSelect.value;
          const fromAtm = atmToUnit[unit];

          resultsBody.innerHTML = "";
          results.forEach((r) => {
            const tr = document.createElement("tr");
            const pDisplay = r.pAtm != null ? r.pAtm * fromAtm : null;
            tr.innerHTML = `
        <td class="px-2 py-1 border-t border-gray-200">${r.name}</td>
        <td class="px-2 py-1 border-t border-gray-200">
          ${
            pDisplay != null
              ? pDisplay.toPrecision(4) + " " + unit
              : '<span class="text-gray-400">N/A</span>'
          }
        </td>
        <td class="px-2 py-1 border-t border-gray-200">
          ${r.y.toPrecision(4)}
        </td>
      `;
            resultsBody.appendChild(tr);

            // also update mole fraction in input table
            const ySpan = r.row.querySelector(".component-y");
            if (ySpan) {
              ySpan.textContent = r.y.toPrecision(4);
            }
          });

          if (totalPressureAtm != null) {
            resultTotalPressure.textContent =
              (totalPressureAtm * fromAtm).toPrecision(5) + " " + unit;
          } else {
            resultTotalPressure.textContent =
              "N/A (total pressure not provided)";
          }
          resultComponentCount.textContent = results.length;
          resultSumY.textContent = sumY.toPrecision(5);

          resultsSection.classList.remove("hidden");

          if (
            mode === "moles" &&
            (!isFinite(parseFloat(totalPressureInput.value)) ||
              parseFloat(totalPressureInput.value) <= 0)
          ) {
            calcMessage.textContent =
              "Mole fractions computed. Enter total pressure to also get partial pressures.";
          } else {
            calcMessage.textContent = "";
          }
        }

        // Event listeners
        addComponentBtn.addEventListener("click", () => {
          createComponentRow();
        });

        clearComponentsBtn.addEventListener("click", () => {
          clearAll();
          createComponentRow();
        });

        exampleAirBtn.addEventListener("click", loadDryAirExample);

        calculateBtn.addEventListener("click", calculate);

        pressureUnitSelect.addEventListener("change", () => {
          updateHeadersAndPlaceholders();
        });

        componentsBody.addEventListener("click", (e) => {
          if (e.target.classList.contains("remove-component")) {
            const row = e.target.closest("tr");
            if (row) {
              row.remove();
            }
          }
        });

        tabPressures.addEventListener("click", () => {
          if (mode !== "pressures") {
            mode = "pressures";
            setActiveTab();
            updateHeadersAndPlaceholders();
            resultsSection.classList.add("hidden");
            calcMessage.textContent = "";
          }
        });

        tabMoles.addEventListener("click", () => {
          if (mode !== "moles") {
            mode = "moles";
            setActiveTab();
            updateHeadersAndPlaceholders();
            resultsSection.classList.add("hidden");
            calcMessage.textContent = "";
          }
        });

        // Initialize with two empty rows
        createComponentRow();
        createComponentRow();
        setActiveTab();
        updateHeadersAndPlaceholders();
      })();
    </script>
  
