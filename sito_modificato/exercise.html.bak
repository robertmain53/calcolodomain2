    <script>
      (function () {
        const weightInput = document.getElementById("weight");
        const weightUnit = document.getElementById("weight-unit");
        const durationInput = document.getElementById("duration");
        const durationUnit = document.getElementById("duration-unit");
        const activitySelect = document.getElementById("activity");
        const metSlider = document.getElementById("met");
        const metDisplay = document.getElementById("met-display");
        const metLabel = document.getElementById("met-label");

        const caloriesTotalEl = document.getElementById("calories-total");
        const caloriesPerMinEl = document.getElementById("calories-per-min");
        const caloriesPerHourEl = document.getElementById("calories-per-hour");
        const equivStepsEl = document.getElementById("equiv-steps");
        const equivDistanceEl = document.getElementById("equiv-distance");
        const weeklyCaloriesEl = document.getElementById("weekly-calories");
        const weeklyFatLossEl = document.getElementById("weekly-fat-loss");

        function toNumber(el, fallback) {
          const v = parseFloat(el.value);
          return isNaN(v) ? fallback : v;
        }

        function getWeightKg() {
          let w = toNumber(weightInput, 70);
          if (w <= 0) w = 70;
          return weightUnit.value === "lb" ? w * 0.45359237 : w;
        }

        function getDurationMinutes() {
          let d = toNumber(durationInput, 30);
          if (d <= 0) d = 30;
          return durationUnit.value === "hr" ? d * 60 : d;
        }

        function getMet() {
          let m = parseFloat(metSlider.value);
          if (isNaN(m) || m <= 0) m = 1;
          return m;
        }

        function syncMetWithActivity() {
          const selected = activitySelect.options[activitySelect.selectedIndex];
          const met = parseFloat(selected.getAttribute("data-met")) || 3.5;
          metSlider.value = met;
          metDisplay.textContent = met.toFixed(1);
          if (selected.value === "custom") {
            metLabel.textContent = "Set your own MET value";
          } else {
            metLabel.textContent =
              "Approximate intensity for selected activity";
          }
        }

        function formatNumber(n, decimals) {
          return n.toLocaleString(undefined, {
            maximumFractionDigits: decimals,
            minimumFractionDigits: decimals,
          });
        }

        function recalc() {
          const weightKg = getWeightKg();
          const durationMin = getDurationMinutes();
          const met = getMet();

          const calPerMin = (met * 3.5 * weightKg) / 200;
          const totalCal = calPerMin * durationMin;

          caloriesTotalEl.textContent = formatNumber(totalCal, 0);
          caloriesPerMinEl.textContent = formatNumber(calPerMin, 1);
          caloriesPerHourEl.textContent = formatNumber(calPerMin * 60, 0);

          // Walking/running equivalent
          const kcalPerStep = 0.05; // rough average
          const steps = totalCal / kcalPerStep;
          const stepsPerKm = 1300; // brisk walking
          const km = steps / stepsPerKm;

          equivStepsEl.textContent = formatNumber(steps, 0);
          equivDistanceEl.textContent = formatNumber(km, 2);

          // Weekly projection (3 sessions/week)
          const weeklyCal = totalCal * 3;
          weeklyCaloriesEl.textContent = formatNumber(weeklyCal, 0);
          const kgFat = weeklyCal / 7700; // 1 kg fat ≈ 7700 kcal
          weeklyFatLossEl.textContent = formatNumber(kgFat, 2);
        }

        // Event listeners
        ["input", "change"].forEach((evt) => {
          weightInput.addEventListener(evt, recalc);
          weightUnit.addEventListener(evt, recalc);
          durationInput.addEventListener(evt, recalc);
          durationUnit.addEventListener(evt, recalc);
          metSlider.addEventListener(evt, function () {
            metDisplay.textContent = parseFloat(metSlider.value).toFixed(1);
            recalc();
          });
          activitySelect.addEventListener(evt, function () {
            syncMetWithActivity();
            recalc();
          });
        });

        // Initial sync and calculation
        syncMetWithActivity();
        recalc();
      })();
    </script>

