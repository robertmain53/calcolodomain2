    <script>
      // Utility functions
      function fmtMoney(value) {
        if (!isFinite(value)) return "$0";
        const abs = Math.abs(value);
        const sign = value < 0 ? "-" : "";
        return (
          sign +
          abs.toLocaleString(undefined, {
            style: "currency",
            currency: "USD",
            maximumFractionDigits: 0,
          })
        );
      }

      function clamp(num, min, max) {
        return Math.min(Math.max(num, min), max);
      }

      // Preset average annual costs (approximate, in today's dollars)
      const COLLEGE_PRESETS = {
        "public-in": 25000,
        "public-out": 42000,
        private: 55000,
      };

      function updateYearsUntilCollege() {
        const age = parseFloat(document.getElementById("child-age").value) || 0;
        const startAge =
          parseFloat(document.getElementById("college-start-age").value) || 0;
        const years = Math.max(0, startAge - age);
        document.getElementById("years-until-college").value = years.toFixed(1);
        return years;
      }

      function applyCollegePreset() {
        const type = document.getElementById("college-type").value;
        if (type === "custom") return;
        const cost = COLLEGE_PRESETS[type];
        if (cost) {
          document.getElementById("current-annual-cost").value = cost;
        }
      }

      function updateCoverageLabel() {
        const slider = document.getElementById("coverage-percent");
        const label = document.getElementById("coverage-percent-value");
        label.textContent = slider.value + "%";
      }

      function calculate529() {
        const yearsUntil = updateYearsUntilCollege();
        const monthsUntil = Math.round(yearsUntil * 12);

        const currentAnnualCost = Math.max(
          0,
          parseFloat(document.getElementById("current-annual-cost").value) || 0
        );
        const yearsOfCollege = clamp(
          parseInt(document.getElementById("years-of-college").value) || 4,
          1,
          8
        );
        const collegeInflation =
          (parseFloat(document.getElementById("college-inflation").value) ||
            0) / 100;
        const grossReturn =
          (parseFloat(document.getElementById("investment-return").value) ||
            0) / 100;
        const annualFee =
          (parseFloat(document.getElementById("annual-fee").value) || 0) / 100;
        const currentBalance = Math.max(
          0,
          parseFloat(document.getElementById("current-balance").value) || 0
        );
        const monthlyContribution = Math.max(
          0,
          parseFloat(document.getElementById("monthly-contribution").value) || 0
        );
        const annualLump = Math.max(
          0,
          parseFloat(document.getElementById("annual-lump").value) || 0
        );
        const coveragePercent =
          clamp(
            parseFloat(document.getElementById("coverage-percent").value) ||
              100,
            0,
            100
          ) / 100;

        const netAnnualReturn = grossReturn - annualFee;
        const monthlyReturn = netAnnualReturn / 12;

        // 1) Project total college cost
        let totalCost = 0;
        for (let year = 0; year < yearsOfCollege; year++) {
          const yearsFromNow = yearsUntil + year;
          const costThisYear =
            currentAnnualCost * Math.pow(1 + collegeInflation, yearsFromNow);
          totalCost += costThisYear;
        }

        // 2) Project 529 balance at college start (simple monthly compounding with contributions)
        let balance = currentBalance;
        for (let m = 0; m < monthsUntil; m++) {
          // Add monthly contribution at start of month
          balance += monthlyContribution;
          // Add annual lump sum at the start of each year (every 12 months)
          if (annualLump > 0 && m % 12 === 0) {
            balance += annualLump;
          }
          // Grow with monthly return
          if (monthlyReturn !== 0) {
            balance *= 1 + monthlyReturn;
          }
        }

        const targetAmount = totalCost * coveragePercent;
        const shortfall = balance - targetAmount;

        // 3) Required monthly contribution to hit target (ignoring annual lump for this inversion)
        let requiredMonthly = 0;
        if (monthsUntil > 0 && monthlyReturn > 0) {
          const fvCurrent =
            currentBalance * Math.pow(1 + monthlyReturn, monthsUntil);
          const neededFromContrib = targetAmount - fvCurrent;
          if (neededFromContrib <= 0) {
            requiredMonthly = 0;
          } else {
            const factor =
              (Math.pow(1 + monthlyReturn, monthsUntil) - 1) / monthlyReturn;
            requiredMonthly = neededFromContrib / factor;
          }
        } else if (monthsUntil > 0 && monthlyReturn === 0) {
          // No growth: simple division
          const needed = targetAmount - currentBalance;
          requiredMonthly = needed > 0 ? needed / monthsUntil : 0;
        } else {
          requiredMonthly = 0;
        }

        // Update UI
        document.getElementById("result-total-cost").textContent =
          fmtMoney(totalCost);
        document.getElementById("result-target-amount").textContent =
          fmtMoney(targetAmount);
        document.getElementById("result-balance").textContent =
          fmtMoney(balance);

        const shortfallEl = document.getElementById("result-shortfall");
        shortfallEl.textContent = fmtMoney(shortfall);
        if (shortfall >= 0) {
          shortfallEl.classList.remove("text-red-700");
          shortfallEl.classList.add("text-green-700");
        } else {
          shortfallEl.classList.remove("text-green-700");
          shortfallEl.classList.add("text-red-700");
        }

        document.getElementById("result-required-monthly").textContent =
          fmtMoney(requiredMonthly);

        const summaryEl = document.getElementById("result-summary");
        let summary = "";
        if (monthsUntil <= 0) {
          summary =
            "Your child is already at or past the selected college start age. Adjust the ages to see a savings projection.";
        } else {
          summary = `With your current plan, your 529 balance is projected to reach ${fmtMoney(
            balance
          )} when college starts in approximately ${yearsUntil.toFixed(
            1
          )} years. `;
          if (shortfall >= 0) {
            summary += `That is about ${fmtMoney(
              Math.abs(shortfall)
            )} more than your target of ${fmtMoney(
              targetAmount
            )}, so you are on track or slightly ahead.`;
          } else {
            summary += `That is about ${fmtMoney(
              Math.abs(shortfall)
            )} less than your target of ${fmtMoney(
              targetAmount
            )}. To fully fund your goal, you would need to contribute about ${fmtMoney(
              requiredMonthly
            )} per month going forward (assuming the same return and fee assumptions).`;
          }
        }
        summaryEl.textContent = summary;
      }

      function resetCalculator() {
        document.getElementById("child-age").value = 3;
        document.getElementById("college-start-age").value = 18;
        document.getElementById("current-annual-cost").value = 25000;
        document.getElementById("college-type").value = "custom";
        document.getElementById("years-of-college").value = 4;
        document.getElementById("college-inflation").value = 5;
        document.getElementById("investment-return").value = 7;
        document.getElementById("annual-fee").value = 0.3;
        document.getElementById("current-balance").value = 5000;
        document.getElementById("monthly-contribution").value = 250;
        document.getElementById("annual-lump").value = 0;
        document.getElementById("coverage-percent").value = 100;
        updateCoverageLabel();
        updateYearsUntilCollege();
        calculate529();
      }

      document.addEventListener("DOMContentLoaded", function () {
        updateCoverageLabel();
        updateYearsUntilCollege();
        calculate529();

        document.getElementById("child-age").addEventListener("input", () => {
          updateYearsUntilCollege();
        });
        document
          .getElementById("college-start-age")
          .addEventListener("input", () => {
            updateYearsUntilCollege();
          });
        document
          .getElementById("college-type")
          .addEventListener("change", () => {
            applyCollegePreset();
          });
        document
          .getElementById("coverage-percent")
          .addEventListener("input", updateCoverageLabel);
        document
          .getElementById("calculate-btn")
          .addEventListener("click", calculate529);
        document
          .getElementById("reset-btn")
          .addEventListener("click", resetCalculator);
      });
    </script>

