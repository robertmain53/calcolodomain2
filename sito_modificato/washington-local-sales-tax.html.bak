    <script>
      // Calculator logic
      (function () {
        const stateRateInput = document.getElementById("state-rate");
        const localRateInput = document.getElementById("local-rate");
        const combinedRateInput = document.getElementById("combined-rate");
        const locationSelect = document.getElementById("wa-location");
        const zipInput = document.getElementById("wa-zip");

        const modeForwardBtn = document.getElementById("mode-forward");
        const modeReverseBtn = document.getElementById("mode-reverse");
        const forwardSection = document.getElementById("forward-section");
        const reverseSection = document.getElementById("reverse-section");

        const amountBeforeInput = document.getElementById("amount-before");
        const taxablePercentInput = document.getElementById("taxable-percent");
        const amountTotalInput = document.getElementById("amount-total");

        const calcBtn = document.getElementById("calc-btn");
        const resetBtn = document.getElementById("reset-btn");

        const resultsBox = document.getElementById("results");
        const resLocation = document.getElementById("res-location");
        const resRate = document.getElementById("res-rate");
        const resBefore = document.getElementById("res-before");
        const resTax = document.getElementById("res-tax");
        const resTotal = document.getElementById("res-total");
        const resTaxablePortion = document.getElementById(
          "res-taxable-portion"
        );

        let mode = "forward"; // 'forward' or 'reverse'

        function updateCombinedRate() {
          const state = parseFloat(stateRateInput.value) || 0;
          const local = parseFloat(localRateInput.value) || 0;
          const combined = state + local;
          combinedRateInput.value = combined ? combined.toFixed(2) : "";
        }

        function handleLocationChange() {
          const option = locationSelect.options[locationSelect.selectedIndex];
          const rateAttr = option.getAttribute("data-rate");
          if (rateAttr) {
            const combined = parseFloat(rateAttr);
            if (!isNaN(combined)) {
              const state = parseFloat(stateRateInput.value) || 0;
              const local = combined - state;
              localRateInput.value = local.toFixed(2);
              combinedRateInput.value = combined.toFixed(2);
            }
          } else if (locationSelect.value === "custom") {
            // Allow manual entry; don't override existing values
          } else {
            // Reset local/combined if no selection
            localRateInput.value = "";
            combinedRateInput.value = "";
          }
        }

        function setMode(newMode) {
          mode = newMode;
          if (mode === "forward") {
            modeForwardBtn.classList.add("bg-blue-600", "text-white");
            modeForwardBtn.classList.remove("bg-gray-100", "text-gray-700");
            modeReverseBtn.classList.add("bg-gray-100", "text-gray-700");
            modeReverseBtn.classList.remove("bg-blue-600", "text-white");
            forwardSection.classList.remove("hidden");
            reverseSection.classList.add("hidden");
          } else {
            modeReverseBtn.classList.add("bg-blue-600", "text-white");
            modeReverseBtn.classList.remove("bg-gray-100", "text-gray-700");
            modeForwardBtn.classList.add("bg-gray-100", "text-gray-700");
            modeForwardBtn.classList.remove("bg-blue-600", "text-white");
            forwardSection.classList.add("hidden");
            reverseSection.classList.remove("hidden");
          }
          resultsBox.classList.add("hidden");
        }

        function formatMoney(value) {
          if (isNaN(value)) return "–";
          return "$" + value.toFixed(2);
        }

        function calculate() {
          const combinedRatePercent = parseFloat(combinedRateInput.value);
          if (!combinedRatePercent || combinedRatePercent <= 0) {
            alert(
              "Please enter a valid combined tax rate or select a location."
            );
            return;
          }
          const r = combinedRatePercent / 100;

          let before = 0;
          let tax = 0;
          let total = 0;
          let taxablePortion = 100;

          if (mode === "forward") {
            const P = parseFloat(amountBeforeInput.value);
            if (isNaN(P) || P < 0) {
              alert("Please enter a valid price before tax.");
              return;
            }
            taxablePortion = Math.min(
              Math.max(parseFloat(taxablePercentInput.value) || 0, 0),
              100
            );
            const taxableAmount = P * (taxablePortion / 100);
            tax = taxableAmount * r;
            before = P;
            total = before + tax;
          } else {
            const T = parseFloat(amountTotalInput.value);
            if (isNaN(T) || T < 0) {
              alert("Please enter a valid total price including tax.");
              return;
            }
            before = T / (1 + r);
            tax = T - before;
            total = T;
          }

          const locLabel = (function () {
            const opt = locationSelect.options[locationSelect.selectedIndex];
            let name =
              opt && opt.value
                ? opt.text.replace(/\s*\(approx.*\)$/i, "")
                : "Custom / unspecified";
            const zip = zipInput.value.trim();
            if (zip) name += " (ZIP " + zip + ")";
            return name;
          })();

          resLocation.textContent = locLabel;
          resRate.textContent = combinedRatePercent.toFixed(2) + "%";
          resBefore.textContent = formatMoney(before);
          resTax.textContent = formatMoney(tax);
          resTotal.textContent = formatMoney(total);
          resTaxablePortion.textContent = taxablePortion.toFixed(0) + "%";

          resultsBox.classList.remove("hidden");
        }

        function resetAll() {
          locationSelect.value = "";
          zipInput.value = "";
          stateRateInput.value = "6.5";
          localRateInput.value = "";
          combinedRateInput.value = "";
          amountBeforeInput.value = "";
          taxablePercentInput.value = "100";
          amountTotalInput.value = "";
          resultsBox.classList.add("hidden");
          setMode("forward");
        }

        // Event listeners
        stateRateInput.addEventListener("input", updateCombinedRate);
        localRateInput.addEventListener("input", updateCombinedRate);
        locationSelect.addEventListener("change", handleLocationChange);

        modeForwardBtn.addEventListener("click", () => setMode("forward"));
        modeReverseBtn.addEventListener("click", () => setMode("reverse"));

        calcBtn.addEventListener("click", calculate);
        resetBtn.addEventListener("click", resetAll);

        // Initialize
        updateCombinedRate();
      })();

      // Simple FAQ toggle (in addition to global script_faq, safe if both run)
      document.addEventListener("DOMContentLoaded", function () {
        const faqButtons = document.querySelectorAll(
          "#faq-container .faq-question"
        );
        faqButtons.forEach((btn) => {
          btn.addEventListener("click", () => {
            const answer = btn.parentElement.querySelector(".faq-answer");
            const isHidden = answer.classList.contains("hidden");
            answer.classList.toggle("hidden");
            btn.querySelector("span:last-child").textContent = isHidden
              ? "−"
              : "+";
          });
        });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
