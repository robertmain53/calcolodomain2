    <script>
      // NEC Dwelling Load Calculator Logic
      (function () {
        const floorAreaEl = document.getElementById("nec-floor-area");
        const voltageEl = document.getElementById("nec-voltage");
        const serviceSizeEl = document.getElementById("nec-service-size");
        const smallApplianceEl = document.getElementById("nec-small-appliance");
        const laundryEl = document.getElementById("nec-laundry");
        const applianceListEl = document.getElementById("nec-appliance-list");
        const addApplianceBtn = document.getElementById("nec-add-appliance");
        const rangeKwEl = document.getElementById("nec-range-kw");
        const dryerKwEl = document.getElementById("nec-dryer-kw");
        const acKwEl = document.getElementById("nec-ac-kw");
        const heatKwEl = document.getElementById("nec-heat-kw");
        const hvacModeEl = document.getElementById("nec-hvac-mode");
        const calcBtn = document.getElementById("nec-calc-btn");
        const clearBtn = document.getElementById("nec-clear-btn");
        const resetDefaultsBtn = document.getElementById("nec-reset-defaults");
        const errorEl = document.getElementById("nec-error");

        const resultsEl = document.getElementById("nec-results");
        const totalKvaEl = document.getElementById("nec-total-kva");
        const totalAmpsEl = document.getElementById("nec-total-amps");
        const voltageLabelEl = document.getElementById("nec-voltage-label");
        const genKvaEl = document.getElementById("nec-gen-kva");
        const applKvaEl = document.getElementById("nec-appl-kva");
        const rangeKvaEl = document.getElementById("nec-range-kva");
        const dryerKvaEl = document.getElementById("nec-dryer-kva");
        const hvacKvaEl = document.getElementById("nec-hvac-kva");
        const serviceCheckEl = document.getElementById("nec-service-check");
        const serviceMessageEl = document.getElementById("nec-service-message");

        function addApplianceRow(name = "", va = "") {
          const row = document.createElement("div");
          row.className = "grid grid-cols-12 gap-2 items-center";
          row.innerHTML = `
        <div class="col-span-5">
          <input type="text" placeholder="Appliance (e.g. Dishwasher)" value="${name}" class="w-full border rounded px-2 py-1 text-sm nec-appl-name">
        </div>
        <div class="col-span-5">
          <input type="number" min="0" step="10" placeholder="VA or W" value="${va}" class="w-full border rounded px-2 py-1 text-sm nec-appl-va">
        </div>
        <div class="col-span-2 text-right">
          <button type="button" class="text-xs text-red-600 hover:underline nec-appl-remove">Remove</button>
        </div>
      `;
          applianceListEl.appendChild(row);
        }

        function initDefaultAppliances() {
          applianceListEl.innerHTML = "";
          addApplianceRow("Dishwasher", "1200");
          addApplianceRow("Disposal", "800");
          addApplianceRow("Water heater", "4500");
        }

        addApplianceBtn.addEventListener("click", () => addApplianceRow());

        applianceListEl.addEventListener("click", (e) => {
          if (e.target.classList.contains("nec-appl-remove")) {
            const row = e.target.closest(".grid");
            if (row) row.remove();
          }
        });

        resetDefaultsBtn.addEventListener("click", () => {
          floorAreaEl.value = 2000;
          smallApplianceEl.value = 2;
          laundryEl.value = 1;
          rangeKwEl.value = 12;
          dryerKwEl.value = 5;
          acKwEl.value = 4;
          heatKwEl.value = 8;
          hvacModeEl.value = "larger";
          initDefaultAppliances();
        });

        function parseNumber(el) {
          const v = parseFloat(el.value);
          return isNaN(v) ? 0 : v;
        }

        function calcGeneralKva() {
          const area = parseNumber(floorAreaEl);
          const nSmall = Math.max(
            2,
            parseInt(smallApplianceEl.value || "0", 10) || 0
          );
          const nLaundry = Math.max(
            0,
            parseInt(laundryEl.value || "0", 10) || 0
          );

          const baseVA = area * 3 + nSmall * 1500 + nLaundry * 1500;

          let demandedVA = 0;
          if (baseVA <= 3000) {
            demandedVA = baseVA;
          } else {
            demandedVA = 3000 + 0.35 * (baseVA - 3000);
          }
          return demandedVA / 1000; // kVA
        }

        function calcApplianceKva() {
          const rows = applianceListEl.querySelectorAll(".grid");
          let totalVA = 0;
          rows.forEach((row) => {
            const vaInput = row.querySelector(".nec-appl-va");
            const va = parseFloat(vaInput.value);
            if (!isNaN(va) && va > 0) totalVA += va;
          });
          const n = rows.length;
          if (n >= 4) {
            totalVA *= 0.75; // NEC 220.53 demand factor
          }
          return totalVA / 1000;
        }

        function calcRangeKva() {
          const kw = Math.max(0, parseNumber(rangeKwEl));
          if (kw === 0) return 0;

          // Approximate NEC 220.55 for a single range:
          // 0–3.5 kW: 100%; 3.5–12 kW: taper to ~80%; >12 kW: 80%
          let df = 1.0;
          if (kw > 3.5 && kw <= 12) {
            const t = (kw - 3.5) / (12 - 3.5); // 0 to 1
            df = 1.0 - 0.2 * t; // 1.0 down to 0.8
          } else if (kw > 12) {
            df = 0.8;
          }
          return kw * df; // kVA ≈ kW
        }

        function calcDryerKva() {
          const kw = Math.max(0, parseNumber(dryerKwEl));
          return kw; // 100% for single dryer
        }

        function calcHvacKva() {
          const ac = Math.max(0, parseNumber(acKwEl));
          const heat = Math.max(0, parseNumber(heatKwEl));
          const mode = hvacModeEl.value;
          switch (mode) {
            case "larger":
              return Math.max(ac, heat);
            case "both":
              return ac + heat;
            case "cooling":
              return ac;
            case "heating":
              return heat;
            default:
              return Math.max(ac, heat);
          }
        }

        function calcAmps(totalKva, system) {
          if (totalKva <= 0) return 0;
          if (system === "120/208") {
            const V = 208;
            return (totalKva * 1000) / (Math.sqrt(3) * V);
          } else {
            const V = 240;
            return (totalKva * 1000) / V;
          }
        }

        calcBtn.addEventListener("click", () => {
          errorEl.textContent = "";
          const area = parseNumber(floorAreaEl);
          if (area <= 0) {
            errorEl.textContent = "Enter a valid floor area greater than zero.";
            resultsEl.classList.add("hidden");
            return;
          }

          const genKva = calcGeneralKva();
          const applKva = calcApplianceKva();
          const rangeKva = calcRangeKva();
          const dryerKva = calcDryerKva();
          const hvacKva = calcHvacKva();

          const totalKva = genKva + applKva + rangeKva + dryerKva + hvacKva;
          const system = voltageEl.value;
          const amps = calcAmps(totalKva, system);

          totalKvaEl.textContent = totalKva.toFixed(2);
          totalAmpsEl.textContent = Math.ceil(amps).toString();
          genKvaEl.textContent = genKva.toFixed(2);
          applKvaEl.textContent = applKva.toFixed(2);
          rangeKvaEl.textContent = rangeKva.toFixed(2);
          dryerKvaEl.textContent = dryerKva.toFixed(2);
          hvacKvaEl.textContent = hvacKva.toFixed(2);
          voltageLabelEl.textContent =
            system === "120/208" ? "120/208 V 3φ" : "120/240 V 1φ";

          // Service size comparison
          const service = parseFloat(serviceSizeEl.value);
          if (!isNaN(service) && service > 0) {
            serviceCheckEl.classList.remove("hidden");
            if (amps <= service) {
              serviceMessageEl.innerHTML = `Calculated load of <strong>${Math.ceil(
                amps
              )} A</strong> is within the entered service rating of <strong>${service} A</strong>.`;
              serviceMessageEl.className = "text-sm text-green-700";
            } else {
              serviceMessageEl.innerHTML = `Calculated load of <strong>${Math.ceil(
                amps
              )} A</strong> exceeds the entered service rating of <strong>${service} A</strong>. Consider a larger service or re-check inputs.`;
              serviceMessageEl.className = "text-sm text-red-700";
            }
          } else {
            serviceCheckEl.classList.add("hidden");
          }

          resultsEl.classList.remove("hidden");
        });

        clearBtn.addEventListener("click", () => {
          floorAreaEl.value = "";
          voltageEl.value = "120/240";
          serviceSizeEl.value = "";
          smallApplianceEl.value = "2";
          laundryEl.value = "1";
          rangeKwEl.value = "";
          dryerKwEl.value = "";
          acKwEl.value = "";
          heatKwEl.value = "";
          hvacModeEl.value = "larger";
          applianceListEl.innerHTML = "";
          resultsEl.classList.add("hidden");
          errorEl.textContent = "";
          serviceCheckEl.classList.add("hidden");
        });

        // Initialize defaults on load
        initDefaultAppliances();
      })();
    </script>

