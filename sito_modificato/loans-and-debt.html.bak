    <script>
      // DTI Calculator
      function formatPercent(value) {
        return value.toFixed(1) + "%";
      }

      function classifyDTI(dti) {
        if (dti < 0) return "";
        if (dti < 36)
          return {
            label: "Healthy",
            color: "text-green-600",
            msg: "Your DTI is in a generally healthy range. Lenders typically view this favorably.",
          };
        if (dti < 43)
          return {
            label: "Borderline",
            color: "text-amber-600",
            msg: "Your DTI is on the high side. You may still qualify for loans, but improving this ratio will give you more flexibility.",
          };
        return {
          label: "High",
          color: "text-red-600",
          msg: "Your DTI is high. Consider focusing on reducing debt before taking on new obligations.",
        };
      }

      document
        .getElementById("calcDtiBtn")
        .addEventListener("click", function () {
          const income =
            parseFloat(document.getElementById("monthlyIncome").value) || 0;
          const debt =
            parseFloat(document.getElementById("monthlyDebt").value) || 0;
          const resultEl = document.getElementById("dtiResult");
          const valueEl = document.getElementById("dtiValue");
          const interpEl = document.getElementById("dtiInterpretation");

          if (income <= 0 || debt < 0) {
            resultEl.classList.remove("hidden");
            valueEl.textContent = "–";
            interpEl.textContent =
              "Please enter a positive monthly income and non-negative debt payments.";
            return;
          }

          const dti = (debt / income) * 100;
          const cls = classifyDTI(dti);
          resultEl.classList.remove("hidden");
          valueEl.textContent = formatPercent(dti);
          interpEl.innerHTML = `<span class="${cls.color} font-semibold">${cls.label}:</span> ${cls.msg}`;
        });

      // Debt Payoff Comparator
      function collectDebts() {
        const debts = [];
        for (let i = 1; i <= 3; i++) {
          const bal = parseFloat(
            document.getElementById("d" + i + "Balance").value
          );
          const rate = parseFloat(
            document.getElementById("d" + i + "Rate").value
          );
          const min = parseFloat(
            document.getElementById("d" + i + "Min").value
          );
          if (bal > 0 && rate >= 0 && min > 0) {
            debts.push({
              id: i,
              balance: bal,
              rate: rate / 100 / 12, // monthly rate
              min: min,
            });
          }
        }
        return debts;
      }

      function simulatePayoff(strategy, extra) {
        let debts = collectDebts();
        if (debts.length === 0) return null;

        let month = 0;
        let totalInterest = 0;
        const maxMonths = 50 * 12; // safety cap

        while (debts.length > 0 && month < maxMonths) {
          month++;

          // choose target index based on strategy
          if (strategy === "snowball") {
            debts.sort((a, b) => a.balance - b.balance);
          } else {
            debts.sort((a, b) => b.rate - a.rate);
          }

          const target = debts[0];
          const totalMin = debts.reduce((sum, d) => sum + d.min, 0);
          const extraForTarget = Math.max(0, extra);
          const paymentToTarget = target.min + extraForTarget;

          // apply interest and payments
          for (let d of debts) {
            const interest = d.balance * d.rate;
            totalInterest += interest;
            d.balance += interest;

            let payment = d === target ? paymentToTarget : d.min;
            if (payment > d.balance) payment = d.balance;
            d.balance -= payment;
          }

          // remove paid-off debts
          debts = debts.filter((d) => d.balance > 0.01);
        }

        if (month >= maxMonths) return null;
        return { months: month, totalInterest: totalInterest };
      }

      document
        .getElementById("calcPayoffBtn")
        .addEventListener("click", function () {
          const extra =
            parseFloat(document.getElementById("extraPayment").value) || 0;
          const strategy = document.querySelector(
            'input[name="strategy"]:checked'
          ).value;
          const debts = collectDebts();
          const resultEl = document.getElementById("payoffResult");
          const monthsEl = document.getElementById("monthsToPayoff");
          const interestEl = document.getElementById("totalInterest");
          const noteEl = document.getElementById("payoffNote");
          const strategyLabelEl = document.getElementById("strategyLabel");

          if (debts.length === 0) {
            resultEl.classList.remove("hidden");
            monthsEl.textContent = "–";
            interestEl.textContent = "–";
            noteEl.textContent =
              "Please enter at least one debt with balance, APR, and minimum payment.";
            return;
          }

          const sim = simulatePayoff(strategy, extra);
          if (!sim) {
            resultEl.classList.remove("hidden");
            monthsEl.textContent = "–";
            interestEl.textContent = "–";
            noteEl.textContent =
              "The payoff period is very long or inputs are unrealistic. Try adjusting balances, rates, or extra payment.";
            return;
          }

          strategyLabelEl.textContent =
            strategy === "snowball" ? "Snowball" : "Avalanche";
          resultEl.classList.remove("hidden");
          const years = (sim.months / 12).toFixed(1);
          monthsEl.textContent = `${sim.months} months (~${years} years)`;
          interestEl.textContent = "$" + sim.totalInterest.toFixed(2);
          noteEl.textContent =
            "This is an estimate assuming fixed interest rates and payments. Actual results will vary with changes in rates, fees, and your payment behavior.";
        });

      // New Loan Affordability
      function calcLoanPayment(P, annualRate, years) {
        const n = years * 12;
        const r = annualRate / 100 / 12;
        if (n <= 0) return 0;
        if (r === 0) return P / n;
        return (P * r) / (1 - Math.pow(1 + r, -n));
      }

      document
        .getElementById("calcNewLoanBtn")
        .addEventListener("click", function () {
          const amount =
            parseFloat(document.getElementById("newLoanAmount").value) || 0;
          const rate =
            parseFloat(document.getElementById("newLoanRate").value) || 0;
          const years =
            parseFloat(document.getElementById("newLoanYears").value) || 0;
          const income =
            parseFloat(document.getElementById("monthlyIncome").value) || 0;
          const existingDebt =
            parseFloat(document.getElementById("monthlyDebt").value) || 0;

          const resultEl = document.getElementById("newLoanResult");
          const payEl = document.getElementById("newLoanPayment");
          const dtiEl = document.getElementById("newDtiValue");
          const interpEl = document.getElementById("newDtiInterpretation");

          if (amount <= 0 || years <= 0 || income <= 0) {
            resultEl.classList.remove("hidden");
            payEl.textContent = "–";
            dtiEl.textContent = "–";
            interpEl.textContent =
              "Please enter a positive loan amount, term, and monthly income. For best results, also enter your existing monthly debt.";
            return;
          }

          const payment = calcLoanPayment(amount, rate, years);
          const newDebtTotal = existingDebt + payment;
          const dti = (newDebtTotal / income) * 100;
          const cls = classifyDTI(dti);

          resultEl.classList.remove("hidden");
          payEl.textContent = "$" + payment.toFixed(2);
          dtiEl.textContent = formatPercent(dti);
          interpEl.innerHTML = `<span class="${cls.color} font-semibold">${cls.label}:</span> ${cls.msg} This includes your existing debts plus the new loan payment.`;
        });

      // Simple FAQ toggle (in addition to global script_faq if present)
      document
        .querySelectorAll("#faq-container .faq-question")
        .forEach((btn) => {
          btn.addEventListener("click", () => {
            const answer = btn.nextElementSibling;
            const icon = btn.querySelector("span:last-child");
            const isHidden = answer.classList.contains("hidden");
            answer.classList.toggle("hidden");
            icon.textContent = isHidden ? "−" : "+";
          });
        });

      window.MathJax = {
        tex: {
