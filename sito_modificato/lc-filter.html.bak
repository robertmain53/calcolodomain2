    <script>
      document.addEventListener('DOMContentLoaded', () => {
        const tabs = {
          lp: document.getElementById('tab-lp'),
          hp: document.getElementById('tab-hp'),
          bp: document.getElementById('tab-bp'),
          bs: document.getElementById('tab-bs')
        };
        let currentType = 'lp';

        const modeSelect = document.getElementById('mode-select');
        const sectionLC = document.getElementById('section-lc');
        const sectionR = document.getElementById('section-R');
        const sectionFZ = document.getElementById('section-fz');
        const sectionBW = document.getElementById('section-bw');

        const LInput = document.getElementById('L');
        const LUnit = document.getElementById('L-unit');
        const CInput = document.getElementById('C');
        const CUnit = document.getElementById('C-unit');
        const RInput = document.getElementById('R');
        const Z0Input = document.getElementById('z0');
        const fTargetInput = document.getElementById('f-target');
        const fUnit = document.getElementById('f-unit');
        const bwInput = document.getElementById('bw');
        const bwUnit = document.getElementById('bw-unit');

        const btnCalc = document.getElementById('btn-calc');
        const btnReset = document.getElementById('btn-reset');
        const errorMsg = document.getElementById('error-msg');

        const resF0 = document.getElementById('res-f0');
        const resW0 = document.getElementById('res-w0');
        const resQ = document.getElementById('res-Q');
        const resBW = document.getElementById('res-BW');
        const resL = document.getElementById('res-L');
        const resC = document.getElementById('res-C');

        let chart;

        function setTab(type) {
          currentType = type;
          Object.entries(tabs).forEach(([key, el]) => {
            if (key === type) {
              el.classList.remove('tab-inactive');
              el.classList.add('tab-active');
            } else {
              el.classList.remove('tab-active');
              el.classList.add('tab-inactive');
            }
          });
          // Band-pass / band-stop need R and BW
          if (type === 'bp' || type === 'bs') {
            sectionR.classList.remove('hidden');
            if (modeSelect.value === 'fromFBW') {
              sectionBW.classList.remove('hidden');
            }
          } else {
            sectionR.classList.add('hidden');
            sectionBW.classList.add('hidden');
          }
          calculate();
        }

        Object.entries(tabs).forEach(([key, el]) => {
          el.addEventListener('click', () => setTab(key));
        });

        modeSelect.addEventListener('change', () => {
          const mode = modeSelect.value;
          if (mode === 'fromLC') {
            sectionLC.classList.remove('hidden');
            sectionFZ.classList.add('hidden');
            sectionBW.classList.add('hidden');
          } else if (mode === 'fromFZ') {
            sectionLC.classList.add('hidden');
            sectionFZ.classList.remove('hidden');
            sectionBW.classList.add('hidden');
          } else if (mode === 'fromFBW') {
            sectionLC.classList.add('hidden');
            sectionFZ.classList.remove('hidden');
            if (currentType === 'bp' || currentType === 'bs') {
              sectionBW.classList.remove('hidden');
            }
          }
          calculate();
        });

        function unitToBaseL(value, unit) {
          const v = parseFloat(value);
          if (isNaN(v)) return NaN;
          switch (unit) {
            case 'H': return v;
            case 'mH': return v * 1e-3;
            case 'uH': return v * 1e-6;
            case 'nH': return v * 1e-9;
            default: return v;
          }
        }

        function unitToBaseC(value, unit) {
          const v = parseFloat(value);
          if (isNaN(v)) return NaN;
          switch (unit) {
            case 'F': return v;
            case 'mF': return v * 1e-3;
            case 'uF': return v * 1e-6;
            case 'nF': return v * 1e-9;
            case 'pF': return v * 1e-12;
            default: return v;
          }
        }

        function unitToBaseF(value, unit) {
          const v = parseFloat(value);
          if (isNaN(v)) return NaN;
          switch (unit) {
            case 'Hz': return v;
            case 'kHz': return v * 1e3;
            case 'MHz': return v * 1e6;
            default: return v;
          }
        }

        function formatSI(value, unit) {
          if (!isFinite(value)) return '–';
          const abs = Math.abs(value);
          let scaled = value;
          let prefix = '';
          if (abs === 0) {
            scaled = 0;
          } else if (abs >= 1e9) {
            scaled = value / 1e9; prefix = 'G';
          } else if (abs >= 1e6) {
            scaled = value / 1e6; prefix = 'M';
          } else if (abs >= 1e3) {
            scaled = value / 1e3; prefix = 'k';
          } else if (abs < 1e-9) {
            scaled = value / 1e-12; prefix = 'p';
          } else if (abs < 1e-6) {
            scaled = value / 1e-9; prefix = 'n';
          } else if (abs < 1e-3) {
            scaled = value / 1e-6; prefix = 'µ';
          } else if (abs < 1) {
            scaled = value / 1e-3; prefix = 'm';
          }
          return scaled.toFixed(3).replace(/\.?0+$/,'') + ' ' + prefix + unit;
        }

        function showError(msg) {
          errorMsg.textContent = msg || '';
        }

        function calculate() {
          showError('');
          let L, C, R = parseFloat(RInput.value) || 0;
          const Z0 = parseFloat(Z0Input.value);
          const mode = modeSelect.value;

          if (!isFinite(Z0) || Z0 <= 0) {
            showError('Please enter a valid positive impedance Z₀.');
            return;
          }

          if (mode === 'fromLC') {
            L = unitToBaseL(LInput.value, LUnit.value);
            C = unitToBaseC(CInput.value, CUnit.value);
            if (!isFinite(L) || L <= 0 || !isFinite(C) || C <= 0) {
              showError('Enter positive L and C values.');
              return;
            }
          } else if (mode === 'fromFZ') {
            const f = unitToBaseF(fTargetInput.value, fUnit.value);
            if (!isFinite(f) || f <= 0) {
              showError('Enter a valid frequency.');
              return;
            }
            const w0 = 2 * Math.PI * f;
            if (currentType === 'lp' || currentType === 'hp') {
              // Simple first-order LC section with X_L = X_C = Z0 at fc
              L = Z0 / w0;
              C = 1 / (w0 * Z0);
            } else {
              // For band-pass/stop, assume simple RLC with chosen R and compute L,C from f0 and Z0
              // Use L = Z0 / w0, C = 1 / (w0 * Z0) as a starting point
              L = Z0 / w0;
              C = 1 / (w0 * Z0);
            }
          } else if (mode === 'fromFBW') {
            const f0 = unitToBaseF(fTargetInput.value, fUnit.value);
            const BW = unitToBaseF(bwInput.value, bwUnit.value);
            if (!isFinite(f0) || f0 <= 0 || !isFinite(BW) || BW <= 0) {
              showError('Enter valid center frequency and bandwidth.');
              return;
            }
            const w0 = 2 * Math.PI * f0;
            const Q = f0 / BW;
            if (currentType === 'bp') {
              // Series RLC: Q = w0 L / R  => L = Q R / w0, C from f0
              if (!isFinite(R) || R <= 0) {
                showError('Enter a positive resistance R for band‑pass design.');
                return;
              }
              L = Q * R / w0;
              C = 1 / (w0 * w0 * L);
            } else if (currentType === 'bs') {
              // Parallel RLC: Q = R / (w0 L) => L = R / (Q w0), C from f0
              if (!isFinite(R) || R <= 0) {
                showError('Enter a positive resistance R for band‑stop design.');
                return;
              }
              L = R / (Q * w0);
              C = 1 / (w0 * w0 * L);
            } else {
              showError('Bandwidth mode is only for band‑pass / band‑stop.');
              return;
            }
          }

          const w0 = 1 / Math.sqrt(L * C);
          const f0 = w0 / (2 * Math.PI);

          let Q = NaN;
          let BW = NaN;
          if (currentType === 'bp') {
            if (R > 0) {
              Q = w0 * L / R;
              BW = f0 / Q;
            }
          } else if (currentType === 'bs') {
            if (R > 0) {
              Q = R / (w0 * L);
              BW = f0 / Q;
            }
          }

          resF0.textContent = formatSI(f0, 'Hz');
          resW0.textContent = formatSI(w0, 'rad/s');
          resQ.textContent = isFinite(Q) ? Q.toFixed(3) : '–';
          resBW.textContent = isFinite(BW) ? formatSI(BW, 'Hz') : '–';
          resL.textContent = formatSI(L, 'H');
          resC.textContent = formatSI(C, 'F');

          drawBode(L, C, R, Z0, currentType);
        }

        function drawBode(L, C, R, Z0, type) {
          const ctx = document.getElementById('bode-canvas').getContext('2d');
          const f0 = 1 / (2 * Math.PI * Math.sqrt(L * C));
          if (!isFinite(f0) || f0 <= 0) return;

          const fStart = f0 / 100;
          const fEnd = f0 * 100;
          const points = 200;
          const freqs = [];
          const mags = [];

          for (let i = 0; i < points; i++) {
            const t = i / (points - 1);
            const f = fStart * Math.pow(fEnd / fStart, t);
            freqs.push(f);
            const w = 2 * Math.PI * f;

            let H = 0;

            if (type === 'lp') {
              // Simple L-series, C-shunt low-pass between source and load Z0
              const ZL = 1j * w * L;
              const ZC = -1j / (w * C);
              // Use simplified transfer: |H| = 1 / sqrt(1 + (f/fc)^2)
              const fc = f0;
              const mag = 1 / Math.sqrt(1 + Math.pow(f / fc, 2));
              H = mag;
            } else if (type === 'hp') {
              const fc = f0;
              const mag = (f / fc) / Math.sqrt(1 + Math.pow(f / fc, 2));
              H = mag;
            } else if (type === 'bp') {
              const w0 = 1 / Math.sqrt(L * C);
              const num = (w / w0);
              const den = Math.sqrt( Math.pow(1 - Math.pow(w / w0, 2), 2) + Math.pow(w / (w0 * (R > 0 ? (w0 * L / R) : 1)), 2) );
              H = den === 0 ? 0 : num / den;
            } else if (type === 'bs') {
              const w0 = 1 / Math.sqrt(L * C);
              const num = Math.sqrt( Math.pow(1 - Math.pow(w / w0, 2), 2) + Math.pow(w / (w0 * (R > 0 ? (R / (w0 * L)) : 1)), 2) );
              const den = Math.sqrt( Math.pow(1 - Math.pow(w / w0, 2), 2) + Math.pow(w / (w0 * (R > 0 ? (R / (w0 * L)) : 1)), 2) + Math.pow(w / w0, 2) );
              H = den === 0 ? 0 : num / den;
            }

            const magdB = 20 * Math.log10(Math.max(H, 1e-6));
            mags.push(magdB);
          }

          const labels = freqs.map(f => f);

          if (chart) {
            chart.destroy();
          }

          chart = new Chart(ctx, {
            type: 'line',
            data: {
              labels,
              datasets: [{
                label: 'Magnitude (dB)',
                data: mags,
                borderColor: '#2563eb',
                borderWidth: 2,
                pointRadius: 0,
                tension: 0.1
              }]
            },
            options: {
              responsive: true,
              maintainAspectRatio: false,
              scales: {
                x: {
                  type: 'logarithmic',
                  title: { display: true, text: 'Frequency (Hz)' },
                  ticks: {
                    callback: (value, index, ticks) => {
                      const v = labels[index];
                      if (!v) return '';
                      const log = Math.log10(v);
                      if (Math.abs(log - Math.round(log)) < 1e-6) {
                        return '10^' + Math.round(log);
                      }
                      return '';
                    },
                    maxTicksLimit: 10
                  },
                  grid: { color: '#e5e7eb' }
                },
                y: {
                  title: { display: true, text: 'Magnitude (dB)' },
                  grid: { color: '#e5e7eb' }
                }
              },
              plugins: {
                legend: { display: false },
                tooltip: {
                  callbacks: {
                    title: (items) => {
                      const idx = items[0].dataIndex;
                      const f = labels[idx];
                      return formatSI(f, 'Hz');
                    },
                    label: (item) => item.parsed.y.toFixed(1) + ' dB'
                  }
                }
              }
            }
          });
        }

        btnCalc.addEventListener('click', calculate);

        btnReset.addEventListener('click', () => {
          LInput.value = 10;
          LUnit.value = 'mH';
          CInput.value = 100;
          CUnit.value = 'uF';
          RInput.value = 10;
          Z0Input.value = 50;
          fTargetInput.value = 1000;
          fUnit.value = 'kHz';
          bwInput.value = 200;
          bwUnit.value = 'kHz';
          modeSelect.value = 'fromLC';
          sectionLC.classList.remove('hidden');
          sectionFZ.classList.add('hidden');
          sectionBW.classList.add('hidden');
          setTab('lp');
          calculate();
        });

        // Initial calculation
        calculate();
      });
    </script>
  
