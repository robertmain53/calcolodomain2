    <script>
      // --- Helper data: simplified performance bands (illustrative, not official) ---
      // Structure: bands[gender][ageBracket] = { pushups: [...], plank: [...], run: [...] }
      // Each array element: { min, max, category }
      const ageBrackets = [
        { min: 17, max: 19, label: "17-19" },
        { min: 20, max: 24, label: "20-24" },
        { min: 25, max: 29, label: "25-29" },
        { min: 30, max: 34, label: "30-34" },
        { min: 35, max: 39, label: "35-39" },
        { min: 40, max: 44, label: "40-44" },
        { min: 45, max: 49, label: "45-49" },
        { min: 50, max: 54, label: "50-54" },
        { min: 55, max: 59, label: "55-59" },
        { min: 60, max: 80, label: "60+" },
      ];

      const categoryScore = {
        Outstanding: 5,
        Excellent: 4,
        Good: 3,
        Satisfactory: 2,
        Probationary: 1,
        Fail: 0,
      };

      const scoreToCategory = (avg) => {
        if (avg >= 4.5) return "Outstanding";
        if (avg >= 3.5) return "Excellent";
        if (avg >= 2.5) return "Good";
        if (avg >= 1.5) return "Satisfactory";
        if (avg > 0) return "Probationary";
        return "Fail";
      };

      // Simplified, approximate bands – tuned separately for male/female and age bracket.
      // For brevity, we reuse the same pattern for most brackets but adjust slightly.
      const baseBandsMale = {
        pushups: [
          { min: 80, category: "Outstanding" },
          { min: 70, category: "Excellent" },
          { min: 55, category: "Good" },
          { min: 45, category: "Satisfactory" },
          { min: 35, category: "Probationary" },
          { min: 0, category: "Fail" },
        ],
        plank: [
          { min: 240, category: "Outstanding" }, // 4:00+
          { min: 210, category: "Excellent" }, // 3:30–3:59
          { min: 180, category: "Good" }, // 3:00–3:29
          { min: 150, category: "Satisfactory" }, // 2:30–2:59
          { min: 120, category: "Probationary" }, // 2:00–2:29
          { min: 0, category: "Fail" },
        ],
        run: [
          { max: 540, category: "Outstanding" }, // <= 9:00
          { max: 600, category: "Excellent" }, // 9:01–10:00
          { max: 690, category: "Good" }, // 10:01–11:30
          { max: 810, category: "Satisfactory" }, // 11:31–13:30
          { max: 870, category: "Probationary" }, // 13:31–14:30
          { max: Infinity, category: "Fail" },
        ],
      };

      const baseBandsFemale = {
        pushups: [
          { min: 60, category: "Outstanding" },
          { min: 50, category: "Excellent" },
          { min: 40, category: "Good" },
          { min: 30, category: "Satisfactory" },
          { min: 20, category: "Probationary" },
          { min: 0, category: "Fail" },
        ],
        plank: [
          { min: 210, category: "Outstanding" }, // 3:30+
          { min: 180, category: "Excellent" }, // 3:00–3:29
          { min: 150, category: "Good" }, // 2:30–2:59
          { min: 120, category: "Satisfactory" }, // 2:00–2:29
          { min: 90, category: "Probationary" }, // 1:30–1:59
          { min: 0, category: "Fail" },
        ],
        run: [
          { max: 600, category: "Outstanding" }, // <= 10:00
          { max: 660, category: "Excellent" }, // 10:01–11:00
          { max: 750, category: "Good" }, // 11:01–12:30
          { max: 870, category: "Satisfactory" }, // 12:31–14:30
          { max: 930, category: "Probationary" }, // 14:31–15:30
          { max: Infinity, category: "Fail" },
        ],
      };

      // Age adjustment factors (simple offsets to make older brackets slightly easier)
      function adjustBandsForAge(base, ageBracketLabel) {
        const factorMap = {
          "17-19": 0,
          "20-24": 0,
          "25-29": -2,
          "30-34": -4,
          "35-39": -6,
          "40-44": -8,
          "45-49": -10,
          "50-54": -12,
          "55-59": -14,
          "60+": -16,
        };
        const runBonusMap = {
          "17-19": 0,
          "20-24": 0,
          "25-29": 10,
          "30-34": 20,
          "35-39": 30,
          "40-44": 40,
          "45-49": 50,
          "50-54": 60,
          "55-59": 70,
          "60+": 80,
        };
        const pushAdj = factorMap[ageBracketLabel] || 0;
        const runAdj = runBonusMap[ageBracketLabel] || 0;

        const clone = JSON.parse(JSON.stringify(base));

        clone.pushups.forEach((b) => {
          b.min = Math.max(0, b.min + pushAdj);
        });
        clone.plank.forEach((b) => {
          b.min = Math.max(0, b.min - pushAdj * 3);
        }); // older → slightly easier time
        clone.run.forEach((b) => {
          if (b.max !== Infinity) b.max += runAdj;
        });

        return clone;
      }

      function getAgeBracket(age) {
        for (const b of ageBrackets) {
          if (age >= b.min && age <= b.max) return b.label;
        }
        return "60+";
      }

      function classifyPushups(reps, bands) {
        for (const band of bands.pushups) {
          if (reps >= band.min) return band.category;
        }
        return "Fail";
      }

      function classifyPlank(seconds, bands) {
        for (const band of bands.plank) {
          if (seconds >= band.min) return band.category;
        }
        return "Fail";
      }

      function classifyRun(seconds, bands) {
        for (const band of bands.run) {
          if (seconds <= band.max) return band.category;
        }
        return "Fail";
      }

      function categoryBadgeClass(cat) {
        switch (cat) {
          case "Outstanding":
            return "badge bg-purple-100 text-purple-800";
          case "Excellent":
            return "badge bg-green-100 text-green-800";
          case "Good":
            return "badge bg-blue-100 text-blue-800";
          case "Satisfactory":
            return "badge bg-yellow-100 text-yellow-800";
          case "Probationary":
            return "badge bg-orange-100 text-orange-800";
          case "Fail":
            return "badge bg-red-100 text-red-800";
          default:
            return "badge bg-gray-100 text-gray-800";
        }
      }

      function formatTime(sec) {
        if (!isFinite(sec) || sec <= 0) return "-";
        const m = Math.floor(sec / 60);
        const s = Math.round(sec % 60);
        return `${m}:${s.toString().padStart(2, "0")}`;
      }

      document.getElementById("calculateBtn").addEventListener("click", () => {
        const gender = document.getElementById("gender").value;
        const age = parseInt(document.getElementById("age").value, 10);
        const pushups = parseInt(document.getElementById("pushups").value, 10);
        const plankMin =
          parseInt(document.getElementById("plankMin").value, 10) || 0;
        const plankSec =
          parseInt(document.getElementById("plankSec").value, 10) || 0;
        const runMin =
          parseInt(document.getElementById("runMin").value, 10) || 0;
        const runSec =
          parseInt(document.getElementById("runSec").value, 10) || 0;

        const resultsEl = document.getElementById("resultsContainer");

        if (isNaN(age) || age < 17 || age > 80) {
          resultsEl.innerHTML =
            '<p class="text-red-600 text-sm">Please enter a valid age between 17 and 80.</p>';
          return;
        }
        if (isNaN(pushups) || pushups < 0) {
          resultsEl.innerHTML =
            '<p class="text-red-600 text-sm">Please enter a valid number of push-ups.</p>';
          return;
        }

        const plankTotal = plankMin * 60 + plankSec;
        const runTotal = runMin * 60 + runSec;

        if (plankTotal <= 0 || runTotal <= 0) {
          resultsEl.innerHTML =
            '<p class="text-red-600 text-sm">Please enter non-zero plank and run times.</p>';
          return;
        }

        const ageBracket = getAgeBracket(age);
        const base = gender === "male" ? baseBandsMale : baseBandsFemale;
        const bands = adjustBandsForAge(base, ageBracket);

        const pushCat = classifyPushups(pushups, bands);
        const plankCat = classifyPlank(plankTotal, bands);
        const runCat = classifyRun(runTotal, bands);

        const pushScore = categoryScore[pushCat];
        const plankScore = categoryScore[plankCat];
        const runScore = categoryScore[runCat];

        const anyFail = [pushCat, plankCat, runCat].includes("Fail");
        const avgScore = (pushScore + plankScore + runScore) / 3;
        const overallCat = anyFail ? "Fail" : scoreToCategory(avgScore);
        const passStatus =
          !anyFail && overallCat !== "Fail" && overallCat !== "Probationary"
            ? "Pass"
            : anyFail
            ? "Fail"
            : "Borderline / Probationary";

        const passClass =
          passStatus === "Pass"
            ? "text-green-700 bg-green-50 border-green-200"
            : passStatus === "Fail"
            ? "text-red-700 bg-red-50 border-red-200"
            : "text-yellow-700 bg-yellow-50 border-yellow-200";

        resultsEl.innerHTML = `
      <div class="space-y-3">
        <div class="flex flex-wrap items-center justify-between gap-2">
          <div>
            <p class="text-sm text-gray-600">Estimated overall category</p>
            <p class="text-xl font-semibold">${overallCat}</p>
          </div>
          <div class="px-3 py-1 border rounded-md text-sm font-semibold ${passClass}">
            Overall status: ${passStatus}
          </div>
        </div>

        <div class="grid md:grid-cols-3 gap-3 mt-2">
          <div class="border border-gray-200 rounded-md p-3 bg-white">
            <p class="text-xs uppercase tracking-wide text-gray-500 mb-1">Push-ups</p>
            <p class="text-sm text-gray-700 mb-1">Reps: <span class="font-semibold">${pushups}</span></p>
            <span class="${categoryBadgeClass(pushCat)}">${pushCat}</span>
          </div>
          <div class="border border-gray-200 rounded-md p-3 bg-white">
            <p class="text-xs uppercase tracking-wide text-gray-500 mb-1">Plank</p>
            <p class="text-sm text-gray-700 mb-1">Time: <span class="font-semibold">${formatTime(
              plankTotal
            )}</span></p>
            <span class="${categoryBadgeClass(plankCat)}">${plankCat}</span>
          </div>
          <div class="border border-gray-200 rounded-md p-3 bg-white">
            <p class="text-xs uppercase tracking-wide text-gray-500 mb-1">1.5-mile run</p>
            <p class="text-sm text-gray-700 mb-1">Time: <span class="font-semibold">${formatTime(
              runTotal
            )}</span></p>
            <span class="${categoryBadgeClass(runCat)}">${runCat}</span>
          </div>
        </div>

        <p class="text-xs text-gray-500 mt-2">
          This is an unofficial estimate based on simplified bands. For official Navy PRT scores and administrative actions, always use the latest Navy instructions and tables.
        </p>
      </div>
    `;
      });

      document.getElementById("resetBtn").addEventListener("click", () => {
        document.getElementById("gender").value = "male";
        document.getElementById("age").value = 25;
        document.getElementById("pushups").value = 50;
        document.getElementById("plankMin").value = 2;
        document.getElementById("plankSec").value = 0;
        document.getElementById("runMin").value = 12;
        document.getElementById("runSec").value = 0;
        document.getElementById("resultsContainer").innerHTML =
          '<p class="text-gray-500 text-sm">Enter your details and click “Calculate PRT Category” to see your estimated performance.</p>';
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
