    <script>
      // Core calculation logic
      function roundSmart(value) {
        if (!isFinite(value)) return value;
        if (Math.abs(value) >= 1000) return value.toFixed(1);
        if (Math.abs(value) >= 100) return value.toFixed(2);
        if (Math.abs(value) >= 10) return value.toFixed(2);
        if (Math.abs(value) >= 1) return value.toFixed(3);
        return value.toFixed(4);
      }

      function torqueToBaseNm(value, unit) {
        const v = Number(value);
        switch (unit) {
          case "Nm":
            return v;
          case "ftlb":
            return v * 1.355817948;
          case "inlb":
            return v * 0.1129848290276167; // 1 in·lb = 0.112984829 N·m
          case "kgfm":
            return v * 9.80665;
          default:
            return NaN;
        }
      }

      function baseNmToTorque(valueNm, unit) {
        const v = Number(valueNm);
        switch (unit) {
          case "Nm":
            return v;
          case "ftlb":
            return v / 1.355817948;
          case "inlb":
            return v / 0.1129848290276167;
          case "kgfm":
            return v / 9.80665;
          default:
            return NaN;
        }
      }

      function hpToKw(hp) {
        return hp * 0.745699872;
      }

      function kwToHp(kw) {
        return kw / 0.745699872;
      }

      document.addEventListener("DOMContentLoaded", function () {
        const modeTorqueToHpBtn = document.getElementById("mode-torque-to-hp");
        const modeHpToTorqueBtn = document.getElementById("mode-hp-to-torque");
        const inputsTorqueToHp = document.getElementById("inputs-torque-to-hp");
        const inputsHpToTorque = document.getElementById("inputs-hp-to-torque");
        const calcBtn = document.getElementById("calc-btn");
        const resetBtn = document.getElementById("reset-btn");
        const errorEl = document.getElementById("calc-error");
        const resultMain = document.getElementById("result-main");
        const resultSecondary = document.getElementById("result-secondary");
        const resultNote = document.getElementById("result-note");

        let currentMode = "torqueToHp";

        function setMode(mode) {
          currentMode = mode;
          if (mode === "torqueToHp") {
            inputsTorqueToHp.classList.remove("hidden");
            inputsHpToTorque.classList.add("hidden");
            modeTorqueToHpBtn.classList.add("toggle-btn-active");
            modeTorqueToHpBtn.classList.remove("toggle-btn-inactive");
            modeHpToTorqueBtn.classList.add("toggle-btn-inactive");
            modeHpToTorqueBtn.classList.remove("toggle-btn-active");
            resultMain.textContent = "Enter torque and RPM to see power.";
          } else {
            inputsTorqueToHp.classList.add("hidden");
            inputsHpToTorque.classList.remove("hidden");
            modeHpToTorqueBtn.classList.add("toggle-btn-active");
            modeHpToTorqueBtn.classList.remove("toggle-btn-inactive");
            modeTorqueToHpBtn.classList.add("toggle-btn-inactive");
            modeTorqueToHpBtn.classList.remove("toggle-btn-active");
            resultMain.textContent = "Enter power and RPM to see torque.";
          }
          errorEl.classList.add("hidden");
          resultSecondary.classList.add("hidden");
        }

        modeTorqueToHpBtn.addEventListener("click", () =>
          setMode("torqueToHp")
        );
        modeHpToTorqueBtn.addEventListener("click", () =>
          setMode("hpToTorque")
        );

        function calculate() {
          errorEl.classList.add("hidden");
          resultSecondary.classList.add("hidden");

          if (currentMode === "torqueToHp") {
            const torqueVal = parseFloat(
              document.getElementById("torque-value").value
            );
            const torqueUnit = document.getElementById("torque-unit").value;
            const rpmVal = parseFloat(
              document.getElementById("rpm-value").value
            );
            const powerUnit = document.getElementById("power-unit").value;

            if (isNaN(torqueVal) || torqueVal <= 0) {
              errorEl.textContent = "Please enter a positive torque value.";
              errorEl.classList.remove("hidden");
              return;
            }
            if (isNaN(rpmVal) || rpmVal <= 0) {
              errorEl.textContent = "Please enter a positive RPM value.";
              errorEl.classList.remove("hidden");
              return;
            }

            // Convert torque to ft·lb and N·m
            const torqueNm = torqueToBaseNm(torqueVal, torqueUnit);
            const torqueFtlb = baseNmToTorque(torqueNm, "ftlb");

            // Power in HP using imperial formula
            const hp = (torqueFtlb * rpmVal) / 5252;
            const kW = hpToKw(hp);

            if (powerUnit === "hp") {
              resultMain.textContent = `${roundSmart(hp)} HP`;
              resultSecondary.innerHTML = `<p>≈ ${roundSmart(
                kW
              )} kW</p><p>Torque: ${roundSmart(torqueFtlb)} ft·lb (${roundSmart(
                torqueNm
              )} N·m)</p>`;
            } else {
              resultMain.textContent = `${roundSmart(kW)} kW`;
              resultSecondary.innerHTML = `<p>≈ ${roundSmart(
                hp
              )} HP</p><p>Torque: ${roundSmart(torqueFtlb)} ft·lb (${roundSmart(
                torqueNm
              )} N·m)</p>`;
            }
            resultSecondary.classList.remove("hidden");
            resultNote.textContent =
              "Calculated using HP = Torque(ft·lb) × RPM ÷ 5252 and exact unit conversions.";
          } else {
            const powerVal = parseFloat(
              document.getElementById("power-value").value
            );
            const powerInputUnit =
              document.getElementById("power-input-unit").value;
            const rpmVal2 = parseFloat(
              document.getElementById("rpm-value-2").value
            );
            const torqueOutUnit =
              document.getElementById("torque-output-unit").value;

            if (isNaN(powerVal) || powerVal <= 0) {
              errorEl.textContent = "Please enter a positive power value.";
              errorEl.classList.remove("hidden");
              return;
            }
            if (isNaN(rpmVal2) || rpmVal2 <= 0) {
              errorEl.textContent = "Please enter a positive RPM value.";
              errorEl.classList.remove("hidden");
              return;
            }

            // Convert input power to HP
            const hpIn = powerInputUnit === "hp" ? powerVal : kwToHp(powerVal);
            const kWIn = hpToKw(hpIn);

            // Torque in ft·lb from HP and RPM
            const torqueFtlb = (hpIn * 5252) / rpmVal2;
            const torqueNm = torqueToBaseNm(torqueFtlb, "ftlb");
            const torqueOut = baseNmToTorque(torqueNm, torqueOutUnit);

            let unitLabel;
            switch (torqueOutUnit) {
              case "ftlb":
                unitLabel = "ft·lb";
                break;
              case "Nm":
                unitLabel = "N·m";
                break;
              case "inlb":
                unitLabel = "in·lb";
                break;
              case "kgfm":
                unitLabel = "kgf·m";
                break;
              default:
                unitLabel = "";
            }

            resultMain.textContent = `${roundSmart(torqueOut)} ${unitLabel}`;
            resultSecondary.innerHTML = `<p>Torque: ${roundSmart(
              torqueFtlb
            )} ft·lb (${roundSmart(torqueNm)} N·m)</p><p>Power: ${roundSmart(
              hpIn
            )} HP (≈ ${roundSmart(kWIn)} kW)</p>`;
            resultSecondary.classList.remove("hidden");
            resultNote.textContent =
              "Calculated using Torque(ft·lb) = HP × 5252 ÷ RPM and exact unit conversions.";
          }
        }

        calcBtn.addEventListener("click", calculate);

        resetBtn.addEventListener("click", function () {
          document.getElementById("torque-value").value = "";
          document.getElementById("rpm-value").value = "";
          document.getElementById("power-value").value = "";
          document.getElementById("rpm-value-2").value = "";
          errorEl.classList.add("hidden");
          resultSecondary.classList.add("hidden");
          if (currentMode === "torqueToHp") {
            resultMain.textContent = "Enter torque and RPM to see power.";
          } else {
            resultMain.textContent = "Enter power and RPM to see torque.";
          }
        });

        // Quick presets
        document.querySelectorAll("[data-preset]").forEach((btn) => {
          btn.addEventListener("click", () => {
            const [torque, rpm] = btn.getAttribute("data-preset").split("-");
            setMode("torqueToHp");
            document.getElementById("torque-unit").value =
              btn.textContent.includes("N·m") ? "Nm" : "ftlb";
            document.getElementById("torque-value").value = torque;
            document.getElementById("rpm-value").value = rpm;
            calculate();
          });
        });

        // Simple FAQ toggle (in addition to global script_faq)
        document
          .querySelectorAll("#faq-container .faq-question")
          .forEach((btn) => {
            btn.addEventListener("click", () => {
              const answer = btn.parentElement.querySelector(".faq-answer");
              const symbol = btn.querySelector("span:last-child");
              const isHidden = answer.classList.contains("hidden");
              answer.classList.toggle("hidden");
              symbol.textContent = isHidden ? "−" : "+";
            });
          });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
