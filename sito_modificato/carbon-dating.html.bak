    <script>
      // Tab switching
      const tabs = document.querySelectorAll(".mode-tab");
      const panels = {
        fraction: document.getElementById("panel-fraction"),
        activity: document.getElementById("panel-activity"),
        age: document.getElementById("panel-age"),
      };

      tabs.forEach((tab) => {
        tab.addEventListener("click", () => {
          const mode = tab.getAttribute("data-mode");
          tabs.forEach((t) => {
            t.classList.remove("border-blue-500", "text-blue-600");
            t.classList.add("border-transparent", "text-gray-500");
          });
          tab.classList.add("border-blue-500", "text-blue-600");
          tab.classList.remove("border-transparent", "text-gray-500");

          Object.keys(panels).forEach((key) => {
            panels[key].classList.toggle("hidden", key !== mode);
          });
        });
      });

      // Half-life custom toggles
      function setupHalfLifeSelect(selectId, customId) {
        const sel = document.getElementById(selectId);
        const custom = document.getElementById(customId);
        sel.addEventListener("change", () => {
          if (sel.value === "custom") {
            custom.classList.remove("hidden");
          } else {
            custom.classList.add("hidden");
          }
        });
      }
      setupHalfLifeSelect("halfLife", "halfLifeCustom");
      setupHalfLifeSelect("halfLife2", "halfLife2Custom");
      setupHalfLifeSelect("halfLife3", "halfLife3Custom");

      // Utility: get half-life from select/custom
      function getHalfLife(selectId, customId) {
        const sel = document.getElementById(selectId);
        const custom = document.getElementById(customId);
        if (sel.value === "custom") {
          const v = parseFloat(custom.value);
          if (!isFinite(v) || v <= 0)
            throw new Error("Please enter a valid positive custom half-life.");
          return v;
        }
        return parseFloat(sel.value);
      }

      function formatNumber(x, digits = 2) {
        if (!isFinite(x)) return "–";
        if (Math.abs(x) >= 1e5 || (Math.abs(x) < 1e-2 && x !== 0)) {
          return x.toExponential(digits);
        }
        return x.toFixed(digits);
      }

      const computeBtn = document.getElementById("computeBtn");
      const resetBtn = document.getElementById("resetBtn");
      const errorBox = document.getElementById("errorBox");
      const errorText = document.getElementById("errorText");
      const results = document.getElementById("results");
      const ageResult = document.getElementById("ageResult");
      const fractionResult = document.getElementById("fractionResult");
      const calendarBox = document.getElementById("calendarBox");
      const calendarResult = document.getElementById("calendarResult");
      const calendarApprox = document.getElementById("calendarApprox");
      const metaResult = document.getElementById("metaResult");

      function showError(msg) {
        errorText.textContent = msg;
        errorBox.classList.remove("hidden");
        results.classList.add("hidden");
      }

      function clearError() {
        errorBox.classList.add("hidden");
        errorText.textContent = "";
      }

      computeBtn.addEventListener("click", () => {
        clearError();
        try {
          // Determine active mode
          let mode = "fraction";
          if (!panels.fraction.classList.contains("hidden")) mode = "fraction";
          if (!panels.activity.classList.contains("hidden")) mode = "activity";
          if (!panels.age.classList.contains("hidden")) mode = "age";

          let Fm = null;
          let sigmaF = null;
          let age = null;
          let sigmaAge = null;
          let hl = null;
          let tau = null;

          if (mode === "fraction") {
            const type = document.getElementById("fractionType").value;
            const val = parseFloat(
              document.getElementById("fractionValue").value
            );
            if (!isFinite(val) || val <= 0) {
              throw new Error("Please enter a positive measured value.");
            }
            hl = getHalfLife("halfLife", "halfLifeCustom");
            tau = hl / Math.log(2);

            if (type === "fraction") {
              Fm = val;
            } else {
              Fm = val / 100.0;
            }
            if (Fm <= 0 || Fm > 2) {
              throw new Error(
                "Fraction modern must be between 0 and about 2 (0–200 pMC)."
              );
            }
            age = -tau * Math.log(Fm);

            const uncVal = parseFloat(
              document.getElementById("fractionUnc").value
            );
            if (isFinite(uncVal) && uncVal > 0) {
              sigmaF = type === "fraction" ? uncVal : uncVal / 100.0;
              sigmaAge = Math.abs((tau * sigmaF) / Fm);
            }
          } else if (mode === "activity") {
            const As = parseFloat(
              document.getElementById("activitySample").value
            );
            const A0 = parseFloat(
              document.getElementById("activityModern").value
            );
            if (!isFinite(As) || As <= 0 || !isFinite(A0) || A0 <= 0) {
              throw new Error(
                "Please enter positive values for sample and modern activities."
              );
            }
            hl = getHalfLife("halfLife2", "halfLife2Custom");
            tau = hl / Math.log(2);
            Fm = As / A0;
            if (Fm <= 0 || Fm > 2) {
              throw new Error(
                "Computed fraction modern is outside a reasonable range (0–2). Check your inputs."
              );
            }
            age = -tau * Math.log(Fm);

            const relUnc = parseFloat(
              document.getElementById("activityUnc").value
            );
            if (isFinite(relUnc) && relUnc > 0) {
              sigmaF = Fm * (relUnc / 100.0);
              sigmaAge = Math.abs((tau * sigmaF) / Fm);
            }
          } else if (mode === "age") {
            const t = parseFloat(document.getElementById("ageInput").value);
            if (!isFinite(t)) {
              throw new Error("Please enter a valid age in years BP.");
            }
            hl = getHalfLife("halfLife3", "halfLife3Custom");
            tau = hl / Math.log(2);
            age = t;
            Fm = Math.exp(-age / tau);
          }

          // Populate results
          const pMC = Fm * 100.0;
          let ageStr = "";
          if (age < 0) {
            ageStr =
              formatNumber(age, 1) +
              " years (negative age – sample appears “post‑modern”)";
          } else {
            ageStr = formatNumber(age, 0) + " years BP";
          }
          if (sigmaAge && sigmaAge > 0) {
            ageStr +=
              " \u00b1 " + formatNumber(sigmaAge, 0) + " years (1\u03c3)";
          }
          ageResult.textContent = ageStr;

          fractionResult.innerHTML =
            "F\u2098 = " +
            formatNumber(Fm, 4) +
            " &nbsp; | &nbsp; pMC = " +
            formatNumber(pMC, 2) +
            " %";

          // Simple calendar-age approximation (very rough)
          if (calendarApprox.checked) {
            calendarBox.classList.remove("hidden");
            // crude factor: radiocarbon years to calendar years ~ 1.03 for mid-Holocene, but varies strongly.
            const factor = 1.03;
            const calAge = age * factor;
            calendarResult.textContent =
              formatNumber(calAge, 0) +
              " calendar years BP (very rough estimate)";
          } else {
            calendarBox.classList.add("hidden");
          }

          metaResult.textContent =
            "Assumed half-life: " +
            formatNumber(hl, 0) +
            " years; mean life τ ≈ " +
            formatNumber(tau, 0) +
            " years. For accurate calendar ages, use official calibration curves (IntCal, Marine, SHCal).";

          results.classList.remove("hidden");
        } catch (e) {
          showError(
            e.message ||
              "An unexpected error occurred while computing the radiocarbon age."
          );
        }
      });

      resetBtn.addEventListener("click", () => {
        document.getElementById("carbon-form").reset();
        clearError();
        results.classList.add("hidden");
        calendarBox.classList.add("hidden");
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
