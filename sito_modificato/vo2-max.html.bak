    <script>
      // VO2 max calculator logic
      function getSelectedTab() {
        const active = document.querySelector(".vo2-tab.border-blue-600");
        return active ? active.getAttribute("data-tab") : "race";
      }

      function timeToMinutes(h, m, s) {
        return (h || 0) * 60 + (m || 0) + (s || 0) / 60;
      }

      function paceToSpeedMetersPerMin(min, sec, unit) {
        const totalMin = (min || 0) + (sec || 0) / 60;
        if (!totalMin || totalMin <= 0) return null;
        let distMeters = unit === "mile" ? 1609.34 : 1000;
        return distMeters / totalMin;
      }

      function danielsVO2FromSpeed(v) {
        // v in m/min
        return -4.6 + 0.182258 * v + 0.000104 * v * v;
      }

      function danielsFractionFromTime(tMin) {
        return (
          0.8 +
          0.1894393 * Math.exp(-0.012778 * tMin) +
          0.2989558 * Math.exp(-0.1932605 * tMin)
        );
      }

      function classifyVO2(age, sex, vo2) {
        // Simplified bands based on age group
        const bands = {
          male: [
            { min: 20, max: 29, limits: [35, 42, 47, 53] },
            { min: 30, max: 39, limits: [33, 40, 45, 50] },
            { min: 40, max: 49, limits: [30, 36, 41, 46] },
            { min: 50, max: 59, limits: [26, 32, 37, 42] },
            { min: 60, max: 120, limits: [22, 27, 32, 37] },
          ],
          female: [
            { min: 20, max: 29, limits: [28, 34, 39, 45] },
            { min: 30, max: 39, limits: [26, 32, 37, 42] },
            { min: 40, max: 49, limits: [24, 30, 35, 40] },
            { min: 50, max: 59, limits: [22, 27, 32, 37] },
            { min: 60, max: 120, limits: [20, 25, 30, 35] },
          ],
        };
        const s = sex === "female" ? "female" : "male";
        let group =
          bands[s].find((g) => age >= g.min && age <= g.max) ||
          bands[s][bands[s].length - 1];
        const [poor, fair, good, veryGood] = group.limits;
        let category, percentile;
        if (vo2 < poor) {
          category = "Poor";
          percentile = "Below ~20th percentile";
        } else if (vo2 < fair) {
          category = "Fair";
          percentile = "Around 20th–40th percentile";
        } else if (vo2 < good) {
          category = "Good";
          percentile = "Around 40th–60th percentile";
        } else if (vo2 < veryGood) {
          category = "Very good";
          percentile = "Around 60th–80th percentile";
        } else {
          category = "Excellent";
          percentile = "Above ~80th percentile";
        }
        return { category, percentile };
      }

      function buildZones(vo2) {
        // Based on % of VO2max pace and HR
        const zones = [
          {
            name: "Zone 1 – Easy / Recovery",
            purpose: "Very easy endurance, recovery runs",
            pace: "55–65%",
            hr: "60–72%",
          },
          {
            name: "Zone 2 – Endurance",
            purpose: "Aerobic base, long runs",
            pace: "65–75%",
            hr: "70–80%",
          },
          {
            name: "Zone 3 – Tempo / Threshold",
            purpose: "Lactate threshold, tempo runs",
            pace: "80–90%",
            hr: "80–90%",
          },
          {
            name: "Zone 4 – VO2 max intervals",
            purpose: "3–6 min intervals near VO2 max",
            pace: "95–100%",
            hr: "90–95%",
          },
          {
            name: "Zone 5 – Anaerobic / Sprint",
            purpose: "Short, very hard efforts",
            pace: "> 100%",
            hr: "95–100%+",
          },
        ];
        const tbody = document.getElementById("vo2-zones-body");
        tbody.innerHTML = "";
        zones.forEach((z) => {
          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="px-3 py-2 border-b align-top font-medium">${z.name}</td>
        <td class="px-3 py-2 border-b align-top">${z.purpose}</td>
        <td class="px-3 py-2 border-b align-top">${z.pace}</td>
        <td class="px-3 py-2 border-b align-top">${z.hr}</td>
      `;
          tbody.appendChild(tr);
        });
      }

      document.addEventListener("DOMContentLoaded", function () {
        // Tabs
        document.querySelectorAll(".vo2-tab").forEach((btn) => {
          btn.addEventListener("click", () => {
            document.querySelectorAll(".vo2-tab").forEach((b) => {
              b.classList.remove("border-blue-600", "text-blue-600");
              b.classList.add("text-gray-600", "border-transparent");
            });
            btn.classList.add("border-blue-600", "text-blue-600");
            const tab = btn.getAttribute("data-tab");
            document
              .querySelectorAll(".vo2-panel")
              .forEach((p) => p.classList.add("hidden"));
            document.getElementById("tab-" + tab).classList.remove("hidden");
          });
        });

        document
          .querySelectorAll("#faq-container .faq-question")
          .forEach((btn) => {
            btn.addEventListener("click", () => {
              const ans = btn.parentElement.querySelector(".faq-answer");
              const open = !ans.classList.contains("hidden");
              document
                .querySelectorAll("#faq-container .faq-answer")
                .forEach((a) => a.classList.add("hidden"));
              document
                .querySelectorAll(
                  "#faq-container .faq-question span:last-child"
                )
                .forEach((s) => (s.textContent = "+"));
              if (!open) {
                ans.classList.remove("hidden");
                btn.querySelector("span:last-child").textContent = "−";
              }
            });
          });

        // Calculate
        document
          .getElementById("vo2-calc-btn")
          .addEventListener("click", () => {
            const errorEl = document.getElementById("vo2-error");
            errorEl.textContent = "";
            let vo2 = null;
            let methodText = "";
            const tab = getSelectedTab();

            const age = parseInt(document.getElementById("age").value, 10);
            const sex = document.getElementById("sex").value || "male";

            if (!age || age < 10 || age > 90) {
              errorEl.textContent =
                "Please enter a valid age between 10 and 90 years.";
              return;
            }

            if (tab === "race") {
              const dist = parseFloat(
                document.getElementById("race-distance").value
              );
              const unit = document.getElementById("race-distance-unit").value;
              const h = parseInt(
                document.getElementById("race-hours").value || "0",
                10
              );
              const m = parseInt(
                document.getElementById("race-minutes").value || "0",
                10
              );
              const s = parseInt(
                document.getElementById("race-seconds").value || "0",
                10
              );
              const tMin = timeToMinutes(h, m, s);
              if (!dist || dist <= 0 || !tMin || tMin <= 0) {
                errorEl.textContent =
                  "Please enter a valid race distance and time.";
                return;
              }
              let distMeters = dist;
              if (unit === "km") distMeters *= 1000;
              if (unit === "miles") distMeters *= 1609.34;
              const v = distMeters / tMin; // m/min
              const vo2run = danielsVO2FromSpeed(v);
              const frac = danielsFractionFromTime(tMin);
              vo2 = vo2run / frac;
              methodText =
                "Estimated from race performance using Daniels & Gilbert model.";
            } else if (tab === "cooper") {
              let d = parseFloat(
                document.getElementById("cooper-distance").value
              );
              const unit = document.getElementById(
                "cooper-distance-unit"
              ).value;
              if (!d || d <= 0) {
                errorEl.textContent =
                  "Please enter the distance covered in 12 minutes.";
                return;
              }
              if (unit === "km") d *= 1000;
              if (unit === "miles") d *= 1609.34;
              vo2 = (d - 504.9) / 44.73;
              methodText = "Estimated from Cooper 12‑minute test.";
            } else if (tab === "hr") {
              const paceMin = parseInt(
                document.getElementById("hr-pace-min").value || "0",
                10
              );
              const paceSec = parseInt(
                document.getElementById("hr-pace-sec").value || "0",
                10
              );
              const paceUnit = document.getElementById("hr-pace-unit").value;
              const hrAvg = parseInt(
                document.getElementById("hr-avg").value || "0",
                10
              );
              let hrMax = parseInt(
                document.getElementById("hr-max").value || "0",
                10
              );
              if (!paceMin && !paceSec) {
                errorEl.textContent = "Please enter your pace.";
                return;
              }
              if (!hrAvg || hrAvg < 60) {
                errorEl.textContent =
                  "Please enter a valid average heart rate.";
                return;
              }
              if (!hrMax || hrMax < 80) {
                hrMax = Math.round(208 - 0.7 * age);
              }
              const v = paceToSpeedMetersPerMin(
                paceMin,
                paceSec,
                paceUnit === "mile" ? "mile" : "km"
              );
              if (!v) {
                errorEl.textContent = "Please enter a valid pace.";
                return;
              }
              const vo2run = danielsVO2FromSpeed(v);
              const fHR = hrAvg / hrMax;
              if (fHR <= 0 || fHR > 1.1) {
                errorEl.textContent =
                  "Heart rate values look unrealistic. Please check them.";
                return;
              }
              vo2 = vo2run / fHR;
              methodText = "Estimated from running pace and heart rate.";
            }

            if (!vo2 || !isFinite(vo2) || vo2 <= 0) {
              errorEl.textContent =
                "Unable to compute a realistic VO2 max from the provided data.";
              return;
            }

            // Clamp to reasonable range
            vo2 = Math.max(15, Math.min(90, vo2));
            const vo2Rounded = vo2.toFixed(1);

            const classification = classifyVO2(age, sex, vo2);
            document.getElementById("vo2-value").textContent = vo2Rounded;
            document.getElementById("vo2-method").textContent = methodText;
            document.getElementById("vo2-category").textContent =
              classification.category;
            document.getElementById("vo2-percentile").textContent =
              classification.percentile;

            buildZones(vo2);

            document.getElementById("vo2-results").classList.remove("hidden");
            document
              .getElementById("vo2-results")
              .scrollIntoView({ behavior: "smooth", block: "start" });
          });

        document
          .getElementById("vo2-reset-btn")
          .addEventListener("click", () => {
            document.querySelectorAll("input").forEach((i) => {
              if (i.type === "number") i.value = "";
            });
            document.getElementById("vo2-error").textContent = "";
            document.getElementById("vo2-results").classList.add("hidden");
          });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
