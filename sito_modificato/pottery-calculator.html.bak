    <script>
      // Tab switching
      const tabs = document.querySelectorAll('[role="tab"]');
      const panels = {
        "panel-shrinkage": document.getElementById("panel-shrinkage"),
        "panel-size": document.getElementById("panel-size"),
        "panel-batch": document.getElementById("panel-batch"),
      };

      tabs.forEach((tab) => {
        tab.addEventListener("click", () => {
          const targetId = tab.getAttribute("data-target");
          // Update tab styles
          tabs.forEach((t) => {
            t.classList.remove("tab-button-active");
            t.classList.add("tab-button-inactive");
            t.setAttribute("aria-selected", "false");
          });
          tab.classList.add("tab-button-active");
          tab.classList.remove("tab-button-inactive");
          tab.setAttribute("aria-selected", "true");
          // Show correct panel
          Object.keys(panels).forEach((id) => {
            panels[id].classList.toggle("hidden", id !== targetId);
          });
        });
      });

      // Shrinkage calculator
      const wetInput = document.getElementById("shrink-wet-length");
      const firedInput = document.getElementById("shrink-fired-length");
      const shrinkBtn = document.getElementById("btn-calc-shrinkage");
      const resetShrinkBtn = document.getElementById("btn-reset-shrinkage");
      const shrinkResultBox = document.getElementById("shrinkage-result");
      const shrinkPercentSpan = document.getElementById("shrinkage-percent");
      const shrinkFactorSpan = document.getElementById("shrinkage-factor");

      shrinkBtn.addEventListener("click", () => {
        const wet = parseFloat(wetInput.value);
        const fired = parseFloat(firedInput.value);
        if (
          !isFinite(wet) ||
          !isFinite(fired) ||
          wet <= 0 ||
          fired <= 0 ||
          fired >= wet
        ) {
          alert(
            "Please enter valid wet and fired lengths. Fired length must be smaller than wet length."
          );
          return;
        }
        const shrinkage = ((wet - fired) / wet) * 100;
        const factor = 1 - shrinkage / 100;
        shrinkPercentSpan.textContent = shrinkage.toFixed(2);
        shrinkFactorSpan.textContent = factor.toFixed(4);
        shrinkResultBox.classList.remove("hidden");

        // Auto-fill shrinkage % in other tabs
        const sizeShrinkInput = document.getElementById(
          "size-shrinkage-percent"
        );
        const batchShrinkInput = document.getElementById("batch-shrinkage");
        if (sizeShrinkInput && !sizeShrinkInput.value)
          sizeShrinkInput.value = shrinkage.toFixed(2);
        if (batchShrinkInput && !batchShrinkInput.value)
          batchShrinkInput.value = shrinkage.toFixed(2);
      });

      resetShrinkBtn.addEventListener("click", () => {
        wetInput.value = "";
        firedInput.value = "";
        shrinkResultBox.classList.add("hidden");
      });

      // Size converter
      const sizeShrinkInput = document.getElementById("size-shrinkage-percent");
      const sizeWetInput = document.getElementById("size-wet");
      const sizeFinishedInput = document.getElementById("size-finished");
      const sizeUnitSelect = document.getElementById("size-unit");
      const sizeBtn = document.getElementById("btn-calc-size");
      const sizeResetBtn = document.getElementById("btn-reset-size");
      const sizeResultBox = document.getElementById("size-result");
      const sizeResultWet = document.getElementById("size-result-wet");
      const sizeResultFinished = document.getElementById(
        "size-result-finished"
      );
      const sizeResultUnit = document.getElementById("size-result-unit");
      const sizeResultUnit2 = document.getElementById("size-result-unit-2");

      sizeBtn.addEventListener("click", () => {
        const s = parseFloat(sizeShrinkInput.value);
        const wet = parseFloat(sizeWetInput.value);
        const fin = parseFloat(sizeFinishedInput.value);

        if (!isFinite(s) || s <= 0 || s >= 50) {
          alert(
            "Please enter a realistic shrinkage percentage (between 1% and 50%)."
          );
          return;
        }

        const factor = 1 - s / 100;
        let wetVal = wet;
        let finVal = fin;

        if (isFinite(wetVal) && !isFinite(finVal)) {
          finVal = wetVal * factor;
        } else if (!isFinite(wetVal) && isFinite(finVal)) {
          wetVal = finVal / factor;
        } else if (isFinite(wetVal) && isFinite(finVal)) {
          // Both provided: we can check consistency but just recompute finished from wet
          finVal = wetVal * factor;
        } else {
          alert(
            "Enter shrinkage % and at least one of wet size or finished size."
          );
          return;
        }

        sizeResultWet.textContent = wetVal.toFixed(2);
        sizeResultFinished.textContent = finVal.toFixed(2);
        const unit = sizeUnitSelect.value;
        sizeResultUnit.textContent = unit;
        sizeResultUnit2.textContent = unit;
        sizeResultBox.classList.remove("hidden");
      });

      sizeResetBtn.addEventListener("click", () => {
        sizeShrinkInput.value = "";
        sizeWetInput.value = "";
        sizeFinishedInput.value = "";
        sizeResultBox.classList.add("hidden");
      });

      // Batch estimator
      const batchCountInput = document.getElementById("batch-count");
      const batchShrinkInput = document.getElementById("batch-shrinkage");
      const batchDiamInput = document.getElementById("batch-diameter");
      const batchHeightInput = document.getElementById("batch-height");
      const batchWallInput = document.getElementById("batch-wall");
      const batchDimUnitSelect = document.getElementById("batch-dim-unit");
      const batchDensityInput = document.getElementById("batch-density");
      const batchBtn = document.getElementById("btn-calc-batch");
      const batchResetBtn = document.getElementById("btn-reset-batch");
      const batchResultBox = document.getElementById("batch-result");
      const batchWetDiamSpan = document.getElementById("batch-wet-diameter");
      const batchWetHeightSpan = document.getElementById("batch-wet-height");
      const batchDimUnitOut = document.getElementById("batch-dim-unit-out");
      const batchDimUnitOut2 = document.getElementById("batch-dim-unit-out-2");
      const batchWeightPieceSpan =
        document.getElementById("batch-weight-piece");
      const batchWeightTotalSpan =
        document.getElementById("batch-weight-total");
      const batchWeightTotalKgSpan = document.getElementById(
        "batch-weight-total-kg"
      );

      function convertToCm(value, unit) {
        if (!isFinite(value)) return NaN;
        switch (unit) {
          case "mm":
            return value / 10;
          case "cm":
            return value;
          case "in":
            return value * 2.54;
          default:
            return value;
        }
      }

      batchBtn.addEventListener("click", () => {
        const n = parseInt(batchCountInput.value, 10);
        const s = parseFloat(batchShrinkInput.value);
        const d = parseFloat(batchDiamInput.value);
        const h = parseFloat(batchHeightInput.value);
        const t = parseFloat(batchWallInput.value);
        const rho = parseFloat(batchDensityInput.value);

        if (!isFinite(n) || n <= 0) {
          alert("Please enter a valid number of pieces.");
          return;
        }
        if (!isFinite(s) || s <= 0 || s >= 50) {
          alert(
            "Please enter a realistic shrinkage percentage (between 1% and 50%)."
          );
          return;
        }
        if (
          !isFinite(d) ||
          !isFinite(h) ||
          !isFinite(t) ||
          d <= 0 ||
          h <= 0 ||
          t <= 0
        ) {
          alert(
            "Please enter positive values for diameter, height, and wall thickness."
          );
          return;
        }
        if (!isFinite(rho) || rho <= 0) {
          alert("Please enter a positive clay density (e.g. 1.9 g/cm³).");
          return;
        }

        const factor = 1 - s / 100;
        const unit = batchDimUnitSelect.value;

        // Convert finished dimensions to wet
        const wetD = d / factor;
        const wetH = h / factor;
        const wetT = t / factor;

        // Convert to cm for volume calculation
        const wetDcm = convertToCm(wetD, unit);
        const wetHcm = convertToCm(wetH, unit);
        const wetTcm = convertToCm(wetT, unit);

        if (!isFinite(wetDcm) || !isFinite(wetHcm) || !isFinite(wetTcm)) {
          alert("Could not convert dimensions. Please check your inputs.");
          return;
        }

        const rOuter = wetDcm / 2;
        const hCm = wetHcm;
        const tCm = wetTcm;

        // Volume of clay (cm³)
        const volumeClay = Math.PI * hCm * (2 * rOuter * tCm - tCm * tCm);
        if (!isFinite(volumeClay) || volumeClay <= 0) {
          alert("Calculated volume is not valid. Try adjusting dimensions.");
          return;
        }

        const weightPerPiece = volumeClay * rho; // g
        const weightTotal = weightPerPiece * n;
        const weightTotalKg = weightTotal / 1000;

        batchWetDiamSpan.textContent = wetD.toFixed(2);
        batchWetHeightSpan.textContent = wetH.toFixed(2);
        batchDimUnitOut.textContent = unit;
        batchDimUnitOut2.textContent = unit;
        batchWeightPieceSpan.textContent = weightPerPiece.toFixed(0);
        batchWeightTotalSpan.textContent = weightTotal.toFixed(0);
        batchWeightTotalKgSpan.textContent = weightTotalKg.toFixed(2);

        batchResultBox.classList.remove("hidden");
      });

      batchResetBtn.addEventListener("click", () => {
        batchCountInput.value = "6";
        batchShrinkInput.value = "";
        batchDiamInput.value = "";
        batchHeightInput.value = "";
        batchWallInput.value = "";
        batchDensityInput.value = "";
        batchResultBox.classList.add("hidden");
      });

      // Mobile menu script safety (if not in external file)
      document.addEventListener("DOMContentLoaded", () => {
        const toggle = document.getElementById("mobile-menu-toggle");
        const menu = document.getElementById("mobile-menu");
        if (toggle && menu) {
          toggle.addEventListener("click", () => {
            const isHidden = menu.classList.contains("hidden");
            menu.classList.toggle("hidden", !isHidden);
            toggle.setAttribute("aria-expanded", String(isHidden));
          });
        }
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
