    <script>
      // Rebar data: US and metric
      const REBAR_US = [
        { size: "#3", diaIn: 0.375 },
        { size: "#4", diaIn: 0.5 },
        { size: "#5", diaIn: 0.625 },
        { size: "#6", diaIn: 0.75 },
        { size: "#7", diaIn: 0.875 },
        { size: "#8", diaIn: 1.0 },
        { size: "#9", diaIn: 1.128 },
        { size: "#10", diaIn: 1.27 },
        { size: "#11", diaIn: 1.41 },
        { size: "#14", diaIn: 1.693 },
        { size: "#18", diaIn: 2.257 },
      ];

      const REBAR_METRIC = [
        { diaMm: 8 },
        { diaMm: 10 },
        { diaMm: 12 },
        { diaMm: 14 },
        { diaMm: 16 },
        { diaMm: 20 },
        { diaMm: 25 },
        { diaMm: 28 },
        { diaMm: 32 },
        { diaMm: 36 },
        { diaMm: 40 },
      ];

      const KG_PER_M3_TO_LB_PER_FT3 = 0.06242796; // 1 kg/m3 = 0.06242796 lb/ft3
      const FT_TO_M = 0.3048;
      const IN_TO_M = 0.0254;
      const MM_TO_M = 0.001;
      const KG_TO_LB = 2.2046226218;

      const modeImperialBtn = document.getElementById("mode-imperial");
      const modeMetricBtn = document.getElementById("mode-metric");
      const barStandardSelect = document.getElementById("bar-standard");
      const barSizeSelect = document.getElementById("bar-size");
      const customDiameterInput = document.getElementById("custom-diameter");
      const diameterUnitLabel = document.getElementById("diameter-unit-label");
      const lengthInput = document.getElementById("bar-length");
      const lengthUnitSelect = document.getElementById("length-unit");
      const qtyInput = document.getElementById("bar-qty");
      const densityInput = document.getElementById("density");
      const calcBtn = document.getElementById("calc-btn");
      const resetBtn = document.getElementById("reset-btn");
      const errorMsg = document.getElementById("error-msg");
      const resultsDiv = document.getElementById("results");

      const perBarKgSpan = document.getElementById("per-bar-kg");
      const perBarLbSpan = document.getElementById("per-bar-lb");
      const perMKgSpan = document.getElementById("per-m-kg");
      const perMLbSpan = document.getElementById("per-m-lb");
      const perFtKgSpan = document.getElementById("per-ft-kg");
      const perFtLbSpan = document.getElementById("per-ft-lb");
      const totalLengthMSpan = document.getElementById("total-length-m");
      const totalLengthFtSpan = document.getElementById("total-length-ft");
      const totalKgSpan = document.getElementById("total-kg");
      const totalLbSpan = document.getElementById("total-lb");

      let currentMode = "imperial"; // 'imperial' or 'metric'

      function populateBarSizes() {
        barSizeSelect.innerHTML = "";
        const standard = barStandardSelect.value;
        if (standard === "imperial") {
          REBAR_US.forEach((bar) => {
            const opt = document.createElement("option");
            opt.value = bar.size;
            opt.textContent = `${bar.size} (${(bar.diaIn * 25.4).toFixed(
              1
            )} mm)`;
            barSizeSelect.appendChild(opt);
          });
          barSizeSelect.classList.remove("hidden");
          customDiameterInput.classList.add("hidden");
          diameterUnitLabel.textContent =
            "Diameter is in inches for US bars, millimeters for metric bars.";
        } else if (standard === "metric") {
          REBAR_METRIC.forEach((bar) => {
            const opt = document.createElement("option");
            opt.value = bar.diaMm;
            opt.textContent = `${bar.diaMm} mm`;
            barSizeSelect.appendChild(opt);
          });
          barSizeSelect.classList.remove("hidden");
          customDiameterInput.classList.add("hidden");
          diameterUnitLabel.textContent =
            "Diameter is in inches for US bars, millimeters for metric bars.";
        } else {
          // custom
          barSizeSelect.classList.add("hidden");
          customDiameterInput.classList.remove("hidden");
          if (currentMode === "imperial") {
            customDiameterInput.placeholder = "Diameter (inches)";
            diameterUnitLabel.textContent = "Enter diameter in inches.";
          } else {
            customDiameterInput.placeholder = "Diameter (mm)";
            diameterUnitLabel.textContent = "Enter diameter in millimeters.";
          }
        }
      }

      function setMode(mode) {
        currentMode = mode;
        if (mode === "imperial") {
          modeImperialBtn.classList.add("bg-blue-600", "text-white");
          modeImperialBtn.classList.remove("bg-gray-100", "text-gray-700");
          modeMetricBtn.classList.remove("bg-blue-600", "text-white");
          modeMetricBtn.classList.add("bg-gray-100", "text-gray-700");
          lengthUnitSelect.value = "ft";
          if (barStandardSelect.value === "metric") {
            barStandardSelect.value = "imperial";
          }
        } else {
          modeMetricBtn.classList.add("bg-blue-600", "text-white");
          modeMetricBtn.classList.remove("bg-gray-100", "text-gray-700");
          modeImperialBtn.classList.remove("bg-blue-600", "text-white");
          modeImperialBtn.classList.add("bg-gray-100", "text-gray-700");
          lengthUnitSelect.value = "m";
          if (barStandardSelect.value === "imperial") {
            barStandardSelect.value = "metric";
          }
        }
        populateBarSizes();
      }

      function formatNumber(num, decimals = 3) {
        if (!isFinite(num)) return "–";
        return Number(num.toFixed(decimals)).toString();
      }

      function calculate() {
        errorMsg.textContent = "";
        resultsDiv.classList.add("hidden");

        const standard = barStandardSelect.value;
        const lengthVal = parseFloat(lengthInput.value);
        const qtyVal = parseInt(qtyInput.value, 10);
        const densityVal = parseFloat(densityInput.value);

        if (!(lengthVal > 0)) {
          errorMsg.textContent = "Enter a valid bar length.";
          return;
        }
        if (!(qtyVal > 0)) {
          errorMsg.textContent = "Enter a valid number of bars.";
          return;
        }
        if (!(densityVal > 0)) {
          errorMsg.textContent = "Enter a valid steel density.";
          return;
        }

        let diaM; // diameter in meters

        if (standard === "imperial") {
          const selectedSize = barSizeSelect.value;
          const bar = REBAR_US.find((b) => b.size === selectedSize);
          if (!bar) {
            errorMsg.textContent = "Select a valid bar size.";
            return;
          }
          diaM = bar.diaIn * IN_TO_M;
        } else if (standard === "metric") {
          const diaMm = parseFloat(barSizeSelect.value);
          if (!(diaMm > 0)) {
            errorMsg.textContent = "Select a valid bar diameter.";
            return;
          }
          diaM = diaMm * MM_TO_M;
        } else {
          const customDia = parseFloat(customDiameterInput.value);
          if (!(customDia > 0)) {
            errorMsg.textContent = "Enter a valid custom diameter.";
            return;
          }
          if (currentMode === "imperial") {
            diaM = customDia * IN_TO_M;
          } else {
            diaM = customDia * MM_TO_M;
          }
        }

        // Convert length to meters
        let lengthM;
        if (lengthUnitSelect.value === "ft") {
          lengthM = lengthVal * FT_TO_M;
        } else {
          lengthM = lengthVal;
        }

        // Cross-sectional area
        const area = (Math.PI * Math.pow(diaM, 2)) / 4; // m²

        // Weight per meter (kg/m)
        const wPerM = area * densityVal;

        // Weight per foot (kg/ft)
        const wPerFt = wPerM * FT_TO_M;

        // Weight per bar (kg)
        const wPerBar = wPerM * lengthM;

        // Totals
        const totalLengthM = lengthM * qtyVal;
        const totalLengthFt = totalLengthM / FT_TO_M;
        const totalKg = wPerBar * qtyVal;
        const totalLb = totalKg * KG_TO_LB;

        // Populate results
        perBarKgSpan.textContent = formatNumber(wPerBar, 3);
        perBarLbSpan.textContent = formatNumber(wPerBar * KG_TO_LB, 3);
        perMKgSpan.textContent = formatNumber(wPerM, 3);
        perMLbSpan.textContent = formatNumber(wPerM * KG_TO_LB, 3);
        perFtKgSpan.textContent = formatNumber(wPerFt, 3);
        perFtLbSpan.textContent = formatNumber(wPerFt * KG_TO_LB, 3);
        totalLengthMSpan.textContent = formatNumber(totalLengthM, 2);
        totalLengthFtSpan.textContent = formatNumber(totalLengthFt, 2);
        totalKgSpan.textContent = formatNumber(totalKg, 2);
        totalLbSpan.textContent = formatNumber(totalLb, 2);

        resultsDiv.classList.remove("hidden");
      }

      function resetCalculator() {
        setMode("imperial");
        barStandardSelect.value = "imperial";
        populateBarSizes();
        lengthInput.value = 20;
        lengthUnitSelect.value = "ft";
        qtyInput.value = 10;
        densityInput.value = 7850;
        customDiameterInput.value = "";
        errorMsg.textContent = "";
        resultsDiv.classList.add("hidden");
      }

      function buildRebarTable() {
        const tbody = document.getElementById("rebar-table-body");
        tbody.innerHTML = "";
        const density = 7850; // kg/m3
        REBAR_US.forEach((bar) => {
          const diaM = bar.diaIn * IN_TO_M;
          const area = (Math.PI * Math.pow(diaM, 2)) / 4;
          const wPerM = area * density;
          const wPerFtLb = wPerM * FT_TO_M * KG_TO_LB;
          const metricMatch = REBAR_METRIC.reduce((closest, m) => {
            return Math.abs(m.diaMm - bar.diaIn * 25.4) <
              Math.abs(closest.diaMm - bar.diaIn * 25.4)
              ? m
              : closest;
          }, REBAR_METRIC[0]);
          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="px-3 py-2 whitespace-nowrap font-medium text-gray-800">${
          bar.size
        }</td>
        <td class="px-3 py-2 whitespace-nowrap text-gray-700">${(
          bar.diaIn * 25.4
        ).toFixed(1)} mm</td>
        <td class="px-3 py-2 whitespace-nowrap text-gray-700">${wPerM.toFixed(
          3
        )}</td>
        <td class="px-3 py-2 whitespace-nowrap text-gray-700">${wPerFtLb.toFixed(
          3
        )}</td>
      `;
          tbody.appendChild(tr);
        });
      }

      // Event listeners
      modeImperialBtn.addEventListener("click", () => setMode("imperial"));
      modeMetricBtn.addEventListener("click", () => setMode("metric"));
      barStandardSelect.addEventListener("change", populateBarSizes);
      calcBtn.addEventListener("click", calculate);
      resetBtn.addEventListener("click", resetCalculator);

      // Init
      setMode("imperial");
      populateBarSizes();
      buildRebarTable();
    </script>

