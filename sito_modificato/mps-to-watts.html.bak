    <script>
      (function () {
        const modeTabs = document.querySelectorAll(".mode-tab");
        const systemTypeEl = document.getElementById("system-type");
        const primaryLabel = document.getElementById("primary-label");
        const primaryUnit = document.getElementById("primary-unit");
        const primaryValue = document.getElementById("primary-value");
        const resultLabel = document.getElementById("result-label");
        const resultUnit = document.getElementById("result-unit");
        const resultEl = document.getElementById("result");
        const resultSecondary = document.getElementById("result-secondary");
        const voltageEl = document.getElementById("voltage");
        const pfEl = document.getElementById("power-factor");
        const pfRange = document.getElementById("pf-range");
        const calcBtn = document.getElementById("calculate-btn");
        const clearBtn = document.getElementById("clear-btn");
        const errorMsg = document.getElementById("error-msg");
        const voltagePresetsToggle = document.getElementById(
          "voltage-presets-toggle"
        );
        const voltagePresets = document.getElementById("voltage-presets");

        let mode = "ampsToWatts";

        function syncPF(fromRange) {
          if (fromRange) {
            pfEl.value = pfRange.value;
          } else {
            let v = parseFloat(pfEl.value);
            if (isNaN(v)) v = 1;
            if (v < 0) v = 0;
            if (v > 1) v = 1;
            pfEl.value = v.toFixed(2);
            pfRange.value = v;
          }
        }

        pfRange.addEventListener("input", () => syncPF(true));
        pfEl.addEventListener("change", () => syncPF(false));

        modeTabs.forEach((tab) => {
          tab.addEventListener("click", () => {
            modeTabs.forEach((t) => {
              t.classList.remove("bg-blue-600", "text-white");
              t.classList.add("bg-white", "text-gray-700");
              t.setAttribute("aria-selected", "false");
            });
            tab.classList.add("bg-blue-600", "text-white");
            tab.classList.remove("bg-white", "text-gray-700");
            tab.setAttribute("aria-selected", "true");
            mode = tab.dataset.mode;
            updateLabels();
            resultEl.value = "";
            resultSecondary.textContent = "";
            errorMsg.textContent = "";
          });
        });

        function updateLabels() {
          if (mode === "ampsToWatts") {
            primaryLabel.textContent = "Current (amps)";
            primaryUnit.textContent = "A";
            resultLabel.textContent = "Power (watts)";
            resultUnit.textContent = "W";
          } else {
            primaryLabel.textContent = "Power (watts)";
            primaryUnit.textContent = "W";
            resultLabel.textContent = "Current (amps)";
            resultUnit.textContent = "A";
          }
        }

        function formatNumber(num) {
          if (!isFinite(num)) return "";
          if (Math.abs(num) >= 1000)
            return num.toLocaleString(undefined, { maximumFractionDigits: 2 });
          return num.toLocaleString(undefined, { maximumFractionDigits: 4 });
        }

        function calculate() {
          errorMsg.textContent = "";
          resultSecondary.textContent = "";

          const primary = parseFloat(primaryValue.value);
          const V = parseFloat(voltageEl.value);
          let pf = parseFloat(pfEl.value);

          if (isNaN(primary) || primary < 0) {
            errorMsg.textContent =
              "Enter a valid non‑negative value for " +
              (mode === "ampsToWatts" ? "current." : "power.");
            return;
          }
          if (isNaN(V) || V <= 0) {
            errorMsg.textContent = "Enter a valid positive voltage.";
            return;
          }
          if (isNaN(pf) || pf < 0 || pf > 1) {
            errorMsg.textContent = "Power factor must be between 0 and 1.";
            return;
          }

          const system = systemTypeEl.value;
          const sqrt3 = Math.sqrt(3);
          let P, I, S;

          if (mode === "ampsToWatts") {
            const Iin = primary;
            if (system === "dc") {
              P = V * Iin;
              S = P;
            } else if (system === "ac1") {
              S = V * Iin;
              P = S * pf;
            } else {
              S = sqrt3 * V * Iin;
              P = S * pf;
            }
            resultEl.value = formatNumber(P);
            const kva = S / 1000;
            const kw = P / 1000;
            resultSecondary.textContent = `Real power ≈ ${kw.toFixed(
              3
            )} kW, apparent power ≈ ${kva.toFixed(3)} kVA.`;
          } else {
            const Pin = primary;
            if (system === "dc") {
              I = Pin / V;
              S = Pin;
            } else if (system === "ac1") {
              I = Pin / (V * pf || 1);
              S = V * I;
            } else {
              I = Pin / (sqrt3 * V * pf || 1);
              S = sqrt3 * V * I;
            }
            resultEl.value = formatNumber(I);
            const kva = S / 1000;
            const kw = Pin / 1000;
            resultSecondary.textContent = `Input power ≈ ${kw.toFixed(
              3
            )} kW, apparent power ≈ ${kva.toFixed(3)} kVA.`;
          }
        }

        calcBtn.addEventListener("click", calculate);

        [primaryValue, voltageEl, pfEl].forEach((el) => {
          el.addEventListener("keydown", (e) => {
            if (e.key === "Enter") {
              e.preventDefault();
              calculate();
            }
          });
        });

        clearBtn.addEventListener("click", () => {
          primaryValue.value = "";
          voltageEl.value = "";
          pfEl.value = "1";
          pfRange.value = "1";
          resultEl.value = "";
          resultSecondary.textContent = "";
          errorMsg.textContent = "";
        });

        voltagePresetsToggle.addEventListener("click", () => {
          voltagePresets.classList.toggle("hidden");
        });

        voltagePresets.querySelectorAll("button[data-v]").forEach((btn) => {
          btn.addEventListener("click", () => {
            voltageEl.value = btn.getAttribute("data-v");
          });
        });

        updateLabels();
      })();
    </script>

