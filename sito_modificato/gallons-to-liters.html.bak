            <script>
              document.addEventListener('DOMContentLoaded', function() {
                const gallonInput = document.getElementById('gallons');
                const literInput = document.getElementById('liters');
                const radioSelector = document.getElementById('gallon-type-selector');
                const conversionInfo = document.getElementById('conversion-info');
                const tableBody = document.getElementById('conversion-table-body');

                const US_GALLON_TO_LITER = 3.78541;
                const UK_GALLON_TO_LITER = 4.54609;

                function getConversionFactor() {
                  const selectedType = document.querySelector('input[name="gallonType"]:checked').value;
                  return (selectedType === 'us') ? US_GALLON_TO_LITER : UK_GALLON_TO_LITER;
                }

                function formatNumber(num) {
                  return parseFloat(num.toFixed(10)).toString();
                }

                function updateInfoAndTable(startGallons) {
                  const factor = getConversionFactor();
                  const gallonType = (factor === US_GALLON_TO_LITER) ? "US" : "UK";
                  
                  // Update info box
                  conversionInfo.textContent = `1 ${gallonType} Gallon = ${formatNumber(factor)} Liters`;
                  
                  // Update table
                  let html = '';
                  const start = Math.max(0, Math.floor(startGallons));
                  for (let i = 0; i < 20; i++) {
                    const currentGallons = start + i;
                    const currentLiters = currentGallons * factor;
                    html += `
                      <tr>
                        <td class="border border-gray-300 px-4 py-2">${currentGallons} ${gallonType} gal</td>
                        <td class="border border-gray-300 px-4 py-2">${formatNumber(currentLiters)} L</td>
                      </tr>
                    `;
                  }
                  tableBody.innerHTML = html;
                }

                function convertGallonsToLiters() {
                  const factor = getConversionFactor();
                  const gallons = parseFloat(gallonInput.value) || 0;
                  
                  if (gallons > 0) {
                    const liters = gallons * factor;
                    literInput.value = formatNumber(liters);
                  } else {
                    literInput.value = (gallonInput.value === '') ? '' : '0';
                  }
                  updateInfoAndTable(gallons || 1);
                }

                function convertLitersToGallons() {
                  const factor = getConversionFactor();
                  const liters = parseFloat(literInput.value) || 0;

                  if (liters > 0) {
                    const gallons = liters / factor;
                    gallonInput.value = formatNumber(gallons);
                    updateInfoAndTable(gallons);
                  } else {
                    gallonInput.value = (literInput.value === '') ? '' : '0';
                    if(literInput.value === '') {
                        updateInfoAndTable(1);
                    }
                  }
                }

                gallonInput.addEventListener('input', convertGallonsToLiters);
                literInput.addEventListener('input', convertLitersToGallons);
                radioSelector.addEventListener('change', convertGallonsToLiters);

                // Initial load
                convertGallonsToLiters();
              });
            </script>

