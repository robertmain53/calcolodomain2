    <script>
      // Utility functions
      function toNumber(value) {
        const n = parseFloat(value);
        return isNaN(n) ? 0 : n;
      }
      function formatMoney(value) {
        if (!isFinite(value)) return "$0.00";
        return "$" + value.toFixed(2);
      }
      function formatPercent(value) {
        if (!isFinite(value)) return "0.00%";
        return value.toFixed(2) + "%";
      }

      // Tab handling
      const tabs = [
        { id: "basic", panel: "panel-basic" },
        { id: "original", panel: "panel-original" },
        { id: "percent", panel: "panel-percent" },
        { id: "multi", panel: "panel-multi" },
      ];

      tabs.forEach((t) => {
        const tabEl = document.getElementById("tab-" + t.id);
        const panelEl = document.getElementById(t.panel);
        if (!tabEl || !panelEl) return;
        tabEl.addEventListener("click", () => {
          tabs.forEach((other) => {
            const otherTab = document.getElementById("tab-" + other.id);
            const otherPanel = document.getElementById(other.panel);
            if (!otherTab || !otherPanel) return;
            if (other.id === t.id) {
              otherTab.classList.remove("tab-inactive");
              otherTab.classList.add("tab-active");
              otherTab.setAttribute("aria-pressed", "true");
              otherPanel.classList.remove("hidden");
            } else {
              otherTab.classList.remove("tab-active");
              otherTab.classList.add("tab-inactive");
              otherTab.setAttribute("aria-pressed", "false");
              otherPanel.classList.add("hidden");
            }
          });
        });
      });

      // Basic discount calculations
      function calcBasic() {
        const P0 = toNumber(document.getElementById("basic-original").value);
        const rMain =
          toNumber(document.getElementById("basic-discount").value) / 100;
        const rExtra =
          toNumber(document.getElementById("basic-extra-discount").value) / 100;
        const taxRate =
          toNumber(document.getElementById("basic-tax-rate").value) / 100;
        const taxMode =
          document.querySelector('input[name="basic-tax-mode"]:checked')
            ?.value || "after";

        if (P0 <= 0 || rMain < 0) {
          // basic validation: nothing to compute
          document.getElementById("basic-res-original").textContent =
            formatMoney(0);
          document.getElementById("basic-res-discount-amount").textContent =
            formatMoney(0);
          document.getElementById("basic-res-effective-rate").textContent =
            formatPercent(0);
          document.getElementById("basic-res-sale").textContent =
            formatMoney(0);
          document.getElementById("basic-res-tax").textContent = formatMoney(0);
          document.getElementById("basic-res-total").textContent =
            formatMoney(0);
          return;
        }

        const r1 = Math.max(0, rMain);
        const r2 = Math.max(0, rExtra);
        const saleBeforeTax = P0 * (1 - r1) * (1 - r2);
        const discountAmount = P0 - saleBeforeTax;
        const effectiveRate = (discountAmount / P0) * 100;

        let taxAmount = 0;
        if (taxRate > 0) {
          if (taxMode === "before") {
            const taxOnOriginal = P0 * taxRate;
            const taxOnSale = saleBeforeTax * taxRate;
            // In "before" mode we show tax on original and then discount; for simplicity show tax on sale
            taxAmount = taxOnSale;
          } else {
            taxAmount = saleBeforeTax * taxRate;
          }
        }
        const total = saleBeforeTax + taxAmount;

        document.getElementById("basic-res-original").textContent =
          formatMoney(P0);
        document.getElementById("basic-res-discount-amount").textContent =
          "-" + formatMoney(discountAmount).slice(1);
        document.getElementById("basic-res-effective-rate").textContent =
          formatPercent(effectiveRate);
        document.getElementById("basic-res-sale").textContent =
          formatMoney(saleBeforeTax);
        document.getElementById("basic-res-tax").textContent =
          formatMoney(taxAmount);
        document.getElementById("basic-res-total").textContent =
          formatMoney(total);
      }

      document
        .getElementById("basic-calc")
        .addEventListener("click", calcBasic);
      [
        "basic-original",
        "basic-discount",
        "basic-extra-discount",
        "basic-tax-rate",
      ].forEach((id) => {
        const el = document.getElementById(id);
        if (el)
          el.addEventListener("input", () => {
            /* live preview optional */
          });
      });
      document.querySelectorAll('input[name="basic-tax-mode"]').forEach((r) => {
        r.addEventListener("change", calcBasic);
      });
      document.getElementById("basic-reset").addEventListener("click", () => {
        [
          "basic-original",
          "basic-discount",
          "basic-extra-discount",
          "basic-tax-rate",
        ].forEach((id) => {
          const el = document.getElementById(id);
          if (el) el.value = "";
        });
        document.querySelector(
          'input[name="basic-tax-mode"][value="after"]'
        ).checked = true;
        calcBasic();
      });

      // Original price calculations
      function calcOriginal() {
        const Ps = toNumber(document.getElementById("orig-sale").value);
        const r =
          toNumber(document.getElementById("orig-discount").value) / 100;
        if (Ps <= 0 || r < 0 || r >= 1) {
          document.getElementById("orig-res-original").textContent =
            formatMoney(0);
          document.getElementById("orig-res-discount-amount").textContent =
            "-" + formatMoney(0).slice(1);
          return;
        }
        const P0 = Ps / (1 - r);
        const discountAmount = P0 - Ps;
        document.getElementById("orig-res-original").textContent =
          formatMoney(P0);
        document.getElementById("orig-res-discount-amount").textContent =
          "-" + formatMoney(discountAmount).slice(1);
      }
      document
        .getElementById("orig-calc")
        .addEventListener("click", calcOriginal);
      document.getElementById("orig-reset").addEventListener("click", () => {
        ["orig-sale", "orig-discount"].forEach((id) => {
          const el = document.getElementById(id);
          if (el) el.value = "";
        });
        calcOriginal();
      });

      // Percent off calculations
      function calcPercent() {
        const P0 = toNumber(document.getElementById("pct-original").value);
        const Ps = toNumber(document.getElementById("pct-sale").value);
        if (P0 <= 0 || Ps < 0 || Ps > P0) {
          document.getElementById("pct-res-discount-amount").textContent =
            "-" + formatMoney(0).slice(1);
          document.getElementById("pct-res-discount-rate").textContent =
            formatPercent(0);
          return;
        }
        const discountAmount = P0 - Ps;
        const r = (discountAmount / P0) * 100;
        document.getElementById("pct-res-discount-amount").textContent =
          "-" + formatMoney(discountAmount).slice(1);
        document.getElementById("pct-res-discount-rate").textContent =
          formatPercent(r);
      }
      document
        .getElementById("pct-calc")
        .addEventListener("click", calcPercent);
      document.getElementById("pct-reset").addEventListener("click", () => {
        ["pct-original", "pct-sale"].forEach((id) => {
          const el = document.getElementById(id);
          if (el) el.value = "";
        });
        calcPercent();
      });

      // Multi-discount calculations
      function calcMulti() {
        const P0 = toNumber(document.getElementById("multi-original").value);
        const r1 = toNumber(document.getElementById("multi-d1").value) / 100;
        const r2 = toNumber(document.getElementById("multi-d2").value) / 100;
        const r3 = toNumber(document.getElementById("multi-d3").value) / 100;

        if (P0 <= 0 || r1 < 0) {
          document.getElementById("multi-res-sale").textContent =
            formatMoney(0);
          document.getElementById("multi-res-discount-amount").textContent =
            "-" + formatMoney(0).slice(1);
          document.getElementById("multi-res-effective-rate").textContent =
            formatPercent(0);
          return;
        }

        const factors = [
          Math.max(0, 1 - r1),
          isNaN(r2) || r2 <= 0 ? 1 : Math.max(0, 1 - r2),
          isNaN(r3) || r3 <= 0 ? 1 : Math.max(0, 1 - r3),
        ];
        const sale = P0 * factors[0] * factors[1] * factors[2];
        const discountAmount = P0 - sale;
        const effectiveRate = (discountAmount / P0) * 100;

        document.getElementById("multi-res-sale").textContent =
          formatMoney(sale);
        document.getElementById("multi-res-discount-amount").textContent =
          "-" + formatMoney(discountAmount).slice(1);
        document.getElementById("multi-res-effective-rate").textContent =
          formatPercent(effectiveRate);
      }
      document
        .getElementById("multi-calc")
        .addEventListener("click", calcMulti);
      document.getElementById("multi-reset").addEventListener("click", () => {
        ["multi-original", "multi-d1", "multi-d2", "multi-d3"].forEach((id) => {
          const el = document.getElementById(id);
          if (el) el.value = "";
        });
        calcMulti();
      });

      // Initialize defaults
      calcBasic();
      calcOriginal();
      calcPercent();
      calcMulti();
    </script>

