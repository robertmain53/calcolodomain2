    <script src="/assets/js/mobile-menu.js"></script>
    <script src="/search.js"></script>

    <script>
      // Helper: format number with units
      function fmt(value, unit) {
        if (value === null || isNaN(value)) return "–";
        const abs = Math.abs(value);
        let digits = 2;
        if (abs >= 1000) digits = 1;
        if (abs >= 10000) digits = 0;
        return value.toFixed(digits) + (unit ? " " + unit : "");
      }

      function updateTrolleyVisibility() {
        const mode = document.getElementById("trolleyPosition").value;
        const wrap = document.getElementById("trolleyDistanceWrapper");
        if (mode === "user") {
          wrap.classList.remove("hidden");
        } else {
          wrap.classList.add("hidden");
        }
      }

      document
        .getElementById("trolleyPosition")
        .addEventListener("change", updateTrolleyVisibility);
      updateTrolleyVisibility();

      document.getElementById("resetCalc").addEventListener("click", () => {
        document.getElementById("span").value = 20;
        document.getElementById("craneCapacity").value = 10;
        document.getElementById("craneSelfWeight").value = 150;
        document.getElementById("trolleyPosition").value = "mid";
        document.getElementById("impactFactor").value = 1.15;
        document.getElementById("beamSelfWeight").value = 3.5;
        document.getElementById("extraUDL").value = 0.0;
        document.getElementById("Fy").value = 345;
        document.getElementById("E").value = 200;
        document.getElementById("Sx").value = 2500;
        document.getElementById("Ix").value = 800000;
        document.getElementById("Ltb").value = 4;
        document.getElementById("Cb").value = 1.0;
        document.getElementById("serviceLimit").value = 600;
        document.getElementById("calcStatus").textContent = "";
        const outputs = [
          "resLiftedLoad",
          "resTotalCraneLoad",
          "resWheelLoad",
          "resWheelLoadFactored",
          "resUDL",
          "resVmax",
          "resMmax",
          "resDefl",
          "resDeflRatio",
          "resSreq",
          "resSprov",
          "resFlexUtil",
          "resLTBUtil",
          "resDeflLimit",
          "resDeflOK",
          "resBendOK",
          "resStatus",
        ];
        outputs.forEach(
          (id) => (document.getElementById(id).textContent = "–")
        );
        updateTrolleyVisibility();
      });

      document.getElementById("runCalc").addEventListener("click", () => {
        const statusEl = document.getElementById("calcStatus");
        statusEl.textContent = "Calculating…";

        const L = parseFloat(document.getElementById("span").value); // m
        const capT = parseFloat(document.getElementById("craneCapacity").value); // tonne
        const Wcrane = parseFloat(
          document.getElementById("craneSelfWeight").value
        ); // kN
        const impact = parseFloat(
          document.getElementById("impactFactor").value
        );
        const wBeam = parseFloat(
          document.getElementById("beamSelfWeight").value
        ); // kN/m
        const wExtra = parseFloat(document.getElementById("extraUDL").value); // kN/m
        const Fy = parseFloat(document.getElementById("Fy").value); // MPa
        const E = parseFloat(document.getElementById("E").value) * 1e3; // GPa -> MPa
        const Sx_cm3 = parseFloat(document.getElementById("Sx").value); // cm^3
        const Ix_cm4 = parseFloat(document.getElementById("Ix").value); // cm^4
        const Lb = parseFloat(document.getElementById("Ltb").value); // m
        const Cb = parseFloat(document.getElementById("Cb").value);
        const deflLimitRatio = parseFloat(
          document.getElementById("serviceLimit").value
        );

        if (
          [
            L,
            capT,
            Wcrane,
            impact,
            wBeam,
            Fy,
            E,
            Sx_cm3,
            Ix_cm4,
            Lb,
            Cb,
            deflLimitRatio,
          ].some((v) => !isFinite(v) || v <= 0)
        ) {
          statusEl.textContent =
            "Please enter valid positive values for all required fields.";
          return;
        }

        // 1. Loads
        const Wlift = capT * 9.81; // kN
        const Wtotal = Wlift + Wcrane; // kN
        const Pw_avg = Wtotal / 4; // kN per wheel (4 wheels per side)
        const Pw_max = 1.1 * Pw_avg; // kN, approximate max wheel
        const Pw_design = Pw_max * impact; // kN
        const wTotal = wBeam + wExtra; // kN/m

        // 2. Internal forces (approximate)
        // Treat two design wheels as equivalent single load at midspan: P_eq ≈ 2 * Pw_design
        const P_eq = 2 * Pw_design; // kN

        const Vmax_UDL = (wTotal * L) / 2; // kN
        const Mmax_UDL = (wTotal * L * L) / 8; // kN·m

        const Vmax_P = P_eq / 2; // kN
        const Mmax_P = (P_eq * L) / 4; // kN·m

        const Vmax = Vmax_UDL + Vmax_P;
        const Mmax = Mmax_UDL + Mmax_P;

        // 3. Deflection (service)
        // Convert to N and mm: 1 kN = 1e3 N, 1 m = 1e3 mm
        const L_mm = L * 1000;
        const wTotal_N_per_mm = (wTotal * 1e3) / 1000; // kN/m -> N/mm
        const P_eq_N = P_eq * 1e3; // N
        const Ix_mm4 = Ix_cm4 * 1e4; // 1 cm^4 = 1e4 mm^4

        const delta_UDL =
          (5 * wTotal_N_per_mm * Math.pow(L_mm, 4)) / (384 * E * Ix_mm4); // mm
        const delta_P = (P_eq_N * Math.pow(L_mm, 3)) / (48 * E * Ix_mm4); // mm
        const delta_max = delta_UDL + delta_P;

        const deflRatio = L_mm / delta_max;

        // 4. Bending strength (elastic)
        // Mmax in N·mm
        const Mmax_Nmm = Mmax * 1e6; // kN·m -> N·mm
        const Sreq_mm3 = Mmax_Nmm / (0.66 * Fy); // mm^3
        const Sprov_mm3 = Sx_cm3 * 1e3; // 1 cm^3 = 1e3 mm^3
        const flexUtil = Sreq_mm3 / Sprov_mm3;

        // 5. Approximate LTB
        const Ix_m4 = Ix_mm4 * 1e-12; // mm^4 -> m^4
        const E_Pa = E * 1e6; // MPa -> Pa
        const Mn_LTB = (Cb * Math.PI * Math.PI * E_Pa * Ix_m4) / (Lb * Lb); // N·m
        const Mn_LTB_kNm = Mn_LTB / 1e3; // kN·m
        const LTBUtil = Mmax / Mn_LTB_kNm;

        // 6. Serviceability & status
        const deflOK = deflRatio >= deflLimitRatio;
        const bendOK = flexUtil <= 1.0 && LTBUtil <= 1.0;

        let statusText = "Check inputs";
        let statusColor = "text-gray-700";
        if (bendOK && deflOK) {
          statusText = "PASS (preliminary)";
          statusColor = "text-green-700";
        } else if (!bendOK && deflOK) {
          statusText = "FAIL – bending / LTB";
          statusColor = "text-red-700";
        } else if (bendOK && !deflOK) {
          statusText = "FAIL – deflection";
          statusColor = "text-red-700";
        } else {
          statusText = "FAIL – bending & deflection";
          statusColor = "text-red-700";
        }

        // Update DOM
        document.getElementById("resLiftedLoad").textContent = fmt(Wlift, "kN");
        document.getElementById("resTotalCraneLoad").textContent = fmt(
          Wtotal,
          "kN"
        );
        document.getElementById("resWheelLoad").textContent = fmt(Pw_max, "kN");
        document.getElementById("resWheelLoadFactored").textContent = fmt(
          Pw_design,
          "kN"
        );
        document.getElementById("resUDL").textContent = fmt(wTotal, "kN/m");

        document.getElementById("resVmax").textContent = fmt(Vmax, "kN");
        document.getElementById("resMmax").textContent = fmt(Mmax, "kN·m");
        document.getElementById("resDefl").textContent = fmt(delta_max, "mm");
        document.getElementById("resDeflRatio").textContent = isFinite(
          deflRatio
        )
          ? "L/" + deflRatio.toFixed(0)
          : "–";

        document.getElementById("resSreq").textContent = fmt(
          Sreq_mm3 / 1e3,
          "cm³"
        );
        document.getElementById("resSprov").textContent = fmt(Sx_cm3, "cm³");
        document.getElementById("resFlexUtil").textContent = isFinite(flexUtil)
          ? (flexUtil * 100).toFixed(1) + " %"
          : "–";
        document.getElementById("resLTBUtil").textContent = isFinite(LTBUtil)
          ? (LTBUtil * 100).toFixed(1) + " %"
          : "–";

        document.getElementById("resDeflLimit").textContent =
          "L/" + deflLimitRatio.toFixed(0);
        document.getElementById("resDeflOK").textContent = deflOK
          ? "OK"
          : "Exceeds limit";
        document.getElementById("resBendOK").textContent = bendOK
          ? "OK"
          : "Not OK";

        const statusElResult = document.getElementById("resStatus");
        statusElResult.textContent = statusText;
        statusElResult.className =
          "font-mono text-right font-semibold " + statusColor;

        statusEl.textContent = "Done (preliminary results only).";
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
