    <script>
      // Utility functions
      function toSeconds(h, m, s) {
        return (
          (Number(h) || 0) * 3600 + (Number(m) || 0) * 60 + (Number(s) || 0)
        );
      }

      function formatTime(seconds) {
        if (!isFinite(seconds) || seconds <= 0) return "–";
        const sign = seconds < 0 ? "-" : "";
        seconds = Math.abs(seconds);
        const h = Math.floor(seconds / 3600);
        const m = Math.floor((seconds % 3600) / 60);
        const s = Math.round(seconds % 60);
        const hh = h > 0 ? h + ":" : "";
        const mm = h > 0 ? String(m).padStart(2, "0") : m;
        const ss = String(s).padStart(2, "0");
        return sign + hh + mm + ":" + ss;
      }

      function formatPace(secondsPerKm, unit) {
        if (!isFinite(secondsPerKm) || secondsPerKm <= 0) return "–";
        if (unit === "mi") {
          const secPerMi = secondsPerKm * 1.609344;
          return formatTime(secPerMi) + " /mi";
        }
        return formatTime(secondsPerKm) + " /km";
      }

      function getDistanceKm(selectEl, customInput, unitSelect) {
        const val = selectEl.value;
        if (val !== "custom") return parseFloat(val);
        const d = parseFloat(customInput.value);
        if (!isFinite(d) || d <= 0) return NaN;
        return unitSelect.value === "mi" ? d * 1.609344 : d;
      }

      function computeAdjustments(
        baseSeconds,
        courseFactor,
        tempC,
        fitnessChange
      ) {
        let adjusted = baseSeconds;

        // Course factor: direct multiplier
        adjusted *= courseFactor;

        // Temperature adjustment: small slowdown above 18°C
        if (tempC > 18) {
          const extra = (tempC - 18) * 0.005; // 0.5% per °C above 18
          adjusted *= 1 + extra;
        }

        // Fitness change: -10% means 10% faster, +10% means 10% slower
        adjusted *= 1 - fitnessChange / 100;

        return adjusted;
      }

      function updateCourseLabel(val) {
        const label = document.getElementById("course-factor-label");
        if (!label) return;
        const f = parseFloat(val);
        let desc = "Flat / typical";
        if (f < 0.98) desc = "Very fast / downhill";
        else if (f < 1.0) desc = "Slightly fast";
        else if (f > 1.06) desc = "Very hilly / tough";
        else if (f > 1.02) desc = "Moderately hilly";
        label.textContent = desc + " (" + f.toFixed(2) + ")";
      }

      function updateTempLabel(val) {
        const label = document.getElementById("temperature-label");
        if (!label) return;
        const t = parseInt(val, 10);
        let desc = "Cool";
        if (t >= 10 && t <= 18) desc = "Ideal";
        else if (t > 18 && t <= 25) desc = "Warm";
        else if (t > 25) desc = "Hot";
        label.textContent = t + "°C (" + desc.toLowerCase() + ")";
      }

      function updateFitnessLabel(val) {
        const label = document.getElementById("fitness-change-label");
        if (!label) return;
        const f = parseInt(val, 10);
        if (f === 0) {
          label.textContent = "No change (0%)";
        } else if (f > 0) {
          label.textContent = "Fitter (+" + f + "%)";
        } else {
          label.textContent = "Detrained (" + f + "%)";
        }
      }

      function generateSplits(totalSeconds, distanceKm, mode) {
        const body = document.getElementById("splits-body");
        if (!body) return;
        body.innerHTML = "";

        if (
          !isFinite(totalSeconds) ||
          totalSeconds <= 0 ||
          !isFinite(distanceKm) ||
          distanceKm <= 0
        ) {
          body.innerHTML =
            '<tr><td class="px-3 py-2 text-gray-500" colspan="2">Run a prediction to see detailed splits.</td></tr>';
          return;
        }

        let step, unitLabel;
        if (mode === "mi") {
          const totalMi = distanceKm / 1.609344;
          step = 1; // 1 mile
          unitLabel = "mi";
          const splits = Math.floor(totalMi);
          if (splits === 0) {
            body.innerHTML =
              '<tr><td class="px-3 py-2 text-gray-500" colspan="2">Distance is less than 1 mile.</td></tr>';
            return;
          }
          const pacePerMi = totalSeconds / totalMi;
          for (let i = 1; i <= splits; i++) {
            const t = pacePerMi * i;
            const tr = document.createElement("tr");
            tr.innerHTML =
              '<td class="px-3 py-1.5 text-gray-700">' +
              i +
              " " +
              unitLabel +
              "</td>" +
              '<td class="px-3 py-1.5 text-gray-700">' +
              formatTime(t) +
              "</td>";
            body.appendChild(tr);
          }
        } else {
          step = 1; // 1 km
          unitLabel = "km";
          const splits = Math.floor(distanceKm);
          if (splits === 0) {
            body.innerHTML =
              '<tr><td class="px-3 py-2 text-gray-500" colspan="2">Distance is less than 1 km.</td></tr>';
            return;
          }
          const pacePerKm = totalSeconds / distanceKm;
          for (let i = 1; i <= splits; i++) {
            const t = pacePerKm * i;
            const tr = document.createElement("tr");
            tr.innerHTML =
              '<td class="px-3 py-1.5 text-gray-700">' +
              i +
              " " +
              unitLabel +
              "</td>" +
              '<td class="px-3 py-1.5 text-gray-700">' +
              formatTime(t) +
              "</td>";
            body.appendChild(tr);
          }
        }
      }

      document.addEventListener("DOMContentLoaded", function () {
        const recentDistanceSelect = document.getElementById("recent-distance");
        const recentDistanceCustom = document.getElementById(
          "recent-distance-custom"
        );
        const recentDistanceUnit = document.getElementById(
          "recent-distance-unit"
        );

        const targetDistanceSelect = document.getElementById("target-distance");
        const targetDistanceCustom = document.getElementById(
          "target-distance-custom"
        );
        const targetDistanceUnit = document.getElementById(
          "target-distance-unit"
        );

        const courseFactorInput = document.getElementById("course-factor");
        const tempInput = document.getElementById("temperature");
        const fitnessInput = document.getElementById("fitness-change");

        const predictBtn = document.getElementById("predict-btn");
        const errorMessage = document.getElementById("error-message");

        const modelTabs = document.querySelectorAll(".model-tab");
        const splitTabs = document.querySelectorAll(".split-tab");

        let lastPrediction = {
          combined: NaN,
          riegel: NaN,
          pace: NaN,
          distanceKm: NaN,
        };
        let currentModel = "combined";
        let currentSplitMode = "km";

        function toggleCustomDistance(selectEl, customInput) {
          if (selectEl.value === "custom") {
            customInput.disabled = false;
            customInput.classList.remove("bg-gray-100");
          } else {
            customInput.disabled = true;
            customInput.classList.remove("input-error");
            customInput.value = "";
          }
        }

        recentDistanceSelect.addEventListener("change", function () {
          toggleCustomDistance(recentDistanceSelect, recentDistanceCustom);
        });
        targetDistanceSelect.addEventListener("change", function () {
          toggleCustomDistance(targetDistanceSelect, targetDistanceCustom);
        });

        courseFactorInput.addEventListener("input", function () {
          updateCourseLabel(this.value);
        });
        tempInput.addEventListener("input", function () {
          updateTempLabel(this.value);
        });
        fitnessInput.addEventListener("input", function () {
          updateFitnessLabel(this.value);
        });

        modelTabs.forEach((tab) => {
          tab.addEventListener("click", function () {
            modelTabs.forEach((t) => t.classList.remove("tab-active"));
            this.classList.add("tab-active");
            currentModel = this.getAttribute("data-model");
            // Re-render summary based on lastPrediction
            if (isFinite(lastPrediction[currentModel])) {
              const time = lastPrediction[currentModel];
              const distanceKm = lastPrediction.distanceKm;
              const pacePerKm = time / distanceKm;
              document.getElementById("predicted-time").textContent =
                formatTime(time);
              document.getElementById("predicted-pace").textContent =
                "Average pace: " +
                formatPace(pacePerKm, "km") +
                " (" +
                formatPace(pacePerKm, "mi") +
                ")";
            }
          });
        });

        splitTabs.forEach((tab) => {
          tab.addEventListener("click", function () {
            splitTabs.forEach((t) => t.classList.remove("tab-active"));
            this.classList.add("tab-active");
            currentSplitMode = this.getAttribute("data-split");
            if (
              isFinite(lastPrediction.combined) &&
              isFinite(lastPrediction.distanceKm)
            ) {
              generateSplits(
                lastPrediction.combined,
                lastPrediction.distanceKm,
                currentSplitMode
              );
            }
          });
        });

        predictBtn.addEventListener("click", function () {
          errorMessage.classList.add("hidden");
          errorMessage.textContent = "";
          [recentDistanceCustom, targetDistanceCustom].forEach((el) =>
            el.classList.remove("input-error")
          );

          const recentDistKm = getDistanceKm(
            recentDistanceSelect,
            recentDistanceCustom,
            recentDistanceUnit
          );
          const targetDistKm = getDistanceKm(
            targetDistanceSelect,
            targetDistanceCustom,
            targetDistanceUnit
          );

          const recentTimeSec = toSeconds(
            document.getElementById("recent-hours").value,
            document.getElementById("recent-minutes").value,
            document.getElementById("recent-seconds").value
          );

          let hasError = false;
          if (!isFinite(recentDistKm) || recentDistKm <= 0) {
            hasError = true;
            if (recentDistanceSelect.value === "custom") {
              recentDistanceCustom.classList.add("input-error");
            }
          }
          if (!isFinite(targetDistKm) || targetDistKm <= 0) {
            hasError = true;
            if (targetDistanceSelect.value === "custom") {
              targetDistanceCustom.classList.add("input-error");
            }
          }
          if (!isFinite(recentTimeSec) || recentTimeSec <= 0) {
            hasError = true;
          }

          if (hasError) {
            errorMessage.textContent =
              "Please enter a valid recent race distance, time, and target distance.";
            errorMessage.classList.remove("hidden");
            return;
          }

          // Riegel model
          const k = 1.06;
          const riegelTime =
            recentTimeSec * Math.pow(targetDistKm / recentDistKm, k);

          // Pace-based model
          const pace1 = recentTimeSec / recentDistKm;
          const c = 0.04; // gentler exponent
          const pace2 = pace1 * Math.pow(targetDistKm / recentDistKm, c);
          const paceTime = pace2 * targetDistKm;

          // Combined (simple average)
          let combinedTime = (riegelTime + paceTime) / 2;

          // Adjustments
          const courseFactor = parseFloat(courseFactorInput.value);
          const tempC = parseFloat(tempInput.value);
          const fitnessChange = parseFloat(fitnessInput.value);

          const adjustedCombined = computeAdjustments(
            combinedTime,
            courseFactor,
            tempC,
            fitnessChange
          );
          const adjustedRiegel = computeAdjustments(
            riegelTime,
            courseFactor,
            tempC,
            fitnessChange
          );
          const adjustedPace = computeAdjustments(
            paceTime,
            courseFactor,
            tempC,
            fitnessChange
          );

          lastPrediction = {
            combined: adjustedCombined,
            riegel: adjustedRiegel,
            pace: adjustedPace,
            distanceKm: targetDistKm,
          };

          // Update detail cards
          document.getElementById("riegel-time").textContent =
            formatTime(adjustedRiegel);
          document.getElementById("riegel-pace").textContent =
            "Pace: " +
            formatPace(adjustedRiegel / targetDistKm, "km") +
            " (" +
            formatPace(adjustedRiegel / targetDistKm, "mi") +
            ")";

          document.getElementById("pace-time").textContent =
            formatTime(adjustedPace);
          document.getElementById("pace-pace").textContent =
            "Pace: " +
            formatPace(adjustedPace / targetDistKm, "km") +
            " (" +
            formatPace(adjustedPace / targetDistKm, "mi") +
            ")";

          // Summary (respect current model tab)
          const modelKey = currentModel || "combined";
          const summaryTime = lastPrediction[modelKey];
          const pacePerKm = summaryTime / targetDistKm;

          document.getElementById("predicted-time").textContent =
            formatTime(summaryTime);
          document.getElementById("predicted-pace").textContent =
            "Average pace: " +
            formatPace(pacePerKm, "km") +
            " (" +
            formatPace(pacePerKm, "mi") +
            ")";

          // Likely range: ±2–4% depending on adjustments
          let rangePct = 0.02;
          if (courseFactor > 1.03 || tempC > 22 || Math.abs(fitnessChange) > 5)
            rangePct = 0.03;
          if (courseFactor > 1.06 || tempC > 28 || Math.abs(fitnessChange) > 10)
            rangePct = 0.04;
          const low = summaryTime * (1 - rangePct);
          const high = summaryTime * (1 + rangePct);
          document.getElementById("predicted-range").textContent =
            "Likely range: " + formatTime(low) + " – " + formatTime(high);

          // Splits
          generateSplits(adjustedCombined, targetDistKm, currentSplitMode);
        });

        // Initialize labels
        updateCourseLabel(courseFactorInput.value);
        updateTempLabel(tempInput.value);
        updateFitnessLabel(fitnessInput.value);
      });
    </script>

