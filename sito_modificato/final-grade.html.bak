    <script>
      // Mode toggle
      const modeSimpleBtn = document.getElementById("mode-simple");
      const modeAdvancedBtn = document.getElementById("mode-advanced");
      const simplePanel = document.getElementById("simple-panel");
      const advancedPanel = document.getElementById("advanced-panel");

      function setMode(mode) {
        if (mode === "simple") {
          simplePanel.classList.remove("hidden");
          advancedPanel.classList.add("hidden");
          modeSimpleBtn.classList.add("bg-blue-600", "text-white");
          modeSimpleBtn.classList.remove("bg-white", "text-gray-700");
          modeAdvancedBtn.classList.remove("bg-blue-600", "text-white");
          modeAdvancedBtn.classList.add("bg-white", "text-gray-700");
        } else {
          simplePanel.classList.add("hidden");
          advancedPanel.classList.remove("hidden");
          modeAdvancedBtn.classList.add("bg-blue-600", "text-white");
          modeAdvancedBtn.classList.remove("bg-white", "text-gray-700");
          modeSimpleBtn.classList.remove("bg-blue-600", "text-white");
          modeSimpleBtn.classList.add("bg-white", "text-gray-700");
        }
        clearValidation();
      }

      modeSimpleBtn.addEventListener("click", () => setMode("simple"));
      modeAdvancedBtn.addEventListener("click", () => setMode("advanced"));

      // Category rows (advanced mode)
      const categoryTbody = document.getElementById("category-rows");
      const addCategoryBtn = document.getElementById("add-category");
      const totalWeightSpan = document.getElementById("total-weight");
      const advCurrentDisplay = document.getElementById("adv-current-display");

      let categoryIdCounter = 0;

      function createCategoryRow(name = "", weight = "", avg = "") {
        const tr = document.createElement("tr");
        tr.dataset.id = ++categoryIdCounter;
        tr.className = "border-t border-gray-100";

        tr.innerHTML = `
      <td class="px-3 py-2">
        <input type="text" class="w-full rounded-md border-gray-300 shadow-sm text-sm px-2 py-1 category-name" placeholder="e.g. Homework" value="${name}">
      </td>
      <td class="px-3 py-2">
        <input type="number" min="0" max="100" step="0.01" class="w-full rounded-md border-gray-300 shadow-sm text-sm px-2 py-1 category-weight" value="${weight}">
      </td>
      <td class="px-3 py-2">
        <input type="number" min="0" max="150" step="0.01" class="w-full rounded-md border-gray-300 shadow-sm text-sm px-2 py-1 category-avg" value="${avg}">
      </td>
      <td class="px-3 py-2 text-center">
        <input type="checkbox" class="category-include" checked aria-label="Include this category in current grade">
      </td>
      <td class="px-3 py-2 text-right">
        <button type="button" class="text-xs text-red-600 hover:underline remove-category">Remove</button>
      </td>
    `;
        categoryTbody.appendChild(tr);
      }

      // Initial example rows
      createCategoryRow("Homework", "20", "");
      createCategoryRow("Quizzes", "20", "");
      createCategoryRow("Midterm", "30", "");

      addCategoryBtn.addEventListener("click", () => createCategoryRow());

      categoryTbody.addEventListener("click", (e) => {
        if (e.target.classList.contains("remove-category")) {
          const row = e.target.closest("tr");
          row.remove();
          updateAdvancedCurrent();
        }
      });

      categoryTbody.addEventListener("input", (e) => {
        if (
          e.target.classList.contains("category-weight") ||
          e.target.classList.contains("category-avg") ||
          e.target.classList.contains("category-include")
        ) {
          updateAdvancedCurrent();
        }
      });

      document
        .getElementById("adv-final-weight")
        .addEventListener("input", updateAdvancedCurrent);

      function updateAdvancedCurrent() {
        const rows = categoryTbody.querySelectorAll("tr");
        let totalWeight = 0;
        let weightedSum = 0;

        rows.forEach((row) => {
          const include = row.querySelector(".category-include").checked;
          const w = parseFloat(row.querySelector(".category-weight").value);
          const avg = parseFloat(row.querySelector(".category-avg").value);
          if (!include || isNaN(w) || isNaN(avg)) return;
          totalWeight += w;
          weightedSum += w * avg;
        });

        const finalWeight = parseFloat(
          document.getElementById("adv-final-weight").value
        );
        const totalWithFinal =
          totalWeight + (isNaN(finalWeight) ? 0 : finalWeight);
        totalWeightSpan.textContent = isNaN(totalWithFinal)
          ? "0"
          : totalWithFinal.toFixed(2);

        if (totalWeight > 0) {
          const current = weightedSum / totalWeight;
          advCurrentDisplay.textContent = current.toFixed(2);
        } else {
          advCurrentDisplay.textContent = "–";
        }
      }

      // Letter grade thresholds
      const threshA = document.getElementById("thresh-a");
      const threshB = document.getElementById("thresh-b");
      const threshC = document.getElementById("thresh-c");
      const threshD = document.getElementById("thresh-d");
      const resetThresholdsBtn = document.getElementById("reset-thresholds");

      function resetThresholds() {
        threshA.value = 90;
        threshB.value = 80;
        threshC.value = 70;
        threshD.value = 60;
      }
      resetThresholdsBtn.addEventListener("click", resetThresholds);

      function getLetterGrade(pct) {
        const a = parseFloat(threshA.value) || 90;
        const b = parseFloat(threshB.value) || 80;
        const c = parseFloat(threshC.value) || 70;
        const d = parseFloat(threshD.value) || 60;
        if (pct >= a) return "A";
        if (pct >= b) return "B";
        if (pct >= c) return "C";
        if (pct >= d) return "D";
        return "F";
      }

      // Calculation
      const calculateBtn = document.getElementById("calculate-btn");
      const clearBtn = document.getElementById("clear-btn");
      const requiredFinalSpan = document.getElementById("required-final");
      const requiredFinalNote = document.getElementById("required-final-note");
      const projectedFinalSpan = document.getElementById("projected-final");
      const projectedLetterSpan = document.getElementById("projected-letter");
      const validationMessage = document.getElementById("validation-message");

      function clearValidation() {
        validationMessage.classList.add("hidden");
        validationMessage.textContent = "";
        document
          .querySelectorAll(".input-error")
          .forEach((el) => el.classList.remove("input-error"));
      }

      function showValidation(msg, elements = []) {
        validationMessage.textContent = msg;
        validationMessage.classList.remove("hidden");
        elements.forEach((el) => el && el.classList.add("input-error"));
      }

      function calculate() {
        clearValidation();
        let currentGrade, finalWeightPct, targetGrade;

        if (!advancedPanel.classList.contains("hidden")) {
          // Advanced mode
          const currentText = advCurrentDisplay.textContent;
          if (currentText === "–") {
            showValidation(
              "Please enter at least one category with weight and average to compute your current grade.",
              []
            );
            return;
          }
          currentGrade = parseFloat(currentText);
          finalWeightPct = parseFloat(
            document.getElementById("adv-final-weight").value
          );
          targetGrade = parseFloat(document.getElementById("adv-target").value);

          const totalWeight = parseFloat(totalWeightSpan.textContent);
          if (!isNaN(totalWeight) && Math.abs(totalWeight - 100) > 0.5) {
            showValidation(
              "Warning: category weights plus final exam weight do not add up to 100%. Please check your syllabus.",
              []
            );
          }
        } else {
          // Simple mode
          const simpleCurrent = document.getElementById("simple-current");
          const simpleFinalWeight = document.getElementById(
            "simple-final-weight"
          );
          const simpleTarget = document.getElementById("simple-target");

          currentGrade = parseFloat(simpleCurrent.value);
          finalWeightPct = parseFloat(simpleFinalWeight.value);
          targetGrade = parseFloat(simpleTarget.value);

          const invalidEls = [];
          if (isNaN(currentGrade)) invalidEls.push(simpleCurrent);
          if (isNaN(finalWeightPct)) invalidEls.push(simpleFinalWeight);
          if (isNaN(targetGrade)) invalidEls.push(simpleTarget);

          if (invalidEls.length) {
            showValidation(
              "Please fill in current grade, final exam weight, and desired final grade.",
              invalidEls
            );
            return;
          }
        }

        if (
          isNaN(finalWeightPct) ||
          finalWeightPct <= 0 ||
          finalWeightPct > 100
        ) {
          const el = advancedPanel.classList.contains("hidden")
            ? document.getElementById("simple-final-weight")
            : document.getElementById("adv-final-weight");
          showValidation("Final exam weight must be between 0 and 100%.", [el]);
          return;
        }

        const wf = finalWeightPct / 100;
        const C = currentGrade;
        const T = targetGrade;

        if (isNaN(C) || isNaN(T)) {
          showValidation("Please provide valid numeric values for grades.", []);
          return;
        }

        // Required final score
        const requiredFinal = (T - C * (1 - wf)) / wf;
        requiredFinalSpan.textContent = isFinite(requiredFinal)
          ? requiredFinal.toFixed(2)
          : "–";

        if (requiredFinal > 100) {
          requiredFinalNote.textContent =
            "You would need more than 100% on the final to reach this target grade.";
          requiredFinalNote.classList.remove("text-gray-500");
          requiredFinalNote.classList.add("text-red-600");
        } else if (requiredFinal < 0) {
          requiredFinalNote.textContent =
            "You have already secured at least this final grade, even with 0% on the final.";
          requiredFinalNote.classList.remove("text-gray-500");
          requiredFinalNote.classList.add("text-emerald-600");
        } else {
          requiredFinalNote.textContent =
            "This is the minimum score you need on the final exam to reach your target.";
          requiredFinalNote.classList.remove(
            "text-red-600",
            "text-emerald-600"
          );
          requiredFinalNote.classList.add("text-gray-500");
        }

        // Projected final grade if you hit the required score (clamped 0–100)
        const F = Math.min(Math.max(requiredFinal, 0), 150);
        const projected = C * (1 - wf) + F * wf;
        projectedFinalSpan.textContent = projected.toFixed(2);
        projectedLetterSpan.textContent = getLetterGrade(projected);
      }

      calculateBtn.addEventListener("click", calculate);

      function clearAll() {
        clearValidation();
        // Simple
        document.getElementById("simple-current").value = "";
        document.getElementById("simple-final-weight").value = "";
        document.getElementById("simple-target").value = "";
        // Advanced
        categoryTbody.innerHTML = "";
        createCategoryRow("Homework", "20", "");
        createCategoryRow("Quizzes", "20", "");
        createCategoryRow("Midterm", "30", "");
        document.getElementById("adv-final-weight").value = "";
        document.getElementById("adv-target").value = "";
        advCurrentDisplay.textContent = "–";
        totalWeightSpan.textContent = "0";
        // Results
        requiredFinalSpan.textContent = "–";
        requiredFinalNote.textContent = "Enter your data and click Calculate.";
        requiredFinalNote.classList.remove("text-red-600", "text-emerald-600");
        requiredFinalNote.classList.add("text-gray-500");
        projectedFinalSpan.textContent = "–";
        projectedLetterSpan.textContent = "–";
      }

      clearBtn.addEventListener("click", clearAll);

      // Initialize
      updateAdvancedCurrent();
      resetThresholds();
    </script>

