    <script>
      // Core dataset: typical modern dress size conversions with measurement ranges
      const SIZE_DATA = [
        {
          int: "XS",
          us: 0,
          uk: 4,
          eu: 32,
          au: 4,
          bust: [78, 81],
          waist: [60, 63],
          hips: [84, 87],
        },
        {
          int: "XS",
          us: 2,
          uk: 6,
          eu: 34,
          au: 6,
          bust: [82, 85],
          waist: [64, 67],
          hips: [88, 91],
        },
        {
          int: "S",
          us: 4,
          uk: 8,
          eu: 36,
          au: 8,
          bust: [86, 89],
          waist: [68, 71],
          hips: [92, 95],
        },
        {
          int: "S",
          us: 6,
          uk: 10,
          eu: 38,
          au: 10,
          bust: [90, 93],
          waist: [72, 75],
          hips: [96, 99],
        },
        {
          int: "M",
          us: 8,
          uk: 12,
          eu: 40,
          au: 12,
          bust: [94, 97],
          waist: [76, 79],
          hips: [100, 103],
        },
        {
          int: "M",
          us: 10,
          uk: 14,
          eu: 42,
          au: 14,
          bust: [98, 101],
          waist: [80, 83],
          hips: [104, 107],
        },
        {
          int: "L",
          us: 12,
          uk: 16,
          eu: 44,
          au: 16,
          bust: [102, 106],
          waist: [84, 88],
          hips: [108, 112],
        },
        {
          int: "L",
          us: 14,
          uk: 18,
          eu: 46,
          au: 18,
          bust: [107, 111],
          waist: [89, 93],
          hips: [113, 117],
        },
        {
          int: "XL",
          us: 16,
          uk: 20,
          eu: 48,
          au: 20,
          bust: [112, 116],
          waist: [94, 99],
          hips: [118, 123],
        },
        {
          int: "XL",
          us: 18,
          uk: 22,
          eu: 50,
          au: 22,
          bust: [117, 121],
          waist: [100, 105],
          hips: [124, 129],
        },
        {
          int: "XXL",
          us: 20,
          uk: 24,
          eu: 52,
          au: 24,
          bust: [122, 127],
          waist: [106, 111],
          hips: [130, 135],
        },
        {
          int: "XXL",
          us: 22,
          uk: 26,
          eu: 54,
          au: 26,
          bust: [128, 133],
          waist: [112, 117],
          hips: [136, 141],
        },
      ];

      const inputSystemEl = document.getElementById("input-system");
      const inputSizeEl = document.getElementById("input-size");
      const sizePillsEl = document.getElementById("size-pills");
      const measurementBodyEl = document.getElementById("measurement-body");
      const rangeLabelEl = document.getElementById("range-label");
      const referenceTableBodyEl = document.getElementById(
        "reference-table-body"
      );
      const fitButtons = document.querySelectorAll(".fit-btn");

      let currentFit = "regular";

      function cmToIn(cm) {
        return cm / 2.54;
      }

      function formatRange(rangeCm) {
        const [minCm, maxCm] = rangeCm;
        const minIn = cmToIn(minCm);
        const maxIn = cmToIn(maxCm);
        return {
          cm: `${minCm.toFixed(0)}–${maxCm.toFixed(0)} cm`,
          inch: `${minIn.toFixed(1)}–${maxIn.toFixed(1)} in`,
        };
      }

      function populateSizeOptions() {
        const system = inputSystemEl.value;
        const values = Array.from(
          new Set(SIZE_DATA.map((row) => row[system.toLowerCase()]))
        );
        inputSizeEl.innerHTML = "";
        values.forEach((v) => {
          const opt = document.createElement("option");
          opt.value = v;
          opt.textContent = system === "INT" ? v : v.toString();
          inputSizeEl.appendChild(opt);
        });
      }

      function findBestRow() {
        const system = inputSystemEl.value;
        const sizeValue =
          system === "INT"
            ? inputSizeEl.value
            : parseInt(inputSizeEl.value, 10);

        // Filter rows that match the selected system size
        const candidates = SIZE_DATA.filter(
          (row) => row[system.toLowerCase()] === sizeValue
        );
        if (candidates.length === 0) return null;

        // For multiple rows (e.g., same INT across two numeric sizes), pick based on fit
        if (candidates.length === 1) return candidates[0];

        if (currentFit === "snug") {
          // choose row with smaller bust upper bound
          return candidates.reduce((a, b) => (a.bust[1] <= b.bust[1] ? a : b));
        } else if (currentFit === "relaxed") {
          // choose row with larger bust upper bound
          return candidates.reduce((a, b) => (a.bust[1] >= b.bust[1] ? a : b));
        }
        // regular: choose middle or first
        return candidates[Math.floor(candidates.length / 2)];
      }

      function updatePills(row) {
        sizePillsEl.innerHTML = "";

        const systems = [
          { key: "int", label: "INT" },
          { key: "us", label: "US" },
          { key: "uk", label: "UK" },
          { key: "eu", label: "EU" },
          { key: "au", label: "AU/NZ" },
        ];

        systems.forEach((sys) => {
          const pill = document.createElement("button");
          pill.type = "button";
          pill.className =
            "size-pill " +
            (inputSystemEl.value.toLowerCase() === sys.key
              ? "size-pill-active"
              : "size-pill-muted");
          pill.textContent = `${sys.label}: ${row[sys.key]}`;
          pill.addEventListener("click", () => {
            // clicking a pill switches system and size
            inputSystemEl.value =
              sys.label === "INT" ? "INT" : sys.label.split("/")[0];
            populateSizeOptions();
            inputSizeEl.value = row[sys.key];
            updateAll();
          });
          sizePillsEl.appendChild(pill);
        });
      }

      function updateMeasurements(row) {
        measurementBodyEl.innerHTML = "";

        const bustRange = formatRange(row.bust);
        const waistRange = formatRange(row.waist);
        const hipsRange = formatRange(row.hips);

        rangeLabelEl.textContent = `Approx. for ${row.int} / US ${row.us} / UK ${row.uk} / EU ${row.eu}`;

        const rows = [
          { label: "Bust", range: bustRange },
          { label: "Waist", range: waistRange },
          { label: "Hips", range: hipsRange },
        ];

        rows.forEach((r) => {
          const tr = document.createElement("tr");
          tr.className = "border-b border-gray-100";
          tr.innerHTML = `
        <td class="px-4 py-2 font-medium text-gray-700">${r.label}</td>
        <td class="px-4 py-2 text-gray-700">${r.range.cm}</td>
        <td class="px-4 py-2 text-gray-700">${r.range.inch}</td>
      `;
          measurementBodyEl.appendChild(tr);
        });
      }

      function populateReferenceTable() {
        referenceTableBodyEl.innerHTML = "";
        SIZE_DATA.forEach((row) => {
          const bust = `${row.bust[0]}–${row.bust[1]}`;
          const waist = `${row.waist[0]}–${row.waist[1]}`;
          const hips = `${row.hips[0]}–${row.hips[1]}`;
          const tr = document.createElement("tr");
          tr.className = "border-b border-gray-100";
          tr.innerHTML = `
        <td class="px-3 py-2 text-gray-700">${row.int}</td>
        <td class="px-3 py-2 text-gray-700">${row.us}</td>
        <td class="px-3 py-2 text-gray-700">${row.uk}</td>
        <td class="px-3 py-2 text-gray-700">${row.eu}</td>
        <td class="px-3 py-2 text-gray-700">${row.au}</td>
        <td class="px-3 py-2 text-gray-700">${bust}</td>
        <td class="px-3 py-2 text-gray-700">${waist}</td>
        <td class="px-3 py-2 text-gray-700">${hips}</td>
      `;
          referenceTableBodyEl.appendChild(tr);
        });
      }

      function updateAll() {
        const row = findBestRow();
        if (!row) return;
        updatePills(row);
        updateMeasurements(row);
      }

      // Event listeners
      inputSystemEl.addEventListener("change", () => {
        populateSizeOptions();
        updateAll();
      });

      inputSizeEl.addEventListener("change", updateAll);

      fitButtons.forEach((btn) => {
        btn.addEventListener("click", () => {
          currentFit = btn.dataset.fit;
          fitButtons.forEach((b) => {
            if (b === btn) {
              b.classList.add("border-blue-500", "bg-blue-50", "text-blue-700");
              b.classList.remove("border-gray-300", "hover:bg-gray-50");
            } else {
              b.classList.remove(
                "border-blue-500",
                "bg-blue-50",
                "text-blue-700"
              );
              b.classList.add("border-gray-300");
            }
          });
          updateAll();
        });
      });

      // Initialize
      populateSizeOptions();
      populateReferenceTable();
      updateAll();
    </script>

