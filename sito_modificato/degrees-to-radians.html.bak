            <script>
              document.addEventListener('DOMContentLoaded', () => {
                const degInput = document.getElementById('degrees-input');
                const radInput = document.getElementById('radians-input');
                const piNumInput = document.getElementById('pi-input-num');
                const piDenInput = document.getElementById('pi-input-den');
                const stepsBox = document.getElementById('calculation-steps');

                // Funzione per calcolare il Massimo Comun Divisore
                function gcd(a, b) {
                    a = Math.abs(a);
                    b = Math.abs(b);
                    if (b > a) { let temp = a; a = b; b = temp; }
                    while (true) {
                        if (b === 0) return a;
                        a %= b;
                        if (a === 0) return b;
                        b %= a;
                    }
                }

                function formatNum(num, precision = 5) {
                    if (isNaN(num) || num === 0) return '';
                    return parseFloat(num.toFixed(precision)).toString();
                }

                function updateSteps(deg, num, den, rad) {
                    let stepsHtml = '<h3 class="text-lg font-semibold text-gray-800 mb-2">Calculation Steps:</h3>';
                    stepsHtml += '<p><strong>1. Formula:</strong> <code>rad = deg × (π / 180)</code></p>';
                    stepsHtml += `<p><strong>2. Fraction:</strong> <code>${deg}° = ${deg}π / 180</code></p>`;
                    if (num !== deg) {
                         stepsHtml += `<p><strong>3. Simplify:</strong> <code>${deg}π / 180 = ${num}π / ${den}</code></p>`;
                    }
                    stepsHtml += `<p><strong>4. Decimal:</strong> <code>${num}π / ${den} ≈ ${formatNum(rad)}</code></p>`;
                    stepsBox.innerHTML = stepsHtml;
                }
                
                function updateFromDegrees(deg) {
                    if (isNaN(deg)) {
                        radInput.value = '';
                        piNumInput.value = '';
                        piDenInput.value = '';
                        stepsBox.innerHTML = '<h3 class="text-lg font-semibold text-gray-800 mb-2">Calculation Steps:</h3><p class="text-gray-600">Enter a value to see the step-by-step conversion.</p>';
                        return;
                    }
                    
                    const rad = deg * (Math.PI / 180);
                    radInput.value = formatNum(rad);

                    // Semplifica la frazione
                    const denominator = 180;
                    const common = gcd(deg, denominator);
                    const num = deg / common;
                    const den = denominator / common;

                    piNumInput.value = num === 1 ? '' : (num === -1 ? '-' : num); // Mostra 'π/2' invece di '1π/2'
                    if (num === 0) piNumInput.value = 0;
                    piDenInput.value = den === 1 ? '' : den; // Mostra '2π' invece di '2π/1'
                    
                    updateSteps(deg, num, den, rad);
                }

                function updateFromRadians(rad) {
                    if (isNaN(rad)) {
                        degInput.value = '';
                        piNumInput.value = '';
                        piDenInput.value = '';
                        stepsBox.innerHTML = '<h3 class="text-lg font-semibold text-gray-800 mb-2">Calculation Steps:</h3><p class="text-gray-600">Enter a value to see the step-by-step conversion.</p>';
                        return;
                    }
                    
                    const deg = rad * (180 / Math.PI);
                    degInput.value = formatNum(deg);
                    
                    // Converte il decimale in una frazione di pi
                    const piFraction = rad / Math.PI;
                    piNumInput.value = formatNum(piFraction);
                    piDenInput.value = ''; // Lascia vuoto per i decimali

                    stepsBox.innerHTML = `
                        <h3 class="text-lg font-semibold text-gray-800 mb-2">Calculation Steps:</h3>
                        <p><strong>1. Formula:</strong> <code>deg = rad × (180 / π)</code></p>
                        <p><strong>2. Calculation:</strong> <code>${formatNum(rad)} × (180 / π) = <strong>${formatNum(deg)}°</strong></code></p>
                        <p><strong>3. As Pi:</strong> <code>${formatNum(rad)} ≈ ${formatNum(piFraction)}π</code></p>
                    `;
                }
                
                function updateFromPi() {
                    const num = parseFloat(piNumInput.value || 1);
                    const den = parseFloat(piDenInput.value || 1);
                    
                    if (isNaN(num) || isNaN(den) || den === 0) {
                        degInput.value = '';
                        radInput.value = '';
                        stepsBox.innerHTML = '<h3 class="text-lg font-semibold text-gray-800 mb-2">Calculation Steps:</h3><p class="text-gray-600">Enter a value to see the step-by-step conversion.</p>';
                        return;
                    }
                    
                    const rad = (num / den) * Math.PI;
                    const deg = rad * (180 / Math.PI);
                    
                    degInput.value = formatNum(deg);
                    radInput.value = formatNum(rad);
                    
                    stepsBox.innerHTML = `
                        <h3 class="text-lg font-semibold text-gray-800 mb-2">Calculation Steps:</h3>
                        <p><strong>1. Radians (decimal):</strong> <code>(${num}/${den}) × π ≈ <strong>${formatNum(rad)} rad</strong></code></p>
                        <p><strong>2. Convert to Degrees:</strong> <code>${formatNum(rad)} rad × (180 / π) = <strong>${formatNum(deg)}°</strong></code></p>
                    `;
                }

                // Aggiungi Event Listeners
                degInput.addEventListener('input', (e) => updateFromDegrees(parseFloat(e.target.value)));
                radInput.addEventListener('input', (e) => updateFromRadians(parseFloat(e.target.value)));
                piNumInput.addEventListener('input', updateFromPi);
                piDenInput.addEventListener('input', updateFromPi);

              });
            </script>
            
