    <script>
      // Percent error calculator logic
      (function () {
        const trueInput = document.getElementById("true-value");
        const observedInput = document.getElementById("observed-value");
        const contextInput = document.getElementById("measurement-context");
        const btnCalc = document.getElementById("btn-calc");
        const btnClear = document.getElementById("btn-clear");
        const errorMessage = document.getElementById("calc-error-message");

        const resultsContainer = document.getElementById("results-container");
        const stepsContainer = document.getElementById("steps-container");

        const mainPercentErrorEl =
          document.getElementById("main-percent-error");
        const mainPercentLabelEl =
          document.getElementById("main-percent-label");
        const absErrorEl = document.getElementById("abs-error");
        const relErrorEl = document.getElementById("rel-error");
        const absPercentErrorEl = document.getElementById("abs-percent-error");
        const signedPercentErrorEl = document.getElementById(
          "signed-percent-error"
        );

        const stepTrue = document.getElementById("step-true-value");
        const stepObs = document.getElementById("step-observed-value");
        const stepAbsError = document.getElementById("step-abs-error");
        const stepRelError = document.getElementById("step-rel-error");
        const stepPercentError = document.getElementById("step-percent-error");

        function parseNumber(value) {
          if (!value) return null;
          const normalized = value.replace(/\s+/g, "").replace(",", ".");
          const num = Number(normalized);
          return Number.isFinite(num) ? num : null;
        }

        function formatNumber(value) {
          if (!Number.isFinite(value)) return "–";
          // Use up to 6 significant digits, but show at least 2 decimals for small values
          const abs = Math.abs(value);
          if (abs === 0) return "0";
          if (abs >= 1000 || abs < 0.001) {
            return value.toExponential(3);
          }
          return value.toFixed(4).replace(/\.?0+$/, "");
        }

        function getErrorMode() {
          const radios = document.querySelectorAll('input[name="error-mode"]');
          for (const r of radios) {
            if (r.checked) return r.value;
          }
          return "absolute";
        }

        btnCalc.addEventListener("click", function () {
          const vTrue = parseNumber(trueInput.value);
          const vObs = parseNumber(observedInput.value);

          errorMessage.classList.add("hidden");
          errorMessage.textContent = "";

          if (vTrue === null || vObs === null) {
            errorMessage.textContent =
              "Please enter numeric values for both the true value and the observed value.";
            errorMessage.classList.remove("hidden");
            resultsContainer.classList.add("hidden");
            stepsContainer.classList.add("hidden");
            return;
          }

          if (vTrue === 0) {
            errorMessage.textContent =
              "Percent error is not defined when the true value is zero. Use absolute error instead.";
            errorMessage.classList.remove("hidden");
            resultsContainer.classList.add("hidden");
            stepsContainer.classList.add("hidden");
            return;
          }

          const absError = vObs - vTrue;
          const absErrorMag = Math.abs(absError);
          const relError = absErrorMag / Math.abs(vTrue);
          const absPercentError = relError * 100;
          const signedPercentError = (absError / vTrue) * 100;

          const mode = getErrorMode();
          const mainPercent =
            mode === "absolute" ? absPercentError : signedPercentError;

          // Populate result widgets
          mainPercentErrorEl.textContent = formatNumber(mainPercent);
          mainPercentLabelEl.textContent =
            mode === "absolute"
              ? "Absolute percent error"
              : "Signed percent error (negative = lower than true value)";

          absErrorEl.textContent = formatNumber(absError);
          relErrorEl.textContent = formatNumber(relError);
          absPercentErrorEl.textContent = formatNumber(absPercentError);
          signedPercentErrorEl.textContent = formatNumber(signedPercentError);

          // Steps
          stepTrue.textContent = formatNumber(vTrue);
          stepObs.textContent = formatNumber(vObs);
          stepAbsError.textContent = formatNumber(absErrorMag);
          stepRelError.textContent = formatNumber(relError);
          stepPercentError.textContent = formatNumber(absPercentError);

          resultsContainer.classList.remove("hidden");
          stepsContainer.classList.remove("hidden");
        });

        btnClear.addEventListener("click", function () {
          trueInput.value = "";
          observedInput.value = "";
          contextInput.value = "";
          errorMessage.textContent = "";
          errorMessage.classList.add("hidden");
          resultsContainer.classList.add("hidden");
          stepsContainer.classList.add("hidden");
        });
      })();
    </script>

    <script>
      // Simple mobile menu toggle to match existing pattern
      (function () {
        const toggle = document.getElementById("mobile-menu-toggle");
        const menu = document.getElementById("mobile-menu");
        if (!toggle || !menu) return;

        toggle.addEventListener("click", function () {
          const isOpen = !menu.classList.contains("hidden");
          menu.classList.toggle("hidden", isOpen);
          toggle.setAttribute("aria-expanded", String(!isOpen));
        });
      })();
