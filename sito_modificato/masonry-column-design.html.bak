    <script src="https://cdn.jsdelivr.net/npm/chart.js@4.4.0/dist/chart.umd.min.js"></script>
    <script>
      // Utility: reset to defaults
      document.getElementById("resetBtn").addEventListener("click", () => {
        const defaults = {
          b: 16,
          h: 16,
          cc: 2.0,
          Le: 10,
          fm: 1500,
          fy: 60,
          phi: 0.8,
          nBars: 4,
          barSize: "0.31",
          tieSpacing: 16,
          Pu: 150,
          Mux: 40,
          Muy: 0,
        };
        for (const [id, val] of Object.entries(defaults)) {
          const el = document.getElementById(id);
          if (!el) continue;
          if (el.tagName === "SELECT") el.value = val;
          else el.value = val;
        }
        document.getElementById("statusMsg").textContent = "";
        document.getElementById("passFailBox").classList.add("hidden");
        if (pmChart) {
          pmChart.data.datasets[0].data = [];
          pmChart.data.datasets[1].data = [];
          pmChart.update();
        }
        [
          "AgOut",
          "AsOut",
          "rhoOut",
          "slenderOut",
          "Pn0Out",
          "PbOut",
          "MnOut",
          "utilPOut",
          "utilMOut",
        ].forEach((id) => {
          document.getElementById(id).textContent = "–";
        });
      });

      // Core calculation
      function calcMasonryColumn() {
        const b = parseFloat(document.getElementById("b").value);
        const h = parseFloat(document.getElementById("h").value);
        const cc = parseFloat(document.getElementById("cc").value);
        const Le = parseFloat(document.getElementById("Le").value);
        const fm = parseFloat(document.getElementById("fm").value); // psi
        const fy = parseFloat(document.getElementById("fy").value) * 1000; // ksi -> psi
        const phi = parseFloat(document.getElementById("phi").value);

        const nBars = parseInt(document.getElementById("nBars").value, 10);
        const As_bar = parseFloat(document.getElementById("barSize").value); // in²
        const Pu = parseFloat(document.getElementById("Pu").value); // kips
        const Mux = parseFloat(document.getElementById("Mux").value); // kip-ft

        if (
          [b, h, cc, Le, fm, fy, phi, nBars, As_bar, Pu, Mux].some(
            (v) => !isFinite(v) || v <= 0
          )
        ) {
          throw new Error(
            "Please enter positive numeric values for all required fields."
          );
        }

        // Geometry
        const Ag = b * h; // in²
        const As = nBars * As_bar; // in²
        const rho = As / Ag;

        // Approx radius of gyration for solid rectangle about strong axis
        const Ix = (b * Math.pow(h, 3)) / 12; // in^4
        const rx = Math.sqrt(Ix / Ag); // in
        const Le_in = Le * 12; // ft -> in
        const slender = Le_in / rx;

        // Pure axial nominal strength (very simplified)
        const Pn_nom = 0.8 * fm * Ag + As * fy; // lb
        const Pn0 = (phi * Pn_nom) / 1000; // kips

        // Balanced axial load (rough estimate, for info)
        const Pb = 0.65 * Pn0;

        // Interaction curve via simple fiber-like sweep over neutral axis depth
        const eps_cu = 0.003;
        const beta1 = 0.8;
        const barY = [
          cc, // bottom
          h - cc, // top
          cc,
          h - cc,
        ];
        const barZ = [cc, cc, b - cc, b - cc];

        const points = [];
        const nSteps = 30;
        for (let i = 0; i <= nSteps; i++) {
          const c = h * 0.05 + ((h * 1.2 - h * 0.05) * i) / nSteps; // neutral axis depth
          const a = beta1 * c;
          const C_m = 0.8 * fm * b * a; // masonry compression (lb)
          let C_s = 0;
          let T_s = 0;
          let Mx = 0;

          for (let j = 0; j < nBars; j++) {
            const y = barY[j];
            const z = barZ[j];
            const eps = eps_cu * (1 - y / c);
            let fs = eps * 29000000; // Es
            if (fs > fy) fs = fy;
            if (fs < -fy) fs = -fy;
            const Fs = fs * As_bar; // lb
            if (Fs >= 0) C_s += Fs;
            else T_s += Fs;

            const ycg = h / 2;
            const ecc = y - ycg; // in
            Mx += Fs * ecc; // lb-in
          }

          const Pn = C_m + C_s + T_s; // lb
          const Mn = Mx; // lb-in
          const Pphi = (phi * Pn) / 1000; // kips
          const Mphi = (phi * Mn) / 12 / 1000; // kip-ft

          if (Pphi > 0 && Mphi >= 0) {
            points.push({ x: Mphi, y: Pphi });
          }
        }

        // Sort by moment
        points.sort((a, b) => a.x - b.x);

        // Capacity at given Pu: interpolate
        let Mn_at_Pu = 0;
        for (let i = 1; i < points.length; i++) {
          const p1 = points[i - 1],
            p2 = points[i];
          if ((Pu >= p1.y && Pu <= p2.y) || (Pu >= p2.y && Pu <= p1.y)) {
            const t = (Pu - p1.y) / (p2.y - p1.y);
            Mn_at_Pu = p1.x + t * (p2.x - p1.x);
            break;
          }
        }
        if (!isFinite(Mn_at_Pu) || Mn_at_Pu <= 0) {
          Mn_at_Pu = points.length ? points[points.length - 1].x : 0;
        }

        const utilP = Pu / Pn0;
        const utilM = Mux / Mn_at_Pu;

        return {
          Ag,
          As,
          rho,
          slender,
          Pn0,
          Pb,
          Mn_at_Pu,
          utilP,
          utilM,
          points,
          Pu,
          Mux,
        };
      }

      // Chart.js setup
      let pmChart = null;
      function initChart() {
        const ctx = document.getElementById("pmChart").getContext("2d");
        pmChart = new Chart(ctx, {
          type: "scatter",
          data: {
            datasets: [
              {
                label: "φPn–φMn curve",
                data: [],
                borderColor: "rgba(37,99,235,0.9)",
                backgroundColor: "rgba(37,99,235,0.15)",
                showLine: true,
                pointRadius: 0,
              },
              {
                label: "Load point (Pu, Mux)",
                data: [],
                backgroundColor: "rgba(220,38,38,0.9)",
                pointRadius: 5,
              },
            ],
          },
          options: {
            responsive: true,
            maintainAspectRatio: false,
            plugins: {
              legend: { position: "bottom" },
              tooltip: { mode: "nearest", intersect: false },
            },
            scales: {
              x: {
                title: { display: true, text: "Moment φM (kip-ft)" },
                beginAtZero: true,
              },
              y: {
                title: {
                  display: true,
                  text: "Axial load φP (kips, compression +)",
                },
                beginAtZero: true,
              },
            },
          },
        });
      }
      document.addEventListener("DOMContentLoaded", initChart);

      // Hook calculate button
      document.getElementById("calcBtn").addEventListener("click", () => {
        const status = document.getElementById("statusMsg");
        status.textContent = "Calculating...";
        try {
          const res = calcMasonryColumn();

          // Update numeric outputs
          document.getElementById("AgOut").textContent =
            res.Ag.toFixed(1) + " in²";
          document.getElementById("AsOut").textContent =
            res.As.toFixed(2) + " in²";
          document.getElementById("rhoOut").textContent =
            (res.rho * 100).toFixed(2) + " %";
          document.getElementById("slenderOut").textContent =
            res.slender.toFixed(1);

          document.getElementById("Pn0Out").textContent =
            res.Pn0.toFixed(1) + " kips";
          document.getElementById("PbOut").textContent =
            res.Pb.toFixed(1) + " kips";
          document.getElementById("MnOut").textContent =
            res.Mn_at_Pu.toFixed(1) + " kip-ft";
          document.getElementById("utilPOut").textContent =
            res.utilP.toFixed(2);
          document.getElementById("utilMOut").textContent =
            res.utilM.toFixed(2);

          // Pass/fail
          const box = document.getElementById("passFailBox");
          const txt = document.getElementById("passFailText");
          const det = document.getElementById("passFailDetail");
          box.classList.remove("hidden");
          if (res.utilP <= 1.0 && res.utilM <= 1.0) {
            box.classList.remove("border-red-300", "bg-red-50");
            box.classList.add("border-green-300", "bg-green-50");
            txt.classList.remove("text-red-700");
            txt.classList.add("text-green-700");
            txt.textContent = "Design OK (within estimated strength envelope)";
            det.textContent =
              "Both axial and flexural utilization ratios are ≤ 1.0 based on the simplified interaction curve.";
          } else {
            box.classList.remove("border-green-300", "bg-green-50");
            box.classList.add("border-red-300", "bg-red-50");
            txt.classList.remove("text-green-700");
            txt.classList.add("text-red-700");
            txt.textContent =
              "Design NOT OK (outside estimated strength envelope)";
            det.textContent =
              "At least one of the utilization ratios exceeds 1.0. Increase section size, reinforcement, or reduce loads.";
          }

          // Update chart
          if (pmChart) {
            pmChart.data.datasets[0].data = res.points.map((p) => ({
              x: p.x,
              y: p.y,
            }));
            pmChart.data.datasets[1].data = [{ x: res.Mux, y: res.Pu }];
            pmChart.update();
          }

          status.textContent = "Done.";
        } catch (err) {
          status.textContent = err.message;
        }
      });
    </script>

