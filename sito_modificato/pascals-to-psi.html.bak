            <script>
              document.addEventListener('DOMContentLoaded', () => {
                const paInput = document.getElementById('pa-input');
                const kpaInput = document.getElementById('kpa-input');
                const mpaInput = document.getElementById('mpa-input');
                const psiInput = document.getElementById('psi-input');
                const sciOutput = document.getElementById('sci-output');
                
                const inputs = [paInput, kpaInput, mpaInput, psiInput];
                
                // Conversione basata su 1 Pa
                const PA_TO_KPA = 0.001;
                const PA_TO_MPA = 0.000001;
                const PA_TO_PSI = 0.0001450377;
                
                function formatNum(num, precision = 6) {
                    if (isNaN(num) || num === 0) return '';
                    return parseFloat(num.toPrecision(precision)).toString();
                }

                function update(sourceUnit, value) {
                    if (isNaN(value) || value === 0) {
                        inputs.forEach(input => input.value = '');
                        sciOutput.textContent = '-';
                        return;
                    }

                    let pa = 0; // Base unit

                    // 1. Converti l'input sorgente in Pascals (Pa)
                    if (sourceUnit === 'pa') {
                        pa = value;
                    } else if (sourceUnit === 'kpa') {
                        pa = value / PA_TO_KPA;
                    } else if (sourceUnit === 'mpa') {
                        pa = value / PA_TO_MPA;
                    } else if (sourceUnit === 'psi') {
                        pa = value / PA_TO_PSI;
                    }
                    
                    // 2. Aggiorna tutti gli altri campi
                    const values = {
                        pa: pa,
                        kpa: pa * PA_TO_KPA,
                        mpa: pa * PA_TO_MPA,
                        psi: pa * PA_TO_PSI
                    };
                    
                    for (const input of inputs) {
                        const unit = input.dataset.unit;
                        if (unit !== sourceUnit) {
                            input.value = formatNum(values[unit]);
                        }
                    }
                    
                    // 3. Aggiorna la notazione scientifica
                    sciOutput.textContent = `${pa.toExponential(6)} Pa`;
                }

                inputs.forEach(input => {
                  input.addEventListener('input', (e) => {
                    const sourceUnit = e.target.dataset.unit;
                    const value = parseFloat(e.target.value);
                    update(sourceUnit, value);
                  });
                });
              });
            </script>
            
