    <script>
      // Mode switching
      const modeUaBtn = document.getElementById("mode-ua");
      const modeStemiBtn = document.getElementById("mode-stemi");
      const formUa = document.getElementById("form-ua");
      const formStemi = document.getElementById("form-stemi");
      const resultsSection = document.getElementById("results");

      function setMode(mode) {
        if (mode === "ua") {
          formUa.classList.remove("hidden");
          formStemi.classList.add("hidden");
          modeUaBtn.classList.add(
            "bg-blue-600",
            "text-white",
            "border-blue-600"
          );
          modeUaBtn.classList.remove(
            "bg-white",
            "text-gray-700",
            "border-gray-300"
          );
          modeStemiBtn.classList.remove(
            "bg-blue-600",
            "text-white",
            "border-blue-600"
          );
          modeStemiBtn.classList.add(
            "bg-white",
            "text-gray-700",
            "border-gray-300"
          );
        } else {
          formUa.classList.add("hidden");
          formStemi.classList.remove("hidden");
          modeStemiBtn.classList.add(
            "bg-blue-600",
            "text-white",
            "border-blue-600"
          );
          modeStemiBtn.classList.remove(
            "bg-white",
            "text-gray-700",
            "border-gray-300"
          );
          modeUaBtn.classList.remove(
            "bg-blue-600",
            "text-white",
            "border-blue-600"
          );
          modeUaBtn.classList.add(
            "bg-white",
            "text-gray-700",
            "border-gray-300"
          );
        }
        resultsSection.classList.add("hidden");
      }

      modeUaBtn.addEventListener("click", () => setMode("ua"));
      modeStemiBtn.addEventListener("click", () => setMode("stemi"));

      // UA/NSTEMI calculation
      function calcUaScore() {
        let score = 0;
        if (document.getElementById("ua-age").checked) score++;
        if (document.getElementById("ua-risk-factors").value === "1") score++;
        if (document.getElementById("ua-known-cad").checked) score++;
        if (document.getElementById("ua-aspirin").checked) score++;
        if (document.getElementById("ua-severe-angina").checked) score++;
        if (document.getElementById("ua-st-deviation").checked) score++;
        if (document.getElementById("ua-biomarker").checked) score++;

        // Map score to risk (approximate)
        let risk = "";
        let category = "";
        if (score <= 1) {
          risk = "~4–5%";
          category = "Low";
        } else if (score === 2) {
          risk = "~8%";
          category = "Low–intermediate";
        } else if (score === 3) {
          risk = "~13%";
          category = "Intermediate";
        } else if (score === 4) {
          risk = "~20%";
          category = "Intermediate–high";
        } else if (score === 5) {
          risk = "~26%";
          category = "High";
        } else {
          risk = "~40%+";
          category = "Very high";
        }

        document.getElementById("result-scenario-label").textContent =
          "Scenario:";
        document.getElementById("result-scenario").textContent = "UA/NSTEMI";
        document.getElementById("result-score").textContent = score.toString();
        document.getElementById("result-category").textContent = category;
        document.getElementById("result-risk-label").textContent =
          "Estimated 14‑day event risk";
        document.getElementById("result-risk").textContent = risk;

        const interp = [];
        interp.push(
          `A TIMI UA/NSTEMI score of ${score} corresponds to an approximate 14‑day risk of death, new or recurrent myocardial infarction, or urgent revascularization of ${risk}.`
        );
        if (score <= 2) {
          interp.push(
            "This falls in the low to low‑intermediate risk range. Many guidelines support an early non‑invasive strategy in the absence of other high‑risk features."
          );
        } else if (score <= 4) {
          interp.push(
            "This is an intermediate risk profile. Early invasive evaluation is often considered, especially if symptoms or biomarkers are concerning."
          );
        } else {
          interp.push(
            "This is a high to very high risk profile. Early invasive management and intensive medical therapy are usually recommended, subject to bleeding risk and comorbidities."
          );
        }
        document.getElementById("result-interpretation").textContent =
          interp.join(" ");

        resultsSection.classList.remove("hidden");
      }

      document.getElementById("ua-calc").addEventListener("click", calcUaScore);
      document.getElementById("ua-reset").addEventListener("click", () => {
        formUa.reset();
        resultsSection.classList.add("hidden");
      });

      // STEMI calculation
      function calcStemiScore() {
        let score = 0;
        score += parseInt(
          document.getElementById("stemi-age").value || "0",
          10
        );
        if (document.getElementById("stemi-diabetes").checked) score += 1;
        if (document.getElementById("stemi-sbp").checked) score += 3;
        if (document.getElementById("stemi-hr").checked) score += 2;
        if (document.getElementById("stemi-killip").checked) score += 2;
        if (document.getElementById("stemi-weight").checked) score += 1;
        if (document.getElementById("stemi-anterior").checked) score += 1;
        if (document.getElementById("stemi-delay").checked) score += 1;

        // Map score to mortality (approximate)
        let risk = "";
        let category = "";
        if (score <= 2) {
          risk = "&lt; 2%";
          category = "Low";
        } else if (score <= 4) {
          risk = "~3–7%";
          category = "Intermediate";
        } else if (score <= 6) {
          risk = "~8–13%";
          category = "High";
        } else if (score <= 8) {
          risk = "~19–26%";
          category = "Very high";
        } else {
          risk = "&gt; 30%";
          category = "Extremely high";
        }

        document.getElementById("result-scenario-label").textContent =
          "Scenario:";
        document.getElementById("result-scenario").textContent = "STEMI";
        document.getElementById("result-score").textContent = score.toString();
        document.getElementById("result-category").textContent = category;
        document.getElementById("result-risk-label").textContent =
          "Estimated 30‑day mortality";
        document.getElementById("result-risk").innerHTML = risk;

        const interp = [];
        interp.push(
          `A TIMI STEMI score of ${score} corresponds to an approximate 30‑day mortality of ${risk}.`
        );
        if (score <= 2) {
          interp.push(
            "This is a relatively low‑risk profile, but urgent reperfusion and guideline‑directed therapy remain essential."
          );
        } else if (score <= 4) {
          interp.push(
            "This is an intermediate‑risk profile. Rapid reperfusion, hemodynamic monitoring, and optimization of secondary prevention are important."
          );
        } else if (score <= 6) {
          interp.push(
            "This is a high‑risk profile. Consider intensive monitoring, early recognition of complications, and multidisciplinary management."
          );
        } else {
          interp.push(
            "This is a very high to extremely high risk profile. Aggressive supportive care, early reperfusion, and consideration of advanced therapies may be warranted."
          );
        }
        document.getElementById("result-interpretation").innerHTML =
          interp.join(" ");

        resultsSection.classList.remove("hidden");
      }

      document
        .getElementById("stemi-calc")
        .addEventListener("click", calcStemiScore);
      document.getElementById("stemi-reset").addEventListener("click", () => {
        formStemi.reset();
        resultsSection.classList.add("hidden");
      });

      // Default mode
      setMode("ua");

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
