    <script>
      // Core conversion factor
      const FT_LB_PER_S_TO_W = 1.3558179483;

      const ftInput = document.getElementById("ftlbs-input");
      const wInput = document.getElementById("watts-input");
      const resultSummary = document.getElementById("result-summary");
      const precisionSelect = document.getElementById("precision-select");
      const quickTableBody = document.getElementById("quick-table-body");

      function formatNumber(value, decimals) {
        if (!isFinite(value)) return "";
        return Number(value).toFixed(decimals);
      }

      function getDecimals() {
        const v = parseInt(precisionSelect.value, 10);
        return isNaN(v) ? 3 : v;
      }

      function updateFromFt() {
        const val = parseFloat(ftInput.value);
        if (isNaN(val)) {
          wInput.value = "";
          resultSummary.classList.add("hidden");
          return;
        }
        const watts = val * FT_LB_PER_S_TO_W;
        const d = getDecimals();
        wInput.value = formatNumber(watts, d);
        updateSummary("ftToW", val, watts, d);
      }

      function updateFromW() {
        const val = parseFloat(wInput.value);
        if (isNaN(val)) {
          ftInput.value = "";
          resultSummary.classList.add("hidden");
          return;
        }
        const ft = val / FT_LB_PER_S_TO_W;
        const d = getDecimals();
        ftInput.value = formatNumber(ft, d);
        updateSummary("wToFt", val, ft, d);
      }

      function updateSummary(direction, fromVal, toVal, decimals) {
        let text = "";
        if (direction === "ftToW") {
          text = `${fromVal} ft⋅lb/s = ${formatNumber(toVal, decimals)} W`;
        } else {
          text = `${fromVal} W = ${formatNumber(toVal, decimals)} ft⋅lb/s`;
        }
        resultSummary.textContent = text;
        resultSummary.classList.remove("hidden");
      }

      ftInput.addEventListener("input", () => {
        if (document.activeElement === ftInput) updateFromFt();
      });

      wInput.addEventListener("input", () => {
        if (document.activeElement === wInput) updateFromW();
      });

      precisionSelect.addEventListener("change", () => {
        if (document.activeElement === ftInput && ftInput.value !== "") {
          updateFromFt();
        } else if (document.activeElement === wInput && wInput.value !== "") {
          updateFromW();
        } else if (ftInput.value !== "") {
          updateFromFt();
        } else if (wInput.value !== "") {
          updateFromW();
        }
        buildQuickTable();
      });

      document.getElementById("swap-btn").addEventListener("click", () => {
        const ftVal = ftInput.value;
        const wVal = wInput.value;
        ftInput.value = wVal;
        wInput.value = ftVal;
        if (ftInput.value !== "" && !isNaN(parseFloat(ftInput.value))) {
          updateFromFt();
          ftInput.focus();
        } else if (wInput.value !== "" && !isNaN(parseFloat(wInput.value))) {
          updateFromW();
          wInput.focus();
        } else {
          resultSummary.classList.add("hidden");
        }
      });

      document.getElementById("clear-btn").addEventListener("click", () => {
        ftInput.value = "";
        wInput.value = "";
        resultSummary.classList.add("hidden");
        ftInput.focus();
      });

      function buildQuickTable() {
        const d = getDecimals();
        const baseValues = [1, 5, 10, 25, 50, 100, 250, 500];
        quickTableBody.innerHTML = "";
        baseValues.forEach((v) => {
          const w = v * FT_LB_PER_S_TO_W;
          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="px-3 py-1.5 border-b">${v}</td>
        <td class="px-3 py-1.5 border-b">${formatNumber(w, d)}</td>
      `;
          quickTableBody.appendChild(tr);
        });
      }

      // Initialize table on load
      buildQuickTable();
    </script>

