    <script>
      // Utility conversions
      const NM_PER_M = 1 / 1852;
      const KM_PER_M = 0.001;
      const MI_PER_M = 1 / 1609.344;

      function speedToMps(value, unit) {
        const v = Number(value);
        if (!isFinite(v)) return NaN;
        switch (unit) {
          case "mps":
            return v;
          case "kt":
            return v * 0.514444;
          case "kmh":
            return v * (1000 / 3600);
          case "mph":
            return v * 0.44704;
          default:
            return NaN;
        }
      }

      function weightToKg(value, unit) {
        const v = Number(value);
        if (!isFinite(v)) return NaN;
        return unit === "lb" ? v * 0.45359237 : v;
      }

      function kgToWeight(vKg, unit) {
        return unit === "lb" ? vKg / 0.45359237 : vKg;
      }

      function fuelToKg(value, unit) {
        const v = Number(value);
        if (!isFinite(v)) return NaN;
        // Assume Jet A density ~0.8 kg/L, 6.7 lb/US gal
        switch (unit) {
          case "kg":
            return v;
          case "lb":
            return v * 0.45359237;
          case "l":
            return v * 0.8;
          case "gal":
            return v * 6.7 * 0.45359237;
          default:
            return NaN;
        }
      }

      function kgToFuel(vKg, unit) {
        switch (unit) {
          case "kg":
            return vKg;
          case "lb":
            return vKg / 0.45359237;
          case "l":
            return vKg / 0.8;
          case "gal":
            return vKg / (6.7 * 0.45359237);
          default:
            return vKg;
        }
      }

      // Tabs
      const tabBreguet = document.getElementById("tab-breguet");
      const tabSimple = document.getElementById("tab-simple");
      const panelBreguet = document.getElementById("panel-breguet");
      const panelSimple = document.getElementById("panel-simple");

      tabBreguet.addEventListener("click", () => {
        tabBreguet.classList.add("tab-active");
        tabSimple.classList.remove("tab-active");
        panelBreguet.classList.remove("hidden");
        panelSimple.classList.add("hidden");
      });

      tabSimple.addEventListener("click", () => {
        tabSimple.classList.add("tab-active");
        tabBreguet.classList.remove("tab-active");
        panelSimple.classList.remove("hidden");
        panelBreguet.classList.add("hidden");
      });

      // Breguet mode toggles
      const breguetType = document.getElementById("breguet-type");
      const breguetSfcLabel = document.getElementById("breguet-sfc-label");
      const breguetSfcHelp = document.getElementById("breguet-sfc-help");
      const breguetDirectWeights = document.getElementById(
        "breguet-direct-weights"
      );
      const breguetFuelWeights = document.getElementById(
        "breguet-fuel-weights"
      );
      const weightModeRadios = document.querySelectorAll(
        ".breguet-weight-mode"
      );

      function updateBreguetSfcLabels() {
        if (breguetType.value === "jet") {
          breguetSfcLabel.innerHTML =
            "Specific fuel consumption (c<sub>T</sub>, 1/hr)";
          breguetSfcHelp.textContent =
            "Jet: thrust-specific fuel consumption in 1/hr (e.g. 0.6 1/hr).";
        } else {
          breguetSfcLabel.innerHTML =
            "Specific fuel consumption (c<sub>P</sub>, 1/hr)";
          breguetSfcHelp.textContent =
            "Prop: power-specific fuel consumption in 1/hr. Propeller efficiency ηp is applied separately.";
        }
      }

      breguetType.addEventListener("change", updateBreguetSfcLabels);
      updateBreguetSfcLabels();

      weightModeRadios.forEach((r) => {
        r.addEventListener("change", () => {
          const mode = document.querySelector(
            'input[name="breguet-weight-mode"]:checked'
          ).value;
          if (mode === "direct") {
            breguetDirectWeights.classList.remove("hidden");
            breguetFuelWeights.classList.add("hidden");
          } else {
            breguetDirectWeights.classList.add("hidden");
            breguetFuelWeights.classList.remove("hidden");
          }
        });
      });

      // Breguet calculation
      document.getElementById("breguet-calc").addEventListener("click", () => {
        const ld = Number(document.getElementById("breguet-ld").value);
        const sfc = Number(document.getElementById("breguet-sfc").value);
        const speedUnit = document.getElementById("breguet-speed-unit").value;
        const weightUnit = document.getElementById("breguet-weight-unit").value;
        const V = speedToMps(
          document.getElementById("breguet-speed").value,
          speedUnit
        );
        const eta = Number(document.getElementById("breguet-eta").value);
        const mode = document.querySelector(
          'input[name="breguet-weight-mode"]:checked'
        ).value;

        let WiKg, WfKg;

        if (mode === "direct") {
          const Wi = weightToKg(
            document.getElementById("breguet-wi").value,
            weightUnit
          );
          const Wf = weightToKg(
            document.getElementById("breguet-wf").value,
            weightUnit
          );
          WiKg = Wi;
          WfKg = Wf;
        } else {
          const oew = weightToKg(
            document.getElementById("breguet-oew").value,
            weightUnit
          );
          const payload = weightToKg(
            document.getElementById("breguet-payload").value,
            weightUnit
          );
          const fuel = weightToKg(
            document.getElementById("breguet-fuel").value,
            weightUnit
          );
          const reserve = weightToKg(
            document.getElementById("breguet-reserve").value,
            weightUnit
          );
          WiKg = oew + payload + fuel;
          const fuelUsed = Math.max(fuel - reserve, 0);
          WfKg = WiKg - fuelUsed;
        }

        const outputBox = document.getElementById("breguet-output");
        const rangeLine = document.getElementById("breguet-range-line");
        const enduranceLine = document.getElementById("breguet-endurance-line");
        const weightLine = document.getElementById("breguet-weight-line");

        if (
          !isFinite(ld) ||
          ld <= 0 ||
          !isFinite(sfc) ||
          sfc <= 0 ||
          !isFinite(V) ||
          V <= 0 ||
          !isFinite(WiKg) ||
          !isFinite(WfKg) ||
          WiKg <= WfKg ||
          WfKg <= 0
        ) {
          outputBox.classList.remove("hidden");
          rangeLine.textContent =
            "Please check your inputs. L/D, SFC, speed must be positive, and initial weight must exceed final weight.";
          enduranceLine.textContent = "";
          weightLine.textContent = "";
          return;
        }

        // Convert SFC from 1/hr to 1/s
        const cPerSec = sfc / 3600;

        let factor;
        if (breguetType.value === "jet") {
          factor = V / cPerSec;
        } else {
          factor = (eta * V) / cPerSec;
        }

        const WiWf = WiKg / WfKg;
        const lnWiWf = Math.log(WiWf);
        const R_m = factor * ld * lnWiWf; // meters

        const R_nm = R_m * NM_PER_M;
        const R_km = R_m * KM_PER_M;
        const R_mi = R_m * MI_PER_M;

        // Approximate endurance from Breguet: E = (L/D / c) * ln(Wi/Wf)
        const E_sec = (ld / cPerSec) * lnWiWf;
        const E_hr = E_sec / 3600;
        const E_min = E_hr * 60;

        const fuelFrac = (WiKg - WfKg) / WiKg;

        outputBox.classList.remove("hidden");
        rangeLine.textContent = `Estimated range: ${R_nm.toFixed(
          0
        )} NM  |  ${R_km.toFixed(0)} km  |  ${R_mi.toFixed(0)} mi`;
        enduranceLine.textContent = `Approximate endurance: ${E_hr.toFixed(
          2
        )} hours (${E_min.toFixed(0)} minutes)`;
        weightLine.textContent = `Weight ratio W_i/W_f = ${WiWf.toFixed(
          3
        )}, fuel fraction ≈ ${(fuelFrac * 100).toFixed(1)}% of initial weight.`;
      });

      document.getElementById("breguet-reset").addEventListener("click", () => {
        document.getElementById("breguet-ld").value = 15;
        document.getElementById("breguet-sfc").value = 0.6;
        document.getElementById("breguet-speed").value = 450;
        document.getElementById("breguet-eta").value = 0.85;
        document.getElementById("breguet-wi").value = 70000;
        document.getElementById("breguet-wf").value = 55000;
        document.getElementById("breguet-oew").value = 40000;
        document.getElementById("breguet-payload").value = 10000;
        document.getElementById("breguet-fuel").value = 20000;
        document.getElementById("breguet-reserve").value = 3000;
        document.getElementById("breguet-output").classList.add("hidden");
      });

      // Simple model calculation
      document.getElementById("simple-calc").addEventListener("click", () => {
        const speed = Number(document.getElementById("simple-speed").value);
        const speedUnit = document.getElementById("simple-speed-unit").value;
        const fuel = Number(document.getElementById("simple-fuel").value);
        const reserve = Number(document.getElementById("simple-reserve").value);
        const flow = Number(document.getElementById("simple-flow").value);
        const fuelUnit = document.getElementById("simple-fuel-unit").value;
        const flowUnit = document.getElementById("simple-flow-unit").value;

        const outputBox = document.getElementById("simple-output");
        const rangeLine = document.getElementById("simple-range-line");
        const enduranceLine = document.getElementById("simple-endurance-line");
        const fuelLine = document.getElementById("simple-fuel-line");

        if (
          !isFinite(speed) ||
          speed <= 0 ||
          !isFinite(fuel) ||
          fuel <= 0 ||
          !isFinite(flow) ||
          flow <= 0
        ) {
          outputBox.classList.remove("hidden");
          rangeLine.textContent =
            "Please enter positive values for speed, fuel, and fuel flow.";
          enduranceLine.textContent = "";
          fuelLine.textContent = "";
          return;
        }

        const fuelKg = fuelToKg(fuel, fuelUnit);
        const reserveKg = fuelToKg(Math.max(reserve, 0), fuelUnit);
        const usableKg = Math.max(fuelKg - reserveKg, 0);

        let flowKgPerHr = fuelToKg(flow, fuelUnit);
        if (flowUnit === "kgpm") {
          flowKgPerHr *= 60;
        }

        if (usableKg <= 0 || flowKgPerHr <= 0) {
          outputBox.classList.remove("hidden");
          rangeLine.textContent =
            "Usable fuel or fuel flow is zero or negative after conversion. Check reserve and units.";
          enduranceLine.textContent = "";
          fuelLine.textContent = "";
          return;
        }

        const enduranceHr = usableKg / flowKgPerHr;
        const enduranceMin = enduranceHr * 60;

        // Convert speed to knots for range in NM
        let speedKt;
        switch (speedUnit) {
          case "kt":
            speedKt = speed;
            break;
          case "kmh":
            speedKt = speed * 0.539957;
            break;
          case "mph":
            speedKt = speed * 0.868976;
            break;
          default:
            speedKt = speed;
        }

        const rangeNm = speedKt * enduranceHr;
        const rangeKm = rangeNm * 1.852;
        const rangeMi = rangeNm * 1.15078;

        outputBox.classList.remove("hidden");
        rangeLine.textContent = `Estimated range: ${rangeNm.toFixed(
          0
        )} NM  |  ${rangeKm.toFixed(0)} km  |  ${rangeMi.toFixed(0)} mi`;
        enduranceLine.textContent = `Endurance: ${enduranceHr.toFixed(
          2
        )} hours (${enduranceMin.toFixed(0)} minutes)`;
        fuelLine.textContent = `Usable fuel: ${usableKg.toFixed(
          0
        )} kg (${kgToFuel(usableKg, fuelUnit).toFixed(
          0
        )} ${fuelUnit}), reserve: ${reserveKg.toFixed(0)} kg.`;
      });

      document.getElementById("simple-reset").addEventListener("click", () => {
        document.getElementById("simple-speed").value = 450;
        document.getElementById("simple-fuel").value = 8000;
        document.getElementById("simple-flow").value = 2000;
        document.getElementById("simple-reserve").value = 1000;
        document.getElementById("simple-output").classList.add("hidden");
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
