<script>
  // Tab switching
  const tabBetween = document.getElementById('tab-between');
  const tabAddSub = document.getElementById('tab-addsub');
  const panelBetween = document.getElementById('panel-between');
  const panelAddSub = document.getElementById('panel-addsub');

  function activateTab(tab) {
    const isBetween = tab === 'between';
    tabBetween.classList.toggle('bg-blue-600', isBetween);
    tabBetween.classList.toggle('text-white', isBetween);
    tabBetween.classList.toggle('bg-white', !isBetween);
    tabBetween.classList.toggle('text-gray-700', !isBetween);
    tabBetween.setAttribute('aria-selected', isBetween ? 'true' : 'false');

    tabAddSub.classList.toggle('bg-blue-600', !isBetween);
    tabAddSub.classList.toggle('text-white', !isBetween);
    tabAddSub.classList.toggle('bg-white', isBetween);
    tabAddSub.classList.toggle('text-gray-700', isBetween);
    tabAddSub.setAttribute('aria-selected', !isBetween ? 'true' : 'false');

    panelBetween.classList.toggle('hidden', !isBetween);
    panelAddSub.classList.toggle('hidden', isBetween);
  }

  tabBetween.addEventListener('click', () => activateTab('between'));
  tabAddSub.addEventListener('click', () => activateTab('addsub'));

  // Utility functions
  function parseDateTime(dateStr, timeStr) {
    if (!dateStr || !timeStr) return null;
    const iso = dateStr + 'T' + timeStr;
    const d = new Date(iso);
    if (isNaN(d.getTime())) return null;
    return d;
  }

  function formatDurationParts(totalSeconds) {
    const sign = totalSeconds < 0 ? -1 : 1;
    let s = Math.abs(Math.round(totalSeconds));

    const days = Math.floor(s / 86400);
    s %= 86400;
    const hours = Math.floor(s / 3600);
    s %= 3600;
    const minutes = Math.floor(s / 60);
    const seconds = s % 60;

    return { sign, days, hours, minutes, seconds };
  }

  function durationToText(parts) {
    const { days, hours, minutes, seconds } = parts;
    const segments = [];
    if (days) segments.push(days + ' day' + (days !== 1 ? 's' : ''));
    if (hours) segments.push(hours + ' hour' + (hours !== 1 ? 's' : ''));
    if (minutes) segments.push(minutes + ' minute' + (minutes !== 1 ? 's' : ''));
    if (seconds || segments.length === 0) segments.push(seconds + ' second' + (seconds !== 1 ? 's' : ''));
    return segments.join(' ');
  }

  function roundSeconds(sec, mode) {
    if (mode === 'none') return sec;
    const unit = mode === 'minute' ? 60 : 900; // 60s or 15min
    return Math.round(sec / unit) * unit;
  }

  function formatDateTimeDisplay(d) {
    const pad = n => String(n).padStart(2, '0');
    return `${d.getFullYear()}-${pad(d.getMonth()+1)}-${pad(d.getDate())} ${pad(d.getHours())}:${pad(d.getMinutes())}:${pad(d.getSeconds())}`;
  }

  // Between two times logic
  const startDateEl = document.getElementById('start-date');
  const startTimeEl = document.getElementById('start-time');
  const endDateEl = document.getElementById('end-date');
  const endTimeEl = document.getElementById('end-time');
  const roundingEl = document.getElementById('rounding');
  const betweenErrorEl = document.getElementById('between-error');
  const betweenResultEl = document.getElementById('between-result');
  const betweenMainEl = document.getElementById('between-main');
  const betweenAltEl = document.getElementById('between-alt');
  const betweenSignEl = document.getElementById('between-sign');
  const betweenTotalSecondsEl = document.getElementById('between-total-seconds');
  const betweenTotalMinutesEl = document.getElementById('between-total-minutes');
  const betweenTotalHoursEl = document.getElementById('between-total-hours');
  const betweenTotalDaysEl = document.getElementById('between-total-days');
  const betweenTotalWeeksEl = document.getElementById('between-total-weeks');

  document.getElementById('calc-between').addEventListener('click', () => {
    betweenErrorEl.classList.add('hidden');
    betweenResultEl.classList.add('hidden');

    const start = parseDateTime(startDateEl.value, startTimeEl.value);
    const end = parseDateTime(endDateEl.value, endTimeEl.value);

    if (!start || !end) {
      betweenErrorEl.textContent = 'Please enter a valid start and end date and time.';
      betweenErrorEl.classList.remove('hidden');
      return;
    }

    let diffSeconds = (end.getTime() - start.getTime()) / 1000;
    const originalSign = diffSeconds < 0 ? -1 : 1;

    diffSeconds = roundSeconds(diffSeconds, roundingEl.value);

    const parts = formatDurationParts(diffSeconds);
    const text = durationToText(parts);

    const totalSeconds = Math.round(diffSeconds);
    const totalMinutes = totalSeconds / 60;
    const totalHours = totalSeconds / 3600;
    const totalDays = totalSeconds / 86400;
    const totalWeeks = totalDays / 7;

    betweenMainEl.textContent = (parts.sign < 0 ? '− ' : '') + text;
    betweenAltEl.textContent = `≈ ${totalHours.toFixed(3)} hours or ${totalMinutes.toFixed(1)} minutes.`;
    betweenSignEl.textContent = originalSign < 0
      ? 'Note: End time is earlier than start time, so the duration is negative.'
      : 'Duration is positive (end is after start).';

    betweenTotalSecondsEl.textContent = `Total seconds: ${totalSeconds.toLocaleString()}`;
    betweenTotalMinutesEl.textContent = `Total minutes: ${totalMinutes.toFixed(3)}`;
    betweenTotalHoursEl.textContent = `Total hours: ${totalHours.toFixed(3)}`;
    betweenTotalDaysEl.textContent = `Total days: ${totalDays.toFixed(3)}`;
    betweenTotalWeeksEl.textContent = `Total weeks: ${totalWeeks.toFixed(3)}`;

    betweenResultEl.classList.remove('hidden');
  });

  document.getElementById('clear-between').addEventListener('click', () => {
    startDateEl.value = '';
    startTimeEl.value = '';
    endDateEl.value = '';
    endTimeEl.value = '';
    roundingEl.value = 'none';
    betweenErrorEl.classList.add('hidden');
    betweenResultEl.classList.add('hidden');
  });

  document.getElementById('swap-button').addEventListener('click', () => {
    const sd = startDateEl.value;
    const st = startTimeEl.value;
    startDateEl.value = endDateEl.value;
    startTimeEl.value = endTimeEl.value;
    endDateEl.value = sd;
    endTimeEl.value = st;
  });

  document.getElementById('now-end-button').addEventListener('click', () => {
    const now = new Date();
    const pad = n => String(n).padStart(2, '0');
    endDateEl.value = `${now.getFullYear()}-${pad(now.getMonth()+1)}-${pad(now.getDate())}`;
    endTimeEl.value = `${pad(now.getHours())}:${pad(now.getMinutes())}:${pad(now.getSeconds())}`;
  });

  // Add/Subtract logic
  const baseDateEl = document.getElementById('base-date');
  const baseTimeEl = document.getElementById('base-time');
  const intDaysEl = document.getElementById('int-days');
  const intHoursEl = document.getElementById('int-hours');
  const intMinutesEl = document.getElementById('int-minutes');
  const intSecondsEl = document.getElementById('int-seconds');
  const intNegativeEl = document.getElementById('int-negative');
  const addsubErrorEl = document.getElementById('addsub-error');
  const addsubResultEl = document.getElementById('addsub-result');
  const addsubMainEl = document.getElementById('addsub-main');
  const addsubAltEl = document.getElementById('addsub-alt');
  const addsubDurEl = document.getElementById('addsub-dur');

  document.getElementById('calc-addsub').addEventListener('click', () => {
    addsubErrorEl.classList.add('hidden');
    addsubResultEl.classList.add('hidden');

    const base = parseDateTime(baseDateEl.value, baseTimeEl.value);
    if (!base) {
      addsubErrorEl.textContent = 'Please enter a valid base date and time.';
      addsubErrorEl.classList.remove('hidden');
      return;
    }

    const days = parseInt(intDaysEl.value || '0', 10);
    const hours = parseInt(intHoursEl.value || '0', 10);
    const minutes = parseInt(intMinutesEl.value || '0', 10);
    const seconds = parseInt(intSecondsEl.value || '0', 10);

    if (days < 0 || hours < 0 || minutes < 0 || seconds < 0) {
      addsubErrorEl.textContent = 'Interval values must be zero or positive.';
      addsubErrorEl.classList.remove('hidden');
      return;
    }

    let totalSec = days * 86400 + hours * 3600 + minutes * 60 + seconds;
    if (totalSec === 0) {
      addsubErrorEl.textContent = 'Please enter a non-zero interval to add or subtract.';
      addsubErrorEl.classList.remove('hidden');
      return;
    }

    if (intNegativeEl.checked) totalSec *= -1;

    const resultDate = new Date(base.getTime() + totalSec * 1000);

    addsubMainEl.textContent = formatDateTimeDisplay(resultDate);

    const baseStr = formatDateTimeDisplay(base);
    const op = intNegativeEl.checked ? '−' : '+';
    const parts = formatDurationParts(totalSec);
    const durText = durationToText(parts);
    addsubAltEl.textContent = `${baseStr} ${op} ${durText} = ${formatDateTimeDisplay(resultDate)}`;
    addsubDurEl.textContent = `Interval applied: ${intNegativeEl.checked ? '− ' : ''}${durText}`;

    addsubResultEl.classList.remove('hidden');
  });

  document.getElementById('clear-addsub').addEventListener('click', () => {
    baseDateEl.value = '';
    baseTimeEl.value = '';
    intDaysEl.value = '0';
    intHoursEl.value = '0';
    intMinutesEl.value = '0';
    intSecondsEl.value = '0';
    intNegativeEl.checked = false;
    addsubErrorEl.classList.add('hidden');
    addsubResultEl.classList.add('hidden');
  });

  document.getElementById('now-base-button').addEventListener('click', () => {
    const now = new Date();
    const pad = n => String(n).padStart(2, '0');
    baseDateEl.value = `${now.getFullYear()}-${pad(now.getMonth()+1)}-${pad(now.getDate())}`;
    baseTimeEl.value = `${pad(now.getHours())}:${pad(now.getMinutes())}:${pad(now.getSeconds())}`;
  });

  // Initialize default values: set start and end to today
  (function initDefaults() {
    const now = new Date();
    const pad = n => String(n).padStart(2, '0');
    const today = `${now.getFullYear()}-${pad(now.getMonth()+1)}-${pad(now.getDate())}`;
    startDateEl.value = today;
    endDateEl.value = today;
    baseDateEl.value = today;
    startTimeEl.value = '09:00:00';
    endTimeEl.value = '17:00:00';
    baseTimeEl.value = `${pad(now.getHours())}:${pad(now.getMinutes())}:${pad(now.getSeconds())}`;
  })();

  // Mobile menu script (simple toggle)
  document.getElementById('mobile-menu-toggle').addEventListener('click', () => {
    const menu = document.getElementById('mobile-menu');
    const expanded = menu.classList.contains('hidden') ? 'true' : 'false';
    menu.classList.toggle('hidden');
    document.getElementById('mobile-menu-toggle').setAttribute('aria-expanded', expanded);
  });
</script>

