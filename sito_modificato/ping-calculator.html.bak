    <script>
      // Ping calculator logic
      (function () {
        const startBtn = document.getElementById("start-btn");
        const stopBtn = document.getElementById("stop-btn");
        const statusText = document.getElementById("status-text");
        const statusIndicator = document.getElementById("status-indicator");
        const packetCountInput = document.getElementById("packet-count");
        const timeoutInput = document.getElementById("timeout-ms");
        const intervalInput = document.getElementById("interval-ms");
        const tableBody = document.getElementById("packet-table-body");

        const avgPingEl = document.getElementById("avg-ping");
        const minMaxPingEl = document.getElementById("min-max-ping");
        const jitterEl = document.getElementById("jitter");
        const packetLossEl = document.getElementById("packet-loss");
        const qualityBadge = document.getElementById("quality-badge");
        const qualityText = document.getElementById("quality-text");
        const mosScoreEl = document.getElementById("mos-score");
        const mosTextEl = document.getElementById("mos-text");

        let isRunning = false;
        let controller = null;
        let timeouts = [];

        function setStatus(text, state) {
          statusText.textContent = text;
          statusIndicator.classList.remove(
            "bg-gray-400",
            "bg-green-500",
            "bg-red-500",
            "bg-yellow-400"
          );
          if (state === "running")
            statusIndicator.classList.add("bg-green-500");
          else if (state === "error")
            statusIndicator.classList.add("bg-red-500");
          else if (state === "warning")
            statusIndicator.classList.add("bg-yellow-400");
          else statusIndicator.classList.add("bg-gray-400");
        }

        function resetResults() {
          avgPingEl.textContent = "–";
          minMaxPingEl.textContent = "– / –";
          jitterEl.textContent = "–";
          packetLossEl.textContent = "–";
          qualityBadge.textContent = "N/A";
          qualityBadge.className =
            "text-xs font-semibold px-2 py-0.5 rounded-full bg-gray-100 text-gray-700";
          qualityText.textContent =
            "Run a test to see how good your connection is for gaming, video calls, and streaming.";
          mosScoreEl.textContent = "–";
          mosTextEl.textContent =
            "MOS (Mean Opinion Score) ranges from 1 (bad) to 4.5+ (excellent) for VoIP and video calls.";
          tableBody.innerHTML = "";
        }

        function computeStats(samples, totalPackets) {
          const successful = samples.filter((s) => s.success);
          const lossCount = totalPackets - successful.length;
          const lossPct =
            totalPackets > 0 ? (lossCount / totalPackets) * 100 : 0;

          if (successful.length === 0) {
            return {
              avg: null,
              min: null,
              max: null,
              jitter: null,
              lossPct,
            };
          }

          const times = successful.map((s) => s.rtt);
          const sum = times.reduce((a, b) => a + b, 0);
          const avg = sum / times.length;
          const min = Math.min(...times);
          const max = Math.max(...times);
          const variance =
            times.reduce((acc, t) => acc + Math.pow(t - avg, 2), 0) /
            times.length;
          const jitter = Math.sqrt(variance);

          return { avg, min, max, jitter, lossPct };
        }

        function estimateMOS(latencyMs, lossPct) {
          if (latencyMs == null) return null;
          const R0 = 94.2;
          const Id = latencyMs / 10;
          const Ie = lossPct * 2.5;
          let R = R0 - Id - Ie;
          if (R < 0) R = 0;
          if (R > 100) R = 100;
          let MOS;
          if (R < 0) MOS = 1;
          else if (R > 100) MOS = 4.5;
          else MOS = 1 + 0.035 * R + R * (R - 60) * (100 - R) * 7e-6;
          return Math.max(1, Math.min(4.5, MOS));
        }

        function updateQualityDisplay(stats) {
          const { avg, jitter, lossPct } = stats;
          if (avg == null) {
            setStatus(
              "No responses received. Check your connection or firewall.",
              "error"
            );
            return;
          }

          avgPingEl.textContent = avg.toFixed(1);
          minMaxPingEl.textContent =
            stats.min.toFixed(1) + " / " + stats.max.toFixed(1);
          jitterEl.textContent = jitter.toFixed(1);
          packetLossEl.textContent = lossPct.toFixed(1) + "%";

          let quality = "Unknown";
          let badgeClass = "bg-gray-100 text-gray-700";
          let desc = "";

          if (lossPct > 5 || avg > 200) {
            quality = "Poor";
            badgeClass =
              "badge-bad px-2 py-0.5 rounded-full text-xs font-semibold";
            desc =
              "High latency or packet loss detected. Expect noticeable lag in games and video calls.";
          } else if (lossPct > 1 || avg > 100 || jitter > 30) {
            quality = "Fair";
            badgeClass =
              "badge-ok px-2 py-0.5 rounded-full text-xs font-semibold";
            desc =
              "Connection is usable but you may notice occasional lag or stutter.";
          } else if (avg > 60 || jitter > 20) {
            quality = "Good";
            badgeClass =
              "badge-good px-2 py-0.5 rounded-full text-xs font-semibold";
            desc =
              "Good connection for most online games, HD streaming, and video calls.";
          } else {
            quality = "Excellent";
            badgeClass =
              "badge-good px-2 py-0.5 rounded-full text-xs font-semibold";
            desc =
              "Excellent connection. Ideal for competitive gaming and real‑time applications.";
          }

          qualityBadge.textContent = quality;
          qualityBadge.className = badgeClass;
          qualityText.textContent = desc;

          const mos = estimateMOS(avg, lossPct);
          if (mos != null) {
            mosScoreEl.textContent = mos.toFixed(2);
            if (mos >= 4.0) {
              mosTextEl.textContent =
                "Excellent voice/video quality expected on most platforms.";
            } else if (mos >= 3.5) {
              mosTextEl.textContent =
                "Good quality. Minor artifacts may appear under heavy load.";
            } else if (mos >= 3.0) {
              mosTextEl.textContent =
                "Fair quality. You may notice glitches or short dropouts.";
            } else {
              mosTextEl.textContent =
                "Poor quality. Expect frequent cuts, delays, and artifacts.";
            }
          } else {
            mosScoreEl.textContent = "–";
          }
        }

        async function runPingTest() {
          const totalPackets = Math.max(
            5,
            Math.min(200, parseInt(packetCountInput.value, 10) || 30)
          );
          const timeoutMs = Math.max(
            100,
            Math.min(5000, parseInt(timeoutInput.value, 10) || 1500)
          );
          const intervalMs = Math.max(
            50,
            Math.min(5000, parseInt(intervalInput.value, 10) || 250)
          );

          packetCountInput.value = totalPackets;
          timeoutInput.value = timeoutMs;
          intervalInput.value = intervalMs;

          resetResults();
          isRunning = true;
          startBtn.disabled = true;
          stopBtn.disabled = false;
          setStatus("Running ping test…", "running");

          const samples = [];
          tableBody.innerHTML = "";

          for (let i = 0; i < totalPackets && isRunning; i++) {
            const row = document.createElement("tr");
            row.innerHTML = `
          <td class="px-2 py-1 text-gray-700">${i + 1}</td>
          <td class="px-2 py-1 text-gray-700">Pending</td>
          <td class="px-2 py-1 text-gray-700">–</td>
          <td class="px-2 py-1 text-gray-700">${new Date().toLocaleTimeString()}</td>
        `;
            tableBody.appendChild(row);

            const startTime = performance.now();
            let success = false;
            let rtt = null;

            try {
              controller = new AbortController();
              const signal = controller.signal;

              const timeoutId = setTimeout(() => controller.abort(), timeoutMs);
              timeouts.push(timeoutId);

              const cacheBuster =
                "cb=" + Date.now() + "-" + Math.random().toString(36).slice(2);
              const url =
                window.location.origin + "/ping-calculator?" + cacheBuster;

              await fetch(url, { method: "HEAD", cache: "no-store", signal });

              const endTime = performance.now();
              rtt = endTime - startTime;
              success = true;

              clearTimeout(timeoutId);
              timeouts = timeouts.filter((id) => id !== timeoutId);
            } catch (e) {
              // timeout or network error
            }

            samples.push({ success, rtt });

            const statusCell = row.children[1];
            const rttCell = row.children[2];

            if (success) {
              statusCell.textContent = "OK";
              statusCell.classList.add("text-green-600");
              rttCell.textContent = rtt.toFixed(1);
            } else {
              statusCell.textContent = "Timeout";
              statusCell.classList.add("text-red-600");
              rttCell.textContent = "–";
            }

            const stats = computeStats(samples, totalPackets);
            updateQualityDisplay(stats);

            if (i < totalPackets - 1 && isRunning) {
              await new Promise((res) => setTimeout(res, intervalMs));
            }
          }

          isRunning = false;
          startBtn.disabled = false;
          stopBtn.disabled = true;

          const finalStats = computeStats(samples, totalPackets);
          if (finalStats.avg == null) {
            setStatus(
              "Test finished: no responses received. Your connection or firewall may be blocking requests.",
              "error"
            );
          } else {
            setStatus(
              "Test finished. Review your latency, jitter, and packet loss above.",
              "idle"
            );
          }
        }

        startBtn.addEventListener("click", () => {
          if (isRunning) return;
          runPingTest();
        });

        stopBtn.addEventListener("click", () => {
          isRunning = false;
          if (controller) controller.abort();
          timeouts.forEach((id) => clearTimeout(id));
          timeouts = [];
          startBtn.disabled = false;
          stopBtn.disabled = true;
          setStatus("Test stopped by user.", "warning");
        });

        // Latency estimator
        const distanceInput = document.getElementById("distance-value");
        const distanceUnit = document.getElementById("distance-unit");
        const theoreticalPingEl = document.getElementById("theoretical-ping");
        const estimateBtn = document.getElementById("estimate-latency-btn");

        estimateBtn.addEventListener("click", () => {
          const d = parseFloat(distanceInput.value);
          if (!d || d <= 0) {
            theoreticalPingEl.textContent = "–";
            return;
          }
          let km = d;
          if (distanceUnit.value === "mi") {
            km = d * 1.60934;
          }
          const v = 200000; // km/s in fiber
          const pingMin = 2 * (km / v) * 1000;
          theoreticalPingEl.textContent = pingMin.toFixed(1);
        });
      })();
    </script>

