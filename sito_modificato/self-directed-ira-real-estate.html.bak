    <script>
      function fmtCurrency(value) {
        if (!isFinite(value)) return "$0";
        const abs = Math.abs(value);
        const sign = value < 0 ? "-" : "";
        return (
          sign + "$" + abs.toFixed(0).replace(/\B(?=(\d{3})+(?!\d))/g, ",")
        );
      }

      function fmtPercent(value) {
        if (!isFinite(value)) return "0%";
        return value.toFixed(1).replace(/\.0$/, "") + "%";
      }

      function approximateIrr(cashFlows) {
        // Simple binary search IRR between -100% and 100%
        let low = -0.99,
          high = 1.0,
          irr = 0;
        for (let iter = 0; iter < 50; iter++) {
          irr = (low + high) / 2;
          let npv = 0;
          for (let t = 0; t < cashFlows.length; t++) {
            npv += cashFlows[t] / Math.pow(1 + irr, t);
          }
          if (npv > 0) {
            low = irr;
          } else {
            high = irr;
          }
        }
        return irr * 100;
      }

      function calculateIraRealEstate() {
        const purchasePrice =
          parseFloat(document.getElementById("purchasePrice").value) || 0;
        const closingCosts =
          parseFloat(document.getElementById("closingCosts").value) || 0;
        const monthlyRent =
          parseFloat(document.getElementById("monthlyRent").value) || 0;
        const vacancyRate =
          (parseFloat(document.getElementById("vacancyRate").value) || 0) / 100;
        const annualExpenses =
          parseFloat(document.getElementById("annualExpenses").value) || 0;
        const expenseGrowth =
          (parseFloat(document.getElementById("expenseGrowth").value) || 0) /
          100;

        const iraCash =
          parseFloat(document.getElementById("iraCash").value) || 0;
        const downPaymentPct =
          (parseFloat(document.getElementById("downPaymentPct").value) || 0) /
          100;
        const interestRate =
          (parseFloat(document.getElementById("interestRate").value) || 0) /
          100;
        const loanTermYears =
          parseFloat(document.getElementById("loanTerm").value) || 0;

        const rentGrowth =
          (parseFloat(document.getElementById("rentGrowth").value) || 0) / 100;
        const appreciation =
          (parseFloat(document.getElementById("appreciation").value) || 0) /
          100;
        const holdingYears =
          parseFloat(document.getElementById("holdingYears").value) || 0;

        const includeUbit = document.getElementById("includeUbit").checked;

        const totalCost = purchasePrice + closingCosts;
        const downPayment = purchasePrice * downPaymentPct;
        const loanAmount = Math.max(purchasePrice - downPayment, 0);
        const iraInvested = downPayment + closingCosts;

        // Check IRA cash sufficiency (soft check)
        if (iraInvested > iraCash && iraCash > 0) {
          // Optional: could show a warning in UI; keep calculator functional
          console.warn("IRA cash is less than required investment.");
        }

        // Loan payment
        const nPayments = loanTermYears * 12;
        const monthlyRate = interestRate / 12;
        let monthlyPayment = 0;
        if (loanAmount > 0 && monthlyRate > 0 && nPayments > 0) {
          const pow = Math.pow(1 + monthlyRate, nPayments);
          monthlyPayment = (loanAmount * (monthlyRate * pow)) / (pow - 1);
        } else if (loanAmount > 0 && monthlyRate === 0 && nPayments > 0) {
          monthlyPayment = loanAmount / nPayments;
        }

        const annualDebtService = monthlyPayment * 12;

        // Year-1 income and NOI
        const egiYear1 = monthlyRent * 12 * (1 - vacancyRate);
        const noiYear1 = egiYear1 - annualExpenses;
        const netCashFlowYear1 = noiYear1 - annualDebtService;

        const cashOnCash =
          iraInvested > 0 ? (netCashFlowYear1 / iraInvested) * 100 : 0;

        // Simplified UBIT exposure share: loan / total cost
        const debtShare =
          totalCost > 0 ? Math.min(Math.max(loanAmount / totalCost, 0), 1) : 0;
        const ubitSharePct = includeUbit ? debtShare * 100 : 0;

        // Projected exit equity
        const futureValue =
          purchasePrice * Math.pow(1 + appreciation, holdingYears || 0);

        // Approximate remaining loan balance after holdingYears
        let remainingBalance = loanAmount;
        if (loanAmount > 0 && monthlyPayment > 0 && monthlyRate >= 0) {
          const months = Math.min(holdingYears * 12, nPayments);
          for (let m = 0; m < months; m++) {
            const interestPortion = remainingBalance * monthlyRate;
            const principalPortion = monthlyPayment - interestPortion;
            remainingBalance -= principalPortion;
            if (remainingBalance <= 0) {
              remainingBalance = 0;
              break;
            }
          }
        }
        const equityExit = futureValue - remainingBalance;

        // IRR approximations: one cash outflow at t=0, then Year-1 CF repeated, then equity at exit
        let leveragedIrr = NaN;
        let allCashIrr = NaN;
        if (iraInvested > 0 && holdingYears > 0) {
          const flowsLeveraged = [-iraInvested];
          for (let y = 1; y <= holdingYears; y++) {
            // grow cash flow with rent growth - expense growth approximation
            const cf =
              netCashFlowYear1 *
              Math.pow(1 + (rentGrowth - expenseGrowth), y - 1);
            flowsLeveraged.push(cf);
          }
          flowsLeveraged[flowsLeveraged.length - 1] += equityExit;
          leveragedIrr = approximateIrr(flowsLeveraged);

          // All-cash scenario: no loan, so NOI is cash flow; IRA invests totalCost
          const flowsAllCash = [-totalCost];
          const noi0 = egiYear1 - annualExpenses;
          for (let y = 1; y <= holdingYears; y++) {
            const noiY =
              noi0 * Math.pow(1 + (rentGrowth - expenseGrowth), y - 1);
            flowsAllCash.push(noiY);
          }
          const futureValueAllCash =
            purchasePrice * Math.pow(1 + appreciation, holdingYears);
          flowsAllCash[flowsAllCash.length - 1] += futureValueAllCash;
          allCashIrr = approximateIrr(flowsAllCash);
        }

        // Update UI
        document.getElementById("resTotalCost").textContent =
          fmtCurrency(totalCost);
        document.getElementById("resIraInvested").textContent =
          fmtCurrency(iraInvested);
        document.getElementById("resLoanAmount").textContent =
          fmtCurrency(loanAmount);
        document.getElementById("resMonthlyPayment").textContent =
          fmtCurrency(monthlyPayment);

        document.getElementById("resYear1CashFlow").textContent =
          fmtCurrency(netCashFlowYear1);
        document.getElementById("resCashOnCash").textContent =
          fmtPercent(cashOnCash);
        document.getElementById("resUbitShare").textContent = includeUbit
          ? fmtPercent(ubitSharePct)
          : "0%";
        document.getElementById("resEquityExit").textContent =
          fmtCurrency(equityExit);

        document.getElementById("resLeveragedIrr").textContent = isFinite(
          leveragedIrr
        )
          ? fmtPercent(leveragedIrr)
          : "–";
        document.getElementById("resAllCashIrr").textContent = isFinite(
          allCashIrr
        )
          ? fmtPercent(allCashIrr)
          : "–";
      }

      document.addEventListener("DOMContentLoaded", function () {
        document
          .getElementById("calculateBtn")
          .addEventListener("click", calculateIraRealEstate);
        // Auto-calc on load with defaults
        calculateIraRealEstate();
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
