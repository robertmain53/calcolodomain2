    <script>
      // Osmolal gap calculator logic
      (function () {
        const naInput = document.getElementById("na");
        const glucoseInput = document.getElementById("glucose");
        const bunUreaInput = document.getElementById("bun-urea");
        const ethanolInput = document.getElementById("ethanol");
        const measuredOsmInput = document.getElementById("measured-osm");

        const formulaModeSelect = document.getElementById("formula-mode");
        const unitRadios = document.querySelectorAll(
          'input[name="unit-system"]'
        );

        const naUnitSpan = document.getElementById("na-unit");
        const glucoseUnitSpan = document.getElementById("glucose-unit");
        const bunUreaUnitSpan = document.getElementById("bun-urea-unit");
        const ethanolUnitSpan = document.getElementById("ethanol-unit");
        const bunUreaLabelSpan = document.getElementById("bun-urea-label");

        const calcOsmDisplay = document.getElementById("calc-osm-display");
        const measuredOsmDisplay = document.getElementById(
          "measured-osm-display"
        );
        const gapDisplay = document.getElementById("gap-display");
        const gapInterpretation = document.getElementById("gap-interpretation");
        const stepByStepDiv = document.getElementById("step-by-step");

        const calcBtn = document.getElementById("calculate-btn");
        const resetBtn = document.getElementById("reset-btn");

        let unitSystem = "si"; // 'si' or 'us'

        function updateUnitLabels() {
          if (unitSystem === "si") {
            naUnitSpan.textContent = "mmol/L";
            glucoseUnitSpan.textContent = "mmol/L";
            ethanolUnitSpan.textContent = "mmol/L";
            bunUreaUnitSpan.textContent = "mmol/L";
            bunUreaLabelSpan.textContent = formulaModeSelect.value.includes(
              "urea"
            )
              ? "(Urea)"
              : "(BUN, converted)";
          } else {
            naUnitSpan.textContent = "mmol/L";
            glucoseUnitSpan.textContent = "mg/dL";
            ethanolUnitSpan.textContent = "mg/dL";
            bunUreaUnitSpan.textContent = formulaModeSelect.value.includes(
              "urea"
            )
              ? "mg/dL (urea)"
              : "mg/dL (BUN)";
            bunUreaLabelSpan.textContent = formulaModeSelect.value.includes(
              "urea"
            )
              ? "(Urea)"
              : "(BUN)";
          }
        }

        function parse(input) {
          const v = parseFloat(input.value);
          return isNaN(v) ? 0 : v;
        }

        function convertToInternal() {
          // Internal representation: Na in mmol/L, glucose in mmol/L, urea in mmol/L, ethanol in mmol/L
          const Na = parse(naInput);

          let glucose = parse(glucoseInput);
          let bunUrea = parse(bunUreaInput);
          let ethanol = parse(ethanolInput);

          if (unitSystem === "us") {
            // mg/dL to mmol/L
            // Glucose: divide by 18
            glucose = glucose / 18;
            // BUN: mg/dL to mmol/L urea nitrogen; for osmolality we use BUN/2.8 in mg/dL formula,
            // but for internal SI we convert to mmol/L of urea nitrogen: divide by 2.8
            // Urea: mg/dL to mmol/L (approx divide by 6)
            if (formulaModeSelect.value.includes("urea")) {
              bunUrea = bunUrea / 6.0;
            } else {
              bunUrea = bunUrea / 2.8;
            }
            // Ethanol: mg/dL to mmol/L (divide by 4.61 approx)
            ethanol = ethanol / 4.61;
          }

          return { Na, glucose, bunUrea, ethanol };
        }

        function calculate() {
          const measuredOsm = parse(measuredOsmInput);
          const { Na, glucose, bunUrea, ethanol } = convertToInternal();
          const mode = formulaModeSelect.value;

          let calcOsm = 0;
          let steps = [];

          if (mode === "bun_no_etoh" || mode === "bun_with_etoh") {
            // Work in US-style terms for clarity in steps
            let NaTerm = 2 * Na;
            // convert back to mg/dL style for explanation
            let glucoseMg =
              unitSystem === "us" ? parse(glucoseInput) : glucose * 18;
            let bunMg =
              unitSystem === "us" ? parse(bunUreaInput) : bunUrea * 2.8;
            let ethanolMg =
              unitSystem === "us" ? parse(ethanolInput) : ethanol * 4.61;

            const glucoseTerm = glucoseMg / 18;
            const bunTerm = bunMg / 2.8;
            let ethanolTerm = 0;

            steps.push(`2 × Na = 2 × ${Na.toFixed(1)} = ${NaTerm.toFixed(1)}`);
            steps.push(
              `Glucose/18 = ${glucoseMg.toFixed(
                1
              )} / 18 = ${glucoseTerm.toFixed(1)}`
            );
            steps.push(
              `BUN/2.8 = ${bunMg.toFixed(1)} / 2.8 = ${bunTerm.toFixed(1)}`
            );

            calcOsm = NaTerm + glucoseTerm + bunTerm;

            if (mode === "bun_with_etoh") {
              ethanolTerm = ethanolMg / 3.7;
              steps.push(
                `Ethanol/3.7 = ${ethanolMg.toFixed(
                  1
                )} / 3.7 = ${ethanolTerm.toFixed(1)}`
              );
              calcOsm += ethanolTerm;
            }
          } else {
            // Urea-based SI formula
            const NaTerm = 2 * Na;
            const glucoseTerm = glucose;
            const ureaTerm = bunUrea;
            steps.push(`2 × Na = 2 × ${Na.toFixed(1)} = ${NaTerm.toFixed(1)}`);
            steps.push(`Glucose = ${glucose.toFixed(1)} mmol/L`);
            steps.push(`Urea = ${bunUrea.toFixed(1)} mmol/L`);
            calcOsm = NaTerm + glucoseTerm + ureaTerm;

            if (mode === "urea_with_etoh") {
              steps.push(`Ethanol = ${ethanol.toFixed(1)} mmol/L`);
              calcOsm += ethanol;
            }
          }

          const gap = measuredOsm - calcOsm;

          calcOsmDisplay.textContent = calcOsm.toFixed(1);
          measuredOsmDisplay.textContent = isNaN(measuredOsm)
            ? "—"
            : measuredOsm.toFixed(1);
          gapDisplay.textContent = isNaN(gap) ? "—" : gap.toFixed(1);

          // Interpretation
          if (isNaN(gap)) {
            gapInterpretation.textContent =
              "Please enter a valid measured serum osmolality to interpret the osmolal gap.";
          } else {
            const absGap = Math.abs(gap);
            let msg = `Osmolal gap = measured (${measuredOsm.toFixed(
              1
            )}) − calculated (${calcOsm.toFixed(1)}) = ${gap.toFixed(
              1
            )} mOsm/kg. `;

            if (absGap <= 10) {
              msg +=
                "This is within the commonly accepted normal range (approximately −10 to +10 mOsm/kg). Correlate with the clinical picture.";
            } else if (absGap <= 20) {
              msg +=
                "This is mildly elevated. Consider unmeasured osmoles, laboratory variation, and formula differences. Interpret together with anion gap, acid–base status, and clinical context.";
            } else {
              msg +=
                "This is significantly elevated (>20 mOsm/kg). In the appropriate clinical context (e.g., high anion gap metabolic acidosis, history of ingestion), this raises concern for toxic alcohols or other exogenous osmoles. Urgent evaluation and toxicology consultation are recommended.";
            }
            gapInterpretation.textContent = msg;
          }

          // Step-by-step
          let stepHtml = "";
          stepHtml += `<p><strong>Step 1 – Choose formula:</strong> ${mode.replace(
            /_/g,
            " "
          )}</p>`;
          stepHtml += "<p><strong>Step 2 – Plug in values:</strong></p><ul>";
          steps.forEach((s) => {
            stepHtml += `<li>${s}</li>`;
          });
          stepHtml += "</ul>";
          stepHtml += `<p><strong>Step 3 – Calculated osmolality:</strong> ${calcOsm.toFixed(
            1
          )} mOsm/kg</p>`;
          if (!isNaN(measuredOsm)) {
            stepHtml += `<p><strong>Step 4 – Osmolal gap:</strong> ${measuredOsm.toFixed(
              1
            )} − ${calcOsm.toFixed(1)} = ${
              isNaN(gap) ? "—" : gap.toFixed(1)
            } mOsm/kg</p>`;
          }
          stepByStepDiv.innerHTML = stepHtml;
        }

        function reset() {
          naInput.value = 140;
          glucoseInput.value = unitSystem === "us" ? 90 : 5;
          bunUreaInput.value = unitSystem === "us" ? 14 : 5;
          ethanolInput.value = 0;
          measuredOsmInput.value = 290;
          calcOsmDisplay.textContent = "290.0";
          measuredOsmDisplay.textContent = "290.0";
          gapDisplay.textContent = "0.0";
          gapInterpretation.textContent =
            "Enter values and click “Calculate” to see interpretation.";
          stepByStepDiv.innerHTML = "";
        }

        calcBtn.addEventListener("click", calculate);
        resetBtn.addEventListener("click", reset);

        unitRadios.forEach((r) => {
          r.addEventListener("change", (e) => {
            unitSystem = e.target.value;
            updateUnitLabels();
          });
        });

        formulaModeSelect.addEventListener("change", updateUnitLabels);

        // Initialize
        updateUnitLabels();
        calculate();
      })();
    </script>

