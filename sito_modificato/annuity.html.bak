    <script>
      (function () {
        const modeEl = document.getElementById("mode");
        const annuityTypeEl = document.getElementById("annuityType");
        const interestRateEl = document.getElementById("interestRate");
        const paymentsPerYearEl = document.getElementById("paymentsPerYear");
        const yearsEl = document.getElementById("years");
        const periodsEl = document.getElementById("periods");
        const paymentEl = document.getElementById("payment");
        const pvEl = document.getElementById("pv");
        const fvEl = document.getElementById("fv");

        const inputPaymentRow = document.getElementById("input-payment");
        const inputPvRow = document.getElementById("input-pv");
        const inputFvRow = document.getElementById("input-fv");

        const calculateBtn = document.getElementById("calculateBtn");
        const resetBtn = document.getElementById("resetBtn");

        const resultsEl = document.getElementById("results");
        const resultLabelEl = document.getElementById("resultLabel");
        const resultValueEl = document.getElementById("resultValue");
        const summaryNEl = document.getElementById("summaryN");
        const summaryTotalPaidEl = document.getElementById("summaryTotalPaid");
        const summaryInterestEl = document.getElementById("summaryInterest");
        const resultNoteEl = document.getElementById("resultNote");

        function formatMoney(value) {
          if (!isFinite(value)) return "—";
          return value.toLocaleString(undefined, {
            minimumFractionDigits: 2,
            maximumFractionDigits: 2,
          });
        }

        function getN() {
          const p = parseFloat(periodsEl.value);
          if (!isNaN(p) && p > 0) return p;
          const yrs = parseFloat(yearsEl.value);
          const m = parseFloat(paymentsPerYearEl.value);
          if (isNaN(yrs) || yrs <= 0 || isNaN(m) || m <= 0) return null;
          return yrs * m;
        }

        function getRatePerPeriod() {
          const r = parseFloat(interestRateEl.value);
          const m = parseFloat(paymentsPerYearEl.value);
          if (isNaN(r) || m <= 0) return null;
          return r / 100 / m;
        }

        function updateInputVisibility() {
          const mode = modeEl.value;
          inputPaymentRow.classList.add("hidden");
          inputPvRow.classList.add("hidden");
          inputFvRow.classList.add("hidden");

          if (mode === "payment") {
            // user provides PV or FV depending on scenario; show both for flexibility
            inputPvRow.classList.remove("hidden");
            inputFvRow.classList.remove("hidden");
          } else if (mode === "pv") {
            inputPaymentRow.classList.remove("hidden");
          } else if (mode === "fv") {
            inputPaymentRow.classList.remove("hidden");
          }
          resultsEl.classList.add("hidden");
        }

        modeEl.addEventListener("change", updateInputVisibility);

        resetBtn.addEventListener("click", function () {
          modeEl.value = "payment";
          annuityTypeEl.value = "ordinary";
          interestRateEl.value = 5;
          paymentsPerYearEl.value = 12;
          yearsEl.value = 20;
          periodsEl.value = "";
          paymentEl.value = 1000;
          if (pvEl) pvEl.value = 100000;
          if (fvEl) fvEl.value = 500000;
          updateInputVisibility();
          resultsEl.classList.add("hidden");
        });

        calculateBtn.addEventListener("click", function () {
          const mode = modeEl.value;
          const type = annuityTypeEl.value;
          const n = getN();
          const i = getRatePerPeriod();

          if (!n || n <= 0) {
            alert("Please enter a valid number of years or total periods.");
            return;
          }
          if (i === null || i < 0) {
            alert("Please enter a valid interest rate.");
            return;
          }

          const isZeroRate = Math.abs(i) < 1e-10;
          let PMT = null,
            PV = null,
            FV = null;

          if (mode === "payment") {
            const pvInput = parseFloat(pvEl && pvEl.value);
            const fvInput = parseFloat(fvEl && fvEl.value);
            if (
              (isNaN(pvInput) || pvInput <= 0) &&
              (isNaN(fvInput) || fvInput <= 0)
            ) {
              alert(
                "Please enter a present value and/or future value to solve for the payment."
              );
              return;
            }

            if (!isNaN(pvInput) && pvInput > 0) {
              if (isZeroRate) {
                PMT = pvInput / n;
              } else {
                const iEff = i;
                let pmtOrd = pvInput * (iEff / (1 - Math.pow(1 + iEff, -n)));
                if (type === "due") {
                  PMT = pmtOrd / (1 + iEff);
                } else {
                  PMT = pmtOrd;
                }
              }
              PV = pvInput;
            }

            if (!isNaN(fvInput) && fvInput > 0) {
              if (isZeroRate) {
                const pmtFromFv = fvInput / n;
                PMT = PMT === null ? pmtFromFv : Math.max(PMT, pmtFromFv);
              } else {
                const iEff = i;
                let pmtOrdFv = fvInput * (iEff / (Math.pow(1 + iEff, n) - 1));
                if (type === "due") {
                  pmtOrdFv = pmtOrdFv / (1 + iEff);
                }
                PMT = PMT === null ? pmtOrdFv : Math.max(PMT, pmtOrdFv);
              }
              FV = fvInput;
            }

            if (PMT === null || !isFinite(PMT)) {
              alert("Unable to compute payment with the given inputs.");
              return;
            }

            if (PV === null) {
              if (isZeroRate) {
                PV = PMT * n;
              } else {
                const iEff = i;
                let pvOrd = PMT * ((1 - Math.pow(1 + iEff, -n)) / iEff);
                PV = type === "due" ? pvOrd * (1 + iEff) : pvOrd;
              }
            }
            if (FV === null) {
              if (isZeroRate) {
                FV = PMT * n;
              } else {
                const iEff = i;
                let fvOrd = PMT * ((Math.pow(1 + iEff, n) - 1) / iEff);
                FV = type === "due" ? fvOrd * (1 + iEff) : fvOrd;
              }
            }

            resultLabelEl.textContent = "Required periodic payment (PMT)";
            resultValueEl.textContent = "$" + formatMoney(PMT);
            summaryNEl.textContent = n.toFixed(0);
            const totalPaid = PMT * n;
            summaryTotalPaidEl.textContent = "$" + formatMoney(totalPaid);
            const principalBasis =
              PV > 0 ? PV : FV > 0 ? FV / Math.pow(1 + i, n) : 0;
            const interest = totalPaid - principalBasis;
            summaryInterestEl.textContent = "$" + formatMoney(interest);
            resultNoteEl.textContent =
              "Payments are " +
              (type === "due"
                ? "at the beginning of each period (annuity due)."
                : "at the end of each period (ordinary annuity).");
          } else if (mode === "pv") {
            const pmtInput = parseFloat(paymentEl.value);
            if (isNaN(pmtInput) || pmtInput <= 0) {
              alert("Please enter a valid periodic payment amount.");
              return;
            }
            PMT = pmtInput;
            if (isZeroRate) {
              PV = PMT * n;
            } else {
              const iEff = i;
              let pvOrd = PMT * ((1 - Math.pow(1 + iEff, -n)) / iEff);
              PV = type === "due" ? pvOrd * (1 + iEff) : pvOrd;
            }
            const totalPaid = PMT * n;
            const interest = totalPaid - PV;

            resultLabelEl.textContent = "Present value (PV) of the annuity";
            resultValueEl.textContent = "$" + formatMoney(PV);
            summaryNEl.textContent = n.toFixed(0);
            summaryTotalPaidEl.textContent = "$" + formatMoney(totalPaid);
            summaryInterestEl.textContent = "$" + formatMoney(interest);
            resultNoteEl.textContent =
              "This is the lump sum equivalent of the payment stream, discounted at the given interest rate.";
          } else if (mode === "fv") {
            const pmtInput = parseFloat(paymentEl.value);
            if (isNaN(pmtInput) || pmtInput <= 0) {
              alert("Please enter a valid periodic payment amount.");
              return;
            }
            PMT = pmtInput;
            if (isZeroRate) {
              FV = PMT * n;
            } else {
              const iEff = i;
              let fvOrd = PMT * ((Math.pow(1 + iEff, n) - 1) / iEff);
              FV = type === "due" ? fvOrd * (1 + iEff) : fvOrd;
            }
            const totalPaid = PMT * n;
            const interest = FV - totalPaid;

            resultLabelEl.textContent = "Future value (FV) of the annuity";
            resultValueEl.textContent = "$" + formatMoney(FV);
            summaryNEl.textContent = n.toFixed(0);
            summaryTotalPaidEl.textContent = "$" + formatMoney(totalPaid);
            summaryInterestEl.textContent = "$" + formatMoney(interest);
            resultNoteEl.textContent =
              "This is the accumulated value of all payments at the end of the term.";
          }

          resultsEl.classList.remove("hidden");
        });

        updateInputVisibility();
      })();
    </script>

