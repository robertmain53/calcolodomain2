      function updateShapeFields() {
        const shape = shapeSelect.value;
        dimLengthGroup.classList.add("hidden");
        dimWidthGroup.classList.add("hidden");
        dimDiameterGroup.classList.add("hidden");
        dimThicknessGroup.classList.add("hidden");
        dimVolumeGroup.classList.add("hidden");

        if (shape === "slab") {
          dimLengthGroup.classList.remove("hidden");
          dimWidthGroup.classList.remove("hidden");
          dimThicknessGroup.classList.remove("hidden");
        } else if (shape === "footing") {
          dimLengthGroup.classList.remove("hidden");
          dimWidthGroup.classList.remove("hidden");
          dimThicknessGroup.classList.remove("hidden");
        } else if (shape === "column") {
          dimDiameterGroup.classList.remove("hidden");
          dimThicknessGroup.classList.remove("hidden");
        } else if (shape === "volume") {
          dimVolumeGroup.classList.remove("hidden");
        }
        resultsSection.classList.add("hidden");
      }

      function updateMixType() {
        const type = mixTypeSelect.value;
        if (type === "mortar") {
          ratioAggregateGroup.classList.add("hidden");
          ratioAggregateInput.value = 0;
          if (parseFloat(ratioSandInput.value) <= 0) ratioSandInput.value = 4;
        } else {
          ratioAggregateGroup.classList.remove("hidden");
          if (parseFloat(ratioAggregateInput.value) <= 0)
            ratioAggregateInput.value = 3;
          if (parseFloat(ratioSandInput.value) <= 0) ratioSandInput.value = 2;
        }
        resultsSection.classList.add("hidden");
      }

      function parsePositive(input) {
        const v = parseFloat(input.value);
        return isFinite(v) && v > 0 ? v : NaN;
      }

      function calculateVolume() {
        const shape = shapeSelect.value;
        let vol_m3 = NaN;

        if (shape === "volume") {
          const V = parsePositive(knownVolumeInput);
          if (isNaN(V)) return NaN;
          if (currentUnit === "imperial") {
            // known volume in yd³ -> m³
            vol_m3 = V * 0.764554857984;
          } else {
            vol_m3 = V;
          }
        } else if (shape === "slab" || shape === "footing") {
          const L = parsePositive(lengthInput);
          const W = parsePositive(widthInput);
          const T = parsePositive(thicknessInput);
          if (isNaN(L) || isNaN(W) || isNaN(T)) return NaN;
          if (currentUnit === "imperial") {
            // ft³ -> m³
            const vol_ft3 = L * W * T;
            vol_m3 = vol_ft3 * 0.028316846592;
          } else {
            vol_m3 = L * W * T;
          }
        } else if (shape === "column") {
          const D = parsePositive(diameterInput);
          const H = parsePositive(thicknessInput);
          if (isNaN(D) || isNaN(H)) return NaN;
          if (currentUnit === "imperial") {
            const vol_ft3 = Math.PI * Math.pow(D / 2, 2) * H;
            vol_m3 = vol_ft3 * 0.028316846592;
          } else {
            vol_m3 = Math.PI * Math.pow(D / 2, 2) * H;
          }
        }
        return vol_m3;
      }

      function calculate() {
        const wastePct = parseFloat(wasteInput.value);
        const wasteFactor =
          isFinite(wastePct) && wastePct >= 0 ? 1 + wastePct / 100 : 1;

        const V_total_m3 = calculateVolume();
        if (!isFinite(V_total_m3) || V_total_m3 <= 0) {
          alert("Please enter valid dimensions or volume.");
          return;
        }

        // Display total volume in main and alternate units
        let volMain, volAlt;
        if (currentUnit === "imperial") {
          const V_ft3 = V_total_m3 / 0.028316846592;
          const V_yd3 = V_ft3 / 27;
          volMain = fmt(V_yd3, 3, "yd³");
          volAlt = fmt(V_ft3, 2, "ft³") + " (" + fmt(V_total_m3, 3, "m³") + ")";
        } else {
          const V_ft3 = V_total_m3 / 0.028316846592;
          const V_yd3 = V_ft3 / 27;
          volMain = fmt(V_total_m3, 3, "m³");
          volAlt = fmt(V_yd3, 3, "yd³") + " (" + fmt(V_ft3, 2, "ft³") + ")";
        }
        resVolumeMain.textContent = volMain;
        resVolumeAlt.textContent = volAlt;

        if (currentMode === "separate") {
          const C = parseFloat(ratioCementInput.value);
          const S = parseFloat(ratioSandInput.value);
          const G = parseFloat(ratioAggregateInput.value);
          if (
            !isFinite(C) ||
            C <= 0 ||
            !isFinite(S) ||
            S < 0 ||
            !isFinite(G) ||
            G < 0
          ) {
            alert("Please enter a valid mix ratio.");
            return;
          }
          const totalParts = C + S + G;
          const V_cement_m3 = V_total_m3 * (C / totalParts);
          const V_sand_m3 = V_total_m3 * (S / totalParts);
          const V_agg_m3 = V_total_m3 * (G / totalParts);

          const rho_cement = parseFloat(densityCementInput.value);
          const m_cement_kg =
            V_cement_m3 *
            (isFinite(rho_cement) && rho_cement > 0 ? rho_cement : 1506);

          const bagSize = parseFloat(bagSizeInput.value);
          if (!isFinite(bagSize) || bagSize <= 0) {
            alert("Please enter a valid cement bag size.");
            return;
          }

          let bagsRaw;
          if (currentUnit === "imperial") {
            // bagSize in lb -> kg
            const bag_kg = bagSize * 0.45359237;
            bagsRaw = m_cement_kg / bag_kg;
          } else {
            // bagSize in kg
            bagsRaw = m_cement_kg / bagSize;
          }
          const bagsFinal = Math.ceil(bagsRaw * wasteFactor);

          // Display cement volume
          if (currentUnit === "imperial") {
            const V_cement_ft3 = V_cement_m3 / 0.028316846592;
            resCementVolume.textContent =
              fmt(V_cement_ft3, 2, "ft³") +
              " (" +
              fmt(V_cement_m3, 3, "m³") +
              ")";
            resCementMass.textContent =
              fmt(m_cement_kg * 2.2046226218, 1, "lb") +
              " (" +
              fmt(m_cement_kg, 1, "kg") +
              ")";
          } else {
            const V_cement_ft3 = V_cement_m3 / 0.028316846592;
            resCementVolume.textContent =
              fmt(V_cement_m3, 3, "m³") +
              " (" +
              fmt(V_cement_ft3, 2, "ft³") +
              ")";
            resCementMass.textContent =
              fmt(m_cement_kg, 1, "kg") +
              " (" +
              fmt(m_cement_kg * 2.2046226218, 1, "lb") +
              ")";
          }
          resCementBags.textContent =
            bagsFinal + " bag" + (bagsFinal !== 1 ? "s" : "");

          // Aggregates
          if (currentUnit === "imperial") {
            const V_sand_ft3 = V_sand_m3 / 0.028316846592;
            const V_agg_ft3 = V_agg_m3 / 0.028316846592;
            resSandVolume.textContent =
              fmt(V_sand_ft3, 2, "ft³") + " (" + fmt(V_sand_m3, 3, "m³") + ")";
            resAggregateVolume.textContent =
              fmt(V_agg_ft3, 2, "ft³") + " (" + fmt(V_agg_m3, 3, "m³") + ")";
          } else {
            const V_sand_ft3 = V_sand_m3 / 0.028316846592;
            const V_agg_ft3 = V_agg_m3 / 0.028316846592;
            resSandVolume.textContent =
              fmt(V_sand_m3, 3, "m³") + " (" + fmt(V_sand_ft3, 2, "ft³") + ")";
            resAggregateVolume.textContent =
              fmt(V_agg_m3, 3, "m³") + " (" + fmt(V_agg_ft3, 2, "ft³") + ")";
          }

          resAggregatesCard.classList.remove("hidden");
          resPremixCard.classList.add("hidden");
        } else {
          // Premix mode
          const V_bag = parseFloat(premixYieldInput.value);
          if (!isFinite(V_bag) || V_bag <= 0) {
            alert("Please enter a valid premix yield per bag.");
            return;
          }
          let V_total_in_bag_units;
          if (currentUnit === "imperial") {
            // V_bag in ft³, V_total_m3 -> ft³
            const V_total_ft3 = V_total_m3 / 0.028316846592;
            V_total_in_bag_units = V_total_ft3;
          } else {
            // V_bag in m³, V_total_m3 already m³
            V_total_in_bag_units = V_total_m3;
          }
          const bagsRaw = V_total_in_bag_units / V_bag;
          const bagsFinal = Math.ceil(bagsRaw * wasteFactor);
          resPremixBags.textContent =
            bagsFinal + " bag" + (bagsFinal !== 1 ? "s" : "");

          // For premix, we can still approximate cement content using densityPremix if desired
          const rho_premix = parseFloat(densityPremixInput.value);
          const m_premix_kg =
            V_total_m3 *
            (isFinite(rho_premix) && rho_premix > 0 ? rho_premix : 2400);
          if (currentUnit === "imperial") {
            resCementVolume.textContent = "—";
            resCementMass.textContent = fmt(
              m_premix_kg * 2.2046226218,
              1,
              "lb total mix"
            );
          } else {
            resCementVolume.textContent = "—";
            resCementMass.textContent = fmt(m_premix_kg, 1, "kg total mix");
          }
          resCementBags.textContent = "—";
          resAggregatesCard.classList.add("hidden");
          resPremixCard.classList.remove("hidden");
        }

        resultsSection.classList.remove("hidden");
      }

      function resetForm() {
        document.getElementById("cement-form").reset();
        setMode("separate");
        setUnit("imperial");
        updateShapeFields();
        updateMixType();
        resultsSection.classList.add("hidden");
      }

      modeSeparateBtn.addEventListener("click", () => setMode("separate"));
      modePremixBtn.addEventListener("click", () => setMode("premix"));
      unitImperialBtn.addEventListener("click", () => setUnit("imperial"));
      unitMetricBtn.addEventListener("click", () => setUnit("metric"));
      shapeSelect.addEventListener("change", updateShapeFields);
      mixTypeSelect.addEventListener("change", updateMixType);
      calculateBtn.addEventListener("click", calculate);
      resetBtn.addEventListener("click", resetForm);

      // Initialize defaults
      setMode("separate");
      setUnit("imperial");
      updateShapeFields();
      updateMixType;
    </script>

