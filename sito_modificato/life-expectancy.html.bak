    <script>
      // Simple baseline life expectancy model
      function getBaselineLEAtBirth(sex, region) {
        const table = {
          high: { female: 84, male: 80 },
          upper: { female: 79, male: 74 },
          lower: { female: 74, male: 70 },
          low: { female: 69, male: 65 },
        };
        return (table[region] || table.high)[sex] || 80;
      }

      // Approximate conditional life expectancy at current age:
      // LE(age) ≈ age + (LE_birth - age) * k, with k slightly > 1 to reflect survival of higher-risk early years.
      function estimateBaselineAgeAtDeath(currentAge, sex, region) {
        const leBirth = getBaselineLEAtBirth(sex, region);
        const k = 1.08; // modest survival adjustment
        const remaining = Math.max(0, (leBirth - currentAge) * k);
        return Math.min(105, currentAge + remaining);
      }

      function getAdjustments(inputs) {
        const factors = [];

        // Smoking
        let smokeAdj = 0;
        if (inputs.smoking === "never") smokeAdj = 0;
        else if (inputs.smoking === "former") smokeAdj = -1.5;
        else if (inputs.smoking === "light") smokeAdj = -4;
        else if (inputs.smoking === "heavy") smokeAdj = -8;
        if (smokeAdj !== 0) {
          factors.push({
            label: "Smoking",
            delta: smokeAdj,
            explanation:
              smokeAdj < 0
                ? "Current or past smoking is associated with higher mortality and shorter life expectancy."
                : "Not smoking avoids a major risk factor for early death.",
          });
        } else {
          factors.push({
            label: "Smoking",
            delta: 0,
            explanation:
              "Not smoking keeps your risk closer to or below the population average.",
          });
        }

        // Physical activity
        let actAdj = 0;
        if (inputs.activity === "sedentary") actAdj = -3;
        else if (inputs.activity === "light") actAdj = -1;
        else if (inputs.activity === "moderate") actAdj = +1.5;
        else if (inputs.activity === "high") actAdj = +2.5;
        if (actAdj !== 0) {
          factors.push({
            label: "Physical activity",
            delta: actAdj,
            explanation:
              actAdj > 0
                ? "Regular exercise is linked to lower cardiovascular and all-cause mortality."
                : "Low activity levels are associated with higher risk of chronic disease and earlier death.",
          });
        } else {
          factors.push({
            label: "Physical activity",
            delta: 0,
            explanation:
              "Your activity level is roughly in line with population averages.",
          });
        }

        // Sleep
        let sleepAdj = 0;
        if (inputs.sleep === "short") sleepAdj = -2;
        else if (inputs.sleep === "normal") sleepAdj = +0.5;
        else if (inputs.sleep === "long") sleepAdj = -1;
        if (sleepAdj !== 0) {
          factors.push({
            label: "Sleep duration",
            delta: sleepAdj,
            explanation:
              sleepAdj > 0
                ? "Sleeping 6–8 hours is associated with the lowest mortality in many studies."
                : "Very short or very long sleep is linked to higher mortality risk.",
          });
        } else {
          factors.push({
            label: "Sleep duration",
            delta: 0,
            explanation:
              "Your sleep duration is similar to population averages.",
          });
        }

        // Alcohol
        let alcAdj = 0;
        if (inputs.alcohol === "none") alcAdj = +0.5;
        else if (inputs.alcohol === "moderate") alcAdj = 0;
        else if (inputs.alcohol === "high") alcAdj = -2.5;
        if (alcAdj !== 0) {
          factors.push({
            label: "Alcohol intake",
            delta: alcAdj,
            explanation:
              alcAdj > 0
                ? "Avoiding heavy drinking reduces risk of liver disease, cancers and injuries."
                : "High alcohol intake is associated with increased risk of several diseases and accidents.",
          });
        } else {
          factors.push({
            label: "Alcohol intake",
            delta: 0,
            explanation:
              "Moderate intake is treated as roughly neutral in this simplified model.",
          });
        }

        // BMI / weight
        let bmiAdj = 0;
        if (inputs.bmi === "normal") bmiAdj = +1;
        else if (inputs.bmi === "overweight") bmiAdj = -0.5;
        else if (inputs.bmi === "obese1") bmiAdj = -2;
        else if (inputs.bmi === "obese2") bmiAdj = -4;
        else if (inputs.bmi === "under") bmiAdj = -2;
        if (bmiAdj !== 0) {
          factors.push({
            label: "Body weight (BMI)",
            delta: bmiAdj,
            explanation:
              bmiAdj > 0
                ? "A BMI in the normal range is associated with lower risk of many chronic diseases."
                : "Being significantly under- or overweight increases risk of cardiovascular disease, diabetes and other conditions.",
          });
        } else {
          factors.push({
            label: "Body weight (BMI)",
            delta: 0,
            explanation:
              "Your BMI category is treated as roughly neutral in this model.",
          });
        }

        // Health status
        let healthAdj = 0;
        if (inputs.health === "good") healthAdj = +1;
        else if (inputs.health === "moderate") healthAdj = -1.5;
        else if (inputs.health === "poor") healthAdj = -5;
        if (healthAdj !== 0) {
          factors.push({
            label: "Existing health conditions",
            delta: healthAdj,
            explanation:
              healthAdj > 0
                ? "No major chronic disease is associated with lower near-term mortality risk."
                : "Serious or multiple chronic conditions can substantially reduce life expectancy.",
          });
        } else {
          factors.push({
            label: "Existing health conditions",
            delta: 0,
            explanation:
              "Your reported health status is treated as neutral in this model.",
          });
        }

        // Stress
        let stressAdj = 0;
        if (inputs.stress === "low") stressAdj = +0.5;
        else if (inputs.stress === "medium") stressAdj = 0;
        else if (inputs.stress === "high") stressAdj = -1;
        if (stressAdj !== 0) {
          factors.push({
            label: "Perceived stress",
            delta: stressAdj,
            explanation:
              stressAdj > 0
                ? "Manageable stress and good coping strategies are associated with better health outcomes."
                : "Chronic high stress is linked to higher risk of cardiovascular disease and mental health issues.",
          });
        } else {
          factors.push({
            label: "Perceived stress",
            delta: 0,
            explanation:
              "Moderate stress is common and treated as neutral in this simplified model.",
          });
        }

        return factors;
      }

      function calculateLifeExpectancy() {
        const sex = document.getElementById("sex").value;
        const age = parseFloat(document.getElementById("age").value);
        const country = document.getElementById("country").value;
        const health = document.getElementById("health").value;
        const smoking = document.getElementById("smoking").value;
        const activity = document.getElementById("activity").value;
        const sleep = document.getElementById("sleep").value;
        const alcohol = document.getElementById("alcohol").value;
        const bmi = document.getElementById("bmi").value;
        const stress = document.getElementById("stress").value;

        if (isNaN(age) || age < 0 || age > 110) {
          alert("Please enter a valid age between 0 and 110.");
          return;
        }

        const baselineAgeAtDeath = estimateBaselineAgeAtDeath(
          age,
          sex,
          country
        );
        const baselineRemaining = Math.max(0, baselineAgeAtDeath - age);

        const factors = getAdjustments({
          smoking,
          activity,
          sleep,
          alcohol,
          bmi,
          health,
          stress,
        });
        let totalAdj = factors.reduce((sum, f) => sum + f.delta, 0);

        // Cap total adjustment to avoid unrealistic extremes
        totalAdj = Math.max(-15, Math.min(10, totalAdj));

        let adjustedAgeAtDeath = baselineAgeAtDeath + totalAdj;
        adjustedAgeAtDeath = Math.max(
          age + 1,
          Math.min(110, adjustedAgeAtDeath)
        );
        const adjustedRemaining = Math.max(0, adjustedAgeAtDeath - age);

        // Simple uncertainty range: ±20% of remaining years, min 3 years
        const spread = Math.max(3, adjustedRemaining * 0.2);
        const low = Math.max(age + 1, adjustedAgeAtDeath - spread);
        const high = Math.min(110, adjustedAgeAtDeath + spread);

        // Longevity score: 50 + scaled adjustment
        const longevityScore = Math.round(50 + (totalAdj / 15) * 40); // roughly 10–90
        const scoreClamped = Math.max(5, Math.min(95, longevityScore));

        // Update UI
        document.getElementById("resultsSection").classList.remove("hidden");
        document.getElementById("resultAgeAtDeath").textContent =
          adjustedAgeAtDeath.toFixed(1) + " yrs";
        document.getElementById("resultYearsRemaining").textContent =
          adjustedRemaining.toFixed(1) + " yrs";
        document.getElementById("resultAgeRange").textContent =
          "Typical range: " + low.toFixed(0) + "–" + high.toFixed(0) + " yrs";

        document.getElementById("resultBaseline").textContent =
          "Baseline (population average): " +
          baselineAgeAtDeath.toFixed(1) +
          " yrs";
        document.getElementById("resultAdjusted").textContent =
          "Adjusted for your profile: " +
          adjustedAgeAtDeath.toFixed(1) +
          " yrs";

        const diff = adjustedAgeAtDeath - baselineAgeAtDeath;
        let diffText = "Difference: ";
        if (Math.abs(diff) < 0.25)
          diffText +=
            "about the same as the population average for your age and sex.";
        else if (diff > 0)
          diffText +=
            "+" +
            diff.toFixed(1) +
            " years vs. population average (favorable profile).";
        else
          diffText +=
            diff.toFixed(1) +
            " years vs. population average (higher risk profile).";
        document.getElementById("resultDelta").textContent = diffText;

        // Factor list
        const factorList = document.getElementById("factorList");
        factorList.innerHTML = "";
        factors.forEach((f) => {
          const row = document.createElement("div");
          row.className = "flex items-start justify-between gap-3";
          const label = document.createElement("div");
          label.innerHTML =
            '<p class="font-medium">' +
            f.label +
            '</p><p class="text-xs text-gray-600">' +
            f.explanation +
            "</p>";
          const badge = document.createElement("span");
          badge.className =
            "pill px-2 py-0.5 text-xs font-semibold " +
            (f.delta > 0
              ? "bg-green-100 text-green-700"
              : f.delta < 0
              ? "bg-red-100 text-red-700"
              : "bg-gray-100 text-gray-700");
          badge.textContent =
            (f.delta > 0 ? "+" : "") + f.delta.toFixed(1) + " yrs";
          row.appendChild(label);
          row.appendChild(badge);
          factorList.appendChild(row);
        });

        // Longevity score
        const scoreBar = document.getElementById("longevityScoreBar");
        const scoreLabel = document.getElementById("longevityScoreLabel");
        scoreBar.value = scoreClamped;
        let labelText = scoreClamped + " / 100 (";
        if (scoreClamped < 35) labelText += "below average)";
        else if (scoreClamped <= 65) labelText += "around average)";
        else labelText += "above average)";
        scoreLabel.textContent = labelText;
      }

      document
        .getElementById("calculateBtn")
        .addEventListener("click", calculateLifeExpectancy);

      document
        .getElementById("resetBtn")
        .addEventListener("click", function () {
          document.getElementById("sex").value = "female";
          document.getElementById("age").value = 40;
          document.getElementById("country").value = "high";
          document.getElementById("health").value = "good";
          document.getElementById("smoking").value = "never";
          document.getElementById("activity").value = "moderate";
          document.getElementById("sleep").value = "normal";
          document.getElementById("alcohol").value = "moderate";
          document.getElementById("bmi").value = "normal";
          document.getElementById("stress").value = "medium";
          document.getElementById("resultsSection").classList.add("hidden");
        });

      // Auto-calc on load with defaults
      window.addEventListener("DOMContentLoaded", () => {
        calculateLifeExpectancy();
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
