    <script>
      // Utility: clamp and parse
      function getNumber(id) {
        const el = document.getElementById(id);
        if (!el) return NaN;
        const v = parseFloat(el.value.replace(",", "."));
        return isNaN(v) ? NaN : v;
      }

      function formatCurrency(value, symbol) {
        if (!isFinite(value)) return "";
        const abs = Math.abs(value);
        const decimals = abs >= 100 ? 0 : abs >= 10 ? 1 : 2;
        return symbol + value.toFixed(decimals);
      }

      function formatNumber(value, decimals = 2) {
        if (!isFinite(value)) return "";
        return value.toFixed(decimals);
      }

      // Convert fuel economy to L/100km
      function economyToLPer100km(value, unit) {
        if (!isFinite(value) || value <= 0) return NaN;
        switch (unit) {
          case "mpg_us":
            return 235.214583 / value;
          case "mpg_uk":
            return 282.480936 / value;
          case "l_per_100km":
            return value;
          case "km_per_l":
            return 100 / value;
          default:
            return NaN;
        }
      }

      // Convert fuel price to price per liter
      function pricePerLiter(price, unit) {
        if (!isFinite(price) || price <= 0) return NaN;
        const L_PER_US_GAL = 3.785411784;
        const L_PER_UK_GAL = 4.54609;
        switch (unit) {
          case "per_liter":
            return price;
          case "per_gallon_us":
            return price / L_PER_US_GAL;
          case "per_gallon_uk":
            return price / L_PER_UK_GAL;
          default:
            return NaN;
        }
      }

      // Tabs
      const tabTrip = document.getElementById("tab-trip");
      const tabUsage = document.getElementById("tab-usage");
      const panelTrip = document.getElementById("panel-trip");
      const panelUsage = document.getElementById("panel-usage");

      function activateTab(tab) {
        if (tab === "trip") {
          tabTrip.classList.add(
            "bg-white",
            "border",
            "border-b-0",
            "border-gray-200",
            "text-blue-600"
          );
          tabTrip.classList.remove("text-gray-600");
          tabUsage.classList.remove(
            "bg-white",
            "border",
            "border-b-0",
            "border-gray-200",
            "text-blue-600"
          );
          tabUsage.classList.add("text-gray-600");
          panelTrip.classList.remove("hidden");
          panelUsage.classList.add("hidden");
        } else {
          tabUsage.classList.add(
            "bg-white",
            "border",
            "border-b-0",
            "border-gray-200",
            "text-blue-600"
          );
          tabUsage.classList.remove("text-gray-600");
          tabTrip.classList.remove(
            "bg-white",
            "border",
            "border-b-0",
            "border-gray-200",
            "text-blue-600"
          );
          tabTrip.classList.add("text-gray-600");
          panelUsage.classList.remove("hidden");
          panelTrip.classList.add("hidden");
        }
      }

      tabTrip.addEventListener("click", () => activateTab("trip"));
      tabUsage.addEventListener("click", () => activateTab("usage"));

      // Advanced toggle
      const advancedToggle = document.getElementById("advanced-toggle");
      const advancedSection = document.getElementById("advanced-section");
      advancedToggle.addEventListener("change", () => {
        if (advancedToggle.checked) {
          advancedSection.classList.remove("hidden");
        } else {
          advancedSection.classList.add("hidden");
        }
      });

      // Swap economy units helper (simple cycle)
      document
        .getElementById("swap-units-btn")
        .addEventListener("click", () => {
          const unitEl = document.getElementById("economy-unit");
          const value = getNumber("economy");
          const current = unitEl.value;
          if (!isFinite(value) || value <= 0) {
            // just cycle unit if no value
            const order = ["mpg_us", "l_per_100km", "km_per_l", "mpg_uk"];
            const idx = order.indexOf(current);
            unitEl.value = order[(idx + 1) % order.length];
            return;
          }
          // Convert via L/100km then to next unit
          const l100 = economyToLPer100km(value, current);
          if (!isFinite(l100) || l100 <= 0) return;
          let nextUnit;
          if (current === "mpg_us") nextUnit = "l_per_100km";
          else if (current === "l_per_100km") nextUnit = "km_per_l";
          else if (current === "km_per_l") nextUnit = "mpg_uk";
          else nextUnit = "mpg_us";

          let newValue;
          switch (nextUnit) {
            case "mpg_us":
              newValue = 235.214583 / l100;
              break;
            case "mpg_uk":
              newValue = 282.480936 / l100;
              break;
            case "l_per_100km":
              newValue = l100;
              break;
            case "km_per_l":
              newValue = 100 / l100;
              break;
          }
          unitEl.value = nextUnit;
          if (isFinite(newValue)) {
            document.getElementById("economy").value = newValue.toFixed(2);
          }
        });

      // Trip calculation
      document.getElementById("calc-trip-btn").addEventListener("click", () => {
        const fuelPrice = getNumber("fuel-price");
        const fuelPriceUnit = document.getElementById("fuel-price-unit").value;
        const economy = getNumber("economy");
        const economyUnit = document.getElementById("economy-unit").value;
        const distance = getNumber("trip-distance");
        const distanceUnit =
          document.getElementById("trip-distance-unit").value;
        const passengers = getNumber("num-passengers") || 1;
        const currencySymbol =
          document.getElementById("currency-symbol").value || "$";

        const errors = [];
        if (!isFinite(fuelPrice) || fuelPrice <= 0) errors.push("fuel price");
        if (!isFinite(economy) || economy <= 0) errors.push("fuel economy");
        if (!isFinite(distance) || distance <= 0) errors.push("trip distance");
        if (passengers <= 0) errors.push("number of people");

        if (errors.length) {
          alert("Please enter valid values for: " + errors.join(", ") + ".");
          return;
        }

        const lPer100km = economyToLPer100km(economy, economyUnit);
        const pricePerL = pricePerLiter(fuelPrice, fuelPriceUnit);
        if (!isFinite(lPer100km) || !isFinite(pricePerL)) {
          alert("The combination of units and values is not valid.");
          return;
        }

        // Convert distance to km
        const distanceKm =
          distanceUnit === "km" ? distance : distance * 1.609344;

        const fuelUsedL = (distanceKm * lPer100km) / 100;
        const totalCost = fuelUsedL * pricePerL;
        const costPerPerson = totalCost / passengers;
        const costPerDistance = totalCost / distance;

        const tripResults = document.getElementById("trip-results");
        document.getElementById("trip-total-cost").textContent =
          "Total trip fuel cost: " + formatCurrency(totalCost, currencySymbol);
        document.getElementById("trip-fuel-used").textContent =
          "Estimated fuel used: " + formatNumber(fuelUsedL, 2) + " L";
        document.getElementById("trip-cost-per-person").textContent =
          "Cost per person (" +
          passengers +
          "): " +
          formatCurrency(costPerPerson, currencySymbol);
        document.getElementById("trip-cost-per-distance").textContent =
          "Cost per " +
          (distanceUnit === "km" ? "km" : "mile") +
          ": " +
          formatCurrency(costPerDistance, currencySymbol);

        tripResults.classList.remove("hidden");
        tripResults.scrollIntoView({ behavior: "smooth", block: "nearest" });
      });

      // Usage calculation
      document
        .getElementById("calc-usage-btn")
        .addEventListener("click", () => {
          const fuelPrice = getNumber("fuel-price");
          const fuelPriceUnit =
            document.getElementById("fuel-price-unit").value;
          const economy = getNumber("economy");
          const economyUnit = document.getElementById("economy-unit").value;
          const distance = getNumber("usage-distance");
          const distanceUnit = document.getElementById(
            "usage-distance-unit"
          ).value;
          const period = document.getElementById("usage-period").value;
          const currencySymbol =
            document.getElementById("currency-symbol").value || "$";

          const errors = [];
          if (!isFinite(fuelPrice) || fuelPrice <= 0) errors.push("fuel price");
          if (!isFinite(economy) || economy <= 0) errors.push("fuel economy");
          if (!isFinite(distance) || distance <= 0)
            errors.push("distance driven");

          if (errors.length) {
            alert("Please enter valid values for: " + errors.join(", ") + ".");
            return;
          }

          const lPer100km = economyToLPer100km(economy, economyUnit);
          const pricePerL = pricePerLiter(fuelPrice, fuelPriceUnit);
          if (!isFinite(lPer100km) || !isFinite(pricePerL)) {
            alert("The combination of units and values is not valid.");
            return;
          }

          // Convert distance to km
          const distanceKm =
            distanceUnit === "km" ? distance : distance * 1.609344;

          // Normalize to daily distance
          let dailyKm;
          switch (period) {
            case "day":
              dailyKm = distanceKm;
              break;
            case "week":
              dailyKm = distanceKm / 7;
              break;
            case "month":
              dailyKm = distanceKm / 30.4375; // average month
              break;
            default:
              dailyKm = distanceKm;
          }

          const dailyFuelL = (dailyKm * lPer100km) / 100;
          const dailyCost = dailyFuelL * pricePerL;
          const weeklyCost = dailyCost * 7;
          const monthlyCost = dailyCost * 30.4375;
          const yearlyCost = dailyCost * 365;

          const usageResults = document.getElementById("usage-results");
          document.getElementById("usage-daily").textContent =
            "Estimated daily fuel cost: " +
            formatCurrency(dailyCost, currencySymbol);
          document.getElementById("usage-weekly").textContent =
            "Estimated weekly fuel cost: " +
            formatCurrency(weeklyCost, currencySymbol);
          document.getElementById("usage-monthly").textContent =
            "Estimated monthly fuel cost: " +
            formatCurrency(monthlyCost, currencySymbol);
          document.getElementById("usage-yearly").textContent =
            "Estimated yearly fuel cost: " +
            formatCurrency(yearlyCost, currencySymbol);

          usageResults.classList.remove("hidden");
          usageResults.scrollIntoView({ behavior: "smooth", block: "nearest" });
        });

      // Presets
      document.querySelectorAll("[data-preset]").forEach((btn) => {
        btn.addEventListener("click", () => {
          const preset = btn.getAttribute("data-preset");
          const econEl = document.getElementById("economy");
          const econUnitEl = document.getElementById("economy-unit");
          const priceEl = document.getElementById("fuel-price");
          const priceUnitEl = document.getElementById("fuel-price-unit");
          const currencyEl = document.getElementById("currency-symbol");

          if (preset === "city") {
            econEl.value = 25;
            econUnitEl.value = "mpg_us";
          } else if (preset === "highway") {
            econEl.value = 35;
            econUnitEl.value = "mpg_us";
          } else if (preset === "suv") {
            econEl.value = 20;
            econUnitEl.value = "mpg_us";
          } else if (preset === "europe") {
            priceEl.value = 1.8;
            priceUnitEl.value = "per_liter";
            currencyEl.value = "€";
            econEl.value = 6.5;
            econUnitEl.value = "l_per_100km";
          }
        });
      });

      // Basic defaults
      window.addEventListener("DOMContentLoaded", () => {
        if (!document.getElementById("fuel-price").value) {
          document.getElementById("fuel-price").value = "3.80";
        }
        if (!document.getElementById("economy").value) {
          document.getElementById("economy").value = "30";
        }
        activateTab("trip");
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
