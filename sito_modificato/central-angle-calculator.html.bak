    <script src="/search.js"></script>

    <script>
      const tabArc = document.getElementById('tab-arc');
      const tabChord = document.getElementById('tab-chord');
      const tabSector = document.getElementById('tab-sector');
      const tabPercent = document.getElementById('tab-percent');

      const panelArc = document.getElementById('panel-arc');
      const panelChord = document.getElementById('panel-chord');
      const panelSector = document.getElementById('panel-sector');
      const panelPercent = document.getElementById('panel-percent');

      function activateTab(btn, panel) {
        [tabArc, tabChord, tabSector, tabPercent].forEach(b => b.classList.remove('tab-active'));
        btn.classList.add('tab-active');
        [panelArc, panelChord, panelSector, panelPercent].forEach(p => p.classList.add('hidden'));
        panel.classList.remove('hidden');
      }

      tabArc.addEventListener('click', () => activateTab(tabArc, panelArc));
      tabChord.addEventListener('click', () => activateTab(tabChord, panelChord));
      tabSector.addEventListener('click', () => activateTab(tabSector, panelSector));
      tabPercent.addEventListener('click', () => activateTab(tabPercent, panelPercent));

      const btnCalc = document.getElementById('btn-calc');
      const btnReset = document.getElementById('btn-reset');

      const resultBox = document.getElementById('result-box');
      const resDeg = document.getElementById('res-deg');
      const resRad = document.getElementById('res-rad');
      const resFrac = document.getElementById('res-frac');
      const resArc = document.getElementById('res-arc');
      const resChord = document.getElementById('res-chord');
      const resSector = document.getElementById('res-sector');

      function round4(x) {
        return Math.round((x + Number.EPSILON) * 10000) / 10000;
      }

      function calcArc() {
        const r = parseFloat(document.getElementById('arc-radius').value);
        const L = parseFloat(document.getElementById('arc-length').value);
        if (isNaN(r) || isNaN(L) || r <= 0 || L <= 0) return { error: 'Enter positive radius and arc length.' };
        const thetaRad = L / r;
        return { thetaRad, r };
      }

      function calcChord() {
        const r = parseFloat(document.getElementById('chord-radius').value);
        const c = parseFloat(document.getElementById('chord-length').value);
        if (isNaN(r) || isNaN(c) || r <= 0 || c <= 0) return { error: 'Enter positive radius and chord.' };
        if (c > 2*r) return { error: 'Chord cannot exceed the diameter (2r).' };
        const thetaRad = 2 * Math.asin(c / (2*r));
        return { thetaRad, r };
      }

      function calcSector() {
        const r = parseFloat(document.getElementById('sector-radius').value);
        const A = parseFloat(document.getElementById('sector-area').value);
        if (isNaN(r) || isNaN(A) || r <= 0 || A <= 0) return { error: 'Enter positive radius and sector area.' };
        const thetaRad = 2 * A / (r*r);
        return { thetaRad, r };
      }

      function calcPercent() {
        const pct = parseFloat(document.getElementById('percent-value').value);
        if (isNaN(pct) || pct <= 0) return { error: 'Enter a positive percentage.' };
        const thetaDeg = pct * 360 / 100;
        const thetaRad = thetaDeg * Math.PI / 180;
        // r unknown here → we can’t give arc/chord unless we assume r
        return { thetaRad, r: null };
      }

      btnCalc.addEventListener('click', function() {
        let res;
        if (!panelArc.classList.contains('hidden')) {
          res = calcArc();
        } else if (!panelChord.classList.contains('hidden')) {
          res = calcChord();
        } else if (!panelSector.classList.contains('hidden')) {
          res = calcSector();
        } else {
          res = calcPercent();
        }

        resultBox.classList.remove('hidden');

        if (res.error) {
          resDeg.textContent = res.error;
          resRad.textContent = '—';
          resFrac.textContent = '—';
          resArc.textContent = '—';
          resChord.textContent = '—';
          resSector.textContent = '—';
          return;
        }

        const thetaRad = res.thetaRad;
        const thetaDeg = thetaRad * 180 / Math.PI;
        const fraction = thetaDeg / 360;

        resDeg.textContent = round4(thetaDeg);
        resRad.textContent = round4(thetaRad);
        resFrac.textContent = round4(fraction);

        if (res.r) {
          const r = res.r;
          const arc = r * thetaRad;
          const chord = 2 * r * Math.sin(thetaRad / 2);
          const sector = 0.5 * r * r * thetaRad;
          resArc.textContent = round4(arc);
          resChord.textContent = round4(chord);
          resSector.textContent = round4(sector);
        } else {
          resArc.textContent = 'Need radius';
          resChord.textContent = 'Need radius';
          resSector.textContent = 'Need radius';
        }
      });

      btnReset.addEventListener('click', function() {
        document.getElementById('arc-radius').value = 10;
        document.getElementById('arc-length').value = 12;
        document.getElementById('chord-radius').value = 10;
        document.getElementById('chord-length').value = 12;
        document.getElementById('sector-radius').value = 10;
        document.getElementById('sector-area').value = 50;
        document.getElementById('percent-value').value = 25;
        resultBox.classList.add('hidden');
      });
    </script>

