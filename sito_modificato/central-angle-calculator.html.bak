              <button type="button" id="tab-sector" class="px-4 py-2 border rounded-md text-sm">Sector area + radius</button>
              <button type="button" id="tab-percent" class="px-4 py-2 border rounded-md text-sm">Arc % of circle</button>
            </div>

            <!-- Arc + radius -->
            <div id="panel-arc" class="mb-6">
              <div class="grid md:grid-cols-3 gap-4 mb-4">
                <div>
                  <label class="block label-sm text-gray-700 mb-1" for="arc-radius">Radius (r)</label>
                  <input id="arc-radius" type="number" value="10" step="0.01" class="w-full border-gray-300 rounded-md focus:ring-blue-500 focus:border-blue-500">
                  <p class="mini-hint">any unit</p>
                </div>
                <div>
                  <label class="block label-sm text-gray-700 mb-1" for="arc-length">Arc length (L)</label>
                  <input id="arc-length" type="number" value="12" step="0.01" class="w-full border-gray-300 rounded-md">
                  <p class="mini-hint">same unit as r</p>
                </div>
                <div class="flex items-end">
                  <p class="text-xs text-gray-500">θ (rad) = L / r</p>
                </div>
              </div>
            </div>

            <!-- Chord + radius -->
            <div id="panel-chord" class="mb-6 hidden">
              <div class="grid md:grid-cols-3 gap-4 mb-4">
                <div>
                  <label class="block label-sm text-gray-700 mb-1" for="chord-radius">Radius (r)</label>
                  <input id="chord-radius" type="number" value="10" step="0.01" class="w-full border-gray-300 rounded-md">
                </div>
                <div>
                  <label class="block label-sm text-gray-700 mb-1" for="chord-length">Chord length (c)</label>
                  <input id="chord-length" type="number" value="12" step="0.01" class="w-full border-gray-300 rounded-md">
                </div>
                <div class="flex items-end">
                  <p class="text-xs text-gray-500">θ = 2 · asin(c / (2r))</p>
                </div>
              </div>
            </div>

            <!-- Sector area + radius -->
            <div id="panel-sector" class="mb-6 hidden">
              <div class="grid md:grid-cols-3 gap-4 mb-4">
