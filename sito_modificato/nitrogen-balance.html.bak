    <script>
      // Nitrogen balance calculator logic
      (function () {
        const weightEl = document.getElementById("nb-weight");
        const weightUnitEl = document.getElementById("nb-weight-unit");
        const proteinEl = document.getElementById("nb-protein");
        const proteinUnitEl = document.getElementById("nb-protein-unit");
        const uunEl = document.getElementById("nb-uun");
        const nonUrinaryEl = document.getElementById("nb-nonurinary");
        const factorEl = document.getElementById("nb-protein-factor");

        const intakeEl = document.getElementById("nb-intake");
        const lossesEl = document.getElementById("nb-losses");
        const balanceEl = document.getElementById("nb-balance");
        const interpEl = document.getElementById("nb-interpretation");
        const warningEl = document.getElementById("nb-warning");
        const proteinTargetEl = document.getElementById("nb-protein-target");

        const calcBtn = document.getElementById("nb-calc-btn");
        const clearBtn = document.getElementById("nb-clear-btn");
        const exampleBtn = document.getElementById("nb-example-btn");

        function toNumber(el) {
          const v = parseFloat(el.value);
          return isNaN(v) ? null : v;
        }

        function round(val, digits) {
          const f = Math.pow(10, digits);
          return Math.round(val * f) / f;
        }

        function lbToKg(lb) {
          return lb / 2.2046226218;
        }

        function updateProteinTargets() {
          const w = toNumber(weightEl);
          if (!w || w <= 0) {
            proteinTargetEl.textContent =
              "Enter body weight to see suggested protein intake ranges (e.g., 1.2–2.0 g/kg/day for many hospitalized adults).";
            return;
          }
          const kg = weightUnitEl.value === "lb" ? lbToKg(w) : w;
          const ranges = [
            { label: "Healthy adults (0.8–1.0 g/kg/day)", min: 0.8, max: 1.0 },
            {
              label: "Older / mild illness (1.0–1.2 g/kg/day)",
              min: 1.0,
              max: 1.2,
            },
            {
              label: "Hospitalized / chronic disease (1.2–1.5 g/kg/day)",
              min: 1.2,
              max: 1.5,
            },
            {
              label: "Critical illness / major surgery (1.5–2.0 g/kg/day)",
              min: 1.5,
              max: 2.0,
            },
          ];
          const lines = ranges.map((r) => {
            const minG = round(r.min * kg, 1);
            const maxG = round(r.max * kg, 1);
            return `${r.label}: ${minG}–${maxG} g/day`;
          });
          proteinTargetEl.textContent = lines.join(" | ");
        }

        function classifyBalance(nb) {
          if (nb === null) return "";
          if (nb > 2)
            return "Positive nitrogen balance (net protein gain / anabolism).";
          if (nb < -2)
            return "Negative nitrogen balance (net protein loss / catabolism).";
          return "Approximately nitrogen equilibrium (maintenance of body protein).";
        }

        function calculate() {
          const proteinVal = toNumber(proteinEl);
          const uunVal = toNumber(uunEl);
          let nonUrinaryVal = toNumber(nonUrinaryEl);
          const factorVal = toNumber(factorEl);

          if (
            !proteinVal ||
            proteinVal <= 0 ||
            !uunVal ||
            uunVal < 0 ||
            !factorVal ||
            factorVal <= 0
          ) {
            interpEl.textContent =
              "Please enter at least protein intake, UUN, and a valid protein-to-nitrogen factor.";
            warningEl.classList.add("hidden");
            intakeEl.textContent = "–";
            lossesEl.textContent = "–";
            balanceEl.textContent = "–";
            return;
          }

          if (nonUrinaryVal === null || nonUrinaryVal < 0) {
            nonUrinaryVal = 4; // default
            nonUrinaryEl.value = "4";
          }

          // Convert protein to g/day if needed
          let proteinGDay;
          const weightVal = toNumber(weightEl);
          if (proteinUnitEl.value === "g_kg_day") {
            if (!weightVal || weightVal <= 0) {
              interpEl.textContent =
                "To use g/kg/day, please enter body weight.";
              warningEl.classList.add("hidden");
              intakeEl.textContent = "–";
              lossesEl.textContent = "–";
              balanceEl.textContent = "–";
              return;
            }
            const kg =
              weightUnitEl.value === "lb" ? lbToKg(weightVal) : weightVal;
            proteinGDay = proteinVal * kg;
          } else {
            proteinGDay = proteinVal;
          }

          const nIntake = proteinGDay / factorVal;
          const nLosses = uunVal + nonUrinaryVal;
          const nBalance = nIntake - nLosses;

          intakeEl.textContent = `${round(nIntake, 2)} g N/day`;
          lossesEl.textContent = `${round(nLosses, 2)} g N/day`;
          balanceEl.textContent = `${round(nBalance, 2)} g N/day`;

          const cls = classifyBalance(nBalance);
          interpEl.textContent = cls
            ? `${cls} Always interpret in clinical context and over several days when possible.`
            : "";
          warningEl.classList.remove("hidden");
        }

        function clearAll() {
          weightEl.value = "";
          proteinEl.value = "";
          uunEl.value = "";
          nonUrinaryEl.value = "";
          factorEl.value = "6.25";
          intakeEl.textContent = "–";
          lossesEl.textContent = "–";
          balanceEl.textContent = "–";
          interpEl.textContent =
            "Enter values and click “Calculate” to see the nitrogen balance.";
          warningEl.classList.add("hidden");
          proteinTargetEl.textContent =
            "Enter body weight to see suggested protein intake ranges (e.g., 1.2–2.0 g/kg/day for many hospitalized adults).";
        }

        function loadExample() {
          weightEl.value = "70";
          weightUnitEl.value = "kg";
          proteinEl.value = "1.5";
          proteinUnitEl.value = "g_kg_day";
          uunEl.value = "12";
          nonUrinaryEl.value = "4";
          factorEl.value = "6.25";
          updateProteinTargets();
          calculate();
        }

        calcBtn.addEventListener("click", calculate);
        clearBtn.addEventListener("click", clearAll);
        exampleBtn.addEventListener("click", loadExample);
        weightEl.addEventListener("input", updateProteinTargets);
        weightUnitEl.addEventListener("change", updateProteinTargets);
      })();

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
