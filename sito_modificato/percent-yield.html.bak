    <script>
      // Percent Yield Calculator Logic
      (function () {
        const solveForEl = document.getElementById("solveFor");
        const unitEl = document.getElementById("yieldUnit");
        const actualEl = document.getElementById("actualYield");
        const theoreticalEl = document.getElementById("theoreticalYield");
        const percentEl = document.getElementById("percentYield");
        const calcBtn = document.getElementById("calculateBtn");
        const clearBtn = document.getElementById("clearBtn");
        const resultBox = document.getElementById("resultBox");
        const warningBox = document.getElementById("warningBox");
        const actualUnitLabel = document.getElementById("actualUnitLabel");
        const theoreticalUnitLabel = document.getElementById(
          "theoreticalUnitLabel"
        );

        function updateUnitLabels() {
          const val = unitEl.value;
          if (val === "custom") {
            actualUnitLabel.textContent = "";
            theoreticalUnitLabel.textContent = "";
          } else {
            actualUnitLabel.textContent = val;
            theoreticalUnitLabel.textContent = val;
          }
        }

        unitEl.addEventListener("change", updateUnitLabels);
        updateUnitLabels();

        function setDisabledFields() {
          const mode = solveForEl.value;
          actualEl.disabled = mode === "actual";
          theoreticalEl.disabled = mode === "theoretical";
          percentEl.disabled = mode === "percent";

          [actualEl, theoreticalEl, percentEl].forEach((el) => {
            el.classList.remove("bg-gray-100");
            if (el.disabled) el.classList.add("bg-gray-100");
          });
        }

        solveForEl.addEventListener("change", () => {
          setDisabledFields();
          resultBox.classList.add("hidden");
          warningBox.classList.add("hidden");
        });

        setDisabledFields();

        function parseVal(el) {
          const v = parseFloat(el.value);
          return isNaN(v) ? null : v;
        }

        function formatNumber(num, decimals = 4) {
          if (!isFinite(num)) return "—";
          if (Math.abs(num) >= 1e4 || (Math.abs(num) <= 1e-3 && num !== 0)) {
            return num.toExponential(3);
          }
          return parseFloat(num.toFixed(decimals)).toString();
        }

        calcBtn.addEventListener("click", () => {
          const mode = solveForEl.value;
          const actual = parseVal(actualEl);
          const theoretical = parseVal(theoreticalEl);
          const percent = parseVal(percentEl);

          resultBox.classList.add("hidden");
          warningBox.classList.add("hidden");

          let resultText = "";
          let warnText = "";

          if (mode === "percent") {
            if (actual === null || theoretical === null) {
              alert("Please enter both actual yield and theoretical yield.");
              return;
            }
            if (theoretical === 0) {
              alert("Theoretical yield cannot be zero.");
              return;
            }
            const py = (actual / theoretical) * 100;
            percentEl.value = formatNumber(py, 2);
            resultText = `Percent yield = (Actual ÷ Theoretical) × 100% = (${actual} ÷ ${theoretical}) × 100% = ${formatNumber(
              py,
              2
            )}%`;
            if (py > 100) {
              warnText =
                "Your percent yield is above 100%. This usually indicates impurities, solvent still present, or a theoretical yield miscalculation.";
            } else if (py < 0) {
              warnText =
                "Percent yield should not be negative. Please check your inputs.";
            }
          } else if (mode === "actual") {
            if (theoretical === null || percent === null) {
              alert("Please enter both theoretical yield and percent yield.");
              return;
            }
            const ay = (percent / 100) * theoretical;
            actualEl.value = formatNumber(ay, 4);
            resultText = `Actual yield = (Percent yield ÷ 100) × Theoretical = (${percent} ÷ 100) × ${theoretical} = ${formatNumber(
              ay,
              4
            )} ${unitEl.value === "custom" ? "" : unitEl.value}`;
            if (percent < 0) {
              warnText =
                "Percent yield should not be negative. Please check your inputs.";
            }
          } else if (mode === "theoretical") {
            if (actual === null || percent === null) {
              alert("Please enter both actual yield and percent yield.");
              return;
            }
            if (percent === 0) {
              alert(
                "Percent yield cannot be zero when solving for theoretical yield."
              );
              return;
            }
            const ty = (actual * 100) / percent;
            theoreticalEl.value = formatNumber(ty, 4);
            resultText = `Theoretical yield = (Actual × 100) ÷ Percent yield = (${actual} × 100) ÷ ${percent} = ${formatNumber(
              ty,
              4
            )} ${unitEl.value === "custom" ? "" : unitEl.value}`;
            if (percent < 0) {
              warnText =
                "Percent yield should not be negative. Please check your inputs.";
            }
          }

          if (resultText) {
            resultBox.textContent = resultText;
            resultBox.classList.remove("hidden");
          }
          if (warnText) {
            warningBox.textContent = warnText;
            warningBox.classList.remove("hidden");
          }
        });

        clearBtn.addEventListener("click", () => {
          actualEl.value = "";
          theoreticalEl.value = "";
          percentEl.value = "";
          resultBox.classList.add("hidden");
          warningBox.classList.add("hidden");
        });
      })();
    </script>

