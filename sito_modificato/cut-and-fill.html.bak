    <script>
      // Toggle mobile menu
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          this.setAttribute("aria-expanded", isHidden ? "true" : "false");
        });

      const unitSystemEl = document.getElementById("unitSystem");
      const inputModeEl = document.getElementById("inputMode");
      const simpleModeEl = document.getElementById("simpleMode");
      const detailedModeEl = document.getElementById("detailedMode");
      const calcBtn = document.getElementById("calcBtn");
      const resetBtn = document.getElementById("resetBtn");

      const totalCutEl = document.getElementById("totalCut");
      const totalFillEl = document.getElementById("totalFill");
      const netVolumeEl = document.getElementById("netVolume");
      const netNoteEl = document.getElementById("netNote");
      const cutFillRatioEl = document.getElementById("cutFillRatio");
      const avgCutDepthEl = document.getElementById("avgCutDepth");
      const avgFillDepthEl = document.getElementById("avgFillDepth");

      const detailedTableWrapper = document.getElementById(
        "detailedTableWrapper"
      );
      const detailedTableBody = document.getElementById("detailedTableBody");
      const copyTableBtn = document.getElementById("copyTableBtn");

      function formatVolume(value, unitSystem) {
        if (isNaN(value)) return "–";
        const unit = unitSystem === "metric" ? "m³" : "yd³";
        return value.toFixed(2) + " " + unit;
      }

      function formatLength(value, unitSystem) {
        if (isNaN(value)) return "–";
        const unit = unitSystem === "metric" ? "m" : "ft";
        return value.toFixed(3) + " " + unit;
      }

      function formatDepth(value, unitSystem) {
        if (isNaN(value)) return "–";
        const unit = unitSystem === "metric" ? "m" : "ft";
        return value.toFixed(3) + " " + unit;
      }

      function resetResults() {
        totalCutEl.textContent = "–";
        totalFillEl.textContent = "–";
        netVolumeEl.textContent = "–";
        netNoteEl.textContent = "";
        cutFillRatioEl.textContent = "–";
        avgCutDepthEl.textContent = "–";
        avgFillDepthEl.textContent = "–";
        detailedTableBody.innerHTML = "";
        detailedTableWrapper.classList.add("hidden");
      }

      inputModeEl.addEventListener("change", () => {
        const mode = inputModeEl.value;
        if (mode === "simple") {
          simpleModeEl.classList.remove("hidden");
          detailedModeEl.classList.add("hidden");
        } else {
          simpleModeEl.classList.add("hidden");
          detailedModeEl.classList.remove("hidden");
        }
        resetResults();
      });

      resetBtn.addEventListener("click", () => {
        document.getElementById("simpleExisting").value = "";
        document.getElementById("simpleDesign").value = "";
        document.getElementById("simpleArea").value = "";
        document.getElementById("stationsInput").value = "";
        document.getElementById("stationSpacing").value = "";
        document.getElementById("defaultWidth").value = "";
        resetResults();
      });

      function parseStations(text, defaultSpacing, defaultWidth) {
        const lines = text
          .split(/\r?\n/)
          .map((l) => l.trim())
          .filter((l) => l.length > 0);
        const rows = [];
        for (let line of lines) {
          const parts = line.split(/[\s,;]+/).filter(Boolean);
          if (parts.length < 3) continue;
          const station = parseFloat(parts[0]);
          const existing = parseFloat(parts[1]);
          const design = parseFloat(parts[2]);
          let width = parts.length >= 4 ? parseFloat(parts[3]) : defaultWidth;
          if (
            !isFinite(station) ||
            !isFinite(existing) ||
            !isFinite(design) ||
            !isFinite(width) ||
            width <= 0
          ) {
            continue;
          }
          rows.push({ station, existing, design, width });
        }
        // sort by station
        rows.sort((a, b) => a.station - b.station);
        // compute spacing per segment
        for (let i = 0; i < rows.length; i++) {
          if (i < rows.length - 1) {
            const spacing = rows[i + 1].station - rows[i].station;
            rows[i].spacing = spacing > 0 ? spacing : defaultSpacing;
          } else {
            rows[i].spacing = defaultSpacing;
          }
        }
        return rows;
      }

      calcBtn.addEventListener("click", () => {
        resetResults();
        const unitSystem = unitSystemEl.value;
        const mode = inputModeEl.value;

        if (mode === "simple") {
          const existing = parseFloat(
            document.getElementById("simpleExisting").value
          );
          const design = parseFloat(
            document.getElementById("simpleDesign").value
          );
          const area = parseFloat(document.getElementById("simpleArea").value);

          if (
            !isFinite(existing) ||
            !isFinite(design) ||
            !isFinite(area) ||
            area <= 0
          ) {
            alert(
              "Please enter valid existing elevation, design elevation, and total area."
            );
            return;
          }

          const delta = existing - design;
          const depth = Math.abs(delta);
          let volume = depth * area; // m³ or ft³

          // convert ft³ to yd³ if imperial
          if (unitSystem === "imperial") {
            volume = volume / 27.0;
          }

          let cut = 0,
            fill = 0;
          if (delta > 0) {
            cut = volume;
          } else if (delta < 0) {
            fill = volume;
          }

          totalCutEl.textContent = formatVolume(cut, unitSystem);
          totalFillEl.textContent = formatVolume(fill, unitSystem);
          const net = cut - fill;
          netVolumeEl.textContent = formatVolume(net, unitSystem);
          if (net > 0.0001) {
            netNoteEl.textContent =
              "Positive net volume: more cut than fill (likely export of material).";
          } else if (net < -0.0001) {
            netNoteEl.textContent =
              "Negative net volume: more fill than cut (likely import of material).";
          } else {
            netNoteEl.textContent =
              "Site is approximately balanced between cut and fill.";
          }

          const avgCutDepth = delta > 0 ? depth : 0;
          const avgFillDepth = delta < 0 ? depth : 0;
          avgCutDepthEl.textContent =
            avgCutDepth > 0 ? formatDepth(avgCutDepth, unitSystem) : "–";
          avgFillDepthEl.textContent =
            avgFillDepth > 0 ? formatDepth(avgFillDepth, unitSystem) : "–";
          cutFillRatioEl.textContent =
            fill > 0 ? (cut / fill).toFixed(2) : cut > 0 ? "∞" : "–";
        } else {
          const text = document.getElementById("stationsInput").value.trim();
          if (!text) {
            alert("Please enter at least one station row.");
            return;
          }
          let defaultSpacing = parseFloat(
            document.getElementById("stationSpacing").value
          );
          let defaultWidth = parseFloat(
            document.getElementById("defaultWidth").value
          );
          if (!isFinite(defaultSpacing) || defaultSpacing <= 0)
            defaultSpacing = 10;
          if (!isFinite(defaultWidth) || defaultWidth <= 0) defaultWidth = 10;

          const rows = parseStations(text, defaultSpacing, defaultWidth);
          if (rows.length === 0) {
            alert(
              "No valid rows found. Check that each line has station, existing, design, [width]."
            );
            return;
          }

          let totalCut = 0;
          let totalFill = 0;
          let cutAreaSum = 0;
          let fillAreaSum = 0;

          detailedTableBody.innerHTML = "";

          rows.forEach((row) => {
            const delta = row.existing - row.design;
            const depth = Math.abs(delta);
            const area = row.spacing * row.width; // m² or ft²
            let volume = depth * area; // m³ or ft³
            if (unitSystem === "imperial") {
              volume = volume / 27.0; // ft³ to yd³
            }

            let cutVol = 0,
              fillVol = 0,
              type = "Neutral";
            if (delta > 0) {
              cutVol = volume;
              type = "Cut";
              cutAreaSum += area;
            } else if (delta < 0) {
              fillVol = volume;
              type = "Fill";
              fillAreaSum += area;
            }

            totalCut += cutVol;
            totalFill += fillVol;

            const tr = document.createElement("tr");
            tr.innerHTML = `
          <td class="px-2 py-1 text-left">${row.station}</td>
          <td class="px-2 py-1 text-right">${row.existing.toFixed(3)}</td>
          <td class="px-2 py-1 text-right">${row.design.toFixed(3)}</td>
          <td class="px-2 py-1 text-right">${delta.toFixed(3)}</td>
          <td class="px-2 py-1 text-right">${area.toFixed(2)}</td>
          <td class="px-2 py-1 text-right">${
            cutVol > 0 ? cutVol.toFixed(2) : "-"
          }</td>
          <td class="px-2 py-1 text-right">${
            fillVol > 0 ? fillVol.toFixed(2) : "-"
          }</td>
          <td class="px-2 py-1 text-center">${type}</td>
        `;
            detailedTableBody.appendChild(tr);
          });

          detailedTableWrapper.classList.remove("hidden");

          totalCutEl.textContent = formatVolume(totalCut, unitSystem);
          totalFillEl.textContent = formatVolume(totalFill, unitSystem);
          const net = totalCut - totalFill;
          netVolumeEl.textContent = formatVolume(net, unitSystem);
          if (net > 0.0001) {
            netNoteEl.textContent =
              "Positive net volume: more cut than fill (likely export of material).";
          } else if (net < -0.0001) {
            netNoteEl.textContent =
              "Negative net volume: more fill than cut (likely import of material).";
          } else {
            netNoteEl.textContent =
              "Site is approximately balanced between cut and fill.";
          }

          cutFillRatioEl.textContent =
            totalFill > 0
              ? (totalCut / totalFill).toFixed(2)
              : totalCut > 0
              ? "∞"
              : "–";

          const avgCutDepth =
            cutAreaSum > 0
              ? (totalCut * (unitSystem === "imperial" ? 27.0 : 1)) / cutAreaSum
              : NaN;
          const avgFillDepth =
            fillAreaSum > 0
              ? (totalFill * (unitSystem === "imperial" ? 27.0 : 1)) /
                fillAreaSum
              : NaN;
          avgCutDepthEl.textContent = isNaN(avgCutDepth)
            ? "–"
            : formatDepth(avgCutDepth, unitSystem);
          avgFillDepthEl.textContent = isNaN(avgFillDepth)
            ? "–"
            : formatDepth(avgFillDepth, unitSystem);
        }
      });

      copyTableBtn.addEventListener("click", () => {
        const rows = [];
        const header = [
          "Station",
          "Existing",
          "Design",
          "DeltaElev",
          "Area",
          "CutVol",
          "FillVol",
          "Type",
        ];
        rows.push(header.join("\t"));
        detailedTableBody.querySelectorAll("tr").forEach((tr) => {
          const cells = Array.from(tr.querySelectorAll("td")).map((td) =>
            td.textContent.trim()
          );
          rows.push(cells.join("\t"));
        });
        const text = rows.join("\n");
        navigator.clipboard
          .writeText(text)
          .then(() => {
            copyTableBtn.textContent = "Copied!";
            setTimeout(() => {
              copyTableBtn.textContent = "Copy table";
            }, 1500);
          })
          .catch(() => {
            alert("Unable to copy table. Please copy manually.");
          });
      });
    </script>

