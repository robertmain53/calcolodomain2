    <script>
      // Core conversion factors in mL per cup
      const CUP_FACTORS = {
        us: 236.588,
        us_legal: 240,
        metric: 250,
      };

      function formatNumber(value, decimals = 2) {
        if (!isFinite(value)) return "";
        return Number(value.toFixed(decimals)).toString();
      }

      function approximateFraction(cups) {
        if (!isFinite(cups) || cups === 0) return "–";
        const commonFractions = [
          { value: 0.125, label: "1/8" },
          { value: 0.25, label: "1/4" },
          { value: 0.3333, label: "1/3" },
          { value: 0.5, label: "1/2" },
          { value: 0.6667, label: "2/3" },
          { value: 0.75, label: "3/4" },
        ];
        const whole = Math.floor(cups);
        const frac = cups - whole;
        let best = null;
        let minDiff = Infinity;
        for (const f of commonFractions) {
          const diff = Math.abs(frac - f.value);
          if (diff < minDiff) {
            minDiff = diff;
            best = f;
          }
        }
        if (!best || minDiff > 0.08) {
          return "–";
        }
        if (whole === 0) return best.label + " cup";
        return `${whole} ${best.label} cups`;
      }

      function convertMlToCups() {
        const mlInput = document.getElementById("mlInput");
        const type = document.getElementById("cupTypeMl").value;
        const resultEl = document.getElementById("mlToCupsResult");
        const ml = parseFloat(mlInput.value);
        if (isNaN(ml)) {
          resultEl.textContent = "Please enter a valid number of milliliters.";
          return;
        }
        const factor = CUP_FACTORS[type];
        const cups = ml / factor;
        const approx = approximateFraction(cups);
        resultEl.innerHTML = `<strong>${formatNumber(
          ml,
          2
        )} mL</strong> ≈ <strong>${formatNumber(cups, 3)} cups</strong>${
          approx !== "–" ? " (" + approx + ")" : ""
        }`;
      }

      function convertCupsToMl() {
        const cupsInput = document.getElementById("cupsInput");
        const type = document.getElementById("cupTypeCups").value;
        const resultEl = document.getElementById("cupsToMlResult");
        const cups = parseFloat(cupsInput.value);
        if (isNaN(cups)) {
          resultEl.textContent = "Please enter a valid number of cups.";
          return;
        }
        const factor = CUP_FACTORS[type];
        const ml = cups * factor;
        resultEl.innerHTML = `<strong>${formatNumber(
          cups,
          3
        )} cups</strong> ≈ <strong>${formatNumber(ml, 1)} mL</strong>`;
      }

      document
        .getElementById("convertMlToCups")
        .addEventListener("click", convertMlToCups);
      document
        .getElementById("convertCupsToMl")
        .addEventListener("click", convertCupsToMl);

      // Enter key support
      document.getElementById("mlInput").addEventListener("keydown", (e) => {
        if (e.key === "Enter") {
          e.preventDefault();
          convertMlToCups();
        }
      });
      document.getElementById("cupsInput").addEventListener("keydown", (e) => {
        if (e.key === "Enter") {
          e.preventDefault();
          convertCupsToMl();
        }
      });

      // Preset buttons
      document.querySelectorAll(".preset-btn").forEach((btn) => {
        btn.addEventListener("click", () => {
          const ml = parseFloat(btn.getAttribute("data-preset-ml"));
          document.getElementById("mlInput").value = ml;
          convertMlToCups();
        });
      });

      // Build conversion table
      (function buildTable() {
        const tbody = document.getElementById("mlCupsTableBody");
        if (!tbody) return;
        const values = [
          50, 75, 100, 125, 150, 175, 200, 225, 250, 300, 400, 500, 750, 1000,
        ];
        const factor = CUP_FACTORS.us;
        tbody.innerHTML = "";
        values.forEach((ml) => {
          const cups = ml / factor;
          const tr = document.createElement("tr");
          tr.className = "odd:bg-white even:bg-gray-50";
          tr.innerHTML = `
        <td class="px-3 py-2 border-b">${ml}</td>
        <td class="px-3 py-2 border-b">${formatNumber(cups, 3)}</td>
        <td class="px-3 py-2 border-b">${approximateFraction(cups)}</td>
      `;
          tbody.appendChild(tr);
        });
      })();

      // Sync cup type selectors for convenience
      const cupTypeMl = document.getElementById("cupTypeMl");
      const cupTypeCups = document.getElementById("cupTypeCups");
      cupTypeMl.addEventListener("change", () => {
        cupTypeCups.value = cupTypeMl.value;
      });
      cupTypeCups.addEventListener("change", () => {
        cupTypeMl.value = cupTypeCups.value;
      });
    </script>

