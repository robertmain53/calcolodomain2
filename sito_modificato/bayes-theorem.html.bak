    <script>
      // Bayes theorem calculator logic
      (function () {
        const priorInput = document.getElementById("prior-a");
        const likeAInput = document.getElementById("likelihood-a");
        const likeNotAInput = document.getElementById("likelihood-not-a");
        const percentToggle = document.getElementById("percent-toggle");
        const computeBtn = document.getElementById("compute-btn");
        const resetBtn = document.getElementById("reset-btn");
        const errorBox = document.getElementById("error-box");
        const resultsSection = document.getElementById("results-section");

        const posteriorAEl = document.getElementById("posterior-a");
        const posteriorNotAEl = document.getElementById("posterior-not-a");
        const evidenceEl = document.getElementById("evidence");

        const popInput = document.getElementById("population-size");
        const popLabel = document.getElementById("population-size-label");

        const cellAB = document.getElementById("cell-a-b");
        const cellANotB = document.getElementById("cell-a-not-b");
        const cellATotal = document.getElementById("cell-a-total");
        const cellNotAB = document.getElementById("cell-not-a-b");
        const cellNotANotB = document.getElementById("cell-not-a-not-b");
        const cellNotATotal = document.getElementById("cell-not-a-total");
        const cellTotalB = document.getElementById("cell-total-b");
        const cellTotalNotB = document.getElementById("cell-total-not-b");
        const cellTotalTotal = document.getElementById("cell-total-total");

        const stepsContent = document.getElementById("steps-content");

        const eventANameInput = document.getElementById("event-a-name");
        const eventBNameInput = document.getElementById("event-b-name");
        const posteriorALabel = document.getElementById("posterior-a-label");
        const posteriorNotALabel = document.getElementById(
          "posterior-not-a-label"
        );
        const evidenceLabel = document.getElementById("evidence-label");
        const tblAHeader = document.getElementById("tbl-a-header");
        const tblNotAHeader = document.getElementById("tbl-not-a-header");
        const tblBHeader = document.getElementById("tbl-b-header");

        function fmtProb(p) {
          if (isNaN(p)) return "–";
          if (p <= 0) return "0%";
          if (p >= 1) return "100%";
          return (p * 100).toFixed(2).replace(/\.00$/, "") + "%";
        }

        function fmtNum(n) {
          if (isNaN(n)) return "–";
          return Math.round(n).toLocaleString();
        }

        function parseProb(input) {
          let v = parseFloat(input.value);
          if (isNaN(v)) return NaN;
          if (percentToggle.checked) {
            v = v / 100;
          }
          return v;
        }

        function validateInputs(pA, pBA, pBnotA) {
          if ([pA, pBA, pBnotA].some((v) => isNaN(v))) {
            return "Please enter numeric values for all probabilities.";
          }
          if (
            pA < 0 ||
            pA > 1 ||
            pBA < 0 ||
            pBA > 1 ||
            pBnotA < 0 ||
            pBnotA > 1
          ) {
            return "Probabilities must be between 0 and 1 (or 0% and 100%).";
          }
          const pNotA = 1 - pA;
          const pB = pBA * pA + pBnotA * pNotA;
          if (pB === 0) {
            return "The evidence probability P(B) is zero with these inputs, so P(A|B) is undefined. Adjust the likelihoods.";
          }
          return null;
        }

        function updateLabels() {
          const A = eventANameInput.value.trim() || "A";
          const B = eventBNameInput.value.trim() || "B";
          posteriorALabel.textContent = `P(${A}|${B})`;
          posteriorNotALabel.textContent = `P(¬${A}|${B})`;
          evidenceLabel.textContent = `P(${B})`;
          tblAHeader.textContent = A;
          tblNotAHeader.textContent = `¬${A}`;
          tblBHeader.textContent = B;
        }

        function compute() {
          errorBox.classList.add("hidden");
          errorBox.textContent = "";

          const pA = parseProb(priorInput);
          const pBA = parseProb(likeAInput);
          const pBnotA = parseProb(likeNotAInput);

          const err = validateInputs(pA, pBA, pBnotA);
          if (err) {
            errorBox.textContent = err;
            errorBox.classList.remove("hidden");
            resultsSection.classList.add("hidden");
            return;
          }

          const pNotA = 1 - pA;
          const pB = pBA * pA + pBnotA * pNotA;
          const pAgivenB = (pBA * pA) / pB;
          const pNotAgivenB = 1 - pAgivenB;

          posteriorAEl.textContent = fmtProb(pAgivenB);
          posteriorNotAEl.textContent = fmtProb(pNotAgivenB);
          evidenceEl.textContent = fmtProb(pB);

          // Frequency table
          let N = parseInt(popInput.value, 10);
          if (isNaN(N) || N <= 0) N = 10000;
          popInput.value = N;
          popLabel.textContent = N.toLocaleString();

          const aTotal = pA * N;
          const notATotal = pNotA * N;
          const aB = pBA * aTotal;
          const aNotB = aTotal - aB;
          const notAB = pBnotA * notATotal;
          const notANotB = notATotal - notAB;
          const totalB = aB + notAB;
          const totalNotB = aNotB + notANotB;

          cellAB.textContent = fmtNum(aB);
          cellANotB.textContent = fmtNum(aNotB);
          cellATotal.textContent = fmtNum(aTotal);
          cellNotAB.textContent = fmtNum(notAB);
          cellNotANotB.textContent = fmtNum(notANotB);
          cellNotATotal.textContent = fmtNum(notATotal);
          cellTotalB.textContent = fmtNum(totalB);
          cellTotalNotB.textContent = fmtNum(totalNotB);
          cellTotalTotal.textContent = fmtNum(N);

          // Step-by-step text
          const A = eventANameInput.value.trim() || "A";
          const B = eventBNameInput.value.trim() || "B";
          stepsContent.innerHTML = `
        <p>1. Prior and complement:</p>
        <p>&nbsp;&nbsp;P(${A}) = ${pA.toFixed(
            4
          )}, &nbsp; P(¬${A}) = 1 − P(${A}) = ${pNotA.toFixed(4)}</p>
        <p>2. Evidence probability using the law of total probability:</p>
        <p>&nbsp;&nbsp;P(${B}) = P(${B}|${A})·P(${A}) + P(${B}|¬${A})·P(¬${A})</p>
        <p>&nbsp;&nbsp;= ${pBA.toFixed(4)}·${pA.toFixed(4)} + ${pBnotA.toFixed(
            4
          )}·${pNotA.toFixed(4)}</p>
        <p>&nbsp;&nbsp;= ${(pBA * pA).toFixed(6)} + ${(pBnotA * pNotA).toFixed(
            6
          )} = ${pB.toFixed(6)}</p>
        <p>3. Apply Bayes’ theorem:</p>
        <p>&nbsp;&nbsp;P(${A}|${B}) = [P(${B}|${A})·P(${A})] / P(${B})</p>
        <p>&nbsp;&nbsp;= ${pBA.toFixed(4)}·${pA.toFixed(4)} / ${pB.toFixed(
            6
          )} = ${pAgivenB.toFixed(6)} (${fmtProb(pAgivenB)})</p>
      `;

          updateLabels();
          resultsSection.classList.remove("hidden");
        }

        computeBtn.addEventListener("click", compute);
        popInput.addEventListener("change", compute);
        eventANameInput.addEventListener("input", updateLabels);
        eventBNameInput.addEventListener("input", updateLabels);

        resetBtn.addEventListener("click", () => {
          eventANameInput.value = "Disease";
          eventBNameInput.value = "Positive test";
          percentToggle.checked = false;
          priorInput.value = 0.01;
          likeAInput.value = 0.95;
          likeNotAInput.value = 0.05;
          popInput.value = 10000;
          updateLabels();
          compute();
        });

        // Initialize
        updateLabels();
        compute();
      })();
    </script>

