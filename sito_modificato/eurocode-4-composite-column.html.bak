    <script>
      // Helper: format number with fixed decimals and thousands separator
      function formatNumber(value, decimals) {
        if (!isFinite(value)) return "-";
        return value.toLocaleString(undefined, {
          minimumFractionDigits: decimals,
          maximumFractionDigits: decimals,
        });
      }

      function computeEC4() {
        const as = parseFloat(document.getElementById("as").value); // cm²
        const fya = parseFloat(document.getElementById("fya").value); // MPa
        const ac = parseFloat(document.getElementById("ac").value); // cm²
        const fck = parseFloat(document.getElementById("fck").value); // MPa
        const alphaCc = parseFloat(document.getElementById("alpha-cc").value);
        const gammaC = parseFloat(document.getElementById("gamma-c").value);
        const asl = parseFloat(document.getElementById("asl").value); // cm²
        const fyd = parseFloat(document.getElementById("fyd").value); // MPa
        const gammaM = parseFloat(document.getElementById("gamma-m").value);
        const ned = parseFloat(document.getElementById("ned").value); // kN

        const inputs = [
          as,
          fya,
          ac,
          fck,
          alphaCc,
          gammaC,
          asl,
          fyd,
          gammaM,
          ned,
        ];
        if (inputs.some((v) => isNaN(v) || v < 0)) {
          alert("Please enter non-negative numeric values for all fields.");
          return;
        }
        if (gammaM <= 0 || gammaC <= 0) {
          alert("Partial factors γM and γC must be greater than zero.");
          return;
        }

        // Convert: 1 cm² * 1 MPa = 10 kN
        const factor = 10.0;

        const Na = ((as * fya) / gammaM) * factor; // kN
        const Nc = ((ac * alphaCc * fck) / gammaC) * factor; // kN
        const Ns = ((asl * fyd) / gammaM) * factor; // kN
        const Npl = Na + Nc + Ns;

        const util = Npl > 0 ? ned / Npl : NaN;

        // Update results
        document.getElementById("res-na").textContent = formatNumber(Na, 0);
        document.getElementById("res-nc").textContent = formatNumber(Nc, 0);
        document.getElementById("res-ns").textContent = formatNumber(Ns, 0);
        document.getElementById("res-npl").textContent = formatNumber(Npl, 0);
        document.getElementById("res-ned").textContent = formatNumber(ned, 0);
        document.getElementById("res-util").textContent = isFinite(util)
          ? formatNumber(util, 2)
          : "-";

        const statusEl = document.getElementById("res-status");
        const summaryEl = document.getElementById("result-summary");
        const resultsSection = document.getElementById("results-section");

        if (!isFinite(util)) {
          summaryEl.className =
            "p-4 rounded-md border border-red-200 bg-red-50 text-red-800";
          summaryEl.textContent =
            "Unable to compute utilization – please check your inputs.";
          statusEl.textContent = "Invalid";
          statusEl.className =
            "inline-block px-2 py-0.5 rounded text-xs font-semibold result-badge-ng";
          resultsSection.classList.remove("hidden");
          return;
        }

        if (util <= 1.0) {
          summaryEl.className =
            "p-4 rounded-md border border-green-200 bg-green-50 text-green-800";
          summaryEl.innerHTML =
            "OK: Cross-section axial resistance is adequate. N<sub>Ed</sub> ≤ N<sub>pl,Rd</sub>.";
          statusEl.textContent = "PASS";
          statusEl.className =
            "inline-block px-2 py-0.5 rounded text-xs font-semibold result-badge-ok";
        } else {
          summaryEl.className =
            "p-4 rounded-md border border-red-200 bg-red-50 text-red-800";
          summaryEl.innerHTML =
            "Not OK: Design axial load exceeds cross-section resistance. N<sub>Ed</sub> &gt; N<sub>pl,Rd</sub>.";
          statusEl.textContent = "FAIL";
          statusEl.className =
            "inline-block px-2 py-0.5 rounded text-xs font-semibold result-badge-ng";
        }

        resultsSection.classList.remove("hidden");
      }

      document.getElementById("btn-calc").addEventListener("click", computeEC4);

      document
        .getElementById("btn-example")
        .addEventListener("click", function () {
          document.getElementById("column-type").value = "encased";
          document.getElementById("gamma-m").value = 1.0;
          document.getElementById("as").value = 80;
          document.getElementById("fya").value = 355;
          document.getElementById("ac").value = 900;
          document.getElementById("fck").value = 30;
          document.getElementById("alpha-cc").value = 0.85;
          document.getElementById("gamma-c").value = 1.5;
          document.getElementById("asl").value = 8.0;
          document.getElementById("fyd").value = 500;
          document.getElementById("ned").value = 4000;
          computeEC4();
        });

      document
        .getElementById("btn-reset")
        .addEventListener("click", function () {
          document.getElementById("results-section").classList.add("hidden");
        });

      // Simple hint when switching type (no structural change, just UX)
      document
        .getElementById("column-type")
        .addEventListener("change", function () {
          const type = this.value;
          const acHelp = document.querySelector('label[for="ac"] + input + p');
          if (!acHelp) return;
          if (type === "cfst") {
            acHelp.textContent =
              "Internal concrete area of the steel tube (ignore steel thickness).";
          } else {
            acHelp.textContent = "Effective concrete area in compression.";
          }
        });
    </script>

