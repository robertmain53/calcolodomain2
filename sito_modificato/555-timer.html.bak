    <script>
      // Simple E12 rounding for resistors (ohms)
      function roundToE12(valueOhms) {
        if (!isFinite(valueOhms) || valueOhms <= 0) return NaN;
        const e12 = [
          1.0, 1.2, 1.5, 1.8, 2.2, 2.7, 3.3, 3.9, 4.7, 5.6, 6.8, 8.2,
        ];
        const decade = Math.pow(10, Math.floor(Math.log10(valueOhms)));
        const norm = valueOhms / decade;
        let best = e12[0],
          bestDiff = Math.abs(norm - e12[0]);
        for (let i = 1; i < e12.length; i++) {
          const diff = Math.abs(norm - e12[i]);
          if (diff < bestDiff) {
            bestDiff = diff;
            best = e12[i];
          }
        }
        return best * decade;
      }

      function formatOhms(ohms) {
        if (!isFinite(ohms) || ohms <= 0) return "–";
        const units = [
          { u: "MΩ", f: 1e6 },
          { u: "kΩ", f: 1e3 },
          { u: "Ω", f: 1 },
        ];
        for (const { u, f } of units) {
          if (ohms >= f)
            return (ohms / f).toFixed(2).replace(/\.00$/, "") + " " + u;
        }
        return ohms.toFixed(2).replace(/\.00$/, "") + " Ω";
      }

      function formatFarads(F) {
        if (!isFinite(F) || F <= 0) return "–";
        const units = [
          { u: "mF", f: 1e-3 },
          { u: "µF", f: 1e-6 },
          { u: "nF", f: 1e-9 },
          { u: "pF", f: 1e-12 },
        ];
        for (const { u, f } of units) {
          if (F >= f) return (F / f).toFixed(3).replace(/\.000$/, "") + " " + u;
        }
        return F.toExponential(2) + " F";
      }

      function formatSeconds(s) {
        if (!isFinite(s) || s <= 0) return "–";
        if (s < 1e-3) return (s * 1e6).toFixed(2).replace(/\.00$/, "") + " µs";
        if (s < 1) return (s * 1e3).toFixed(2).replace(/\.00$/, "") + " ms";
        if (s < 60)
          return s.toFixed(4).replace(/0+$/, "").replace(/\.$/, "") + " s";
        return (s / 60).toFixed(2).replace(/\.00$/, "") + " min";
      }

      function formatHz(f) {
        if (!isFinite(f) || f <= 0) return "–";
        if (f < 1)
          return f.toFixed(3).replace(/0+$/, "").replace(/\.$/, "") + " Hz";
        if (f < 1e3) return f.toFixed(2).replace(/\.00$/, "") + " Hz";
        if (f < 1e6) return (f / 1e3).toFixed(2).replace(/\.00$/, "") + " kHz";
        return (f / 1e6).toFixed(2).replace(/\.00$/, "") + " MHz";
      }

      // Tab switching
      const tabAstable = document.getElementById("tab-astable");
      const tabMono = document.getElementById("tab-monostable");
      const panelAstable = document.getElementById("panel-astable");
      const panelMono = document.getElementById("panel-monostable");

      function setMode(mode) {
        if (mode === "astable") {
          tabAstable.classList.add("tab-active");
          tabMono.classList.remove("tab-active");
          panelAstable.classList.remove("hidden");
          panelMono.classList.add("hidden");
          tabAstable.setAttribute("aria-selected", "true");
          tabMono.setAttribute("aria-selected", "false");
        } else {
          tabMono.classList.add("tab-active");
          tabAstable.classList.remove("tab-active");
          panelMono.classList.remove("hidden");
          panelAstable.classList.add("hidden");
          tabMono.setAttribute("aria-selected", "true");
          tabAstable.setAttribute("aria-selected", "false");
        }
      }

      tabAstable.addEventListener("click", () => setMode("astable"));
      tabMono.addEventListener("click", () => setMode("monostable"));

      // Astable calculation
      document.getElementById("astable-calc").addEventListener("click", () => {
        const errEl = document.getElementById("astable-error");
        errEl.textContent = "";
        const results = document.getElementById("astable-results");
        results.classList.add("hidden");

        const Cval = parseFloat(document.getElementById("astable-cap").value);
        const Cunit = parseFloat(
          document.getElementById("astable-cap-unit").value
        );
        const fVal = parseFloat(document.getElementById("astable-freq").value);
        const fUnit = parseFloat(
          document.getElementById("astable-freq-unit").value
        );
        const dutyVal = parseFloat(
          document.getElementById("astable-duty").value
        );
        const R1in = parseFloat(document.getElementById("astable-r1").value);
        const R2in = parseFloat(document.getElementById("astable-r2").value);

        const f = isFinite(fVal) ? fVal * fUnit : NaN;
        const C = isFinite(Cval) ? Cval * Cunit : NaN;
        const D = isFinite(dutyVal) ? dutyVal / 100 : NaN;
        const R1k = isFinite(R1in) ? R1in : NaN;
        const R2k = isFinite(R2in) ? R2in : NaN;

        let R1,
          R2,
          Cuse = C,
          fuse = f;

        if (!isFinite(fuse) && (!isFinite(R1k) || !isFinite(R2k))) {
          errEl.textContent =
            "Provide either a target frequency (and optionally duty cycle) or both R1 and R2.";
          return;
        }

        if (!isFinite(Cuse)) {
          // Suggest C based on frequency if possible
          if (isFinite(fuse)) {
            // Aim for R in tens of kΩ range: choose C so that (R1+2R2) ~ 100 kΩ
            // For rough estimate, assume (R1+2R2) ≈ 100 kΩ => C ≈ 1.44 / (f * 100k)
            Cuse = 1.44 / (fuse * 100e3);
          } else {
            Cuse = 0.000001; // 1 µF fallback
          }
        }

        if (isFinite(R1k) && isFinite(R2k)) {
          // Use given R1, R2, compute f and D
          R1 = R1k * 1e3;
          R2 = R2k * 1e3;
          const T = 0.693 * (R1 + 2 * R2) * Cuse;
          fuse = 1 / T;
        } else if (isFinite(fuse) && isFinite(D)) {
          // Solve for R1 and R2 from f and D
          // From formulas:
          // T = 1/f = 0.693 (R1 + 2R2) C
          // D = (R1 + R2) / (R1 + 2R2)
          // Let X = R1 + 2R2 = T / (0.693 C)
          const T = 1 / fuse;
          const X = T / (0.693 * Cuse);
          if (D <= 0.5 || D >= 1) {
            errEl.textContent =
              "Duty cycle must be between 50% and 100% for the classic astable circuit.";
            return;
          }
          // From D = (R1 + R2)/X and X = R1 + 2R2:
          // Solve: R2 = X (1 - D), R1 = X (2D - 1)
          R2 = X * (1 - D);
          R1 = X * (2 * D - 1);
          if (R1 <= 0 || R2 <= 0) {
            errEl.textContent =
              "Unable to find positive resistor values for the requested duty cycle.";
            return;
          }
        } else if (isFinite(fuse) && isFinite(R1k)) {
          // Known f and R1, solve for R2 assuming some duty (approx 60% if not given)
          const T = 1 / fuse;
          const X = T / (0.693 * Cuse);
          R1 = R1k * 1e3;
          R2 = (X - R1) / 2;
          if (R2 <= 0) {
            errEl.textContent =
              "Given R1 and frequency lead to negative R2. Try a different R1 or C.";
            return;
          }
        } else if (isFinite(fuse) && isFinite(R2k)) {
          // Known f and R2, solve for R1 (no duty constraint)
          const T = 1 / fuse;
          const X = T / (0.693 * Cuse);
          R2 = R2k * 1e3;
          R1 = X - 2 * R2;
          if (R1 <= 0) {
            errEl.textContent =
              "Given R2 and frequency lead to negative R1. Try a different R2 or C.";
            return;
          }
        } else {
          errEl.textContent =
            "Not enough information to solve. Provide frequency plus duty, or R1 and R2.";
          return;
        }

        // Round resistors to E12
        const R1r = roundToE12(R1);
        const R2r = roundToE12(R2);

        // Recompute timing with rounded values
        const Treal = 0.693 * (R1r + 2 * R2r) * Cuse;
        const freal = 1 / Treal;
        const tH = 0.693 * (R1r + R2r) * Cuse;
        const tL = 0.693 * R2r * Cuse;
        const Dreal = tH / (tH + tL);

        document.getElementById("astable-r1-out").textContent = formatOhms(R1r);
        document.getElementById("astable-r2-out").textContent = formatOhms(R2r);
        document.getElementById("astable-c-out").textContent =
          formatFarads(Cuse);
        document.getElementById("astable-f-out").textContent = formatHz(freal);
        document.getElementById("astable-t-out").textContent =
          formatSeconds(Treal);
        document.getElementById("astable-duty-out").textContent =
          (Dreal * 100).toFixed(2).replace(/\.00$/, "") + " %";

        results.classList.remove("hidden");
      });

      document.getElementById("astable-reset").addEventListener("click", () => {
        [
          "astable-cap",
          "astable-freq",
          "astable-duty",
          "astable-r1",
          "astable-r2",
        ].forEach((id) => {
          document.getElementById(id).value = "";
        });
        document.getElementById("astable-cap-unit").value = "1e-6";
        document.getElementById("astable-freq-unit").value = "1e3";
        document.getElementById("astable-error").textContent = "";
        document.getElementById("astable-results").classList.add("hidden");
      });

      // Monostable calculation
      document.getElementById("mono-calc").addEventListener("click", () => {
        const errEl = document.getElementById("mono-error");
        errEl.textContent = "";
        const results = document.getElementById("mono-results");
        results.classList.add("hidden");

        const tVal = parseFloat(document.getElementById("mono-pulse").value);
        const tUnit = parseFloat(
          document.getElementById("mono-pulse-unit").value
        );
        const Cval = parseFloat(document.getElementById("mono-cap").value);
        const Cunit = parseFloat(
          document.getElementById("mono-cap-unit").value
        );
        const Rval = parseFloat(document.getElementById("mono-r").value);

        if (!isFinite(tVal) || tVal <= 0) {
          errEl.textContent = "Enter a valid pulse width.";
          return;
        }
        const t = tVal * tUnit;
        let C = isFinite(Cval) ? Cval * Cunit : NaN;
        let R = isFinite(Rval) ? Rval * 1e3 : NaN;

        if (!isFinite(C) && !isFinite(R)) {
          // Suggest C and compute R
          // Aim for R around 100 kΩ: C ≈ t / (1.1 * 100k)
          C = t / (1.1 * 100e3);
        }

        if (!isFinite(C) && isFinite(R)) {
          C = t / (1.1 * R);
        } else if (isFinite(C) && !isFinite(R)) {
          R = t / (1.1 * C);
        }

        if (!isFinite(C) || !isFinite(R) || C <= 0 || R <= 0) {
          errEl.textContent =
            "Unable to find valid R and C values. Adjust your inputs.";
          return;
        }

        const Rr = roundToE12(R);
        const treal = 1.1 * Rr * C;

        document.getElementById("mono-r-out").textContent = formatOhms(Rr);
        document.getElementById("mono-c-out").textContent = formatFarads(C);
        document.getElementById("mono-t-out").textContent =
          formatSeconds(treal);

        results.classList.remove("hidden");
      });

      document.getElementById("mono-reset").addEventListener("click", () => {
        ["mono-pulse", "mono-cap", "mono-r"].forEach((id) => {
          document.getElementById(id).value = "";
        });
        document.getElementById("mono-pulse-unit").value = "1";
        document.getElementById("mono-cap-unit").value = "1e-6";
        document.getElementById("mono-error").textContent = "";
        document.getElementById("mono-results").classList.add("hidden");
      });

      // Mobile menu script (simple toggle)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", () => {
          const menu = document.getElementById("mobile-menu");
          const expanded = menu.classList.contains("hidden") ? "true" : "false";
          menu.classList.toggle("hidden");
          document
            .getElementById("mobile-menu-toggle")
            .setAttribute("aria-expanded", expanded);
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
