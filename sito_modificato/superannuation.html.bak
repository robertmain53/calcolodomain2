    <script>
      function formatCurrency(value) {
        if (isNaN(value)) return "$0";
        const abs = Math.abs(value);
        let formatted;
        if (abs >= 1e9) {
          formatted = (value / 1e9).toFixed(2) + "b";
        } else if (abs >= 1e6) {
          formatted = (value / 1e6).toFixed(2) + "m";
        } else if (abs >= 1e3) {
          formatted = (value / 1e3).toFixed(1) + "k";
        } else {
          formatted = value.toFixed(0);
        }
        return "$" + formatted;
      }

      function formatCurrencyFull(value) {
        return value.toLocaleString("en-AU", {
          style: "currency",
          currency: "AUD",
          maximumFractionDigits: 0,
        });
      }

      function formatPercent(value, decimals = 1) {
        return value.toFixed(decimals) + "%";
      }

      function updateRangeLabels() {
        const grossReturn = document.getElementById("grossReturn");
        const grossReturnVal = document.getElementById("grossReturnVal");
        const earningsTax = document.getElementById("earningsTax");
        const earningsTaxVal = document.getElementById("earningsTaxVal");
        const adminFee = document.getElementById("adminFee");
        const adminFeeVal = document.getElementById("adminFeeVal");
        const contribTax = document.getElementById("contribTax");
        const contribTaxVal = document.getElementById("contribTaxVal");

        if (grossReturn && grossReturnVal)
          grossReturnVal.textContent = formatPercent(
            parseFloat(grossReturn.value)
          );
        if (earningsTax && earningsTaxVal)
          earningsTaxVal.textContent = formatPercent(
            parseFloat(earningsTax.value)
          );
        if (adminFee && adminFeeVal)
          adminFeeVal.textContent = formatPercent(
            parseFloat(adminFee.value),
            2
          );
        if (contribTax && contribTaxVal)
          contribTaxVal.textContent = formatPercent(
            parseFloat(contribTax.value)
          );
      }

      document.addEventListener("input", function (e) {
        if (
          ["grossReturn", "earningsTax", "adminFee", "contribTax"].includes(
            e.target.id
          )
        ) {
          updateRangeLabels();
        }
      });

      function calculateProjection() {
        const errorMsg = document.getElementById("errorMsg");
        errorMsg.textContent = "";

        const age = parseFloat(document.getElementById("age").value);
        const retirementAge = parseFloat(
          document.getElementById("retirementAge").value
        );
        const salary = parseFloat(document.getElementById("salary").value);
        const currentBalance = parseFloat(
          document.getElementById("currentBalance").value
        );
        const sgRate =
          parseFloat(document.getElementById("sgRate").value) / 100;
        const salarySacrifice =
          parseFloat(document.getElementById("salarySacrifice").value) || 0;
        const afterTaxContrib =
          parseFloat(document.getElementById("afterTaxContrib").value) || 0;
        const grossReturn =
          parseFloat(document.getElementById("grossReturn").value) / 100;
        const earningsTax =
          parseFloat(document.getElementById("earningsTax").value) / 100;
        const adminFee =
          parseFloat(document.getElementById("adminFee").value) / 100;
        const fixedFee =
          parseFloat(document.getElementById("fixedFee").value) || 0;
        const contribTax =
          parseFloat(document.getElementById("contribTax").value) / 100;

        if (
          isNaN(age) ||
          isNaN(retirementAge) ||
          isNaN(salary) ||
          isNaN(currentBalance)
        ) {
          errorMsg.textContent =
            "Please fill in at least age, retirement age, salary and current balance.";
          return;
        }
        if (retirementAge <= age) {
          errorMsg.textContent =
            "Retirement age must be greater than current age.";
          return;
        }
        if (age < 15 || age > 75 || retirementAge < 40 || retirementAge > 80) {
          errorMsg.textContent =
            "Please use realistic ages (15–75 for current age, 40–80 for retirement).";
          return;
        }

        const years = Math.round(retirementAge - age);

        const sgContrib = salary * sgRate;
        const concessionalBeforeTax = sgContrib + salarySacrifice;
        const concessionalAfterTax = concessionalBeforeTax * (1 - contribTax);
        const totalAnnualContrib = concessionalAfterTax + afterTaxContrib;

        const netReturnBeforeFees = grossReturn * (1 - earningsTax);
        const netRate = netReturnBeforeFees - adminFee;

        let balance = currentBalance;
        let totalContribs = 0;
        let totalFees = 0;
        const yearlyData = [];

        for (let i = 0; i < years; i++) {
          const year = new Date().getFullYear() + i;
          const openingBalance = balance;

          const fixedFeesThisYear = fixedFee;
          const percentFeesThisYear = openingBalance * adminFee;
          const feesThisYear = fixedFeesThisYear + percentFeesThisYear;

          const earningsBeforeTaxFees = openingBalance * grossReturn;
          const earningsTaxAmount = earningsBeforeTaxFees * earningsTax;
          const earningsAfterTax =
            earningsBeforeTaxFees - earningsTaxAmount - percentFeesThisYear;

          balance =
            openingBalance +
            totalAnnualContrib -
            fixedFeesThisYear +
            earningsAfterTax;

          totalContribs += totalAnnualContrib;
          totalFees += feesThisYear + earningsTaxAmount;

          yearlyData.push({
            year,
            age: age + i,
            openingBalance,
            contributions: totalAnnualContrib,
            earningsAfterTax,
            fees: feesThisYear + earningsTaxAmount,
            closingBalance: balance,
          });
        }

        const finalBalance = balance;
        const conservativeIncome = finalBalance * 0.035;
        const moderateIncome = finalBalance * 0.04;
        const higherIncome = finalBalance * 0.05;

        renderResults({
          years,
          finalBalance,
          totalContribs,
          totalFees,
          netRate,
          yearlyData,
          conservativeIncome,
          moderateIncome,
          higherIncome,
        });
      }

      function renderResults(data) {
        const container = document.getElementById("resultsContainer");
        if (!container) return;

        const {
          years,
          finalBalance,
          totalContribs,
          totalFees,
          netRate,
          yearlyData,
          conservativeIncome,
          moderateIncome,
          higherIncome,
        } = data;

        const first = yearlyData[0];
        const mid = yearlyData[Math.floor(yearlyData.length / 2)];
        const last = yearlyData[yearlyData.length - 1];

        const rows = [first, mid, last]
          .filter(Boolean)
          .map(
            (row) => `
      <tr>
        <td class="whitespace-nowrap">${row.year}</td>
        <td class="text-right">${formatCurrencyFull(row.openingBalance)}</td>
        <td class="text-right">${formatCurrencyFull(row.contributions)}</td>
        <td class="text-right">${formatCurrencyFull(row.earningsAfterTax)}</td>
        <td class="text-right">${formatCurrencyFull(row.fees)}</td>
        <td class="text-right font-medium">${formatCurrencyFull(
          row.closingBalance
        )}</td>
      </tr>
    `
          )
          .join("");

        container.innerHTML = `
      <div class="grid md:grid-cols-3 gap-4">
        <div class="border border-gray-200 rounded-md p-3 bg-gray-50">
          <p class="text-xs text-gray-500 uppercase tracking-wide">Projected final balance</p>
          <p class="text-xl font-semibold mt-1">${formatCurrencyFull(
            finalBalance
          )}</p>
          <p class="text-xs text-gray-500 mt-1">After ${years} years of contributions and compounding.</p>
        </div>
        <div class="border border-gray-200 rounded-md p-3">
          <p class="text-xs text-gray-500 uppercase tracking-wide">Total contributions paid in</p>
          <p class="text-lg font-semibold mt-1">${formatCurrencyFull(
            totalContribs
          )}</p>
          <p class="text-xs text-gray-500 mt-1">Includes employer SG, salary sacrifice and after-tax contributions.</p>
        </div>
        <div class="border border-gray-200 rounded-md p-3">
          <p class="text-xs text-gray-500 uppercase tracking-wide">Total fees & tax (approx.)</p>
          <p class="text-lg font-semibold mt-1">${formatCurrencyFull(
            totalFees
          )}</p>
          <p class="text-xs text-gray-500 mt-1">Estimated fund fees and tax on earnings over the projection.</p>
        </div>
      </div>

      <div class="mt-5 border border-blue-100 bg-blue-50 rounded-md p-4">
        <p class="text-sm font-semibold text-blue-900 mb-2">Indicative retirement income from super</p>
        <div class="grid md:grid-cols-3 gap-3 text-sm">
          <div>
            <p class="text-gray-500 text-xs uppercase">Conservative (3.5% drawdown)</p>
            <p class="font-semibold">${formatCurrencyFull(
              conservativeIncome
            )} / year</p>
          </div>
          <div>
            <p class="text-gray-500 text-xs uppercase">Moderate (4.0% drawdown)</p>
            <p class="font-semibold">${formatCurrencyFull(
              moderateIncome
            )} / year</p>
          </div>
          <div>
            <p class="text-gray-500 text-xs uppercase">Higher risk (5.0% drawdown)</p>
            <p class="font-semibold">${formatCurrencyFull(
              higherIncome
            )} / year</p>
          </div>
        </div>
        <p class="text-xs text-blue-900 mt-2">
          These drawdown rates are simple rules-of-thumb only and do not guarantee your money will last for a particular period.
        </p>
      </div>

