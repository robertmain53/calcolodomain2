          knownFeed: document.getElementById("label-mill-known-feed-unit"),
          feedUnitText: document.getElementById("label-mill-feed-unit-text"),
          sfmUnitText: document.getElementById("label-mill-sfm-unit-text"),
          chiploadOutUnitText: document.getElementById(
            "label-mill-chipload-out-unit-text"
          ),
          mrrUnitText: document.getElementById("label-mill-mrr-unit-text"),
        },
        turn: {
          diameter: document.getElementById("label-turn-diameter-unit"),
          surfaceSpeed: document.getElementById(
            "label-turn-surface-speed-unit"
          ),
          feedPerRev: document.getElementById("label-turn-feed-per-rev-unit"),
          depthCut: document.getElementById("label-turn-depth-cut-unit"),
          knownFeed: document.getElementById("label-turn-known-feed-unit"),
          feedUnitText: document.getElementById("label-turn-feed-unit-text"),
          sfmUnitText: document.getElementById("label-turn-sfm-unit-text"),
          fprOutUnitText: document.getElementById(
            "label-turn-fpr-out-unit-text"
          ),
          mrrUnitText: document.getElementById("label-turn-mrr-unit-text"),
        },
      };

      // Inputs
      const inputs = {
        mill: {
          toolDiameter: document.getElementById("mill-tool-diameter"),
          flutes: document.getElementById("mill-flutes"),
          surfaceSpeed: document.getElementById("mill-surface-speed"),
          chipLoad: document.getElementById("mill-chip-load"),
          widthCut: document.getElementById("mill-width-cut"),
          depthCut: document.getElementById("mill-depth-cut"),
          knownRpm: document.getElementById("mill-known-rpm"),
          knownFeed: document.getElementById("mill-known-feed"),
        },
        turn: {
          diameter: document.getElementById("turn-diameter"),
          surfaceSpeed: document.getElementById("turn-surface-speed"),
          feedPerRev: document.getElementById("turn-feed-per-rev"),
          depthCut: document.getElementById("turn-depth-cut"),
          knownRpm: document.getElementById("turn-known-rpm"),
          knownFeed: document.getElementById("turn-known-feed"),
        },
      };

      // Outputs
      const outputs = {
        mill: {
          rpm: document.getElementById("mill-rpm"),
          feed: document.getElementById("mill-feed"),
          sfm: document.getElementById("mill-sfm"),
          chiploadOut: document.getElementById("mill-chipload-out"),
          mrr: document.getElementById("mill-mrr"),
        },
        turn: {
          rpm: document.getElementById("turn-rpm"),
          feed: document.getElementById("turn-feed"),
          sfm: document.getElementById("turn-sfm"),
          fprOut: document.getElementById("turn-fpr-out"),
          mrr: document.getElementById("turn-mrr"),
        },
      };

      function setUnits(units) {
        state.units = units;
        const isImperial = units === "imperial";

        // Toggle buttons
        if (isImperial) {
          unitsImperialBtn.classList.add(
            "border-blue-600",
            "bg-blue-50",
            "text-blue-700"
          );
          unitsImperialBtn.classList.remove("border-gray-300", "text-gray-600");
          unitsMetricBtn.classList.remove(
            "border-blue-600",
            "bg-blue-50",
            "text-blue-700"
          );
          unitsMetricBtn.classList.add("border-gray-300", "text-gray-600");
        } else {
          unitsMetricBtn.classList.add(
            "border-blue-600",
            "bg-blue-50",
            "text-blue-700"
          );
          unitsMetricBtn.classList.remove("border-gray-300", "text-gray-600");
          unitsImperialBtn.classList.remove(
            "border-blue-600",
            "bg-blue-50",
            "text-blue-700"
          );
          unitsImperialBtn.classList.add("border-gray-300", "text-gray-600");
        }

        // Update labels
        labels.mill.toolDiameter.textContent = isImperial ? "in" : "mm";
        labels.mill.surfaceSpeed.textContent = isImperial ? "SFM" : "m/min";
        labels.mill.chipLoad.textContent = isImperial ? "in/tooth" : "mm/tooth";
        labels.mill.widthCut.textContent = isImperial ? "in" : "mm";
        labels.mill.depthCut.textContent = isImperial ? "in" : "mm";
        labels.mill.knownFeed.textContent = isImperial ? "IPM" : "mm/min";
        labels.mill.feedUnitText.textContent = isImperial ? "IPM" : "mm/min";
        labels.mill.sfmUnitText.textContent = isImperial ? "SFM" : "m/min";
        labels.mill.chiploadOutUnitText.textContent = isImperial
          ? "in/tooth"
          : "mm/tooth";
        labels.mill.mrrUnitText.textContent = isImperial
          ? "in³/min"
          : "cm³/min";

        labels.turn.diameter.textContent = isImperial ? "in" : "mm";
        labels.turn.surfaceSpeed.textContent = isImperial ? "SFM" : "m/min";
        labels.turn.feedPerRev.textContent = isImperial ? "in/rev" : "mm/rev";
        labels.turn.depthCut.textContent = isImperial ? "in" : "mm";
        labels.turn.knownFeed.textContent = isImperial ? "IPM" : "mm/min";
        labels.turn.feedUnitText.textContent = isImperial ? "IPM" : "mm/min";
        labels.turn.sfmUnitText.textContent = isImperial ? "SFM" : "m/min";
        labels.turn.fprOutUnitText.textContent = isImperial
          ? "in/rev"
          : "mm/rev";
        labels.turn.mrrUnitText.textContent = isImperial
          ? "in³/min"
          : "cm³/min";
      }

      function setMode(mode) {
        state.mode = mode;
        if (mode === "milling") {
          tabMilling.classList.add("border-blue-600", "text-blue-600");
          tabMilling.classList.remove("border-transparent", "text-gray-600");
          tabTurning.classList.remove("border-blue-600", "text-blue-600");
          tabTurning.classList.add("border-transparent", "text-gray-600");
          panelMilling.classList.remove("hidden");
          panelTurning.classList.add("hidden");
        } else {
          tabTurning.classList.add("border-blue-600", "text-blue-600");
          tabTurning.classList.remove("border-transparent", "text-gray-600");
          tabMilling.classList.remove("border-blue-600", "text-blue-600");
          tabMilling.classList.add("border-transparent", "text-gray-600");
          panelTurning.classList.remove("hidden");
          panelMilling.classList.add("hidden");
        }
      }

      function clearAll() {
        Object.values(inputs.mill).forEach((el) => {
          if (el) el.value = "";
        });
        Object.values(inputs.turn).forEach((el) => {
          if (el) el.value = "";
        });
        Object.values(outputs.mill).forEach((el) => (el.textContent = "–"));
        Object.values(outputs.turn).forEach((el) => (el.textContent = "–"));
      }

      // Milling calculations
      function calcMilling() {
        const D = parseFloat(inputs.mill.toolDiameter.value);
        const z = parseFloat(inputs.mill.flutes.value);
        const V = parseFloat(inputs.mill.surfaceSpeed.value);
        const fz = parseFloat(inputs.mill.chipLoad.value);
        const W = parseFloat(inputs.mill.widthCut.value);
        const ap = parseFloat(inputs.mill.depthCut.value);

        if (!D || !z || !V || !fz) {
          alert(
            "Please enter tool diameter, flutes, cutting speed, and chip load."
          );
          return;
        }

        const isImperial = state.units === "imperial";
        let D_in = D;
        let W_in = W || 0;
        let ap_in = ap || 0;

        if (!isImperial) {
          // convert mm to inches
          D_in = D / 25.4;
          W_in = W_in / 25.4;
          ap_in = ap_in / 25.4;
        }

        // RPM from cutting speed
        let rpm;
        if (isImperial) {
          rpm = (12 * V) / (Math.PI * D_in);
        } else {
          // V in m/min, D in mm
          rpm = (1000 * V) / (Math.PI * D);
        }

        // Feed rate
        const feed = fz * z * rpm; // IPM or mm/min

        // Surface speed back-calculated for consistency
        let surf;
        if (isImperial) {
          surf = (Math.PI * D_in * rpm) / 12;
        } else {
          surf = (Math.PI * D * rpm) / 1000;
        }

        // MRR
        let mrr = 0;
        if (W_in > 0 && ap_in > 0) {
          if (isImperial) {
            mrr = W_in * ap_in * feed; // in³/min
          } else {
            // convert W, ap to cm, feed to cm/min
            const W_cm = (W || 0) / 10;
            const ap_cm = (ap || 0) / 10;
            const feed_cm_min = feed / 10;
            mrr = W_cm * ap_cm * feed_cm_min; // cm³/min
          }
        }

        outputs.mill.rpm.textContent = roundSmart(rpm, 1);
        outputs.mill.feed.textContent = roundSmart(feed, 2);
        outputs.mill.sfm.textContent = roundSmart(surf, 1);
        outputs.mill.chiploadOut.textContent = roundSmart(fz, 4);
        outputs.mill.mrr.textContent = mrr ? roundSmart(mrr, 2) : "–";
      }

      function calcMillingFromKnown() {
        const D = parseFloat(inputs.mill.toolDiameter.value);
        const z = parseFloat(inputs.mill.flutes.value);
        const rpm = parseFloat(inputs.mill.knownRpm.value);
        const feed = parseFloat(inputs.mill.knownFeed.value);

        if (!D || !z || !rpm || !feed) {
          alert("Please enter tool diameter, flutes, RPM, and feed rate.");
          return;
        }

        const isImperial = state.units === "imperial";
        let D_in = D;
        if (!isImperial) D_in = D / 25.4;

        // Chip load per tooth
        const fz = feed / (z * rpm);

        // Surface speed
        let surf;
        if (isImperial) {
          surf = (Math.PI * D_in * rpm) / 12;
        } else {
          surf = (Math.PI * D * rpm) / 1000;
        }

        outputs.mill.rpm.textContent = roundSmart(rpm, 1);
        outputs.mill.feed.textContent = roundSmart(feed, 2);
        outputs.mill.sfm.textContent = roundSmart(surf, 1);
        outputs.mill.chiploadOut.textContent = roundSmart(fz, 4);
        // MRR not computed here (no width/depth)
      }

      // Turning calculations
      function calcTurning() {
        const D = parseFloat(inputs.turn.diameter.value);
        const V = parseFloat(inputs.turn.surfaceSpeed.value);
        const f_rev = parseFloat(inputs.turn.feedPerRev.value);
        const doc = parseFloat(inputs.turn.depthCut.value);

        if (!D || !V || !f_rev) {
          alert(
            "Please enter diameter, cutting speed, and feed per revolution."
          );
          return;
        }

        const isImperial = state.units === "imperial";
        let D_in = D;
        let doc_in = doc || 0;

        if (!isImperial) {
          D_in = D / 25.4;
          doc_in = doc_in / 25.4;
        }

        // RPM from cutting speed
        let rpm;
        if (isImperial) {
          rpm = (12 * V) / (Math.PI * D_in);
        } else {
          rpm = (1000 * V) / (Math.PI * D);
        }

        const feed = f_rev * rpm;

        // Surface speed back-calculated
        let surf;
        if (isImperial) {
          surf = (Math.PI * D_in * rpm) / 12;
        } else {
          surf = (Math.PI * D * rpm) / 1000;
        }

        // MRR (approx, per unit length)
        let mrr = 0;
        if (doc_in > 0) {
          if (isImperial) {
            mrr = 2 * doc_in * feed; // in²/min * 1 in length
          } else {
            const doc_cm = (doc || 0) / 10;
            const feed_cm_min = feed / 10;
            mrr = 2 * doc_cm * feed_cm_min; // cm²/min * 1 cm length
          }
        }

        outputs.turn.rpm.textContent = roundSmart(rpm, 1);
        outputs.turn.feed.textContent = roundSmart(feed, 2);
        outputs.turn.sfm.textContent = roundSmart(surf, 1);
        outputs.turn.fprOut.textContent = roundSmart(f_rev, 4);
        outputs.turn.mrr.textContent = mrr ? roundSmart(mrr, 2) : "–";
      }

      function calcTurningFromKnown() {
        const D = parseFloat(inputs.turn.diameter.value);
        const rpm = parseFloat(inputs.turn.knownRpm.value);
        const feed = parseFloat(inputs.turn.knownFeed.value);

        if (!D || !rpm || !feed) {
          alert("Please enter diameter, RPM, and feed rate.");
          return;
        }

        const isImperial = state.units === "imperial";
        let D_in = D;
        if (!isImperial) D_in = D / 25.4;

        // Feed per rev
        const f_rev = feed / rpm;

        // Surface speed
        let surf;
        if (isImperial) {
          surf = (Math.PI * D_in * rpm) / 12;
        } else {
          surf = (Math.PI * D * rpm) / 1000;
        }

        outputs.turn.rpm.textContent = roundSmart(rpm, 1);
        outputs.turn.feed.textContent = roundSmart(feed, 2);
        outputs.turn.sfm.textContent = roundSmart(surf, 1);
        outputs.turn.fprOut.textContent = roundSmart(f_rev, 4);
      }

      // Event listeners
      unitsImperialBtn.addEventListener("click", () => setUnits("imperial"));
      unitsMetricBtn.addEventListener("click", () => setUnits("metric"));

      tabMilling.addEventListener("click", () => setMode("milling"));
      tabTurning.addEventListener("click", () => setMode("turning"));

      btnClear.addEventListener("click", clearAll);

      btnMillCalc.addEventListener("click", calcMilling);
      btnMillFromRpm.addEventListener("click", () => {
        panelMillKnown.classList.toggle("hidden");
      });
      btnMillCalcChipload.addEventListener("click", calcMillingFromKnown);

      btnTurnCalc.addEventListener("click", calcTurning);
      btnTurnFromRpm.addEventListener("click", () => {
        panelTurnKnown.classList.toggle("hidden");
      });
      btnTurnCalcFpr.addEventListener("click", calcTurningFromKnown);

      // Initialize defaults
      setUnits("imperial");
      setMode("milling");
    </script>

