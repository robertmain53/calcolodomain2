    <script>
      // Shape handling
      const shapeButtons = document.querySelectorAll(".shape-btn");
      const lengthInput = document.getElementById("length");
      const widthInput = document.getElementById("width");
      const widthWrapper = document.getElementById("width-wrapper");
      const innerLengthWrapper = document.getElementById(
        "inner-length-wrapper"
      );
      const innerWidthWrapper = document.getElementById("inner-width-wrapper");

      let currentShape = "rectangle";

      shapeButtons.forEach((btn) => {
        btn.addEventListener("click", () => {
          shapeButtons.forEach((b) => {
            b.classList.remove(
              "border-blue-600",
              "text-blue-600",
              "bg-blue-50"
            );
            b.classList.add("border-gray-300", "text-gray-700", "bg-white");
          });
          btn.classList.add("border-blue-600", "text-blue-600", "bg-blue-50");
          btn.classList.remove("border-gray-300", "text-gray-700", "bg-white");

          currentShape = btn.dataset.shape;
          updateShapeFields();
        });
      });

      function updateShapeFields() {
        if (currentShape === "rectangle") {
          widthWrapper.classList.remove("hidden");
          innerLengthWrapper.classList.add("hidden");
          innerWidthWrapper.classList.add("hidden");
        } else if (currentShape === "circle") {
          widthWrapper.classList.add("hidden");
          innerLengthWrapper.classList.add("hidden");
          innerWidthWrapper.classList.add("hidden");
        } else if (currentShape === "border") {
          widthWrapper.classList.remove("hidden");
          innerLengthWrapper.classList.remove("hidden");
          innerWidthWrapper.classList.remove("hidden");
        }
      }

      // Sand type / density presets
      const sandTypeSelect = document.getElementById("sand-type");
      const densityInput = document.getElementById("density");
      const densityUnitSelect = document.getElementById("density-unit");

      const densityPresets = {
        play: 1500, // kg/m3
        masonry: 1650,
        concrete: 1700,
        dry: 1450,
        wet: 1850,
      };

      function applyDensityPreset() {
        const type = sandTypeSelect.value;
        if (type === "custom") {
          densityInput.removeAttribute("readonly");
        } else {
          densityInput.value = densityPresets[type];
          densityUnitSelect.value = "kgm3";
          densityInput.setAttribute("readonly", "readonly");
        }
      }

      sandTypeSelect.addEventListener("change", applyDensityPreset);
      applyDensityPreset();

      // Unit conversion helpers
      function toMeters(value, unit) {
        const v = parseFloat(value);
        if (isNaN(v)) return 0;
        switch (unit) {
          case "m":
            return v;
          case "cm":
            return v / 100;
          case "ft":
            return v * 0.3048;
          case "in":
            return v * 0.0254;
          default:
            return v;
        }
      }

      function densityToKgPerM3(value, unit) {
        const v = parseFloat(value);
        if (isNaN(v) || v <= 0) return 0;
        if (unit === "kgm3") return v;
        if (unit === "lbft3") return v * 16.018463; // 1 lb/ft3 = 16.018463 kg/m3
        return v;
      }

      // Results elements
      const resVolumeMain = document.getElementById("res-volume-main");
      const resVolumeAlt = document.getElementById("res-volume-alt");
      const resArea = document.getElementById("res-area");
      const resDepth = document.getElementById("res-depth");
      const resWeightMain = document.getElementById("res-weight-main");
      const resWeightAlt = document.getElementById("res-weight-alt");
      const resTrucks = document.getElementById("res-trucks");
      const resCost = document.getElementById("res-cost");
      const errorMsg = document.getElementById("error-msg");

      function formatNumber(num, decimals = 2) {
        return Number(num).toLocaleString(undefined, {
          minimumFractionDigits: decimals,
          maximumFractionDigits: decimals,
        });
      }

      function calculateSand() {
        errorMsg.textContent = "";

        const lengthVal = parseFloat(document.getElementById("length").value);
        const lengthUnit = document.getElementById("length-unit").value;
        const widthVal = parseFloat(document.getElementById("width").value);
        const widthUnit = document.getElementById("width-unit").value;
        const innerLengthVal = parseFloat(
          document.getElementById("inner-length").value
        );
        const innerLengthUnit =
          document.getElementById("inner-length-unit").value;
        const innerWidthVal = parseFloat(
          document.getElementById("inner-width").value
        );
        const innerWidthUnit =
          document.getElementById("inner-width-unit").value;
        const depthVal = parseFloat(document.getElementById("depth").value);
        const depthUnit = document.getElementById("depth-unit").value;

        if (isNaN(depthVal) || depthVal <= 0) {
          errorMsg.textContent = "Please enter a positive sand depth.";
          return;
        }

        let area_m2 = 0;

        if (currentShape === "rectangle") {
          if (
            isNaN(lengthVal) ||
            isNaN(widthVal) ||
            lengthVal <= 0 ||
            widthVal <= 0
          ) {
            errorMsg.textContent = "Please enter positive length and width.";
            return;
          }
          const L = toMeters(lengthVal, lengthUnit);
          const W = toMeters(widthVal, widthUnit);
          area_m2 = L * W;
        } else if (currentShape === "circle") {
          if (isNaN(lengthVal) || lengthVal <= 0) {
            errorMsg.textContent = "Please enter a positive radius.";
            return;
          }
          const r = toMeters(lengthVal, lengthUnit);
          area_m2 = Math.PI * r * r;
        } else if (currentShape === "border") {
          if (
            isNaN(lengthVal) ||
            isNaN(widthVal) ||
            lengthVal <= 0 ||
            widthVal <= 0 ||
            isNaN(innerLengthVal) ||
            isNaN(innerWidthVal) ||
            innerLengthVal <= 0 ||
            innerWidthVal <= 0
          ) {
            errorMsg.textContent =
              "Please enter positive outer and inner dimensions.";
            return;
          }
          const Lo = toMeters(lengthVal, lengthUnit);
          const Wo = toMeters(widthVal, widthUnit);
          const Li = toMeters(innerLengthVal, innerLengthUnit);
          const Wi = toMeters(innerWidthVal, innerWidthUnit);
          if (Li >= Lo || Wi >= Wo) {
            errorMsg.textContent =
              "Inner dimensions must be smaller than outer dimensions.";
            return;
          }
          area_m2 = Lo * Wo - Li * Wi;
        }

        if (area_m2 <= 0) {
          errorMsg.textContent = "Calculated area is zero. Check your inputs.";
          return;
        }

        const depth_m = toMeters(depthVal, depthUnit);
        const volume_m3 = area_m2 * depth_m;

        const densityKgM3 = densityToKgPerM3(
          densityInput.value,
          densityUnitSelect.value
        );
        if (densityKgM3 <= 0) {
          errorMsg.textContent = "Please enter a valid sand density.";
          return;
        }

        const weight_kg = volume_m3 * densityKgM3;
        const weight_ton_metric = weight_kg / 1000;
        const weight_lb = weight_kg * 2.2046226218;
        const weight_ton_short = weight_lb / 2000;

        const volume_ft3 = volume_m3 * 35.3146667;
        const volume_yd3 = volume_ft3 / 27;

        // Area in ft² for display
        const area_ft2 = area_m2 * 10.7639104;

        // Cost and loads
        const priceVal = parseFloat(document.getElementById("price").value);
        const priceUnit = document.getElementById("price-unit").value;
        const truckCapVal = parseFloat(
          document.getElementById("truck-capacity").value
        );
        const truckUnit = document.getElementById("truck-unit").value;

        let costText = "–";
        if (!isNaN(priceVal) && priceVal > 0) {
          let qtyForPrice = 0;
          if (priceUnit === "per_ton") {
            qtyForPrice = weight_ton_short;
          } else if (priceUnit === "per_cy") {
            qtyForPrice = volume_yd3;
          } else if (priceUnit === "per_m3") {
            qtyForPrice = volume_m3;
          }
          const cost = qtyForPrice * priceVal;
          costText = isFinite(cost) ? "$" + formatNumber(cost, 2) : "–";
        }

        let loadsText = "–";
        if (!isNaN(truckCapVal) && truckCapVal > 0) {
          let requiredQty = 0;
          if (truckUnit === "ton") {
            requiredQty = weight_ton_short;
          } else if (truckUnit === "cy") {
            requiredQty = volume_yd3;
          } else if (truckUnit === "m3") {
            requiredQty = volume_m3;
          } else if (truckUnit === "kg") {
            requiredQty = weight_kg;
          } else if (truckUnit === "lb") {
            requiredQty = weight_lb;
          }
          const loads = requiredQty / truckCapVal;
          if (isFinite(loads) && loads > 0) {
            loadsText =
              formatNumber(loads, 2) +
              " loads (round up to " +
              Math.ceil(loads) +
              ")";
          }
        }

        // Update results
        resVolumeMain.textContent = formatNumber(volume_yd3, 2) + " yd³";
        resVolumeAlt.textContent =
          formatNumber(volume_m3, 3) +
          " m³ (" +
          formatNumber(volume_ft3, 1) +
          " ft³)";
        resArea.textContent =
          formatNumber(area_ft2, 1) +
          " ft² (" +
          formatNumber(area_m2, 2) +
          " m²)";
        resDepth.textContent =
          formatNumber(depth_m * 39.3701, 1) +
          " in (" +
          formatNumber(depth_m * 100, 1) +
          " cm)";

        resWeightMain.textContent =
          formatNumber(weight_ton_short, 2) + " short tons";
        resWeightAlt.textContent =
          formatNumber(weight_kg, 0) +
          " kg (" +
          formatNumber(weight_lb, 0) +
          " lb)";
        resTrucks.textContent = loadsText;
        resCost.textContent = costText;
      }

      document
        .getElementById("calculate-btn")
        .addEventListener("click", calculateSand);

      document.getElementById("reset-btn").addEventListener("click", () => {
        document.getElementById("length").value = 10;
        document.getElementById("width").value = 10;
        document.getElementById("inner-length").value = "";
        document.getElementById("inner-width").value = "";
        document.getElementById("depth").value = 2;
        document.getElementById("length-unit").value = "ft";
        document.getElementById("width-unit").value = "ft";
        document.getElementById("inner-length-unit").value = "ft";
        document.getElementById("inner-width-unit").value = "ft";
