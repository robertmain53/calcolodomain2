      function getNearestTableVoltage(motorType, voltage) {
        const tables = flcData[motorType];
        const available = Object.keys(tables).map(Number);
        let best = available[0];
        let minDiff = Math.abs(voltage - best);
        for (const v of available) {
          const diff = Math.abs(voltage - v);
          if (diff < minDiff) {
            minDiff = diff;
            best = v;
          }
        }
        return best;
      }

      function interpolateFLC(table, hp) {
        const hps = Object.keys(table)
          .map(Number)
          .sort((a, b) => a - b);
        if (hp <= hps[0]) return table[hps[0]];
        if (hp >= hps[hps.length - 1]) return table[hps[hps.length - 1]];
        for (let i = 0; i < hps.length - 1; i++) {
          const h1 = hps[i];
          const h2 = hps[i + 1];
          if (hp >= h1 && hp <= h2) {
            const i1 = table[h1];
            const i2 = table[h2];
            const t = (hp - h1) / (h2 - h1);
            return i1 + t * (i2 - i1);
          }
        }
        return null;
      }

      function formatAmp(value) {
        if (value === null || isNaN(value)) return "–";
        if (value < 10) return value.toFixed(2);
        if (value < 100) return value.toFixed(1);
        return value.toFixed(0);
      }

      function calculatePhysicsCurrent(motorType, hp, voltage, effPercent) {
        if (!effPercent || effPercent <= 0) return null;
        const eta = effPercent / 100;
        const watts = hp * 746;
        if (motorType === "three") {
          return watts / (Math.sqrt(3) * voltage * eta);
        } else {
          return watts / (voltage * eta);
        }
      }

      function populateReferenceTable() {
        const tbody = document.getElementById("reference-table-body");
        const hpList = [
          0.5, 0.75, 1, 1.5, 2, 3, 5, 7.5, 10, 15, 20, 25, 30, 40, 50,
        ];
        const single120 = flcData.single[120];
        const single240 = flcData.single[240];
        const three208 = flcData.three[208];
        const three230 = flcData.three[230];
        const three460 = flcData.three[460];

        hpList.forEach((hp) => {
          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="px-2 py-1 font-medium">${hp}</td>
        <td class="px-2 py-1">${
          single120 && single120[hp] ? formatAmp(single120[hp]) : "–"
        }</td>
        <td class="px-2 py-1">${
          single240 && single240[hp] ? formatAmp(single240[hp]) : "–"
        }</td>
        <td class="px-2 py-1">${
          three208 && three208[hp] ? formatAmp(three208[hp]) : "–"
        }</td>
        <td class="px-2 py-1">${
          three230 && three230[hp] ? formatAmp(three230[hp]) : "–"
        }</td>
        <td class="px-2 py-1">${
          three460 && three460[hp] ? formatAmp(three460[hp]) : "–"
        }</td>
      `;
          tbody.appendChild(tr);
        });
      }

      document.addEventListener("DOMContentLoaded", () => {
        populateReferenceTable();

        const motorTypeEl = document.getElementById("motor-type");
        const voltageEl = document.getElementById("voltage");
        const hpEl = document.getElementById("hp");
        const effEl = document.getElementById("efficiency");
        const effClearEl = document.getElementById("efficiency-clear");
        const calcBtn = document.getElementById("calc-btn");
        const resetBtn = document.getElementById("reset-btn");
        const resultsContainer = document.getElementById("results-container");

        const flcNecEl = document.getElementById("flc-nec");
        const flcPhysicsEl = document.getElementById("flc-physics");
        const summaryTypeEl = document.getElementById("summary-type");
        const summaryHpEl = document.getElementById("summary-hp");
        const summaryVoltageEl = document.getElementById("summary-voltage");

        const amp125El = document.getElementById("amp-125");
        const amp115El = document.getElementById("amp-115");
        const amp125OlEl = document.getElementById("amp-125-ol");
        const amp250El = document.getElementById("amp-250");

        function runCalculation() {
          const motorType = motorTypeEl.value; // 'single' or 'three'
          const voltage = parseFloat(voltageEl.value);
          const hp = parseFloat(hpEl.value);
          const eff = effEl.value ? parseFloat(effEl.value) : null;

          if (!hp || hp <= 0 || !voltage) {
            alert("Please enter a valid horsepower and voltage.");
            return;
          }

          const nearestV = getNearestTableVoltage(motorType, voltage);
          const table = flcData[motorType][nearestV];
          const flcNec = interpolateFLC(table, hp);

          if (!flcNec) {
            alert(
              "No FLC data available for this combination. Try a different HP or voltage."
            );
            return;
          }

          const flcPhysics = eff
            ? calculatePhysicsCurrent(motorType, hp, voltage, eff)
            : null;

          flcNecEl.textContent = formatAmp(flcNec);
          flcPhysicsEl.textContent = flcPhysics ? formatAmp(flcPhysics) : "–";

          summaryTypeEl.textContent =
            motorType === "three" ? "3-phase" : "1-phase";
          summaryHpEl.textContent = hp;
          summaryVoltageEl.textContent = nearestV;

          amp125El.textContent = formatAmp(flcNec * 1.25);
          amp115El.textContent = formatAmp(flcNec * 1.15);
          amp125OlEl.textContent = formatAmp(flcNec * 1.25);
          amp250El.textContent = formatAmp(flcNec * 2.5);

          resultsContainer.classList.remove("hidden");
        }

        function resetForm() {
          motorTypeEl.value = "three";
          voltageEl.value = "230";
          hpEl.value = 10;
          effEl.value = "";
          flcNecEl.textContent = "–";
          flcPhysicsEl.textContent = "–";
          amp125El.textContent = "–";
          amp115El.textContent = "–";
          amp125OlEl.textContent = "–";
          amp250El.textContent = "–";
          resultsContainer.classList.add("hidden");
        }

        calcBtn.addEventListener("click", runCalculation);
        resetBtn.addEventListener("click", resetForm);
        effClearEl.addEventListener("click", () => {
          effEl.value = "";
          flcPhysicsEl.textContent = "–";
        });

        hpEl.addEventListener("keydown", (e) => {
          if (e.key === "Enter") {
            e.preventDefault();
            runCalculation();
          }
        });
      });

