  <script>
    (function() {
      const G = 9.80665; // standard gravity
      const wmValue = document.getElementById('wmValue');
      const wmFrom = document.getElementById('wmFrom');

      const outKg = document.getElementById('outKg');
      const outLb = document.getElementById('outLb');
      const outN = document.getElementById('outN');
      const outG = document.getElementById('outG');
      const outOz = document.getElementById('outOz');
      const outSt = document.getElementById('outSt');
      const outT = document.getElementById('outT');
      const outN2 = document.getElementById('outN2');
      const outKN = document.getElementById('outKN');
      const outLbf = document.getElementById('outLbf');
      const tableBody = document.getElementById('tableBody');

      // convert any unit to kg (mass) using G when needed
      function toKg(value, unit) {
        if (unit === 'kg') return value;
        if (unit === 'g') return value / 1000;
        if (unit === 'mg') return value / 1e6;
        if (unit === 'lb') return value * 0.45359237;
        if (unit === 'oz') return value * 0.028349523125;
        if (unit === 'st') return value * 6.35029318;
        if (unit === 't') return value * 1000;
        if (unit === 'n') return value / G;     // weight to mass
        if (unit === 'kn') return (value * 1000) / G;
        return value;
      }

      function fmt(val) {
        if (!isFinite(val)) return '—';
        if (Math.abs(val) >= 1) return Number(val.toFixed(6)).toString();
        return Number(val.toExponential(6)).toString();
      }

      function update() {
        const val = parseFloat(wmValue.value);
        if (isNaN(val)) {
          [outKg,outLb,outN,outG,outOz,outSt,outT,outN2,outKN,outLbf].forEach(el => el.textContent = '—');
          return;
        }
        const kg = toKg(val, wmFrom.value);

        // mass-based outputs
        const g = kg * 1000;
        const oz = kg / 0.028349523125;
        const lb = kg / 0.45359237;
        const st = kg / 6.35029318;
        const t = kg / 1000;

        // weight-based outputs
        const N = kg * G;
        const kN = N / 1000;
        const lbf = N / 4.4482216152605; // 1 lbf = 4.4482216152605 N

        outKg.textContent = fmt(kg);
        outG.textContent = fmt(g);
        outOz.textContent = fmt(oz);
        outLb.textContent = fmt(lb);
        outSt.textContent = fmt(st);
        outT.textContent = fmt(t);
        outN.textContent = fmt(N);
        outN2.textContent = fmt(N);
        outKN.textContent = fmt(kN);
        outLbf.textContent = fmt(lbf);
      }

      wmValue.addEventListener('input', update);
      wmFrom.addEventListener('change', update);

      // fill table
      const sampleKg = [1, 5, 10, 50, 100];
      sampleKg.forEach(k => {
        const lb = k / 0.45359237;
        const N = k * G;
        const oz = k / 0.028349523125;
        const tr = document.createElement('tr');
        tr.innerHTML = `
          <td>${k}</td>
          <td>${lb.toFixed(3)}</td>
          <td>${N.toFixed(3)}</td>
          <td>${oz.toFixed(3)}</td>
        `;
        tableBody.appendChild(tr);
      });

      // init
      wmValue.value = 75;
      update();
    })();
  </script>

