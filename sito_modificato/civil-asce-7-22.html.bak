    <script>
      // Tab switching
      document.querySelectorAll(".asce-tab").forEach((btn) => {
        btn.addEventListener("click", () => {
          const target = btn.getAttribute("data-tab");
          document.querySelectorAll(".asce-tab").forEach((b) => {
            b.classList.remove("border-blue-600", "text-blue-600");
            b.classList.add("border-transparent", "text-gray-600");
          });
          btn.classList.add("border-blue-600", "text-blue-600");
          btn.classList.remove("border-transparent", "text-gray-600");

          document
            .querySelectorAll(".asce-panel")
            .forEach((p) => p.classList.add("hidden"));
          const panel = document.getElementById("tab-" + target);
          if (panel) panel.classList.remove("hidden");
        });
      });

      // Wind calculator
      function computeKz(z, exposure) {
        // Very simplified Kz approximation based on typical ASCE curves
        z = Math.max(15, Math.min(z, 150));
        let alpha;
        if (exposure === "B") alpha = 7.0;
        else if (exposure === "C") alpha = 9.5;
        else alpha = 11.5; // D
        const z_g = exposure === "B" ? 1200 : exposure === "C" ? 900 : 700;
        return Math.pow(z / z_g, 2 / alpha);
      }

      document.getElementById("wind-calc-btn").addEventListener("click", () => {
        const V = parseFloat(document.getElementById("wind-speed").value) || 0;
        const exposure = document.getElementById("wind-exposure").value;
        const z = parseFloat(document.getElementById("wind-height").value) || 0;
        const Kzt = parseFloat(document.getElementById("wind-kzt").value) || 1;
        const Kd = parseFloat(document.getElementById("wind-kd").value) || 0.85;
        const Cpi =
          parseFloat(document.getElementById("wind-gcpi").value) || -0.18;
        const Cp =
          parseFloat(document.getElementById("wind-gcpe").value) || 0.8;

        const Kz = computeKz(z, exposure);
        const qz = 0.00256 * Kz * Kzt * Kd * V * V;
        const p = qz * (Cp - Cpi);

        document.getElementById("wind-qz").textContent = qz.toFixed(1);
        document.getElementById("wind-p").textContent = p.toFixed(1);

        const kzDetail = [
          `Exposure: ${exposure}`,
          `Mean roof height z = ${z.toFixed(1)} ft`,
          `Approximate Kz = (z / z_g)^(2/α)`,
          `→ Kz ≈ ${Kz.toFixed(3)} (simplified)`,
        ].join("\n");
        document.getElementById("wind-kz-detail").textContent = kzDetail;

        document.getElementById("wind-results").classList.remove("hidden");
      });

      // Snow calculator
      document.getElementById("snow-calc-btn").addEventListener("click", () => {
        const pg = parseFloat(document.getElementById("snow-pg").value) || 0;
        const Is = parseFloat(document.getElementById("snow-I").value) || 1;
        const Ce = parseFloat(document.getElementById("snow-Ce").value) || 1;
        const Ct = parseFloat(document.getElementById("snow-Ct").value) || 1;
        const Cs = parseFloat(document.getElementById("snow-Cs").value) || 1;

        const pf = 0.7 * Ce * Ct * Is * pg;
        const pr = Cs * pf;

        document.getElementById("snow-pf").textContent = pf.toFixed(1);
        document.getElementById("snow-pr").textContent = pr.toFixed(1);
        document.getElementById("snow-results").classList.remove("hidden");
      });

      // Seismic calculator
      document
        .getElementById("seismic-calc-btn")
        .addEventListener("click", () => {
          const Ss = parseFloat(document.getElementById("Ss").value) || 0;
          const S1 = parseFloat(document.getElementById("S1").value) || 0;
          const Fa = parseFloat(document.getElementById("Fa").value) || 1;
          const Fv = parseFloat(document.getElementById("Fv").value) || 1;

          const SMS = Fa * Ss;
          const SM1 = Fv * S1;
          const SDS = (2 / 3) * SMS;
          const SD1 = (2 / 3) * SM1;

          document.getElementById("SMS").textContent = SMS.toFixed(3);
          document.getElementById("SM1").textContent = SM1.toFixed(3);
          document.getElementById("SDS").textContent = SDS.toFixed(3);
          document.getElementById("SD1").textContent = SD1.toFixed(3);

          document.getElementById("seismic-results").classList.remove("hidden");
        });

      // Load combination calculator
      document
        .getElementById("combo-calc-btn")
        .addEventListener("click", () => {
          const D = parseFloat(document.getElementById("combo-D").value) || 0;
          const L = parseFloat(document.getElementById("combo-L").value) || 0;
          const S = parseFloat(document.getElementById("combo-S").value) || 0;
          const W = parseFloat(document.getElementById("combo-W").value) || 0;
          const E = parseFloat(document.getElementById("combo-E").value) || 0;
          const type = document.getElementById("combo-type").value;

          let U = 0;
          let eq = "";

          switch (type) {
            case "1":
              U = 1.2 * D + 1.6 * L + 0.5 * S;
              eq = `U = 1.2D + 1.6L + 0.5S = 1.2·${D} + 1.6·${L} + 0.5·${S}`;
              break;
            case "2":
              U = 1.2 * D + 1.0 * W + 1.0 * L + 0.5 * S;
              eq = `U = 1.2D + 1.0W + 1.0L + 0.5S = 1.2·${D} + 1.0·${W} + 1.0·${L} + 0.5·${S}`;
              break;
            case "3":
              U = 1.2 * D + 1.0 * E + 1.0 * L + 0.2 * S;
              eq = `U = 1.2D + 1.0E + 1.0L + 0.2S = 1.2·${D} + 1.0·${E} + 1.0·${L} + 0.2·${S}`;
              break;
            case "4":
              U = D + L + S;
              eq = `U = D + L + S = ${D} + ${L} + ${S}`;
              break;
            case "5":
              U = D + 0.6 * W + L + 0.5 * S;
              eq = `U = D + 0.6W + L + 0.5S = ${D} + 0.6·${W} + ${L} + 0.5·${S}`;
              break;
            case "6":
              U = D + 0.7 * E + L + 0.2 * S;
              eq = `U = D + 0.7E + L + 0.2S = ${D} + 0.7·${E} + ${L} + 0.2·${S}`;
              break;
          }

          document.getElementById("combo-U").textContent = U.toFixed(2);
          document.getElementById("combo-equation").textContent = eq;
          document.getElementById("combo-results").classList.remove("hidden");
        });

      document
        .querySelectorAll("#faq-container .faq-question")
        .forEach((btn) => {
          btn.addEventListener("click", () => {
            const answer = btn.parentElement.querySelector(".faq-answer");
            const icon = btn.querySelector("span:last-child");
            const isHidden = answer.classList.contains("hidden");
            document
              .querySelectorAll("#faq-container .faq-answer")
              .forEach((a) => a.classList.add("hidden"));
            document
              .querySelectorAll("#faq-container .faq-question span:last-child")
              .forEach((i) => (i.textContent = "+"));
            if (isHidden) {
              answer.classList.remove("hidden");
              icon.textContent = "−";
            }
          });
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
