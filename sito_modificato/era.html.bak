    <script>
      // ERA calculator logic
      function parseInningsDecimal(value) {
        if (isNaN(value) || value < 0) return NaN;
        const whole = Math.trunc(value);
        const decimal = Math.round((value - whole) * 10); // scoreboard style .0, .1, .2
        if (decimal === 0) return whole;
        if (decimal === 1) return whole + 1 / 3;
        if (decimal === 2) return whole + 2 / 3;
        // If user enters a true decimal like 7.33, just use it directly
        return value;
      }

      function inningsFromOuts(inningsWhole, outs) {
        if (isNaN(inningsWhole) || inningsWhole < 0) inningsWhole = 0;
        outs = Number(outs) || 0;
        return inningsWhole + outs / 3;
      }

      function classifyEra(era) {
        if (!isFinite(era)) return "";
        if (era < 0) return "";
        if (era < 3) return "Elite ERA in most leagues.";
        if (era < 3.75) return "Very good ERA.";
        if (era < 4.25) return "Around league average ERA.";
        if (era < 5) return "Below-average ERA.";
        return "Poor ERA; allows many runs.";
      }

      function updateEra() {
        const earnedRuns = parseFloat(
          document.getElementById("earnedRuns").value
        );
        const ipMode = document.querySelector(
          'input[name="ipMode"]:checked'
        ).value;
        const leagueMode = document.querySelector(
          'input[name="leagueMode"]:checked'
        ).value;

        let inningsPitched;
        if (ipMode === "decimal") {
          const val = parseFloat(
            document.getElementById("inningsDecimal").value
          );
          inningsPitched = parseInningsDecimal(val);
        } else {
          const whole = parseFloat(
            document.getElementById("inningsWhole").value
          );
          const outs = parseInt(document.getElementById("outs").value, 10);
          inningsPitched = inningsFromOuts(whole, outs);
        }

        let inningsPerGame = 9;
        if (leagueMode === "custom") {
          const custom = parseFloat(
            document.getElementById("inningsPerGame").value
          );
          if (!isNaN(custom) && custom > 0) inningsPerGame = custom;
        }

        const eraResultEl = document.getElementById("eraResult");
        const eraContextEl = document.getElementById("eraContext");
        const ipConvertedEl = document.getElementById("ipConverted");
        const leagueLengthLabelEl =
          document.getElementById("leagueLengthLabel");
        const formulaUsedEl = document.getElementById("formulaUsed");

        if (
          isNaN(earnedRuns) ||
          earnedRuns < 0 ||
          isNaN(inningsPitched) ||
          inningsPitched <= 0
        ) {
          eraResultEl.textContent = "—";
          eraContextEl.textContent =
            "Enter valid earned runs and innings pitched.";
          ipConvertedEl.textContent = "—";
          return;
        }

        const era = (earnedRuns * inningsPerGame) / inningsPitched;
        const eraRounded = era.toFixed(2);

        eraResultEl.textContent = eraRounded;
        eraContextEl.textContent = classifyEra(era);
        ipConvertedEl.textContent = inningsPitched.toFixed(3);
        leagueLengthLabelEl.textContent = inningsPerGame + " innings";
        formulaUsedEl.textContent = `(Earned Runs × ${inningsPerGame}) ÷ Innings Pitched`;
      }

      document.addEventListener("DOMContentLoaded", function () {
        const ipModeRadios = document.querySelectorAll(".ip-mode-radio");
        const leagueModeRadios =
          document.querySelectorAll(".league-mode-radio");
        const ipDecimalWrapper = document.getElementById("ip-decimal-wrapper");
        const ipOutsWrapper = document.getElementById("ip-outs-wrapper");
        const customLeagueWrapper = document.getElementById(
          "custom-league-wrapper"
        );

        ipModeRadios.forEach((radio) => {
          radio.addEventListener("change", () => {
            if (radio.value === "decimal" && radio.checked) {
              ipDecimalWrapper.classList.remove("hidden");
              ipOutsWrapper.classList.add("hidden");
            } else if (radio.value === "outs" && radio.checked) {
              ipDecimalWrapper.classList.add("hidden");
              ipOutsWrapper.classList.remove("hidden");
            }
            updateEra();
          });
        });

        leagueModeRadios.forEach((radio) => {
          radio.addEventListener("change", () => {
            if (radio.value === "custom" && radio.checked) {
              customLeagueWrapper.classList.remove("hidden");
            } else if (radio.value === "9" && radio.checked) {
              customLeagueWrapper.classList.add("hidden");
            }
            updateEra();
          });
        });

        document
          .getElementById("calculateEraBtn")
          .addEventListener("click", function (e) {
            e.preventDefault();
            updateEra();
          });

        [
          "earnedRuns",
          "inningsDecimal",
          "inningsWhole",
          "outs",
          "inningsPerGame",
        ].forEach((id) => {
          const el = document.getElementById(id);
          if (el) {
            el.addEventListener("input", updateEra);
            el.addEventListener("change", updateEra);
          }
        });

        updateEra();
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
