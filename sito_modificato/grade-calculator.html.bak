    <script>
      // Utility: parse number safely
      function parseNum(val) {
        const n = parseFloat(val);
        return isNaN(n) ? null : n;
      }

      // Letter grade helper
      function getLetterGrade(percent, scale) {
        if (percent == null) return "";
        if (percent >= scale.a) return "A";
        if (percent >= scale.b) return "B";
        if (percent >= scale.c) return "C";
        if (percent >= scale.d) return "D";
        return "F";
      }

      // Global state
      const state = {
        gradingMode: "points", // 'points' or 'percent'
        scale: { a: 90, b: 80, c: 70, d: 60 },
      };

      // Initialize on DOM ready
      document.addEventListener("DOMContentLoaded", () => {
        initGradingMode();
        initScalePanel();
        initCategories();
        initModeTabs();
        initCurrentMode();
        initFinalMode();
        initWhatIfMode();
      });

      function initGradingMode() {
        const pointsBtn = document.getElementById("mode-points");
        const percentBtn = document.getElementById("mode-percent");

        function setMode(mode) {
          state.gradingMode = mode;
          if (mode === "points") {
            pointsBtn.classList.add("bg-blue-600", "text-white");
            pointsBtn.classList.remove("bg-white", "text-gray-700");
            percentBtn.classList.remove("bg-blue-600", "text-white");
            percentBtn.classList.add("bg-white", "text-gray-700");
            document.querySelectorAll(".scores-input").forEach((inp) => {
              inp.placeholder = "e.g. 18/20, 15/15, 9/10";
            });
          } else {
            percentBtn.classList.add("bg-blue-600", "text-white");
            percentBtn.classList.remove("bg-white", "text-gray-700");
            pointsBtn.classList.remove("bg-blue-600", "text-white");
            pointsBtn.classList.add("bg-white", "text-gray-700");
            document.querySelectorAll(".scores-input").forEach((inp) => {
              inp.placeholder = "e.g. 90, 85.5, 100";
            });
          }
        }

        pointsBtn.addEventListener("click", () => setMode("points"));
        percentBtn.addEventListener("click", () => setMode("percent"));
        setMode("points");
      }

      function initScalePanel() {
        const panel = document.getElementById("scale-panel");
        const toggle = document.getElementById("toggle-scale");
        const reset = document.getElementById("reset-scale");
        const aMin = document.getElementById("scale-a-min");
        const bMin = document.getElementById("scale-b-min");
        const cMin = document.getElementById("scale-c-min");
        const dMin = document.getElementById("scale-d-min");

        toggle.addEventListener("click", () => {
          panel.classList.toggle("hidden");
        });

        function syncScale() {
          const a = parseNum(aMin.value);
          const b = parseNum(bMin.value);
          const c = parseNum(cMin.value);
          const d = parseNum(dMin.value);
          if (a != null && b != null && c != null && d != null) {
            state.scale = { a, b, c, d };
          }
        }

        [aMin, bMin, cMin, dMin].forEach((inp) => {
          inp.addEventListener("input", syncScale);
        });

        reset.addEventListener("click", () => {
          aMin.value = 90;
          bMin.value = 80;
          cMin.value = 70;
          dMin.value = 60;
          syncScale();
        });
      }

      // Category rows for current grade
      function initCategories() {
        const tbody = document.getElementById("category-body");
        const addBtn = document.getElementById("add-category");
        const weightSumEl = document.getElementById("weight-sum");

        function updateWeightSum() {
          let sum = 0;
          tbody.querySelectorAll(".weight-input").forEach((inp) => {
            const v = parseNum(inp.value);
            if (v != null) sum += v;
          });
          weightSumEl.textContent = sum.toFixed(1).replace(/\.0$/, "");
        }

        function createRow(name = "", weight = "", scores = "") {
          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="px-2 py-2 border-b align-top">
          <input type="text" class="category-name w-full border-gray-300 rounded-md px-2 py-1.5 text-sm" placeholder="e.g. Homework" value="${name}">
        </td>
        <td class="px-2 py-2 border-b align-top">
          <input type="number" min="0" max="100" step="0.1" class="weight-input w-full border-gray-300 rounded-md px-2 py-1.5 text-sm" placeholder="e.g. 20" value="${weight}">
        </td>
        <td class="px-2 py-2 border-b align-top">
          <textarea rows="2" class="scores-input w-full border-gray-300 rounded-md px-2 py-1.5 text-xs" placeholder="${
            state.gradingMode === "points"
              ? "e.g. 18/20, 15/15, 9/10"
              : "e.g. 90, 85.5, 100"
          }">${scores}</textarea>
          <p class="text-[11px] text-gray-500 mt-1">Separate scores with commas or spaces.</p>
        </td>
        <td class="px-2 py-2 border-b align-top">
          <span class="category-avg text-sm text-gray-700">–</span>
        </td>
        <td class="px-2 py-2 border-b align-top text-right">
          <button type="button" class="remove-category text-xs text-red-600 hover:underline">Remove</button>
        </td>
      `;
          tbody.appendChild(tr);

          tr.querySelector(".weight-input").addEventListener(
            "input",
            updateWeightSum
          );
          tr.querySelector(".remove-category").addEventListener("click", () => {
            tr.remove();
            updateWeightSum();
          });
        }

        addBtn.addEventListener("click", () => {
          createRow();
        });

        // Initialize with common categories
        createRow("Homework", "20", "");
        createRow("Quizzes", "20", "");
        createRow("Midterm", "25", "");
        createRow("Final Exam", "35", "");
        updateWeightSum();
      }

      function parseScores(text, mode) {
        const cleaned = text.replace(/\n/g, " ").split(/[, ]+/).filter(Boolean);
        if (cleaned.length === 0) return null;

        if (mode === "points") {
          let earned = 0,
            possible = 0;
          for (const token of cleaned) {
            const parts = token.split("/");
            if (parts.length !== 2) continue;
            const e = parseNum(parts[0]);
            const p = parseNum(parts[1]);
            if (e == null || p == null || p <= 0) continue;
            earned += e;
            possible += p;
          }
          if (possible === 0) return null;
          return (earned / possible) * 100;
        } else {
          let sum = 0,
            count = 0;
          for (const token of cleaned) {
            const v = parseNum(token);
            if (v == null) continue;
            sum += v;
            count++;
          }
          if (count === 0) return null;
          return sum / count;
        }
      }

      function initModeTabs() {
        const tabs = document.querySelectorAll(".mode-tab");
        const sections = {
          current: document.getElementById("mode-current"),
          final: document.getElementById("mode-final"),
          whatif: document.getElementById("mode-whatif"),
        };

        tabs.forEach((tab) => {
          tab.addEventListener("click", () => {
            const mode = tab.getAttribute("data-mode");
            tabs.forEach((t) => {
              t.classList.remove("border-blue-600", "text-blue-600");
              t.classList.add("border-transparent", "text-gray-600");
            });
            tab.classList.add("border-blue-600", "text-blue-600");
            tab.classList.remove("text-gray-600");

            Object.values(sections).forEach((sec) =>
              sec.classList.add("hidden")
            );
            sections[mode].classList.remove("hidden");
          });
        });
      }

      function initCurrentMode() {
        const calcBtn = document.getElementById("calc-current");
        const clearBtn = document.getElementById("clear-current");
        const resultSection = document.getElementById("current-result");
        const percentEl = document.getElementById("current-percent");
        const letterEl = document.getElementById("current-letter");
        const breakdownEl = document.getElementById("current-breakdown");
        const tbody = document.getElementById("category-body");

        calcBtn.addEventListener("click", () => {
          let total = 0;
          let weightSum = 0;
          breakdownEl.innerHTML = "";
          let hasData = false;

          tbody.querySelectorAll("tr").forEach((tr) => {
            const name =
              tr.querySelector(".category-name").value.trim() || "Category";
            const weight = parseNum(tr.querySelector(".weight-input").value);
            const scoresText = tr.querySelector(".scores-input").value;
            const avgSpan = tr.querySelector(".category-avg");

            avgSpan.textContent = "–";
            tr.querySelector(".scores-input").classList.remove("input-error");

            if (weight == null || weight <= 0) return;
            const avg = parseScores(scoresText, state.gradingMode);
            if (avg == null) {
              return;
            }
            hasData = true;
            avgSpan.textContent = avg.toFixed(1) + "%";
            total += avg * (weight / 100);
            weightSum += weight;

            const p = document.createElement("p");
            p.textContent = `${name}: ${avg.toFixed(1)}% × ${weight
              .toFixed(1)
              .replace(/\.0$/, "")}%`;
            breakdownEl.appendChild(p);
          });

          if (!hasData || weightSum === 0) {
            alert(
              "Please enter at least one category with a weight and valid scores."
            );
            resultSection.classList.add("hidden");
            return;
          }

          const overall = total;
          percentEl.textContent = overall
            .toFixed(2)
            .replace(/\.00$/, ".0")
            .replace(/(\.\d)0$/, "$1");
          const letter = getLetterGrade(overall, state.scale);
          letterEl.textContent = letter ? `(${letter})` : "";
          resultSection.classList.remove("hidden");
        });

        clearBtn.addEventListener("click", () => {
          tbody.querySelectorAll("tr").forEach((tr) => {
            tr.querySelector(".scores-input").value = "";
            tr.querySelector(".category-avg").textContent = "–";
          });
          document.getElementById("current-result").classList.add("hidden");
        });
      }

      function initFinalMode() {
        const calcBtn = document.getElementById("calc-final");
        const clearBtn = document.getElementById("clear-final");
        const currentInp = document.getElementById("final-current-grade");
        const targetInp = document.getElementById("final-target-grade");
        const weightInp = document.getElementById("final-weight");
        const letterSel = document.getElementById("final-scale");
        const resultSection = document.getElementById("final-result");
        const neededEl = document.getElementById("final-needed");
        const feasEl = document.getElementById("final-feasibility");

        calcBtn.addEventListener("click", () => {
          let current = parseNum(currentInp.value);
          let target = parseNum(targetInp.value);
          const weightPct = parseNum(weightInp.value);
          const letterTarget = letterSel.value;

          if (letterTarget) {
            const s = state.scale;
            if (letterTarget === "A") target = s.a;
            else if (letterTarget === "B") target = s.b;
            else if (letterTarget === "C") target = s.c;
            else if (letterTarget === "D") target = s.d;
            targetInp.value = target.toFixed(1).replace(/\.0$/, "");
          }

          if (
            current == null ||
            target == null ||
            weightPct == null ||
            weightPct <= 0 ||
            weightPct >= 100
          ) {
            alert(
              "Please enter a current grade, target grade, and a final exam weight between 0 and 100."
            );
            resultSection.classList.add("hidden");
            return;
          }

          const w = weightPct / 100;
          const needed = (target - current * (1 - w)) / w;
          neededEl.textContent = needed
            .toFixed(2)
            .replace(/\.00$/, ".0")
            .replace(/(\.\d)0$/, "$1");

          let msg = "";
          if (needed > 100) {
            msg =
              "This target is mathematically impossible because it requires more than 100% on the final.";
          } else if (needed < 0) {
            msg =
              "You have already secured at least this target grade even with 0% on the final.";
          } else {
            msg =
              "This is the minimum percentage you must score on the final exam to reach your target course grade.";
          }
          feasEl.textContent = msg;
          resultSection.classList.remove("hidden");
        });

        clearBtn.addEventListener("click", () => {
          currentInp.value = "";
          targetInp.value = "";
          weightInp.value = "";
          letterSel.value = "";
          resultSection.classList.add("hidden");
        });
      }

      function initWhatIfMode() {
        const calcBtn = document.getElementById("calc-whatif");
        const clearBtn = document.getElementById("clear-whatif");
        const currentInp = document.getElementById("whatif-current");
        const weightInp = document.getElementById("whatif-weight");
        const scoreInp = document.getElementById("whatif-score");
        const missingSel = document.getElementById("whatif-missing");
        const resultSection = document.getElementById("whatif-result");
        const newGradeEl = document.getElementById("whatif-new-grade");
        const letterEl = document.getElementById("whatif-letter");
        const changeEl = document.getElementById("whatif-change");

        calcBtn.addEventListener("click", () => {
          const current = parseNum(currentInp.value);
          const weightPct = parseNum(weightInp.value);
          let score = parseNum(scoreInp.value);
          const missing = missingSel.value === "yes";

          if (
            current == null ||
            weightPct == null ||
            weightPct < 0 ||
            weightPct > 100
          ) {
            alert(
              "Please enter your current grade and a valid assignment weight."
            );
            resultSection.classList.add("hidden");
            return;
          }

          if (missing) {
            score = 0;
          } else if (score == null) {
            alert(
              "Please enter the expected score for the assignment, or choose to treat it as missing."
            );
            resultSection.classList.add("hidden");
            return;
          }

          const w = weightPct / 100;
          const newGrade = current * (1 - w) + score * w;
          newGradeEl.textContent = newGrade
            .toFixed(2)
            .replace(/\.00$/, ".0")
            .replace(/(\.\d)0$/, "$1");
          const letter = getLetterGrade(newGrade, state.scale);
          letterEl.textContent = letter ? `(${letter})` : "";

          const diff = newGrade - current;
          let msg;
          if (Math.abs(diff) < 0.05) {
            msg = "This assignment has almost no effect on your overall grade.";
          } else if (diff > 0) {
            msg = `Your grade would increase by about ${diff.toFixed(
              2
            )} percentage points.`;
          } else {
            msg = `Your grade would decrease by about ${Math.abs(diff).toFixed(
              2
            )} percentage points.`;
          }
          changeEl.textContent = msg;
          resultSection.classList.remove("hidden");
        });

        clearBtn.addEventListener("click", () => {
          currentInp.value = "";
          weightInp.value = "";
          scoreInp.value = "";
          missingSel.value = "no";
          resultSection.classList.add("hidden");
        });
      }
    </script>

