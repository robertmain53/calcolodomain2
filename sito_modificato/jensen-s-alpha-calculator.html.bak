    <script>
      // Jensen's Alpha Calculator Logic
      function formatPercent(value, decimals = 2) {
        if (!isFinite(value)) return "—";
        return value.toFixed(decimals) + "%";
      }

      function formatNumber(value, decimals = 4) {
        if (!isFinite(value)) return "—";
        return value.toFixed(decimals);
      }

      function calcJensenAlpha() {
        const rp = parseFloat(document.getElementById("rp").value);
        const rf = parseFloat(document.getElementById("rf").value);
        const rm = parseFloat(document.getElementById("rm").value);
        const beta = parseFloat(document.getElementById("beta").value);

        if ([rp, rf, rm, beta].some((v) => isNaN(v))) {
          alert("Please enter valid numeric values for all inputs.");
          return;
        }

        // Convert percentage inputs to decimals
        const Rp = rp / 100;
        const Rf = rf / 100;
        const Rm = rm / 100;

        const excessPortfolio = Rp - Rf;
        const excessBenchmark = Rm - Rf;
        const expectedReturn = Rf + beta * excessBenchmark;
        const alpha = Rp - expectedReturn;
        const rawOutperformance = Rp - Rm;

        // Update results
        const resultsSection = document.getElementById("results-section");
        resultsSection.classList.remove("hidden");

        const alphaPct = alpha * 100;
        document.getElementById("alpha-main").textContent = formatPercent(
          alphaPct,
          3
        );
        document.getElementById("alpha-decimal").textContent =
          "Decimal alpha: " + formatNumber(alpha, 6);

        document.getElementById("expected-return").textContent = formatPercent(
          expectedReturn * 100,
          3
        );
        document.getElementById("excess-portfolio").textContent = formatPercent(
          excessPortfolio * 100,
          3
        );
        document.getElementById("excess-benchmark").textContent = formatPercent(
          excessBenchmark * 100,
          3
        );
        document.getElementById("raw-outperformance").textContent =
          formatPercent(rawOutperformance * 100, 3);

        // Badge & interpretation
        const badge = document.getElementById("alpha-badge");
        const interp = document.getElementById("interpretation");
        badge.className = "badge mt-2 inline-block";

        if (alphaPct > 0.0001) {
          badge.textContent = "Positive alpha (outperformance)";
          badge.classList.add(
            "bg-green-100",
            "text-green-800",
            "border",
            "border-green-200"
          );
          interp.textContent =
            "The portfolio generated a positive Jensen’s alpha, meaning it outperformed its CAPM‑expected return given its beta and the benchmark’s performance. This suggests value added by active management or factor tilts, assuming the inputs are measured consistently over time.";
        } else if (alphaPct < -0.0001) {
          badge.textContent = "Negative alpha (underperformance)";
          badge.classList.add(
            "bg-red-100",
            "text-red-800",
            "border",
            "border-red-200"
          );
          interp.textContent =
            "The portfolio produced a negative Jensen’s alpha, indicating underperformance relative to its CAPM‑expected return. Given its level of systematic risk (beta), it delivered less return than the benchmark would imply.";
        } else {
          badge.textContent = "Alpha ≈ 0 (in line with CAPM)";
          badge.classList.add(
            "bg-gray-100",
            "text-gray-800",
            "border",
            "border-gray-200"
          );
          interp.textContent =
            "The portfolio’s return is approximately in line with its CAPM‑expected return. On a beta‑adjusted basis, it neither meaningfully outperformed nor under‑performed the benchmark over the period considered.";
        }
      }

      document
        .getElementById("calc-btn")
        .addEventListener("click", calcJensenAlpha);

      document
        .getElementById("clear-btn")
        .addEventListener("click", function () {
          ["rp", "rf", "rm", "beta"].forEach(
            (id) => (document.getElementById(id).value = "")
          );
          document.getElementById("results-section").classList.add("hidden");
        });

      document
        .getElementById("example-btn")
        .addEventListener("click", function () {
          // Example from the worked example in the article
          document.getElementById("rp").value = "10";
          document.getElementById("rf").value = "2";
          document.getElementById("rm").value = "8";
          document.getElementById("beta").value = "1.2";
          calcJensenAlpha();
        });

      // Allow Enter key to trigger calculation
      ["rp", "rf", "rm", "beta"].forEach((id) => {
        document.getElementById(id).addEventListener("keydown", function (e) {
          if (e.key === "Enter") {
            e.preventDefault();
            calcJensenAlpha();
          }
        });
      });
    </script>

