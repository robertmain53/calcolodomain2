    <script>
      // Utility: format date nicely
      function formatDate(date) {
        if (!date || isNaN(date.getTime())) return "–";
        return date.toLocaleDateString(undefined, {
          year: "numeric",
          month: "short",
          day: "numeric",
        });
      }

      function addDays(date, days) {
        const d = new Date(date.getTime());
        d.setDate(d.getDate() + days);
        return d;
      }

      function diffInDays(start, end) {
        const msPerDay = 24 * 60 * 60 * 1000;
        return Math.round((end - start) / msPerDay);
      }

      function getSpeciesDefaults(species) {
        if (species === "dog") {
          return { gestation: 63, min: 58, max: 68 };
        }
        if (species === "cat") {
          return { gestation: 64, min: 58, max: 70 };
        }
        // custom
        return { gestation: 63, min: 63, max: 63 };
      }

      function updateSpeciesUI() {
        const species = document.getElementById("species").value;
        const breedSizeWrapper = document.getElementById("breed-size-wrapper");
        const customWrapper = document.getElementById(
          "custom-gestation-wrapper"
        );

        if (species === "dog") {
          breedSizeWrapper.classList.remove("hidden");
          customWrapper.classList.add("hidden");
        } else if (species === "cat") {
          breedSizeWrapper.classList.add("hidden");
          customWrapper.classList.add("hidden");
        } else {
          breedSizeWrapper.classList.add("hidden");
          customWrapper.classList.remove("hidden");
        }
      }

      function buildTimeline(startDate, gestationLength) {
        const tbody = document.getElementById("timeline-body");
        tbody.innerHTML = "";

        if (!startDate || isNaN(startDate.getTime())) {
          return;
        }

        const weeks = Math.ceil(gestationLength / 7);
        const species = document.getElementById("species").value;

        const dogMilestones = {
          1: "Fertilization and early cell division; no visible signs.",
          2: "Embryos travel to uterus; mild behavior or appetite changes possible.",
          3: "Implantation in uterus; some dogs may show morning sickness.",
          4: "Pregnancy often detectable by ultrasound; nipples may enlarge.",
          5: "Abdomen begins to enlarge; fetuses develop organs and skeleton.",
          6: "Fetal skeletons mineralize; movement may be felt in some dogs.",
          7: "Rapid fetal growth; nesting behavior may begin.",
          8: "Milk may appear; whelping area should be prepared.",
          9: "Whelping expected; temperature drop and restlessness before labor.",
        };

        const catMilestones = {
          1: "Fertilization; no obvious external signs.",
          2: "Embryos implant; queen may be more affectionate or sleepy.",
          3: 'Nipples become pink and enlarged ("pinking up").',
          4: "Pregnancy often visible on ultrasound; abdomen starts to round.",
          5: "Fetuses grow quickly; appetite usually increases.",
          6: "You may feel kittens move; queen may seek quiet nesting spots.",
          7: "Milk may be present; nesting behavior increases.",
          8: "Queening (birth) expected soon; monitor closely for labor signs.",
          9: "Late or prolonged pregnancy; contact your vet if no kittens yet.",
        };

        for (let w = 1; w <= weeks; w++) {
          const weekStart = addDays(startDate, (w - 1) * 7);
          const weekEnd = addDays(startDate, w * 7 - 1);

          let desc = "Normal fetal development and maternal changes.";
          if (species === "dog" && dogMilestones[w]) desc = dogMilestones[w];
          if (species === "cat" && catMilestones[w]) desc = catMilestones[w];

          const tr = document.createElement("tr");
          tr.className = "timeline-row";
          tr.innerHTML = `
        <td class="px-3 py-2 align-top font-medium text-gray-800">Week ${w}</td>
        <td class="px-3 py-2 align-top text-gray-700">${formatDate(
          weekStart
        )} – ${formatDate(weekEnd)}</td>
        <td class="px-3 py-2 align-top text-gray-700">${desc}</td>
      `;
          tbody.appendChild(tr);
        }
      }

      function calculateGestation() {
        const species = document.getElementById("species").value;
        const dateType =
          document.querySelector('input[name="date-type"]:checked')?.value ||
          "mating";
        const refInput = document.getElementById("reference-date").value;
        const todayInput = document.getElementById("today-date").value;
        const errorEl = document.getElementById("error-message");

        errorEl.classList.add("hidden");
        errorEl.textContent = "";

        if (!refInput) {
          errorEl.textContent =
            "Please enter the reference date (mating, last mating, or ovulation).";
          errorEl.classList.remove("hidden");
          return;
        }

        const refDate = new Date(refInput);
        const today = todayInput ? new Date(todayInput) : new Date();

        if (isNaN(refDate.getTime())) {
          errorEl.textContent = "The reference date is invalid.";
          errorEl.classList.remove("hidden");
          return;
        }
        if (isNaN(today.getTime())) {
          errorEl.textContent = "The assessment date is invalid.";
          errorEl.classList.remove("hidden");
          return;
        }

        let { gestation, min, max } = getSpeciesDefaults(species);

        if (species === "other") {
          const custom = parseInt(
            document.getElementById("custom-gestation").value,
            10
          );
          if (isNaN(custom) || custom <= 0) {
            errorEl.textContent =
              "Please enter a valid custom gestation length in days.";
            errorEl.classList.remove("hidden");
            return;
          }
          gestation = custom;
          min = custom;
          max = custom;
        }

        // Adjust window slightly based on dog size (heuristic, not medical)
        if (species === "dog") {
          const size = document.getElementById("breed-size").value;
          if (size === "small") {
            min -= 1;
            max -= 1;
          } else if (size === "large") {
            min += 1;
            max += 1;
          }
        }

        // If date type is "mating", conception may be a few days later.
        // We keep gestation length the same but interpret the window wider.
        let mainGestation = gestation;
        let windowMin = min;
        let windowMax = max;

        if (dateType === "mating") {
          windowMin -= 2;
          windowMax += 2;
        } else if (dateType === "last-mating") {
          // last mating tends to be closer to ovulation
          windowMin -= 1;
          windowMax += 1;
        }

        const dueMain = addDays(refDate, mainGestation);
        const dueEarliest = addDays(refDate, windowMin);
        const dueLatest = addDays(refDate, windowMax);

        const dayOfGestation = diffInDays(refDate, today) + 1; // day 1 on ref date
        const progress = Math.max(
          0,
          Math.min(100, (dayOfGestation / mainGestation) * 100)
        );
        const week = Math.max(0, (dayOfGestation - 1) / 7 + 1);

        // Update UI
        document.getElementById("due-date-main").textContent =
          formatDate(dueMain);
        document.getElementById("due-date-window").textContent = `${formatDate(
          dueEarliest
        )} – ${formatDate(dueLatest)} (approx.)`;

        document.getElementById("gestation-day").textContent =
          dayOfGestation <= 0
            ? `Not yet pregnant (pregnancy would start in ${
                Math.abs(dayOfGestation) + 1
              } day(s))`
            : `${dayOfGestation} of ~${mainGestation} days`;

        document.getElementById("gestation-week").textContent =
          dayOfGestation <= 0 ? "Not yet started" : `Week ${week.toFixed(1)}`;

        document.getElementById("gestation-progress").textContent =
          dayOfGestation <= 0
            ? "0% (pregnancy not started yet)"
            : `${progress.toFixed(0)}% of typical gestation completed`;

        const bar = document.getElementById("progress-bar");
        bar.style.width = `${Math.max(0, Math.min(100, progress))}%`;

        document.getElementById("progress-label").textContent = `${Math.max(
          0,
          Math.min(100, progress)
        ).toFixed(0)}% of typical gestation completed`;

        // Build timeline
        buildTimeline(refDate, mainGestation);
      }

      function resetForm() {
        document.getElementById("species").value = "dog";
        document.getElementById("breed-size").value = "medium";
        document.querySelector(
          'input[name="date-type"][value="mating"]'
        ).checked = true;
        document.getElementById("reference-date").value = "";
        const today = new Date().toISOString().slice(0, 10);
        document.getElementById("today-date").value = today;
        document.getElementById("custom-gestation").value = 63;
        document.getElementById("error-message").classList.add("hidden");
        document.getElementById("error-message").textContent = "";

        document.getElementById("due-date-main").textContent = "–";
        document.getElementById("due-date-window").textContent = "–";
        document.getElementById("gestation-day").textContent = "–";
        document.getElementById("gestation-week").textContent = "–";
        document.getElementById("gestation-progress").textContent = "–";
        document.getElementById("progress-bar").style.width = "0%";
        document.getElementById("progress-label").textContent =
          "0% of typical gestation completed";
        document.getElementById("timeline-body").innerHTML = "";

        updateSpeciesUI();
      }

      document.addEventListener("DOMContentLoaded", function () {
        const todayStr = new Date().toISOString().slice(0, 10);
        document.getElementById("today-date").value = todayStr;

        updateSpeciesUI();

        document
          .getElementById("species")
          .addEventListener("change", updateSpeciesUI);
        document
          .getElementById("calculate-btn")
          .addEventListener("click", calculateGestation);
        document
          .getElementById("reset-btn")
          .addEventListener("click", resetForm);
      });
    </script>

