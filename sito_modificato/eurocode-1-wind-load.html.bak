    <script>
      // Simple Eurocode 1 wind load calculation
      function getAlphaForTerrain(terrain) {
        switch (terrain) {
          case "I":
            return 0.1;
          case "II":
            return 0.16;
          case "III":
            return 0.22;
          case "IV":
            return 0.28;
          default:
            return 0.16;
        }
      }

      function formatNumber(val, decimals) {
        if (!isFinite(val)) return "–";
        return val.toFixed(decimals);
      }

      document
        .getElementById("calculateBtn")
        .addEventListener("click", function () {
          const vb0 = parseFloat(document.getElementById("vb0").value);
          const cdir = parseFloat(document.getElementById("cdir").value);
          const cseason = parseFloat(document.getElementById("cseason").value);
          const terrain = document.getElementById("terrain").value;
          const co = parseFloat(document.getElementById("orography").value);
          const z = Math.max(
            1,
            parseFloat(document.getElementById("height").value)
          ); // avoid zero
          const rho = parseFloat(document.getElementById("rho").value);
          const ce = parseFloat(document.getElementById("ce").value);
          const cscd = parseFloat(document.getElementById("cscd").value);
          const cpeWind = parseFloat(document.getElementById("cpe_wind").value);
          const cpeLee = parseFloat(document.getElementById("cpe_lee").value);
          const cpi = parseFloat(document.getElementById("cpi").value);
          const A = Math.max(
            0,
            parseFloat(document.getElementById("area").value)
          );

          if (
            [
              vb0,
              cdir,
              cseason,
              co,
              z,
              rho,
              ce,
              cscd,
              cpeWind,
              cpeLee,
              cpi,
              A,
            ].some((v) => isNaN(v))
          ) {
            alert("Please fill in all input fields with valid numbers.");
            return;
          }

          // 1. Basic wind velocity
          const vb = vb0 * cdir * cseason;

          // 2. Roughness factor using power-law approximation
          const alpha = getAlphaForTerrain(terrain);
          const zref = 10.0;
          const cr = Math.pow(z / zref, alpha);

          // 3. Mean wind velocity
          const vm = cr * co * vb;

          // 4. Peak velocity pressure (N/m²)
          const qp_N = 0.5 * rho * vm * vm * ce;
          const qp_kN = qp_N / 1000.0;

          // 5. External and internal pressures (kN/m²)
          const we_wind = qp_kN * cpeWind * cscd;
          const we_lee = qp_kN * cpeLee * cscd;
          const wi = qp_kN * cpi;

          const wnet_wind = we_wind + wi;
          const wnet_lee = we_lee + wi;

          // 6. Forces (kN)
          const F_wind = wnet_wind * A;
          const F_lee = wnet_lee * A;

          // Output
          document.getElementById("vb_out").textContent = formatNumber(vb, 2);
          document.getElementById("cr_out").textContent = formatNumber(cr, 3);
          document.getElementById("vm_out").textContent = formatNumber(vm, 2);
          document.getElementById("qp_out").textContent = formatNumber(
            qp_kN,
            3
          );

          document.getElementById("we_wind_out").textContent = formatNumber(
            we_wind,
            3
          );
          document.getElementById("we_lee_out").textContent = formatNumber(
            we_lee,
            3
          );
          document.getElementById("wi_out").textContent = formatNumber(wi, 3);
          document.getElementById("wnet_wind_out").textContent = formatNumber(
            wnet_wind,
            3
          );
          document.getElementById("wnet_lee_out").textContent = formatNumber(
            wnet_lee,
            3
          );
          document.getElementById("F_wind_out").textContent = formatNumber(
            F_wind,
            2
          );
          document.getElementById("F_lee_out").textContent = formatNumber(
            F_lee,
            2
          );
        });

      // Mobile menu toggle
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          this.setAttribute("aria-expanded", isHidden ? "true" : "false");
        });
    </script>

