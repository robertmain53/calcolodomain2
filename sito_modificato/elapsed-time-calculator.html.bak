    <script>
      // Utility: pad with leading zero
      function pad2(n) {
        return n.toString().padStart(2, "0");
      }

      function createIntervalRow(index) {
        const row = document.createElement("div");
        row.className =
          "p-4 flex flex-col gap-3 md:gap-2 md:flex-row md:items-end";
        row.dataset.index = index;

        row.innerHTML = `
      <div class="flex-1">
        <p class="text-sm font-semibold text-gray-800 mb-1">Interval ${
          index + 1
        }</p>
        <div class="grid grid-cols-1 sm:grid-cols-2 gap-3">
          <div>
            <label class="input-label">Start date</label>
            <input type="date" class="date-input start-date">
          </div>
          <div>
            <label class="input-label">End date</label>
            <input type="date" class="date-input end-date">
          </div>
        </div>
      </div>
      <div class="flex-1 grid grid-cols-1 sm:grid-cols-2 gap-3">
        <div>
          <label class="input-label">Start time</label>
          <div class="flex items-center gap-1">
            <input type="number" min="0" max="23" class="time-input start-hour" aria-label="Start hour">
            <span>:</span>
            <input type="number" min="0" max="59" class="time-input start-minute" aria-label="Start minute">
            <span class="seconds-wrapper flex items-center gap-1">
              <span>:</span>
              <input type="number" min="0" max="59" class="time-input start-second" aria-label="Start second">
            </span>
            <select class="hidden ml-1 border border-gray-300 rounded-md px-1.5 py-1 text-xs bg-white start-ampm" aria-label="Start AM/PM">
              <option value="AM">AM</option>
              <option value="PM">PM</option>
            </select>
          </div>
        </div>
        <div>
          <label class="input-label">End time</label>
          <div class="flex items-center gap-1">
            <input type="number" min="0" max="23" class="time-input end-hour" aria-label="End hour">
            <span>:</span>
            <input type="number" min="0" max="59" class="time-input end-minute" aria-label="End minute">
            <span class="seconds-wrapper flex items-center gap-1">
              <span>:</span>
              <input type="number" min="0" max="59" class="time-input end-second" aria-label="End second">
            </span>
            <select class="hidden ml-1 border border-gray-300 rounded-md px-1.5 py-1 text-xs bg-white end-ampm" aria-label="End AM/PM">
              <option value="AM">AM</option>
              <option value="PM">PM</option>
            </select>
          </div>
        </div>
      </div>
      <div class="flex items-center gap-2">
        <button type="button" class="remove-interval text-xs text-red-600 hover:underline">Remove</button>
      </div>
    `;

        // Default dates: today
        const todayStr = new Date().toISOString().slice(0, 10);
        row.querySelector(".start-date").value = todayStr;
        row.querySelector(".end-date").value = todayStr;

        return row;
      }

      function getTimeFormat() {
        const radios = document.querySelectorAll('input[name="timeFormat"]');
        for (const r of radios) {
          if (r.checked) return r.value;
        }
        return "24";
      }

      function updateTimeFormatUI() {
        const format = getTimeFormat();
        const rows = document.querySelectorAll("#intervals-container > div");
        rows.forEach((row) => {
          const startHour = row.querySelector(".start-hour");
          const endHour = row.querySelector(".end-hour");
          const startAmPm = row.querySelector(".start-ampm");
          const endAmPm = row.querySelector(".end-ampm");

          if (format === "24") {
            startAmPm.classList.add("hidden");
            endAmPm.classList.add("hidden");
            startHour.min = 0;
            startHour.max = 23;
            endHour.min = 0;
            endHour.max = 23;
          } else {
            startAmPm.classList.remove("hidden");
            endAmPm.classList.remove("hidden");
            startHour.min = 1;
            startHour.max = 12;
            endHour.min = 1;
            endHour.max = 12;
            if (!startHour.value) startHour.value = 9;
            if (!endHour.value) endHour.value = 5;
          }
        });
      }

      function updateSecondsVisibility() {
        const includeSeconds =
          document.getElementById("includeSeconds").checked;
        document.querySelectorAll(".seconds-wrapper").forEach((el) => {
          el.style.display = includeSeconds ? "flex" : "none";
        });
      }

      function parseTime(row, prefix) {
        const format = getTimeFormat();
        const hourInput = row.querySelector(`.${prefix}-hour`);
        const minuteInput = row.querySelector(`.${prefix}-minute`);
        const secondInput = row.querySelector(`.${prefix}-second`);
        const ampmSelect = row.querySelector(`.${prefix}-ampm`);

        let h = parseInt(hourInput.value, 10);
        let m = parseInt(minuteInput.value, 10);
        let s =
          secondInput && secondInput.value !== ""
            ? parseInt(secondInput.value, 10)
            : 0;

        if (
          isNaN(h) ||
          isNaN(m) ||
          h < parseInt(hourInput.min) ||
          h > parseInt(hourInput.max) ||
          m < 0 ||
          m > 59 ||
          s < 0 ||
          s > 59
        ) {
          throw new Error(
            "Please enter valid times (hours, minutes, and seconds)."
          );
        }

        if (format === "12") {
          const ampm = ampmSelect.value;
          if (ampm === "AM") {
            if (h === 12) h = 0;
          } else {
            // PM
            if (h !== 12) h += 12;
          }
        }

        return { h, m, s };
      }

      function computeIntervalMs(startDateStr, endDateStr, startTime, endTime) {
        const start = new Date(startDateStr + "T00:00:00");
        const end = new Date(endDateStr + "T00:00:00");

        if (isNaN(start.getTime()) || isNaN(end.getTime())) {
          throw new Error("Please enter valid dates.");
        }

        const startMs = new Date(
          start.getFullYear(),
          start.getMonth(),
          start.getDate(),
          startTime.h,
          startTime.m,
          startTime.s
        ).getTime();
        let endMs = new Date(
          end.getFullYear(),
          end.getMonth(),
          end.getDate(),
          endTime.h,
          endTime.m,
          endTime.s
        ).getTime();

        // If end is before start, assume it is on the next day
        if (endMs < startMs) {
          endMs += 24 * 60 * 60 * 1000;
        }

        return endMs - startMs;
      }

      function formatDuration(ms) {
        if (ms < 0) return "0 seconds";
        let seconds = Math.floor(ms / 1000);
        const days = Math.floor(seconds / 86400);
        seconds -= days * 86400;
        const hours = Math.floor(seconds / 3600);
        seconds -= hours * 3600;
        const minutes = Math.floor(seconds / 60);
        seconds -= minutes * 60;

        const parts = [];
        if (days) parts.push(days + " day" + (days !== 1 ? "s" : ""));
        if (hours) parts.push(hours + " hour" + (hours !== 1 ? "s" : ""));
        if (minutes)
          parts.push(minutes + " minute" + (minutes !== 1 ? "s" : ""));
        if (seconds)
          parts.push(seconds + " second" + (seconds !== 1 ? "s" : ""));
        if (!parts.length) return "0 seconds";
        return parts.join(" ");
      }

      function formatAltDurations(ms) {
        const totalSeconds = ms / 1000;
        const totalMinutes = totalSeconds / 60;
        const totalHours = totalMinutes / 60;
        const totalDays = totalHours / 24;
        return `Total: ${totalHours.toFixed(2)} hours · ${totalMinutes.toFixed(
          2
        )} minutes · ${totalSeconds.toFixed(0)} seconds · ${totalDays.toFixed(
          3
        )} days`;
      }

      document.addEventListener("DOMContentLoaded", () => {
        const container = document.getElementById("intervals-container");
        const addBtn = document.getElementById("add-interval");
        const resetBtn = document.getElementById("reset-intervals");
        const calcBtn = document.getElementById("calculate-btn");
        const includeSecondsCheckbox =
          document.getElementById("includeSeconds");

        const resultOverall = document.getElementById("result-overall");
        const resultMain = document.getElementById("result-main");
        const resultAlt = document.getElementById("result-alt");
        const resultBreakdown = document.getElementById("result-breakdown");
        const resultIntervalList = document.getElementById(
          "result-interval-list"
        );
        const resultError = document.getElementById("result-error");

        function addInterval() {
          const index = container.children.length;
          const row = createIntervalRow(index);
          container.appendChild(row);
          updateTimeFormatUI();
          updateSecondsVisibility();
        }

        function resetIntervals() {
          container.innerHTML = "";
          addInterval();
          resultOverall.classList.add("hidden");
          resultBreakdown.classList.add("hidden");
          resultError.classList.add("hidden");
          resultIntervalList.innerHTML = "";
        }

        addBtn.addEventListener("click", () => {
          addInterval();
        });

        resetBtn.addEventListener("click", () => {
          resetIntervals();
        });

        container.addEventListener("click", (e) => {
          if (e.target.classList.contains("remove-interval")) {
            const row = e.target.closest("div[data-index]");
            if (container.children.length > 1) {
              row.remove();
              // Re-label intervals
              Array.from(container.children).forEach((child, idx) => {
                child.dataset.index = idx;
                const label = child.querySelector("p.text-sm.font-semibold");
                if (label) label.textContent = `Interval ${idx + 1}`;
              });
            }
          }
        });

        document
          .querySelectorAll('input[name="timeFormat"]')
          .forEach((radio) => {
            radio.addEventListener("change", updateTimeFormatUI);
          });

        includeSecondsCheckbox.addEventListener(
          "change",
          updateSecondsVisibility
        );

        calcBtn.addEventListener("click", () => {
          resultError.classList.add("hidden");
          resultError.textContent = "";
          resultIntervalList.innerHTML = "";
          resultOverall.classList.add("hidden");
          resultBreakdown.classList.add("hidden");

          const rows = Array.from(container.children);
          if (!rows.length) {
            resultError.textContent = "Please add at least one interval.";
            resultError.classList.remove("hidden");
            return;
          }

          try {
            let totalMs = 0;
            rows.forEach((row, idx) => {
              const startDate = row.querySelector(".start-date").value;
              const endDate = row.querySelector(".end-date").value;
              if (!startDate || !endDate) {
                throw new Error(
                  "Please enter both start and end dates for all intervals."
                );
              }
              const startTime = parseTime(row, "start");
              const endTime = parseTime(row, "end");
              const ms = computeIntervalMs(
                startDate,
                endDate,
                startTime,
                endTime
              );
              totalMs += ms;

              const item = document.createElement("div");
              item.className = "flex justify-between gap-2";
              item.innerHTML = `
            <span class="text-gray-700">Interval ${idx + 1}:</span>
            <span class="font-medium text-gray-900">${formatDuration(ms)}</span>
          `;
              resultIntervalList.appendChild(item);
            });

            resultMain.textContent = formatDuration(totalMs);
            resultAlt.textContent = formatAltDurations(totalMs);
            resultOverall.classList.remove("hidden");
            if (rows.length > 1) {
              resultBreakdown.classList.remove("hidden");
            }
          } catch (err) {
            resultError.textContent =
              err.message ||
              "An error occurred while calculating elapsed time.";
            resultError.classList.remove("hidden");
          }
        });

        // Initialize with one interval
        resetIntervals();
      });
    </script>

