          name: "Queen Anne's County",
          rate: 0.032,
          note: "",
        },
        { code: "SOMERSET", name: "Somerset County", rate: 0.032, note: "" },
        { code: "ST_MARYS", name: "St. Mary's County", rate: 0.03, note: "" },
        { code: "TALBOT", name: "Talbot County", rate: 0.0284, note: "" },
        {
          code: "WASHINGTON",
          name: "Washington County",
          rate: 0.028,
          note: "",
        },
        { code: "WICOMICO", name: "Wicomico County", rate: 0.032, note: "" },
        {
          code: "WORCESTER",
          name: "Worcester County",
          rate: 0.0225,
          note: "Lower local rate",
        },
      ];

      function formatCurrency(value) {
        if (!isFinite(value)) return "$0";
        return value.toLocaleString("en-US", {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 0,
        });
      }

      function formatPercent(value, digits = 2) {
        if (!isFinite(value)) return "0%";
        return (value * 100).toFixed(digits) + "%";
      }

      // Simplified Maryland state tax calculation
      function calcMarylandStateTax(taxableIncome, filingStatus) {
        const income = Math.max(0, taxableIncome || 0);

        // Base brackets for single / MFS
        let brackets = [
          { upTo: 1000, rate: 0.02 },
          { upTo: 2000, rate: 0.03 },
          { upTo: 3000, rate: 0.04 },
          { upTo: 100000, rate: 0.0475 },
          { upTo: 125000, rate: 0.05 },
          { upTo: 150000, rate: 0.0525 },
          { upTo: 250000, rate: 0.055 },
          { upTo: Infinity, rate: 0.0575 },
        ];

        // For MFJ and HOH, double the upper thresholds for higher brackets
        if (filingStatus === "mfj" || filingStatus === "hoh") {
          brackets = [
            { upTo: 1000, rate: 0.02 },
            { upTo: 2000, rate: 0.03 },
            { upTo: 3000, rate: 0.04 },
            { upTo: 150000, rate: 0.0475 },
            { upTo: 175000, rate: 0.05 },
            { upTo: 225000, rate: 0.0525 },
            { upTo: 300000, rate: 0.055 },
            { upTo: Infinity, rate: 0.0575 },
          ];
        }

        let remaining = income;
        let lastLimit = 0;
        let tax = 0;

        for (const bracket of brackets) {
          if (remaining <= 0) break;
          const taxableAtThisRate = Math.min(
            remaining,
            bracket.upTo - lastLimit
          );
          tax += taxableAtThisRate * bracket.rate;
          remaining -= taxableAtThisRate;
          lastLimit = bracket.upTo;
        }

        return tax;
      }

      function initMarylandCountyUI() {
        const countySelect = document.getElementById("md-county");
        const countyRateDisplay = document.getElementById(
          "md-county-rate-display"
        );
        const tableBody = document.getElementById("md-county-table-body");

        // Populate select
        MD_COUNTY_RATES.forEach((c, index) => {
          const opt = document.createElement("option");
          opt.value = c.code;
          opt.textContent = c.name;
          if (c.rate >= 0.032) {
            opt.textContent += " (3.20%)";
          }
          if (index === 0) opt.selected = true;
          countySelect.appendChild(opt);
        });

        function updateCountyRateDisplay() {
          const selectedCode = countySelect.value;
          const county = MD_COUNTY_RATES.find((c) => c.code === selectedCode);
          if (!county) {
            countyRateDisplay.textContent = "";
            return;
          }
          countyRateDisplay.textContent = `Illustrative local income tax rate: ${formatPercent(
            county.rate,
            2
          )} of Maryland taxable income.`;
        }

        countySelect.addEventListener("change", updateCountyRateDisplay);
        updateCountyRateDisplay();

        // Populate table
        MD_COUNTY_RATES.forEach((c) => {
          const tr = document.createElement("tr");
          const nameTd = document.createElement("td");
          nameTd.className = "px-3 py-2 whitespace-nowrap";
          nameTd.textContent = c.name;

          const rateTd = document.createElement("td");
          rateTd.className = "px-3 py-2 text-right";
          rateTd.textContent = formatPercent(c.rate, 2);

          const noteTd = document.createElement("td");
          noteTd.className = "px-3 py-2";
          if (c.note) {
            const span = document.createElement("span");
            span.className =
              "badge bg-blue-100 text-blue-800 border border-blue-200";
            span.textContent = c.note;
            noteTd.appendChild(span);
          } else {
            noteTd.textContent = "";
          }

          tr.appendChild(nameTd);
          tr.appendChild(rateTd);
          tr.appendChild(noteTd);
          tableBody.appendChild(tr);
        });
      }

      function setupMarylandCalculator() {
        const btn = document.getElementById("md-calc-btn");
        const filingStatusEl = document.getElementById("md-filing-status");
        const countySelect = document.getElementById("md-county");
        const incomeEl = document.getElementById("md-taxable-income");
        const withheldEl = document.getElementById("md-withheld");
        const creditsEl = document.getElementById("md-credits");

        const stateTaxEl = document.getElementById("md-state-tax");
        const countyTaxEl = document.getElementById("md-county-tax");
        const totalTaxEl = document.getElementById("md-total-tax");
        const combinedRateEl = document.getElementById("md-combined-rate");
        const effectiveRateEl = document.getElementById("md-effective-rate");
        const resultsContainer = document.getElementById(
          "md-results-container"
        );
        const resultsNote = document.getElementById("md-results-note");
        const warningEl = document.getElementById("md-warning");
        const refundRow = document.getElementById("md-refund-row");
        const refundAmountEl = document.getElementById("md-refund-amount");
        const refundLabelEl = document.getElementById("md-refund-label");

        btn.addEventListener("click", () => {
          const filingStatus = filingStatusEl.value || "single";
          const countyCode = countySelect.value;
          const county = MD_COUNTY_RATES.find((c) => c.code === countyCode);

          const income = parseFloat(incomeEl.value);
          if (!isFinite(income) || income <= 0) {
            alert("Please enter a positive Maryland taxable income amount.");
            return;
          }

          const withheld = parseFloat(withheldEl.value) || 0;
          const credits = parseFloat(creditsEl.value) || 0;

          const stateTax = calcMarylandStateTax(income, filingStatus);
          const countyTax = county ? income * county.rate : 0;
          let totalTax = stateTax + countyTax - credits;
          if (totalTax < 0) totalTax = 0;

          const combinedRate = totalTax / income;
          const effectiveRate = totalTax / income;

          stateTaxEl.textContent = formatCurrency(stateTax);
          countyTaxEl.textContent = formatCurrency(countyTax);
          totalTaxEl.textContent = formatCurrency(totalTax);
          combinedRateEl.textContent = formatPercent(combinedRate, 2);
          effectiveRateEl.textContent = formatPercent(effectiveRate, 2);

          // Refund / amount due
          if (withheld > 0) {
            const diff = withheld - totalTax;
            refundRow.classList.remove("hidden");
            if (Math.abs(diff) < 1) {
              refundAmountEl.textContent = "$0";
              refundAmountEl.className = "font-semibold text-gray-800";
              refundLabelEl.textContent =
                "Your withholding is approximately equal to your estimated Maryland tax.";
            } else if (diff > 0) {
              refundAmountEl.textContent = formatCurrency(diff);
              refundAmountEl.className = "font-semibold text-green-700";
              refundLabelEl.textContent =
                "Estimated refund (Maryland state + local), before interest or penalties.";
            } else {
              refundAmountEl.textContent = formatCurrency(-diff);
              refundAmountEl.className = "font-semibold text-red-700";
              refundLabelEl.textContent =
                "Estimated amount you may still owe to Maryland (state + local), before interest or penalties.";
            }
          } else {
            refundRow.classList.add("hidden");
          }

          // Warning for high income / complexity
          if (income > 500000) {
            warningEl.textContent =
              "High-income taxpayers may be subject to additional rules, phase-outs, or surtaxes not modeled here. Use this only as a rough estimate and consult a tax professional or the Maryland Comptroller for precise calculations.";
            warningEl.classList.remove("hidden");
          } else {
            warningEl.classList.add("hidden");
          }

          resultsContainer.classList.remove("hidden");
          resultsNote.textContent =
            "These figures are estimates based on illustrative 2025-style Maryland brackets and county rates. For filing, always use official Maryland tax forms and instructions.";
        });
      }

      document.addEventListener("DOMContentLoaded", () => {
        initMarylandCountyUI();
        setupMarylandCalculator();
      });

      // MathJax config
      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
