    <script>
      // EIN validation logic
      (function () {
        const einInput = document.getElementById("ein-input");
        const validateBtn = document.getElementById("ein-validate-btn");
        const resultBox = document.getElementById("ein-result");
        const statusBadge = document.getElementById("ein-status-badge");
        const statusText = document.getElementById("ein-status-text");
        const normalizedText = document.getElementById("ein-normalized");
        const detailList = document.getElementById("ein-detail-list");
        const copyBtn = document.getElementById("ein-copy-btn");

        const bulkInput = document.getElementById("bulk-ein-input");
        const bulkBtn = document.getElementById("bulk-ein-validate-btn");
        const bulkSummary = document.getElementById("bulk-ein-summary");
        const bulkWrapper = document.getElementById("bulk-ein-result-wrapper");
        const bulkTbody = document.getElementById("bulk-ein-tbody");

        // Approximate valid EIN prefix ranges (01-99 excluding 00)
        function isValidPrefix(prefix) {
          const p = parseInt(prefix, 10);
          if (isNaN(p) || p < 1 || p > 99) return false;
          // 00 is invalid; others are generally acceptable
          return true;
        }

        function normalizeEin(raw) {
          if (!raw) return { cleaned: "", error: "Empty input" };
          let s = raw.trim();
          // Allow digits, hyphen, spaces
          if (!/^[0-9\-\s]+$/.test(s)) {
            return {
              cleaned: "",
              error: "EIN must contain only digits, spaces, or a hyphen",
            };
          }
          // Remove spaces
          s = s.replace(/\s+/g, "");
          // If hyphen present, ensure it is after 2 digits
          if (s.includes("-")) {
            if (!/^\d{2}-\d{1,7}$/.test(s)) {
              return {
                cleaned: "",
                error:
                  "Hyphen must appear after the first two digits (format XX-XXXXXXX)",
              };
            }
            s = s.replace("-", "");
          }
          if (s.length !== 9) {
            return { cleaned: "", error: "EIN must have exactly 9 digits" };
          }
          if (!/^\d{9}$/.test(s)) {
            return { cleaned: "", error: "EIN must contain only digits" };
          }
          const prefix = s.slice(0, 2);
          if (!isValidPrefix(prefix)) {
            return {
              cleaned: "",
              error: "Invalid EIN prefix (first two digits)",
            };
          }
          const formatted = prefix + "-" + s.slice(2);
          return { cleaned: s, formatted };
        }

        function describePrefix(prefix) {
          const p = parseInt(prefix, 10);
          if (p >= 1 && p <= 9) return "Northeast / legacy IRS campus";
          if (p >= 10 && p <= 19) return "Cincinnati / Midwest (legacy)";
          if (p >= 20 && p <= 27) return "Philadelphia / Mid-Atlantic (legacy)";
          if ((p >= 30 && p <= 34) || (p >= 36 && p <= 39))
            return "Southeast (legacy)";
          if (p >= 40 && p <= 49) return "Central / Midwest (legacy)";
          if (p >= 50 && p <= 59) return "West / Southwest (legacy)";
          if (p >= 60 && p <= 69) return "West Coast (legacy)";
          if ((p >= 70 && p <= 73) || (p >= 75 && p <= 77))
            return "Small Business / centralized processing (legacy)";
          if (p >= 80 && p <= 88)
            return "International / special entities (legacy)";
          if (p >= 90 && p <= 99)
            return "Special assignment / centralized processing";
          return "Unknown prefix range";
        }

        function setStatus(type, mainText) {
          statusBadge.className =
            "inline-flex items-center px-2 py-0.5 rounded-full text-xs font-semibold";
          if (type === "valid") {
            statusBadge.classList.add("badge-valid");
            statusBadge.textContent = "Valid";
            statusText.textContent = mainText || "Structurally valid EIN";
          } else if (type === "warning") {
            statusBadge.classList.add("badge-warning");
            statusBadge.textContent = "Check";
            statusText.textContent = mainText || "Check details";
          } else {
            statusBadge.classList.add("badge-invalid");
            statusBadge.textContent = "Invalid";
            statusText.textContent = mainText || "Invalid EIN";
          }
        }

        function validateSingle() {
          const raw = einInput.value;
          const { cleaned, formatted, error } = normalizeEin(raw);
          detailList.innerHTML = "";
          copyBtn.classList.add("hidden");

          if (error) {
            setStatus("invalid", "Invalid EIN");
            normalizedText.textContent = "";
            const li = document.createElement("li");
            li.textContent = error;
            detailList.appendChild(li);
            resultBox.classList.remove("hidden");
            resultBox.classList.remove("border-green-200", "bg-green-50");
            resultBox.classList.add("border-red-200", "bg-red-50");
            return;
          }

          const prefix = cleaned.slice(0, 2);
          const prefixDesc = describePrefix(prefix);

          setStatus("valid", "Structurally valid EIN");
          normalizedText.innerHTML =
            '<span class="font-semibold">Normalized format:</span> ' +
            formatted;
          const li1 = document.createElement("li");
          li1.textContent = "Length: 9 digits (OK)";
          const li2 = document.createElement("li");
          li2.textContent = "Prefix: " + prefix + " – " + prefixDesc;
          const li3 = document.createElement("li");
          li3.textContent =
            "Note: This tool does not confirm IRS assignment or ownership.";

          detailList.appendChild(li1);
          detailList.appendChild(li2);
          detailList.appendChild(li3);

          resultBox.classList.remove("hidden");
          resultBox.classList.remove("border-red-200", "bg-red-50");
          resultBox.classList.add("border-green-200", "bg-green-50");

          copyBtn.classList.remove("hidden");
          copyBtn.onclick = function () {
            navigator.clipboard.writeText(formatted).catch(() => {});
          };
        }

        validateBtn.addEventListener("click", validateSingle);
        einInput.addEventListener("keydown", function (e) {
          if (e.key === "Enter") {
            e.preventDefault();
            validateSingle();
          }
        });

        // Bulk validation
        bulkBtn.addEventListener("click", function () {
          const lines = bulkInput.value.split(/\r?\n/);
          bulkTbody.innerHTML = "";
          let total = 0;
          let validCount = 0;
          let invalidCount = 0;

          lines.forEach((line, idx) => {
            const trimmed = line.trim();
            if (!trimmed) return;
            total++;
            const { cleaned, formatted, error } = normalizeEin(trimmed);
            const tr = document.createElement("tr");
            const isValid = !error;

            if (isValid) {
              tr.className = "bg-green-50";
              validCount++;
            } else {
              tr.className = "bg-red-50";
              invalidCount++;
            }

            const tdIndex = document.createElement("td");
            tdIndex.className = "px-3 py-1 whitespace-nowrap";
            tdIndex.textContent = total;

            const tdInput = document.createElement("td");
            tdInput.className = "px-3 py-1 break-all";
            tdInput.textContent = trimmed;

            const tdNorm = document.createElement("td");
            tdNorm.className = "px-3 py-1 break-all";
            tdNorm.textContent = formatted || "";

            const tdStatus = document.createElement("td");
            tdStatus.className = "px-3 py-1";
            tdStatus.textContent = isValid ? "Valid" : "Invalid";

            const tdDetail = document.createElement("td");
            tdDetail.className = "px-3 py-1 text-xs";
            if (isValid) {
              const prefix = cleaned.slice(0, 2);
              tdDetail.textContent =
                "Prefix " + prefix + " – " + describePrefix(prefix);
            } else {
              tdDetail.textContent = error;
            }

            tr.appendChild(tdIndex);
            tr.appendChild(tdInput);
            tr.appendChild(tdNorm);
            tr.appendChild(tdStatus);
            tr.appendChild(tdDetail);
            bulkTbody.appendChild(tr);
          });

          if (total === 0) {
            bulkSummary.textContent = "No EINs found to validate.";
            bulkWrapper.classList.add("hidden");
          } else {
            bulkSummary.textContent =
              total +
              " EIN(s) checked – " +
              validCount +
              " valid, " +
              invalidCount +
              " invalid.";
            bulkWrapper.classList.remove("hidden");
          }
        });
      })();

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
