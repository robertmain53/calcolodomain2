    <script>
      // ---------- Utility helpers ----------
      function soNormalizeToken(token, caseSensitive) {
        var t = token.trim();
        if (!t) return "";
        // strip surrounding braces and quotes
        if (t.startsWith("{") && t.endsWith("}")) {
          t = t.slice(1, -1).trim();
        }
        if (
          (t.startsWith('"') && t.endsWith('"')) ||
          (t.startsWith("'") && t.endsWith("'"))
        ) {
          t = t.slice(1, -1);
        }
        if (!caseSensitive) {
          t = t.toLowerCase();
        }
        return t.trim();
      }

      function soParseSet(raw, caseSensitive) {
        if (!raw) {
          return { list: [], set: new Set() };
        }
        // Split on commas, semicolons, whitespace and newlines
        var parts = raw.split(/[\s,;]+/);
        var seen = new Set();
        var ordered = [];
        for (var i = 0; i < parts.length; i++) {
          var tok = soNormalizeToken(parts[i], caseSensitive);
          if (!tok) continue;
          if (!seen.has(tok)) {
            seen.add(tok);
            ordered.push(tok);
          }
        }
        return { list: ordered, set: seen };
      }

      function soSetToString(list) {
        return "{" + list.join(", ") + "}";
      }

      function soUnion(listA, listB) {
        var seen = new Set();
        var out = [];
        listA.concat(listB).forEach(function (x) {
          if (!seen.has(x)) {
            seen.add(x);
            out.push(x);
          }
        });
        return out;
      }

      function soIntersection(listA, setB) {
        var out = [];
        var seen = new Set();
        for (var i = 0; i < listA.length; i++) {
          var x = listA[i];
          if (setB.has(x) && !seen.has(x)) {
            seen.add(x);
            out.push(x);
          }
        }
        return out;
      }

      function soDifference(listA, setB) {
        var out = [];
        for (var i = 0; i < listA.length; i++) {
          var x = listA[i];
          if (!setB.has(x)) {
            out.push(x);
          }
        }
        return out;
      }

      function soIsSubset(setA, setB) {
        // is A subset of B?
        for (var x of setA) {
          if (!setB.has(x)) return false;
        }
        return true;
      }

      function soFormatRatio(num, den, decimals) {
        if (!den || den <= 0) return "undefined";
        var val = num / den;
        if (!isFinite(val)) return "undefined";
        var d = Math.max(0, Math.min(10, decimals || 3));
        var s = val.toFixed(d).replace(/0+$/, "").replace(/\.$/, "");
        if (s === "-0") s = "0";
        return s;
      }

      function soCompute() {
        var resultOut = document.getElementById("so-result");
        var stepsOut = document.getElementById("so-steps");
        var showSteps = document.getElementById("so-show-steps").checked;
        var dec =
          parseInt(document.getElementById("so-decimals").value, 10) || 3;
        var caseSensitive =
          document.getElementById("so-case-sensitive").checked;

        try {
          var rawU = document.getElementById("so-universe").value;
          var rawA = document.getElementById("so-set-a").value;
          var rawB = document.getElementById("so-set-b").value;

          if (!rawA.trim() && !rawB.trim()) {
            throw new Error(
              "Please enter at least one non-empty set (A or B)."
            );
          }

          var U = soParseSet(rawU, caseSensitive);
          var A = soParseSet(rawA, caseSensitive);
          var B = soParseSet(rawB, caseSensitive);

          var hasU = U.list.length > 0;

          // Basic operations
          var unionAB = soUnion(A.list, B.list);
          var interAB = soIntersection(A.list, B.set);
          var AminusB = soDifference(A.list, B.set);
          var BminusA = soDifference(B.list, A.set);
          var symmDiff = soUnion(AminusB, BminusA);

          // Relations
          var AsubsetB = soIsSubset(A.set, B.set);
          var BsubsetA = soIsSubset(B.set, A.set);
          var equalAB = AsubsetB && BsubsetA;
          var disjointAB = interAB.length === 0;

          var complementsValid = false;
          var Ac = [],
            Bc = [];
          var probNote = "";
          if (hasU) {
            var AinU = soIsSubset(A.set, U.set);
            var BinU = soIsSubset(B.set, U.set);
            complementsValid = AinU && BinU;
            if (complementsValid) {
              Ac = soDifference(U.list, A.set);
              Bc = soDifference(U.list, B.set);
            } else {
              probNote =
                "Cannot compute complements or probability-style ratios because not all elements of A and B belong to U.";
            }
          }

          var cardU = U.list.length;
          var cardA = A.list.length;
          var cardB = B.list.length;
          var cardInter = interAB.length;

          var html = "";

          html += '<p class="mb-1 text-sm font-medium">Canonical sets:</p>';
          html += '<ul class="list-disc ml-5 text-sm mb-2">';
          if (hasU) {
            html +=
              "<li>U = " +
              soSetToString(U.list) +
              " (|U| = " +
              cardU +
              ")</li>";
          } else {
            html +=
              "<li>U not specified (complements and probabilities disabled).</li>";
          }
          html +=
            "<li>A = " + soSetToString(A.list) + " (|A| = " + cardA + ")</li>";
          html +=
            "<li>B = " + soSetToString(B.list) + " (|B| = " + cardB + ")</li>";
          html += "</ul>";

          html +=
            '<p class="mb-1 text-sm font-medium">Core set operations:</p>';
          html += '<ul class="list-disc ml-5 text-sm mb-2">';
          html += "<li>A ∪ B = " + soSetToString(unionAB) + "</li>";
          html +=
            "<li>A ∩ B = " +
            soSetToString(interAB) +
            " (|A ∩ B| = " +
            cardInter +
            ")</li>";
          html += "<li>A \\ B = " + soSetToString(AminusB) + "</li>";
          html += "<li>B \\ A = " + soSetToString(BminusA) + "</li>";
          html += "<li>A Δ B = " + soSetToString(symmDiff) + "</li>";
          html += "</ul>";

          html +=
            '<p class="mb-1 text-sm font-medium">Relations between A and B:</p>';
          html += '<ul class="list-disc ml-5 text-sm mb-2">';
          html += "<li>A ⊆ B: " + (AsubsetB ? "yes" : "no") + "</li>";
          html += "<li>B ⊆ A: " + (BsubsetA ? "yes" : "no") + "</li>";
          html += "<li>A = B: " + (equalAB ? "yes" : "no") + "</li>";
          html +=
            "<li>A and B disjoint (A ∩ B = ∅): " +
            (disjointAB ? "yes" : "no") +
            "</li>";
          html += "</ul>";

          if (hasU && complementsValid) {
            html +=
              '<p class="mb-1 text-sm font-medium">Complements and probability-style ratios (relative to U):</p>';
            html += '<ul class="list-disc ml-5 text-sm mb-2">';
            html += "<li>Aᶜ = U \\ A = " + soSetToString(Ac) + "</li>";
            html += "<li>Bᶜ = U \\ B = " + soSetToString(Bc) + "</li>";
            html +=
              "<li>|A| / |U| ≈ " + soFormatRatio(cardA, cardU, dec) + "</li>";
            html +=
              "<li>|B| / |U| ≈ " + soFormatRatio(cardB, cardU, dec) + "</li>";
            html +=
              "<li>|A ∩ B| / |U| ≈ " +
              soFormatRatio(cardInter, cardU, dec) +
              "</li>";
            html += "</ul>";
          } else if (hasU && !complementsValid) {
            html +=
              '<p class="text-xs text-yellow-700 bg-yellow-50 border border-yellow-200 rounded px-2 py-1 mb-2">';
            html +=
              probNote + " Check that U contains every element of A and B.";
            html += "</p>";
          }

          resultOut.innerHTML = html;

          if (showSteps) {
            var steps = "";
            steps +=
              '<p class="text-sm mb-1 font-medium">How the calculator builds each operation:</p>';
            steps += '<ol class="list-decimal ml-5 text-sm mb-2">';
            steps +=
              "<li>Normalises each list into a set by trimming tokens, optionally lowering case, removing braces and quotes, and discarding duplicates.</li>";
            steps +=
              "<li>Constructs A ∪ B by taking every element appearing in either A or B and keeping one copy of each.</li>";
            steps +=
              "<li>Constructs A ∩ B by taking the elements that appear in both A and B.</li>";
            steps +=
              "<li>Constructs A \\ B and B \\ A by filtering out elements that belong to the other set.</li>";
            steps +=
              "<li>Forms the symmetric difference A Δ B as (A \\ B) ∪ (B \\ A).</li>";
            steps +=
              "<li>Checks subset and equality relations by verifying membership of each element of one set in the other.</li>";
            steps +=
              "<li>If a universal set U is provided and contains all of A and B, it computes complements U \\ A and U \\ B and derives ratios like |A| / |U|.</li>";
            steps += "</ol>";
            steps +=
              '<p class="text-xs text-gray-600 mb-1">Symbolic summary:</p>';
            steps += '<div class="formula-box text-xs">\\[';
            steps +=
              "A \\cup B = \\{x : x \\in A \\text{ or } x \\in B\\},\\\\";
            steps +=
              "A \\cap B = \\{x : x \\in A \\text{ and } x \\in B\\},\\\\";
            steps +=
              "A \\setminus B = \\{x : x \\in A \\text{ and } x \\notin B\\},\\\\";
            steps += "A^c = U \\setminus A, \\quad B^c = U \\setminus B.";
            steps += "\\]</div>";
            stepsOut.innerHTML = steps;
          } else {
            stepsOut.innerHTML =
              '<span class="text-gray-500 text-sm">Steps are hidden. Enable "Show detailed steps" to see how each operation is constructed.</span>';
          }

          if (window.MathJax && window.MathJax.typeset) {
            window.MathJax.typeset();
          }
        } catch (err) {
          resultOut.innerHTML =
            '<p class="text-red-600 text-sm">' + err.message + "</p>";
          stepsOut.innerHTML =
            '<span class="text-gray-500 text-sm">No steps available due to invalid input.</span>';
        }
      }

      function soReset() {
        document.getElementById("so-universe").value = "1, 2, 3, 4, 5, 6";
        document.getElementById("so-set-a").value = "1, 2, 3";
        document.getElementById("so-set-b").value = "3, 4, 5";
        document.getElementById("so-case-sensitive").checked = true;
        document.getElementById("so-decimals").value = "3";
        document.getElementById("so-show-steps").checked = true;

        document.getElementById("so-result").innerHTML =
          '<span class="text-gray-500">Computed sets and relations will appear here.</span>';
        document.getElementById("so-steps").innerHTML =
          '<span class="text-gray-500">Step-by-step explanation and formulas will appear here.</span>';
      }

      document.addEventListener("DOMContentLoaded", function () {
        soReset();
        document
          .getElementById("so-calc-btn")
          ?.addEventListener("click", soCompute);
        document
          .getElementById("so-reset-btn")
          ?.addEventListener("click", soReset);
      });
    </script>

