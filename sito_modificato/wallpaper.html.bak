    <script>
      // State
      let unitSystem = "ft"; // 'ft' or 'm'
      let mode = "simple"; // 'simple' or 'advanced'
      let wallIdCounter = 1;

      const $ = (id) => document.getElementById(id);

      function updateUnitLabels() {
        if (unitSystem === "ft") {
          $("label-perimeter-unit").textContent = "ft";
          $("label-height-unit").textContent = "ft";
          $("label-roll-width-unit").textContent = "in";
          $("label-roll-length-unit").textContent = "ft";
          $("label-pattern-repeat-unit").textContent = "in";
          $("label-area-unit-doors").textContent = "ft²";
          $("label-area-unit-windows").textContent = "ft²";
        } else {
          $("label-perimeter-unit").textContent = "m";
          $("label-height-unit").textContent = "m";
          $("label-roll-width-unit").textContent = "cm";
          $("label-roll-length-unit").textContent = "m";
          $("label-pattern-repeat-unit").textContent = "cm";
          $("label-area-unit-doors").textContent = "m²";
          $("label-area-unit-windows").textContent = "m²";
        }
      }

      function setDefaultRollValues() {
        if (unitSystem === "ft") {
          if (!$("roll-width").value) $("roll-width").value = 20.5;
          if (!$("roll-length").value) $("roll-length").value = 33;
          if (!$("pattern-repeat").value) $("pattern-repeat").value = 0;
        } else {
          if (!$("roll-width").value) $("roll-width").value = 53;
          if (!$("roll-length").value) $("roll-length").value = 10;
          if (!$("pattern-repeat").value) $("pattern-repeat").value = 0;
        }
      }

      function switchUnit(system) {
        if (unitSystem === system) return;
        unitSystem = system;

        $("unit-ft").className =
          system === "ft"
            ? "px-3 py-1 rounded-full border border-blue-600 text-blue-600 bg-blue-50"
            : "px-3 py-1 rounded-full border border-gray-300 text-gray-600 bg-white";
        $("unit-m").className =
          system === "m"
            ? "px-3 py-1 rounded-full border border-blue-600 text-blue-600 bg-blue-50"
            : "px-3 py-1 rounded-full border border-gray-300 text-gray-600 bg-white";

        updateUnitLabels();
        setDefaultRollValues();
      }

      function switchMode(newMode) {
        mode = newMode;
        $("mode-simple").className =
          newMode === "simple"
            ? "px-3 py-1 rounded-full border border-blue-600 text-blue-600 bg-blue-50"
            : "px-3 py-1 rounded-full border border-gray-300 text-gray-600 bg-white";
        $("mode-advanced").className =
          newMode === "advanced"
            ? "px-3 py-1 rounded-full border border-blue-600 text-blue-600 bg-blue-50"
            : "px-3 py-1 rounded-full border border-gray-300 text-gray-600 bg-white";

        $("simple-mode").classList.toggle("hidden", newMode !== "simple");
        $("advanced-mode").classList.toggle("hidden", newMode !== "advanced");
      }

      function addWallRow(initialWidth = "", initialHeight = "") {
        const container = $("walls-container");
        const id = wallIdCounter++;
        const div = document.createElement("div");
        div.className =
          "grid grid-cols-1 md:grid-cols-3 gap-3 items-end border rounded-md p-3";
        div.dataset.wallId = id;
        div.innerHTML = `
      <div>
        <label class="block text-sm font-medium text-gray-700 mb-1">Wall width</label>
        <div class="flex space-x-2">
          <input type="number" min="0" step="0.01" class="wall-width w-full border border-gray-300 rounded-md px-3 py-2" value="${initialWidth}">
          <span class="self-center text-gray-500 text-sm">${
            unitSystem === "ft" ? "ft" : "m"
          }</span>
        </div>
      </div>
      <div>
        <label class="block text-sm font-medium text-gray-700 mb-1">Wall height</label>
        <div class="flex space-x-2">
          <input type="number" min="0" step="0.01" class="wall-height w-full border border-gray-300 rounded-md px-3 py-2" value="${initialHeight}">
          <span class="self-center text-gray-500 text-sm">${
            unitSystem === "ft" ? "ft" : "m"
          }</span>
        </div>
      </div>
      <div class="flex items-end justify-between">
        <span class="text-xs text-gray-500">Wall #${id}</span>
        <button type="button" class="remove-wall text-xs text-red-600 hover:underline">Remove</button>
      </div>
    `;
        container.appendChild(div);

        div.querySelector(".remove-wall").addEventListener("click", () => {
          container.removeChild(div);
        });
      }

      function getSimpleWallArea() {
        const per = parseFloat($("simple-perimeter").value);
        const h = parseFloat($("simple-height").value);
        if (!per || !h || per <= 0 || h <= 0) return null;
        return per * h;
      }

      function getSimpleOpeningsArea() {
        if ($("simple-openings-toggle").value !== "yes") return 0;
        const doors = parseInt($("simple-doors-count").value || "0", 10);
        const windows = parseInt($("simple-windows-count").value || "0", 10);

        let doorArea, windowArea;
        if (unitSystem === "ft") {
          doorArea = (80 / 12) * (30 / 12); // 80x30 in
          windowArea = (48 / 12) * (36 / 12);
        } else {
          doorArea = 2.03 * 0.76;
          windowArea = 1.2 * 0.9;
        }
        return doors * doorArea + windows * windowArea;
      }

      function getAdvancedWallArea() {
        const rows = $("walls-container").querySelectorAll("[data-wall-id]");
        if (!rows.length) return null;
        let total = 0;
        for (const row of rows) {
          const w = parseFloat(row.querySelector(".wall-width").value);
          const h = parseFloat(row.querySelector(".wall-height").value);
          if (!w || !h || w <= 0 || h <= 0) continue;
          total += w * h;
        }
        return total > 0 ? total : null;
      }

      function getAdvancedOpeningsArea() {
        const d = parseFloat($("adv-door-area").value);
        const w = parseFloat($("adv-window-area").value);
        let total = 0;
        if (d && d > 0) total += d;
        if (w && w > 0) total += w;
        return total;
      }

      function computeCoveragePerRoll(wallHeight) {
        let rollWidth = parseFloat($("roll-width").value);
        let rollLength = parseFloat($("roll-length").value);
        let patternRepeat = parseFloat($("pattern-repeat").value || "0");
        const rollType = $("roll-type").value;

        if (!rollWidth || !rollLength || rollWidth <= 0 || rollLength <= 0)
          return null;

        if (rollType === "double") {
          rollLength *= 2;
        }

        let rollWidth_m, rollLength_m, patternRepeat_m, wallHeight_m;

        if (unitSystem === "ft") {
          rollWidth_m = rollWidth * 0.0254;
          rollLength_m = rollLength * 0.3048;
          patternRepeat_m = patternRepeat * 0.0254;
          wallHeight_m = wallHeight * 0.3048;
        } else {
          rollWidth_m = rollWidth / 100;
          rollLength_m = rollLength;
          patternRepeat_m = patternRepeat / 100;
          wallHeight_m = wallHeight;
        }

        let effectiveDropHeight = wallHeight_m;
        if (patternRepeat_m && patternRepeat_m > 0) {
          const dropsNeeded = Math.ceil(wallHeight_m / patternRepeat_m);
          effectiveDropHeight = dropsNeeded * patternRepeat_m;
        }

        if (effectiveDropHeight <= 0 || effectiveDropHeight > rollLength_m) {
          const theoretical = rollWidth_m * rollLength_m;
          return theoretical;
        }

        const dropsPerRoll = Math.floor(rollLength_m / effectiveDropHeight);
        if (dropsPerRoll <= 0) {
          return rollWidth_m * rollLength_m;
        }
        const coverage = dropsPerRoll * rollWidth_m * effectiveDropHeight;
        return coverage;
      }

      function formatArea(value) {
        if (value == null || isNaN(value)) return "–";
        const rounded = Math.round(value * 100) / 100;
        return unitSystem === "ft" ? `${rounded} ft²` : `${rounded} m²`;
      }

      function formatCurrency(value) {
        if (!value || isNaN(value)) return "–";
        const rounded = Math.round(value * 100) / 100;
        return `$${rounded.toLocaleString()}`;
      }

      function calculate() {
        $("error-msg").textContent = "";
        const wallArea =
          mode === "simple" ? getSimpleWallArea() : getAdvancedWallArea();
        if (wallArea == null) {
          $("error-msg").textContent = "Please enter valid wall dimensions.";
          $("results-section").classList.add("hidden");
          return;
        }

        const openingsArea =
          mode === "simple"
            ? getSimpleOpeningsArea()
            : getAdvancedOpeningsArea();
        let netArea = wallArea - openingsArea;
        if (netArea <= 0) {
          $("error-msg").textContent =
            "Openings area cannot exceed total wall area.";
          $("results-section").classList.add("hidden");
          return;
        }

        let representativeHeight;
        if (mode === "simple") {
          representativeHeight = parseFloat($("simple-height").value);
        } else {
          const rows = $("walls-container").querySelectorAll("[data-wall-id]");
          let maxH = 0;
          for (const row of rows) {
            const h = parseFloat(row.querySelector(".wall-height").value);
            if (h && h > maxH) maxH = h;
          }
          representativeHeight = maxH;
        }
        if (!representativeHeight || representativeHeight <= 0) {
          $("error-msg").textContent = "Please enter a valid wall height.";
          $("results-section").classList.add("hidden");
          return;
        }

        const coveragePerRoll_m2 = computeCoveragePerRoll(representativeHeight);
        if (!coveragePerRoll_m2 || coveragePerRoll_m2 <= 0) {
          $("error-msg").textContent =
            "Please check roll dimensions and pattern repeat.";
          $("results-section").classList.add("hidden");
          return;
        }

        let netArea_m2;
        if (unitSystem === "ft") {
          netArea_m2 = netArea * 0.092903;
        } else {
          netArea_m2 = netArea;
        }

        const wastePercent = parseInt($("waste-margin").value || "0", 10);
        const adjustedArea_m2 = netArea_m2 * (1 + wastePercent / 100);

        const rollsNeeded = adjustedArea_m2 / coveragePerRoll_m2;
        const rollsRounded = Math.ceil(rollsNeeded);

        const totalCoverage_m2 = rollsRounded * coveragePerRoll_m2;
        const wasteArea_m2 = totalCoverage_m2 - netArea_m2;

        let netArea_display = netArea;
        let wallArea_display = wallArea;
        let wasteArea_display = wasteArea_m2;
        let coverage_display = coveragePerRoll_m2;

        if (unitSystem === "ft") {
          netArea_display = netArea;
          wallArea_display = wallArea;
          wasteArea_display = wasteArea_m2 / 0.092903;
          coverage_display = coveragePerRoll_m2 / 0.092903;
        }

        $("res-wall-area").textContent = formatArea(wallArea_display);
        $("res-net-area").textContent = formatArea(netArea_display);
        $("res-coverage-per-roll").textContent = formatArea(coverage_display);
        $("res-waste-margin").textContent = wastePercent + "%";
        $("res-rolls").textContent = isFinite(rollsRounded)
          ? rollsRounded
          : "–";
        $("res-waste-area").textContent = formatArea(wasteArea_display);

        const price = parseFloat($("price-per-roll").value);
        if (price && price > 0 && isFinite(rollsRounded)) {
          $("res-total-cost").textContent = formatCurrency(
            rollsRounded * price
          );
        } else {
          $("res-total-cost").textContent = "–";
        }

        $("results-section").classList.remove("hidden");
      }

      function resetCalculator() {
        $("simple-perimeter").value = "";
        $("simple-height").value = "";
        $("simple-openings-toggle").value = "no";
        $("simple-openings-fields").classList.add("hidden");
        $("simple-doors-count").value = "";
        $("simple-windows-count").value = "";

        $("walls-container").innerHTML = "";
        addWallRow();

        $("adv-door-area").value = "";
        $("adv-window-area").value = "";

        $("roll-width").value = "";
        $("roll-length").value = "";
        $("pattern-repeat").value = "";
        setDefaultRollValues();

        $("waste-margin").value = 10;
        $("waste-margin-label").textContent = "10%";
        $("price-per-roll").value = "";
        $("roll-type").value = "single";

        $("results-section").classList.add("hidden");
        $("error-msg").textContent = "";
      }

      document.addEventListener("DOMContentLoaded", () => {
        updateUnitLabels();
        setDefaultRollValues();
        addWallRow();

        $("unit-ft").addEventListener("click", () => switchUnit("ft"));
        $("unit-m").addEventListener("click", () => switchUnit("m"));

        $("mode-simple").addEventListener("click", () => switchMode("simple"));
        $("mode-advanced").addEventListener("click", () =>
          switchMode("advanced")
        );

        $("add-wall").addEventListener("click", () => addWallRow());

        $("simple-openings-toggle").addEventListener("change", (e) => {
          $("simple-openings-fields").classList.toggle(
            "hidden",
            e.target.value !== "yes"
          );
        });

        $("waste-margin").addEventListener("input", (e) => {
          $("waste-margin-label").textContent = e.target.value + "%";
        });

        $("calculate-btn").addEventListener("click", calculate);
        $("reset-btn").addEventListener("click", resetCalculator);
      });
    </script>

