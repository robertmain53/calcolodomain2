    <script>
      // Utility: format currency
      function formatCurrency(value) {
        if (!isFinite(value)) return "–";
        const abs = Math.abs(value);
        const options = {
          style: "currency",
          currency: "USD",
          minimumFractionDigits: abs < 1 ? 4 : 2,
          maximumFractionDigits: abs < 1 ? 4 : 2,
        };
        return new Intl.NumberFormat("en-US", options).format(value);
      }

      function formatPercent(value) {
        if (!isFinite(value)) return "–";
        return (value * 100).toFixed(2) + "%";
      }

      const cashflowBody = document.getElementById("cashflowBody");
      const addRowBtn = document.getElementById("addRowBtn");
      const resetExampleBtn = document.getElementById("resetExampleBtn");
      const clearAllBtn = document.getElementById("clearAllBtn");
      const calculateBtn = document.getElementById("calculateBtn");
      const calcError = document.getElementById("calcError");

      const financeRateInput = document.getElementById("financeRate");
      const reinvestRateInput = document.getElementById("reinvestRate");
      const periodUnitSelect = document.getElementById("periodUnit");

      const resultsPanel = document.getElementById("resultsPanel");
      const mirrResult = document.getElementById("mirrResult");
      const mirrPeriodLabel = document.getElementById("mirrPeriodLabel");
      const fvPosResult = document.getElementById("fvPosResult");
      const pvNegResult = document.getElementById("pvNegResult");
      const nPeriodsLabel = document.getElementById("nPeriodsLabel");
      const financeRateLabel = document.getElementById("financeRateLabel");
      const reinvestRateLabel = document.getElementById("reinvestRateLabel");

      function createRow(period, amount) {
        const tr = document.createElement("tr");
        tr.innerHTML = `
      <td>
        <input type="number" class="period-input w-20 border rounded px-2 py-1 text-sm" value="${period}" min="0" step="1" />
      </td>
      <td>
        <input type="number" class="amount-input w-32 border rounded px-2 py-1 text-sm" value="${amount}" step="0.01" />
      </td>
      <td>
        <span class="type-badge inline-flex items-center badge border ${
          amount < 0
            ? "bg-red-50 text-red-700 border-red-200"
            : "bg-green-50 text-green-700 border-green-200"
        }">
          ${amount < 0 ? "Outflow" : "Inflow"}
        </span>
      </td>
      <td>
        <button type="button" class="remove-row text-xs text-red-600 hover:underline">Remove</button>
      </td>
    `;
        cashflowBody.appendChild(tr);

        const amountInput = tr.querySelector(".amount-input");
        amountInput.addEventListener("input", () => updateTypeBadge(tr));

        const removeBtn = tr.querySelector(".remove-row");
        removeBtn.addEventListener("click", () => {
          tr.remove();
          renumberPeriods();
        });
      }

      function updateTypeBadge(row) {
        const amount = parseFloat(
          row.querySelector(".amount-input").value || "0"
        );
        const badge = row.querySelector(".type-badge");
        if (!badge) return;
        if (amount < 0) {
          badge.textContent = "Outflow";
          badge.className =
            "type-badge inline-flex items-center badge border bg-red-50 text-red-700 border-red-200";
        } else if (amount > 0) {
          badge.textContent = "Inflow";
          badge.className =
            "type-badge inline-flex items-center badge border bg-green-50 text-green-700 border-green-200";
        } else {
          badge.textContent = "Zero";
          badge.className =
            "type-badge inline-flex items-center badge border bg-gray-50 text-gray-600 border-gray-200";
        }
      }

      function renumberPeriods() {
        const rows = cashflowBody.querySelectorAll("tr");
        rows.forEach((row, index) => {
          const periodInput = row.querySelector(".period-input");
          if (periodInput) periodInput.value = index;
        });
      }

      function loadExample() {
        cashflowBody.innerHTML = "";
        // Example: -100, 30, 40, 50
        createRow(0, -100);
        createRow(1, 30);
        createRow(2, 40);
        createRow(3, 50);
        financeRateInput.value = 8;
        reinvestRateInput.value = 10;
        periodUnitSelect.value = "year";
        resultsPanel.classList.add("hidden");
        calcError.classList.add("hidden");
      }

      function clearAll() {
        cashflowBody.innerHTML = "";
        resultsPanel.classList.add("hidden");
        calcError.classList.add("hidden");
      }

      function getCashflows() {
        const rows = Array.from(cashflowBody.querySelectorAll("tr"));
        const data = [];
        rows.forEach((row) => {
          const t = parseInt(row.querySelector(".period-input").value, 10);
          const cf = parseFloat(row.querySelector(".amount-input").value);
          if (!isNaN(t) && !isNaN(cf)) {
            data.push({ t, cf });
          }
        });
        return data;
      }

      function calculateMIRR() {
        calcError.classList.add("hidden");
        const cfData = getCashflows();

        if (cfData.length === 0) {
          calcError.textContent = "Please enter at least one cash flow.";
          calcError.classList.remove("hidden");
          resultsPanel.classList.add("hidden");
          return;
        }

        // Sort by period
        cfData.sort((a, b) => a.t - b.t);

        const periods = cfData.map((d) => d.t);
        const minT = Math.min(...periods);
        const maxT = Math.max(...periods);
        const n = maxT - minT;

        if (n <= 0) {
          calcError.textContent =
            "MIRR requires at least two periods (e.g., t = 0 and t = 1).";
          calcError.classList.remove("hidden");
          resultsPanel.classList.add("hidden");
          return;
        }

        let financeRate = parseFloat(financeRateInput.value) / 100;
        let reinvestRate = parseFloat(reinvestRateInput.value) / 100;

        if (!isFinite(financeRate)) financeRate = 0;
        if (!isFinite(reinvestRate)) reinvestRate = 0;

        let pvNeg = 0;
        let fvPos = 0;
        let hasNeg = false;
        let hasPos = false;

        cfData.forEach(({ t, cf }) => {
          const relT = t - minT;
          if (cf < 0) {
            hasNeg = true;
            pvNeg += cf / Math.pow(1 + financeRate, relT);
          } else if (cf > 0) {
            hasPos = true;
            fvPos += cf * Math.pow(1 + reinvestRate, maxT - t);
          }
        });

        if (!hasNeg || !hasPos) {
          calcError.textContent =
            "MIRR requires at least one negative and one positive cash flow.";
          calcError.classList.remove("hidden");
          resultsPanel.classList.add("hidden");
          return;
        }

        if (pvNeg >= 0 || fvPos <= 0) {
          calcError.textContent =
            "Cash flow pattern does not allow a valid MIRR (check signs and rates).";
          calcError.classList.remove("hidden");
          resultsPanel.classList.add("hidden");
          return;
        }

        const ratio = fvPos / -pvNeg;
        if (ratio <= 0) {
          calcError.textContent =
            "Unable to compute MIRR due to invalid ratio of FV to PV.";
          calcError.classList.remove("hidden");
          resultsPanel.classList.add("hidden");
          return;
        }

        const mirr = Math.pow(ratio, 1 / n) - 1;

        // Display results
        mirrResult.textContent = formatPercent(mirr);
        fvPosResult.textContent = formatCurrency(fvPos);
        pvNegResult.textContent = formatCurrency(pvNeg);

        nPeriodsLabel.textContent = n.toString();
        financeRateLabel.textContent = formatPercent(financeRate);
        reinvestRateLabel.textContent = formatPercent(reinvestRate);

        const unit = periodUnitSelect.value;
        let unitLabel = "per period";
        if (unit === "year") unitLabel = "per year";
        else if (unit === "quarter") unitLabel = "per quarter";
        else if (unit === "month") unitLabel = "per month";
        mirrPeriodLabel.textContent = unitLabel;

        resultsPanel.classList.remove("hidden");
      }

      addRowBtn.addEventListener("click", () => {
        const rows = cashflowBody.querySelectorAll("tr");
        const nextPeriod = rows.length;
        createRow(nextPeriod, 0);
      });

      resetExampleBtn.addEventListener("click", loadExample);
      clearAllBtn.addEventListener("click", clearAll);
      calculateBtn.addEventListener("click", calculateMIRR);

      // Initialize with example
      loadExample();
    </script>

