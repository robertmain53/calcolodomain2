<script>
  // Tab switching
  const tabLittle = document.getElementById('tab-littleslaw');
  const tabTs = document.getElementById('tab-timestamps');
  const panelLittle = document.getElementById('panel-littleslaw');
  const panelTs = document.getElementById('panel-timestamps');

  function activateTab(tab) {
    if (tab === 'little') {
      tabLittle.classList.remove('bg-white','text-gray-700','border-gray-300');
      tabLittle.classList.add('bg-blue-600','text-white','border-blue-600');
      tabLittle.setAttribute('aria-selected','true');

      tabTs.classList.remove('bg-blue-600','text-white','border-blue-600');
      tabTs.classList.add('bg-white','text-gray-700','border-gray-300');
      tabTs.setAttribute('aria-selected','false');

      panelLittle.classList.remove('hidden');
      panelTs.classList.add('hidden');
    } else {
      tabTs.classList.remove('bg-white','text-gray-700','border-gray-300');
      tabTs.classList.add('bg-blue-600','text-white','border-blue-600');
      tabTs.setAttribute('aria-selected','true');

      tabLittle.classList.remove('bg-blue-600','text-white','border-blue-600');
      tabLittle.classList.add('bg-white','text-gray-700','border-gray-300');
      tabLittle.setAttribute('aria-selected','false');

      panelTs.classList.remove('hidden');
      panelLittle.classList.add('hidden');
    }
  }

  tabLittle.addEventListener('click', () => activateTab('little'));
  tabTs.addEventListener('click', () => activateTab('ts'));

  // Little's Law calculation
  const wipInput = document.getElementById('wip');
  const throughputInput = document.getElementById('throughput');
  const throughputUnit = document.getElementById('throughput-unit');
  const cycleOutputUnit = document.getElementById('cycle-output-unit');
  const btnCalcLittle = document.getElementById('btn-calc-littleslaw');
  const btnResetLittle = document.getElementById('btn-reset-littleslaw');
  const resultLittle = document.getElementById('result-littleslaw');
  const cycleLittleMain = document.getElementById('cycle-littleslaw-main');
  const cycleLittleAlt = document.getElementById('cycle-littleslaw-alt');

  function formatNumber(n, decimals) {
    return Number.isFinite(n) ? n.toFixed(decimals) : '';
  }

  btnCalcLittle.addEventListener('click', () => {
    const wip = parseFloat(wipInput.value);
    const th = parseFloat(throughputInput.value);

    if (!Number.isFinite(wip) || wip < 0) {
      alert('Please enter a valid non-negative WIP value.');
      return;
    }
    if (!Number.isFinite(th) || th <= 0) {
      alert('Please enter a valid positive throughput value.');
      return;
    }

    // Cycle time in the natural unit of throughput
    let cycleInUnit = wip / th; // e.g. days if throughput is items/day
    let unitLabel = '';
    if (throughputUnit.value === 'per_day') {
      unitLabel = 'days';
    } else if (throughputUnit.value === 'per_week') {
      unitLabel = 'weeks';
    } else {
      unitLabel = 'hours';
    }

    // Convert to requested output unit
    let cycleOut;
    let outLabel = cycleOutputUnit.value === 'days' ? 'days' : 'hours';

    if (throughputUnit.value === 'per_day') {
      if (cycleOutputUnit.value === 'days') {
        cycleOut = cycleInUnit;
      } else {
        cycleOut = cycleInUnit * 24;
      }
    } else if (throughputUnit.value === 'per_week') {
      if (cycleOutputUnit.value === 'days') {
        cycleOut = cycleInUnit * 7;
      } else {
        cycleOut = cycleInUnit * 7 * 24;
      }
    } else { // per_hour
      if (cycleOutputUnit.value === 'days') {
        cycleOut = cycleInUnit / 24;
      } else {
        cycleOut = cycleInUnit;
      }
    }

    cycleLittleMain.textContent = `${formatNumber(cycleOut, 2)} ${outLabel} per item`;
    cycleLittleAlt.textContent = `Raw Little’s Law result: ${formatNumber(cycleInUnit, 2)} ${unitLabel} per item (based on your throughput unit).`;
    resultLittle.classList.remove('hidden');
  });

  btnResetLittle.addEventListener('click', () => {
    wipInput.value = '';
    throughputInput.value = '';
    throughputUnit.value = 'per_day';
    cycleOutputUnit.value = 'days';
    resultLittle.classList.add('hidden');
  });

  // Timestamp-based calculation
  const tsInput = document.getElementById('timestamps-input');
  const tsOutputUnit = document.getElementById('timestamp-output-unit');
  const btnCalcTs = document.getElementById('btn-calc-timestamps');
  const btnResetTs = document.getElementById('btn-reset-timestamps');
  const resultTs = document.getElementById('result-timestamps');
  const tsCount = document.getElementById('ts-count');
  const tsAvg = document.getElementById('ts-avg');
  const tsMin = document.getElementById('ts-min');
  const tsMax = document.getElementById('ts-max');
  const tsTableBody = document.getElementById('ts-table-body');

  function parseDateFlexible(str) {
    // Try native Date parsing; expects ISO-like strings to be reliable
    const d = new Date(str.trim());
    return isNaN(d.getTime()) ? null : d;
  }

  btnCalcTs.addEventListener('click', () => {
    const lines = tsInput.value.split('\n').map(l => l.trim()).filter(l => l.length > 0);
    if (lines.length === 0) {
      alert('Please enter at least one line with start and end timestamps.');
      return;
    }

    const durations = [];
    const rows = [];
    for (let i = 0; i < lines.length; i++) {
      const line = lines[i];
      const parts = line.split(/,|\t/);
      if (parts.length < 2) continue;
      const startStr = parts[0];
      const endStr = parts[1];

      const start = parseDateFlexible(startStr);
      const end = parseDateFlexible(endStr);
      if (!start || !end) continue;

      const diffMs = end - start;
      if (!Number.isFinite(diffMs) || diffMs < 0) continue;

      let diff;
      let label;
      if (tsOutputUnit.value === 'hours') {
        diff = diffMs / (1000 * 60 * 60);
        label = 'h';
      } else {
        diff = diffMs / (1000 * 60 * 60 * 24);
        label = 'd';
      }
      durations.push(diff);
      rows.push({
        index: i + 1,
        start: startStr.trim(),
        end: endStr.trim(),
        value: diff,
        label
      });
    }

    if (durations.length === 0) {
      alert('No valid lines found. Please check the date format (e.g. 2025-01-02 09:00).');
      return;
    }

    const sum = durations.reduce((a, b) => a + b, 0);
    const avg = sum / durations.length;
    const min = Math.min(...durations);
    const max = Math.max(...durations);
    const unitLabel = tsOutputUnit.value === 'hours' ? 'hours' : 'days';

    tsCount.textContent = durations.length;
    tsAvg.textContent = `${formatNumber(avg, 2)} ${unitLabel}`;
    tsMin.textContent = `${formatNumber(min, 2)} ${unitLabel}`;
    tsMax.textContent = `${formatNumber(max, 2)} ${unitLabel}`;

    tsTableBody.innerHTML = '';
    rows.forEach(r => {
      const tr = document.createElement('tr');
      tr.className = 'border-b border-blue-50';
      tr.innerHTML = `
        <td class="py-1 pr-2">${r.index}</td>
        <td class="py-1 pr-2 whitespace-nowrap">${r.start}</td>
        <td class="py-1 pr-2 whitespace-nowrap">${r.end}</td>
        <td class="py-1 pr-2">${formatNumber(r.value, 2)} ${r.label}</td>
      `;
      tsTableBody.appendChild(tr);
    });

    resultTs.classList.remove('hidden');
  });

  btnResetTs.addEventListener('click', () => {
    tsInput.value = '';
    tsOutputUnit.value = 'hours';
    resultTs.classList.add('hidden');
    tsTableBody.innerHTML = '';
  });

  window.MathJax = {
    tex: { inlineMath: [['\\(','\\)'], ['$', '$']], displayMath: [['$','$'], ['\\[','\\]']] },
    svg: { fontCache: 'global' }
  };
</script>
<script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>

