    <script>
      // Tab switching
      document.querySelectorAll(".mode-tab").forEach((btn) => {
        btn.addEventListener("click", () => {
          const mode = btn.dataset.mode;
          document.querySelectorAll(".mode-tab").forEach((b) => {
            b.classList.remove("border-blue-600", "text-blue-600");
            b.classList.add("border-transparent", "text-gray-600");
          });
          btn.classList.add("border-blue-600", "text-blue-600");
          btn.classList.remove("text-gray-600");

          document
            .querySelectorAll(".mode-section")
            .forEach((sec) => sec.classList.add("hidden"));
          const active = document.getElementById("mode-" + mode);
          if (active) active.classList.remove("hidden");
        });
      });

      // Unit helpers
      function toSquareMeters(value, unit) {
        const v = Number(value);
        if (!isFinite(v)) return NaN;
        switch (unit) {
          case "mm2":
            return v * 1e-6;
          case "cm2":
            return v * 1e-4;
          case "m2":
            return v;
          default:
            return NaN;
        }
      }
      function toMeters(value, unit) {
        const v = Number(value);
        if (!isFinite(v)) return NaN;
        switch (unit) {
          case "mm":
            return v * 1e-3;
          case "cm":
            return v * 1e-2;
          case "m":
            return v;
          default:
            return NaN;
        }
      }
      function stressToPa(value, unit) {
        const v = Number(value);
        if (!isFinite(v)) return NaN;
        switch (unit) {
          case "Pa":
            return v;
          case "MPa":
            return v * 1e6;
          case "GPa":
            return v * 1e9;
          default:
            return NaN;
        }
      }
      function paToStressUnit(pa, unit) {
        switch (unit) {
          case "Pa":
            return pa;
          case "MPa":
            return pa / 1e6;
          case "GPa":
            return pa / 1e9;
          default:
            return pa;
        }
      }

      // Basic stress / force solver
      document.getElementById("calcStressBtn").addEventListener("click", () => {
        const F = parseFloat(document.getElementById("force").value);
        const Funit = document.getElementById("forceUnit").value;
        const A = parseFloat(document.getElementById("area").value);
        const Aunit = document.getElementById("areaUnit").value;
        const sigma = parseFloat(document.getElementById("stress").value);
        const sigmaUnit = document.getElementById("stressUnit").value;
        const summary = document.getElementById("stressSummary");

        const A_m2 = toSquareMeters(A, Aunit);
        if (!isFinite(A_m2) || A_m2 <= 0) {
          summary.textContent = "Please enter a valid positive area.";
          return;
        }

        let knownCount = 0;
        if (isFinite(F)) knownCount++;
        if (isFinite(sigma)) knownCount++;

        if (knownCount === 0) {
          summary.textContent =
            "Enter either force or stress (with area) to solve.";
          return;
        }
        if (knownCount > 1) {
          summary.textContent =
            "Provide only one of force or stress; the other will be computed.";
          return;
        }

        let F_N, sigma_Pa;

        if (isFinite(F)) {
          F_N = Funit === "kN" ? F * 1000 : F;
          sigma_Pa = F_N / A_m2;
          const sigmaOut = paToStressUnit(sigma_Pa, sigmaUnit);
          document.getElementById("stress").value = sigmaOut.toPrecision(6);
        } else {
          sigma_Pa = stressToPa(sigma, sigmaUnit);
          if (!isFinite(sigma_Pa)) {
            summary.textContent = "Invalid stress value.";
            return;
          }
          F_N = sigma_Pa * A_m2;
          const Fout =
            document.getElementById("forceUnit").value === "kN"
              ? F_N / 1000
              : F_N;
          document.getElementById("force").value = Fout.toPrecision(6);
        }

        const sigmaMPa = sigma_Pa / 1e6;
        summary.textContent = `Computed stress: ${sigmaMPa.toFixed(
          3
        )} MPa (area = ${A} ${Aunit}).`;
      });

      // Basic strain / elongation solver
      document.getElementById("calcStrainBtn").addEventListener("click", () => {
        const L0 = parseFloat(document.getElementById("L0").value);
        const L0Unit = document.getElementById("L0Unit").value;
        const dL = parseFloat(document.getElementById("dL").value);
        const dLUnit = document.getElementById("dLUnit").value;
        const eps = parseFloat(document.getElementById("strain").value);
        const summary = document.getElementById("strainSummary");

        const L0_m = toMeters(L0, L0Unit);
        if (!isFinite(L0_m) || L0_m <= 0) {
          summary.textContent =
            "Please enter a valid positive original length.";
          return;
        }

        let knownCount = 0;
        if (isFinite(dL)) knownCount++;
        if (isFinite(eps)) knownCount++;

        if (knownCount === 0) {
          summary.textContent =
            "Enter either change in length or strain to solve.";
          return;
        }
        if (knownCount > 1) {
          summary.textContent =
            "Provide only one of change in length or strain; the other will be computed.";
          return;
        }

        let dL_m, strain;

        if (isFinite(dL)) {
          dL_m = toMeters(dL, dLUnit);
          if (!isFinite(dL_m)) {
            summary.textContent = "Invalid change in length.";
            return;
          }
          strain = dL_m / L0_m;
          document.getElementById("strain").value = strain.toPrecision(6);
        } else {
          strain = eps;
          dL_m = strain * L0_m;
          const dLout = (() => {
            const u = document.getElementById("dLUnit").value;
            switch (u) {
              case "mm":
                return dL_m * 1e3;
              case "cm":
                return dL_m * 1e2;
              case "m":
                return dL_m;
              default:
                return dL_m;
            }
          })();
          document.getElementById("dL").value = dLout.toPrecision(6);
        }

        summary.textContent = `Computed strain: ε = ${strain.toExponential(
          3
        )} (${(strain * 100).toFixed(3)} %).`;
      });

      // Modulus & safety factor
      document
        .getElementById("calcModulusBtn")
        .addEventListener("click", () => {
          const sigma = parseFloat(document.getElementById("modStress").value);
          const sigmaUnit = document.getElementById("modStressUnit").value;
          const eps = parseFloat(document.getElementById("modStrain").value);
          const ys = parseFloat(document.getElementById("yieldStrength").value);
          const ysUnit = document.getElementById("yieldUnit").value;
          const out = document.getElementById("modulusResults");
          out.innerHTML = "";

          if (!isFinite(sigma) || !isFinite(eps) || eps === 0) {
            out.textContent =
              "Enter both a valid stress and a non-zero strain.";
            return;
          }

          const sigmaPa = stressToPa(sigma, sigmaUnit);
          const E = sigmaPa / eps; // Pa
          const EGPa = E / 1e9;
          const EMPa = E / 1e6;

          let html = `<p>Estimated Young’s modulus: <strong>${EGPa.toFixed(
            3
          )} GPa</strong> (${EMPa.toFixed(1)} MPa).</p>`;
          html += `<p>Using E = σ / ε with σ = ${sigma} ${sigmaUnit} and ε = ${eps}.</p>`;

          if (isFinite(ys)) {
            const ysPa = stressToPa(ys, ysUnit);
            const n = ysPa / sigmaPa;
            html += `<p>Yield strength: ${ys} ${ysUnit}. Working stress: ${sigma} ${sigmaUnit}. Estimated safety factor: <strong>n = ${n.toFixed(
              2
            )}</strong>.</p>`;
          } else {
            html += `<p class="text-gray-600">Provide a yield strength to compute a safety factor.</p>`;
          }

          out.innerHTML = html;
        });

      // Stress-strain curve plotting
      let chart;
      document.getElementById("plotCurveBtn").addEventListener("click", () => {
        const raw = document.getElementById("curveData").value.trim();
        const summary = document.getElementById("curveSummary");
        summary.innerHTML = "";

        if (!raw) {
          summary.textContent =
            "Paste at least a few lines of stress–strain data.";
          return;
        }

        const lines = raw.split(/\r?\n/);
        const points = [];
        for (const line of lines) {
          const clean = line.trim();
          if (!clean) continue;
          const parts = clean.split(/[\s,;]+/);
          if (parts.length < 2) continue;
          const s = parseFloat(parts[0]);
          const e = parseFloat(parts[1]);
          if (isFinite(s) && isFinite(e)) {
            points.push({ x: e, y: s });
          }
        }

        if (points.length < 2) {
          summary.textContent =
            "Could not parse enough valid data points. Make sure each line has stress and strain values.";
          return;
        }

        points.sort((a, b) => a.x - b.x);

        const ctx = document
          .getElementById("stressStrainChart")
          .getContext("2d");
        if (chart) chart.destroy();
        chart = new Chart(ctx, {
          type: "scatter",
          data: {
            datasets: [
              {
                label: "Stress–strain data",
                data: points,
                showLine: true,
                borderColor: "rgb(37, 99, 235)",
                backgroundColor: "rgba(37, 99, 235, 0.3)",
                pointRadius: 3,
                tension: 0.1,
              },
            ],
          },
          options: {
            responsive: true,
            scales: {
              x: {
                type: "linear",
                title: { display: true, text: "Strain ε (dimensionless)" },
              },
              y: {
                title: { display: true, text: "Stress σ (MPa)" },
              },
            },
          },
        });

        // Estimate E from first ~20% of points
        const n = points.length;
        const k = Math.max(2, Math.floor(n * 0.2));
        let sumX = 0,
          sumY = 0,
          sumXY = 0,
          sumXX = 0;
        for (let i = 0; i < k; i++) {
          const { x, y } = points[i];
          sumX += x;
          sumY += y;
          sumXY += x * y;
          sumXX += x * x;
        }
        const denom = k * sumXX - sumX * sumX;
        let slope = NaN;
        if (Math.abs(denom) > 0) {
          slope = (k * sumXY - sumX * sumY) / denom; // MPa per strain
        }

        if (isFinite(slope)) {
          const EMPa = slope;
          const EGPa = EMPa / 1000;
          summary.innerHTML = `<p>Estimated Young’s modulus from initial ${k} points: <strong>${EGPa.toFixed(
            3
          )} GPa</strong> (${EMPa.toFixed(1)} MPa).</p>
      <p class="text-gray-600 text-sm">Assumes the first portion of the curve is linear elastic. For more accurate results, use carefully measured data and restrict to the purely elastic region.</p>`;
        } else {
          summary.textContent =
            "Could not estimate Young’s modulus from the provided data.";
        }
      });

      document
        .querySelectorAll("#faq-container .faq-question")
        .forEach((btn) => {
          btn.addEventListener("click", () => {
            const ans = btn.parentElement.querySelector(".faq-answer");
            const isHidden = ans.classList.contains("hidden");
            document
              .querySelectorAll("#faq-container .faq-answer")
              .forEach((a) => a.classList.add("hidden"));
            document
              .querySelectorAll("#faq-container .faq-question span:last-child")
              .forEach((icon) => (icon.textContent = "+"));
            if (isHidden) {
              ans.classList.remove("hidden");
              btn.querySelector("span:last-child").textContent = "−";
            }
          });
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
