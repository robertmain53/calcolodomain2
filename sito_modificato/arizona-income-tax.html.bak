    <script>
      // NOTE: For demonstration we use a placeholder flat rate and deduction values.
      // Update these constants when Arizona publishes new official rates/amounts.
      const AZ_TAX_CONFIG = {
        2024: {
          flatRate: 0.025, // 2.5% flat rate (example; adjust to official rate if it changes)
          standardDeduction: {
            single: 13850,
            mfj: 27700,
            mfs: 13850,
            hoh: 20800,
          },
        },
        2023: {
          flatRate: 0.025, // 2.5% flat rate
          standardDeduction: {
            single: 13850,
            mfj: 27700,
            mfs: 13850,
            hoh: 20800,
          },
        },
      };

      function formatCurrency(value) {
        return value.toLocaleString("en-US", {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 0,
        });
      }

      function formatPercent(value) {
        return (value * 100).toFixed(1) + "%";
      }

      function getConfig() {
        const year = document.getElementById("tax-year").value;
        return AZ_TAX_CONFIG[year];
      }

      function calculateArizonaTax() {
        const wages = Number(document.getElementById("wages").value) || 0;
        const otherIncome =
          Number(document.getElementById("other-income").value) || 0;
        const pretax =
          Number(document.getElementById("pretax-deductions").value) || 0;
        const azAdj =
          Number(document.getElementById("az-adjustments").value) || 0;
        const filingStatus = document.getElementById("filing-status").value;
        const deductionType = document.getElementById("deduction-type").value;
        const itemized =
          Number(document.getElementById("itemized-deductions").value) || 0;
        const credits =
          Number(document.getElementById("nonrefundable-credits").value) || 0;

        const errorMsg = document.getElementById("error-msg");

        if (
          wages < 0 ||
          otherIncome < 0 ||
          pretax < 0 ||
          credits < 0 ||
          itemized < 0
        ) {
          errorMsg.classList.remove("hidden");
          return;
        } else {
          errorMsg.classList.add("hidden");
        }

        const cfg = getConfig();

        const totalIncome = wages + otherIncome;
        const incomeBase = Math.max(0, totalIncome - pretax + azAdj);

        let deduction = 0;
        if (deductionType === "standard") {
          deduction = cfg.standardDeduction[filingStatus] || 0;
        } else if (deductionType === "itemized") {
          deduction = itemized;
        } else {
          deduction = 0;
        }

        const taxableIncome = Math.max(0, incomeBase - deduction);
        const grossTax = taxableIncome * cfg.flatRate;
        const netTax = Math.max(0, grossTax - credits);
        const takeHome = totalIncome - netTax;

        const effectiveRate = totalIncome > 0 ? netTax / totalIncome : 0;
        const marginalRate = taxableIncome > 0 ? cfg.flatRate : 0;

        document.getElementById("result-tax").textContent =
          formatCurrency(netTax);
        document.getElementById("result-takehome").textContent = formatCurrency(
          Math.max(0, takeHome)
        );
        document.getElementById("result-effective-rate").textContent =
          "Effective AZ tax rate: " + formatPercent(effectiveRate);
        document.getElementById("result-marginal-rate").textContent =
          "Marginal AZ tax rate: " + formatPercent(marginalRate);

        const breakdownBody = document.getElementById("breakdown-body");
        breakdownBody.innerHTML = "";

        const rows = [
          { label: "Total income (wages + other)", value: totalIncome },
          { label: "Pre‑tax deductions", value: -pretax },
          { label: "Arizona additions / subtractions (net)", value: azAdj },
          { label: "Arizona adjusted gross income (AGI)", value: incomeBase },
          { label: "Standard / itemized deduction", value: -deduction },
          { label: "Arizona taxable income", value: taxableIncome },
          { label: "Gross Arizona tax (before credits)", value: grossTax },
          { label: "Nonrefundable credits", value: -credits },
          { label: "Net Arizona state income tax", value: netTax },
        ];

        rows.forEach((row, idx) => {
          const tr = document.createElement("tr");
          const tdLabel = document.createElement("td");
          const tdValue = document.createElement("td");
          tdLabel.className =
            "px-4 py-2 text-gray-700" +
            (idx === rows.length - 1 ? " font-semibold" : "");
          tdValue.className =
            "px-4 py-2 text-right text-gray-900" +
            (idx === rows.length - 1 ? " font-semibold" : "");
          tdLabel.textContent = row.label;
          const sign = row.value < 0 ? "-" : "";
          tdValue.textContent = sign + formatCurrency(Math.abs(row.value));
          tr.appendChild(tdLabel);
          tr.appendChild(tdValue);
          breakdownBody.appendChild(tr);
        });
      }

      document.addEventListener("DOMContentLoaded", function () {
        const deductionTypeSelect = document.getElementById("deduction-type");
        const itemizedWrapper = document.getElementById("itemized-wrapper");
        deductionTypeSelect.addEventListener("change", function () {
          if (this.value === "itemized") {
            itemizedWrapper.classList.remove("hidden");
          } else {
            itemizedWrapper.classList.add("hidden");
          }
        });

        document
          .getElementById("calculate-btn")
          .addEventListener("click", calculateArizonaTax);

        document
          .getElementById("reset-btn")
          .addEventListener("click", function () {
            document.getElementById("filing-status").value = "single";
            document.getElementById("tax-year").value = "2024";
            document.getElementById("wages").value = 60000;
            document.getElementById("other-income").value = 0;
            document.getElementById("pretax-deductions").value = 0;
            document.getElementById("az-adjustments").value = 0;
            document.getElementById("deduction-type").value = "standard";
            document.getElementById("itemized-deductions").value = 0;
            document.getElementById("nonrefundable-credits").value = 0;
            document.getElementById("itemized-wrapper").classList.add("hidden");
            document.getElementById("error-msg").classList.add("hidden");
            calculateArizonaTax();
          });

        calculateArizonaTax();
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
