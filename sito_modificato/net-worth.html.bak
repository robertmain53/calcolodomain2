    <script>
      // Initial data
      const defaultAssets = [
        { name: "Checking accounts", amount: 0, liquid: true },
        { name: "Savings & money market", amount: 0, liquid: true },
        {
          name: "Retirement accounts (401(k), IRA, etc.)",
          amount: 0,
          liquid: false,
        },
        { name: "Taxable investments / brokerage", amount: 0, liquid: true },
        { name: "Primary home (market value)", amount: 0, liquid: false },
        { name: "Other real estate", amount: 0, liquid: false },
        { name: "Vehicles (cars, etc.)", amount: 0, liquid: false },
        { name: "Business interests", amount: 0, liquid: false },
        {
          name: "Other valuables (jewelry, art, etc.)",
          amount: 0,
          liquid: false,
        },
      ];

      const defaultLiabilities = [
        { name: "Mortgage(s)", amount: 0 },
        { name: "Student loans", amount: 0 },
        { name: "Auto loans / leases", amount: 0 },
        { name: "Credit card debt", amount: 0 },
        { name: "Personal loans / lines of credit", amount: 0 },
        { name: "Other debts", amount: 0 },
      ];

      const assetsTbody = document.getElementById("assets-tbody");
      const liabilitiesTbody = document.getElementById("liabilities-tbody");
      const currencySelect = document.getElementById("currency-select");
      const incomeCurrencyPrefix = document.getElementById(
        "income-currency-prefix"
      );

      function createAssetRow(asset, isCustom = false) {
        const tr = document.createElement("tr");
        tr.className = "border-t border-gray-100";
        tr.innerHTML = `
      <td class="px-3 py-2">
        <input type="text" class="w-full text-sm border border-transparent focus:border-gray-300 rounded-md px-2 py-1" value="${
          asset.name || ""
        }" placeholder="Asset name">
      </td>
      <td class="px-3 py-2 text-right">
        <div class="flex justify-end items-center">
          <span class="currency-prefix mr-1 text-gray-500">$</span>
          <input type="number" class="table-input w-32 border border-gray-300 rounded-md px-2 py-1 text-right text-sm asset-amount" min="0" step="0.01" value="${
            asset.amount || ""
          }">
        </div>
      </td>
      <td class="px-3 py-2 text-center">
        <input type="checkbox" class="asset-liquid-checkbox" ${
          asset.liquid ? "checked" : ""
        }>
      </td>
      <td class="px-3 py-2 text-right">
        ${
          isCustom
            ? '<button type="button" class="text-xs text-red-500 hover:underline remove-row">Remove</button>'
            : ""
        }
      </td>
    `;
        assetsTbody.appendChild(tr);
      }

      function createLiabilityRow(liability, isCustom = false) {
        const tr = document.createElement("tr");
        tr.className = "border-t border-gray-100";
        tr.innerHTML = `
      <td class="px-3 py-2">
        <input type="text" class="w-full text-sm border border-transparent focus:border-gray-300 rounded-md px-2 py-1" value="${
          liability.name || ""
        }" placeholder="Liability name">
      </td>
      <td class="px-3 py-2 text-right">
        <div class="flex justify-end items-center">
          <span class="currency-prefix mr-1 text-gray-500">$</span>
          <input type="number" class="table-input w-32 border border-gray-300 rounded-md px-2 py-1 text-right text-sm liability-amount" min="0" step="0.01" value="${
            liability.amount || ""
          }">
        </div>
      </td>
      <td class="px-3 py-2 text-right">
        ${
          isCustom
            ? '<button type="button" class="text-xs text-red-500 hover:underline remove-row">Remove</button>'
            : ""
        }
      </td>
    `;
        liabilitiesTbody.appendChild(tr);
      }

      function initTables() {
        assetsTbody.innerHTML = "";
        liabilitiesTbody.innerHTML = "";
        defaultAssets.forEach((a) => createAssetRow(a, false));
        defaultLiabilities.forEach((l) => createLiabilityRow(l, false));
        updateCurrencySymbols();
        calculateNetWorth();
      }

      function parseAmount(value) {
        const n = parseFloat(value);
        return isNaN(n) ? 0 : n;
      }

      function formatCurrency(value) {
        const symbol = currencySelect.value || "$";
        const absVal = Math.abs(value);
        const formatted = absVal.toLocaleString(undefined, {
          minimumFractionDigits: 2,
          maximumFractionDigits: 2,
        });
        if (value < 0) {
          return `-${symbol}${formatted}`;
        }
        return `${symbol}${formatted}`;
      }

      function calculateNetWorth() {
        let totalAssets = 0;
        let totalLiquidAssets = 0;
        let totalLiabilities = 0;

        // Assets
        assetsTbody.querySelectorAll("tr").forEach((tr) => {
          const amountInput = tr.querySelector(".asset-amount");
          const liquidCheckbox = tr.querySelector(".asset-liquid-checkbox");
          if (!amountInput) return;
          const amount = parseAmount(amountInput.value);
          totalAssets += amount;
          if (liquidCheckbox && liquidCheckbox.checked) {
            totalLiquidAssets += amount;
          }
        });

        // Liabilities
        liabilitiesTbody.querySelectorAll("tr").forEach((tr) => {
          const amountInput = tr.querySelector(".liability-amount");
          if (!amountInput) return;
          const amount = parseAmount(amountInput.value);
          totalLiabilities += amount;
        });

        const netWorth = totalAssets - totalLiabilities;
        const liquidNetWorth = totalLiquidAssets - totalLiabilities;
        const debtAssetRatio =
          totalAssets > 0 ? totalLiabilities / totalAssets : null;

        document.getElementById("total-assets-display").textContent =
          formatCurrency(totalAssets);
        document.getElementById("total-liquid-assets-display").textContent =
          formatCurrency(totalLiquidAssets);
        document.getElementById("total-liabilities-display").textContent =
          formatCurrency(totalLiabilities);
        document.getElementById("net-worth-display").textContent =
          formatCurrency(netWorth);
        document.getElementById("liquid-net-worth-display").textContent =
          formatCurrency(liquidNetWorth);

        const ratioEl = document.getElementById("debt-asset-ratio-display");
        if (debtAssetRatio === null) {
          ratioEl.textContent = "–";
        } else {
          ratioEl.textContent = (debtAssetRatio * 100).toFixed(1) + "%";
        }

        const statusEl = document.getElementById("net-worth-status");
        if (totalAssets === 0 && totalLiabilities === 0) {
          statusEl.textContent = "Enter values above to see your net worth.";
        } else if (netWorth < 0) {
          statusEl.textContent =
            "Your net worth is negative. Focus on reducing high-interest debt and building savings.";
        } else {
          statusEl.textContent =
            "Your net worth is positive. Track it over time to see your financial progress.";
        }

        updateBenchmark();
      }

      function updateCurrencySymbols() {
        const symbol = currencySelect.value || "$";
        document.querySelectorAll(".currency-prefix").forEach((el) => {
          el.textContent = symbol;
        });
        incomeCurrencyPrefix.textContent = symbol;
        // Re-render displayed currency values
        calculateNetWorth();
      }

      function updateBenchmark() {
        const age = parseAmount(document.getElementById("age-input").value);
        const income = parseAmount(
          document.getElementById("income-input").value
        );
        const benchmarkSummary = document.getElementById("benchmark-summary");
        const netWorthText =
          document.getElementById("net-worth-display").textContent;

        if (!age || !income) {
          benchmarkSummary.textContent =
            "Enter your age and annual income above to see a rough target net worth.";
          return;
        }

        const target = age * income;
        benchmarkSummary.textContent =
          `Rule-of-thumb target net worth at age ${age} with income ${formatCurrency(
            income
          )} is about ${formatCurrency(target)}. ` +
          `Your current net worth is ${netWorthText}. Use this only as a loose benchmark, not a strict goal.`;
      }

      // Event listeners
      document.addEventListener("input", (e) => {
        if (
          e.target.classList.contains("asset-amount") ||
          e.target.classList.contains("liability-amount") ||
          e.target.id === "age-input" ||
          e.target.id === "income-input"
        ) {
          calculateNetWorth();
        }
        if (e.target.classList.contains("asset-liquid-checkbox")) {
          calculateNetWorth();
        }
      });

      document.getElementById("add-asset-row").addEventListener("click", () => {
        createAssetRow({ name: "", amount: 0, liquid: true }, true);
        updateCurrencySymbols();
      });

      document
        .getElementById("add-liability-row")
        .addEventListener("click", () => {
          createLiabilityRow({ name: "", amount: 0 }, true);
          updateCurrencySymbols();
        });

      document.addEventListener("click", (e) => {
        if (e.target.classList.contains("remove-row")) {
          const tr = e.target.closest("tr");
          if (tr) tr.remove();
          calculateNetWorth();
        }
      });

      currencySelect.addEventListener("change", updateCurrencySymbols);

      document
        .getElementById("reset-calculator")
        .addEventListener("click", () => {
          document.getElementById("age-input").value = "";
          document.getElementById("income-input").value = "";
          initTables();
        });

      document.getElementById("export-json").addEventListener("click", () => {
        const data = {
          currency: currencySelect.value,
          age: parseAmount(document.getElementById("age-input").value) || null,
          income:
            parseAmount(document.getElementById("income-input").value) || null,
          assets: [],
          liabilities: [],
        };

        assetsTbody.querySelectorAll("tr").forEach((tr) => {
          const nameInput = tr.querySelector('input[type="text"]');
          const amountInput = tr.querySelector(".asset-amount");
          const liquidCheckbox = tr.querySelector(".asset-liquid-checkbox");
          if (!nameInput || !amountInput) return;
          const name = nameInput.value.trim() || "Asset";
          const amount = parseAmount(amountInput.value);
          const liquid = liquidCheckbox ? liquidCheckbox.checked : false;
          if (amount !== 0 || nameInput.value.trim() !== "") {
            data.assets.push({ name, amount, liquid });
          }
        });

        liabilitiesTbody.querySelectorAll("tr").forEach((tr) => {
          const nameInput = tr.querySelector('input[type="text"]');
          const amountInput = tr.querySelector(".liability-amount");
          if (!nameInput || !amountInput) return;
          const name = nameInput.value.trim() || "Liability";
          const amount = parseAmount(amountInput.value);
          if (amount !== 0 || nameInput.value.trim() !== "") {
            data.liabilities.push({ name, amount });
          }
        });

        const blob = new Blob([JSON.stringify(data, null, 2)], {
          type: "application/json",
        });
        const url = URL.createObjectURL(blob);
        const a = document.createElement("a");
        const dateStr = new Date().toISOString().slice(0, 10);
        a.href = url;
        a.download = `net-worth-${dateStr}.json`;
        document.body.appendChild(a);
        a.click();
        document.body.removeChild(a);
        URL.revokeObjectURL(url);
      });

      document
        .getElementById("toggle-benchmark-details")
        .addEventListener("click", () => {
          const details = document.getElementById("benchmark-details");
          details.classList.toggle("hidden");
        });

      // Simple FAQ toggle (in addition to global script_faq if any)
      document
        .querySelectorAll("#faq-container .faq-question")
        .forEach((btn) => {
          btn.addEventListener("click", () => {
            const answer = btn.parentElement.querySelector(".faq-answer");
            const symbol = btn.querySelector("span:last-child");
            const isHidden = answer.classList.contains("hidden");
            document
              .querySelectorAll("#faq-container .faq-answer")
              .forEach((a) => a.classList.add("hidden"));
            document
              .querySelectorAll("#faq-container .faq-question span:last-child")
              .forEach((s) => (s.textContent = "+"));
            if (isHidden) {
              answer.classList.remove("hidden");
              symbol.textContent = "−";
            } else {
              answer.classList.add("hidden");
              symbol.textContent = "+";
            }
          });
        });

      // Initialize
      initTables();
    </script>

