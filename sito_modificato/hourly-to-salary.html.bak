    <script>
      // Core calculation logic
      function parseNumber(id, fallback) {
        const el = document.getElementById(id);
        if (!el) return fallback;
        const v = parseFloat(el.value);
        return isNaN(v) ? fallback : v;
      }

      function formatMoney(amount, symbol) {
        if (!isFinite(amount)) return "—";
        const abs = Math.abs(amount);
        const decimals = abs >= 1000 ? 0 : 2;
        return (
          symbol +
          amount.toLocaleString(undefined, {
            minimumFractionDigits: decimals,
            maximumFractionDigits: decimals,
          })
        );
      }

      function getCurrencySymbol() {
        const select = document.getElementById("currency");
        const custom = document.getElementById("customCurrency");
        if (!select) return "$";
        if (select.value === "custom") {
          return custom && custom.value.trim() ? custom.value.trim() : "";
        }
        return select.value;
      }

      function updateCurrencyPrefix() {
        const prefix = document.getElementById("currencyPrefix");
        if (prefix) prefix.textContent = getCurrencySymbol() || "$";
      }

      function updateQuickTable() {
        const tbody = document.getElementById("quickTableBody");
        if (!tbody) return;
        const symbol = getCurrencySymbol() || "$";
        const hoursPerWeek = 40;
        const weeksPerYear = 52;
        const hourlyValues = [10, 15, 20, 25, 30, 35, 40, 50];

        tbody.innerHTML = "";
        hourlyValues.forEach((rate) => {
          const annual = rate * hoursPerWeek * weeksPerYear;
          const monthly = annual / 12;
          const weekly = annual / weeksPerYear;
          const tr = document.createElement("tr");
          tr.className = "odd:bg-white even:bg-gray-50";
          tr.innerHTML = `
        <td class="px-3 py-2 border-t">${formatMoney(rate, symbol)}</td>
        <td class="px-3 py-2 border-t">${formatMoney(annual, symbol)}</td>
        <td class="px-3 py-2 border-t">${formatMoney(monthly, symbol)}</td>
        <td class="px-3 py-2 border-t">${formatMoney(weekly, symbol)}</td>
      `;
          tbody.appendChild(tr);
        });
      }

      function recalc() {
        const symbol = getCurrencySymbol() || "$";
        const hourly = parseNumber("hourlyRate", 0);
        let hoursPerWeek = parseNumber("hoursPerWeek", 40);
        const weeksPerYear = parseNumber("weeksPerYear", 52);
        const unpaidBreakMinutes = parseNumber("unpaidBreakMinutes", 0);
        const daysPerWeek = parseNumber("daysPerWeek", 5);
        const overtimeHours = parseNumber("overtimeHours", 0);
        const overtimeMultiplier = parseNumber("overtimeMultiplier", 1.5);
        const includeOvertime =
          document.getElementById("includeOvertime")?.checked;

        if (hoursPerWeek < 0) hoursPerWeek = 0;

        // Effective paid hours per day after unpaid breaks
        const paidHoursPerDay =
          daysPerWeek > 0
            ? Math.max(hoursPerWeek / daysPerWeek - unpaidBreakMinutes / 60, 0)
            : 0;
        const effectiveHoursPerWeek = paidHoursPerDay * daysPerWeek;

        let weeklyPayBase = hourly * effectiveHoursPerWeek;
        let weeklyPay = weeklyPayBase;

        if (includeOvertime && overtimeHours > 0) {
          weeklyPay += hourly * overtimeHours * overtimeMultiplier;
        }

        const annualSalary = weeklyPay * weeksPerYear;
        const monthlySalary = annualSalary / 12;
        const biweeklySalary = weeklyPay * 2;
        const weeklySalary = weeklyPay;
        const dailySalary = daysPerWeek > 0 ? weeklyPay / daysPerWeek : 0;
        const totalAnnualHours =
          effectiveHoursPerWeek * weeksPerYear +
          (includeOvertime ? overtimeHours * weeksPerYear : 0);
        const effectiveHourly =
          totalAnnualHours > 0 ? annualSalary / totalAnnualHours : 0;

        document.getElementById("annualSalary").textContent = formatMoney(
          annualSalary,
          symbol
        );
        document.getElementById("monthlySalary").textContent = formatMoney(
          monthlySalary,
          symbol
        );
        document.getElementById("biweeklySalary").textContent = formatMoney(
          biweeklySalary,
          symbol
        );
        document.getElementById("weeklySalary").textContent = formatMoney(
          weeklySalary,
          symbol
        );
        document.getElementById("dailySalary").textContent = formatMoney(
          dailySalary,
          symbol
        );
        document.getElementById("effectiveHourly").textContent = formatMoney(
          effectiveHourly,
          symbol
        );

        const summaryHours = document.getElementById("summaryHoursPerWeek");
        const summaryWeeks = document.getElementById("summaryWeeksPerYear");
        const summaryTotal = document.getElementById("summaryTotalHours");
        if (summaryHours)
          summaryHours.textContent = effectiveHoursPerWeek
            .toFixed(2)
            .replace(/\.00$/, "");
        if (summaryWeeks) summaryWeeks.textContent = weeksPerYear.toString();
        if (summaryTotal)
          summaryTotal.textContent = totalAnnualHours.toFixed(0);
      }

      document.addEventListener("DOMContentLoaded", function () {
        const inputs = [
          "hourlyRate",
          "hoursPerWeek",
          "weeksPerYear",
          "unpaidBreakMinutes",
          "daysPerWeek",
          "overtimeHours",
          "overtimeMultiplier",
          "includeOvertime",
        ];
        inputs.forEach((id) => {
          const el = document.getElementById(id);
          if (!el) return;
          el.addEventListener("input", recalc);
          el.addEventListener("change", recalc);
        });

        const currencySelect = document.getElementById("currency");
        const customCurrency = document.getElementById("customCurrency");
        if (currencySelect) {
          currencySelect.addEventListener("change", () => {
            if (currencySelect.value === "custom") {
              customCurrency.classList.remove("hidden");
              customCurrency.focus();
            } else {
              customCurrency.classList.add("hidden");
            }
            updateCurrencyPrefix();
            updateQuickTable();
            recalc();
          });
        }
        if (customCurrency) {
          customCurrency.addEventListener("input", () => {
            updateCurrencyPrefix();
            updateQuickTable();
            recalc();
          });
        }

        // Preset buttons
        const resetBtn = document.getElementById("resetDefaults");
        const usBtn = document.getElementById("setUSFullTime");
        const euBtn = document.getElementById("setEUFullTime");

        if (resetBtn) {
          resetBtn.addEventListener("click", () => {
            document.getElementById("hoursPerWeek").value = 40;
            document.getElementById("weeksPerYear").value = 52;
            document.getElementById("unpaidBreakMinutes").value = 0;
            document.getElementById("daysPerWeek").value = 5;
            document.getElementById("overtimeHours").value = 0;
            document.getElementById("includeOvertime").checked = false;
            recalc();
          });
        }

        if (usBtn) {
          usBtn.addEventListener("click", () => {
            document.getElementById("hoursPerWeek").value = 40;
            document.getElementById("weeksPerYear").value = 50; // assume ~2 weeks unpaid
            document.getElementById("unpaidBreakMinutes").value = 30;
            document.getElementById("daysPerWeek").value = 5;
            recalc();
          });
        }

        if (euBtn) {
          euBtn.addEventListener("click", () => {
            document.getElementById("hoursPerWeek").value = 37.5;
            document.getElementById("weeksPerYear").value = 52; // assume paid leave included
            document.getElementById("unpaidBreakMinutes").value = 0;
            document.getElementById("daysPerWeek").value = 5;
            recalc();
          });
        }

        document
          .querySelectorAll("#faq-container .faq-question")
          .forEach((btn) => {
            btn.addEventListener("click", () => {
              const answer = btn.parentElement.querySelector(".faq-answer");
              const isHidden = answer.classList.contains("hidden");
              document
                .querySelectorAll("#faq-container .faq-answer")
                .forEach((a) => a.classList.add("hidden"));
              document
                .querySelectorAll(
                  "#faq-container .faq-question span:last-child"
                )
                .forEach((icon) => (icon.textContent = "+"));
              if (isHidden) {
                answer.classList.remove("hidden");
                btn.querySelector("span:last-child").textContent = "−";
              }
            });
