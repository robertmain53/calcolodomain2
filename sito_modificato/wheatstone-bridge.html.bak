    <script>
      // Tab handling
      const tabs = {
        "tab-balance": "panel-balance",
        "tab-vout": "panel-vout",
        "tab-strain": "panel-strain",
      };
      Object.keys(tabs).forEach((tabId) => {
        document.getElementById(tabId).addEventListener("click", () => {
          // panels
          Object.values(tabs).forEach((pid) => {
            document.getElementById(pid).classList.add("hidden");
          });
          document.getElementById(tabs[tabId]).classList.remove("hidden");
          // buttons
          Object.keys(tabs).forEach((id) => {
            const btn = document.getElementById(id);
            if (id === tabId) {
              btn.classList.remove(
                "bg-white",
                "text-blue-600",
                "border",
                "border-blue-600"
              );
              btn.classList.add("bg-blue-600", "text-white");
            } else {
              btn.classList.remove("bg-blue-600", "text-white");
              btn.classList.add(
                "bg-white",
                "text-blue-600",
                "border",
                "border-blue-600"
              );
            }
          });
        });
      });

      function parsePositive(value) {
        const v = parseFloat(value);
        return isFinite(v) && v > 0 ? v : null;
      }

      // Balance / unknown resistance
      document
        .getElementById("b-solve-unknown")
        .addEventListener("click", () => {
          const r1 = parsePositive(document.getElementById("b-r1").value);
          const r2 = parsePositive(document.getElementById("b-r2").value);
          const r3 = parsePositive(document.getElementById("b-r3").value);
          let r4 = parsePositive(document.getElementById("b-r4").value);

          const results = document.getElementById("b-results");
          const unknownLine = document.getElementById("b-unknown-line");
          const balanceLine = document.getElementById("b-balance-line");
          const voutLine = document.getElementById("b-vout-line");
          results.classList.remove("hidden");
          unknownLine.textContent = "";
          balanceLine.textContent = "";
          voutLine.textContent = "";

          const values = [r1, r2, r3, r4];
          const filled = values.filter((v) => v !== null).length;
          if (filled !== 3) {
            unknownLine.textContent =
              "Enter exactly three positive resistances; leave the unknown one blank.";
            return;
          }

          if (r1 === null) {
            if (!r2 || !r3 || !r4) {
              unknownLine.textContent = "Invalid inputs.";
              return;
            }
            const val = (r2 * r3) / r4;
            document.getElementById("b-r1").value = val.toFixed(6);
            unknownLine.textContent = `Solved R1 for balance: R1 = ${val.toFixed(
              6
            )} Ω`;
          } else if (r2 === null) {
            if (!r1 || !r3 || !r4) {
              unknownLine.textContent = "Invalid inputs.";
              return;
            }
            const val = (r1 * r4) / r3;
            document.getElementById("b-r2").value = val.toFixed(6);
            unknownLine.textContent = `Solved R2 for balance: R2 = ${val.toFixed(
              6
            )} Ω`;
          } else if (r3 === null) {
            if (!r1 || !r2 || !r4) {
              unknownLine.textContent = "Invalid inputs.";
              return;
            }
            const val = (r1 * r4) / r2;
            document.getElementById("b-r3").value = val.toFixed(6);
            unknownLine.textContent = `Solved R3 for balance: R3 = ${val.toFixed(
              6
            )} Ω`;
          } else if (r4 === null) {
            if (!r1 || !r2 || !r3) {
              unknownLine.textContent = "Invalid inputs.";
              return;
            }
            const val = (r3 * r2) / r1;
            document.getElementById("b-r4").value = val.toFixed(6);
            unknownLine.textContent = `Solved R4 for balance: R4 = ${val.toFixed(
              6
            )} Ω`;
          }

          balanceLine.textContent =
            "By construction, the bridge is balanced (R1/R2 = R3/R4). Vout = 0 V.";
        });

      document
        .getElementById("b-check-balance")
        .addEventListener("click", () => {
          const r1 = parsePositive(document.getElementById("b-r1").value);
          const r2 = parsePositive(document.getElementById("b-r2").value);
          const r3 = parsePositive(document.getElementById("b-r3").value);
          const r4 = parsePositive(document.getElementById("b-r4").value);
          const vs = parseFloat(document.getElementById("b-vs").value);

          const results = document.getElementById("b-results");
          const unknownLine = document.getElementById("b-unknown-line");
          const balanceLine = document.getElementById("b-balance-line");
          const voutLine = document.getElementById("b-vout-line");
          results.classList.remove("hidden");
          unknownLine.textContent = "";
          balanceLine.textContent = "";
          voutLine.textContent = "";

          if (!r1 || !r2 || !r3 || !r4) {
            balanceLine.textContent =
              "Enter all four positive resistances to check balance.";
            return;
          }

          const ratioLeft = r1 / r2;
          const ratioRight = r3 / r4;
          const tol = 1e-6;
          if (
            Math.abs(ratioLeft - ratioRight) <=
            tol * Math.max(ratioLeft, ratioRight, 1)
          ) {
            balanceLine.textContent = `Bridge is balanced: R1/R2 = ${ratioLeft.toFixed(
              6
            )}, R3/R4 = ${ratioRight.toFixed(6)}.`;
          } else {
            balanceLine.textContent = `Bridge is NOT balanced: R1/R2 = ${ratioLeft.toFixed(
              6
            )}, R3/R4 = ${ratioRight.toFixed(6)}.`;
          }

          if (isFinite(vs)) {
            const va = vs * (r3 / (r1 + r3));
            const vb = vs * (r4 / (r2 + r4));
            const vout = va - vb;
            voutLine.textContent = `Node voltages: VA = ${va.toFixed(
              6
            )} V, VB = ${vb.toFixed(
              6
            )} V, Vout = VA − VB = ${vout.toExponential(6)} V.`;
          }
        });

      document.getElementById("b-reset").addEventListener("click", () => {
        ["b-r1", "b-r2", "b-r3", "b-r4", "b-vs"].forEach(
          (id) => (document.getElementById(id).value = "")
        );
        document.getElementById("b-results").classList.add("hidden");
      });

      // Vout calculator
      document.getElementById("v-solve").addEventListener("click", () => {
        const r1 = parsePositive(document.getElementById("v-r1").value);
        const r2 = parsePositive(document.getElementById("v-r2").value);
        const r3 = parsePositive(document.getElementById("v-r3").value);
        const r4 = parsePositive(document.getElementById("v-r4").value);
        const vs = parseFloat(document.getElementById("v-vs").value);

        const results = document.getElementById("v-results");
        const voutLine = document.getElementById("v-vout-line");
        const balanceLine = document.getElementById("v-balance-line");
        results.classList.remove("hidden");
        voutLine.textContent = "";
        balanceLine.textContent = "";

        if (!r1 || !r2 || !r3 || !r4 || !isFinite(vs)) {
          voutLine.textContent =
            "Enter all four positive resistances and a valid supply voltage.";
          return;
        }

        const va = vs * (r3 / (r1 + r3));
        const vb = vs * (r4 / (r2 + r4));
        const vout = va - vb;

        voutLine.textContent = `VA = ${va.toFixed(6)} V, VB = ${vb.toFixed(
          6
        )} V, Vout = VA − VB = ${vout.toExponential(6)} V.`;

        const ratioLeft = r1 / r2;
        const ratioRight = r3 / r4;
        const tol = 1e-6;
        if (
          Math.abs(ratioLeft - ratioRight) <=
          tol * Math.max(ratioLeft, ratioRight, 1)
        ) {
          balanceLine.textContent = `Bridge is balanced (R1/R2 ≈ R3/R4). Any non‑zero Vout is due to rounding.`;
        } else {
          balanceLine.textContent = `Bridge is unbalanced: R1/R2 = ${ratioLeft.toFixed(
            6
          )}, R3/R4 = ${ratioRight.toFixed(6)}.`;
        }
      });

      document.getElementById("v-reset").addEventListener("click", () => {
        ["v-r1", "v-r2", "v-r3", "v-r4", "v-vs"].forEach(
          (id) => (document.getElementById(id).value = "")
        );
        document.getElementById("v-results").classList.add("hidden");
      });

      // Strain gauge bridge
      document.getElementById("sg-solve").addEventListener("click", () => {
        const r = parsePositive(document.getElementById("sg-r").value);
        const gf = parseFloat(document.getElementById("sg-gf").value);
        const strainMicro = parseFloat(
          document.getElementById("sg-strain").value
        );
        const vs = parseFloat(document.getElementById("sg-vs").value);
        const config = document.querySelector(
          'input[name="sg-config"]:checked'
        ).value;

        const results = document.getElementById("sg-results");
        const deltaRLine = document.getElementById("sg-deltaR-line");
        const voutLine = document.getElementById("sg-vout-line");
        const sensLine = document.getElementById("sg-sens-line");
        results.classList.remove("hidden");
        deltaRLine.textContent = "";
        voutLine.textContent = "";
        sensLine.textContent = "";

        if (!r || !isFinite(gf) || !isFinite(strainMicro) || !isFinite(vs)) {
          deltaRLine.textContent =
            "Enter nominal resistance, gauge factor, strain, and supply voltage.";
          return;
        }

        const strain = strainMicro * 1e-6;
        const deltaR = gf * r * strain;

        let k;
        if (config === "quarter") k = 0.25;
        else if (config === "half") k = 0.5;
        else k = 1.0;

        const vout = k * gf * strain * vs;
        const sensPerStrain = k * gf * vs; // V per unit strain
        const sens_mV_per_V_per_1000ue = k * gf * 1e3; // mV/V at 1000 µε

        deltaRLine.textContent = `Gauge resistance change: ΔR = ${deltaR.toExponential(
          6
        )} Ω (for ε = ${strainMicro} µε).`;
        voutLine.textContent = `Approximate bridge output: Vout ≈ ${vout.toExponential(
          6
        )} V.`;
        sensLine.textContent = `Sensitivity: ${sensPerStrain.toExponential(
          6
        )} V per unit strain, or ${sens_mV_per_V_per_1000ue.toFixed(
          3
        )} mV/V at 1000 µε.`;
      });

      document.getElementById("sg-reset").addEventListener("click", () => {
        document.getElementById("sg-r").value = "350";
        document.getElementById("sg-gf").value = "2";
        document.getElementById("sg-strain").value = "";
        document.getElementById("sg-vs").value = "5";
        document.querySelector(
          'input[name="sg-config"][value="quarter"]'
        ).checked = true;
        document.getElementById("sg-results").classList.add("hidden");
      });

      // Mobile menu script (simple)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", () => {
          const menu = document.getElementById("mobile-menu");
          menu.classList.toggle("hidden");
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
