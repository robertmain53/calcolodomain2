<script>
  // Utility: safely parse date + optional time in local time zone
  function parseLocalDateTime(dateStr, timeStr) {
    if (!dateStr) return null;
    const [year, month, day] = dateStr.split('-').map(Number);
    if (!year || !month || !day) return null;
    let hours = 0, minutes = 0;
    if (timeStr) {
      const parts = timeStr.split(':').map(Number);
      if (parts.length >= 2) {
        hours = parts[0] || 0;
        minutes = parts[1] || 0;
      }
    }
    return new Date(year, month - 1, day, hours, minutes, 0, 0);
  }

  function diffInDays(start, end, includeEnd) {
    const msPerDay = 24 * 60 * 60 * 1000;
    const startUTC = Date.UTC(start.getFullYear(), start.getMonth(), start.getDate());
    const endUTC = Date.UTC(end.getFullYear(), end.getMonth(), end.getDate());
    let days = Math.round((endUTC - startUTC) / msPerDay);
    if (includeEnd) days += 1;
    return days;
  }

  function diffYMD(start, end) {
    // Ensure start <= end
    let s = new Date(start.getFullYear(), start.getMonth(), start.getDate());
    let e = new Date(end.getFullYear(), end.getMonth(), end.getDate());
    if (s > e) { const tmp = s; s = e; e = tmp; }

    let years = e.getFullYear() - s.getFullYear();
    let months = e.getMonth() - s.getMonth();
    let days = e.getDate() - s.getDate();

    if (days < 0) {
      months -= 1;
      const prevMonth = new Date(e.getFullYear(), e.getMonth(), 0);
      days += prevMonth.getDate();
    }
    if (months < 0) {
      years -= 1;
      months += 12;
    }
    return { years, months, days };
  }

  function countSelectedWeekdays(start, end, includeEnd, allowedDays) {
    if (!allowedDays || allowedDays.size === 0) return 0;
    let s = new Date(start.getFullYear(), start.getMonth(), start.getDate());
    let e = new Date(end.getFullYear(), end.getMonth(), end.getDate());
    if (s > e) { const tmp = s; s = e; e = tmp; }

    let count = 0;
    while (s <= e) {
      const isEnd = s.getTime() === e.getTime();
      if (!isEnd || includeEnd) {
        if (allowedDays.has(s.getDay())) count++;
      }
      s.setDate(s.getDate() + 1);
    }
    return count;
  }

  function formatNumber(n) {
    return n.toLocaleString(undefined, { maximumFractionDigits: 4 });
  }

  document.addEventListener('DOMContentLoaded', function () {
    const startDateEl = document.getElementById('start-date');
    const endDateEl = document.getElementById('end-date');
    const startTimeEl = document.getElementById('start-time');
    const endTimeEl = document.getElementById('end-time');
    const includeEndEl = document.getElementById('include-end');
    const countModeEl = document.getElementById('count-mode');
    const customWeekdaysRow = document.getElementById('custom-weekdays-row');
    const weekdayToggles = document.querySelectorAll('.weekday-toggle');
    const calculateBtn = document.getElementById('calculate-btn');
    const swapBtn = document.getElementById('swap-btn');
    const todayRangeBtn = document.getElementById('today-range-btn');
    const errorMsgEl = document.getElementById('error-msg');

    const totalDaysEl = document.getElementById('total-days');
    const totalWeeksEl = document.getElementById('total-weeks');
    const totalMonthsEl = document.getElementById('total-months');
    const totalYearsEl = document.getElementById('total-years');
    const ymdBreakdownEl = document.getElementById('ymd-breakdown');
    const weekdayCountEl = document.getElementById('weekday-count');
    const weekdayModeLabelEl = document.getElementById('weekday-mode-label');
    const hoursDiffEl = document.getElementById('hours-diff');
    const minutesDiffEl = document.getElementById('minutes-diff');
    const inclusiveBadgeEl = document.getElementById('inclusive-badge');

    // Initialize with today and today+1
    const today = new Date();
    const tomorrow = new Date(today.getFullYear(), today.getMonth(), today.getDate() + 1);
    const toISODate = d => d.toISOString().slice(0, 10);
    startDateEl.value = toISODate(today);
    endDateEl.value = toISODate(tomorrow);

    countModeEl.addEventListener('change', () => {
      if (countModeEl.value === 'custom') {
        customWeekdaysRow.classList.remove('hidden');
      } else {
        customWeekdaysRow.classList.add('hidden');
      }
    });

    swapBtn.addEventListener('click', () => {
      const sd = startDateEl.value;
      const ed = endDateEl.value;
      const st = startTimeEl.value;
      const et = endTimeEl.value;
      startDateEl.value = ed;
      endDateEl.value = sd;
      startTimeEl.value = et;
      endTimeEl.value = st;
      errorMsgEl.textContent = '';
    });

    todayRangeBtn.addEventListener('click', () => {
      const todayLocal = new Date();
      const todayISO = toISODate(todayLocal);
      startDateEl.value = todayISO;
      endDateEl.value = todayISO;
      startTimeEl.value = '';
      endTimeEl.value = '';
      includeEndEl.checked = true;
      errorMsgEl.textContent = '';
    });

    function getAllowedDays() {
      const mode = countModeEl.value;
      if (mode === 'all') return null;
      if (mode === 'weekdays') {
        return new Set([1, 2, 3, 4, 5]);
      }
      const set = new Set();
      weekdayToggles.forEach(cb => {
        if (cb.checked) set.add(Number(cb.dataset.day));
      });
      return set;
    }

    function updateInclusiveBadge() {
      if (includeEndEl.checked) {
        inclusiveBadgeEl.textContent = 'Inclusive';
        inclusiveBadgeEl.classList.remove('bg-gray-100', 'text-gray-700');
        inclusiveBadgeEl.classList.add('bg-blue-100', 'text-blue-700');
      } else {
        inclusiveBadgeEl.textContent = 'Exclusive';
        inclusiveBadgeEl.classList.remove('bg-blue-100', 'text-blue-700');
        inclusiveBadgeEl.classList.add('bg-gray-100', 'text-gray-700');
      }
    }

    includeEndEl.addEventListener('change', updateInclusiveBadge);
    updateInclusiveBadge();

    function calculate() {
      errorMsgEl.textContent = '';

      const start = parseLocalDateTime(startDateEl.value, startTimeEl.value);
      const end = parseLocalDateTime(endDateEl.value, endTimeEl.value);
      if (!start || !end) {
        errorMsgEl.textContent = 'Please enter both start and end dates.';
        return;
      }

      // Ensure chronological order for calculations
      let s = start;
      let e = end;
      if (s > e) {
        const tmp = s; s = e; e = tmp;
      }

      const includeEnd = includeEndEl.checked;

      // Total days
      const days = diffInDays(s, e, includeEnd);
      if (days < 0) {
        errorMsgEl.textContent = 'End date must be on or after start date.';
        return;
      }

      totalDaysEl.textContent = formatNumber(days);
      totalWeeksEl.textContent = formatNumber(days / 7);
      totalMonthsEl.textContent = formatNumber(days / 30.4375);
      totalYearsEl.textContent = formatNumber(days / 365.25);

      // Y-M-D breakdown (always exclusive of end for the breakdown)
      const ymd = diffYMD(s, e);
      ymdBreakdownEl.textContent = `${ymd.years} year${ymd.years === 1 ? '' : 's'}, ${ymd.months} month${ymd.months === 1 ? '' : 's'}, ${ymd.days} day${ymd.days === 1 ? '' : 's'}`;

      // Weekday counting
      const allowedDays = getAllowedDays();
      if (!allowedDays) {
        weekdayCountEl.textContent = formatNumber(days);
        weekdayModeLabelEl.textContent = 'all days';
      } else {
        const weekdayCount = countSelectedWeekdays(s, e, includeEnd, allowedDays);
        weekdayCountEl.textContent = formatNumber(weekdayCount);
        if (countModeEl.value === 'weekdays') {
          weekdayModeLabelEl.textContent = 'Mon–Fri only';
        } else {
          weekdayModeLabelEl.textContent = 'custom weekdays';
        }
      }

      // Hours and minutes difference (exact, using full date-time)
      const msDiff = e.getTime() - s.getTime();
      const hours = msDiff / (1000 * 60 * 60);
      const minutes = msDiff / (1000 * 60);
      hoursDiffEl.textContent = formatNumber(hours);
      minutesDiffEl.textContent = formatNumber(minutes);
    }

    calculateBtn.addEventListener('click', calculate);

    // Auto-calc when dates change (for responsiveness)
    [startDateEl, endDateEl, startTimeEl, endTimeEl, countModeEl].forEach(el => {
      el.addEventListener('change', () => {
        if (startDateEl.value && endDateEl.value) {
          calculate();
        }
      });
    });
    weekdayToggles.forEach(cb => cb.addEventListener('change', () => {
      if (countModeEl.value === 'custom' && startDateEl.value && endDateEl.value) {
        calculate();
      }
    }));

    // Initial calculation
    calculate();
  });

  window.MathJax = {
    tex: { inlineMath: [['\\(','\\)'], ['$', '$']], displayMath: [['$','$'], ['\\[','\\]']] },
    svg: { fontCache: 'global' }
  };
</script>
