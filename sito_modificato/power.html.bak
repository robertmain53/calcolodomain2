    <script>
      // Tab switching
      const modeTabs = document.querySelectorAll(".mode-tab");
      const modePanels = document.querySelectorAll(".mode-panel");
      modeTabs.forEach((tab) => {
        tab.addEventListener("click", () => {
          const mode = tab.dataset.mode;
          modeTabs.forEach((t) => {
            t.classList.remove("bg-white", "text-blue-600", "border-gray-200");
            t.classList.add("text-gray-600", "border-transparent");
          });
          tab.classList.add("bg-white", "text-blue-600", "border-gray-200");
          tab.classList.remove("text-gray-600", "border-transparent");
          modePanels.forEach((panel) => {
            panel.classList.toggle("hidden", panel.id !== "mode-" + mode);
          });
        });
      });

      // Helper conversions
      function toBase(value, unit, map) {
        const factor = map[unit] ?? 1;
        return value * factor;
      }
      function fromBase(value, unit, map) {
        const factor = map[unit] ?? 1;
        return value / factor;
      }
      function formatPowerW(watts) {
        if (!isFinite(watts)) return "—";
        const abs = Math.abs(watts);
        if (abs >= 1e6) return (watts / 1e6).toFixed(3) + " MW";
        if (abs >= 1e3) return (watts / 1e3).toFixed(3) + " kW";
        if (abs >= 1) return watts.toFixed(3) + " W";
        return watts.toExponential(3) + " W";
      }
      function formatHp(watts) {
        const hp = watts / 745.699872;
        return hp.toFixed(3) + " hp";
      }

      // ELECTRICAL
      const elecKnown = document.getElementById("elec-known");
      const elecPhase = document.getElementById("elec-phase");
      const elecV = document.getElementById("elec-v");
      const elecVUnit = document.getElementById("elec-v-unit");
      const elecI = document.getElementById("elec-i");
      const elecIUnit = document.getElementById("elec-i-unit");
      const elecR = document.getElementById("elec-r");
      const elecRUnit = document.getElementById("elec-r-unit");
      const elecPf = document.getElementById("elec-pf");
      const elecCalc = document.getElementById("elec-calc");
      const elecClear = document.getElementById("elec-clear");
      const elecResult = document.getElementById("elec-result");
      const elecPMain = document.getElementById("elec-p-main");
      const elecPAlt = document.getElementById("elec-p-alt");
      const elecDetails = document.getElementById("elec-details");

      elecCalc.addEventListener("click", () => {
        const known = elecKnown.value;
        const phase = elecPhase.value;
        const pf = parseFloat(elecPf.value || "1");
        let V = parseFloat(elecV.value);
        let I = parseFloat(elecI.value);
        let R = parseFloat(elecR.value);

        if (known === "vi" && (isNaN(V) || isNaN(I))) {
          alert("Please enter both voltage and current.");
          return;
        }
        if (known === "ir" && (isNaN(I) || isNaN(R))) {
          alert("Please enter both current and resistance.");
          return;
        }
        if (known === "vr" && (isNaN(V) || isNaN(R))) {
          alert("Please enter both voltage and resistance.");
          return;
        }

        // Convert units
        if (!isNaN(V)) V = toBase(V, elecVUnit.value, { V: 1, kV: 1000 });
        if (!isNaN(I)) I = toBase(I, elecIUnit.value, { A: 1, mA: 1e-3 });
        if (!isNaN(R)) R = toBase(R, elecRUnit.value, { ohm: 1, kOhm: 1e3 });

        // Derive missing quantity
        if (known === "ir") {
          V = I * R;
        } else if (known === "vr") {
          I = V / R;
        } else if (known === "vi") {
          R = V / I;
        }

        let pBase = V * I; // single-phase or DC
        let multiplier = 1;
        if (phase === "3p-line") {
          multiplier = Math.sqrt(3);
        } else if (phase === "3p-phase") {
          multiplier = 3;
        }
        const pApparent = pBase * multiplier; // VA
        const pReal = pApparent * pf; // W

        elecPMain.textContent = "Real power: " + formatPowerW(pReal);
        elecPAlt.textContent =
          "Apparent power: " + (pApparent / 1000).toFixed(3) + " kVA";
        elecDetails.textContent = `Voltage ≈ ${V.toFixed(
          3
        )} V, Current ≈ ${I.toFixed(3)} A, Resistance ≈ ${R.toFixed(
          3
        )} Ω, power factor = ${pf.toFixed(3)}.`;
        elecResult.classList.remove("hidden");
      });

      elecClear.addEventListener("click", () => {
        elecV.value = "";
        elecI.value = "";
        elecR.value = "";
        elecPf.value = "";
        elecResult.classList.add("hidden");
      });

      // MECHANICAL
      const mechKnown = document.getElementById("mech-known");
      const mechTorqueFields = document.getElementById(
        "mech-torque-rpm-fields"
      );
      const mechForceFields = document.getElementById("mech-force-vel-fields");
      const mechTorque = document.getElementById("mech-torque");
      const mechTorqueUnit = document.getElementById("mech-torque-unit");
      const mechSpeed = document.getElementById("mech-speed");
      const mechSpeedUnit = document.getElementById("mech-speed-unit");
      const mechForce = document.getElementById("mech-force");
      const mechForceUnit = document.getElementById("mech-force-unit");
      const mechVel = document.getElementById("mech-vel");
      const mechVelUnit = document.getElementById("mech-vel-unit");
      const mechCalc = document.getElementById("mech-calc");
      const mechClear = document.getElementById("mech-clear");
      const mechResult = document.getElementById("mech-result");
      const mechPMain = document.getElementById("mech-p-main");
      const mechPAlt = document.getElementById("mech-p-alt");
      const mechDetails = document.getElementById("mech-details");

      mechKnown.addEventListener("change", () => {
        const v = mechKnown.value;
        mechTorqueFields.classList.toggle("hidden", v !== "torque-rpm");
        mechForceFields.classList.toggle("hidden", v !== "force-velocity");
        mechResult.classList.add("hidden");
      });

      mechCalc.addEventListener("click", () => {
        const mode = mechKnown.value;
        let P = 0;
        if (mode === "torque-rpm") {
          let tau = parseFloat(mechTorque.value);
          let n = parseFloat(mechSpeed.value);
          if (isNaN(tau) || isNaN(n)) {
            alert("Please enter both torque and speed.");
            return;
          }
          // Convert torque
          tau = toBase(tau, mechTorqueUnit.value, {
            Nm: 1,
            ftlb: 1.3558179483314004,
          });
          // Convert speed to rad/s
          let omega;
          if (mechSpeedUnit.value === "rpm") {
            omega = (2 * Math.PI * n) / 60;
          } else {
            omega = n;
          }
          P = tau * omega;
          mechPMain.textContent = "Mechanical power: " + formatPowerW(P);
          mechPAlt.textContent = "Equivalent: " + formatHp(P);
          mechDetails.textContent = `Torque ≈ ${tau.toFixed(
            3
          )} N·m, angular speed ≈ ${omega.toFixed(3)} rad/s.`;
        } else {
          let F = parseFloat(mechForce.value);
          let v = parseFloat(mechVel.value);
          if (isNaN(F) || isNaN(v)) {
            alert("Please enter both force and velocity.");
            return;
          }
          F = toBase(F, mechForceUnit.value, { N: 1, kN: 1000 });
          if (mechVelUnit.value === "km_h") {
            v = (v * 1000) / 3600;
          }
          P = F * v;
          mechPMain.textContent = "Mechanical power: " + formatPowerW(P);
          mechPAlt.textContent = "Equivalent: " + formatHp(P);
          mechDetails.textContent = `Force ≈ ${F.toFixed(
            3
          )} N, velocity ≈ ${v.toFixed(3)} m/s.`;
        }
        mechResult.classList.remove("hidden");
      });

      mechClear.addEventListener("click", () => {
        mechTorque.value = "";
        mechSpeed.value = "";
        mechForce.value = "";
        mechVel.value = "";
        mechResult.classList.add("hidden");
      });

      // GENERAL
      const genKnown = document.getElementById("gen-known");
      const genWorkFields = document.getElementById("gen-work-fields");
      const genEnergyFields = document.getElementById("gen-energy-fields");
      const genWork = document.getElementById("gen-work");
      const genWorkUnit = document.getElementById("gen-work-unit");
      const genTime1 = document.getElementById("gen-time1");
      const genTime1Unit = document.getElementById("gen-time1-unit");
      const genEnergy = document.getElementById("gen-energy");
      const genEnergyUnit = document.getElementById("gen-energy-unit");
      const genTime2 = document.getElementById("gen-time2");
      const genTime2Unit = document.getElementById("gen-time2-unit");
      const genCalc = document.getElementById("gen-calc");
      const genClear = document.getElementById("gen-clear");
      const genResult = document.getElementById("gen-result");
      const genPMain = document.getElementById("gen-p-main");
      const genPAlt = document.getElementById("gen-p-alt");
      const genDetails = document.getElementById("gen-details");

      genKnown.addEventListener("change", () => {
        const v = genKnown.value;
        genWorkFields.classList.toggle("hidden", v !== "work-time");
        genEnergyFields.classList.toggle("hidden", v !== "energy-time");
        genResult.classList.add("hidden");
      });

      genCalc.addEventListener("click", () => {
        const mode = genKnown.value;
        let P = 0;
        if (mode === "work-time") {
          let W = parseFloat(genWork.value);
          let t = parseFloat(genTime1.value);
          if (isNaN(W) || isNaN(t)) {
            alert("Please enter both work and time.");
            return;
          }
          W = toBase(W, genWorkUnit.value, { J: 1, kJ: 1000 });
          t = toBase(t, genTime1Unit.value, { s: 1, min: 60, h: 3600 });
          P = W / t;
          genPMain.textContent = "Average power: " + formatPowerW(P);
          genPAlt.textContent = "Equivalent: " + formatHp(P);
          genDetails.textContent = `Work ≈ ${W.toFixed(
            3
          )} J, time ≈ ${t.toFixed(3)} s.`;
        } else {
          let E = parseFloat(genEnergy.value);
          let t = parseFloat(genTime2.value);
          if (isNaN(E) || isNaN(t)) {
            alert("Please enter both energy and time.");
            return;
          }
          // Convert energy to joules
          const energyMap = {
            J: 1,
            kJ: 1000,
            Wh: 3600,
            kWh: 3.6e6,
          };
          E = toBase(E, genEnergyUnit.value, energyMap);
          t = toBase(t, genTime2Unit.value, { s: 1, min: 60, h: 3600 });
          P = E / t;
          genPMain.textContent = "Average power: " + formatPowerW(P);
          genPAlt.textContent = "Equivalent: " + formatHp(P);
          genDetails.textContent = `Energy ≈ ${E.toFixed(
            3
          )} J, time ≈ ${t.toFixed(3)} s.`;
        }
        genResult.classList.remove("hidden");
      });

      genClear.addEventListener("click", () => {
        genWork.value = "";
        genTime1.value = "";
        genEnergy.value = "";
        genTime2.value = "";
        genResult.classList.add("hidden");
      });

      // Mobile menu script (simple)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", () => {
          const menu = document.getElementById("mobile-menu");
          const expanded = menu.classList.toggle("hidden");
          document
            .getElementById("mobile-menu-toggle")
            .setAttribute("aria-expanded", (!expanded).toString());
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
