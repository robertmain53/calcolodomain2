    <script>
      // Mobile menu toggle
      document
        .getElementById("mobile-menu-toggle")
        ?.addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          if (!menu) return;
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          this.setAttribute("aria-expanded", String(isHidden));
        });

      function formatOhms(value) {
        if (!isFinite(value)) return "–";
        if (value >= 1) return value.toFixed(2);
        if (value >= 0.1) return value.toFixed(3);
        return value.toExponential(2);
      }

      function updateVisibility() {
        const systemType = document.getElementById("systemType").value;
        const deviceType = document.getElementById("deviceType").value;

        const tnSettings = document.getElementById("tn-settings");
        const ttSettings = document.getElementById("tt-settings");
        const mcbSettings = document.getElementById("mcb-settings");
        const rcdSettings = document.getElementById("rcd-settings");
        const customIaSettings = document.getElementById("custom-ia-settings");

        if (systemType === "tn") {
          tnSettings.classList.remove("hidden");
          ttSettings.classList.add("hidden");
        } else {
          tnSettings.classList.add("hidden");
          ttSettings.classList.remove("hidden");
        }

        if (deviceType.startsWith("mcb")) {
          mcbSettings.classList.remove("hidden");
          rcdSettings.classList.add("hidden");
          customIaSettings.classList.add("hidden");
        } else if (deviceType === "rcd") {
          mcbSettings.classList.add("hidden");
          rcdSettings.classList.remove("hidden");
          customIaSettings.classList.add("hidden");
        } else if (deviceType === "custom") {
          mcbSettings.classList.add("hidden");
          rcdSettings.classList.add("hidden");
          customIaSettings.classList.remove("hidden");
        }
      }

      function calculateZsLimit() {
        const systemType = document.getElementById("systemType").value;
        const zsLimitEl = document.getElementById("zsLimit");
        const noteEl = document.getElementById("zsLimitNote");

        let zsMax = NaN;
        let note = "";

        if (systemType === "tn") {
          const u0 = parseFloat(document.getElementById("u0").value) || 230;
          const deviceType = document.getElementById("deviceType").value;

          if (deviceType.startsWith("mcb")) {
            const In = parseFloat(document.getElementById("in").value) || 32;
            let multiple;
            if (deviceType === "mcb-b") multiple = 5;
            else if (deviceType === "mcb-c") multiple = 10;
            else multiple = 20;

            const Ia = In * multiple;
            zsMax = u0 / Ia;
            note = `Assumes instantaneous trip at ${multiple.toFixed(
              0
            )} × In (conservative for Type ${deviceType
              .slice(-1)
              .toUpperCase()} MCB).`;
          } else if (deviceType === "rcd") {
            const rcdIn =
              parseFloat(document.getElementById("rcdIn").value) || 30;
            const touchV =
              parseFloat(document.getElementById("rcdTouchVoltage").value) ||
              50;
            const Idelta = rcdIn / 1000; // mA to A
            zsMax = touchV / Idelta;
            note = `RCD-based disconnection: Zs,max = ${touchV} V / ${Idelta.toFixed(
              3
            )} A (IΔn) for touch voltage limit.`;
          } else if (deviceType === "custom") {
            const u0 = parseFloat(document.getElementById("u0").value) || 230;
            const Ia = parseFloat(document.getElementById("customIa").value);
            if (Ia > 0) {
              zsMax = u0 / Ia;
              note = `Custom Ia: Zs,max = U0 / Ia = ${u0} V / ${Ia.toFixed(
                1
              )} A.`;
            }
          }
        } else if (systemType === "tt") {
          const ra = parseFloat(document.getElementById("ra").value);
          const rcdIn =
            parseFloat(document.getElementById("ttRcdIn").value) || 30;
          const Idelta = rcdIn / 1000;
          const touchLimit = 50;
          zsMax = touchLimit / Idelta;
          note = `TT system: RA × IΔn ≤ 50 V → RA,max = 50 V / ${Idelta.toFixed(
            3
          )} A = ${formatOhms(zsMax)} Ω.`;
        }

        if (!isFinite(zsMax) || zsMax <= 0) {
          zsLimitEl.textContent = "–";
          noteEl.textContent = "";
        } else {
          zsLimitEl.textContent = formatOhms(zsMax);
          noteEl.textContent = note;
        }
      }

      function checkMeasuredZs() {
        const systemType = document.getElementById("systemType").value;
        const measured = parseFloat(
          document.getElementById("measuredZs").value
        );
        const applyFactor = document.getElementById("applyFactor").checked;
        const zsLimitText = document.getElementById("zsLimit").textContent;
        const resultEl = document.getElementById("zsCheckResult");

        if (!zsLimitText || zsLimitText === "–" || !isFinite(measured)) {
          resultEl.textContent =
            "Enter a measured value and ensure a valid Zs limit is calculated.";
          resultEl.className = "mt-3 text-sm font-medium text-yellow-700";
          return;
        }

        const zsMax = parseFloat(zsLimitText);
        const designLimit = applyFactor ? 0.8 * zsMax : zsMax;
        const passes = measured <= designLimit + 1e-9;

        const label = systemType === "tt" ? "RA" : "Zs";
        const factorText = applyFactor
          ? ` (with 0.8 factor: limit = ${formatOhms(designLimit)} Ω)`
          : "";

        if (passes) {
          resultEl.textContent = `${label} = ${formatOhms(
            measured
          )} Ω → PASS. Within permitted limit of ${formatOhms(
            zsMax
          )} Ω${factorText}.`;
          resultEl.className = "mt-3 text-sm font-medium text-green-700";
        } else {
          resultEl.textContent = `${label} = ${formatOhms(
            measured
          )} Ω → FAIL. Exceeds permitted limit of ${formatOhms(
            zsMax
          )} Ω${factorText}.`;
          resultEl.className = "mt-3 text-sm font-medium text-red-700";
        }
      }

      document.addEventListener("DOMContentLoaded", () => {
        updateVisibility();
        calculateZsLimit();

        [
          "systemType",
          "deviceType",
          "u0",
          "in",
          "disconnectionTime",
          "rcdIn",
          "rcdTouchVoltage",
          "ra",
          "ttRcdIn",
          "customIa",
        ].forEach((id) => {
          const el = document.getElementById(id);
          if (el) {
            el.addEventListener("input", () => {
              updateVisibility();
              calculateZsLimit();
            });
            el.addEventListener("change", () => {
              updateVisibility();
              calculateZsLimit();
            });
          }
        });

        document
          .getElementById("checkZsBtn")
          ?.addEventListener("click", checkMeasuredZs);
      });
    </script>

