          <div class="prose">
            <h2>Formulas</h2>
            <div class="formula-box">
              <p><strong>Simple moving average (SMA):</strong></p>
              <p>\( \text{SMA}_t = \dfrac{X_t + X_{t-1} + \dots + X_{t-n+1}}{n} \)</p>
              <hr />
              <p><strong>Exponential moving average (EMA):</strong></p>
              <p>\( \alpha = \dfrac{2}{n+1} \quad\Rightarrow\quad \text{EMA}_t = \alpha X_t + (1 - \alpha)\text{EMA}_{t-1} \)</p>
              <p>with initialization \( \text{EMA}_1 = X_1 \).</p>
            </div>

            <h2>When to use SMA vs EMA</h2>
            <ul>
              <li><strong>SMA</strong>: easy to interpret, good for smoothing.</li>
              <li><strong>EMA</strong>: reacts faster to recent moves (used a lot in trading, e.g. 12/26 EMA).</li>
            </ul>

            <h2>Common periods</h2>
            <p>Finance: 5, 10, 20, 50, 100, 200. Operations/time series: 3, 4, 6, 12 depending on seasonality.</p>
          </div>

          <script>
            (function() {
              const tabSMA = document.getElementById('tab-sma');
              const tabEMA = document.getElementById('tab-ema');
              const panelSMA = document.getElementById('panel-sma');
              const panelEMA = document.getElementById('panel-ema');

              const dataInput = document.getElementById('data-input');
              const errBox = document.getElementById('error-box');
              const errMsg = document.getElementById('error-msg');

              const smaPeriod = document.getElementById('sma-period');
              const btnSMA = document.getElementById('btn-sma');

              const emaPeriod = document.getElementById('ema-period');
              const emaAlpha = document.getElementById('ema-alpha');
              const btnEMA = document.getElementById('btn-ema');

              const outLatest = document.getElementById('out-latest');
              const outLast = document.getElementById('out-last');
              const outDiff = document.getElementById('out-diff');
              const outMethod = document.getElementById('out-method');

              const tableBody = document.querySelector('#ma-table tbody');
              const plot = document.getElementById('ma-plot');

              function showError(msg) {
                errMsg.textContent = msg;
                errBox.classList.remove('hidden');
              }
              function clearError() {
                errBox.classList.add('hidden');
              }
              function parseSeries() {
                const lines = dataInput.value.split(/\r?\n/).map(l => l.trim()).filter(l => l.length > 0);
                const arr = lines.map(Number).filter(v => !isNaN(v));
                return arr;
              }
              function renderTable(values, ma) {
                tableBody.innerHTML = "";
                for (let i = 0; i < values.length; i++) {
                  const tr = document.createElement('tr');
                  const v = values[i];
                  const m = ma[i];
                  const diff = (v != null && m != null) ? (v - m) : null;
                  const cells = [
                    i+1,
                    v != null ? v.toFixed(4).replace(/\.?0+$/,'') : "",
                    m != null ? m.toFixed(4).replace(/\.?0+$/,'') : "",
                    diff != null ? diff.toFixed(4).replace(/\.?0+$/,'') : ""
                  ];
                  cells.forEach((c, idx) => {
                    const td = document.createElement('td');
                    td.textContent = c;
                    tr.appendChild(td);
                  });
                  tableBody.appendChild(tr);
                }
              }
              function drawPlot(values, ma) {
                plot.innerHTML = '<rect x="0" y="0" width="520" height="180" fill="white" />';
                const width = 520, height = 180;
                const padL = 35, padR = 10, padT = 15, padB = 25;
                const maxPoints = 30;
                const n = Math.min(values.length, maxPoints);
                if (n === 0) return;

                // compute min/max
                const allVals = values.slice(0, n).concat(ma.filter((v, i) => i < n && v != null));
                const maxV = Math.max.apply(null, allVals);
                const minV = Math.min.apply(null, allVals);

                function xPos(i) {
                  if (n === 1) return (width - padL - padR)/2 + padL;
                  return padL + (i/(n-1))*(width - padL - padR);
                }
                function yPos(v) {
                  if (maxV === minV) return height/2;
                  return height - padB - ( (v - minV)/(maxV - minV) )*(height - padT - padB);
                }

                // x axis
                const axis = document.createElementNS("http://www.w3.org/2000/svg", "line");
                axis.setAttribute("x1", padL);
                axis.setAttribute("y1", height - padB);
                axis.setAttribute("x2", width - padR);
                axis.setAttribute("y2", height - padB);
                axis.setAttribute("stroke", "#d1d5db");
                axis.setAttribute("stroke-width", "1");
                plot.appendChild(axis);

                // values line (blue)
                let d1 = "";
                for (let i = 0; i < n; i++) {
                  const x = xPos(i);
                  const y = yPos(values[i]);
                  d1 += (i === 0 ? "M" : " L") + x + " " + y;
                }
                const path1 = document.createElementNS("http://www.w3.org/2000/svg", "path");
                path1.setAttribute("d", d1);
                path1.setAttribute("fill", "none");
                path1.setAttribute("stroke", "#2563eb");
                path1.setAttribute("stroke-width", "2");
                plot.appendChild(path1);

                // ma line (orange)
                let d2 = "";
                for (let i = 0; i < n; i++) {
                  if (ma[i] == null) continue;
                  const x = xPos(i);
                  const y = yPos(ma[i]);
                  d2 += (d2 === "" ? "M" : " L") + x + " " + y;
                }
                if (d2 !== "") {
                  const path2 = document.createElementNS("http://www.w3.org/2000/svg", "path");
                  path2.setAttribute("d", d2);
                  path2.setAttribute("fill", "none");
                  path2.setAttribute("stroke", "#f97316");
                  path2.setAttribute("stroke-width", "2");
                  plot.appendChild(path2);
                }
              }

              function calcSMA() {
                clearError();
                const values = parseSeries();
                const n = parseInt(smaPeriod.value, 10);
                if (values.length < n || values.length < 2) {
                  showError('Need at least ' + n + ' data points for this SMA.');
                  return;
                }
                const ma = new Array(values.length).fill(null);
                for (let i = n-1; i < values.length; i++) {
                  let sum = 0;
                  for (let k = 0; k < n; k++) sum += values[i - k];
                  ma[i] = sum / n;
                }
                const lastVal = values[values.length - 1];
                const lastMA = ma[ma.length - 1];
                outLast.textContent = lastVal.toFixed(4).replace(/\.?0+$/,'');
                outLatest.textContent = lastMA != null ? lastMA.toFixed(4).replace(/\.?0+$/,'') : '—';
                outDiff.textContent = (lastMA != null ? (lastVal - lastMA).toFixed(4).replace(/\.?0+$/,'') : '—');
                outMethod.textContent = 'SMA (n=' + n + ')';

                renderTable(values, ma);
                drawPlot(values, ma);
              }

              function calcEMA() {
                clearError();
                const values = parseSeries();
                const n = parseInt(emaPeriod.value, 10);
                if (values.length < 2) {
                  showError('Need at least 2 data points for EMA.');
                  return;
                }
                let alpha = parseFloat(emaAlpha.value);
                if (isNaN(alpha) || alpha <= 0 || alpha > 1) {
                  alpha = 2 / (n + 1);
                }
                const ema = new Array(values.length).fill(null);
                ema[0] = values[0];
                for (let i = 1; i < values.length; i++) {
                  ema[i] = alpha * values[i] + (1 - alpha) * ema[i - 1];
                }
                const lastVal = values[values.length - 1];
                const lastEMA = ema[ema.length - 1];
                outLast.textContent = lastVal.toFixed(4).replace(/\.?0+$/,'');
                outLatest.textContent = lastEMA.toFixed(4).replace(/\.?0+$/,'');
                outDiff.textContent = (lastVal - lastEMA).toFixed(4).replace(/\.?0+$/,'');
                outMethod.textContent = 'EMA (n=' + n + ', α=' + alpha.toFixed(3) + ')';

                renderTable(values, ema);
                drawPlot(values, ema);
              }

              // tab switching
              tabSMA.addEventListener('click', () => {
                tabSMA.classList.add('active');
                tabEMA.classList.remove('active');
                panelSMA.classList.remove('hidden');
                panelEMA.classList.add('hidden');
              });
              tabEMA.addEventListener('click', () => {
                tabEMA.classList.add('active');
                tabSMA.classList.remove('active');
                panelEMA.classList.remove('hidden');
                panelSMA.classList.add('hidden');
              });

              btnSMA.addEventListener('click', calcSMA);
              btnEMA.addEventListener('click', calcEMA);

              // initial
              calcSMA();
            })();
          </script>

        </div>



