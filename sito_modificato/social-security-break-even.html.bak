    <script>
      // Toggle helpers
      function setupToggle(trackId, thumbId, checkboxId, panelId) {
        const track = document.getElementById(trackId);
        const thumb = document.getElementById(thumbId);
        const checkbox = document.getElementById(checkboxId);
        const panel = panelId ? document.getElementById(panelId) : null;

        function sync() {
          if (checkbox.checked) {
            track.classList.remove("bg-gray-300");
            track.classList.add("bg-blue-500");
            thumb.style.transform = "translateX(20px)";
            if (panel) panel.classList.remove("hidden");
          } else {
            track.classList.add("bg-gray-300");
            track.classList.remove("bg-blue-500");
            thumb.style.transform = "translateX(0)";
            if (panel) panel.classList.add("hidden");
          }
        }

        track.addEventListener("click", () => {
          checkbox.checked = !checkbox.checked;
          sync();
        });
        checkbox.addEventListener("change", sync);
        sync();
      }

      document.addEventListener("DOMContentLoaded", () => {
        setupToggle(
          "cola-toggle-track",
          "cola-toggle-thumb",
          "cola-enabled",
          "cola-settings"
        );
        setupToggle(
          "discount-toggle-track",
          "discount-toggle-thumb",
          "discount-enabled",
          "discount-settings"
        );

        const ageEarlySlider = document.getElementById("age-early");
        const ageLateSlider = document.getElementById("age-late");
        const ageEarlyOutput = document.getElementById("age-early-output");
        const ageLateOutput = document.getElementById("age-late-output");
        const currentAgeInput = document.getElementById("current-age");
        const lifeExpInput = document.getElementById("life-expectancy");
        const fraSelect = document.getElementById("fra-age");
        const fraBenefitInput = document.getElementById("fra-benefit");
        const colaEnabled = document.getElementById("cola-enabled");
        const colaRateInput = document.getElementById("cola-rate");
        const discountEnabled = document.getElementById("discount-enabled");
        const discountRateInput = document.getElementById("discount-rate");
        const errorEl = document.getElementById("input-error");

        const breakevenAgeEl = document.getElementById("breakeven-age");
        const breakevenMonthsEl = document.getElementById("breakeven-months");
        const totalEarlyEl = document.getElementById("total-early");
        const totalLateEl = document.getElementById("total-late");
        const totalEarlyPvEl = document.getElementById("total-early-pv");
        const totalLatePvEl = document.getElementById("total-late-pv");
        const interpretationEl = document.getElementById(
          "results-interpretation"
        );
        const tableBody = document.getElementById("breakeven-table-body");

        let chart;

        function syncAgeOutputs() {
          ageEarlyOutput.textContent = ageEarlySlider.value;
          ageLateOutput.textContent = ageLateSlider.value;
        }

        function ensureOrder() {
          let early = parseInt(ageEarlySlider.value, 10);
          let late = parseInt(ageLateSlider.value, 10);
          if (late <= early) {
            late = early + 1;
            if (late > parseInt(ageLateSlider.max, 10)) {
              early = late - 1;
            }
            ageEarlySlider.value = early;
            ageLateSlider.value = late;
          }
          syncAgeOutputs();
        }

        ageEarlySlider.addEventListener("input", () => {
          ensureOrder();
        });
        ageLateSlider.addEventListener("input", () => {
          ensureOrder();
        });

        syncAgeOutputs();

        function formatCurrency(value) {
          return value.toLocaleString("en-US", {
            style: "currency",
            currency: "USD",
            maximumFractionDigits: 0,
          });
        }

        function computeMonthlyBenefitAtAge(age, fraAge, pia) {
          const maxAge = 70;
          const minAge = 62;
          const ageNum = parseFloat(age);
          const fra = parseFloat(fraAge);

          if (ageNum === fra) return pia;

          if (ageNum < fra) {
            // Approximate early retirement reduction:
            // First 36 months: 5/9 of 1% per month; additional months: 5/12 of 1% per month.
            const monthsEarly = Math.round((fra - ageNum) * 12);
            const first36 = Math.min(monthsEarly, 36);
            const additional = Math.max(monthsEarly - 36, 0);
            const reduction =
              first36 * (5 / 9 / 100) + additional * (5 / 12 / 100);
            const factor = Math.max(0, 1 - reduction);
            return pia * factor;
          } else {
            // Delayed retirement credits: 8% per year between FRA and 70
            const yearsDelayed = Math.min(maxAge, ageNum) - fra;
            const factor = 1 + 0.08 * Math.max(0, yearsDelayed);
            return pia * factor;
          }
        }

        function calculate() {
          errorEl.classList.add("hidden");

          const currentAge = parseFloat(currentAgeInput.value);
          const lifeExp = parseFloat(lifeExpInput.value);
          let ageEarly = parseInt(ageEarlySlider.value, 10);
          let ageLate = parseInt(ageLateSlider.value, 10);
          const fraAge = parseFloat(fraSelect.value);
          const pia = parseFloat(fraBenefitInput.value);
          const useCola = colaEnabled.checked;
          const colaRate = parseFloat(colaRateInput.value) / 100 || 0;
          const useDiscount = discountEnabled.checked;
          const discountRate = parseFloat(discountRateInput.value) / 100 || 0;

          if (
            isNaN(currentAge) ||
            isNaN(lifeExp) ||
            isNaN(fraAge) ||
            isNaN(pia) ||
            currentAge < 50 ||
            currentAge > 80 ||
            lifeExp <= currentAge ||
            lifeExp > 100
          ) {
            errorEl.textContent =
              "Please enter realistic ages and benefit amount.";
            errorEl.classList.remove("hidden");
            return;
          }

          // Ensure claiming ages are within [currentAge, lifeExp]
          ageEarly = Math.max(
            Math.ceil(currentAge),
            Math.min(ageEarly, Math.floor(lifeExp))
          );
          ageLate = Math.max(
            ageEarly + 1,
            Math.min(ageLate, Math.floor(lifeExp))
          );

          ageEarlySlider.value = ageEarly;
          ageLateSlider.value = ageLate;
          syncAgeOutputs();

          if (ageEarly >= ageLate) {
            errorEl.textContent =
              "Earlier claiming age must be before later claiming age.";
            errorEl.classList.remove("hidden");
            return;
          }

          const monthlyEarly = computeMonthlyBenefitAtAge(
            ageEarly,
            fraAge,
            pia
          );
          const monthlyLate = computeMonthlyBenefitAtAge(ageLate, fraAge, pia);

          const annualEarly0 = monthlyEarly * 12;
          const annualLate0 = monthlyLate * 12;

          const ages = [];
          const cumEarly = [];
          const cumLate = [];
          const annualEarlyArr = [];
          const annualLateArr = [];
          const pvEarlyArr = [];
          const pvLateArr = [];

          let cumulativeEarly = 0;
          let cumulativeLate = 0;
          let cumulativeEarlyPv = 0;
          let cumulativeLatePv = 0;

          let breakevenAge = null;

          for (
            let age = Math.floor(currentAge);
            age <= Math.floor(lifeExp);
            age++
          ) {
            let annualEarly = 0;
            let annualLate = 0;

            if (age >= ageEarly) {
              const yearsSinceClaimEarly = age - ageEarly;
              const colaFactorEarly = useCola
                ? Math.pow(1 + colaRate, yearsSinceClaimEarly)
                : 1;
              annualEarly = annualEarly0 * colaFactorEarly;
            }

            if (age >= ageLate) {
              const yearsSinceClaimLate = age - ageLate;
              const colaFactorLate = useCola
                ? Math.pow(1 + colaRate, yearsSinceClaimLate)
                : 1;
              annualLate = annualLate0 * colaFactorLate;
            }

            cumulativeEarly += annualEarly;
            cumulativeLate += annualLate;

            let pvEarly = annualEarly;
            let pvLate = annualLate;
            if (useDiscount && discountRate > 0) {
              const yearsFromNow = age - currentAge;
              const discFactor = Math.pow(1 + discountRate, yearsFromNow);
              pvEarly = annualEarly / discFactor;
              pvLate = annualLate / discFactor;
            }

            cumulativeEarlyPv += pvEarly;
            cumulativeLatePv += pvLate;

            ages.push(age);
            annualEarlyArr.push(annualEarly);
            annualLateArr.push(annualLate);
            cumEarly.push(cumulativeEarly);
            cumLate.push(cumulativeLate);
            pvEarlyArr.push(cumulativeEarlyPv);
            pvLateArr.push(cumulativeLatePv);

            if (
              breakevenAge === null &&
              cumulativeLate >= cumulativeEarly &&
              age >= ageLate
            ) {
              breakevenAge = age;
            }
          }

          // Update summary
          breakevenAgeEl.textContent = breakevenAge
            ? breakevenAge.toFixed(0)
            : "No break-even before " + lifeExp.toFixed(0);
          breakevenMonthsEl.textContent = breakevenAge
            ? "At about age " +
              breakevenAge.toFixed(0) +
              ", total benefits from claiming later catch up with claiming earlier."
            : "Claiming earlier pays more in total up to your selected life expectancy.";

          const totalEarly = cumEarly[cumEarly.length - 1] || 0;
          const totalLate = cumLate[cumLate.length - 1] || 0;
          const totalEarlyPv = pvEarlyArr[pvEarlyArr.length - 1] || 0;
          const totalLatePv = pvLateArr[pvLateArr.length - 1] || 0;

          totalEarlyEl.textContent = formatCurrency(totalEarly);
          totalLateEl.textContent = formatCurrency(totalLate);
          totalEarlyPvEl.textContent = useDiscount
            ? "Present value: " + formatCurrency(totalEarlyPv)
            : "Present value: (no discounting)";
          totalLatePvEl.textContent = useDiscount
            ? "Present value: " + formatCurrency(totalLatePv)
            : "Present value: (no discounting)";

          // Interpretation
          let interp = "";
          if (breakevenAge) {
            interp = `If you live past about age ${breakevenAge.toFixed(
              0
            )}, delaying Social Security to age ${ageLate} is projected to pay more in total than claiming at age ${ageEarly}. `;
            interp += `If you do not expect to live that long or you need income sooner, claiming earlier may still be appropriate.`;
          } else {
            interp = `Given your inputs, claiming earlier at age ${ageEarly} produces higher total benefits than claiming at age ${ageLate} all the way through age ${lifeExp.toFixed(
              0
            )}. `;
            interp += `This can happen when the delay period is short or life expectancy is relatively low.`;
          }
          interpretationEl.textContent = interp;
          interpretationEl.classList.remove("hidden");

          // Update chart
          const ctx = document
            .getElementById("breakeven-chart")
            .getContext("2d");
          if (chart) chart.destroy();
          chart = new Chart(ctx, {
            type: "line",
            data: {
              labels: ages,
              datasets: [
                {
                  label: "Cumulative – earlier",
                  data: cumEarly,
                  borderColor: "#2563eb",
                  backgroundColor: "rgba(37, 99, 235, 0.1)",
                  tension: 0.1,
                },
                {
                  label: "Cumulative – later",
                  data: cumLate,
                  borderColor: "#16a34a",
                  backgroundColor: "rgba(22, 163, 74, 0.1)",
                  tension: 0.1,
                },
              ],
            },
            options: {
              responsive: true,
              maintainAspectRatio: false,
              interaction: { mode: "index", intersect: false },
              plugins: {
                legend: { position: "bottom" },
                tooltip: {
                  callbacks: {
                    label: function (context) {
                      return (
                        context.dataset.label +
                        ": " +
                        formatCurrency(context.parsed.y)
                      );
                    },
                  },
                },
              },
              scales: {
                x: {
                  title: { display: true, text: "Age" },
                },
                y: {
                  title: { display: true, text: "Cumulative benefits (USD)" },
                  ticks: {
                    callback: function (value) {
                      return "$" + (value / 1000).toFixed(0) + "k";
                    },
                  },
                },
              },
            },
          });

          // Update table (sample every 1 year)
          tableBody.innerHTML = "";
          for (let i = 0; i < ages.length; i++) {
            const tr = document.createElement("tr");
            tr.className = i % 2 === 0 ? "bg-white" : "bg-gray-50";
            tr.innerHTML = `
          <td class="px-2 py-1 border-t text-gray-700">${ages[i]}</td>
          <td class="px-2 py-1 border-t text-right text-gray-700">${
            annualEarlyArr[i] > 0 ? formatCurrency(annualEarlyArr[i]) : "—"
          }</td>
          <td class="px-2 py-1 border-t text-right text-gray-700">${
            cumEarly[i] > 0 ? formatCurrency(cumEarly[i]) : "—"
          }</td>
          <td class="px-2 py-1 border-t text-right text-gray-700">${
            annualLateArr[i] > 0 ? formatCurrency(annualLateArr[i]) : "—"
          }</td>
          <td class="px-2 py-1 border-t text-right text-gray-700">${
            cumLate[i] > 0 ? formatCurrency(cumLate[i]) : "—"
          }</td>
        `;
            tableBody.appendChild(tr);
          }
        }

        document
          .getElementById("calculate-btn")
          .addEventListener("click", calculate);

        document.getElementById("reset-btn").addEventListener("click", () => {
          currentAgeInput.value = 62;
          fraSelect.value = 67;
          fraBenefitInput.value = 2000;
          ageEarlySlider.value = 62;
          ageLateSlider.value = 70;
          lifeExpInput.value = 90;
          colaEnabled.checked = false;
          discountEnabled.checked = false;
          colaRateInput.value = 2.0;
          discountRateInput.value = 0;
          setupToggle(
            "cola-toggle-track",
            "cola-toggle-thumb",
            "cola-enabled",
            "cola-settings"
          );
          setupToggle(
            "discount-toggle-track",
            "discount-toggle-thumb",
            "discount-enabled",
            "discount-settings"
          );
          syncAgeOutputs();
          errorEl.classList.add("hidden");
          breakevenAgeEl.textContent = "–";
          breakevenMonthsEl.textContent = "–";
          totalEarlyEl.textContent = "$0";
          totalLateEl.textContent = "$0";
          totalEarlyPvEl.textContent = "Present value: –";
          totalLatePvEl.textContent = "Present value: –";
          interpretationEl.classList.add("hidden");
          tableBody.innerHTML = "";
          if (chart) {
            chart.destroy();
            chart = null;
          }
        });

        // Auto-calc on load with defaults
        calculate();
      });
    </script>

