    <script>
      (function () {
        const FACTOR_BAR_TO_KGCM2 = 1.019716; // 1 bar = 1.019716 kg/cm²

        const barInput = document.getElementById("barInput");
        const kgcm2Input = document.getElementById("kgcm2Input");
        const liveResult = document.getElementById("liveResult");
        const swapBtn = document.getElementById("swapBtn");
        const clearBtn = document.getElementById("clearBtn");
        const tableBody = document.getElementById("conversionTableBody");

        function formatNumber(value, decimals) {
          if (isNaN(value) || !isFinite(value)) return "";
          return Number(value).toFixed(decimals);
        }

        function updateFromBar() {
          const barVal = parseFloat(barInput.value);
          if (isNaN(barVal)) {
            kgcm2Input.value = "";
            liveResult.textContent =
              "Enter a value in either field to see the converted pressure.";
            return;
          }
          const kgcm2Val = barVal * FACTOR_BAR_TO_KGCM2;
          kgcm2Input.value = formatNumber(kgcm2Val, 6);
          liveResult.textContent = `${formatNumber(
            barVal,
            6
          )} bar = ${formatNumber(kgcm2Val, 6)} kg/cm²`;
        }

        function updateFromKgcm2() {
          const kgcm2Val = parseFloat(kgcm2Input.value);
          if (isNaN(kgcm2Val)) {
            barInput.value = "";
            liveResult.textContent =
              "Enter a value in either field to see the converted pressure.";
            return;
          }
          const barVal = kgcm2Val / FACTOR_BAR_TO_KGCM2;
          barInput.value = formatNumber(barVal, 6);
          liveResult.textContent = `${formatNumber(
            kgcm2Val,
            6
          )} kg/cm² = ${formatNumber(barVal, 6)} bar`;
        }

        barInput.addEventListener("input", function () {
          if (document.activeElement === barInput) {
            updateFromBar();
          }
        });

        kgcm2Input.addEventListener("input", function () {
          if (document.activeElement === kgcm2Input) {
            updateFromKgcm2();
          }
        });

        swapBtn.addEventListener("click", function () {
          const barVal = barInput.value;
          const kgVal = kgcm2Input.value;
          barInput.value = kgVal;
          kgcm2Input.value = barVal;
          if (document.activeElement === barInput || (!barVal && kgVal)) {
            updateFromBar();
          } else {
            updateFromKgcm2();
          }
        });

        clearBtn.addEventListener("click", function () {
          barInput.value = "";
          kgcm2Input.value = "";
          liveResult.textContent =
            "Enter a value in either field to see the converted pressure.";
          barInput.focus();
        });

        function buildTable() {
          const rows = [];
          const values = [0, 0.5, 1, 2, 5, 10, 20, 50, 100, 200];
          values.forEach(function (barVal) {
            const kgVal = barVal * FACTOR_BAR_TO_KGCM2;
            rows.push(
              `<tr class="border-t border-gray-100">
            <td class="px-3 py-1.5">${formatNumber(barVal, 4)}</td>
            <td class="px-3 py-1.5">${formatNumber(kgVal, 4)}</td>
          </tr>`
            );
          });
          tableBody.innerHTML = rows.join("");
        }

        buildTable();
      })();
    </script>

