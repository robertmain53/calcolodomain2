            <script>
              document.addEventListener('DOMContentLoaded', function() {
                const inputRadius = document.getElementById('input-radius');
                const inputDiameter = document.getElementById('input-diameter');
                const calculateBtn = document.getElementById('calculate-btn');
                const resultsContainer = document.getElementById('results-container');
                const errorMessage = document.getElementById('error-message');
                
                const resultVolume = document.getElementById('result-volume');
                const resultSurfaceArea = document.getElementById('result-surface-area');
                const resultCircumference = document.getElementById('result-circumference');
                const resultRadius = document.getElementById('result-radius');
                const stepByStep = document.getElementById('step-by-step');

                // --- Helper Functions ---
                function parseInput(val) {
                    const num = parseFloat(val);
                    return (num > 0 && !isNaN(num)) ? num : null;
                }
                
                function formatNumber(num) {
                    if (Math.abs(num) < 1e-10) return '0';
                    return parseFloat(num.toFixed(6)).toString();
                }

                function showError(message) {
                    errorMessage.textContent = `Error: ${message}`;
                    resultsContainer.classList.add('hidden');
                }

                // --- Main Calculation Logic ---
                calculateBtn.addEventListener('click', () => {
                    let r = parseInput(inputRadius.value);
                    let d = parseInput(inputDiameter.value);
                    let knownCount = 0;
                    
                    if (r !== null) knownCount++;
                    if (d !== null) knownCount++;
                    
                    try {
                        if (knownCount === 0) {
                            throw new Error('Please enter EITHER the Radius OR the Diameter.');
                        }
                        if (knownCount > 1) {
                            throw new Error('Please enter a value for ONLY ONE field (Radius or Diameter).');
                        }
                        
                        errorMessage.textContent = '';
                        
                        // 1. Ensure Radius is known
                        if (r === null) {
                            r = d / 2;
                        } else {
                            d = 2 * r;
                        }

                        // 2. Calculate Properties
                        const r_cubed = r * r * r;
                        const r_squared = r * r;
                        
                        // Volume
                        const V_coeff = (4 / 3) * Math.PI;
                        const V = V_coeff * r_cubed;
                        
                        // Surface Area
                        const A_coeff = 4 * Math.PI;
                        const A = A_coeff * r_squared;
                        
                        // Circumference
                        const C = 2 * Math.PI * r;
                        
                        // --- Step-by-Step Construction ---
                        const fR = formatNumber(r);
                        const fD = formatNumber(d);
                        const fV = formatNumber(V);
                        const fA = formatNumber(A);
                        const fC = formatNumber(C);
                        
                        let steps = '';

                        // 1. Initial Step
                        if (r === parseInput(inputRadius.value)) {
                            steps += `<p>1. <strong>Input:</strong> Radius $r = ${fR}$</p>`;
                        } else {
                            steps += `<p>1. <strong>Find Radius:</strong> $r = d / 2 = ${fD} / 2 = ${fR}$</p>`;
                        }
                        
                        // 2. Volume Step
                        steps += `<h4 class="font-semibold text-lg mt-4">2. Calculate Volume ($V$):</h4>`;
                        steps += `<p>Formula: $V = \\frac{4}{3} \\pi r^3$</p>`;
                        steps += `<p class="formula-box text-center">$V = \\frac{4}{3} \\pi \\times (${fR})^3 = ${formatNumber(V_coeff)} \\times ${formatNumber(r_cubed)} \\approx ${fV}$</p>`;

                        // 3. Surface Area Step
                        steps += `<h4 class="font-semibold text-lg mt-4">3. Calculate Surface Area ($A$):</h4>`;
                        steps += `<p>Formula: $A = 4 \\pi r^2$</p>`;
                        steps += `<p class="formula-box text-center">$A = 4 \\pi \\times (${fR})^2 = ${formatNumber(A_coeff)} \\times ${formatNumber(r_squared)} \\approx ${fA}$</p>`;

                        // 4. Circumference Step
                        steps += `<h4 class="font-semibold text-lg mt-4">4. Calculate Circumference ($C$):</h4>`;
                        steps += `<p>Formula: $C = 2 \\pi r$</p>`;
                        steps += `<p class="formula-box text-center">$C = 2 \\pi \\times ${fR} \\approx ${fC}$</p>`;

                        // 5. Update DOM
                        resultVolume.textContent = fV;
                        resultSurfaceArea.textContent = fA;
                        resultCircumference.textContent = fC;
                        resultRadius.textContent = fR;
                        stepByStep.innerHTML = steps;
                        resultsContainer.classList.remove('hidden');

                        // Re-render MathJax
                        if (window.MathJax) {
                            window.MathJax.typesetPromise([stepByStep, resultVolume, resultSurfaceArea, resultCircumference, resultRadius]);
                        }

                    } catch (e) {
                        showError(e.message);
                    }
                });

                // Initial calculation (for example values)
                inputRadius.value = 5;
                inputDiameter.value = '';
                calculateBtn.click();
            </script>

