    <script>
      // Utility conversions
      const PI = Math.PI;

      function flOzToMl(v) {
        return v * 29.5735;
      }
      function mlToFlOz(v) {
        return v / 29.5735;
      }
      function LToMl(v) {
        return v * 1000;
      }
      function mlToL(v) {
        return v / 1000;
      }

      function ft2ToM2(v) {
        return v * 0.092903;
      }
      function m2ToFt2(v) {
        return v / 0.092903;
      }

      function ftToM(v) {
        return v * 0.3048;
      }
      function mToFt(v) {
        return v / 0.3048;
      }

      function inToMm(v) {
        return v * 25.4;
      }
      function mmToIn(v) {
        return v / 25.4;
      }

      function showError(msg) {
        const el = document.getElementById("calc-error");
        el.textContent = msg;
        el.classList.remove("hidden");
      }
      function clearError() {
        const el = document.getElementById("calc-error");
        el.textContent = "";
        el.classList.add("hidden");
      }

      // Mode switching
      document.querySelectorAll(".mode-tab").forEach((btn) => {
        btn.addEventListener("click", () => {
          const mode = btn.getAttribute("data-mode");
          document
            .querySelectorAll(".mode-tab")
            .forEach((b) => b.classList.remove("tab-active"));
          btn.classList.add("tab-active");
          document.querySelectorAll(".mode-panel").forEach((panel) => {
            panel.classList.toggle(
              "hidden",
              panel.getAttribute("data-mode-panel") !== mode
            );
          });
          clearError();
        });
      });

      // Area coverage calculation
      document.getElementById("area-calc-btn").addEventListener("click", () => {
        clearError();
        const areaVal = parseFloat(document.getElementById("area-value").value);
        const areaUnit = document.getElementById("area-unit").value;
        const covRate = parseFloat(
          document.getElementById("coverage-rate").value
        );
        const covUnit = document.getElementById("coverage-unit").value;
        const unitVol = parseFloat(
          document.getElementById("unit-volume").value
        );
        const unitVolUnit = document.getElementById("unit-volume-unit").value;
        const waste =
          parseFloat(document.getElementById("area-waste").value) || 0;

        if (!areaVal || areaVal <= 0 || !covRate || covRate <= 0) {
          showError(
            "Please enter a positive area and coverage rate for the area calculation."
          );
          return;
        }

        // Convert area to a common basis matching coverage unit
        let areaInCovUnits = areaVal;
        if (areaUnit === "m2" && covUnit.startsWith("ft2")) {
          areaInCovUnits = m2ToFt2(areaVal);
        } else if (areaUnit === "ft2" && covUnit.startsWith("m2")) {
          areaInCovUnits = ft2ToM2(areaVal);
        }

        const adjustedArea = areaInCovUnits * (1 + waste / 100);
        const unitsRequired = adjustedArea / covRate;

        // Total volume/weight if unit volume is provided
        let totalVolumeText = "n/a";
        if (!isNaN(unitVol) && unitVol > 0) {
          const totalUnitsVolume = unitsRequired * unitVol;
          totalVolumeText = totalUnitsVolume.toFixed(2) + " " + unitVolUnit;
        }

        document.getElementById("area-adjusted").textContent =
          adjustedArea.toFixed(2) +
          (covUnit.startsWith("ft2") ? " ft²" : " m²");
        document.getElementById("area-units").textContent =
          unitsRequired.toFixed(2) + " units";
        document.getElementById("area-total-volume").textContent =
          totalVolumeText;
        document.getElementById("area-results").classList.remove("hidden");
      });

      // Linear bead calculation
      document.getElementById("bead-calc-btn").addEventListener("click", () => {
        clearError();
        const lengthVal = parseFloat(
          document.getElementById("bead-length").value
        );
        const lengthUnit = document.getElementById("bead-length-unit").value;
        const diaVal = parseFloat(
          document.getElementById("bead-diameter").value
        );
        const diaUnit = document.getElementById("bead-diameter-unit").value;
        const cartVolVal = parseFloat(
          document.getElementById("cartridge-volume").value
        );
        const cartVolUnit = document.getElementById(
          "cartridge-volume-unit"
        ).value;
        const waste =
          parseFloat(document.getElementById("bead-waste").value) || 0;

        if (
          !lengthVal ||
          lengthVal <= 0 ||
          !diaVal ||
          diaVal <= 0 ||
          !cartVolVal ||
          cartVolVal <= 0
        ) {
          showError(
            "Please enter positive values for bead length, diameter, and cartridge size."
          );
          return;
        }

        // Convert length to meters
        let lengthM = lengthVal;
        if (lengthUnit === "ft") lengthM = ftToM(lengthVal);

        // Convert diameter to meters
        let diaM = diaVal;
        if (diaUnit === "mm") diaM = diaVal / 1000;
        else if (diaUnit === "in") diaM = inToMm(diaVal) / 1000;

        // Bead cross-sectional area (m²)
        const area = PI * Math.pow(diaM / 2, 2);
        // Volume in m³
        let volumeM3 = area * lengthM;
        volumeM3 *= 1 + waste / 100;

        // Convert cartridge volume to mL
        let cartMl = cartVolVal;
        if (cartVolUnit === "fl_oz") cartMl = flOzToMl(cartVolVal);
        else if (cartVolUnit === "L") cartMl = LToMl(cartVolVal);

        const volumeMl = volumeM3 * 1e6; // 1 m³ = 1,000,000 mL
        const cartridges = volumeMl / cartMl;

        // Display in both mL and fl oz for convenience
        const volumeFlOz = mlToFlOz(volumeMl);

        document.getElementById("bead-adjusted-length").textContent =
          lengthM.toFixed(2) + " m (including waste factor)";
        document.getElementById("bead-total-volume").textContent =
          volumeMl.toFixed(0) + " mL (" + volumeFlOz.toFixed(1) + " fl oz)";
        document.getElementById("bead-cartridges").textContent =
          cartridges.toFixed(2) + " cartridges";
        document.getElementById("bead-results").classList.remove("hidden");
      });

      // Anchor / hole fill calculation
      document
        .getElementById("anchor-calc-btn")
        .addEventListener("click", () => {
          clearError();
          const dVal = parseFloat(
            document.getElementById("hole-diameter").value
          );
          const dUnit = document.getElementById("hole-diameter-unit").value;
          const hVal = parseFloat(document.getElementById("embed-depth").value);
          const hUnit = document.getElementById("embed-depth-unit").value;
          const nHoles = parseInt(
            document.getElementById("num-holes").value,
            10
          );
          const cartVolVal = parseFloat(
            document.getElementById("anchor-cartridge-volume").value
          );
          const cartVolUnit = document.getElementById(
            "anchor-cartridge-volume-unit"
          ).value;
          const fillFactor =
            parseFloat(document.getElementById("anchor-fill-factor").value) ||
            100;
          const waste =
            parseFloat(document.getElementById("anchor-waste").value) || 0;

          if (
            !dVal ||
            dVal <= 0 ||
            !hVal ||
            hVal <= 0 ||
            !nHoles ||
            nHoles <= 0 ||
            !cartVolVal ||
            cartVolVal <= 0
          ) {
            showError(
              "Please enter positive values for hole diameter, embedment depth, number of holes, and cartridge size."
            );
            return;
          }

          // Convert diameter and depth to meters
          let dM = dVal;
          if (dUnit === "mm") dM = dVal / 1000;
          else if (dUnit === "in") dM = inToMm(dVal) / 1000;

          let hM = hVal;
          if (hUnit === "mm") hM = hVal / 1000;
          else if (hUnit === "in") hM = inToMm(hVal) / 1000;

          // Hole volume in m³
          const vHole = PI * Math.pow(dM / 2, 2) * hM;
          const vEffPerHole = vHole * (fillFactor / 100);
          let vTotal = vEffPerHole * nHoles;
          vTotal *= 1 + waste / 100;

          // Convert cartridge volume to mL
          let cartMl = cartVolVal;
          if (cartVolUnit === "fl_oz") cartMl = flOzToMl(cartVolVal);
          else if (cartVolUnit === "L") cartMl = LToMl(cartVolVal);

          const totalMl = vTotal * 1e6;
          const cartridges = totalMl / cartMl;
          const perHoleMl = vEffPerHole * 1e6;

          document.getElementById("anchor-total-volume").textContent =
            totalMl.toFixed(0) +
            " mL (" +
            mlToFlOz(totalMl).toFixed(1) +
            " fl oz)";
          document.getElementById("anchor-cartridges").textContent =
            cartridges.toFixed(2) + " cartridges";
          document.getElementById("anchor-volume-per-hole").textContent =
            perHoleMl.toFixed(1) + " mL per hole (before waste)";
          document.getElementById("anchor-results").classList.remove("hidden");
        });

      // MathJax config
      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
