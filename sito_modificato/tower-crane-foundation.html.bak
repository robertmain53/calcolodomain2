    <script>
      // Core calculation logic
      function runTowerCraneFoundationCalc() {
        const B = parseFloat(document.getElementById("B").value) || 0;
        const L = parseFloat(document.getElementById("L").value) || 0;
        const t = parseFloat(document.getElementById("t").value) || 0;
        const embed = parseFloat(document.getElementById("embed").value) || 0;
        const gammaConc =
          parseFloat(document.getElementById("gammaConc").value) || 0;
        const qa = parseFloat(document.getElementById("qa").value) || 0;
        const muInput = parseFloat(document.getElementById("mu").value) || 0;

        const N = parseFloat(document.getElementById("N").value) || 0;
        const M = parseFloat(document.getElementById("M").value) || 0;
        const V = parseFloat(document.getElementById("V").value) || 0;
        const gammaOT =
          parseFloat(document.getElementById("gammaOT").value) || 1.5;
        const gammaSL =
          parseFloat(document.getElementById("gammaSL").value) || 1.5;
        const fck = parseFloat(document.getElementById("fck").value) || 30;
        const fy = parseFloat(document.getElementById("fy").value) || 500;
        const phi = parseFloat(document.getElementById("phi").value) || 30;

        if (B <= 0 || L <= 0 || t <= 0 || qa <= 0 || N <= 0) {
          alert(
            "Please enter positive values for geometry, soil bearing and vertical load."
          );
          return;
        }

        const mu = muInput > 0 ? muInput : Math.tan((phi * Math.PI) / 180);

        // Footing self-weight and total vertical load
        const Wc = gammaConc * B * L * t; // kN
        const Ntot = N + Wc;

        // Bearing pressures (rectangular, moment about B)
        const A = B * L;
        const q0 = Ntot / A; // kPa
        const qm = (6 * M) / (B * L * L); // kPa
        let qmax = q0 + qm;
        let qmin = q0 - qm;

        // Eccentricity
        const e = Ntot > 0 ? M / Ntot : 0;

        // Overturning FS: approximate using resultant location
        // Assume resultant at distance B/2 - e from toe; resisting moment ~ Ntot * (B/2 - e)
        const leverArm = Math.max(B / 2 - Math.abs(e), 0);
        const Mres = Ntot * leverArm; // kNm
        const FS_OT = M > 0 ? Mres / M : Infinity;

        // Sliding FS
        const Rsl = mu * Ntot;
        const FS_SL = V > 0 ? Rsl / V : Infinity;

        // Bearing utilization
        const bearingUtil = qa > 0 ? qmax / qa : 0;

        // Approximate bending and reinforcement
        const span = B / 2;
        const Md = (qmax * span * span) / 2; // kNm/m
        const d = 0.9 * t; // m
        const z = 0.9 * d; // m
        let AsReq = 0;
        if (fy > 0 && z > 0) {
          AsReq = (Md * 1e6) / (0.87 * fy * 1e6 * z); // m²/m
          AsReq = AsReq * 1e6; // mm²/m
        }

        // Suggest bar layout (simple heuristic)
        let barLayout = "–";
        if (AsReq > 0) {
          const barAreas = [
            { dia: 16, area: 201 },
            { dia: 20, area: 314 },
            { dia: 25, area: 491 },
          ];
          let best = null;
          barAreas.forEach((bar) => {
            const spacing = (bar.area * 1000) / AsReq; // mm
            if (spacing > 75 && spacing < 300) {
              if (!best || spacing < best.spacing) {
                best = { dia: bar.dia, spacing: spacing };
              }
            }
          });
          if (best) {
            barLayout = `Use T${best.dia} @ ${best.spacing.toFixed(
              0
            )} mm c/c (one layer, each way)`;
          } else {
            barLayout =
              "Increase footing thickness or choose custom bar/spacing to meet A_s,req.";
          }
        }

        // Update UI
        const setText = (id, value, decimals = 2) => {
          const el = document.getElementById(id);
          if (!el) return;
          if (typeof value === "number") {
            el.textContent = isFinite(value) ? value.toFixed(decimals) : "–";
          } else {
            el.textContent = value;
          }
        };

        setText("fosOT", FS_OT);
        setText("fosSL", FS_SL);
        setText("bearingUtil", bearingUtil, 2);
        setText("eccentricity", e, 3);
        setText("qmax", qmax, 1);
        setText("qmin", qmin, 1);
        setText("Md", Md, 1);
        setText("AsReq", AsReq, 0);
        document.getElementById("barLayout").textContent = barLayout;

        // Status messages
        const fosOTStatus = document.getElementById("fosOTStatus");
        const fosSLStatus = document.getElementById("fosSLStatus");
        const bearingStatus = document.getElementById("bearingStatus");
        const upliftWarning = document.getElementById("upliftWarning");

        if (FS_OT < gammaOT) {
          fosOTStatus.textContent = `Not OK – FS_OT < target (${gammaOT.toFixed(
            2
          )}). Increase footing size, thickness or embedment.`;
          fosOTStatus.className = "text-xs text-red-600";
        } else {
          fosOTStatus.textContent = `OK – FS_OT ≥ target (${gammaOT.toFixed(
            2
          )}).`;
          fosOTStatus.className = "text-xs text-green-600";
        }

        if (FS_SL < gammaSL) {
          fosSLStatus.textContent = `Not OK – FS_SL < target (${gammaSL.toFixed(
            2
          )}). Consider larger base, deeper embedment or shear key.`;
          fosSLStatus.className = "text-xs text-red-600";
        } else {
          fosSLStatus.textContent = `OK – FS_SL ≥ target (${gammaSL.toFixed(
            2
          )}).`;
          fosSLStatus.className = "text-xs text-green-600";
        }

        if (bearingUtil > 1.0) {
          bearingStatus.textContent =
            "Not OK – q_max exceeds allowable bearing. Increase base area or improve ground.";
          bearingStatus.className = "text-xs text-red-600";
        } else {
          bearingStatus.textContent =
            "OK – q_max ≤ allowable bearing (serviceability).";
          bearingStatus.className = "text-xs text-green-600";
        }

        if (Math.abs(e) > B / 6) {
          upliftWarning.textContent =
            "Warning: eccentricity e > B/6. Part of the footing is in uplift; triangular bearing distribution should be used. Consider increasing B or N.";
        } else {
          upliftWarning.textContent = "";
        }
      }

      document
        .getElementById("runCalc")
        .addEventListener("click", runTowerCraneFoundationCalc);
      document.getElementById("resetCalc").addEventListener("click", () => {
        // Simple reset: reload page to restore defaults
        window.location.reload();
      });

      document
        .querySelectorAll("#faq-container .faq-question")
        .forEach((btn) => {
          btn.addEventListener("click", () => {
            const answer = btn.parentElement.querySelector(".faq-answer");
            const icon = btn.querySelector("span:last-child");
            if (answer.classList.contains("hidden")) {
              answer.classList.remove("hidden");
              icon.textContent = "−";
            } else {
              answer.classList.add("hidden");
              icon.textContent = "+";
            }
          });
        });
    </script>

