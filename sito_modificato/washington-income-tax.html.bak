      function formatCurrency(value) {
        if (!isFinite(value)) return "$0";
        return value.toLocaleString("en-US", {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 0,
        });
      }

      function formatPercent(value) {
        if (!isFinite(value)) return "0%";
        return value.toFixed(1).replace(/\.0$/, "") + "%";
      }

      function computeFederalTax(taxableIncome, year, status) {
        const brackets = TAX_DATA[year].brackets[status];
        let remaining = Math.max(0, taxableIncome);
        let tax = 0;
        let lastRate = 0;

        let lowerLimit = 0;
        for (const bracket of brackets) {
          const upper = bracket.upTo;
          const width = Math.min(remaining, upper - lowerLimit);
          if (width > 0) {
            tax += width * bracket.rate;
            remaining -= width;
            lastRate = bracket.rate;
          }
          lowerLimit = upper;
          if (remaining <= 0) break;
        }
        return { tax, marginalRate: lastRate };
      }

      function computeFica(wageBase, year, status) {
        const data = TAX_DATA[year];
        const ssBase = data.socialSecurityWageBase;
        const addMedThreshold = data.additionalMedicareThreshold[status];

        const socialSecurityTax = Math.min(wageBase, ssBase) * 0.062;
        const medicareTax = wageBase * 0.0145;
        const additionalMedicareTax =
          wageBase > addMedThreshold ? (wageBase - addMedThreshold) * 0.009 : 0;

        return {
          socialSecurityTax,
          medicareTax,
          additionalMedicareTax,
          totalFica: socialSecurityTax + medicareTax + additionalMedicareTax,
        };
      }

      function calculate() {
        const year = document.getElementById("taxYear").value;
        const status = document.getElementById("filingStatus").value;
        const modeSalary = document
          .getElementById("modeSalary")
          .classList.contains("bg-blue-50");

        let grossIncome = 0;
        if (modeSalary) {
          grossIncome =
            parseFloat(document.getElementById("annualIncome").value) || 0;
        } else {
          const rate =
            parseFloat(document.getElementById("hourlyRate").value) || 0;
          const hours =
            parseFloat(document.getElementById("hoursPerWeek").value) || 0;
          const weeks =
            parseFloat(document.getElementById("weeksPerYear").value) || 0;
          grossIncome = rate * hours * weeks;
        }

        const additionalIncome =
          parseFloat(document.getElementById("additionalIncome").value) || 0;
        grossIncome += additionalIncome;

        const pretax401k =
          parseFloat(document.getElementById("pretax401k").value) || 0;
        const pretaxHsa =
          parseFloat(document.getElementById("pretaxHsa").value) || 0;
        const otherPretax =
          parseFloat(document.getElementById("otherPretax").value) || 0;
        const postTax =
          parseFloat(document.getElementById("postTax").value) || 0;

        const totalPretax = pretax401k + pretaxHsa + otherPretax;
        const wageBaseForFica = Math.max(0, grossIncome - totalPretax);
        const adjustedGrossForFederal = wageBaseForFica;

        const stdDeduction = TAX_DATA[year].standardDeduction[status];
        const taxableIncome = Math.max(
          0,
          adjustedGrossForFederal - stdDeduction
        );

        const fed = computeFederalTax(taxableIncome, year, status);
        const fica = computeFica(wageBaseForFica, year, status);

        const federalTax = fed.tax;
        const ficaTax = fica.totalFica;
        const stateTax = 0; // Washington has no wage income tax

        const totalTax = federalTax + ficaTax + stateTax;
        const netPay = grossIncome - totalTax - postTax;

        const payFreq =
          parseFloat(document.getElementById("payFrequency").value) || 26;
        const grossPerPay = grossIncome / payFreq;
        const netPerPay = netPay / payFreq;

        const effectiveRate =
          grossIncome > 0 ? (totalTax / grossIncome) * 100 : 0;

        document.getElementById("netPay").textContent = formatCurrency(netPay);
        document.getElementById("totalTax").textContent =
          formatCurrency(totalTax);
        document.getElementById("effectiveRate").textContent =
          formatPercent(effectiveRate);
        document.getElementById("federalTax").textContent =
          formatCurrency(federalTax);
        document.getElementById("ficaTax").textContent =
          formatCurrency(ficaTax);
        document.getElementById("stateTax").textContent =
          formatCurrency(stateTax);
        document.getElementById("marginalBracket").textContent =
          fed.marginalRate ? formatPercent(fed.marginalRate * 100) : "0%";
        document.getElementById("grossPerPay").textContent =
          formatCurrency(grossPerPay);
        document.getElementById("netPerPay").textContent =
          formatCurrency(netPerPay);
      }

      document.addEventListener("DOMContentLoaded", () => {
        const modeSalaryBtn = document.getElementById("modeSalary");
        const modeHourlyBtn = document.getElementById("modeHourly");
        const salaryGroup = document.getElementById("salaryGroup");
        const hourlyGroup = document.getElementById("hourlyGroup");

        modeSalaryBtn.addEventListener("click", () => {
          modeSalaryBtn.classList.add(
            "bg-blue-50",
            "border-blue-600",
            "text-blue-700",
            "font-medium"
          );
          modeHourlyBtn.classList.remove(
            "bg-blue-50",
            "border-blue-600",
            "text-blue-700",
            "font-medium"
          );
          modeHourlyBtn.classList.add("border-gray-300", "text-gray-700");
          salaryGroup.classList.remove("hidden");
          hourlyGroup.classList.add("hidden");
          calculate();
        });

        modeHourlyBtn.addEventListener("click", () => {
          modeHourlyBtn.classList.add(
            "bg-blue-50",
            "border-blue-600",
            "text-blue-700",
            "font-medium"
          );
          modeSalaryBtn.classList.remove(
            "bg-blue-50",
            "border-blue-600",
            "text-blue-700",
            "font-medium"
          );
          modeSalaryBtn.classList.add("border-gray-300", "text-gray-700");
          salaryGroup.classList.add("hidden");
          hourlyGroup.classList.remove("hidden");
          calculate();
        });

        document
          .getElementById("calculateBtn")
          .addEventListener("click", calculate);
        document
          .getElementById("payFrequency")
          .addEventListener("change", calculate);
        document
          .getElementById("taxYear")
          .addEventListener("change", calculate);
        document
          .getElementById("filingStatus")
          .addEventListener("change", calculate);

        [
          "annualIncome",
          "hourlyRate",
          "hoursPerWeek",
          "weeksPerYear",
          "pretax401k",
          "pretaxHsa",
          "otherPretax",
          "postTax",
          "additionalIncome",
        ].forEach((id) => {
          const el = document.getElementById(id);
          el.addEventListener("input", () => {
            // Debounce slightly
            clearTimeout(el._t);
            el._t = setTimeout(calculate, 200);
          });
        });

        calculate();
      });
