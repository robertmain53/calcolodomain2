    <script>
      (function () {
        const SQMI_TO_ACRES = 640; // exact

        const sqmiInput = document.getElementById("sqmi-input");
        const acresInput = document.getElementById("acres-input");
        const precisionSelect = document.getElementById("precision-select");
        const swapButton = document.getElementById("swap-button");
        const clearButton = document.getElementById("clear-button");
        const summaryEl = document.getElementById("conversion-summary");

        let activeSide = "sqmi"; // 'sqmi' or 'acres'
        let isSwappedVisual = false;

        function formatNumber(value, decimals) {
          if (value === null || value === undefined || isNaN(value)) return "";
          const factor = Math.pow(10, decimals);
          const rounded = Math.round(value * factor) / factor;
          return rounded.toLocaleString(undefined, {
            minimumFractionDigits: 0,
            maximumFractionDigits: decimals,
          });
        }

        function parseInput(value) {
          if (value === "" || value === null || value === undefined)
            return null;
          const num = Number(value);
          return isNaN(num) ? null : num;
        }

        function updateSummary(fromUnit, fromValue, toUnit, toValue) {
          if (fromValue === null || toValue === null) {
            summaryEl.classList.add("hidden");
            summaryEl.textContent = "";
            return;
          }
          const decimals = Number(precisionSelect.value);
          const fromStr = formatNumber(fromValue, decimals);
          const toStr = formatNumber(toValue, decimals);
          summaryEl.textContent = `${fromStr} ${fromUnit} = ${toStr} ${toUnit}`;
          summaryEl.classList.remove("hidden");
        }

        function convertFromSqmi() {
          const decimals = Number(precisionSelect.value);
          const sqmiVal = parseInput(sqmiInput.value);
          if (sqmiVal === null) {
            acresInput.value = "";
            updateSummary(null, null, null, null);
            return;
          }
          const acresVal = sqmiVal * SQMI_TO_ACRES;
          acresInput.value = formatNumber(acresVal, decimals);
          updateSummary("square miles", sqmiVal, "acres", acresVal);
        }

        function convertFromAcres() {
          const decimals = Number(precisionSelect.value);
          const acresVal = parseInput(acresInput.value);
          if (acresVal === null) {
            sqmiInput.value = "";
            updateSummary(null, null, null, null);
            return;
          }
          const sqmiVal = acresVal / SQMI_TO_ACRES;
          sqmiInput.value = formatNumber(sqmiVal, decimals);
          updateSummary("acres", acresVal, "square miles", sqmiVal);
        }

        sqmiInput.addEventListener("input", () => {
          activeSide = "sqmi";
          convertFromSqmi();
        });

        acresInput.addEventListener("input", () => {
          activeSide = "acres";
          convertFromAcres();
        });

        precisionSelect.addEventListener("change", () => {
          if (activeSide === "sqmi") {
            convertFromSqmi();
          } else if (activeSide === "acres") {
            convertFromAcres();
          }
        });

        clearButton.addEventListener("click", () => {
          sqmiInput.value = "";
          acresInput.value = "";
          activeSide = "sqmi";
          updateSummary(null, null, null, null);
          sqmiInput.focus();
        });

        swapButton.addEventListener("click", () => {
          // Swap values
          const tmp = sqmiInput.value;
          sqmiInput.value = acresInput.value;
          acresInput.value = tmp;

          // Toggle active side so recalculation is consistent
          activeSide = activeSide === "sqmi" ? "acres" : "sqmi";

          // Recalculate based on new active side
          if (activeSide === "sqmi") {
            convertFromSqmi();
            sqmiInput.focus();
          } else {
            convertFromAcres();
            acresInput.focus();
          }

          // Visual rotation
          isSwappedVisual = !isSwappedVisual;
          if (isSwappedVisual) {
            swapButton.classList.add("swapped");
          } else {
            swapButton.classList.remove("swapped");
          }
        });
      })();
    </script>

