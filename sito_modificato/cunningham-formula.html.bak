        document.getElementById('cunningham-form').addEventListener('submit', function(event) {
            event.preventDefault();
            const bodyWeight = parseFloat(document.getElementById('bodyWeight').value);
            const leanBodyMass = parseFloat(document.getElementById('leanBodyMass').value);
            const rmr = 500 + (22 * leanBodyMass);
            document.getElementById('rmrValue').textContent = rmr.toFixed(2) + ' kcal/day';
        });
