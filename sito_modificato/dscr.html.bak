    <script>
      // DSCR calculator logic
      (function () {
        const modeButtons = document.querySelectorAll(".mode-btn");
        const calculateBtn = document.getElementById("calculateBtn");
        const resetBtn = document.getElementById("resetBtn");

        const cashFlowInput = document.getElementById("cashFlow");
        const interestInput = document.getElementById("interest");
        const principalInput = document.getElementById("principal");
        const targetDscrInput = document.getElementById("targetDscr");

        const resultsContainer = document.getElementById("resultsContainer");
        const dscrValueEl = document.getElementById("dscrValue");
        const dscrInterpretationEl =
          document.getElementById("dscrInterpretation");
        const covenantBadge = document.getElementById("covenantBadge");
        const resultTitle = document.getElementById("resultTitle");

        const summaryCash = document.getElementById("summaryCash");
        const summaryDebtService =
          document.getElementById("summaryDebtService");
        const summaryMargin = document.getElementById("summaryMargin");

        const scenarioInterestUp =
          document.getElementById("scenarioInterestUp");
        const scenarioCashDown = document.getElementById("scenarioCashDown");
        const scenarioPrincipalUp = document.getElementById(
          "scenarioPrincipalUp"
        );

        let currentMode = "dscr";

        function formatCurrency(value) {
          if (!isFinite(value)) return "€0";
          return new Intl.NumberFormat("en-US", {
            style: "currency",
            currency: "EUR",
            maximumFractionDigits: 0,
          }).format(value);
        }

        function formatRatio(value) {
          if (!isFinite(value)) return "–";
          return value.toFixed(2) + "×";
        }

        function setMode(mode) {
          currentMode = mode;
          modeButtons.forEach((btn) => {
            if (btn.dataset.mode === mode) {
              btn.classList.remove("bg-gray-100", "text-gray-700");
              btn.classList.add("bg-blue-600", "text-white");
            } else {
              btn.classList.remove("bg-blue-600", "text-white");
              btn.classList.add("bg-gray-100", "text-gray-700");
            }
          });

          if (mode === "dscr") {
            resultTitle.textContent = "DSCR Result";
          } else if (mode === "cashflow") {
            resultTitle.textContent = "Required Cash Flow for Target DSCR";
          } else {
            resultTitle.textContent = "Maximum Debt Service for Target DSCR";
          }
        }

        modeButtons.forEach((btn) => {
          btn.addEventListener("click", () => setMode(btn.dataset.mode));
        });

        function interpretDSCR(dscr, target) {
          if (!isFinite(dscr) || dscr <= 0) {
            covenantBadge.classList.add("hidden");
            return "Enter positive values for cash flow and debt service to compute DSCR.";
          }

          let text;
          if (dscr < 1) {
            text =
              "DSCR is below 1.0: operating cash flow is not sufficient to cover scheduled debt service. This is a strong warning signal and may indicate financial distress or covenant breach.";
          } else if (dscr < 1.1) {
            text =
              "DSCR is just above 1.0: coverage is extremely thin. Any negative shock to cash flow or increase in interest rates could lead to problems.";
          } else if (dscr < 1.2) {
            text =
              "DSCR is between 1.1 and 1.2: borderline coverage. Many banks would consider this the minimum acceptable level for stressed borrowers.";
          } else if (dscr < 1.5) {
            text =
              "DSCR is between 1.2 and 1.5: acceptable coverage for many loans, with some margin to absorb shocks.";
          } else {
            text =
              "DSCR is above 1.5: strong coverage. The business generates significantly more cash than required to service its debt.";
          }

          if (isFinite(target) && target > 0) {
            covenantBadge.classList.remove("hidden");
            if (dscr >= target) {
              covenantBadge.textContent = "Covenant met";
              covenantBadge.className = "badge bg-green-100 text-green-700";
            } else {
              covenantBadge.textContent = "Covenant breached";
              covenantBadge.className = "badge bg-red-100 text-red-700";
            }
          } else {
            covenantBadge.classList.add("hidden");
          }

          return text;
        }

        function calculate() {
          const cash = parseFloat(cashFlowInput.value);
          const interest = parseFloat(interestInput.value);
          const principal = parseFloat(principalInput.value);
          const target = parseFloat(targetDscrInput.value);

          const debtService =
            (isFinite(interest) ? interest : 0) +
            (isFinite(principal) ? principal : 0);

          if (currentMode === "dscr") {
            if (!isFinite(cash) || !isFinite(debtService) || debtService <= 0) {
              alert(
                "Please enter valid positive values for cash flow, interest, and principal."
              );
              return;
            }
            const dscr = cash / debtService;
            dscrValueEl.textContent = formatRatio(dscr);
            dscrInterpretationEl.textContent = interpretDSCR(dscr, target);

            summaryCash.textContent = formatCurrency(cash);
            summaryDebtService.textContent = formatCurrency(debtService);
            summaryMargin.textContent = formatCurrency(cash - debtService);

            // Scenarios
            const dscrInterestUp = cash / (debtService * 1.1);
            const dscrCashDown = (cash * 0.9) / debtService;
            const dscrPrincipalUp = cash / (interest + principal * 1.1);

            scenarioInterestUp.textContent = formatRatio(dscrInterestUp);
            scenarioCashDown.textContent = formatRatio(dscrCashDown);
            scenarioPrincipalUp.textContent = formatRatio(dscrPrincipalUp);
          } else if (currentMode === "cashflow") {
            if (
              !isFinite(debtService) ||
              debtService <= 0 ||
              !isFinite(target) ||
              target <= 0
            ) {
              alert(
                "Please enter valid positive values for interest, principal, and target DSCR."
              );
              return;
            }
            const requiredCash = target * debtService;
            dscrValueEl.textContent = formatCurrency(requiredCash);
            dscrInterpretationEl.textContent =
              "This is the minimum cash flow (e.g. EBITDA or operating cash flow) required to achieve the target DSCR given the current debt service.";

            summaryCash.textContent = formatCurrency(requiredCash);
            summaryDebtService.textContent = formatCurrency(debtService);
            summaryMargin.textContent = formatCurrency(
              requiredCash - debtService
            );

            scenarioInterestUp.textContent = "–";
            scenarioCashDown.textContent = "–";
            scenarioPrincipalUp.textContent = "–";

            covenantBadge.classList.add("hidden");
          } else if (currentMode === "maxdebt") {
            if (
              !isFinite(cash) ||
              cash <= 0 ||
              !isFinite(target) ||
              target <= 0
            ) {
              alert(
                "Please enter valid positive values for cash flow and target DSCR."
              );
              return;
            }
            const maxDebtService = cash / target;
            dscrValueEl.textContent = formatCurrency(maxDebtService);
            dscrInterpretationEl.textContent =
              "This is the maximum total annual debt service (interest + principal) compatible with the target DSCR, given the current cash flow.";

            summaryCash.textContent = formatCurrency(cash);
            summaryDebtService.textContent = formatCurrency(maxDebtService);
            summaryMargin.textContent = formatCurrency(cash - maxDebtService);

            scenarioInterestUp.textContent = "–";
            scenarioCashDown.textContent = "–";
            scenarioPrincipalUp.textContent = "–";

            covenantBadge.classList.add("hidden");
          }

          resultsContainer.classList.remove("hidden");
        }

        function reset() {
          cashFlowInput.value = "";
          interestInput.value = "";
          principalInput.value = "";
          targetDscrInput.value = "1.20";
          resultsContainer.classList.add("hidden");
          covenantBadge.classList.add("hidden");
        }

        calculateBtn.addEventListener("click", calculate);
        resetBtn.addEventListener("click", reset);
      })();
    </script>

