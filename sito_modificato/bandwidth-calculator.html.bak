    <script>
      // Tab handling
      document.querySelectorAll(".tab-btn").forEach((btn) => {
        btn.addEventListener("click", () => {
          const target = btn.getAttribute("data-tab");
          document.querySelectorAll(".tab-btn").forEach((b) => {
            b.classList.remove("border-blue-600", "text-blue-600");
            b.classList.add("border-transparent", "text-gray-500");
          });
          btn.classList.add("border-blue-600", "text-blue-600");
          btn.classList.remove("border-transparent", "text-gray-500");

          document.querySelectorAll(".tab-panel").forEach((panel) => {
            panel.classList.add("hidden");
          });
          document.getElementById(target).classList.remove("hidden");
        });
      });

      // Helper: round to n decimals
      function round(value, decimals) {
        const factor = Math.pow(10, decimals);
        return Math.round(value * factor) / factor;
      }

      // INTERNET BANDWIDTH CALC
      function calcInternetBandwidth() {
        const nBrowsing =
          parseFloat(document.getElementById("act-browsing").value) || 0;
        const nHd =
          parseFloat(document.getElementById("act-hd-stream").value) || 0;
        const n4k =
          parseFloat(document.getElementById("act-4k-stream").value) || 0;
        const nCalls =
          parseFloat(document.getElementById("act-video-call").value) || 0;
        const nGaming =
          parseFloat(document.getElementById("act-gaming").value) || 0;
        const nBackup =
          parseFloat(document.getElementById("act-backup").value) || 0;

        // Typical per-activity Mbps (down/up)
        const browsingDown = 1.0,
          browsingUp = 0.3;
        const hdDown = 5.0,
          hdUp = 0.5;
        const k4Down = 20.0,
          k4Up = 1.0;
        const callDown = 3.0,
          callUp = 3.0;
        const gameDown = 3.0,
          gameUp = 1.0;
        const backupDown = 0.5,
          backupUp = 5.0;

        let rawDown =
          nBrowsing * browsingDown +
          nHd * hdDown +
          n4k * k4Down +
          nCalls * callDown +
          nGaming * gameDown +
          nBackup * backupDown;

        let rawUp =
          nBrowsing * browsingUp +
          nHd * hdUp +
          n4k * k4Up +
          nCalls * callUp +
          nGaming * gameUp +
          nBackup * backupUp;

        const c =
          (parseFloat(document.getElementById("concurrency-factor").value) ||
            0) / 100;
        const s =
          (parseFloat(document.getElementById("safety-margin").value) || 0) /
          100;
        const o =
          (parseFloat(document.getElementById("overhead-factor").value) || 0) /
          100;

        const effDown = rawDown * c;
        const effUp = rawUp * c;

        let reqDown = effDown * (1 + o) * (1 + s);
        let reqUp = effUp * (1 + o) * (1 + s);

        if (!isFinite(reqDown)) reqDown = 0;
        if (!isFinite(reqUp)) reqUp = 0;

        const downDisplay = reqDown > 0 ? round(reqDown, 1) : 0;
        const upDisplay = reqUp > 0 ? round(reqUp, 1) : 0;

        document.getElementById("res-down-mbps").textContent =
          downDisplay.toString();
        document.getElementById("res-up-mbps").textContent =
          upDisplay.toString();

        // Round to "plan" speeds (common tiers)
        const tiers = [
          10, 25, 50, 75, 100, 150, 200, 300, 500, 750, 1000, 2000,
        ];
        function nearestTier(value) {
          for (let i = 0; i < tiers.length; i++) {
            if (value <= tiers[i]) return tiers[i];
          }
          return tiers[tiers.length - 1];
        }
        document.getElementById("res-down-mbps-rounded").textContent =
          nearestTier(reqDown);
        document.getElementById("res-up-mbps-rounded").textContent =
          nearestTier(reqUp);
      }

      document
        .getElementById("btn-calc-internet")
        .addEventListener("click", calcInternetBandwidth);

      // FILE TRANSFER CALC
      function sizeToMB(size, unit) {
        const v = size || 0;
        switch (unit) {
          case "MB":
            return v;
          case "GB":
            return v * 1024;
          case "TB":
            return v * 1024 * 1024;
          default:
            return v;
        }
      }

      function mbToUnit(mb, unit) {
        switch (unit) {
          case "MB":
            return mb;
          case "GB":
            return mb / 1024;
          case "TB":
            return mb / (1024 * 1024);
          default:
            return mb;
        }
      }

      function bandwidthToMbps(value, unit) {
        const v = value || 0;
        switch (unit) {
          case "Mbps":
            return v;
          case "Gbps":
            return v * 1000;
          case "MBps":
            return v * 8;
          case "GBps":
            return v * 8 * 1000;
          default:
            return v;
        }
      }

      function mbpsToUnit(mbps, unit) {
        switch (unit) {
          case "Mbps":
            return mbps;
          case "Gbps":
            return mbps / 1000;
          case "MBps":
            return mbps / 8;
          case "GBps":
            return mbps / (8 * 1000);
          default:
            return mbps;
        }
      }

      function timeToSeconds(value, unit) {
        const v = value || 0;
        switch (unit) {
          case "seconds":
            return v;
          case "minutes":
            return v * 60;
          case "hours":
            return v * 3600;
          default:
            return v;
        }
      }

      function secondsToUnit(sec, unit) {
        switch (unit) {
          case "seconds":
            return sec;
          case "minutes":
            return sec / 60;
          case "hours":
            return sec / 3600;
          default:
            return sec;
        }
      }

      function updateTransferSummary(text) {
        document.getElementById("transfer-summary").textContent = text;
      }

      document
        .getElementById("btn-solve-time")
        .addEventListener("click", () => {
          const size = parseFloat(document.getElementById("file-size").value);
          const sizeUnit = document.getElementById("file-size-unit").value;
          const bw = parseFloat(
            document.getElementById("transfer-bandwidth").value
          );
          const bwUnit = document.getElementById(
            "transfer-bandwidth-unit"
          ).value;
          const timeUnit = document.getElementById("transfer-time-unit").value;

          if (!size || !bw) {
            updateTransferSummary(
              "Please enter both file size and bandwidth to solve for time."
            );
            return;
          }

          const sizeMB = sizeToMB(size, sizeUnit);
          const bwMbps = bandwidthToMbps(bw, bwUnit);
          const timeSec = (sizeMB * 8) / bwMbps;
          const timeOut = secondsToUnit(timeSec, timeUnit);

          document.getElementById("transfer-time").value = round(timeOut, 2);
          updateTransferSummary(
            `At ${round(bw, 2)} ${bwUnit}, transferring ${round(
              size,
              2
            )} ${sizeUnit} will take about ${round(timeOut, 2)} ${timeUnit}.`
          );
        });

      document
        .getElementById("btn-solve-bandwidth")
        .addEventListener("click", () => {
          const size = parseFloat(document.getElementById("file-size").value);
          const sizeUnit = document.getElementById("file-size-unit").value;
          const time = parseFloat(
            document.getElementById("transfer-time").value
          );
          const timeUnit = document.getElementById("transfer-time-unit").value;
          const bwUnit = document.getElementById(
            "transfer-bandwidth-unit"
          ).value;

          if (!size || !time) {
            updateTransferSummary(
              "Please enter both file size and transfer time to solve for bandwidth."
            );
            return;
          }

          const sizeMB = sizeToMB(size, sizeUnit);
          const timeSec = timeToSeconds(time, timeUnit);
          const bwMbps = (sizeMB * 8) / timeSec;
          const bwOut = mbpsToUnit(bwMbps, bwUnit);

          document.getElementById("transfer-bandwidth").value = round(bwOut, 2);
          updateTransferSummary(
            `To transfer ${round(size, 2)} ${sizeUnit} in ${round(
              time,
              2
            )} ${timeUnit}, you need about ${round(bwOut, 2)} ${bwUnit}.`
          );
        });

      document
        .getElementById("btn-solve-size")
        .addEventListener("click", () => {
          const bw = parseFloat(
            document.getElementById("transfer-bandwidth").value
          );
          const bwUnit = document.getElementById(
            "transfer-bandwidth-unit"
          ).value;
          const time = parseFloat(
            document.getElementById("transfer-time").value
          );
          const timeUnit = document.getElementById("transfer-time-unit").value;
          const sizeUnit = document.getElementById("file-size-unit").value;

          if (!bw || !time) {
            updateTransferSummary(
              "Please enter both bandwidth and transfer time to solve for file size."
            );
            return;
          }

          const bwMbps = bandwidthToMbps(bw, bwUnit);
          const timeSec = timeToSeconds(time, timeUnit);
          const sizeMB = (bwMbps * timeSec) / 8;
          const sizeOut = mbToUnit(sizeMB, sizeUnit);

          document.getElementById("file-size").value = round(sizeOut, 2);
          updateTransferSummary(
            `At ${round(bw, 2)} ${bwUnit} for ${round(
              time,
              2
            )} ${timeUnit}, you can transfer about ${round(
              sizeOut,
              2
            )} ${sizeUnit}.`
          );
        });

      // CCTV CALC
      function formatStorage(bytes) {
        const KB = 1024;
        const MB = KB * 1024;
        const GB = MB * 1024;
        const TB = GB * 1024;
        if (bytes >= TB) return `${round(bytes / TB, 2)} TB`;
        if (bytes >= GB) return `${round(bytes / GB, 2)} GB`;
        if (bytes >= MB) return `${round(bytes / MB, 2)} MB`;
        if (bytes >= KB) return `${round(bytes / KB, 2)} KB`;
        return `${round(bytes, 2)} B`;
      }

      document.getElementById("btn-calc-cctv").addEventListener("click", () => {
        const nCam =
          parseFloat(document.getElementById("cctv-cameras").value) || 0;
        let bitrate =
          parseFloat(document.getElementById("cctv-bitrate").value) || 0;
        const bitrateUnit = document.getElementById("cctv-bitrate-unit").value;
        const hours =
          parseFloat(document.getElementById("cctv-recording-hours").value) ||
          0;
        const days =
          parseFloat(document.getElementById("cctv-days").value) || 0;
        const overheadPct =
          (parseFloat(document.getElementById("cctv-overhead").value) || 0) /
          100;

        // Convert per-camera bitrate to Mbps
        if (bitrateUnit === "Kbps") {
          bitrate = bitrate / 1000;
        }

        let totalMbps = nCam * bitrate;
        totalMbps *= 1 + overheadPct;

        if (!isFinite(totalMbps)) totalMbps = 0;

        document.getElementById("cctv-total-mbps").textContent = round(
          totalMbps,
          2
        );

        // Storage
        const mbPerSec = totalMbps / 8; // MB/s
        const bytesPerSec = mbPerSec * 1024 * 1024;
        const secondsPerDay = hours * 3600;
        const dailyBytes = bytesPerSec * secondsPerDay;
        const totalBytes = dailyBytes * days;

        document.getElementById("cctv-storage-day").textContent =
          secondsPerDay > 0 ? formatStorage(dailyBytes) : "–";
        document.getElementById("cctv-storage-total").textContent =
          secondsPerDay > 0 && days > 0 ? formatStorage(totalBytes) : "–";
      });

      // Initial calculations
      calcInternetBandwidth();
    </script>

