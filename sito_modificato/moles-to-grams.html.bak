  <script>
    (function() {
      const molarMass = document.getElementById('molarMass');
      const molesInput = document.getElementById('molesInput');
      const gramsInput = document.getElementById('gramsInput');
      const resultGrams = document.getElementById('resultGrams');
      const resultMoles = document.getElementById('resultMoles');
      const pills = document.querySelectorAll('[data-mm]');

      let updating = false;

      function toFixedSmart(num) {
        if (!isFinite(num)) return '—';
        if (Math.abs(num) >= 1) return Number(num.toFixed(5)).toString();
        return Number(num.toExponential(5)).toString();
      }

      function recalcFromMoles() {
        if (updating) return;
        updating = true;
        const mm = parseFloat(molarMass.value);
        const mol = parseFloat(molesInput.value);
        if (!isNaN(mm) && mm > 0 && !isNaN(mol)) {
          const g = mm * mol;
          gramsInput.value = toFixedSmart(g);
          resultGrams.textContent = toFixedSmart(g);
        } else {
          resultGrams.textContent = '—';
        }
        // also compute moles from grams in case grams field has value
        const g2 = parseFloat(gramsInput.value);
        if (!isNaN(mm) && mm > 0 && !isNaN(g2)) {
          const mol2 = g2 / mm;
          resultMoles.textContent = toFixedSmart(mol2);
        } else {
          resultMoles.textContent = '—';
        }
        updating = false;
      }

      function recalcFromGrams() {
        if (updating) return;
        updating = true;
        const mm = parseFloat(molarMass.value);
        const g = parseFloat(gramsInput.value);
        if (!isNaN(mm) && mm > 0 && !isNaN(g)) {
          const mol = g / mm;
          molesInput.value = toFixedSmart(mol);
          resultMoles.textContent = toFixedSmart(mol);
        } else {
          resultMoles.textContent = '—';
        }
        // also compute grams from moles in case moles field has value
        const mol2 = parseFloat(molesInput.value);
        if (!isNaN(mm) && mm > 0 && !isNaN(mol2)) {
          const g2 = mm * mol2;
          resultGrams.textContent = toFixedSmart(g2);
        } else {
          resultGrams.textContent = '—';
        }
        updating = false;
      }

      molarMass.addEventListener('input', function() {
        recalcFromMoles();
      });
      molesInput.addEventListener('input', recalcFromMoles);
      gramsInput.addEventListener('input', recalcFromGrams);

      pills.forEach(btn => {
        btn.addEventListener('click', () => {
          molarMass.value = btn.dataset.mm;
          recalcFromMoles();
        });
      });

      // init defaults
      molarMass.value = 18.015;
      molesInput.value = 1;
      recalcFromMoles();
    })();
  </script>

