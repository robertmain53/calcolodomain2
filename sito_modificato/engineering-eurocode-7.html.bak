<script>
  // Simple tab switching
  document.querySelectorAll('.ec7-tab-btn').forEach(btn => {
    btn.addEventListener('click', () => {
      const target = btn.getAttribute('data-ec7-tab');
      document.querySelectorAll('.ec7-tab-btn').forEach(b => {
        b.classList.remove('border-blue-600','text-blue-600');
        b.classList.add('border-transparent','text-gray-600');
      });
      btn.classList.add('border-blue-600','text-blue-600');
      btn.classList.remove('border-transparent','text-gray-600');

      document.querySelectorAll('.ec7-tab-panel').forEach(p => p.classList.add('hidden'));
      const panel = document.getElementById('ec7-tab-' + target);
      if (panel) panel.classList.remove('hidden');
    });
  });

  function bearingCapacityFactors(phiDeg) {
    const phi = phiDeg * Math.PI / 180;
    const Nq = Math.exp(Math.PI * Math.tan(phi)) * Math.tan(Math.PI/4 + phi/2) * Math.tan(Math.PI/4 + phi/2);
    const Nc = (Nq - 1) / Math.tan(phi || 1e-6);
    const Ngamma = 2 * (Nq - 1) * Math.tan(phi);
    return {Nc, Nq, Ngamma};
  }

  function formatNumber(x, digits = 2) {
    if (!isFinite(x)) return '-';
    return Number(x).toFixed(digits);
  }

  // Bearing calculation
  document.getElementById('ec7-calc-bearing').addEventListener('click', () => {
    const gammaK = parseFloat(document.getElementById('ec7-gamma-k').value);
    const cK = parseFloat(document.getElementById('ec7-c-k').value);
    const phiK = parseFloat(document.getElementById('ec7-phi-k').value);
    const B = parseFloat(document.getElementById('ec7-b').value);
    const Df = parseFloat(document.getElementById('ec7-df').value);
    const Vk = parseFloat(document.getElementById('ec7-vk').value);

    const gammaG = parseFloat(document.getElementById('ec7-gamma-g').value);
    const gammaQ = parseFloat(document.getElementById('ec7-gamma-q').value);
    const gammaR = parseFloat(document.getElementById('ec7-gamma-r-b').value);

    if ([gammaK,cK,phiK,B,Df,Vk,gammaG,gammaQ,gammaR].some(v => isNaN(v) || v < 0)) {
      alert('Please enter non-negative numeric values for all inputs.');
      return;
    }

    // Assume all vertical load is permanent for simplicity
    const Vd = Vk * gammaG;

    const {Nc, Nq, Ngamma} = bearingCapacityFactors(phiK);
    const qult_k = cK * Nc + gammaK * Df * Nq + 0.5 * gammaK * B * Ngamma; // kPa
    const Rd = (qult_k * B) / gammaR; // kN per metre length

    const util = Vd / Rd;

    document.getElementById('ec7-vd').textContent = formatNumber(Vd, 1);
    document.getElementById('ec7-rd').textContent = formatNumber(Rd, 1);
    document.getElementById('ec7-util-b').textContent = formatNumber(util, 2);

    const statusEl = document.getElementById('ec7-status-b');
    statusEl.textContent = util <= 1 ? 'PASS – Bearing OK' : 'FAIL – Bearing exceeded';
    statusEl.className = 'inline-flex items-center px-2 py-1 rounded text-xs font-semibold ' +
      (util <= 1 ? 'bg-green-100 text-green-800' : 'bg-red-100 text-red-800');

    document.getElementById('ec7-bearing-result').classList.remove('hidden');
  });

  // Sliding calculation
  document.getElementById('ec7-calc-sliding').addEventListener('click', () => {
    const Vk = parseFloat(document.getElementById('ec7-vk-s').value);
    const Hk = parseFloat(document.getElementById('ec7-hk-s').value);
    const muK = parseFloat(document.getElementById('ec7-mu-k').value);
    const caK = parseFloat(document.getElementById('ec7-ca-k').value);

    const gammaV = parseFloat(document.getElementById('ec7-gamma-fv').value);
    const gammaH = parseFloat(document.getElementById('ec7-gamma-fh').value);
    const gammaR = parseFloat(document.getElementById('ec7-gamma-r-s').value);

    if ([Vk,Hk,muK,caK,gammaV,gammaH,gammaR].some(v => isNaN(v) || v < 0)) {
      alert('Please enter non-negative numeric values for all inputs.');
      return;
    }

    const Vd = Vk * gammaV;
    const Hd = Hk * gammaH;

    const muD = muK; // simple assumption
    const caD = caK; // kPa

    const B = parseFloat(document.getElementById('ec7-b').value) || 1;
    const A = B * 1; // per metre length

    const Rd_sliding = (Vd * muD + A * caD) / gammaR;

    const util = Hd / Rd_sliding;

    document.getElementById('ec7-hd').textContent = formatNumber(Hd, 1);
    document.getElementById('ec7-rd-s').textContent = formatNumber(Rd_sliding, 1);
    document.getElementById('ec7-util-s').textContent = formatNumber(util, 2);

    const statusEl = document.getElementById('ec7-status-s');
    statusEl.textContent = util <= 1 ? 'PASS – Sliding OK' : 'FAIL – Sliding exceeded';
    statusEl.className = 'inline-flex items-center px-2 py-1 rounded text-xs font-semibold ' +
      (util <= 1 ? 'bg-green-100 text-green-800' : 'bg-red-100 text-red-800');

    document.getElementById('ec7-sliding-result').classList.remove('hidden');
  });

  window.MathJax = {
    tex: { inlineMath: [['\\(','\\)'], ['$', '$']], displayMath: [['$','$'], ['\\[','\\]']] },
    svg: { fontCache: 'global' }
  };
</script>
