    <script>
      function safeInt(value) {
        const n = parseInt(value, 10);
        return isNaN(n) || n < 0 ? NaN : n;
      }

      function updateTotals() {
        const a = safeInt(document.getElementById("a").value);
        const b = safeInt(document.getElementById("b").value);
        const c = safeInt(document.getElementById("c").value);
        const d = safeInt(document.getElementById("d").value);

        const row1 = (isNaN(a) ? 0 : a) + (isNaN(b) ? 0 : b);
        const row2 = (isNaN(c) ? 0 : c) + (isNaN(d) ? 0 : d);
        const col1 = (isNaN(a) ? 0 : a) + (isNaN(c) ? 0 : c);
        const col2 = (isNaN(b) ? 0 : b) + (isNaN(d) ? 0 : d);
        const grand = row1 + row2;

        document.getElementById("row1-total").textContent = row1;
        document.getElementById("row2-total").textContent = row2;
        document.getElementById("col1-total").textContent = col1;
        document.getElementById("col2-total").textContent = col2;
        document.getElementById("grand-total").textContent = grand;
      }

      function formatNumber(x, decimals) {
        if (!isFinite(x)) return "—";
        return x.toFixed(decimals);
      }

      function formatPercent(x, decimals) {
        if (!isFinite(x)) return "—";
        return (x * 100).toFixed(decimals) + "%";
      }

      function getZForCI(ci) {
        if (ci >= 0.999) return 3.29;
        if (ci >= 0.99) return 2.576;
        if (ci >= 0.95) return 1.96;
        if (ci >= 0.9) return 1.645;
        return 1.96;
      }

      function calculateRR() {
        const a = safeInt(document.getElementById("a").value);
        const b = safeInt(document.getElementById("b").value);
        const c = safeInt(document.getElementById("c").value);
        const d = safeInt(document.getElementById("d").value);
        const ciLevel = parseFloat(document.getElementById("ci-level").value);
        const errorEl = document.getElementById("input-error");

        if ([a, b, c, d].some((v) => isNaN(v))) {
          errorEl.textContent =
            "Please enter non-negative integer counts for all four cells (a, b, c, d).";
          errorEl.classList.remove("hidden");
          return;
        }

        const n1 = a + b;
        const n2 = c + d;

        if (n1 === 0 || n2 === 0) {
          errorEl.textContent =
            "Each row (exposed and unexposed) must have at least one subject.";
          errorEl.classList.remove("hidden");
          return;
        }

        if (c === 0 || a === 0) {
          errorEl.textContent =
            "Relative risk cannot be computed when either exposed or unexposed group has zero events (a = 0 or c = 0). Consider adding a small continuity correction.";
          errorEl.classList.remove("hidden");
          return;
        }

        errorEl.classList.add("hidden");

        const Re = a / n1;
        const Ru = c / n2;
        const RR = Re / Ru;

        const seLogRR = Math.sqrt(1 / a - 1 / n1 + 1 / c - 1 / n2);

        const logRR = Math.log(RR);
        const z = getZForCI(ciLevel);
        const lowerLog = logRR - z * seLogRR;
        const upperLog = logRR + z * seLogRR;
        const lowerRR = Math.exp(lowerLog);
        const upperRR = Math.exp(upperLog);

        document.getElementById("risk-exposed").textContent = formatNumber(
          Re,
          4
        );
        document.getElementById("risk-unexposed").textContent = formatNumber(
          Ru,
          4
        );
        document.getElementById("risk-diff").textContent = formatNumber(
          Re - Ru,
          4
        );

        const riskExposedPct = formatPercent(Re, 1);
        const riskUnexposedPct = formatPercent(Ru, 1);
        document.getElementById("risk-exposed").nextElementSibling.textContent =
          "(" + riskExposedPct + ")";
        document.getElementById(
          "risk-unexposed"
        ).nextElementSibling.textContent = "(" + riskUnexposedPct + ")";
        document.getElementById("risk-diff").nextElementSibling.textContent =
          "(" + formatPercent(Re - Ru, 1) + " percentage points)";

        document.getElementById("rr-value").textContent = formatNumber(RR, 2);
        document.getElementById("log-rr").textContent = formatNumber(logRR, 4);
        document.getElementById("se-log-rr").textContent = formatNumber(
          seLogRR,
          4
        );
        document.getElementById("rr-ci").textContent =
          formatNumber(lowerRR, 2) + " to " + formatNumber(upperRR, 2);

        const badge = document.getElementById("rr-interpretation-badge");
        const sigEl = document.getElementById("rr-significance");
        const includesOne = lowerRR <= 1 && upperRR >= 1;

        if (RR > 1) {
          badge.textContent = "Increased risk";
          badge.className = "result-badge bg-red-100 text-red-700";
        } else if (RR < 1) {
          badge.textContent = "Reduced risk";
          badge.className = "result-badge bg-green-100 text-green-700";
        } else {
          badge.textContent = "No difference";
          badge.className = "result-badge bg-gray-100 text-gray-700";
        }

        if (includesOne) {
          sigEl.textContent = "Not statistically significant (CI includes 1)";
          sigEl.className =
            "ml-2 text-xs font-semibold text-gray-700 bg-gray-100 px-2 py-0.5 rounded-full";
        } else {
          sigEl.textContent = "Significant (CI excludes 1)";
          sigEl.className =
            "ml-2 text-xs font-semibold text-red-700 bg-red-50 px-2 py-0.5 rounded-full";
        }

        const interpretationEl = document.getElementById(
          "rr-plain-interpretation"
        );
        let directionText = "the same risk as";
        if (RR > 1) directionText = formatNumber(RR, 2) + " times the risk of";
        if (RR < 1)
          directionText = formatNumber(1 / RR, 2) + " times lower risk than";

        const ciPercent = Math.round(ciLevel * 100);
        const sigText = includesOne
          ? `The ${ciPercent}% confidence interval (${formatNumber(
              lowerRR,
              2
            )} to ${formatNumber(
              upperRR,
              2
            )}) includes 1, so the association is not statistically significant at the ${
              ciPercent === 95 ? "0.05" : ""
            } level.`
          : `The ${ciPercent}% confidence interval (${formatNumber(
              lowerRR,
              2
            )} to ${formatNumber(
              upperRR,
              2
            )}) does not include 1, suggesting a statistically significant association at the ${
              ciPercent === 95 ? "0.05" : ""
            } level.`;

        interpretationEl.textContent =
          `The exposed group has ${directionText} the unexposed group (${riskExposedPct} vs ${riskUnexposedPct}). ` +
          sigText;
      }

      document.addEventListener("DOMContentLoaded", function () {
        ["a", "b", "c", "d"].forEach((id) => {
          document.getElementById(id).addEventListener("input", updateTotals);
        });
        document
          .getElementById("calculate-btn")
          .addEventListener("click", function (e) {
            e.preventDefault();
            updateTotals();
            calculateRR();
          });
        document
          .getElementById("reset-btn")
          .addEventListener("click", function (e) {
            e.preventDefault();
            document.getElementById("a").value = 30;
            document.getElementById("b").value = 70;
            document.getElementById("c").value = 10;
            document.getElementById("d").value = 90;
            document.getElementById("ci-level").value = "0.95";
            document.getElementById("input-error").classList.add("hidden");
            updateTotals();
            calculateRR();
          });

        updateTotals();
        calculateRR();
      });
    </script>

