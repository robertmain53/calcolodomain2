    <script>
      // Office Space Calculator Logic
      function round(value, decimals) {
        const factor = Math.pow(10, decimals);
        return Math.round(value * factor) / factor;
      }

      function updateProfileDefaults(profile) {
        const sqftPerDeskInput = document.getElementById("sqft-per-desk");
        const circulationSelect = document.getElementById("circulation-factor");

        if (profile === "lean") {
          sqftPerDeskInput.value = 50;
          circulationSelect.value = "0.25";
        } else if (profile === "standard") {
          sqftPerDeskInput.value = 60;
          circulationSelect.value = "0.35";
        } else if (profile === "spacious") {
          sqftPerDeskInput.value = 75;
          circulationSelect.value = "0.45";
        }
      }

      function calculateOfficeSpace() {
        const numEmployees = Math.max(
          0,
          Number(document.getElementById("num-employees").value) || 0
        );
        const hybridPercent = Math.min(
          100,
          Math.max(
            0,
            Number(document.getElementById("hybrid-percent").value) || 0
          )
        );
        const avgDaysOnsite = Math.min(
          5,
          Math.max(
            1,
            Number(document.getElementById("avg-days-onsite").value) || 3
          )
        );
        const deskSharingRatio = Math.max(
          1,
          Number(document.getElementById("desk-sharing-ratio").value) || 1
        );

        const sqftPerDesk = Math.max(
          30,
          Number(document.getElementById("sqft-per-desk").value) || 60
        );
        const circulationFactor =
          Number(document.getElementById("circulation-factor").value) || 0.35;

        const numSmallMeeting = Math.max(
          0,
          Number(document.getElementById("num-small-meeting").value) || 0
        );
        const numLargeMeeting = Math.max(
          0,
          Number(document.getElementById("num-large-meeting").value) || 0
        );
        const numFocusRooms = Math.max(
          0,
          Number(document.getElementById("num-focus-rooms").value) || 0
        );
        const numCollabAreas = Math.max(
          0,
          Number(document.getElementById("num-collab-areas").value) || 0
        );

        const supportSqft = Math.max(
          0,
          Number(document.getElementById("support-sqft").value) || 0
        );
        const receptionSqft = Math.max(
          0,
          Number(document.getElementById("reception-sqft").value) || 0
        );

        const rentPerSqftYear = Math.max(
          0,
          Number(document.getElementById("rent-per-sqft-year").value) || 0
        );
        const leaseTermYears = Math.max(
          1,
          Number(document.getElementById("lease-term-years").value) || 1
        );

        // 1) Peak occupancy
        const hybridShare = hybridPercent / 100;
        const onsiteEmployees = numEmployees * (1 - hybridShare);
        const hybridEmployees = numEmployees * hybridShare;
        const hybridDaily = hybridEmployees * (avgDaysOnsite / 5);
        const peakOccupancy = Math.round(onsiteEmployees + hybridDaily);

        // 2) Desks and workstation area
        const desksRequired =
          peakOccupancy > 0 ? Math.ceil(peakOccupancy / deskSharingRatio) : 0;
        const workstationArea = desksRequired * sqftPerDesk;

        // 3) Meeting & focus rooms
        const smallMeetingSqft = 120;
        const largeMeetingSqft = 220;
        const focusRoomSqft = 40;
        const collabAreaSqft = 180;

        const meetingSqft =
          numSmallMeeting * smallMeetingSqft +
          numLargeMeeting * largeMeetingSqft +
          numFocusRooms * focusRoomSqft +
          numCollabAreas * collabAreaSqft;

        // 4) Support & circulation
        const supportTotalSqft = supportSqft + receptionSqft;
        const baseArea = workstationArea + meetingSqft + supportTotalSqft;
        const circulationSqft = baseArea * circulationFactor;
        const totalSqft = baseArea + circulationSqft;
        const totalSqm = totalSqft / 10.7639;

        const sqftPerEmployeeGross =
          numEmployees > 0 ? totalSqft / numEmployees : 0;
        const sqftPerDeskGross =
          desksRequired > 0 ? totalSqft / desksRequired : 0;

        // 5) Rent
        const annualRent = totalSqft * rentPerSqftYear;
        const monthlyRent = annualRent / 12;
        const totalLeaseCost = annualRent * leaseTermYears;

        // Update DOM
        document.getElementById("result-peak-occupancy").textContent =
          peakOccupancy.toLocaleString();
        document.getElementById("result-desks").textContent =
          desksRequired.toLocaleString();
        document.getElementById("result-workstation-sqft").textContent =
          Math.round(workstationArea).toLocaleString();
        document.getElementById("result-meeting-sqft").textContent =
          Math.round(meetingSqft).toLocaleString();
        document.getElementById("result-support-sqft").textContent =
          Math.round(supportTotalSqft).toLocaleString();
        document.getElementById("result-circulation-sqft").textContent =
          Math.round(circulationSqft).toLocaleString();
        document.getElementById("result-total-sqft").textContent =
          Math.round(totalSqft).toLocaleString();
        document.getElementById("result-total-sqm").textContent = round(
          totalSqm,
          1
        ).toLocaleString();
        document.getElementById("result-sqft-per-employee").textContent =
          Math.round(sqftPerEmployeeGross).toLocaleString();
        document.getElementById("result-sqft-per-desk-gross").textContent =
          Math.round(sqftPerDeskGross).toLocaleString();

        document.getElementById("result-monthly-rent").textContent =
          Math.round(monthlyRent).toLocaleString();
        document.getElementById("result-annual-rent").textContent =
          Math.round(annualRent).toLocaleString();
        document.getElementById("result-total-lease-cost").textContent =
          Math.round(totalLeaseCost).toLocaleString();
        document.getElementById("result-lease-years").textContent =
          leaseTermYears.toString();
      }

      document.addEventListener("DOMContentLoaded", function () {
        // Profile radio buttons
        document
          .querySelectorAll('input[name="space-profile"]')
          .forEach(function (radio) {
            radio.addEventListener("change", function () {
              updateProfileDefaults(this.value);
              calculateOfficeSpace();
            });
          });

        // Hybrid slider label
        const hybridSlider = document.getElementById("hybrid-percent");
        const hybridLabel = document.getElementById("hybrid-percent-label");
        hybridSlider.addEventListener("input", function () {
          hybridLabel.textContent = this.value + "% hybrid/remote";
        });

        // Calculate button
        document
          .getElementById("calculate-btn")
          .addEventListener("click", calculateOfficeSpace);

        // Auto-calc on input changes for a responsive UX
        document
          .querySelectorAll(
            "#num-employees, #avg-days-onsite, #desk-sharing-ratio, #sqft-per-desk, #circulation-factor, #num-small-meeting, #num-large-meeting, #num-focus-rooms, #num-collab-areas, #support-sqft, #reception-sqft, #rent-per-sqft-year, #lease-term-years"
          )
          .forEach(function (el) {
            el.addEventListener("change", calculateOfficeSpace);
          });
        hybridSlider.addEventListener("change", calculateOfficeSpace);

        // Initial calculation
        calculateOfficeSpace();
      });
    </script>

