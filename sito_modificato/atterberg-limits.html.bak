    <script>
      // Tab switching
      const tabSimple = document.getElementById("tab-simple");
      const tabDetailed = document.getElementById("tab-detailed");
      const panelSimple = document.getElementById("panel-simple");
      const panelDetailed = document.getElementById("panel-detailed");
      const noteLlSource = document.getElementById("note_ll_source");

      function setTab(active) {
        if (active === "simple") {
          panelSimple.classList.remove("hidden");
          panelDetailed.classList.add("hidden");
          tabSimple.classList.add("bg-blue-600", "text-white");
          tabSimple.classList.remove("bg-gray-100", "text-gray-700");
          tabDetailed.classList.remove("bg-blue-600", "text-white");
          tabDetailed.classList.add("bg-gray-100", "text-gray-700");
          noteLlSource.classList.add("hidden");
        } else {
          panelSimple.classList.add("hidden");
          panelDetailed.classList.remove("hidden");
          tabDetailed.classList.add("bg-blue-600", "text-white");
          tabDetailed.classList.remove("bg-gray-100", "text-gray-700");
          tabSimple.classList.remove("bg-blue-600", "text-white");
          tabSimple.classList.add("bg-gray-100", "text-gray-700");
        }
      }
      tabSimple.addEventListener("click", () => setTab("simple"));
      tabDetailed.addEventListener("click", () => setTab("detailed"));

      // Flow curve rows
      const flowRows = document.getElementById("flow_rows");
      const addRowBtn = document.getElementById("add_row");
      const testTypeSel = document.getElementById("test_type");
      const colHeaderN = document.getElementById("col_header_n");

      function updateNHeader() {
        if (testTypeSel.value === "casagrande") {
          colHeaderN.textContent = "Blows N";
        } else {
          colHeaderN.textContent = "Penetration (mm)";
        }
      }
      testTypeSel.addEventListener("change", updateNHeader);

      function addFlowRow(w = "", n = "") {
        const tr = document.createElement("tr");
        tr.innerHTML = `
      <td class="px-3 py-1.5">
        <input type="number" step="0.1" class="w-full border border-gray-300 rounded-md px-2 py-1 text-sm flow-w" value="${w}">
      </td>
      <td class="px-3 py-1.5">
        <input type="number" step="0.01" class="w-full border border-gray-300 rounded-md px-2 py-1 text-sm flow-n" value="${n}">
      </td>
      <td class="px-3 py-1.5">
        <button type="button" class="text-xs text-red-600 hover:underline remove-row">Remove</button>
      </td>
    `;
        flowRows.appendChild(tr);
        tr.querySelector(".remove-row").addEventListener("click", () => {
          flowRows.removeChild(tr);
        });
      }

      addRowBtn.addEventListener("click", () => addFlowRow());

      // Initialize with 4 example rows
      ["45,15", "42,20", "39,25", "36,35"].forEach((pair) => {
        const [w, n] = pair.split(",");
        addFlowRow(w, n);
      });
      updateNHeader();

      // Calculation helpers
      function parseNumber(id) {
        const el = document.getElementById(id);
        if (!el) return { value: null, el: null };
        const v = el.value.trim();
        if (v === "") return { value: null, el };
        const num = Number(v);
        return isNaN(num) ? { value: null, el } : { value: num, el };
      }

      function setResult(id, value, decimals = 2, suffix = " %") {
        const el = document.getElementById(id);
        if (!el) return;
        if (value === null || isNaN(value)) {
          el.textContent = "–";
        } else {
          el.textContent = value.toFixed(decimals) + suffix;
        }
      }

      function classifyPlasticity(LL, PI) {
        if (PI === null || isNaN(PI)) return "–";
        if (PI < 4) return "Non-plastic / very low plasticity";
        if (PI < 7) return "Low plasticity";
        if (PI < 17) return "Medium plasticity";
        return "High plasticity";
      }

      function computeLLFromFlow() {
        const rows = Array.from(flowRows.querySelectorAll("tr"));
        const xs = [];
        const ys = [];
        rows.forEach((tr) => {
          const wEl = tr.querySelector(".flow-w");
          const nEl = tr.querySelector(".flow-n");
          const w = Number(wEl.value);
          const n = Number(nEl.value);
          if (!isNaN(w) && !isNaN(n) && n > 0) {
            ys.push(w);
            xs.push(Math.log10(n));
          }
        });
        if (xs.length < 3) return null;

        // Linear regression y = a + b x
        const n = xs.length;
        const sumX = xs.reduce((a, b) => a + b, 0);
        const sumY = ys.reduce((a, b) => a + b, 0);
        const sumXY = xs.reduce((a, b, i) => a + b * ys[i], 0);
        const sumX2 = xs.reduce((a, b) => a + b * b, 0);
        const denom = n * sumX2 - sumX * sumX;
        if (denom === 0) return null;
        const b = (n * sumXY - sumX * sumY) / denom;
        const a = (sumY - b * sumX) / n;

        const ref = testTypeSel.value === "casagrande" ? 25 : 20; // 20 mm for cone
        const xRef = Math.log10(ref);
        const LL = a + b * xRef;
        return LL;
      }

      const calcBtn = document.getElementById("calc_btn");
      const resetBtn = document.getElementById("reset_btn");
      const errMsg = document.getElementById("error_msg");

      function clearErrors() {
        errMsg.textContent = "";
        document
          .querySelectorAll(".input-error")
          .forEach((el) => el.classList.remove("input-error"));
      }

      calcBtn.addEventListener("click", () => {
        clearErrors();
        let LL = null,
          PL = null,
          wn = null,
          clay = null,
          SL = null;
        let llFromFlow = false;

        if (!panelSimple.classList.contains("hidden")) {
          // Simple mode
          const llObj = parseNumber("ll_simple");
          const plObj = parseNumber("pl_simple");
          const wnObj = parseNumber("wn_simple");
          const clayObj = parseNumber("clay_simple");
          const slObj = parseNumber("shrinkage_simple");

          if (llObj.value === null || plObj.value === null) {
            errMsg.textContent = "Please enter both LL and PL in simple mode.";
            if (llObj.el && llObj.value === null)
              llObj.el.classList.add("input-error");
            if (plObj.el && plObj.value === null)
              plObj.el.classList.add("input-error");
            return;
          }
          LL = llObj.value;
          PL = plObj.value;
          wn = wnObj.value;
          clay = clayObj.value;
          SL = slObj.value;
          noteLlSource.classList.add("hidden");
        } else {
          // Detailed mode
          const plObj = parseNumber("pl_detailed");
          const wnObj = parseNumber("wn_detailed");
          const clayObj = parseNumber("clay_detailed");

          if (plObj.value === null) {
            errMsg.textContent =
              "Please enter the plastic limit PL in detailed mode.";
            if (plObj.el) plObj.el.classList.add("input-error");
            return;
          }
          PL = plObj.value;
          wn = wnObj.value;
          clay = clayObj.value;

          LL = computeLLFromFlow();
          if (LL === null || isNaN(LL)) {
            errMsg.textContent =
              "Unable to compute LL: please provide at least 3 valid flow-curve points with positive N.";
            return;
          }
          llFromFlow = true;
          noteLlSource.classList.remove("hidden");
        }

        if (LL <= PL) {
          errMsg.textContent =
            "Liquid limit must be greater than plastic limit (LL > PL). Please check your inputs.";
          return;
        }

        const PI = LL - PL;
        let LI = null,
          CI = null,
          A = null;

        if (wn !== null && !isNaN(wn)) {
          const denom = LL - PL;
          if (denom !== 0) {
            LI = (wn - PL) / denom;
            CI = 1 - LI;
          }
        }

        if (clay !== null && !isNaN(clay) && clay > 0) {
          A = PI / clay;
        }

        // Update results
        setResult("res_ll", LL, 2, " %");
        setResult("res_pl", PL, 2, " %");
        setResult("res_sl", SL, 2, " %");
        setResult("res_pi", PI, 2, " %");

        if (LI === null || isNaN(LI)) {
          document.getElementById("res_li").textContent = "–";
          document.getElementById("res_ci").textContent = "–";
        } else {
          document.getElementById("res_li").textContent = LI.toFixed(2);
          document.getElementById("res_ci").textContent = CI.toFixed(2);
        }

        if (A === null || isNaN(A)) {
          document.getElementById("res_activity").textContent = "–";
        } else {
          document.getElementById("res_activity").textContent = A.toFixed(2);
        }

        document.getElementById("res_plasticity").textContent =
          classifyPlasticity(LL, PI);
      });

      resetBtn.addEventListener("click", () => {
        clearErrors();
        // Simple
        [
          "ll_simple",
          "pl_simple",
          "wn_simple",
          "clay_simple",
          "shrinkage_simple",
        ].forEach((id) => {
          const el = document.getElementById(id);
          if (el) el.value = "";
        });
        // Detailed
        ["pl_detailed", "wn_detailed", "clay_detailed"].forEach((id) => {
          const el = document.getElementById(id);
          if (el) el.value = "";
        });
        while (flowRows.firstChild) flowRows.removeChild(flowRows.firstChild);
        ["45,15", "42,20", "39,25", "36,35"].forEach((pair) => {
          const [w, n] = pair.split(",");
          addFlowRow(w, n);
        });
        // Results
        [
          "res_ll",
          "res_pl",
          "res_sl",
          "res_pi",
          "res_li",
          "res_ci",
          "res_activity",
          "res_plasticity",
        ].forEach((id) => {
          const el = document.getElementById(id);
          if (el) el.textContent = "–";
        });
        noteLlSource.classList.add("hidden");
        errMsg.textContent = "";
      });

      // Mobile menu script (from template)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const expanded = this.getAttribute("aria-expanded") === "true";
          this.setAttribute("aria-expanded", String(!expanded));
          menu.classList.toggle("hidden");
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
