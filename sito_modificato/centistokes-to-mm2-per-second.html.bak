    <script>
      // Core logic: since 1 cSt = 1 mm²/s, we just mirror values with formatting.
      (function () {
        const cstInput = document.getElementById("cstInput");
        const mm2sInput = document.getElementById("mm2sInput");
        const decimalsSelect = document.getElementById("decimalsSelect");
        const resultText = document.getElementById("resultText");
        const cstClear = document.getElementById("cstClear");
        const mm2sClear = document.getElementById("mm2sClear");
        const copyButtons = document.querySelectorAll(".copy-btn");
        const copyStatus = document.getElementById("copyStatus");

        let activeField = null; // 'cst' or 'mm2s'
        let isUpdating = false;

        function formatNumber(value, decimals) {
          if (!isFinite(value)) return "";
          const d = parseInt(decimals, 10);
          return value.toFixed(d);
        }

        function parseValue(str) {
          if (str === null || str === undefined) return null;
          const trimmed = String(str).trim();
          if (trimmed === "") return null;
          const num = Number(trimmed);
          return isNaN(num) ? null : num;
        }

        function updateFromCst() {
          if (isUpdating) return;
          activeField = "cst";
          const val = parseValue(cstInput.value);
          const decimals = decimalsSelect.value;

          if (val === null) {
            isUpdating = true;
            mm2sInput.value = "";
            isUpdating = false;
            resultText.textContent =
              "Enter a kinematic viscosity in centistokes to see the value in mm²/s.";
            return;
          }

          const converted = val; // 1:1
          const formatted = formatNumber(converted, decimals);

          isUpdating = true;
          mm2sInput.value = formatted;
          isUpdating = false;

          resultText.textContent = `${val} cSt = ${formatted} mm²/s (1 cSt = 1 mm²/s exactly).`;
        }

        function updateFromMm2s() {
          if (isUpdating) return;
          activeField = "mm2s";
          const val = parseValue(mm2sInput.value);
          const decimals = decimalsSelect.value;

          if (val === null) {
            isUpdating = true;
            cstInput.value = "";
            isUpdating = false;
            resultText.textContent =
              "Enter a kinematic viscosity in mm²/s to see the value in centistokes.";
            return;
          }

          const converted = val; // 1:1
          const formatted = formatNumber(converted, decimals);

          isUpdating = true;
          cstInput.value = formatted;
          isUpdating = false;

          resultText.textContent = `${val} mm²/s = ${formatted} cSt (1 mm²/s = 1 cSt exactly).`;
        }

        function handleDecimalsChange() {
          if (activeField === "cst") {
            updateFromCst();
          } else if (activeField === "mm2s") {
            updateFromMm2s();
          }
        }

        function clearField(field) {
          if (field === "cst") {
            cstInput.value = "";
            updateFromCst();
          } else {
            mm2sInput.value = "";
            updateFromMm2s();
          }
        }

        function showCopyStatus() {
          if (!copyStatus) return;
          copyStatus.classList.remove("hidden");
          clearTimeout(showCopyStatus._timer);
          showCopyStatus._timer = setTimeout(() => {
            copyStatus.classList.add("hidden");
          }, 1500);
        }

        async function copyFromInput(inputId) {
          const el = document.getElementById(inputId);
          if (!el) return;
          const val = el.value;
          if (!val) return;
          try {
            await navigator.clipboard.writeText(val);
            showCopyStatus();
          } catch (e) {
            // Fallback: select text so user can copy manually
            el.focus();
            el.select();
          }
        }

        // Event listeners
        cstInput.addEventListener("input", updateFromCst);
        mm2sInput.addEventListener("input", updateFromMm2s);
        decimalsSelect.addEventListener("change", handleDecimalsChange);

        cstClear.addEventListener("click", () => clearField("cst"));
        mm2sClear.addEventListener("click", () => clearField("mm2s"));

        copyButtons.forEach((btn) => {
          btn.addEventListener("click", () => {
            const targetId = btn.getAttribute("data-copy-target");
            if (targetId) copyFromInput(targetId);
          });
        });
      })();
    </script>

