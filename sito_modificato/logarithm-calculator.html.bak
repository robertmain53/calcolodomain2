      const btnCommon = document.getElementById('btn-common-log');
      const btnNatural = document.getElementById('btn-natural-log');
      const btnLog2 = document.getElementById('btn-log2');

      const eqBase = document.getElementById('eq-base');
      const eqY = document.getElementById('eq-y');
      const eqResult = document.getElementById('eq-result');
      const btnSolveEq = document.getElementById('btn-solve-eq');

      function computeLog() {
        const x = parseFloat(numInput.value);
        const b = parseFloat(baseInput.value);

        if (!(x > 0) || !(b > 0) || b === 1) {
          resultInput.value = '';
          errorMsg.classList.remove('hidden');
          formulaMsg.textContent = '';
          return;
        }

        errorMsg.classList.add('hidden');
        // change of base using natural log
        const value = Math.log(x) / Math.log(b);
        resultInput.value = value.toFixed(8).replace(/0+$/,'').replace(/\.$/,'');
        formulaMsg.textContent = 'Used: log_b(x) = ln(x) / ln(b) with x=' + x + ', b=' + b;
      }

      numInput.addEventListener('input', computeLog);
      baseInput.addEventListener('input', computeLog);

      btnCommon.addEventListener('click', function() {
        baseInput.value = 10;
        computeLog();
      });

      btnNatural.addEventListener('click', function() {
        baseInput.value = Math.E.toFixed(6);
        computeLog();
      });

      btnLog2.addEventListener('click', function() {
        baseInput.value = 2;
        computeLog();
      });

      // equation: log_b(x) = y -> x = b^y
      btnSolveEq.addEventListener('click', function() {
        const b = parseFloat(eqBase.value);
        const y = parseFloat(eqY.value);
        if (!(b > 0) || b === 1 || isNaN(y)) {
          eqResult.value = 'Invalid';
          return;
        }
        const x = Math.pow(b, y);
        eqResult.value = x.toPrecision(10).replace(/0+$/,'').replace(/\.$/,'');
      });

      // initial compute
      computeLog();
    </script>

