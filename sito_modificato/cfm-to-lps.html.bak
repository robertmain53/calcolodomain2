        }

        function updateFrom(source) {
          if (isUpdating) return;
          isUpdating = true;

          let cfm, lps, lpm, m3h;

          if (source === 'cfm') {
            cfm = parseFloat(cfmInput.value);
            if (isNaN(cfm)) { lpsInput.value = lpmInput.value = m3hInput.value = ''; isUpdating = false; return; }
            lps = cfm * FACTOR_CFM_TO_LPS;
            lpm = lps * 60;
            m3h = lps * 3.6;
          } else if (source === 'lps') {
            lps = parseFloat(lpsInput.value);
            if (isNaN(lps)) { cfmInput.value = lpmInput.value = m3hInput.value = ''; isUpdating = false; return; }
            cfm = lps / FACTOR_CFM_TO_LPS;
            lpm = lps * 60;
            m3h = lps * 3.6;
          } else if (source === 'lpm') {
            lpm = parseFloat(lpmInput.value);
            if (isNaN(lpm)) { cfmInput.value = lpsInput.value = m3hInput.value = ''; isUpdating = false; return; }
            lps = lpm / 60;
            cfm = lps / FACTOR_CFM_TO_LPS;
            m3h = lps * 3.6;
          } else if (source === 'm3h') {
            m3h = parseFloat(m3hInput.value);
            if (isNaN(m3h)) { cfmInput.value = lpsInput.value = lpmInput.value = ''; isUpdating = false; return; }
            lps = m3h / 3.6;
            cfm = lps / FACTOR_CFM_TO_LPS;
            lpm = lps * 60;
          }

          cfmInput.value = typeof cfm === 'number' ? round(cfm, 4) : '';
          lpsInput.value = typeof lps === 'number' ? round(lps, 4) : '';
          lpmInput.value = typeof lpm === 'number' ? round(lpm, 2) : '';
          m3hInput.value = typeof m3h === 'number' ? round(m3h, 2) : '';

          isUpdating = false;
        }

        if (cfmInput) cfmInput.addEventListener('input', () => updateFrom('cfm'));
        if (lpsInput) lpsInput.addEventListener('input', () => updateFrom('lps'));
        if (lpmInput) lpmInput.addEventListener('input', () => updateFrom('lpm'));
        if (m3hInput) m3hInput.addEventListener('input', () => updateFrom('m3h'));

        // Populate table with common HVAC flows
        const sampleCFM = [50, 75, 100, 150, 200, 250, 300, 400, 500, 750, 1000, 1500];
        if (tableBody) {
          sampleCFM.forEach(cfmVal => {
            const lps = cfmVal * FACTOR_CFM_TO_LPS;
            const lpm = lps * 60;
            const m3h = lps * 3.6;
            const tr = document.createElement('tr');
            tr.innerHTML = `
              <td class="text-right">${cfmVal}</td>
              <td class="text-right">${round(lps, 3)}</td>
              <td class="text-right">${round(lpm, 1)}</td>
              <td class="text-right">${round(m3h, 1)}</td>
            `;
            tableBody.appendChild(tr);
          });
        }
      })();
    </script>

