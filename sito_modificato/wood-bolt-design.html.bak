    <script>
      // Wood bolt design calculator logic
      function formatNumber(val, decimals) {
        if (!isFinite(val)) return "–";
        return val.toLocaleString(undefined, {
          maximumFractionDigits: decimals,
          minimumFractionDigits: decimals,
        });
      }

      function checkMin(value, min) {
        return value + 1e-9 >= min;
      }

      document.addEventListener("DOMContentLoaded", function () {
        const calcBtn = document.getElementById("calculateBtn");
        const resetBtn = document.getElementById("resetBtn");
        const statusEl = document.getElementById("calcStatus");
        const resultsSection = document.getElementById("resultsSection");
        const spacingWarnings = document.getElementById("spacingWarnings");

        calcBtn.addEventListener("click", () => {
          statusEl.textContent = "";
          spacingWarnings.innerHTML = "";

          const d = parseFloat(document.getElementById("boltDiameter").value);
          const tm = parseFloat(
            document.getElementById("mainMemberThickness").value
          );
          const ts = parseFloat(
            document.getElementById("sideMemberThickness").value
          );
          const Fes = parseFloat(document.getElementById("Fes").value);
          const Fem = parseFloat(document.getElementById("Fem").value);
          const Ctotal = parseFloat(document.getElementById("Ctotal").value);
          const phi = parseFloat(document.getElementById("phi").value);
          const nRows = parseInt(document.getElementById("nRows").value, 10);
          const nCols = parseInt(document.getElementById("nCols").value, 10);
          const e = parseFloat(document.getElementById("endDistance").value);
          const ePerp = parseFloat(
            document.getElementById("edgeDistance").value
          );
          const sPar = parseFloat(
            document.getElementById("spacingParallel").value
          );
          const sPerp = parseFloat(
            document.getElementById("spacingPerp").value
          );
          const V = parseFloat(document.getElementById("appliedLoad").value);

          if (
            [
              d,
              tm,
              ts,
              Fes,
              Fem,
              Ctotal,
              phi,
              nRows,
              nCols,
              e,
              ePerp,
              sPar,
              sPerp,
              V,
            ].some((v) => !isFinite(v) || v <= 0)
          ) {
            statusEl.textContent =
              "Please enter positive numeric values for all fields.";
            statusEl.className = "text-sm text-red-600";
            resultsSection.classList.add("hidden");
            return;
          }

          // Simplified yield mode equations
          const Z_Im = Fem * d * tm;
          const Z_Is = Fes * d * ts;
          const Z_II = 1.15 * Math.sqrt(Fes * Fem) * d * Math.sqrt(ts * tm);
          const Z_III = 2.3 * Fes * d * ts;

          const modes = [
            { name: "Im (main member bearing)", value: Z_Im },
            { name: "Is (side member bearing)", value: Z_Is },
            { name: "II (single plastic hinge)", value: Z_II },
            { name: "III (double plastic hinge)", value: Z_III },
          ];

          let controlling = modes[0];
          for (let i = 1; i < modes.length; i++) {
            if (modes[i].value < controlling.value) controlling = modes[i];
          }

          const Zmin = controlling.value;
          const Zdesign = Zmin * Ctotal * phi;
          const nBolts = nRows * nCols;
          const Rn = Zdesign * nBolts;
          const utilization = V / Rn;

          // Populate results
          document.getElementById("controllingMode").textContent =
            controlling.name;
          document.getElementById("Zdesign").textContent = formatNumber(
            Zdesign,
            0
          );
          document.getElementById("ZdesignKip").textContent = formatNumber(
            Zdesign / 1000,
            3
          );
          document.getElementById("nBolts").textContent = nBolts.toString();
          document.getElementById("Rn").textContent = formatNumber(Rn, 0);
          document.getElementById("utilization").textContent = formatNumber(
            utilization,
            2
          );

          const statusText = document.getElementById("statusText");
          if (utilization <= 1.0) {
            statusText.textContent = "OK (capacity ≥ demand)";
            statusText.className = "font-semibold result-badge text-green-700";
          } else {
            statusText.textContent = "Overstressed (increase capacity)";
            statusText.className = "font-semibold result-badge text-red-700";
          }

          // Intermediate mode values
          document.getElementById("ZmodeIm").textContent = formatNumber(
            Z_Im,
            0
          );
          document.getElementById("ZmodeIs").textContent = formatNumber(
            Z_Is,
            0
          );
          document.getElementById("ZmodeII").textContent = formatNumber(
            Z_II,
            0
          );
          document.getElementById("ZmodeIII").textContent = formatNumber(
            Z_III,
            0
          );

          // Spacing checks (typical NDS minimums for bolts parallel to grain)
          const issues = [];
          const minEnd = 7 * d;
          const minEdge = 1.5 * d;
          const minSpar = 4 * d;
          const minSperp = 3 * d;

          if (!checkMin(e, minEnd)) {
            issues.push(
              `End distance e = ${formatNumber(
                e,
                2
              )} in &lt; 7d = ${formatNumber(
                minEnd,
                2
              )} in (increase distance from loaded end).`
            );
          }
          if (!checkMin(ePerp, minEdge)) {
            issues.push(
              `Edge distance e′ = ${formatNumber(
                ePerp,
                2
              )} in &lt; 1.5d = ${formatNumber(
                minEdge,
                2
              )} in (move bolts away from edge).`
            );
          }
          if (!checkMin(sPar, minSpar)) {
            issues.push(
              `Spacing parallel to grain s = ${formatNumber(
                sPar,
                2
              )} in &lt; 4d = ${formatNumber(
                minSpar,
                2
              )} in (increase row spacing).`
            );
          }
          if (!checkMin(sPerp, minSperp)) {
            issues.push(
              `Spacing perpendicular to grain s′ = ${formatNumber(
                sPerp,
                2
              )} in &lt; 3d = ${formatNumber(
                minSperp,
                2
              )} in (increase column spacing).`
            );
          }

          if (issues.length > 0) {
            const list = document.createElement("ul");
            list.className = "list-disc pl-5 text-red-700";
            issues.forEach((msg) => {
              const li = document.createElement("li");
              li.innerHTML = msg;
              list.appendChild(li);
            });
            const box = document.createElement("div");
            box.className = "bg-red-50 border border-red-200 rounded-lg p-3";
            const title = document.createElement("p");
            title.className = "font-semibold text-red-800 mb-1";
            title.textContent = "Spacing / edge distance warnings";
            box.appendChild(title);
            box.appendChild(list);
            spacingWarnings.appendChild(box);
          } else {
            const okBox = document.createElement("div");
            okBox.className =
              "bg-green-50 border border-green-200 rounded-lg p-3 text-sm text-green-800";
            okBox.textContent =
              "Bolt spacing, end distance, and edge distance satisfy typical minimum NDS requirements for bolts parallel to grain (verify for your specific case).";
            spacingWarnings.appendChild(okBox);
          }

          resultsSection.classList.remove("hidden");
          statusEl.textContent = "Calculation completed.";
          statusEl.className = "text-sm text-gray-600";
        });

        resetBtn.addEventListener("click", () => {
          document.getElementById("bolt-form").reset();
          document.getElementById("boltDiameter").value = 0.5;
          document.getElementById("mainMemberThickness").value = 3.5;
          document.getElementById("sideMemberThickness").value = 1.5;
          document.getElementById("Fes").value = 650;
          document.getElementById("Fem").value = 650;
          document.getElementById("Ctotal").value = 1.0;
          document.getElementById("phi").value = 0.8;
          document.getElementById("nRows").value = 2;
          document.getElementById("nCols").value = 2;
          document.getElementById("endDistance").value = 4.0;
          document.getElementById("edgeDistance").value = 2.0;
          document.getElementById("spacingParallel").value = 4.0;
          document.getElementById("spacingPerp").value = 3.0;
          document.getElementById("appliedLoad").value = 8000;
          document.getElementById("calcStatus").textContent = "";
          document.getElementById("resultsSection").classList.add("hidden");
