    <script>
      // Unit conversion helpers
      const pressureToPa = (value, unit) => {
        const v = Number(value);
        switch (unit) {
          case "pa":
            return v;
          case "kpa":
            return v * 1e3;
          case "bar":
            return v * 1e5;
          case "psi":
            return v * 6894.75729;
          default:
            return NaN;
        }
      };

      const paToPressure = (valuePa, unit) => {
        const v = Number(valuePa);
        switch (unit) {
          case "pa":
            return v;
          case "kpa":
            return v / 1e3;
          case "bar":
            return v / 1e5;
          case "psi":
            return v / 6894.75729;
          default:
            return NaN;
        }
      };

      const velToMS = (value, unit) => {
        const v = Number(value);
        switch (unit) {
          case "ms":
            return v;
          case "fts":
            return v * 0.3048;
          default:
            return NaN;
        }
      };

      const msToVel = (valueMs, unit) => {
        const v = Number(valueMs);
        switch (unit) {
          case "ms":
            return v;
          case "fts":
            return v / 0.3048;
          default:
            return NaN;
        }
      };

      const lenToM = (value, unit) => {
        const v = Number(value);
        switch (unit) {
          case "m":
            return v;
          case "ft":
            return v * 0.3048;
          default:
            return NaN;
        }
      };

      const mToLen = (valueM, unit) => {
        const v = Number(valueM);
        switch (unit) {
          case "m":
            return v;
          case "ft":
            return v / 0.3048;
          default:
            return NaN;
        }
      };

      const rhoToSI = (value, unit) => {
        const v = Number(value);
        switch (unit) {
          case "kgm3":
            return v;
          case "slugft3":
            return v * 515.378818; // 1 slug/ft³ ≈ 515.3788 kg/m³
          default:
            return NaN;
        }
      };

      const gToSI = (value, unit) => {
        const v = Number(value);
        switch (unit) {
          case "ms2":
            return v;
          case "fts2":
            return v * 0.3048;
          default:
            return NaN;
        }
      };

      document.addEventListener("DOMContentLoaded", () => {
        const unknownSelect = document.getElementById("unknownSelect");
        const p1Input = document.getElementById("p1");
        const p1Unit = document.getElementById("p1Unit");
        const v1Input = document.getElementById("v1");
        const v1Unit = document.getElementById("v1Unit");
        const z1Input = document.getElementById("z1");
        const z1Unit = document.getElementById("z1Unit");

        const p2Input = document.getElementById("p2");
        const p2Unit = document.getElementById("p2Unit");
        const v2Input = document.getElementById("v2");
        const v2Unit = document.getElementById("v2Unit");
        const z2Input = document.getElementById("z2");
        const z2Unit = document.getElementById("z2Unit");

        const densityInput = document.getElementById("density");
        const densityUnit = document.getElementById("densityUnit");
        const headLossInput = document.getElementById("headLoss");
        const headLossUnit = document.getElementById("headLossUnit");
        const gravityInput = document.getElementById("gravity");
        const gravityUnit = document.getElementById("gravityUnit");
        const useGauge = document.getElementById("useGauge");

        const calcBtn = document.getElementById("calcBtn");
        const resetBtn = document.getElementById("resetBtn");
        const errorMsg = document.getElementById("errorMsg");
        const resultsSection = document.getElementById("resultsSection");
        const primaryResult = document.getElementById("primaryResult");
        const secondaryResult = document.getElementById("secondaryResult");
        const headBreakdown = document.getElementById("headBreakdown");

        function updateDisabledFields() {
          const mode = unknownSelect.value;
          [p2Input, v2Input, z2Input].forEach((el) => {
            el.disabled = false;
            el.classList.remove("bg-gray-100");
          });
          if (mode === "p2") {
            p2Input.disabled = true;
            p2Input.classList.add("bg-gray-100");
          } else if (mode === "v2") {
            v2Input.disabled = true;
            v2Input.classList.add("bg-gray-100");
          } else if (mode === "z2") {
            z2Input.disabled = true;
            z2Input.classList.add("bg-gray-100");
          }
        }

        unknownSelect.addEventListener("change", () => {
          updateDisabledFields();
          resultsSection.classList.add("hidden");
          errorMsg.textContent = "";
        });

        updateDisabledFields();

        resetBtn.addEventListener("click", () => {
          densityInput.value = 1000;
          densityUnit.value = "kgm3";
          p1Input.value = 200000;
          p1Unit.value = "pa";
          v1Input.value = 3;
          v1Unit.value = "ms";
          z1Input.value = 0;
          z1Unit.value = "m";

          p2Input.value = 101325;
          p2Unit.value = "pa";
          v2Input.value = 0;
          v2Unit.value = "ms";
          z2Input.value = 5;
          z2Unit.value = "m";

          headLossInput.value = 0;
          headLossUnit.value = "m";
          gravityInput.value = 9.81;
          gravityUnit.value = "ms2";
          useGauge.checked = false;
          unknownSelect.value = "p2";
          updateDisabledFields();
          errorMsg.textContent = "";
          resultsSection.classList.add("hidden");
        });

        calcBtn.addEventListener("click", () => {
          errorMsg.textContent = "";
          resultsSection.classList.add("hidden");

          const mode = unknownSelect.value;

          const rho = rhoToSI(densityInput.value, densityUnit.value);
          const g = gToSI(gravityInput.value, gravityUnit.value);

          const p1Pa = pressureToPa(p1Input.value, p1Unit.value);
          const p2PaInput = pressureToPa(p2Input.value, p2Unit.value);
          const v1Ms = velToMS(v1Input.value, v1Unit.value);
          const v2MsInput = velToMS(v2Input.value, v2Unit.value);
          const z1M = lenToM(z1Input.value, z1Unit.value);
          const z2MInput = lenToM(z2Input.value, z2Unit.value);
          const hL = lenToM(headLossInput.value, headLossUnit.value);

          if ([rho, g, p1Pa, v1Ms, z1M, hL].some((v) => !isFinite(v))) {
            errorMsg.textContent =
              "Please check your inputs. Some values are missing or invalid.";
            return;
          }

          let p2Pa = p2PaInput;
          let v2Ms = v2MsInput;
          let z2M = z2MInput;

          try {
            if (mode === "p2") {
              if (!isFinite(v2Ms) || !isFinite(z2M)) {
                throw new Error("Enter valid values for v₂ and z₂.");
              }
              p2Pa =
                p1Pa +
                rho * ((v1Ms * v1Ms - v2Ms * v2Ms) / 2 + g * (z1M - z2M)) -
                rho * g * hL;
            } else if (mode === "v2") {
              if (!isFinite(p2Pa) || !isFinite(z2M)) {
                throw new Error("Enter valid values for p₂ and z₂.");
              }
              const term =
                (p1Pa - p2Pa) / (rho * g) +
                (v1Ms * v1Ms) / (2 * g) +
                (z1M - z2M) -
                hL;
              if (term < 0) {
                throw new Error(
                  "No real solution for v₂ with the given data (energy deficit). Check pressures, elevations, or head loss."
                );
              }
              v2Ms = Math.sqrt(2 * g * term);
            } else if (mode === "z2") {
              if (!isFinite(p2Pa) || !isFinite(v2Ms)) {
                throw new Error("Enter valid values for p₂ and v₂.");
              }
              z2M =
                (p1Pa - p2Pa) / (rho * g) +
                (v1Ms * v1Ms - v2Ms * v2Ms) / (2 * g) +
                z1M -
                hL;
            }
          } catch (e) {
            errorMsg.textContent = e.message;
            return;
          }

          // Build head terms for breakdown
          const p1Head = p1Pa / (rho * g);
          const p2Head = p2Pa / (rho * g);
          const v1Head = (v1Ms * v1Ms) / (2 * g);
          const v2Head = (v2Ms * v2Ms) / (2 * g);

          // Format results
          let mainText = "";
          let secondaryText = "";

          if (mode === "p2") {
            const p2Out = paToPressure(p2Pa, p2Unit.value);
            mainText = `Pressure at point 2: ${p2Out.toFixed(3)} ${
              p2Unit.options[p2Unit.selectedIndex].text
            }`;
            secondaryText = `Equivalent head at point 2: ${p2Head.toFixed(
              3
            )} m of fluid.`;
          } else if (mode === "v2") {
            const v2Out = msToVel(v2Ms, v2Unit.value);
            mainText = `Velocity at point 2: ${v2Out.toFixed(3)} ${
              v2Unit.options[v2Unit.selectedIndex].text
            }`;
            secondaryText = `Velocity head at point 2: ${v2Head.toFixed(3)} m.`;
          } else if (mode === "z2") {
            const z2Out = mToLen(z2M, z2Unit.value);
            mainText = `Elevation at point 2: ${z2Out.toFixed(3)} ${
              z2Unit.options[z2Unit.selectedIndex].text
            }`;
            secondaryText = `Elevation head difference z₂ − z₁: ${(
              z2M - z1M
            ).toFixed(3)} m.`;
          }

          primaryResult.textContent = mainText;
          secondaryResult.textContent = secondaryText;

          headBreakdown.innerHTML = `
        <p><strong>Point 1:</strong></p>
        <ul class="list-disc ml-5">
          <li>Pressure head: ${p1Head.toFixed(4)} m</li>
          <li>Velocity head: ${v1Head.toFixed(4)} m</li>
          <li>Elevation head: ${z1M.toFixed(4)} m</li>
          <li>Total head at 1: ${(p1Head + v1Head + z1M).toFixed(4)} m</li>
        </ul>
        <p class="mt-2"><strong>Point 2 (after solving):</strong></p>
        <ul class="list-disc ml-5">
          <li>Pressure head: ${p2Head.toFixed(4)} m</li>
          <li>Velocity head: ${v2Head.toFixed(4)} m</li>
          <li>Elevation head: ${z2M.toFixed(4)} m</li>
          <li>Total head at 2 + loss: ${(p2Head + v2Head + z2M + hL).toFixed(
            4
          )} m</li>
        </ul>
        <p class="mt-2 text-gray-600">Check: total head at 1 ≈ total head at 2 + h<sub>L</sub> (within rounding).</p>
      `;

          resultsSection.classList.remove("hidden");
        });
      });
    </script>

