<script>
  // Eurocode 2 beam design JS
  (function() {
    const modeDesignBtn = document.getElementById('mode-design');
    const modeCheckBtn = document.getElementById('mode-check');
    const designInputs = document.getElementById('design-inputs');
    const checkInputs = document.getElementById('check-inputs');
    const calcBtn = document.getElementById('calculateBtn');
    const errorMsg = document.getElementById('errorMsg');
    const resultsContent = document.getElementById('resultsContent');
    const statusBadge = document.getElementById('statusBadge');
    const statusBadgeFail = document.getElementById('statusBadgeFail');
    const resAsProvRow = document.getElementById('resAsProvRow');

    let mode = 'design';

    function setMode(newMode) {
      mode = newMode;
      if (mode === 'design') {
        modeDesignBtn.classList.add('bg-blue-600','text-white');
        modeDesignBtn.classList.remove('bg-gray-100','text-gray-700','border','border-gray-300');
        modeCheckBtn.classList.remove('bg-blue-600','text-white');
        modeCheckBtn.classList.add('bg-gray-100','text-gray-700','border','border-gray-300');
        designInputs.classList.remove('hidden');
        checkInputs.classList.add('hidden');
      } else {
        modeCheckBtn.classList.add('bg-blue-600','text-white');
        modeCheckBtn.classList.remove('bg-gray-100','text-gray-700','border','border-gray-300');
        modeDesignBtn.classList.remove('bg-blue-600','text-white');
        modeDesignBtn.classList.add('bg-gray-100','text-gray-700','border','border-gray-300');
        designInputs.classList.add('hidden');
        checkInputs.classList.remove('hidden');
      }
      errorMsg.classList.add('hidden');
    }

    modeDesignBtn.addEventListener('click', () => setMode('design'));
    modeCheckBtn.addEventListener('click', () => setMode('check'));

    function val(id) {
      return parseFloat(document.getElementById(id).value);
    }

    function round(value, digits) {
      const f = Math.pow(10, digits);
      return Math.round(value * f) / f;
    }

    calcBtn.addEventListener('click', () => {
      errorMsg.classList.add('hidden');
      statusBadge.classList.add('hidden');
      statusBadgeFail.classList.add('hidden');

      try {
        const L = val('span'); // m
        const b_mm = val('width');
        const h_mm = val('depth');
        const cover = val('cover');
        const fck = val('fck');
        const fyk = val('fyk');
        const gk = val('gk');
        const qk = val('qk');
        const gammaG = val('gammaG');
        const gammaQ = val('gammaQ');
        const gammaC = val('gammaC');
        const gammaS = val('gammaS');
        const includeSW = document.getElementById('includeSelfWeight').value === 'yes';

        if ([L,b_mm,h_mm,cover,fck,fyk,gk,qk,gammaG,gammaQ,gammaC,gammaS].some(x => !isFinite(x) || x <= 0)) {
          throw new Error('Please enter positive numeric values for all inputs.');
        }
        if (cover >= h_mm) {
          throw new Error('Cover must be less than overall depth h.');
        }

        const b = b_mm / 1000; // m
        const h = h_mm / 1000; // m

        // Self-weight (kN/m)
        const density = 25; // kN/m3
        const w_sw = includeSW ? density * b * h : 0;

        const w_k = w_sw + gk + qk;
        const w_d = gammaG * (w_sw + gk) + gammaQ * qk;

        const MEd = w_d * L * L / 8; // kNm

        // Material design strengths
        const alpha_cc = 0.85;
        const fcd = alpha_cc * fck / gammaC; // MPa
        const fyd = fyk / gammaS; // MPa

        // Effective depth and lever arm
        const d_mm = h_mm - cover;
        const d = d_mm / 1000; // m
        const z_mm = 0.9 * d_mm;
        const z = z_mm / 1000; // m

        // Required As
        // Convert MEd (kNm) to Nmm: 1 kNm = 1e6 Nmm
        const MEd_Nmm = MEd * 1e6;
        const z_mm_eff = z_mm;
        const As_req = MEd_Nmm / (fyd * z_mm_eff); // mm2

        // fctm approx (for normal-weight concrete, EC2 Table 3.1)
        function fctmFromFck(fck) {
          if (fck <= 30) return 2.9 + 0.1 * (fck - 20); // linear approx between C20/25 and C30/37
          if (fck <= 50) return 3.2 + 0.05 * (fck - 30);
          return 3.2 + 0.05 * (50 - 30); // cap
        }
        const fctm = fctmFromFck(fck);

        const As_min = 0.26 * fctm / fyk * b_mm * d_mm;
        const As_max = 0.04 * b_mm * h_mm;

        let As_used = As_req;
        let As_prov = null;

        if (mode === 'check') {
          const nBars = val('nBars');
          const barDia = val('barDia');
          if (!isFinite(nBars) || nBars <= 0 || !isFinite(barDia) || barDia <= 0) {
            throw new Error('Please enter a valid number and diameter of bars.');
          }
          const As_bar = Math.PI * Math.pow(barDia, 2) / 4;
          As_prov = nBars * As_bar;
          As_used = As_prov;
        }

        // Moment resistance with As_used
        const MRd_Nmm = As_used * fyd * z_mm_eff;
        const MRd = MRd_Nmm / 1e6; // kNm

        const util = MEd > 0 ? MEd / MRd : 0;

        // Steel strain estimate (very simplified)
        const Es = 200000; // MPa
        const eps_yd = fyd / Es;
        // Assume under-reinforced, approximate eps_s ~ 10 * eps_yd if As_used <= As_max
        const eps_s = As_used <= As_max ? 10 * eps_yd : eps_yd;

        // Populate results
        document.getElementById('resWsw').textContent = round(w_sw, 2);
        document.getElementById('resWk').textContent = round(w_k, 2);
        document.getElementById('resWd').textContent = round(w_d, 2);
        document.getElementById('resMed').textContent = round(MEd, 2);

        document.getElementById('resD').textContent = round(d_mm, 0);
        document.getElementById('resFcd').textContent = round(fcd, 2);
        document.getElementById('resFyd').textContent = round(fyd, 1);
        document.getElementById('resZ').textContent = round(z_mm_eff, 0);

        document.getElementById('resAsReq').textContent = round(As_req, 1);
        document.getElementById('resAsMin').textContent = round(As_min, 1);
        document.getElementById('resAsMax').textContent = round(As_max, 1);

        if (mode === 'check' && As_prov !== null) {
          resAsProvRow.classList.remove('hidden');
          document.getElementById('resAsProv').textContent = round(As_prov, 1);
        } else {
          resAsProvRow.classList.add('hidden');
        }

        document.getElementById('resMrd').textContent = round(MRd, 2);
        document.getElementById('resUtil').textContent = round(util, 3);

        document.getElementById('resEs').textContent = round(eps_s, 4);
        document.getElementById('resSteelDuctility').textContent = eps_s > eps_yd ? 'Yes (under‑reinforced)' : 'No (over‑reinforced)';

        if (util <= 1.0) {
          statusBadge.classList.remove('hidden');
          statusBadgeFail.classList.add('hidden');
        } else {
          statusBadgeFail.classList.remove('hidden');
          statusBadge.classList.add('hidden');
        }

        resultsContent.classList.remove('hidden');
      } catch (e) {
        errorMsg.textContent = e.message;
        errorMsg.classList.remove('hidden');
        resultsContent.classList.add('hidden');
      }
    });
  })();
</script>

