    <script>
      // FHA calculator logic
      function formatCurrency(value) {
        if (!isFinite(value)) return "$0";
        return value.toLocaleString("en-US", {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 0,
        });
      }
      function formatCurrency2(value) {
        if (!isFinite(value)) return "$0";
        return value.toLocaleString("en-US", {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 2,
        });
      }

      function calculateFHALoan() {
        const homePrice =
          parseFloat(document.getElementById("homePrice").value) || 0;
        const downPct =
          parseFloat(document.getElementById("downPaymentPct").value) || 0;
        const rate =
          parseFloat(document.getElementById("interestRate").value) || 0;
        const termYears =
          parseInt(document.getElementById("loanTerm").value, 10) || 30;
        const annualTax =
          parseFloat(document.getElementById("propertyTax").value) || 0;
        const annualIns =
          parseFloat(document.getElementById("homeInsurance").value) || 0;
        const hoa = parseFloat(document.getElementById("hoaDues").value) || 0;
        const mipFactor =
          parseFloat(document.getElementById("mipFactor").value) || 0;
        const financeUFMIP =
          document.getElementById("financeUpfrontMIP").checked;
        const showLimits = document.getElementById("showFHALimits").checked;

        const downPayment = homePrice * (downPct / 100);
        const baseLoan = Math.max(homePrice - downPayment, 0);

        const ufmipRate = 1.75; // %
        const upfrontMIP = baseLoan * (ufmipRate / 100);
        const financedLoan = financeUFMIP ? baseLoan + upfrontMIP : baseLoan;

        const n = termYears * 12;
        const monthlyRate = rate > 0 ? rate / 100 / 12 : 0;
        let pi = 0;
        if (monthlyRate > 0 && n > 0) {
          const pow = Math.pow(1 + monthlyRate, n);
          pi = financedLoan * ((monthlyRate * pow) / (pow - 1));
        } else if (n > 0) {
          pi = financedLoan / n;
        }

        const monthlyTax = annualTax / 12;
        const monthlyIns = annualIns / 12;
        const annualMIP = financedLoan * (mipFactor / 100);
        const monthlyMIP = annualMIP / 12;
        const totalMonthly = pi + monthlyTax + monthlyIns + monthlyMIP + hoa;

        // Update UI
        document.getElementById("downPaymentPctLabel").textContent =
          downPct.toFixed(1) + "%";
        document.getElementById("baseLoanAmount").textContent =
          formatCurrency(baseLoan);
        document.getElementById("financedLoanAmount").textContent =
          formatCurrency(financedLoan);
        document.getElementById("totalMonthlyPayment").textContent =
          formatCurrency(totalMonthly);

        document.getElementById("piPayment").textContent = formatCurrency2(pi);
        document.getElementById("taxPayment").textContent =
          formatCurrency2(monthlyTax);
        document.getElementById("insPayment").textContent =
          formatCurrency2(monthlyIns);
        document.getElementById("mipPayment").textContent =
          formatCurrency2(monthlyMIP);
        document.getElementById("hoaPayment").textContent =
          formatCurrency2(hoa);

        document.getElementById("upfrontMIPAmount").textContent =
          formatCurrency2(upfrontMIP);
        document.getElementById("financedUpfrontMIP").textContent = financeUFMIP
          ? formatCurrency2(upfrontMIP)
          : "$0.00";
        document.getElementById("mipFactorDisplay").textContent =
          mipFactor.toFixed(2) + "%";
        document.getElementById("annualMIPAmount").textContent =
          formatCurrency2(annualMIP);

        // Simple amortization snapshot (first year interest & principal)
        const firstYearInterest =
          monthlyRate > 0 ? financedLoan * monthlyRate * 12 : 0;
        const firstYearPrincipal = pi * 12 - firstYearInterest;
        const amortText = `
      In the first year, you'll pay about ${formatCurrency2(
        firstYearPrincipal
      )} toward principal and
      ${formatCurrency2(
        firstYearInterest
      )} in interest (excluding taxes, insurance, and MIP).
    `;
        document.getElementById("amortizationSummary").textContent =
          amortText.trim();

        // FHA limit check (very generic example)
        const fhaLimitPanel = document.getElementById("fhaLimitPanel");
        const fhaLimitMessage = document.getElementById("fhaLimitMessage");
        if (showLimits) {
          // Example generic national baseline limit (not county-specific)
          const exampleLimit = 498257; // illustrative baseline
          fhaLimitPanel.classList.remove("hidden");
          if (baseLoan <= exampleLimit) {
            fhaLimitMessage.textContent =
              `Your estimated base FHA loan of ${formatCurrency2(
                baseLoan
              )} is below a typical national baseline limit of ${formatCurrency2(
                exampleLimit
              )}. ` + `Local county limits may be higher or lower.`;
          } else {
            fhaLimitMessage.textContent =
              `Your estimated base FHA loan of ${formatCurrency2(
                baseLoan
              )} is above a typical national baseline limit of ${formatCurrency2(
                exampleLimit
              )}. ` +
              `You may need a higher-cost area limit or a smaller loan amount. Check your county's FHA limits.`;
          }
        } else {
          fhaLimitPanel.classList.add("hidden");
        }
      }

      document.addEventListener("DOMContentLoaded", function () {
        const downSlider = document.getElementById("downPaymentPct");
        downSlider.addEventListener("input", calculateFHALoan);
        document
          .getElementById("calculateBtn")
          .addEventListener("click", function (e) {
            e.preventDefault();
            calculateFHALoan();
          });
        document
          .querySelectorAll(
            "#homePrice,#interestRate,#loanTerm,#propertyTax,#homeInsurance,#hoaDues,#mipFactor"
          )
          .forEach((el) => el.addEventListener("change", calculateFHALoan));
        document
          .getElementById("financeUpfrontMIP")
          .addEventListener("change", calculateFHALoan);
        document
          .getElementById("showFHALimits")
          .addEventListener("change", calculateFHALoan);

        calculateFHALoan();
      });
    </script>

