    const FACTOR_NM_TO_INLB = 8.85074579;

    const ftlbInput = document.getElementById('ftlb-input');
    const nmOutput = document.getElementById('nm-output');
    const nmInput = document.getElementById('nm-input');
    const ftlbOutput = document.getElementById('ftlb-output');

    const nmInlbInput = document.getElementById('nm-inlb-input');
    const inlbOutput = document.getElementById('inlb-output');

    let isUpdating = false;

    function updateFromFtLb() {
      if (isUpdating) return;
      isUpdating = true;
      const val = parseFloat(ftlbInput.value || 0);
      const nm = val * FACTOR_FTLB_TO_NM;
      nmOutput.value = +nm.toFixed(6);
      isUpdating = false;
    }

    function updateFromNm() {
      if (isUpdating) return;
      isUpdating = true;
      const val = parseFloat(nmInput.value || 0);
      const ft = val * FACTOR_NM_TO_FTLB;
      ftlbOutput.value = +ft.toFixed(6);
      isUpdating = false;
    }

    function updateInLb() {
      const nm = parseFloat(nmInlbInput.value || 0);
      const inlb = nm * FACTOR_NM_TO_INLB;
      inlbOutput.value = +inlb.toFixed(4);
    }

    ftlbInput.addEventListener('input', updateFromFtLb);
    nmInput.addEventListener('input', updateFromNm);
    nmInlbInput.addEventListener('input', updateInLb);

    // init
    updateFromFtLb();
    updateFromNm();
    updateInLb();

    // populate quick table
    const tblVals = [
      {id: 'tbl-1ftlb', val: 1},
      {id: 'tbl-5ftlb', val: 5},
      {id: 'tbl-10ftlb', val: 10},
      {id: 'tbl-50ftlb', val: 50},
      {id: 'tbl-100ftlb', val: 100},
    ];
    tblVals.forEach(row => {
      const nm = row.val * FACTOR_FTLB_TO_NM;
      document.getElementById(row.id).textContent = nm.toFixed(3);
    });
  </script>

