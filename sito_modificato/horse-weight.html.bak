    <script>
      // Horse weight calculator logic
      (function () {
        const unitRadios = document.querySelectorAll(".unit-radio");
        const girthInput = document.getElementById("girth-input");
        const lengthInput = document.getElementById("length-input");
        const measuredWeightInput = document.getElementById(
          "measured-weight-input"
        );
        const heightInput = document.getElementById("height-input");
        const horseTypeSelect = document.getElementById("horse-type");

        const girthUnitLabel = document.getElementById("girth-unit-label");
        const lengthUnitLabel = document.getElementById("length-unit-label");
        const measuredWeightUnitLabel = document.getElementById(
          "measured-weight-unit-label"
        );
        const heightUnitLabel = document.getElementById("height-unit-label");

        const calcBtn = document.getElementById("calculate-btn");
        const resetBtn = document.getElementById("reset-btn");
        const errorEl = document.getElementById("calc-error");

        const resultsCard = document.getElementById("results-card");
        const weightKgEl = document.getElementById("weight-kg");
        const weightLbEl = document.getElementById("weight-lb");
        const conditionTextEl = document.getElementById("condition-text");
        const conditionDetailEl = document.getElementById("condition-detail");
        const scaleDiffEl = document.getElementById("scale-diff");
        const scaleDiffDetailEl = document.getElementById("scale-diff-detail");
        const typeRangeTextEl = document.getElementById("type-range-text");

        let unitSystem = "metric";

        const typeRanges = {
          light: {
            kgMin: 430,
            kgMax: 600,
            label: "Light horse (average riding horse)",
          },
          pony: { kgMin: 200, kgMax: 400, label: "Pony" },
          draft: { kgMin: 700, kgMax: 1000, label: "Draft / heavy horse" },
          mini: { kgMin: 90, kgMax: 200, label: "Miniature horse" },
        };

        const typeFactors = {
          light: 1.0,
          pony: 0.96,
          draft: 1.06,
          mini: 0.9,
        };

        function updateUnitLabels() {
          if (unitSystem === "metric") {
            girthUnitLabel.textContent = "(cm)";
            lengthUnitLabel.textContent = "(cm)";
            measuredWeightUnitLabel.textContent = "(kg)";
            heightUnitLabel.textContent = "(cm)";
            girthInput.placeholder = "e.g. 185";
            lengthInput.placeholder = "e.g. 200";
            measuredWeightInput.placeholder = "If you have a scale value";
            heightInput.placeholder = "e.g. 160";
          } else {
            girthUnitLabel.textContent = "(in)";
            lengthUnitLabel.textContent = "(in)";
            measuredWeightUnitLabel.textContent = "(lb)";
            heightUnitLabel.textContent = "(in)";
            girthInput.placeholder = "e.g. 73";
            lengthInput.placeholder = "e.g. 79";
            measuredWeightInput.placeholder = "If you have a scale value";
            heightInput.placeholder = "e.g. 63";
          }
        }

        unitRadios.forEach((r) => {
          r.addEventListener("change", () => {
            unitSystem = r.value;
            updateUnitLabels();
          });
        });

        function parsePositive(value) {
          const v = parseFloat(value);
          return isNaN(v) || v <= 0 ? null : v;
        }

        function estimateWeightKg(girth, length) {
          // girth and length are in current unit system
          let girthCm, lengthCm;

          if (unitSystem === "metric") {
            girthCm = girth;
            lengthCm = length;
          } else {
            // convert inches to cm
            girthCm = girth * 2.54;
            lengthCm = length * 2.54;
          }

          // Base metric formula
          let weightKg = (girthCm * girthCm * lengthCm) / 11877;

          const type = horseTypeSelect.value;
          const factor = typeFactors[type] || 1.0;
          weightKg *= factor;

          return weightKg;
        }

        function classifyCondition(weightKg, type) {
          const range = typeRanges[type];
          if (!range) {
            return {
              label: "Unknown",
              detail: "No reference range available for this type.",
              colorClass: "",
            };
          }

          if (weightKg < range.kgMin * 0.9) {
            return {
              label: "Below typical range",
              detail: `Estimated weight is well below the usual ${range.kgMin}–${range.kgMax} kg for ${range.label}. Check body condition and consult your vet.`,
              colorClass: "text-red-600",
            };
          } else if (weightKg < range.kgMin) {
            return {
              label: "Slightly below typical",
              detail: `Estimated weight is just under the usual ${range.kgMin}–${range.kgMax} kg for ${range.label}. Monitor body condition closely.`,
              colorClass: "text-amber-600",
            };
          } else if (weightKg <= range.kgMax) {
            return {
              label: "Within typical range",
              detail: `Estimated weight falls within the usual ${range.kgMin}–${range.kgMax} kg for ${range.label}. Confirm with body condition scoring.`,
              colorClass: "text-green-700",
            };
          } else if (weightKg <= range.kgMax * 1.1) {
            return {
              label: "Slightly above typical",
              detail: `Estimated weight is slightly above the usual ${range.kgMin}–${range.kgMax} kg for ${range.label}. Watch for signs of overweight.`,
              colorClass: "text-amber-600",
            };
          } else {
            return {
              label: "Above typical range",
              detail: `Estimated weight is well above the usual ${range.kgMin}–${range.kgMax} kg for ${range.label}. Discuss diet and exercise with your vet.`,
              colorClass: "text-red-600",
            };
          }
        }

        function formatRangeText(type) {
          const range = typeRanges[type];
          if (!range) return "";
          const kgMin = range.kgMin;
          const kgMax = range.kgMax;
          const lbMin = kgMin * 2.20462;
          const lbMax = kgMax * 2.20462;
          return `${range.label}: approximately ${kgMin.toFixed(
            0
          )}–${kgMax.toFixed(0)} kg (${lbMin.toFixed(0)}–${lbMax.toFixed(
            0
          )} lb) for an adult in moderate condition.`;
        }

        calcBtn.addEventListener("click", () => {
          errorEl.textContent = "";
          resultsCard.classList.add("hidden");

          const girth = parsePositive(girthInput.value);
          const length = parsePositive(lengthInput.value);

          if (!girth || !length) {
            errorEl.textContent =
              "Please enter both heart girth and body length with positive values.";
            return;
          }

          // Basic sanity checks
          if (unitSystem === "metric") {
            if (girth < 80 || girth > 300 || length < 80 || length > 300) {
              errorEl.textContent =
                "The measurements entered are outside the typical range for horses. Please double-check your units and values.";
              return;
            }
          } else {
            if (girth < 30 || girth > 120 || length < 30 || length > 120) {
              errorEl.textContent =
                "The measurements entered are outside the typical range for horses. Please double-check your units and values.";
              return;
            }
          }

          const weightKg = estimateWeightKg(girth, length);
          const weightLb = weightKg * 2.20462;

          weightKgEl.textContent = `${weightKg.toFixed(1)} kg`;
          weightLbEl.textContent = `${weightLb.toFixed(0)} lb`;

          const type = horseTypeSelect.value;
          const condition = classifyCondition(weightKg, type);
          conditionTextEl.textContent = condition.label;
          conditionTextEl.className = "font-semibold " + condition.colorClass;
          conditionDetailEl.textContent = condition.detail;

          // Scale comparison
          const measured = parsePositive(measuredWeightInput.value);
          if (measured) {
            let measuredKg = measured;
            if (unitSystem === "imperial") {
              measuredKg = measured / 2.20462;
            }
            const diffKg = weightKg - measuredKg;
            const diffPct = (diffKg / measuredKg) * 100;
            const sign = diffKg >= 0 ? "+" : "−";
            const absDiffKg = Math.abs(diffKg);
            const absDiffLb = absDiffKg * 2.20462;

            scaleDiffEl.textContent = `${sign}${absDiffKg.toFixed(
              1
            )} kg (${sign}${absDiffLb.toFixed(0)} lb), ${sign}${Math.abs(
              diffPct
            ).toFixed(1)}% vs. scale`;
            scaleDiffDetailEl.textContent =
              "Differences within ±10% are common with tape‑measure formulas.";
          } else {
            scaleDiffEl.textContent = "No scale weight entered";
            scaleDiffDetailEl.textContent =
              "Enter a known weight to compare tape estimate vs. scale.";
          }

          typeRangeTextEl.textContent = formatRangeText(type);

          resultsCard.classList.remove("hidden");
        });

        resetBtn.addEventListener("click", () => {
          girthInput.value = "";
          lengthInput.value = "";
          measuredWeightInput.value = "";
          heightInput.value = "";
          horseTypeSelect.value = "light";
          errorEl.textContent = "";
          resultsCard.classList.add("hidden");
        });

        // Initialize labels
        updateUnitLabels();
        typeRangeTextEl.textContent = formatRangeText("light");
      })();
    </script>

