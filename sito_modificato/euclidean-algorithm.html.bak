    <script>
      document.addEventListener("DOMContentLoaded", function () {
        // Mobile menu toggle (fallback)
        const toggle = document.getElementById("mobile-menu-toggle");
        const menu = document.getElementById("mobile-menu");
        if (toggle && menu) {
          toggle.addEventListener("click", () => {
            const isHidden = menu.classList.contains("hidden");
            menu.classList.toggle("hidden", !isHidden);
            toggle.setAttribute("aria-expanded", String(isHidden));
          });
        }

        const modeBasicBtn = document.getElementById("euclid-mode-basic-btn");
        const modeExtendedBtn = document.getElementById(
          "euclid-mode-extended-btn"
        );
        const modularBlock = document.getElementById("euclid-modular-block");
        const basicResults = document.getElementById("euclid-basic-results");
        const extendedResults = document.getElementById(
          "euclid-extended-results"
        );
        const modularResults = document.getElementById(
          "euclid-modular-results"
        );

        function setMode(mode) {
          const isBasic = mode === "basic";
          modeBasicBtn.classList.toggle("pill-active", isBasic);
          modeBasicBtn.classList.toggle("pill-inactive", !isBasic);
          modeExtendedBtn.classList.toggle("pill-active", !isBasic);
          modeExtendedBtn.classList.toggle("pill-inactive", isBasic);
          modularBlock.classList.toggle("hidden", isBasic);
          basicResults.classList.add("hidden");
          extendedResults.classList.add("hidden");
          modularResults.classList.add("hidden");
        }

        modeBasicBtn.addEventListener("click", () => setMode("basic"));
        modeExtendedBtn.addEventListener("click", () => setMode("extended"));

        const aInput = document.getElementById("euclid-a");
        const bInput = document.getElementById("euclid-b");
        const modInput = document.getElementById("euclid-mod");
        const errorSpan = document.getElementById("euclid-error");
        const computeBtn = document.getElementById("euclid-compute-btn");
        const clearBtn = document.getElementById("euclid-clear-btn");
        const exampleBtn = document.getElementById("euclid-example-btn");

        // Basic result elements
        const basicInputSpan = document.getElementById("euclid-basic-input");
        const basicGcdSpan = document.getElementById("euclid-basic-gcd");
        const basicStepsSpan = document.getElementById("euclid-basic-steps");
        const basicTableBody = document.getElementById("euclid-basic-table");
        const basicInterpretation = document.getElementById(
          "euclid-basic-interpretation"
        );
        const basicBadge = document.getElementById("euclid-basic-badge");

        // Extended result elements
        const extInputSpan = document.getElementById("euclid-extended-input");
        const extGcdSpan = document.getElementById("euclid-extended-gcd");
        const extBezoutSpan = document.getElementById("euclid-extended-bezout");
        const extTableBody = document.getElementById("euclid-extended-table");
        const extInterpretation = document.getElementById(
          "euclid-extended-interpretation"
        );
        const extBadge = document.getElementById("euclid-extended-badge");
        const modularText = document.getElementById("euclid-modular-text");

        function parseInteger(value, label) {
          const t = (value || "").trim();
          if (!t) throw new Error(label + " is required.");
          if (!/^[+-]?\d+$/.test(t)) {
            throw new Error(label + " must be an integer.");
          }
          const n = Number(t);
          if (!Number.isSafeInteger(n)) {
            throw new Error(
              label +
                " must be within JavaScript’s safe integer range (~±9×10¹5)."
            );
          }
          return n;
        }

        function clearOutputs() {
          errorSpan.textContent = "";
          errorSpan.classList.add("hidden");
          basicResults.classList.add("hidden");
          extendedResults.classList.add("hidden");
          modularResults.classList.add("hidden");
          basicTableBody.innerHTML = "";
          extTableBody.innerHTML = "";
          basicBadge.textContent = "–";
          basicBadge.className =
            "inline-flex items-center px-2.5 py-1 rounded-full text-xs font-semibold badge-neutral";
          extBadge.textContent = "–";
          extBadge.className =
            "inline-flex items-center px-2.5 py-1 rounded-full text-xs font-semibold badge-neutral";
        }

        function runEuclideanAlgorithm(a, b) {
          let A = Math.abs(a);
          let B = Math.abs(b);
          const steps = [];
          let stepIndex = 1;
          while (B !== 0) {
            const q = Math.trunc(A / B);
            const r = A - q * B;
            steps.push({ step: stepIndex, a: A, b: B, q, r });
            A = B;
            B = r;
            stepIndex++;
          }
          return { gcd: A, steps };
        }

        function runExtendedEuclid(a, b) {
          // Based on standard iterative extended Euclidean algorithm
          let old_r = Math.abs(a);
          let r = Math.abs(b);
          let old_s = 1,
            s = 0;
          let old_t = 0,
            t = 1;

          const rows = [];
          let step = 0;

          rows.push({ step, q: null, r: old_r, x: old_s, y: old_t });

          while (r !== 0) {
            const q = Math.trunc(old_r / r);
            const temp_r = old_r - q * r;
            const temp_s = old_s - q * s;
            const temp_t = old_t - q * t;
            step++;
            rows.push({ step, q, r, x: s, y: t });
            old_r = r;
            r = temp_r;
            old_s = s;
            s = temp_s;
            old_t = t;
            t = temp_t;
          }
          const gcd = old_r;
          const x = a >= 0 ? old_s : -old_s;
          const y = b >= 0 ? old_t : -old_t;
          return { gcd, x, y, rows };
        }

        function summarizeSize(gcd, a, b) {
          const maxAbs = Math.max(Math.abs(a), Math.abs(b));
          if (gcd === 1) return "The numbers are coprime (relatively prime).";
          if (maxAbs <= 1000)
            return "The GCD is greater than 1, so the numbers share nontrivial common factors.";
          return "The GCD is greater than 1, indicating shared factors even for these relatively large inputs.";
        }

        function badgeClassForGcd(gcd) {
          if (gcd === 1) return "badge-good";
          if (gcd <= 10) return "badge-warning";
          return "badge-risk";
        }

        function compute() {
          clearOutputs();
          try {
            const a = parseInteger(aInput.value, "First integer (a)");
            const b = parseInteger(bInput.value, "Second integer (b)");
            if (a === 0 && b === 0) {
              throw new Error("At least one of a or b must be nonzero.");
            }

            const isExtendedMode =
              modeExtendedBtn.classList.contains("pill-active");

            if (!isExtendedMode) {
              // Basic mode
              const { gcd, steps } = runEuclideanAlgorithm(a, b);
              const stepsCount = steps.length;

              basicInputSpan.textContent = `a = ${a}, b = ${b}`;
              basicGcdSpan.textContent = gcd.toString();
              basicStepsSpan.textContent = stepsCount.toString();

              basicTableBody.innerHTML = "";
              steps.forEach((s, idx) => {
                const tr = document.createElement("tr");
                if (idx === steps.length - 1) {
                  tr.classList.add("step-highlight");
                }
                const equation = `${s.a} = ${s.q} · ${s.b} + ${s.r}`;
                tr.innerHTML = `
                  <td class="px-3 py-1.5 text-left">${s.step}</td>
                  <td class="px-3 py-1.5 text-right">${s.a}</td>
                  <td class="px-3 py-1.5 text-right">${s.b}</td>
                  <td class="px-3 py-1.5 text-right">${s.q}</td>
                  <td class="px-3 py-1.5 text-right">${s.r}</td>
                  <td class="px-3 py-1.5 text-left mono-small">${equation}</td>
                `;
                basicTableBody.appendChild(tr);
              });

              basicInterpretation.textContent =
                `The last nonzero remainder is ${gcd}, so gcd(${a}, ${b}) = ${gcd}. ` +
                summarizeSize(gcd, a, b);

              const badgeClass = badgeClassForGcd(gcd);
              basicBadge.className = `inline-flex items-center px-2.5 py-1 rounded-full text-xs font-semibold ${badgeClass}`;
              if (gcd === 1) {
                basicBadge.textContent = "Coprime";
              } else {
                basicBadge.textContent = "Common factors";
              }

              basicResults.classList.remove("hidden");
            } else {
              // Extended mode
              const { gcd, x, y, rows } = runExtendedEuclid(a, b);

              extInputSpan.textContent = `a = ${a}, b = ${b}`;
              extGcdSpan.textContent = gcd.toString();
              extBezoutSpan.textContent = `${gcd} = ${a} · (${x}) + ${b} · (${y})`;

              extTableBody.innerHTML = "";
              rows.forEach((row, idx) => {
                const tr = document.createElement("tr");
                if (idx === rows.length - 1) {
                  tr.classList.add("step-highlight");
                }
                tr.innerHTML = `
                  <td class="px-3 py-1.5 text-left">${row.step}</td>
                  <td class="px-3 py-1.5 text-right">${
                    row.q === null ? "–" : row.q
                  }</td>
                  <td class="px-3 py-1.5 text-right">${row.r}</td>
                  <td class="px-3 py-1.5 text-right">${row.x}</td>
                  <td class="px-3 py-1.5 text-right">${row.y}</td>
                `;
                extTableBody.appendChild(tr);
              });

              const badgeClass = badgeClassForGcd(gcd);
              extBadge.className = `inline-flex items-center px-2.5 py-1 rounded-full text-xs font-semibold ${badgeClass}`;
              if (gcd === 1) {
                extBadge.textContent = "Coprime – inverse exists";
              } else {
                extBadge.textContent = "No inverse for all moduli";
              }

              extInterpretation.textContent =
                `The extended Euclidean algorithm shows that gcd(${a}, ${b}) = ${gcd}, ` +
                `and produces Bézout coefficients x = ${x}, y = ${y} with ${gcd} = ${a}·${x} + ${b}·${y}. ` +
                `When gcd(a, m) = 1, the same procedure can be used to obtain modular inverses of a modulo m.`;

              // Modular inverse (optional)
              const modStr = (modInput.value || "").trim();
              modularResults.classList.add("hidden");
              modularText.textContent = "";
              if (modStr !== "") {
                const m = parseInteger(modStr, "Modulus m");
                if (m === 0) {
                  throw new Error(
                    "Modulus m must be nonzero when requesting a modular inverse."
                  );
                }
                const g_am = runExtendedEuclid(a, m);
                if (g_am.gcd !== 1) {
                  modularText.textContent = `gcd(${a}, ${m}) = ${g_am.gcd} ≠ 1, so ${a} has no modular inverse modulo ${m}.`;
                } else {
                  const inv = ((g_am.x % m) + Math.abs(m)) % Math.abs(m);
                  modularText.textContent =
                    `Because gcd(${a}, ${m}) = 1, a modular inverse exists. ` +
                    `One inverse is ${inv}, since ${a} · ${inv} ≡ 1 (mod ${m}).`;
                }
                modularResults.classList.remove("hidden");
              }

              extendedResults.classList.remove("hidden");
            }
          } catch (err) {
            errorSpan.textContent =
              err.message || "An error occurred while running the algorithm.";
            errorSpan.classList.remove("hidden");
          }
        }

        function clearAll() {
          aInput.value = "";
          bInput.value = "";
          modInput.value = "";
          clearOutputs();
        }

        function loadExample() {
          aInput.value = "252";
          bInput.value = "198";
          modInput.value = "";
          compute();
        }

        computeBtn.addEventListener("click", compute);
        clearBtn.addEventListener("click", clearAll);
        exampleBtn.addEventListener("click", loadExample);

        aInput.addEventListener("keydown", function (e) {
          if (e.key === "Enter") {
            e.preventDefault();
            compute();
          }
        });
        bInput.addEventListener("keydown", function (e) {
          if (e.key === "Enter") {
            e.preventDefault();
            compute();
          }
        });
        modInput.addEventListener("keydown", function (e) {
          if (e.key === "Enter") {
            e.preventDefault();
            compute();
          }
        });

        // Initial mode
        setMode("basic");
      });
    </script>
  
