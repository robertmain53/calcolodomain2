    <script>
      // Utility formatting
      function fmt(num, decimals = 2) {
        if (num === null || isNaN(num)) return "–";
        return Number(num.toFixed(decimals)).toString();
      }

      // Single-task PERT
      function calcSinglePert() {
        const o = parseFloat(document.getElementById("optTime").value);
        const m = parseFloat(document.getElementById("mlTime").value);
        const p = parseFloat(document.getElementById("pessTime").value);
        const errEl = document.getElementById("singlePertError");
        errEl.classList.add("hidden");
        errEl.textContent = "";

        if ([o, m, p].some((v) => isNaN(v))) {
          errEl.textContent =
            "Please enter optimistic, most likely, and pessimistic times.";
          errEl.classList.remove("hidden");
          return;
        }
        if (o < 0 || m < 0 || p < 0) {
          errEl.textContent = "Times must be zero or positive.";
          errEl.classList.remove("hidden");
          return;
        }
        if (!(o <= m && m <= p)) {
          errEl.textContent =
            "Recommended: Optimistic ≤ Most likely ≤ Pessimistic for a consistent estimate.";
          errEl.classList.remove("hidden");
        }

        const te = (o + 4 * m + p) / 6;
        const sigma = (p - o) / 6;
        const variance = sigma * sigma;

        document.getElementById("singleTe").textContent = fmt(te);
        document.getElementById("singleSigma").textContent = fmt(sigma);
        document.getElementById("singleVar").textContent = fmt(variance);
        document.getElementById("singleRange").textContent = `${fmt(
          te - sigma
        )} – ${fmt(te + sigma)}`;
      }

      function resetSinglePert() {
        document.getElementById("optTime").value = "";
        document.getElementById("mlTime").value = "";
        document.getElementById("pessTime").value = "";
        document.getElementById("singleTe").textContent = "–";
        document.getElementById("singleSigma").textContent = "–";
        document.getElementById("singleVar").textContent = "–";
        document.getElementById("singleRange").textContent = "–";
        const errEl = document.getElementById("singlePertError");
        errEl.textContent = "";
        errEl.classList.add("hidden");
      }

      document
        .getElementById("calcSinglePert")
        .addEventListener("click", calcSinglePert);
      document
        .getElementById("resetSinglePert")
        .addEventListener("click", resetSinglePert);

      // Multi-task PERT
      const bodyEl = document.getElementById("pertTasksBody");

      function createTaskRow(index) {
        const tr = document.createElement("tr");
        tr.innerHTML = `
      <td class="border px-2 py-1 text-xs text-gray-500 align-middle task-index">${index}</td>
      <td class="border px-2 py-1">
        <input type="text" class="w-full text-sm border border-gray-200 rounded px-1 py-0.5" placeholder="Task ${index}">
      </td>
      <td class="border px-2 py-1 text-right">
        <input type="number" class="table-input w-20 text-right border rounded px-1 py-0.5" step="0.01" min="0">
      </td>
      <td class="border px-2 py-1 text-right">
        <input type="number" class="table-input w-20 text-right border rounded px-1 py-0.5" step="0.01" min="0">
      </td>
      <td class="border px-2 py-1 text-right">
        <input type="number" class="table-input w-20 text-right border rounded px-1 py-0.5" step="0.01" min="0">
      </td>
      <td class="border px-2 py-1 text-right text-xs task-te">–</td>
      <td class="border px-2 py-1 text-right text-xs task-sigma">–</td>
      <td class="border px-2 py-1 text-right text-xs task-var">–</td>
      <td class="border px-2 py-1 text-center">
        <button type="button" class="text-red-600 text-xs hover:underline remove-row">Remove</button>
      </td>
    `;
        return tr;
      }

      function renumberRows() {
        Array.from(bodyEl.querySelectorAll("tr")).forEach((tr, idx) => {
          const idxCell = tr.querySelector(".task-index");
          if (idxCell) idxCell.textContent = idx + 1;
          const nameInput = tr.querySelector(
            'td:nth-child(2) input[type="text"]'
          );
          if (
            nameInput &&
            (!nameInput.value || /^Task \d+$/.test(nameInput.value))
          ) {
            nameInput.value = `Task ${idx + 1}`;
          }
        });
      }

      function addTaskRow() {
        const index = bodyEl.querySelectorAll("tr").length + 1;
        const row = createTaskRow(index);
        bodyEl.appendChild(row);
      }

      function resetProjectPert() {
        bodyEl.innerHTML = "";
        for (let i = 0; i < 3; i++) addTaskRow();
        document.getElementById("projTe").textContent = "–";
        document.getElementById("projSigma").textContent = "–";
        document.getElementById("projVar").textContent = "–";
        document.getElementById("projRange").textContent = "–";
        const errEl = document.getElementById("projectPertError");
        errEl.textContent = "";
        errEl.classList.add("hidden");
      }

      function calcProjectPert() {
        const rows = Array.from(bodyEl.querySelectorAll("tr"));
        const errEl = document.getElementById("projectPertError");
        errEl.classList.add("hidden");
        errEl.textContent = "";

        if (!rows.length) {
          errEl.textContent = "Please add at least one task.";
          errEl.classList.remove("hidden");
          return;
        }

        let totalTe = 0;
        let totalVar = 0;
        let anyValid = false;

        rows.forEach((row) => {
          const inputs = row.querySelectorAll('input[type="number"]');
          const o = parseFloat(inputs[0].value);
          const m = parseFloat(inputs[1].value);
          const p = parseFloat(inputs[2].value);

          const teCell = row.querySelector(".task-te");
          const sigmaCell = row.querySelector(".task-sigma");
          const varCell = row.querySelector(".task-var");

          if ([o, m, p].some((v) => isNaN(v))) {
            teCell.textContent = "–";
            sigmaCell.textContent = "–";
            varCell.textContent = "–";
            return;
          }
          if (o < 0 || m < 0 || p < 0) {
            teCell.textContent = "–";
            sigmaCell.textContent = "–";
            varCell.textContent = "–";
            return;
          }

          const te = (o + 4 * m + p) / 6;
          const sigma = (p - o) / 6;
          const variance = sigma * sigma;

          teCell.textContent = fmt(te);
          sigmaCell.textContent = fmt(sigma);
          varCell.textContent = fmt(variance);

          totalTe += te;
          totalVar += variance;
          anyValid = true;
        });

        if (!anyValid) {
          errEl.textContent = "Enter O, M, and P for at least one task.";
          errEl.classList.remove("hidden");
          document.getElementById("projTe").textContent = "–";
          document.getElementById("projSigma").textContent = "–";
          document.getElementById("projVar").textContent = "–";
          document.getElementById("projRange").textContent = "–";
          return;
        }

        const projSigma = Math.sqrt(totalVar);

        document.getElementById("projTe").textContent = fmt(totalTe);
        document.getElementById("projSigma").textContent = fmt(projSigma);
        document.getElementById("projVar").textContent = fmt(totalVar);
        document.getElementById("projRange").textContent = `${fmt(
          totalTe - 2 * projSigma
        )} – ${fmt(totalTe + 2 * projSigma)}`;
      }

      document.getElementById("addTaskRow").addEventListener("click", () => {
        addTaskRow();
        renumberRows();
      });

      bodyEl.addEventListener("click", (e) => {
        if (e.target.classList.contains("remove-row")) {
          const row = e.target.closest("tr");
          if (row) {
            row.remove();
            renumberRows();
          }
        }
      });

      document
        .getElementById("calcProjectPert")
        .addEventListener("click", calcProjectPert);
      document
        .getElementById("resetProjectPert")
        .addEventListener("click", resetProjectPert);

      // Initialize default rows
      resetProjectPert();

      // Mobile menu toggle
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const expanded = this.getAttribute("aria-expanded") === "true";
          this.setAttribute("aria-expanded", String(!expanded));
          menu.classList.toggle("hidden");
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
