        kirpich: document.getElementById("method-kirpich"),
        tr55: document.getElementById("method-tr55"),
        faa: document.getElementById("method-faa"),
        velocity: document.getElementById("method-velocity"),
      };

      methodButtons.forEach((btn) => {
        btn.addEventListener("click", () => {
          const m = btn.getAttribute("data-method");
          currentMethod = m;
          methodButtons.forEach((b) => b.classList.remove("method-active"));
          btn.classList.add("method-active");
          Object.keys(methodSections).forEach((key) => {
            methodSections[key].classList.toggle("hidden", key !== m);
          });
          document.getElementById("results").classList.add("hidden");
          document.getElementById("form-error").classList.add("hidden");
        });
      });

      // Units toggle
      const unitsSiBtn = document.getElementById("units-si");
      const unitsUsBtn = document.getElementById("units-us");

      function setUnits(units) {
        currentUnits = units;
        const isSI = units === "si";

        // Update unit labels
        document.getElementById("kirpich-L-unit").textContent = isSI
          ? "m"
          : "ft";

        document.getElementById("tr55-Ls-unit").textContent = isSI ? "m" : "ft";
        document.getElementById("tr55-Lsh-unit").textContent = isSI
          ? "m"
          : "ft";
        document.getElementById("tr55-Lc-unit").textContent = isSI ? "m" : "ft";
        document.getElementById("tr55-P2-unit").textContent = isSI
          ? "mm"
          : "in";
        document.getElementById("tr55-Vsh-unit").textContent = isSI
          ? "m/s"
          : "ft/s";
        document.getElementById("tr55-Vc-unit").textContent = isSI
          ? "m/s"
          : "ft/s";

        document.getElementById("faa-L-unit").textContent = isSI ? "m" : "ft";

        ["1", "2", "3"].forEach((i) => {
          document.getElementById("vel-L" + i + "-unit").textContent = isSI
            ? "m"
            : "ft";
          document.getElementById("vel-V" + i + "-unit").textContent = isSI
            ? "m/s"
            : "ft/s";
        });

        unitsSiBtn.classList.toggle("bg-blue-600", isSI);
        unitsSiBtn.classList.toggle("text-white", isSI);
        unitsSiBtn.classList.toggle("bg-white", !isSI);
        unitsSiBtn.classList.toggle("text-gray-700", !isSI);

        unitsUsBtn.classList.toggle("bg-blue-600", !isSI);
        unitsUsBtn.classList.toggle("text-white", !isSI);
        unitsUsBtn.classList.toggle("bg-white", isSI);
        unitsUsBtn.classList.toggle("text-gray-700", isSI);

        document.getElementById("results").classList.add("hidden");
        document.getElementById("form-error").classList.add("hidden");
      }

      unitsSiBtn.addEventListener("click", () => setUnits("si"));
      unitsUsBtn.addEventListener("click", () => setUnits("us"));

      // Helper conversions
      function mToFt(m) {
        return m * 3.28084;
      }
      function ftToM(ft) {
        return ft / 3.28084;
      }
      function mmToIn(mm) {
        return mm / 25.4;
      }
      function inToMm(i) {
        return i * 25.4;
      }

      // Calculation
      const btnCalc = document.getElementById("btn-calc");
      const btnReset = document.getElementById("btn-reset");
      const resSection = document.getElementById("results");
      const resTcMin = document.getElementById("res-tc-min");
      const resTcHr = document.getElementById("res-tc-hr");
      const resMethod = document.getElementById("res-method");
      const resNote = document.getElementById("res-note");
      const resBreakdown = document.getElementById("res-breakdown");
      const formError = document.getElementById("form-error");

      function parsePositive(id) {
        const el = document.getElementById(id);
        const val = parseFloat(el.value);
        if (isNaN(val) || val <= 0) return null;
        return val;
      }

      function calcKirpich() {
        let L = parsePositive("kirpich-L");
        let S = parsePositive("kirpich-S");
        if (L === null || S === null) return null;

        // Convert to SI if needed
        if (currentUnits === "us") {
          L = ftToM(L);
          // S is dimensionless, same
        }

        const Tc_min = 0.0195 * Math.pow(L, 0.77) * Math.pow(S, -0.385);
        return {
          Tc_min,
          breakdown: `Kirpich (SI form): Tc = 0.0195 · L^0.77 · S^-0.385 = ${Tc_min.toFixed(
            2
          )} min`,
        };
      }

      function calcTR55() {
        // Sheet flow
        let Ls = parsePositive("tr55-Ls");
        let Ss = parsePositive("tr55-Ss");
        let n = parsePositive("tr55-n");
        let P2 = parsePositive("tr55-P2");
        if (Ls === null || Ss === null || n === null || P2 === null)
          return null;

        // Convert to US customary for TR-55 formula
        if (currentUnits === "si") {
          Ls = mToFt(Ls);
          P2 = mmToIn(P2);
          // Ss dimensionless
        }

        // TR-55 sheet flow: T_sheet in hours
        const T_sheet_hr =
          (0.007 * n * Math.pow(Ls, 0.8)) /
          (Math.pow(P2, 0.5) * Math.pow(Ss, 0.4));
        const T_sheet_min = T_sheet_hr * 60;

        // Shallow flow
        let Lsh = parsePositive("tr55-Lsh");
        let Ssh = parsePositive("tr55-Ssh");
        let Vsh = parsePositive("tr55-Vsh");
        let T_shallow_min = 0;
        if (Lsh && Ssh && Vsh) {
          if (currentUnits === "si") {
            // L in m, V in m/s
            T_shallow_min = Lsh / Vsh / 60;
          } else {
            // ft and ft/s
            T_shallow_min = Lsh / Vsh / 60;
          }
        }

        // Channel flow
        let Lc = parsePositive("tr55-Lc");
        let Vc = parsePositive("tr55-Vc");
        let T_channel_min = 0;
        if (Lc && Vc) {
          if (currentUnits === "si") {
            T_channel_min = Lc / Vc / 60;
          } else {
            T_channel_min = Lc / Vc / 60;
          }
        }

        const Tc_min = T_sheet_min + T_shallow_min + T_channel_min;

        let breakdown = `
      <ul class="list-disc ml-5">
        <li>Sheet flow: T<sub>sheet</sub> = ${T_sheet_min.toFixed(2)} min</li>
        <li>Shallow flow: T<sub>shallow</sub> = ${T_shallow_min.toFixed(
          2
        )} min</li>
        <li>Channel flow: T<sub>channel</sub> = ${T_channel_min.toFixed(
          2
        )} min</li>
      </ul>
    `;

        return { Tc_min, breakdown };
      }

      function calcFAA() {
        let L = parsePositive("faa-L");
        let S = parsePositive("faa-S");
        if (L === null || S === null) return null;

        // FAA formula expects ft and ft/ft
        if (currentUnits === "si") {
          L = mToFt(L);
          // S dimensionless
        }

        const Tc_min = (1.8 * Math.pow(L, 0.8)) / Math.pow(S, 0.5);
        return {
          Tc_min,
          breakdown: `FAA: Tc = 1.8 · L^0.8 / S^0.5 = ${Tc_min.toFixed(2)} min`,
        };
      }

      function calcVelocity() {
        let segments = [];
        for (let i = 1; i <= 3; i++) {
          const L = parseFloat(document.getElementById("vel-L" + i).value);
          const V = parseFloat(document.getElementById("vel-V" + i).value);
          if (!isNaN(L) && L > 0 && !isNaN(V) && V > 0) {
            segments.push({ L, V });
          }
        }
        if (segments.length === 0) return null;

        let Tc_min = 0;
        let breakdownItems = [];
        segments.forEach((seg, idx) => {
          const T_sec = seg.L / seg.V;
          const T_min = T_sec / 60;
          Tc_min += T_min;
          breakdownItems.push(
            `Segment ${idx + 1}: T = L / V = ${T_min.toFixed(2)} min`
          );
        });

        const breakdown = `
      <ul class="list-disc ml-5">
        ${breakdownItems.map((i) => `<li>${i}</li>`).join("")}
      </ul>
    `;

        return { Tc_min, breakdown };
      }

      btnCalc.addEventListener("click", () => {
        formError.classList.add("hidden");

        let result = null;
        let methodLabel = "";
        let note = "";

        switch (currentMethod) {
          case "kirpich":
            result = calcKirpich();
            methodLabel = "Kirpich equation";
            note = "Suitable for small, steep, natural basins.";
            break;
          case "tr55":
            result = calcTR55();
            methodLabel = "NRCS TR‑55 segmented method";
            note = "Sheet + shallow + channel flow travel times.";
            break;
          case "faa":
            result = calcFAA();
            methodLabel = "FAA empirical equation";
            note = "Often used for airport and urban drainage.";
            break;
          case "velocity":
            result = calcVelocity();
            methodLabel = "Velocity-based segmented method";
            note = "Uses user-defined velocities for each segment.";
            break;
        }

        if (!result || !isFinite(result.Tc_min) || result.Tc_min <= 0) {
          formError.classList.remove("hidden");
          resSection.classList.add("hidden");
          return;
        }

        const Tc_min = result.Tc_min;
        const Tc_hr = Tc_min / 60;

        resTcMin.textContent = Tc_min.toFixed(2);
        resTcHr.textContent = Tc_hr.toFixed(3);
        resMethod.textContent = methodLabel;
        resNote.textContent = note;
        resBreakdown.innerHTML = result.breakdown || "";
        resSection.classList.remove("hidden");
      });

      btnReset.addEventListener("click", () => {
        document.getElementById("tc-form").reset();
        resSection.classList.add("hidden");
        formError.classList.add("hidden");
      });

      // Initialize
      setUnits("si");
    </script>

