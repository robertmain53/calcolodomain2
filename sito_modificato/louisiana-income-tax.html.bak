    <script>
      // --- Core tax logic ---
      function getStandardDeduction(year, status) {
        // Simple, conservative estimates for LA standard deduction equivalents.
        // These are placeholders and can be tuned as law changes.
        const base2024 = {
          single: 4500,
          mfj: 9000,
          mfs: 4500,
          hoh: 6750,
        };
        const base2025 = {
          single: 5000,
          mfj: 10000,
          mfs: 5000,
          hoh: 7500,
        };
        const map = year >= 2025 ? base2025 : base2024;
        return map[status] || base2024.single;
      }

      function getLaTaxAndMarginal(year, taxable, status) {
        if (taxable <= 0) return { tax: 0, marginal: 0 };

        if (year >= 2025) {
          // Flat rate model; adjust as needed when final rate is confirmed.
          const flatRate = 0.035; // 3.5% example
          return { tax: taxable * flatRate, marginal: flatRate };
        }

        // 2024 progressive brackets model (illustrative, simplified)
        // Brackets differ by filing status; here we use a simple structure and
        // scale thresholds for MFJ/HOH.
        let brackets;
        if (status === "mfj" || status === "hoh") {
          brackets = [
            { upTo: 25000, rate: 0.018 },
            { upTo: 100000, rate: 0.032 },
            { upTo: Infinity, rate: 0.042 },
          ];
        } else if (status === "mfs") {
          brackets = [
            { upTo: 12500, rate: 0.018 },
            { upTo: 50000, rate: 0.032 },
            { upTo: Infinity, rate: 0.042 },
          ];
        } else {
          // single
          brackets = [
            { upTo: 12500, rate: 0.018 },
            { upTo: 50000, rate: 0.032 },
            { upTo: Infinity, rate: 0.042 },
          ];
        }

        let remaining = taxable;
        let lastCap = 0;
        let tax = 0;
        let marginal = 0;

        for (const b of brackets) {
          const span = Math.min(remaining, b.upTo - lastCap);
          if (span > 0) {
            tax += span * b.rate;
            remaining -= span;
            marginal = b.rate;
            lastCap = b.upTo;
          }
          if (remaining <= 0) break;
        }
        return { tax, marginal };
      }

      function formatMoney(v) {
        if (!isFinite(v)) return "$0";
        return v.toLocaleString("en-US", {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 0,
        });
      }

      function formatPercent(v) {
        if (!isFinite(v)) return "0.00%";
        return (v * 100).toFixed(2) + "%";
      }

      function calculateLaTax() {
        const year = parseInt(document.getElementById("tax-year").value, 10);
        const status = document.getElementById("filing-status").value;

        const gross =
          parseFloat(document.getElementById("gross-income").value) || 0;
        const preTax =
          parseFloat(document.getElementById("pre-tax-deductions").value) || 0;
        const itemized =
          parseFloat(document.getElementById("itemized-deductions").value) || 0;
        const nonrefCred =
          parseFloat(document.getElementById("nonrefundable-credits").value) ||
          0;
        const refCred =
          parseFloat(document.getElementById("refundable-credits").value) || 0;

        const salesRate =
          parseFloat(document.getElementById("sales-tax-rate").value) || 0;
        const taxableSpending =
          parseFloat(document.getElementById("taxable-spending").value) || 0;

        const adjustedIncome = Math.max(0, gross - preTax);
        const stdDed = getStandardDeduction(year, status);
        const usedItemized = Math.max(itemized, 0);
        const deductionUsed = Math.max(stdDed, usedItemized);
        const taxable = Math.max(0, adjustedIncome - deductionUsed);

        const { tax: rawTax, marginal } = getLaTaxAndMarginal(
          year,
          taxable,
          status
        );

        const laTaxBeforeCredits = rawTax;
        const laTaxAfterNonref = Math.max(0, laTaxBeforeCredits - nonrefCred);
        const laTaxFinal = Math.max(0, laTaxAfterNonref - refCred); // refundable can reduce to zero; refund not modeled separately

        const effectiveRate = gross > 0 ? laTaxFinal / gross : 0;
        const takeHome = gross - laTaxFinal;

        const salesTax = taxableSpending * (salesRate / 100);

        // Update UI
        document.getElementById("la-tax-output").textContent =
          formatMoney(laTaxFinal);
        document.getElementById("effective-rate-output").textContent =
          formatPercent(effectiveRate);
        document.getElementById("marginal-rate-output").textContent =
          formatPercent(marginal);
        document.getElementById("takehome-output").textContent =
          formatMoney(takeHome);
        document.getElementById("sales-tax-output").textContent =
          formatMoney(salesTax);

        document.getElementById("summary-gross").textContent =
          formatMoney(gross);
        document.getElementById("summary-pre-tax").textContent =
          "-" + formatMoney(preTax);
        document.getElementById("summary-standard-deduction").textContent =
          "-" + formatMoney(stdDed);
        document.getElementById("summary-itemized-used").textContent =
          "-" + formatMoney(usedItemized > stdDed ? usedItemized : 0);
        document.getElementById("summary-taxable").textContent =
          formatMoney(taxable);
      }

      function resetForm() {
        document.getElementById("tax-year").value = "2025";
        document.getElementById("filing-status").value = "mfj";
        document.getElementById("gross-income").value = 80000;
        document.getElementById("pre-tax-deductions").value = 0;
        document.getElementById("itemized-deductions").value = 0;
        document.getElementById("nonrefundable-credits").value = 0;
        document.getElementById("refundable-credits").value = 0;
        document.getElementById("sales-tax-rate").value = 4.45;
        document.getElementById("taxable-spending").value = 20000;
        updateSalesRateLabel();
        calculateLaTax();
      }

      function updateSalesRateLabel() {
        const rate =
          parseFloat(document.getElementById("sales-tax-rate").value) || 0;
        const label = rate.toFixed(2) + "%";
        document.getElementById("sales-tax-rate-label").textContent = label;
        document.getElementById("sales-tax-rate-display").textContent = label;
      }

      document.addEventListener("DOMContentLoaded", function () {
        document
          .getElementById("calculate-btn")
          .addEventListener("click", calculateLaTax);
        document
          .getElementById("reset-btn")
          .addEventListener("click", resetForm);
        document
          .getElementById("sales-tax-rate")
          .addEventListener("input", function () {
            updateSalesRateLabel();
            calculateLaTax();
          });

        // Auto-recalculate on key inputs
        [
          "tax-year",
          "filing-status",
          "gross-income",
          "pre-tax-deductions",
          "itemized-deductions",
          "nonrefundable-credits",
          "refundable-credits",
          "taxable-spending",
        ].forEach((id) => {
          const el = document.getElementById(id);
          if (!el) return;
          el.addEventListener("change", calculateLaTax);
          el.addEventListener("blur", calculateLaTax);
        });

        updateSalesRateLabel();
        calculateLaTax();
      });
    </script>

