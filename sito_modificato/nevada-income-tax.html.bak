    <script>
      // 2025 standard deductions (approx, can be updated easily)
      const STD_DEDUCTION_2025 = {
        single: 15000,
        married_joint: 30000,
        married_separate: 15000,
        hoh: 22500,
      };

      // 2025 federal tax brackets (approx) for ordinary income
      // Each status: array of {limit, rate}, applied progressively
      const FEDERAL_BRACKETS_2025 = {
        single: [
          { limit: 11600, rate: 0.1 },
          { limit: 47150, rate: 0.12 },
          { limit: 100525, rate: 0.22 },
          { limit: 191950, rate: 0.24 },
          { limit: 243725, rate: 0.32 },
          { limit: 609350, rate: 0.35 },
          { limit: Infinity, rate: 0.37 },
        ],
        married_joint: [
          { limit: 23200, rate: 0.1 },
          { limit: 94300, rate: 0.12 },
          { limit: 201050, rate: 0.22 },
          { limit: 383900, rate: 0.24 },
          { limit: 487450, rate: 0.32 },
          { limit: 731200, rate: 0.35 },
          { limit: Infinity, rate: 0.37 },
        ],
        married_separate: [
          { limit: 11600, rate: 0.1 },
          { limit: 47150, rate: 0.12 },
          { limit: 100525, rate: 0.22 },
          { limit: 191950, rate: 0.24 },
          { limit: 243725, rate: 0.32 },
          { limit: 365600, rate: 0.35 },
          { limit: Infinity, rate: 0.37 },
        ],
        hoh: [
          { limit: 16550, rate: 0.1 },
          { limit: 63100, rate: 0.12 },
          { limit: 100500, rate: 0.22 },
          { limit: 191950, rate: 0.24 },
          { limit: 243700, rate: 0.32 },
          { limit: 609350, rate: 0.35 },
          { limit: Infinity, rate: 0.37 },
        ],
      };

      const SS_WAGE_BASE_2025 = 168600; // Social Security wage base (approx)

      function calcFederalTax(taxableIncome, status) {
        if (taxableIncome <= 0) return 0;
        const brackets =
          FEDERAL_BRACKETS_2025[status] || FEDERAL_BRACKETS_2025.single;
        let remaining = taxableIncome;
        let lastLimit = 0;
        let tax = 0;

        for (const bracket of brackets) {
          const band = Math.min(remaining, bracket.limit - lastLimit);
          if (band <= 0) break;
          tax += band * bracket.rate;
          remaining -= band;
          lastLimit = bracket.limit;
          if (remaining <= 0) break;
        }
        return tax;
      }

      function calcFica(wages) {
        if (wages <= 0) return 0;
        const ssTaxable = Math.min(wages, SS_WAGE_BASE_2025);
        const socialSecurity = ssTaxable * 0.062;
        const medicare = wages * 0.0145;
        return socialSecurity + medicare;
      }

      function formatCurrency(value) {
        return value.toLocaleString("en-US", {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 0,
        });
      }

      function formatPercent(value) {
        return (value * 100).toFixed(1) + "%";
      }

      function calculateNevadaTaxes() {
        const status = document.getElementById("filing-status").value;
        const wages = Number(document.getElementById("wages").value) || 0;
        const otherIncome =
          Number(document.getElementById("other-income").value) || 0;
        const pretax =
          Number(document.getElementById("pretax-401k").value) || 0;
        const itemized =
          Number(document.getElementById("itemized-deductions").value) || 0;
        const includeFica =
          document.getElementById("include-fica").value === "yes";

        const grossIncome = Math.max(0, wages + otherIncome);
        const agi = Math.max(0, grossIncome - pretax);

        const stdDeduction =
          STD_DEDUCTION_2025[status] || STD_DEDUCTION_2025.single;
        const deduction = Math.max(stdDeduction, itemized);
        const taxableIncome = Math.max(0, agi - deduction);

        const federalTax = calcFederalTax(taxableIncome, status);
        const ficaTax = includeFica ? calcFica(wages) : 0;
        const nvTax = 0; // Nevada has no state income tax

        const totalTax = federalTax + ficaTax + nvTax;
        const takeHome = Math.max(0, grossIncome - totalTax);
        const effectiveRate = grossIncome > 0 ? totalTax / grossIncome : 0;

        document.getElementById("nv-tax").textContent = formatCurrency(nvTax);
        document.getElementById("federal-tax").textContent =
          formatCurrency(federalTax);
        document.getElementById("fica-tax").textContent =
          formatCurrency(ficaTax);
        document.getElementById("total-tax").textContent =
          formatCurrency(totalTax);
        document.getElementById("take-home").textContent =
          formatCurrency(takeHome);
        document.getElementById("effective-rate").textContent =
          formatPercent(effectiveRate);
      }

      document
        .getElementById("calculate-btn")
        .addEventListener("click", calculateNevadaTaxes);

      // Auto-calc on load
      document.addEventListener("DOMContentLoaded", calculateNevadaTaxes);
    </script>

