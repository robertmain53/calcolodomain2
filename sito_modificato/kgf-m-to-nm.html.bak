    <script>
      (function () {
        const G = 9.80665; // exact standard gravity
        const kgfmInput = document.getElementById("kgfm-input");
        const nmInput = document.getElementById("nm-input");
        const nmFromKgfm = document.getElementById("nm-from-kgfm");
        const kgfmFromNm = document.getElementById("kgfm-from-nm");
        const decimalsSelect = document.getElementById("decimal-places");
        const resetBtn = document.getElementById("reset-btn");
        const swapToKgfmBtn = document.getElementById("swap-to-kgfm");
        const swapToNmBtn = document.getElementById("swap-to-nm");
        const quickTableBody = document.getElementById("quick-table-body");

        function formatNumber(value) {
          const decimals = parseInt(decimalsSelect.value, 10);
          if (!isFinite(value)) return "—";
          return value.toFixed(decimals);
        }

        function parseInput(el) {
          const v = parseFloat(el.value);
          return isNaN(v) ? null : v;
        }

        function updateFromKgfm() {
          const v = parseInput(kgfmInput);
          if (v === null) {
            nmFromKgfm.textContent = "0";
            return;
          }
          const result = v * G;
          nmFromKgfm.textContent = formatNumber(result);
        }

        function updateFromNm() {
          const v = parseInput(nmInput);
          if (v === null) {
            kgfmFromNm.textContent = "0";
            return;
          }
          const result = v / G;
          kgfmFromNm.textContent = formatNumber(result);
        }

        function buildQuickTable() {
          const baseValues = [0.5, 1, 2, 5, 10, 20, 50];
          quickTableBody.innerHTML = "";
          baseValues.forEach((val) => {
            const nm = val * G;
            const tr = document.createElement("tr");
            tr.innerHTML = `
          <td class="px-3 py-2 text-gray-800">${val}</td>
          <td class="px-3 py-2 text-gray-800">${formatNumber(nm)}</td>
        `;
            quickTableBody.appendChild(tr);
          });
        }

        // Event listeners
        kgfmInput.addEventListener("input", updateFromKgfm);
        nmInput.addEventListener("input", updateFromNm);
        decimalsSelect.addEventListener("change", () => {
          updateFromKgfm();
          updateFromNm();
          buildQuickTable();
        });

        resetBtn.addEventListener("click", () => {
          kgfmInput.value = "";
          nmInput.value = "";
          nmFromKgfm.textContent = "0";
          kgfmFromNm.textContent = "0";
        });

        swapToKgfmBtn.addEventListener("click", () => {
          const v = nmFromKgfm.textContent.replace(/,/g, "");
          const num = parseFloat(v);
          if (!isNaN(num)) {
            nmInput.value = num;
            updateFromNm();
            nmInput.focus();
          }
        });

        swapToNmBtn.addEventListener("click", () => {
          const v = kgfmFromNm.textContent.replace(/,/g, "");
          const num = parseFloat(v);
          if (!isNaN(num)) {
            kgfmInput.value = num;
            updateFromKgfm();
            kgfmInput.focus();
          }
        });

        // Initial render
        buildQuickTable();
      })();

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
