    <script>
      // Tab switching
      const tabSimple = document.getElementById("tab-simple");
      const tabSerial = document.getElementById("tab-serial");
      const panelSimple = document.getElementById("panel-simple");
      const panelSerial = document.getElementById("panel-serial");

      function activateTab(tab) {
        if (tab === "simple") {
          tabSimple.classList.add("border-blue-600", "text-blue-600");
          tabSimple.classList.remove("text-gray-500");
          tabSerial.classList.remove("border-blue-600", "text-blue-600");
          tabSerial.classList.add("text-gray-500");
          panelSimple.classList.remove("hidden");
          panelSerial.classList.add("hidden");
        } else {
          tabSerial.classList.add("border-blue-600", "text-blue-600");
          tabSerial.classList.remove("text-gray-500");
          tabSimple.classList.remove("border-blue-600", "text-blue-600");
          tabSimple.classList.add("text-gray-500");
          panelSerial.classList.remove("hidden");
          panelSimple.classList.add("hidden");
        }
      }

      tabSimple.addEventListener("click", () => activateTab("simple"));
      tabSerial.addEventListener("click", () => activateTab("serial"));

      // Simple dilution calculator
      const c1Input = document.getElementById("c1");
      const c1UnitInput = document.getElementById("c1-unit");
      const v1Input = document.getElementById("v1");
      const v1UnitSelect = document.getElementById("v1-unit");
      const c2Input = document.getElementById("c2");
      const c2UnitInput = document.getElementById("c2-unit");
      const v2Input = document.getElementById("v2");
      const v2UnitSelect = document.getElementById("v2-unit");
      const simpleError = document.getElementById("simple-error");
      const simpleResults = document.getElementById("simple-results");
      const simpleMainResult = document.getElementById("simple-main-result");
      const simpleDiluent = document.getElementById("simple-diluent");
      const simpleFactor = document.getElementById("simple-factor");

      document
        .getElementById("btn-calc-simple")
        .addEventListener("click", () => {
          simpleError.textContent = "";
          simpleResults.classList.add("hidden");

          const C1 = parseFloat(c1Input.value);
          const C2 = parseFloat(c2Input.value);
          const V1 = v1Input.value === "" ? null : parseFloat(v1Input.value);
          const V2 = v2Input.value === "" ? null : parseFloat(v2Input.value);

          if (isNaN(C1) || isNaN(C2)) {
            simpleError.textContent =
              "Please enter both stock (C₁) and final (C₂) concentrations.";
            return;
          }
          if (C1 <= 0 || C2 <= 0) {
            simpleError.textContent =
              "Concentrations must be positive numbers.";
            return;
          }
          if (V1 === null && V2 === null) {
            simpleError.textContent =
              "Enter either stock volume (V₁) or final volume (V₂) so the other can be calculated.";
            return;
          }
          if (V1 !== null && V2 !== null) {
            simpleError.textContent =
              "Leave either V₁ or V₂ blank. The calculator solves for one unknown at a time.";
            return;
          }

          let v1Calc, v2Calc;
          if (V1 === null) {
            // Solve for V1: V1 = C2*V2 / C1
            if (isNaN(V2) || V2 <= 0) {
              simpleError.textContent =
                "Final volume (V₂) must be a positive number.";
              return;
            }
            v2Calc = V2;
            v1Calc = (C2 * V2) / C1;
          } else {
            // Solve for V2: V2 = C1*V1 / C2
            if (V1 <= 0) {
              simpleError.textContent =
                "Stock volume (V₁) must be a positive number.";
              return;
            }
            v1Calc = V1;
            v2Calc = (C1 * V1) / C2;
          }

          if (
            !isFinite(v1Calc) ||
            !isFinite(v2Calc) ||
            v1Calc <= 0 ||
            v2Calc <= 0
          ) {
            simpleError.textContent =
              "The combination of values leads to an invalid dilution. Check that C₁ > C₂ for a dilution.";
            return;
          }

          const v1Unit = v1UnitSelect.value;
          const v2Unit = v2UnitSelect.value;

          // If solving for V1, it should be in the same unit as V1 selector; if solving for V2, in V2 selector.
          let v1Display = v1Calc;
          let v2Display = v2Calc;

          // Simple formatting
          function fmt(x) {
            if (x >= 0.01 && x < 10000)
              return x.toFixed(3).replace(/\.?0+$/, "");
            return x.toExponential(3);
          }

          const diluentVol = v2Calc - v1Calc;
          const df = v2Calc / v1Calc;

          const cUnit = c2UnitInput.value || c1UnitInput.value || "units";

          simpleMainResult.textContent = `Use ${fmt(
            v1Display
          )} ${v1Unit} of stock at ${C1} ${cUnit} and dilute to a final volume of ${fmt(
            v2Display
          )} ${v2Unit} to obtain ${C2} ${cUnit}.`;

          simpleDiluent.textContent = `This means adding ${fmt(
            diluentVol
          )} ${v2Unit} of diluent (buffer/solvent).`;

          simpleFactor.textContent = `Effective dilution factor: ${fmt(
            df
          )}-fold (C₂ = C₁ / DF).`;

          simpleResults.classList.remove("hidden");
        });

      document
        .getElementById("btn-clear-simple")
        .addEventListener("click", () => {
          [
            c1Input,
            c1UnitInput,
            v1Input,
            c2Input,
            c2UnitInput,
            v2Input,
          ].forEach((el) => (el.value = ""));
          simpleError.textContent = "";
          simpleResults.classList.add("hidden");
        });

      // Serial dilution calculator
      const serialStart = document.getElementById("serial-start");
      const serialUnit = document.getElementById("serial-unit");
      const serialFactor = document.getElementById("serial-factor");
      const serialSteps = document.getElementById("serial-steps");
      const serialVfinal = document.getElementById("serial-vfinal");
      const serialVunit = document.getElementById("serial-vunit");
      const serialError = document.getElementById("serial-error");
      const serialResults = document.getElementById("serial-results");
      const serialTableBody = document.getElementById("serial-table-body");
      const serialSummary = document.getElementById("serial-summary");

      document
        .getElementById("btn-calc-serial")
        .addEventListener("click", () => {
          serialError.textContent = "";
          serialResults.classList.add("hidden");
          serialTableBody.innerHTML = "";
          serialSummary.textContent = "";

          const C0 = parseFloat(serialStart.value);
          const DF = parseFloat(serialFactor.value);
          const nSteps = parseInt(serialSteps.value, 10);
          const Vfinal = parseFloat(serialVfinal.value);

          if (isNaN(C0) || C0 <= 0) {
            serialError.textContent =
              "Enter a positive starting concentration.";
            return;
          }
          if (isNaN(DF) || DF <= 1) {
            serialError.textContent =
              "Dilution factor per step must be greater than 1 (e.g., 10 for 1:10).";
            return;
          }
          if (isNaN(nSteps) || nSteps < 2) {
            serialError.textContent =
              "Number of tubes/steps must be at least 2.";
            return;
          }
          if (isNaN(Vfinal) || Vfinal <= 0) {
            serialError.textContent =
              "Final volume per tube must be a positive number.";
            return;
          }

          const vUnit = serialVunit.value;
          const cUnit = serialUnit.value || "units";
          const VstockPerStep = Vfinal / DF;
          const VdiluentPerStep = Vfinal - VstockPerStep;

          function fmt(x) {
            if (x >= 0.01 && x < 10000)
              return x.toFixed(3).replace(/\.?0+$/, "");
            return x.toExponential(3);
          }

          let overallDF = 1;
          let currentConc = C0;

          for (let i = 1; i <= nSteps; i++) {
            if (i === 1) {
              overallDF = DF;
              currentConc = C0 / DF;
            } else {
              overallDF *= DF;
              currentConc = C0 / overallDF;
            }

            const row = document.createElement("tr");
            row.className = i % 2 === 0 ? "bg-gray-50" : "";

            const fromLabel = i === 1 ? "Stock" : `Tube ${i - 1}`;

            row.innerHTML = `
        <td class="px-2 py-1 border-b">Tube ${i}</td>
        <td class="px-2 py-1 border-b">${fromLabel}</td>
        <td class="px-2 py-1 border-b text-right">${fmt(
          VstockPerStep
        )} ${vUnit}</td>
        <td class="px-2 py-1 border-b text-right">${fmt(
          VdiluentPerStep
        )} ${vUnit}</td>
        <td class="px-2 py-1 border-b text-right">${fmt(
          currentConc
        )} ${cUnit}</td>
        <td class="px-2 py-1 border-b text-right">${fmt(overallDF)}</td>
      `;
            serialTableBody.appendChild(row);
          }

          serialSummary.textContent =
            `Each step is a 1:${fmt(DF)} dilution using ${fmt(
              VstockPerStep
            )} ${vUnit} from the previous tube plus ${fmt(
              VdiluentPerStep
            )} ${vUnit} diluent. ` +
            `Tube ${nSteps} is diluted ${fmt(
              Math.pow(DF, nSteps)
            )}-fold relative to the original stock.`;

          serialResults.classList.remove("hidden");
        });

      document
        .getElementById("btn-clear-serial")
        .addEventListener("click", () => {
          [
            serialStart,
            serialUnit,
            serialFactor,
            serialSteps,
            serialVfinal,
          ].forEach((el) => (el.value = ""));
          serialError.textContent = "";
          serialResults.classList.add("hidden");
          serialTableBody.innerHTML = "";
          serialSummary.textContent = "";
        });

      // FAQ toggle (simple, in case global script not loaded yet)
      document.querySelectorAll(".faq-question").forEach((btn) => {
        btn.addEventListener("click", () => {
          const answer = btn.parentElement.querySelector(".faq-answer");
          const isHidden = answer.classList.contains("hidden");
          document
            .querySelectorAll(".faq-answer")
            .forEach((a) => a.classList.add("hidden"));
          document
            .querySelectorAll(".faq-question span:last-child")
            .forEach((icon) => (icon.textContent = "+"));
          if (isHidden) {
            answer.classList.remove("hidden");
            btn.querySelector("span:last-child").textContent = "−";
          }
        });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
