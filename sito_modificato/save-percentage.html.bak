    <script>
      // Toggle known type (saves vs goals)
      const knownTypeRadios = document.querySelectorAll(
        'input[name="knownType"]'
      );
      const savesInputGroup = document.getElementById("savesInputGroup");
      const goalsInputGroup = document.getElementById("goalsInputGroup");
      const savesInput = document.getElementById("saves");
      const goalsInput = document.getElementById("goals");

      knownTypeRadios.forEach((r) => {
        r.addEventListener("change", () => {
          if (r.checked && r.value === "saves") {
            savesInputGroup.classList.remove("hidden");
            goalsInputGroup.classList.add("hidden");
            goalsInput.value = "";
          } else if (r.checked && r.value === "goals") {
            goalsInputGroup.classList.remove("hidden");
            savesInputGroup.classList.add("hidden");
            savesInput.value = "";
          }
        });
      });

      function formatClassic(decimal) {
        if (isNaN(decimal)) return ".---";
        const rounded = Math.round(decimal * 1000) / 1000;
        return "." + String(Math.floor(rounded * 1000)).padStart(3, "0");
      }

      function classifyTier(sv) {
        if (isNaN(sv)) return { label: "–", hint: "" };
        if (sv < 0.9)
          return {
            label: "Below average (season-long, pro level)",
            hint: "Below .900 is usually considered weak over a full season.",
          };
        if (sv < 0.91)
          return {
            label: "Slightly below average",
            hint: "Between .900 and .909 is often a bit below league average.",
          };
        if (sv < 0.92)
          return {
            label: "Around league average",
            hint: "Roughly .910–.919 is typical for many modern pro leagues.",
          };
        if (sv < 0.93)
          return {
            label: "Strong / above average",
            hint: "A save percentage in the .920s is usually considered strong.",
          };
        return {
          label: "Elite (over a large sample)",
          hint: "Sustaining .930+ over many games is elite at high levels.",
        };
      }

      const calcBtn = document.getElementById("calcBtn");
      const resetBtn = document.getElementById("resetBtn");
      const errorMsg = document.getElementById("errorMsg");

      const svPercentDisplay = document.getElementById("svPercentDisplay");
      const svPercentClassic = document.getElementById("svPercentClassic");
      const savesOut = document.getElementById("savesOut");
      const goalsOut = document.getElementById("goalsOut");
      const shotsOut = document.getElementById("shotsOut");
      const savesPerGame = document.getElementById("savesPerGame");
      const tierLabel = document.getElementById("tierLabel");
      const tierHint = document.getElementById("tierHint");

      function clearResults() {
        svPercentDisplay.textContent = "–";
        svPercentClassic.textContent = "(.--- format)";
        savesOut.textContent = "–";
        goalsOut.textContent = "–";
        shotsOut.textContent = "–";
        savesPerGame.textContent = "–";
        tierLabel.textContent = "–";
        tierHint.textContent = "Benchmarks based on modern pro-level hockey.";
      }

      function calculateSavePercentage() {
        errorMsg.classList.add("hidden");
        errorMsg.textContent = "";

        const shots = parseFloat(document.getElementById("shotsAgainst").value);
        const knownType = document.querySelector(
          'input[name="knownType"]:checked'
        )?.value;

        if (isNaN(shots) || shots <= 0) {
          errorMsg.textContent =
            "Please enter a positive number of shots against.";
          errorMsg.classList.remove("hidden");
          clearResults();
          return;
        }

        let savesVal, goalsVal;

        if (knownType === "saves") {
          savesVal = parseFloat(savesInput.value);
          if (isNaN(savesVal) || savesVal < 0) {
            errorMsg.textContent = "Please enter a valid number of saves.";
            errorMsg.classList.remove("hidden");
            clearResults();
            return;
          }
          if (savesVal > shots) {
            errorMsg.textContent = "Saves cannot exceed shots against.";
            errorMsg.classList.remove("hidden");
            clearResults();
            return;
          }
          goalsVal = shots - savesVal;
        } else {
          goalsVal = parseFloat(goalsInput.value);
          if (isNaN(goalsVal) || goalsVal < 0) {
            errorMsg.textContent =
              "Please enter a valid number of goals allowed.";
            errorMsg.classList.remove("hidden");
            clearResults();
            return;
          }
          if (goalsVal > shots) {
            errorMsg.textContent = "Goals allowed cannot exceed shots against.";
            errorMsg.classList.remove("hidden");
            clearResults();
            return;
          }
          savesVal = shots - goalsVal;
        }

        const sv = savesVal / shots;
        const svPercent = sv * 100;

        svPercentDisplay.textContent = isFinite(svPercent)
          ? svPercent.toFixed(2) + "%"
          : "–";
        svPercentClassic.textContent = "(" + formatClassic(sv) + " format)";
        savesOut.textContent = savesVal.toFixed(0);
        goalsOut.textContent = goalsVal.toFixed(0);
        shotsOut.textContent = shots.toFixed(0);

        // Saves per game estimate: assume 1 game unless user wants to divide; here we just echo shots as 1 game
        savesPerGame.textContent = savesVal.toFixed(1);

        const tier = classifyTier(sv);
        tierLabel.textContent = tier.label;
        if (tier.hint) {
          tierHint.textContent = tier.hint;
        } else {
          tierHint.textContent = "Benchmarks based on modern pro-level hockey.";
        }
      }

      calcBtn.addEventListener("click", calculateSavePercentage);

      resetBtn.addEventListener("click", () => {
        document.getElementById("shotsAgainst").value = "";
        savesInput.value = "";
        goalsInput.value = "";
        errorMsg.classList.add("hidden");
        errorMsg.textContent = "";
        clearResults();
      });

      // Preset buttons
      document.querySelectorAll("button[data-preset]").forEach((btn) => {
        btn.addEventListener("click", () => {
          const preset = btn.getAttribute("data-preset");
          let shots = 0,
            goals = 0;

          if (preset === "elite") {
            shots = 40;
            goals = 1;
          } else if (preset === "solid") {
            shots = 32;
            goals = 3;
          } else if (preset === "tough") {
            shots = 25;
            goals = 5;
          }

          document.getElementById("shotsAgainst").value = shots;
          document.querySelector(
            'input[name="knownType"][value="goals"]'
          ).checked = true;
          goalsInputGroup.classList.remove("hidden");
          savesInputGroup.classList.add("hidden");
          goalsInput.value = goals;
          savesInput.value = "";

          calculateSavePercentage();
        });
      });

      // Initialize
      clearResults();
    </script>

