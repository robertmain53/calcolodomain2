    <script>
      // Mode switching
      const ddmMode = document.getElementById("ddmMode");
      const gordonForm = document.getElementById("gordonForm");
      const multistageForm = document.getElementById("multistageForm");
      const zerogrowthForm = document.getElementById("zerogrowthForm");

      ddmMode.addEventListener("change", () => {
        const mode = ddmMode.value;
        gordonForm.classList.toggle("hidden", mode !== "gordon");
        multistageForm.classList.toggle("hidden", mode !== "multistage");
        zerogrowthForm.classList.toggle("hidden", mode !== "zerogrowth");
        document.getElementById("resultsCard").classList.add("hidden");
        document.getElementById("calcError").classList.add("hidden");
      });

      function formatCurrency(value) {
        if (!isFinite(value)) return "—";
        return "$" + value.toFixed(2);
      }

      function formatPercent(value) {
        if (!isFinite(value)) return "—";
        return value.toFixed(1) + "%";
      }

      const calculateBtn = document.getElementById("calculateBtn");
      const resetBtn = document.getElementById("resetBtn");
      const resultsCard = document.getElementById("resultsCard");
      const intrinsicValueEl = document.getElementById("intrinsicValue");
      const calcError = document.getElementById("calcError");
      const valuationBadge = document.getElementById("valuationBadge");
      const valuationBadgeText = document.getElementById("valuationBadgeText");
      const comparisonRow = document.getElementById("comparisonRow");
      const currentPriceDisplay = document.getElementById(
        "currentPriceDisplay"
      );
      const discountDisplay = document.getElementById("discountDisplay");
      const modelSummary = document.getElementById("modelSummary");
      const multistageBreakdown = document.getElementById(
        "multistageBreakdown"
      );
      const multistageTableBody = document.getElementById(
        "multistageTableBody"
      );
      const pvHighGrowthCell = document.getElementById("pvHighGrowthCell");
      const pvTerminalCell = document.getElementById("pvTerminalCell");

      calculateBtn.addEventListener("click", () => {
        calcError.classList.add("hidden");
        valuationBadge.classList.add("hidden");
        comparisonRow.classList.add("hidden");
        multistageBreakdown.classList.add("hidden");
        multistageTableBody.innerHTML = "";
        pvHighGrowthCell.textContent = "";
        pvTerminalCell.textContent = "";
        modelSummary.textContent = "";

        const mode = ddmMode.value;
        let intrinsic = NaN;
        let currentPrice = null;
        let summary = "";

        try {
          if (mode === "gordon") {
            const D1 = parseFloat(document.getElementById("gordonD1").value);
            const r =
              parseFloat(document.getElementById("gordonR").value) / 100;
            const g =
              parseFloat(document.getElementById("gordonG").value) / 100;
            const priceInput = document.getElementById("gordonPrice").value;

            if (!(D1 >= 0) || !(r > 0)) {
              throw new Error(
                "Please enter a positive next-year dividend and required return."
              );
            }
            if (g < 0) {
              throw new Error(
                "Growth rate cannot be negative in this simplified Gordon model."
              );
            }
            if (r <= g) {
              throw new Error(
                "Required return must be greater than growth rate (r > g) for the Gordon Growth model."
              );
            }

            intrinsic = D1 / (r - g);
            if (priceInput !== "") currentPrice = parseFloat(priceInput);
            summary = `Gordon Growth DDM with D₁ = ${formatCurrency(
              D1
            )}, r = ${formatPercent(r * 100)}, g = ${formatPercent(g * 100)}.`;
          } else if (mode === "multistage") {
            const D0 = parseFloat(document.getElementById("multiD0").value);
            const years = parseInt(
              document.getElementById("multiYears").value,
              10
            );
            const g1 =
              parseFloat(document.getElementById("multiGHigh").value) / 100;
            const g2 =
              parseFloat(document.getElementById("multiGLow").value) / 100;
            const r = parseFloat(document.getElementById("multiR").value) / 100;
            const priceInput = document.getElementById("multiPrice").value;

            if (!(D0 >= 0) || !(years >= 1) || !(r > 0)) {
              throw new Error(
                "Please enter a non-negative last dividend, at least 1 high-growth year, and a positive required return."
              );
            }
            if (r <= g2) {
              throw new Error(
                "Required return must be greater than terminal growth rate (r > g₂)."
              );
            }

            let pvHigh = 0;
            let dividends = [];
            for (let t = 1; t <= years; t++) {
              const Dt = D0 * Math.pow(1 + g1, t);
              const df = Math.pow(1 + r, t);
              const pv = Dt / df;
              pvHigh += pv;
              dividends.push({ year: t, Dt, df, pv });
            }

            const DN = D0 * Math.pow(1 + g1, years);
            const DNext = DN * (1 + g2);
            const TV = DNext / (r - g2);
            const pvTerminal = TV / Math.pow(1 + r, years);

            intrinsic = pvHigh + pvTerminal;
            if (priceInput !== "") currentPrice = parseFloat(priceInput);

            // Fill breakdown table
            dividends.forEach((row) => {
              const tr = document.createElement("tr");
              tr.innerHTML = `
            <td class="px-2 py-1 border-b text-left">${row.year}</td>
            <td class="px-2 py-1 border-b text-right">${formatCurrency(
              row.Dt
            )}</td>
            <td class="px-2 py-1 border-b text-right">${row.df.toFixed(3)}</td>
            <td class="px-2 py-1 border-b text-right">${formatCurrency(
              row.pv
            )}</td>
          `;
              multistageTableBody.appendChild(tr);
            });
            pvHighGrowthCell.textContent = formatCurrency(pvHigh);
            pvTerminalCell.textContent = formatCurrency(pvTerminal);
            multistageBreakdown.classList.remove("hidden");

            summary = `Multi-stage DDM with D₀ = ${formatCurrency(
              D0
            )}, high growth g₁ = ${formatPercent(
              g1 * 100
            )} for ${years} years, terminal growth g₂ = ${formatPercent(
              g2 * 100
            )}, and r = ${formatPercent(r * 100)}.`;
          } else if (mode === "zerogrowth") {
            const D = parseFloat(document.getElementById("zeroD").value);
            const r = parseFloat(document.getElementById("zeroR").value) / 100;
            const priceInput = document.getElementById("zeroPrice").value;

            if (!(D >= 0) || !(r > 0)) {
              throw new Error(
                "Please enter a non-negative dividend and a positive required return."
              );
            }

            intrinsic = D / r;
            if (priceInput !== "") currentPrice = parseFloat(priceInput);
            summary = `Zero-growth DDM with constant dividend D = ${formatCurrency(
              D
            )} and required return r = ${formatPercent(r * 100)}.`;
          }

          if (!isFinite(intrinsic) || intrinsic <= 0) {
            throw new Error(
              "The inputs produced an invalid intrinsic value. Please review your assumptions."
            );
          }

          intrinsicValueEl.textContent = formatCurrency(intrinsic);
          modelSummary.textContent = summary;

          if (currentPrice !== null && currentPrice > 0) {
            currentPriceDisplay.textContent = formatCurrency(currentPrice);
            const diff = intrinsic - currentPrice;
            const discountPct = (diff / currentPrice) * 100;
            discountDisplay.textContent =
              (discountPct >= 0 ? "+" : "") + discountPct.toFixed(1) + "%";
            comparisonRow.classList.remove("hidden");

            const badgeEl = valuationBadgeText;
            const badgeWrapper = valuationBadge;
            badgeWrapper.classList.remove("hidden");
            badgeWrapper.classList.remove(
              "bg-green-100",
              "text-green-800",
              "bg-red-100",
              "text-red-800",
              "bg-yellow-100",
              "text-yellow-800",
              "border",
              "border-green-200",
              "border-red-200",
              "border-yellow-200"
            );

            if (discountPct > 10) {
              badgeEl.textContent = "Potentially undervalued";
              badgeWrapper.classList.add(
                "bg-green-100",
                "text-green-800",
                "border",
                "border-green-200"
              );
            } else if (discountPct < -10) {
              badgeEl.textContent = "Potentially overvalued";
              badgeWrapper.classList.add(
                "bg-red-100",
                "text-red-800",
                "border",
                "border-red-200"
              );
            } else {
              badgeEl.textContent = "Close to fair value";
              badgeWrapper.classList.add(
                "bg-yellow-100",
                "text-yellow-800",
                "border",
                "border-yellow-200"
              );
            }
          }

          resultsCard.classList.remove("hidden");
        } catch (err) {
          calcError.textContent = err.message;
          calcError.classList.remove("hidden");
          resultsCard.classList.add("hidden");
        }
      });

      resetBtn.addEventListener("click", () => {
        // Reset to defaults
        document.getElementById("gordonD1").value = 2.0;
        document.getElementById("gordonR").value = 9;
        document.getElementById("gordonG").value = 3;
        document.getElementById("gordonPrice").value = "";

        document.getElementById("multiD0").value = 1.8;
        document.getElementById("multiYears").value = 5;
        document.getElementById("multiGHigh").value = 10;
        document.getElementById("multiGLow").value = 3;
