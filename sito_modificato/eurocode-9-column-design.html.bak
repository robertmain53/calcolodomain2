    <script>
      // Eurocode 9 column design calculator logic
      function getAlphaForCurve(curve) {
        switch (curve) {
          case "a":
            return 0.21;
          case "b":
            return 0.34;
          case "c":
            return 0.49;
          case "d":
            return 0.76;
          default:
            return 0.34;
        }
      }

      function calculateEurocode9Column() {
        const ned = parseFloat(document.getElementById("ned").value);
        const gammaM1 = parseFloat(document.getElementById("gammaM1").value);
        const f0 = parseFloat(document.getElementById("f0").value); // MPa
        const E = parseFloat(document.getElementById("E").value); // GPa
        const A_cm2 = parseFloat(document.getElementById("A").value);
        const Wel_cm3 = parseFloat(document.getElementById("Wel").value);
        const L = parseFloat(document.getElementById("L").value); // m

        let kSel = document.getElementById("kFactor").value;
        let k;
        if (kSel === "custom") {
          k = parseFloat(document.getElementById("kCustom").value);
        } else {
          k = parseFloat(kSel);
        }
        const curve = document.getElementById("bucklingCurve").value;

        const statusEl = document.getElementById("statusMessage");
        const nrdEl = document.getElementById("nrdDisplay");
        const utilEl = document.getElementById("utilizationDisplay");
        const lambdaEl = document.getElementById("lambdaBarDisplay");
        const chiEl = document.getElementById("chiDisplay");
        const badgeEl = document.getElementById("passFailBadge");
        const notesEl = document.getElementById("notesDisplay");

        // Basic validation
        if (
          [ned, gammaM1, f0, E, A_cm2, Wel_cm3, L, k].some(
            (v) => !isFinite(v) || v <= 0
          )
        ) {
          statusEl.textContent =
            "Please enter positive numeric values for all inputs.";
          nrdEl.textContent = "– kN";
          utilEl.textContent = "–";
          lambdaEl.textContent = "–";
          chiEl.textContent = "–";
          badgeEl.textContent = "Invalid input";
          badgeEl.className =
            "inline-flex items-center px-3 py-1 rounded-full text-xs font-semibold bg-red-100 text-red-700";
          notesEl.textContent = "";
          return;
        }

        statusEl.textContent = "";

        // Unit conversions
        const A = A_cm2 * 1e-4; // cm² -> m²
        const Wel = Wel_cm3 * 1e-6; // cm³ -> m³
        const E_Pa = E * 1e9; // GPa -> Pa
        const f0_Pa = f0 * 1e6; // MPa -> Pa

        // Squash load (characteristic and design)
        const Npl_Rk = A * f0_Pa; // N
        const Npl_Rd = Npl_Rk / gammaM1; // N

        // Approximate I from Wel and stress at extreme fiber ~ f0
        // I ≈ Wel * (Npl_Rk / A) = Wel * f0_Pa
        const I = Wel * f0_Pa; // m^4 (approx)

        const Leff = k * L; // m
        const Ncr = (Math.PI * Math.PI * E_Pa * I) / (Leff * Leff); // N

        // Non-dimensional slenderness
        let lambdaBar = Math.sqrt(Npl_Rk / Ncr);

        // Imperfection factor
        const alpha = getAlphaForCurve(curve);

        // Buckling reduction factor
        const phi =
          0.5 * (1 + alpha * (lambdaBar - 0.2) + lambdaBar * lambdaBar);
        const sqrtTerm = phi * phi - lambdaBar * lambdaBar;

        let chi;
        if (sqrtTerm <= 0 || !isFinite(sqrtTerm)) {
          chi = 0;
        } else {
          chi = 1 / (phi + Math.sqrt(sqrtTerm));
        }

        // Limit chi between 0 and 1
        chi = Math.max(0, Math.min(1, chi));

        const Nrd = chi * Npl_Rd; // N
        const Nrd_kN = Nrd / 1000.0;
        const utilization = ned / Nrd_kN;

        // Update UI
        nrdEl.textContent = Nrd_kN.toFixed(1) + " kN";
        utilEl.textContent = utilization.toFixed(2);
        lambdaEl.textContent = lambdaBar.toFixed(3);
        chiEl.textContent = chi.toFixed(3);

        if (utilization <= 1) {
          badgeEl.textContent = "OK – column passes axial compression check";
          badgeEl.className =
            "inline-flex items-center px-3 py-1 rounded-full text-xs font-semibold bg-green-100 text-green-700";
        } else {
          badgeEl.textContent =
            "Not OK – increase section or reduce length/load";
          badgeEl.className =
            "inline-flex items-center px-3 py-1 rounded-full text-xs font-semibold bg-red-100 text-red-700";
        }

        notesEl.textContent =
          "Npl,Rd (no buckling) = " +
          (Npl_Rd / 1000).toFixed(1) +
          " kN, effective length L_eff = " +
          Leff.toFixed(3) +
          " m, buckling curve " +
          curve.toUpperCase() +
          " (α = " +
          alpha.toFixed(2) +
          "). I is approximated from W_el; for final design use exact I from section tables.";
      }

      document.addEventListener("DOMContentLoaded", function () {
        const kSelect = document.getElementById("kFactor");
        const kCustomInput = document.getElementById("kCustom");
        const calcBtn = document.getElementById("calculateBtn");
        const resetBtn = document.getElementById("resetBtn");

        kSelect.addEventListener("change", function () {
          if (this.value === "custom") {
            kCustomInput.classList.remove("hidden");
          } else {
            kCustomInput.classList.add("hidden");
          }
        });

        calcBtn.addEventListener("click", calculateEurocode9Column);

        resetBtn.addEventListener("click", function () {
          document.getElementById("ned").value = 300;
          document.getElementById("gammaM1").value = 1.1;
          document.getElementById("f0").value = 215;
          document.getElementById("E").value = 70;
          document.getElementById("A").value = 20;
          document.getElementById("Wel").value = 150;
          document.getElementById("L").value = 3.0;
          document.getElementById("kFactor").value = "1.0";
          document.getElementById("kCustom").value = "";
          document.getElementById("kCustom").classList.add("hidden");
          document.getElementById("bucklingCurve").value = "b";
          document.getElementById("axis").value = "major";

          document.getElementById("statusMessage").textContent = "";
          document.getElementById("nrdDisplay").textContent = "– kN";
          document.getElementById("utilizationDisplay").textContent = "–";
          document.getElementById("lambdaBarDisplay").textContent = "–";
          document.getElementById("chiDisplay").textContent = "–";
          const badgeEl = document.getElementById("passFailBadge");
          badgeEl.textContent = "Awaiting input…";
          badgeEl.className =
            "inline-flex items-center px-3 py-1 rounded-full text-xs font-semibold bg-gray-200 text-gray-700";
          document.getElementById("notesDisplay").textContent = "";
        });

        // Auto-run once with default values
        calculateEurocode9Column();
      });
    </script>

