    <script>
      function formatRatio(value) {
        if (!isFinite(value)) return "–";
        if (value === 0) return "0.00";
        if (Math.abs(value) < 0.01) return value.toExponential(2);
        return value.toFixed(2);
      }

      function interpretCashRatio(ratio) {
        if (!isFinite(ratio) || ratio < 0) {
          return "Please enter non-negative values. Current liabilities must be greater than zero to compute a meaningful cash ratio.";
        }
        if (ratio === 0) {
          return "Cash ratio of 0.00 – no cash or cash equivalents relative to current liabilities. This is an extremely risky liquidity position.";
        }
        if (ratio < 0.2) {
          return "Very low cash ratio – the company relies heavily on collections, refinancing, or inventory sales to meet short-term obligations. Liquidity risk is high.";
        }
        if (ratio < 0.5) {
          return "Below-conservative cash ratio – may be acceptable in some stable or capital-intensive industries, but leaves limited buffer for shocks.";
        }
        if (ratio < 1.0) {
          return "Comfortable cash ratio – cash and cash equivalents cover a significant portion of current liabilities, indicating solid short-term liquidity.";
        }
        if (ratio < 2.0) {
          return "Very strong cash ratio – the company could fully cover current liabilities with cash and near-cash assets. Check whether excess cash is being deployed efficiently.";
        }
        return "Extremely high cash ratio – the balance sheet is very liquid, but the company may be holding more cash than it needs for operations and risk management.";
      }

      document.addEventListener("DOMContentLoaded", function () {
        const cashInput = document.getElementById("cash");
        const marketableInput = document.getElementById("marketable");
        const currentLiabInput = document.getElementById("currentLiabilities");
        const calcBtn = document.getElementById("calcBtn");
        const resetBtn = document.getElementById("resetBtn");
        const exampleBtn = document.getElementById("exampleBtn");
        const ratioDisplay = document.getElementById("cashRatioDisplay");
        const ratioLabel = document.getElementById("cashRatioLabel");
        const interpretationEl = document.getElementById(
          "cashRatioInterpretation"
        );

        function calculate() {
          const cash = parseFloat(cashInput.value) || 0;
          const marketable = parseFloat(marketableInput.value) || 0;
          const currentLiab = parseFloat(currentLiabInput.value);

          if (!currentLiab || currentLiab <= 0) {
            ratioDisplay.textContent = "–";
            ratioLabel.textContent = "";
            interpretationEl.textContent =
              "Please enter a positive amount for current liabilities to calculate the cash ratio.";
            return;
          }

          const numerator = cash + marketable;
          const ratio = numerator / currentLiab;

          ratioDisplay.textContent = formatRatio(ratio);
          ratioLabel.textContent = "times current liabilities";
          interpretationEl.textContent = interpretCashRatio(ratio);
        }

        function resetForm() {
          cashInput.value = "";
          marketableInput.value = "";
          currentLiabInput.value = "";
          ratioDisplay.textContent = "–";
          ratioLabel.textContent = "";
          interpretationEl.textContent =
            "Enter values and click “Calculate” to see your cash ratio.";
        }

        function loadExample() {
          cashInput.value = 250000;
          marketableInput.value = 50000;
          currentLiabInput.value = 400000;
          calculate();
        }

        calcBtn.addEventListener("click", function (e) {
          e.preventDefault();
          calculate();
        });

        resetBtn.addEventListener("click", function (e) {
          e.preventDefault();
          resetForm();
        });

        exampleBtn.addEventListener("click", function (e) {
          e.preventDefault();
          loadExample();
        });

        [cashInput, marketableInput, currentLiabInput].forEach(function (el) {
          el.addEventListener("keyup", function (event) {
            if (event.key === "Enter") {
              calculate();
            }
          });
        });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
