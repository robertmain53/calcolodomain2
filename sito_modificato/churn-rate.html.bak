    <script>
      // Tab switching
      const tabButtons = document.querySelectorAll(".churn-tab");
      const tabPanels = {
        customer: document.getElementById("tab-customer"),
        revenue: document.getElementById("tab-revenue"),
        cohort: document.getElementById("tab-cohort"),
      };

      tabButtons.forEach((btn) => {
        btn.addEventListener("click", () => {
          const target = btn.getAttribute("data-tab");
          tabButtons.forEach((b) => {
            b.classList.remove("border-blue-600", "text-blue-600");
            b.classList.add("border-transparent", "text-gray-500");
          });
          btn.classList.remove("border-transparent", "text-gray-500");
          btn.classList.add("border-blue-600", "text-blue-600");

          Object.keys(tabPanels).forEach((key) => {
            tabPanels[key].classList.toggle("hidden", key !== target);
          });
        });
      });

      // Helper: parse number safely
      function num(val) {
        const n = parseFloat(val);
        return isNaN(n) ? 0 : n;
      }

      // Helper: format percentage
      function fmtPct(value, decimals = 2) {
        if (!isFinite(value)) return "–";
        return value.toFixed(decimals) + "%";
      }

      // Helper: annualize churn from period churn and days
      function annualizeChurn(periodChurn, days) {
        if (periodChurn <= 0 || days <= 0) return 0;
        const rate = periodChurn / 100;
        const annualRate = 1 - Math.pow(1 - rate, 365 / days);
        return annualRate * 100;
      }

      // CUSTOMER CHURN LOGIC
      const custPeriodEl = document.getElementById("cust-period");
      const custCustomDaysWrapper = document.getElementById(
        "cust-custom-days-wrapper"
      );
      const custCustomDaysEl = document.getElementById("cust-custom-days");

      custPeriodEl.addEventListener("change", () => {
        custCustomDaysWrapper.classList.toggle(
          "hidden",
          custPeriodEl.value !== "custom"
        );
      });

      document.getElementById("cust-calc-btn").addEventListener("click", () => {
        const start = num(document.getElementById("cust-start").value);
        const endRaw = document.getElementById("cust-end").value;
        const end = endRaw === "" ? null : num(endRaw);
        const acquired = num(document.getElementById("cust-new").value);

        if (start <= 0) {
          alert(
            "Please enter customers at the start of the period (must be greater than 0)."
          );
          return;
        }

        let customersLost;
        if (end !== null) {
          customersLost = start + acquired - end;
        } else {
          // If end is unknown, ask user to input customers lost directly via prompt
          const lostInput = prompt(
            "Customers at end is blank. Enter customers lost during the period (or Cancel to abort):",
            ""
          );
          if (lostInput === null || lostInput.trim() === "") return;
          customersLost = num(lostInput);
        }

        if (customersLost < 0) customersLost = 0;

        const denomMethod = document.querySelector(
          'input[name="cust-denominator"]:checked'
        ).value;
        let denominator = start;
        if (denomMethod === "average" && end !== null) {
          denominator = (start + end) / 2;
        }
        if (denominator <= 0) denominator = start;

        const churnRate =
          denominator > 0 ? (customersLost / denominator) * 100 : 0;

        // Period length
        let days;
        switch (custPeriodEl.value) {
          case "month":
            days = 30;
            break;
          case "quarter":
            days = 90;
            break;
          case "year":
            days = 365;
            break;
          case "custom":
            days = num(custCustomDaysEl.value);
            if (days <= 0) {
              alert("Please enter a valid custom period length in days.");
              return;
            }
            break;
          default:
            days = 30;
        }

        const annualChurn = annualizeChurn(churnRate, days);
        const lifetimePeriods =
          churnRate > 0 ? 1 / (churnRate / 100) : Infinity;
        let lifetimeText;
        if (!isFinite(lifetimePeriods)) {
          lifetimeText = "–";
        } else {
          let unit;
          switch (custPeriodEl.value) {
            case "month":
              unit = "months";
              break;
            case "quarter":
              unit = "quarters";
              break;
            case "year":
              unit = "years";
              break;
            default:
              unit = "periods";
          }
          lifetimeText = lifetimePeriods.toFixed(1) + " " + unit;
        }

        document.getElementById("cust-lost").textContent =
          customersLost.toFixed(0);
        document.getElementById("cust-churn-rate").textContent = fmtPct(
          churnRate,
          2
        );
        document.getElementById("cust-churn-annual").textContent = fmtPct(
          annualChurn,
          2
        );
        document.getElementById("cust-lifetime").textContent = lifetimeText;

        const interp = [];
        interp.push(
          `Your ${
            custPeriodEl.value === "month"
              ? "monthly"
              : custPeriodEl.value === "year"
              ? "annual"
              : "period"
          } customer churn is approximately ${fmtPct(churnRate, 2)}.`
        );
        if (annualChurn > 0) {
          interp.push(
            `This corresponds to about ${fmtPct(
              annualChurn,
              2
            )} annualized churn.`
          );
        }
        if (isFinite(lifetimePeriods) && lifetimePeriods > 0) {
          interp.push(
            `At this churn rate, the average customer lifetime is roughly ${lifetimeText}.`
          );
        }
        document.getElementById("cust-interpretation").textContent =
          interp.join(" ");

        document.getElementById("cust-results").classList.remove("hidden");
      });

      document
        .getElementById("cust-reset-btn")
        .addEventListener("click", () => {
          ["cust-start", "cust-end", "cust-new", "cust-custom-days"].forEach(
            (id) => {
              const el = document.getElementById(id);
              if (el) el.value = "";
            }
          );
          document.getElementById("cust-results").classList.add("hidden");
        });

      // REVENUE CHURN LOGIC
      const revPeriodEl = document.getElementById("rev-period");
      const revCustomDaysWrapper = document.getElementById(
        "rev-custom-days-wrapper"
      );
      const revCustomDaysEl = document.getElementById("rev-custom-days");

      revPeriodEl.addEventListener("change", () => {
        revCustomDaysWrapper.classList.toggle(
          "hidden",
          revPeriodEl.value !== "custom"
        );
      });

      document.getElementById("rev-calc-btn").addEventListener("click", () => {
        const startMRR = num(document.getElementById("rev-start-mrr").value);
        const churnedMRR = num(
          document.getElementById("rev-churned-mrr").value
        );
        const contractionMRR = num(
          document.getElementById("rev-contraction-mrr").value
        );
        const expansionMRR = num(
          document.getElementById("rev-expansion-mrr").value
        );

        if (startMRR <= 0) {
          alert(
            "Please enter starting MRR from existing customers (must be greater than 0)."
          );
          return;
        }

        const grossChurn = (churnedMRR / startMRR) * 100;
        const netChurn =
          ((churnedMRR + contractionMRR - expansionMRR) / startMRR) * 100;
        const nrr = 100 - netChurn;
        const endingMRR = startMRR - churnedMRR - contractionMRR + expansionMRR;

        document.getElementById("rev-gross-churn").textContent = fmtPct(
          Math.max(grossChurn, 0),
          2
        );
        document.getElementById("rev-net-churn").textContent = fmtPct(
          netChurn,
          2
        );
        document.getElementById("rev-nrr").textContent = fmtPct(nrr, 2);
        document.getElementById("rev-ending-mrr").textContent = isFinite(
          endingMRR
        )
          ? endingMRR.toFixed(2)
          : "–";

        const interp = [];
        interp.push(
          `Your gross revenue churn for this period is ${fmtPct(
            Math.max(grossChurn, 0),
            2
          )}.`
        );
        interp.push(
          `After accounting for downgrades and expansions, your net revenue churn is ${fmtPct(
            netChurn,
            2
          )}, which implies an NRR of ${fmtPct(nrr, 2)}.`
        );
        if (nrr > 100) {
          interp.push(
            "This is strong: existing customers are increasing their spend overall."
          );
        } else if (nrr >= 90) {
          interp.push(
            "NRR is slightly below 100%; consider focusing on expansion and reducing downgrades."
          );
        } else {
          interp.push(
            "NRR is well below 100%; investigate churn reasons and pricing or product fit."
          );
        }
        document.getElementById("rev-interpretation").textContent =
          interp.join(" ");

        document.getElementById("rev-results").classList.remove("hidden");
      });

      document.getElementById("rev-reset-btn").addEventListener("click", () => {
        [
          "rev-start-mrr",
          "rev-new-mrr",
          "rev-churned-mrr",
          "rev-contraction-mrr",
          "rev-expansion-mrr",
          "rev-custom-days",
        ].forEach((id) => {
          const el = document.getElementById(id);
          if (el) el.value = "";
        });
        document.getElementById("rev-results").classList.add("hidden");
      });

      // COHORT COMPARISON LOGIC
      document
        .getElementById("cohort-calc-btn")
        .addEventListener("click", () => {
          const aStart = num(document.getElementById("cohort-a-start").value);
          const aLost = num(document.getElementById("cohort-a-lost").value);
          const aDays = num(document.getElementById("cohort-a-days").value);

          const bStart = num(document.getElementById("cohort-b-start").value);
          const bLost = num(document.getElementById("cohort-b-lost").value);
          const bDays = num(document.getElementById("cohort-b-days").value);

          if (aStart <= 0 || bStart <= 0 || aDays <= 0 || bDays <= 0) {
            alert(
              "Please enter valid start customers and period length (days) for both periods."
            );
            return;
          }

          const aChurn = (aLost / aStart) * 100;
          const bChurn = (bLost / bStart) * 100;

          const aAnnual = annualizeChurn(aChurn, aDays);
          const bAnnual = annualizeChurn(bChurn, bDays);

          document.getElementById(
            "cohort-a-summary"
          ).textContent = `Churn: ${fmtPct(
            aChurn,
            2
          )} for this period (${aDays} days), ≈ ${fmtPct(
            aAnnual,
            2
          )} annualized.`;
          document.getElementById(
            "cohort-b-summary"
          ).textContent = `Churn: ${fmtPct(
            bChurn,
            2
          )} for this period (${bDays} days), ≈ ${fmtPct(
            bAnnual,
            2
          )} annualized.`;

          let interp = "";
          if (aAnnual < bAnnual) {
            interp =
              "Period A has lower annualized churn than Period B. Whatever changed between A and B (pricing, onboarding, product) may have hurt retention.";
          } else if (aAnnual > bAnnual) {
            interp =
              "Period B has lower annualized churn than Period A. Changes between A and B may have improved retention.";
          } else {
            interp =
              "Both periods have similar annualized churn. Look at more granular segments (plan, channel, country) to find differences.";
          }
          document.getElementById("cohort-interpretation").textContent = interp;

          document.getElementById("cohort-results").classList.remove("hidden");
        });

      document
        .getElementById("cohort-reset-btn")
        .addEventListener("click", () => {
          [
            "cohort-a-start",
            "cohort-a-lost",
            "cohort-a-days",
            "cohort-b-start",
            "cohort-b-lost",
            "cohort-b-days",
          ].forEach((id) => {
            const el = document.getElementById(id);
            if (el) el.value = "";
          });
          document.getElementById("cohort-results").classList.add("hidden");
        });

      // Mobile menu script (simple)
      const mobileToggle = document.getElementById("mobile-menu-toggle");
      const mobileMenu = document.getElementById("mobile-menu");
      if (mobileToggle && mobileMenu) {
        mobileToggle.addEventListener("click", () => {
          const isHidden = mobileMenu.classList.contains("hidden");
          mobileMenu.classList.toggle("hidden", !isHidden);
          mobileToggle.setAttribute(
            "aria-expanded",
            isHidden ? "true" : "false"
          );
        });
      }

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
