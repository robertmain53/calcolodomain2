    <script>
      // Height sync
      function syncHeightFromCm() {
        const cm = parseFloat(document.getElementById("heightCm").value);
        if (!cm || cm <= 0) return;
        const totalIn = cm / 2.54;
        const ft = Math.floor(totalIn / 12);
        const inch = Math.round(totalIn - ft * 12);
        document.getElementById("heightFt").value = ft;
        document.getElementById("heightIn").value = inch;
      }
      function syncHeightFromFtIn() {
        const ft = parseFloat(document.getElementById("heightFt").value) || 0;
        const inch = parseFloat(document.getElementById("heightIn").value) || 0;
        const totalIn = ft * 12 + inch;
        if (!totalIn || totalIn <= 0) return;
        const cm = Math.round(totalIn * 2.54);
        document.getElementById("heightCm").value = cm;
      }

      // Weight sync
      function syncWeightFromKg() {
        const kg = parseFloat(document.getElementById("weightKg").value);
        if (!kg || kg <= 0) return;
        const lb = Math.round(kg * 2.20462);
        document.getElementById("weightLb").value = lb;
      }
      function syncWeightFromLb() {
        const lb = parseFloat(document.getElementById("weightLb").value);
        if (!lb || lb <= 0) return;
        const kg = Math.round(lb / 2.20462);
        document.getElementById("weightKg").value = kg;
      }

      document
        .getElementById("heightCm")
        .addEventListener("input", syncHeightFromCm);
      document
        .getElementById("heightFt")
        .addEventListener("input", syncHeightFromFtIn);
      document
        .getElementById("heightIn")
        .addEventListener("input", syncHeightFromFtIn);
      document
        .getElementById("weightKg")
        .addEventListener("input", syncWeightFromKg);
      document
        .getElementById("weightLb")
        .addEventListener("input", syncWeightFromLb);

      function calculateBmi(weightKg, heightCm) {
        const m = heightCm / 100;
        if (!m || m <= 0) return null;
        return weightKg / (m * m);
      }

      function getMacroPattern() {
        const selected = document.querySelector(
          'input[name="macroPattern"]:checked'
        );
        const val = selected ? selected.value : "balanced";
        if (val === "higher_protein") {
          return { carbPct: 0.4, proteinPct: 0.3, fatPct: 0.3 };
        }
        if (val === "lower_carb") {
          return { carbPct: 0.3, proteinPct: 0.3, fatPct: 0.4 };
        }
        return { carbPct: 0.5, proteinPct: 0.2, fatPct: 0.3 };
      }

      function adjustCaloriesForGoal(tdee, goal) {
        if (!tdee) return null;
        let delta = 0;
        if (goal === "mild_loss") delta = -250;
        else if (goal === "loss") delta = -500;
        else if (goal === "mild_gain") delta = 250;
        else if (goal === "gain") delta = 500;
        const result = tdee + delta;
        return result < 1200 ? 1200 : Math.round(result);
      }

      function estimateFoodServings(calories) {
        // Simple scaling around 2000 kcal baseline
        const factor = calories / 2000;
        function clamp(val, min, max) {
          return Math.min(Math.max(val, min), max);
        }
        return {
          veg: clamp((4 * factor).toFixed(1), 3, 7) + " servings/day",
          fruit: clamp((3 * factor).toFixed(1), 2, 5) + " servings/day",
          grains: clamp((5 * factor).toFixed(1), 3, 9) + " servings/day",
          protein: clamp((2.5 * factor).toFixed(1), 2, 5) + " servings/day",
          dairy: clamp((2.5 * factor).toFixed(1), 1, 4) + " servings/day",
          fats:
            clamp((3 * factor).toFixed(1), 2, 6) +
            " servings/day (teaspoons–tablespoons equivalent)",
        };
      }

      document
        .getElementById("calculateBtn")
        .addEventListener("click", function () {
          const age = parseFloat(document.getElementById("age").value);
          const sex = document.getElementById("sex").value;
          const heightCm = parseFloat(
            document.getElementById("heightCm").value
          );
          const weightKg = parseFloat(
            document.getElementById("weightKg").value
          );
          const activity = parseFloat(
            document.getElementById("activity").value
          );
          const goal = document.getElementById("goal").value;

          if (!age || !heightCm || !weightKg || !activity) {
            alert("Please fill in age, height, weight, and activity level.");
            return;
          }

          // BMR
          let bmr;
          if (sex === "male") {
            bmr = 10 * weightKg + 6.25 * heightCm - 5 * age + 5;
          } else {
            bmr = 10 * weightKg + 6.25 * heightCm - 5 * age - 161;
          }

          const tdee = bmr * activity;
          const calories = adjustCaloriesForGoal(tdee, goal);

          const macros = getMacroPattern();
          const carbKcal = calories * macros.carbPct;
          const proteinKcal = calories * macros.proteinPct;
          const fatKcal = calories * macros.fatPct;

          const carbGrams = Math.round(carbKcal / 4);
          const proteinGrams = Math.round(proteinKcal / 4);
          const fatGrams = Math.round(fatKcal / 9);

          const proteinPerKg = (proteinGrams / weightKg).toFixed(2);

          document.getElementById("caloriesResult").textContent =
            calories.toLocaleString();
          document.getElementById("proteinGrams").textContent = proteinGrams;
          document.getElementById("carbGrams").textContent = carbGrams;
          document.getElementById("fatGrams").textContent = fatGrams;
          document.getElementById("proteinPerKg").textContent = proteinPerKg;

          const servings = estimateFoodServings(calories);
          document.getElementById("vegServings").textContent = servings.veg;
          document.getElementById("fruitServings").textContent = servings.fruit;
          document.getElementById("grainServings").textContent =
            servings.grains;
          document.getElementById("proteinServings").textContent =
            servings.protein;
          document.getElementById("dairyServings").textContent = servings.dairy;
          document.getElementById("fatServings").textContent = servings.fats;

          const bmi = calculateBmi(weightKg, heightCm);
          if (bmi) {
            let category = "";
            if (bmi < 18.5) category = "underweight";
            else if (bmi < 25) category = "normal weight";
            else if (bmi < 30) category = "overweight";
            else category = "obesity range";
            document.getElementById("bmiInfo").textContent =
              "Approx. BMI: " +
              bmi.toFixed(1) +
              " (" +
              category +
              "). BMI is a screening tool, not a diagnosis.";
          } else {
            document.getElementById("bmiInfo").textContent = "";
          }
        });

      // Initial auto-calc on load
      window.addEventListener("load", function () {
        syncHeightFromCm();
        syncWeightFromKg();
        document.getElementById("calculateBtn").click();
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
