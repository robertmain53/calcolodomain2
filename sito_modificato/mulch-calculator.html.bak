    <script>
      // Utility functions
      function roundTo(value, decimals) {
        const factor = Math.pow(10, decimals);
        return Math.round((value + Number.EPSILON) * factor) / factor;
      }

      // Dynamic areas
      const areasContainer = document.getElementById("areas-container");
      let areaIndex = 0;

      function createAreaRow() {
        areaIndex++;
        const div = document.createElement("div");
        div.className = "area-row border rounded-lg p-3 bg-gray-50 relative";
        div.dataset.index = areaIndex;
        div.innerHTML = `
      <div class="flex justify-between items-center mb-2">
        <h3 class="text-sm font-semibold text-gray-800">Area ${areaIndex}</h3>
        <button type="button" class="remove-area text-xs text-red-600 hover:underline">Remove</button>
      </div>
      <div class="grid md:grid-cols-3 gap-3 area-inputs">
        <div class="rect-inputs">
          <label class="block text-xs font-medium text-gray-700 mb-1">Length</label>
          <div class="flex gap-2">
            <input type="number" min="0" step="0.01" class="length-input w-full border rounded px-2 py-1.5 text-sm">
            <span class="length-unit text-xs text-gray-500 self-center">ft</span>
          </div>
        </div>
        <div class="rect-inputs">
          <label class="block text-xs font-medium text-gray-700 mb-1">Width</label>
          <div class="flex gap-2">
            <input type="number" min="0" step="0.01" class="width-input w-full border rounded px-2 py-1.5 text-sm">
            <span class="width-unit text-xs text-gray-500 self-center">ft</span>
          </div>
        </div>
        <div class="circle-inputs hidden">
          <label class="block text-xs font-medium text-gray-700 mb-1">Radius</label>
          <div class="flex gap-2">
            <input type="number" min="0" step="0.01" class="radius-input w-full border rounded px-2 py-1.5 text-sm">
            <span class="radius-unit text-xs text-gray-500 self-center">ft</span>
          </div>
          <p class="text-[11px] text-gray-500 mt-1">Or enter diameter instead:</p>
          <div class="flex gap-2 mt-1">
            <input type="number" min="0" step="0.01" class="diameter-input w-full border rounded px-2 py-1.5 text-sm" placeholder="Diameter">
            <span class="diameter-unit text-xs text-gray-500 self-center">ft</span>
          </div>
        </div>
        <div class="irregular-inputs hidden">
          <label class="block text-xs font-medium text-gray-700 mb-1">Area</label>
          <div class="flex gap-2">
            <input type="number" min="0" step="0.01" class="area-input w-full border rounded px-2 py-1.5 text-sm">
            <span class="area-unit text-xs text-gray-500 self-center">ft²</span>
          </div>
        </div>
      </div>
    `;
        areasContainer.appendChild(div);
      }

      // Initial area
      createAreaRow();

      document.getElementById("add-area-btn").addEventListener("click", () => {
        createAreaRow();
        syncShapeInputs();
        syncUnitLabels();
      });

      areasContainer.addEventListener("click", (e) => {
        if (e.target.classList.contains("remove-area")) {
          const rows = areasContainer.querySelectorAll(".area-row");
          if (rows.length > 1) {
            e.target.closest(".area-row").remove();
          }
        }
      });

      // Shape tabs
      const shapeTabs = document.querySelectorAll(".shape-tab");
      let currentShape = "rectangle";

      function syncShapeInputs() {
        const rows = areasContainer.querySelectorAll(".area-row");
        rows.forEach((row) => {
          const rect = row.querySelectorAll(".rect-inputs");
          const circle = row.querySelector(".circle-inputs");
          const irregular = row.querySelector(".irregular-inputs");
          rect.forEach((el) => el.classList.add("hidden"));
          circle.classList.add("hidden");
          irregular.classList.add("hidden");

          if (currentShape === "rectangle") {
            rect.forEach((el) => el.classList.remove("hidden"));
          } else if (currentShape === "circle") {
            circle.classList.remove("hidden");
          } else {
            irregular.classList.remove("hidden");
          }
        });
      }

      shapeTabs.forEach((tab) => {
        tab.addEventListener("click", () => {
          shapeTabs.forEach((t) => t.classList.remove("tab-active"));
          tab.classList.add("tab-active");
          currentShape = tab.dataset.shape;
          syncShapeInputs();
        });
      });

      // Unit tabs
      const unitTabs = document.querySelectorAll(".unit-tab");
      let currentUnits = "imperial";

      function syncUnitLabels() {
        const rows = areasContainer.querySelectorAll(".area-row");
        rows.forEach((row) => {
          row
            .querySelectorAll(
              ".length-unit, .width-unit, .radius-unit, .diameter-unit"
            )
            .forEach((span) => {
              span.textContent = currentUnits === "imperial" ? "ft" : "m";
            });
          const areaUnitSpan = row.querySelector(".area-unit");
          if (areaUnitSpan)
            areaUnitSpan.textContent =
              currentUnits === "imperial" ? "ft²" : "m²";
        });

        // Depth unit options
        const depthUnit = document.getElementById("depth-unit");
        const prev = depthUnit.value;
        depthUnit.innerHTML =
          currentUnits === "imperial"
            ? '<option value="in">inches</option><option value="ft">feet</option>'
            : '<option value="cm">cm</option><option value="m">meters</option>';
        if ([...depthUnit.options].some((o) => o.value === prev))
          depthUnit.value = prev;
      }

      unitTabs.forEach((tab) => {
        tab.addEventListener("click", () => {
          unitTabs.forEach((t) => t.classList.remove("tab-active"));
          tab.classList.add("tab-active");
          currentUnits = tab.dataset.units;
          syncUnitLabels();
        });
      });

      // Calculation
      const errorMsg = document.getElementById("error-msg");
      const resultsSection = document.getElementById("results-section");

      function clearErrors() {
        errorMsg.textContent = "";
        areasContainer
          .querySelectorAll("input")
          .forEach((i) => i.classList.remove("input-error"));
        document.getElementById("depth-input").classList.remove("input-error");
      }

      function getTotalArea() {
        let totalArea = 0;
        const rows = areasContainer.querySelectorAll(".area-row");
        for (const row of rows) {
          if (currentShape === "rectangle") {
            const len = parseFloat(row.querySelector(".length-input").value);
            const wid = parseFloat(row.querySelector(".width-input").value);
            if (isNaN(len) || len <= 0) {
              row.querySelector(".length-input").classList.add("input-error");
              throw new Error("Enter a valid length for each rectangle.");
            }
            if (isNaN(wid) || wid <= 0) {
              row.querySelector(".width-input").classList.add("input-error");
              throw new Error("Enter a valid width for each rectangle.");
            }
            totalArea += len * wid;
          } else if (currentShape === "circle") {
            const rInput = row.querySelector(".radius-input");
            const dInput = row.querySelector(".diameter-input");
            let r = parseFloat(rInput.value);
            const d = parseFloat(dInput.value);
            if ((isNaN(r) || r <= 0) && (isNaN(d) || d <= 0)) {
              rInput.classList.add("input-error");
              dInput.classList.add("input-error");
              throw new Error("Enter a radius or diameter for each circle.");
            }
            if (!isNaN(d) && d > 0) {
              r = d / 2;
            }
            totalArea += Math.PI * r * r;
          } else {
            const aInput = row.querySelector(".area-input");
            const a = parseFloat(aInput.value);
            if (isNaN(a) || a <= 0) {
              aInput.classList.add("input-error");
              throw new Error("Enter a valid area for each irregular bed.");
            }
            totalArea += a;
          }
        }
        return totalArea;
      }

      document.getElementById("calculate-btn").addEventListener("click", () => {
        clearErrors();
        try {
          let area = getTotalArea(); // in ft² or m² depending on currentUnits

          // Depth
          const depthVal = parseFloat(
            document.getElementById("depth-input").value
          );
          const depthUnit = document.getElementById("depth-unit").value;
          if (isNaN(depthVal) || depthVal <= 0) {
            document.getElementById("depth-input").classList.add("input-error");
            throw new Error("Enter a valid mulch depth.");
          }

          // Convert area to ft² and depth to ft
          let areaFt2;
          if (currentUnits === "imperial") {
            areaFt2 = area;
          } else {
            areaFt2 = area * 10.7639; // m² to ft²
          }

          let depthFt;
          if (depthUnit === "in") depthFt = depthVal / 12;
          else if (depthUnit === "ft") depthFt = depthVal;
          else if (depthUnit === "cm") depthFt = depthVal / 30.48;
          else depthFt = depthVal * 3.28084; // m to ft

          const volumeFt3 = areaFt2 * depthFt;
          if (volumeFt3 <= 0)
            throw new Error("Calculated volume is zero. Check your inputs.");

          const volumeYd3 = volumeFt3 / 27;
          const volumeM3 = volumeFt3 * 0.0283168;
          const volumeL = volumeM3 * 1000;

          // Bags
          const bagSize = parseFloat(document.getElementById("bag-size").value);
          const bagUnit = document.getElementById("bag-size-unit").value;
          let bags = 0;
          if (!isNaN(bagSize) && bagSize > 0) {
            if (bagUnit === "ft3") {
              bags = volumeFt3 / bagSize;
            } else {
              bags = volumeL / bagSize;
            }
          }

          // Cost
          const pricePerYard = parseFloat(
            document.getElementById("price-per-yard").value
          );
          const pricePerBag = parseFloat(
            document.getElementById("price-per-bag").value
          );
          let costBulk = null;
          let costBags = null;
          if (!isNaN(pricePerYard) && pricePerYard > 0) {
            costBulk = volumeYd3 * pricePerYard;
          }
          if (!isNaN(pricePerBag) && pricePerBag > 0 && bags > 0) {
            costBags = bags * pricePerBag;
          }

          // Weight (optional)
          const density = parseFloat(
            document.getElementById("mulch-density").value
          );
          let weightText = "";
          if (!isNaN(density) && density > 0) {
            const massKg = volumeM3 * density;
            const massLb = massKg * 2.20462;
            weightText = `Approximate mulch weight: ${roundTo(
              massKg,
              0
            ).toLocaleString()} kg (${roundTo(
              massLb,
              0
            ).toLocaleString()} lb).`;
          }

          // Update UI
          document.getElementById("result-yards").textContent = roundTo(
            volumeYd3,
            2
          ).toLocaleString();
          document.getElementById("result-feet").textContent = roundTo(
            volumeFt3,
            1
          ).toLocaleString();
          document.getElementById("result-m3").textContent = roundTo(
            volumeM3,
            3
          ).toLocaleString();
          document.getElementById("result-liters").textContent = roundTo(
            volumeL,
            0
          ).toLocaleString();
          document.getElementById("result-bags").textContent =
            bags > 0 ? roundTo(bags, 1).toLocaleString() : "0";
          document.getElementById("result-cost-bulk").textContent =
            costBulk !== null
              ? `$${roundTo(costBulk, 2).toLocaleString()}`
              : "–";
          document.getElementById("result-cost-bags").textContent =
            costBags !== null
              ? `$${roundTo(costBags, 2).toLocaleString()}`
              : "–";
          document.getElementById("result-weight").textContent = weightText;

          resultsSection.classList.remove("hidden");
        } catch (err) {
          errorMsg.textContent = err.message;
          resultsSection.classList.add("hidden");
        }
      });

      document.getElementById("reset-btn").addEventListener("click", () => {
        areasContainer.innerHTML = "";
        areaIndex = 0;
        createAreaRow();
        syncShapeInputs();
        syncUnitLabels();
        document.getElementById("depth-input").value = 3;
        document.getElementById("depth-unit").value =
          currentUnits === "imperial" ? "in" : "cm";
        document.getElementById("mulch-density").value = "";
        document.getElementById("bag-size").value = 2;
        document.getElementById("bag-size-unit").value = "ft3";
        document.getElementById("price-per-yard").value = "";
        document.getElementById("price-per-bag").value = "";
        clearErrors();
        resultsSection.classList.add("hidden");
      });

      // Initialize labels for first load
      syncShapeInputs();
      syncUnitLabels();

      // Mobile menu script (simple)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", () => {
          const menu = document.getElementById("mobile-menu");
          const expanded = menu.classList.contains("hidden");
          menu.classList.toggle("hidden");
          document
            .getElementById("mobile-menu-toggle")
            .setAttribute("aria-expanded", expanded ? "true" : "false");
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
