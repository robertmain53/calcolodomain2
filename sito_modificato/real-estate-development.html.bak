    <script>
      function parseNumber(id) {
        const v = parseFloat(
          document.getElementById(id).value.replace(/,/g, "")
        );
        return isNaN(v) ? 0 : v;
      }

      function formatCurrency(value, currencySymbol) {
        if (!isFinite(value)) return "–";
        const abs = Math.abs(value);
        const decimals = abs >= 1000000 ? 0 : 0;
        return (
          currencySymbol +
          value.toLocaleString(undefined, { maximumFractionDigits: decimals })
        );
      }

      function formatPercent(value) {
        if (!isFinite(value)) return "–";
        return value.toFixed(1) + "%";
      }

      function formatSmallPercent(value) {
        if (!isFinite(value)) return "–";
        return value.toFixed(2) + "%";
      }

      function updateProjectTypeUI() {
        const type = document.getElementById("project-type").value;
        const saleWrapper = document.getElementById("for-sale-price-wrapper");
        const rentWrapper = document.getElementById("for-rent-rent-wrapper");
        const capWrapper = document.getElementById("for-rent-caprate-wrapper");

        if (type === "for-sale") {
          saleWrapper.classList.remove("hidden");
          rentWrapper.classList.add("hidden");
          capWrapper.classList.add("hidden");
        } else {
          saleWrapper.classList.add("hidden");
          rentWrapper.classList.remove("hidden");
          capWrapper.classList.remove("hidden");
        }
      }

      function calculateFeasibility() {
        const currencySymbol = document.getElementById("currency").value || "$";
        [
          "currency-symbol-1",
          "currency-symbol-2",
          "currency-symbol-3",
          "currency-symbol-4",
        ].forEach((id) => {
          const el = document.getElementById(id);
          if (el) el.textContent = currencySymbol;
        });

        const type = document.getElementById("project-type").value;
        const area = parseNumber("sellable-area");

        let totalRevenue = 0;
        let revPerSf = 0;

        if (type === "for-sale") {
          const pricePerSf = parseNumber("price-per-sf");
          totalRevenue = area * pricePerSf;
          revPerSf = pricePerSf;
        } else {
          const rentPerSfYear = parseNumber("rent-per-sf-year");
          const capRate = parseNumber("exit-cap-rate") / 100;
          const grossRent = rentPerSfYear * area;
          const vacancyRate = 0.05;
          const expenseRatio = 0.35;
          const noi = grossRent * (1 - vacancyRate) * (1 - expenseRatio);
          totalRevenue = capRate > 0 ? noi / capRate : 0;
          revPerSf = area > 0 ? totalRevenue / area : 0;
        }

        const land = parseNumber("land-cost");
        const hardCostPerSf = parseNumber("hard-cost-per-sf");
        const hardCost = area * hardCostPerSf;
        const softPct = parseNumber("soft-cost-pct") / 100;
        const softCost = (land + hardCost) * softPct;
        const contPct = parseNumber("contingency-pct") / 100;
        const contingency = (hardCost + softCost) * contPct;

        const preInterestCost = land + hardCost + softCost + contingency;

        const ltc = parseNumber("ltc-pct") / 100;
        const loanAmount = preInterestCost * ltc;
        const interestRate = parseNumber("interest-rate") / 100;
        const durationMonths = parseNumber("project-duration");
        const avgDrawFactor = 0.5;
        const interestCost =
          loanAmount * interestRate * (durationMonths / 12) * avgDrawFactor;

        const totalCost = preInterestCost + interestCost;
        const profit = totalRevenue - totalCost;

        const profitOnCost = totalCost > 0 ? (profit / totalCost) * 100 : NaN;
        const profitOnValue =
          totalRevenue > 0 ? (profit / totalRevenue) * 100 : NaN;

        const equityRequired = totalCost - loanAmount;
        const equityMultiple =
          equityRequired > 0 ? (equityRequired + profit) / equityRequired : NaN;

        const absorptionMonths = parseNumber("absorption-months");
        const totalMonths = durationMonths + absorptionMonths;
        const years = totalMonths / 12;
        let equityIRR = NaN;
        if (equityRequired > 0 && years > 0 && equityRequired + profit > 0) {
          equityIRR =
            Math.pow((equityRequired + profit) / equityRequired, 1 / years) - 1;
        }

        const costPerSf = area > 0 ? totalCost / area : 0;
        const profitPerSf = area > 0 ? profit / area : 0;

        document.getElementById("total-revenue").textContent = formatCurrency(
          totalRevenue,
          currencySymbol
        );
        document.getElementById("total-cost").textContent = formatCurrency(
          totalCost,
          currencySymbol
        );
        document.getElementById("profit-absolute").textContent = formatCurrency(
          profit,
          currencySymbol
        );
        document.getElementById("profit-on-cost").textContent = isFinite(
          profitOnCost
        )
          ? formatPercent(profitOnCost)
          : "–";
        document.getElementById("profit-on-value").textContent = isFinite(
          profitOnValue
        )
          ? formatPercent(profitOnValue)
          : "–";

        document.getElementById("loan-amount").textContent = formatCurrency(
          loanAmount,
          currencySymbol
        );
        document.getElementById("interest-cost").textContent = formatCurrency(
          interestCost,
          currencySymbol
        );
        document.getElementById("equity-required").textContent = formatCurrency(
          equityRequired,
          currencySymbol
        );
        document.getElementById("equity-multiple").textContent = isFinite(
          equityMultiple
        )
          ? equityMultiple.toFixed(2) + "×"
          : "–";
        document.getElementById("equity-irr").textContent = isFinite(equityIRR)
          ? formatSmallPercent(equityIRR * 100)
          : "–";

        document.getElementById("rev-per-sf").textContent = isFinite(revPerSf)
          ? formatCurrency(revPerSf, currencySymbol) + "/sf"
          : "–";
        document.getElementById("cost-per-sf").textContent = isFinite(costPerSf)
          ? formatCurrency(costPerSf, currencySymbol) + "/sf"
          : "–";
        document.getElementById("profit-per-sf").textContent = isFinite(
          profitPerSf
        )
          ? formatCurrency(profitPerSf, currencySymbol) + "/sf"
          : "–";

        const flag = document.getElementById("feasibility-flag");
        flag.classList.remove(
          "bg-green-100",
          "text-green-800",
          "border-green-300",
          "bg-yellow-100",
          "text-yellow-800",
          "border-yellow-300",
          "bg-red-100",
          "text-red-800",
          "border-red-300",
          "bg-gray-100",
          "text-gray-700",
          "border-gray-300"
        );

        if (!isFinite(profitOnCost) || !isFinite(equityIRR)) {
          flag.textContent = "Awaiting inputs";
          flag.classList.add(
            "bg-gray-100",
            "text-gray-700",
            "border",
            "border-gray-300"
          );
          return;
        }

        if (profit <= 0 || profitOnCost < 10 || equityIRR < 0.1) {
          flag.textContent = "Not attractive (on these assumptions)";
          flag.classList.add(
            "bg-red-100",
            "text-red-800",
            "border",
            "border-red-300"
          );
        } else if (profitOnCost >= 20 && equityIRR >= 0.15) {
          flag.textContent = "Feasible (on paper)";
          flag.classList.add(
            "bg-green-100",
            "text-green-800",
            "border",
            "border-green-300"
          );
        } else {
          flag.textContent = "Tight, proceed with caution";
          flag.classList.add(
            "bg-yellow-100",
            "text-yellow-800",
            "border",
            "border-yellow-300"
          );
        }
      }

      function resetForm() {
        document.getElementById("project-type").value = "for-sale";
        document.getElementById("currency").value = "$";
        document.getElementById("sellable-area").value = 50000;
        document.getElementById("price-per-sf").value = 450;
        document.getElementById("rent-per-sf-year").value = 30;
        document.getElementById("exit-cap-rate").value = 5;
        document.getElementById("land-cost").value = 5000000;
        document.getElementById("hard-cost-per-sf").value = 250;
        document.getElementById("soft-cost-pct").value = 20;
        document.getElementById("contingency-pct").value = 10;
        document.getElementById("ltc-pct").value = 65;
        document.getElementById("interest-rate").value = 7;
        document.getElementById("project-duration").value = 30;
        document.getElementById("absorption-months").value = 12;
        updateProjectTypeUI();
        calculateFeasibility();
      }

      document.addEventListener("DOMContentLoaded", function () {
        updateProjectTypeUI();
        calculateFeasibility();

        document
          .getElementById("project-type")
          .addEventListener("change", function () {
            updateProjectTypeUI();
            calculateFeasibility();
          });

        document
          .getElementById("calculate-btn")
          .addEventListener("click", function (e) {
            e.preventDefault();
            calculateFeasibility();
          });

        document
          .getElementById("reset-btn")
          .addEventListener("click", function (e) {
            e.preventDefault();
            resetForm();
          });

        [
          "currency",
          "sellable-area",
          "price-per-sf",
          "rent-per-sf-year",
          "exit-cap-rate",
          "land-cost",
          "hard-cost-per-sf",
          "soft-cost-pct",
          "contingency-pct",
          "ltc-pct",
          "interest-rate",
          "project-duration",
          "absorption-months",
        ].forEach((id) => {
          const el = document.getElementById(id);
          if (el) {
            el.addEventListener("change", calculateFeasibility);
            el.addEventListener("blur", calculateFeasibility);
          }
        });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
