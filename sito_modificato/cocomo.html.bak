    <script>
      // COCOMO calculator logic
      (function () {
        const modeSelect = document.getElementById("projectMode");
        const modelLevelSelect = document.getElementById("modelLevel");
        const modeDescription = document.getElementById("modeDescription");
        const sizeInput = document.getElementById("sizeKLOC");
        const salaryInput = document.getElementById("avgSalary");
        const phaseProfileSelect = document.getElementById("phaseProfile");

        const cdIds = [
          "cdReliability",
          "cdComplexity",
          "cdTeamExp",
          "cdTools",
          "cdSchedule",
          "cdPlatform",
        ];

        const eafDisplay = document.getElementById("eafDisplay");
        const eafValueSpan = document.getElementById("eafValue");

        const calculateBtn = document.getElementById("calculateBtn");
        const resetBtn = document.getElementById("resetBtn");

        const resultsContainer = document.getElementById("resultsContainer");
        const phaseCard = document.getElementById("phaseBreakdownCard");

        const effortPMSpan = document.getElementById("effortPM");
        const devTimeSpan = document.getElementById("devTime");
        const teamSizeSpan = document.getElementById("teamSize");
        const productivitySpan = document.getElementById("productivity");
        const totalCostSpan = document.getElementById("totalCost");

        const phasePlanSpan = document.getElementById("phasePlan");
        const phaseDesignSpan = document.getElementById("phaseDesign");
        const phaseCodeSpan = document.getElementById("phaseCode");
        const phaseTestSpan = document.getElementById("phaseTest");

        const warningText = document.getElementById("warningText");

        const modeInfo = {
          organic: {
            desc: "Organic: small team, familiar domain, flexible requirements.",
            a: 2.4,
            b: 1.05,
            c: 2.5,
            d: 0.38,
          },
          semi: {
            desc: "Semi-detached: medium projects, mixed experience or some novelty.",
            a: 3.0,
            b: 1.12,
            c: 2.5,
            d: 0.35,
          },
          embedded: {
            desc: "Embedded: large, complex systems with tight hardware or regulatory constraints.",
            a: 3.6,
            b: 1.2,
            c: 2.5,
            d: 0.32,
          },
        };

        const phaseProfiles = {
          default: { plan: 0.1, design: 0.25, code: 0.4, test: 0.25 },
          frontend: { plan: 0.12, design: 0.33, code: 0.3, test: 0.25 },
          backend: { plan: 0.08, design: 0.2, code: 0.5, test: 0.22 },
        };

        function updateModeDescription() {
          const mode = modeSelect.value;
          modeDescription.textContent = modeInfo[mode].desc;
        }

        function computeEAF() {
          let eaf = 1;
          cdIds.forEach((id) => {
            const el = document.getElementById(id);
            if (el) {
              const v = parseFloat(el.value);
              if (!isNaN(v) && v > 0) {
                eaf *= v;
              }
            }
          });
          return eaf;
        }

        function formatNumber(num, decimals = 2) {
          if (!isFinite(num)) return "–";
          return Number(num.toFixed(decimals)).toLocaleString();
        }

        function calculate() {
          const mode = modeSelect.value;
          const modelLevel = modelLevelSelect.value;

          let kloc = parseFloat(sizeInput.value);
          if (isNaN(kloc) || kloc <= 0) {
            alert("Please enter a valid positive project size in KLOC.");
            return;
          }

          const { a, b, c, d } = modeInfo[mode];

          // Basic effort
          let effort = a * Math.pow(kloc, b);

          // EAF for intermediate/detailed
          let eaf = 1;
          if (modelLevel !== "basic") {
            eaf = computeEAF();
            effort *= eaf;
            eafDisplay.classList.remove("hidden");
            eafValueSpan.textContent = eaf.toFixed(2);
          } else {
            eafDisplay.classList.add("hidden");
          }

          // Development time
          const devTime = c * Math.pow(effort, d);
          const teamSize = effort / devTime;
          const productivity = (kloc * 1000) / effort;

          // Cost
          const salary = parseFloat(salaryInput.value);
          let totalCost = null;
          if (!isNaN(salary) && salary > 0) {
            totalCost = effort * salary;
          }

          // Phase breakdown for detailed
          if (modelLevel === "detailed") {
            const profile =
              phaseProfiles[phaseProfileSelect.value] || phaseProfiles.default;
            phasePlanSpan.textContent = formatNumber(effort * profile.plan, 2);
            phaseDesignSpan.textContent = formatNumber(
              effort * profile.design,
              2
            );
            phaseCodeSpan.textContent = formatNumber(effort * profile.code, 2);
            phaseTestSpan.textContent = formatNumber(effort * profile.test, 2);
            phaseCard.classList.remove("hidden");
          } else {
            phaseCard.classList.add("hidden");
          }

          // Populate results
          effortPMSpan.textContent = formatNumber(effort, 2);
          devTimeSpan.textContent = formatNumber(devTime, 2);
          teamSizeSpan.textContent = formatNumber(teamSize, 2);
          productivitySpan.textContent =
            formatNumber(productivity, 0) + " LOC/PM";
          totalCostSpan.textContent =
            totalCost !== null ? "$" + formatNumber(totalCost, 0) : "–";

          resultsContainer.classList.remove("hidden");

          // Simple sanity warning
          let warnings = [];
          if (kloc < 2) {
            warnings.push(
              "COCOMO is less reliable for very small projects (< 2 KLOC)."
            );
          }
          if (kloc > 1000) {
            warnings.push(
              "For extremely large systems, calibrate COCOMO with your own historical data."
            );
          }
          if (eaf > 1.6 || eaf < 0.6) {
            warnings.push(
              "Your Effort Adjustment Factor (EAF) is extreme; review the cost driver ratings."
            );
          }

          if (warnings.length) {
            warningText.textContent = warnings.join(" ");
            warningText.classList.remove("hidden");
          } else {
            warningText.classList.add("hidden");
          }
        }

        function resetForm() {
          modeSelect.value = "organic";
          modelLevelSelect.value = "basic";
          sizeInput.value = 50;
          salaryInput.value = 10000;
          phaseProfileSelect.value = "default";

          cdIds.forEach((id) => {
            const el = document.getElementById(id);
            if (el) {
              // reset to Nominal where applicable
              if (
                id === "cdReliability" ||
                id === "cdComplexity" ||
                id === "cdTeamExp" ||
                id === "cdTools" ||
                id === "cdSchedule" ||
                id === "cdPlatform"
              ) {
                for (let i = 0; i < el.options.length; i++) {
                  if (parseFloat(el.options[i].value) === 1.0) {
                    el.selectedIndex = i;
                    break;
                  }
                }
              }
            }
          });

          resultsContainer.classList.add("hidden");
          phaseCard.classList.add("hidden");
          eafDisplay.classList.add("hidden");
          warningText.classList.add("hidden");
          updateModeDescription();
        }

        modeSelect.addEventListener("change", updateModeDescription);
        calculateBtn.addEventListener("click", calculate);
        resetBtn.addEventListener("click", resetForm);

        // Initialize
        updateModeDescription();
      })();
    </script>

