    <script>
      // Utility helpers
      function toNumber(id) {
        const v = document.getElementById(id).value.trim();
        if (v === "") return null;
        const n = Number(v);
        return isFinite(n) ? n : null;
      }
      function degToRad(d) {
        return (d * Math.PI) / 180;
      }
      function radToDeg(r) {
        return (r * 180) / Math.PI;
      }

      function setText(id, value, unit) {
        const el = document.getElementById(id);
        if (!el) return;
        if (value == null || !isFinite(value)) {
          el.textContent = "–";
          return;
        }
        const rounded = Math.round(value * 10000) / 10000;
        el.textContent = unit ? rounded + " " + unit : String(rounded);
      }

      function solveTriangle() {
        const unitSel = document.getElementById("unit-select").value;
        const lenUnit = unitSel === "none" ? "" : unitSel;

        const aIn = toNumber("side-a");
        const bIn = toNumber("side-b");
        const cIn = toNumber("side-c");

        const AdegIn = toNumber("angle-A");
        const BdegIn = toNumber("angle-B");
        const CdegIn = toNumber("angle-C");

        const haIn = toNumber("height-ha");
        const hbIn = toNumber("height-hb");
        const hcIn = toNumber("height-hc");

        const areaIn = toNumber("area-input");
        const perIn = toNumber("perimeter-input");

        const status = document.getElementById("status-message");
        status.textContent = "";

        let a = aIn,
          b = bIn,
          c = cIn;
        let A = AdegIn != null ? degToRad(AdegIn) : null;
        let B = BdegIn != null ? degToRad(BdegIn) : null;
        let C = CdegIn != null ? degToRad(CdegIn) : null;
        let K = areaIn;
        let ha = haIn,
          hb = hbIn,
          hc = hcIn;

        // Basic sanity checks
        const anyInput = [
          aIn,
          bIn,
          cIn,
          AdegIn,
          BdegIn,
          CdegIn,
          haIn,
          hbIn,
          hcIn,
          areaIn,
          perIn,
        ].some((v) => v != null);
        if (!anyInput) {
          status.textContent =
            "Enter at least one side, angle, height, or area.";
          return;
        }

        // 1) If three sides given: SSS
        if (a != null && b != null && c != null) {
          if (a <= 0 || b <= 0 || c <= 0) {
            status.textContent = "Sides must be positive.";
            return;
          }
          if (!(a + b > c && a + c > b && b + c > a)) {
            status.textContent =
              "Triangle inequality violated: these sides cannot form a triangle.";
            return;
          }
          const s = (a + b + c) / 2;
          K = Math.sqrt(Math.max(0, s * (s - a) * (s - b) * (s - c)));
          if (K === 0) {
            status.textContent = "Degenerate triangle (area = 0).";
            return;
          }
          A = Math.acos((b * b + c * c - a * a) / (2 * b * c));
          B = Math.acos((a * a + c * c - b * b) / (2 * a * c));
          C = Math.PI - A - B;
        } else {
          // 2) Try SAS: two sides and included angle
          // Cases: b,c,A or a,c,B or a,b,C
          let solved = false;

          if (!solved && b != null && c != null && A != null) {
            if (b <= 0 || c <= 0) {
              status.textContent = "Sides must be positive.";
              return;
            }
            a = Math.sqrt(b * b + c * c - 2 * b * c * Math.cos(A));
            const s = (a + b + c) / 2;
            K = Math.sqrt(Math.max(0, s * (s - a) * (s - b) * (s - c)));
            B = Math.asin(Math.max(-1, Math.min(1, (b * Math.sin(A)) / a)));
            C = Math.PI - A - B;
            solved = true;
          }

          if (!solved && a != null && c != null && B != null) {
            if (a <= 0 || c <= 0) {
              status.textContent = "Sides must be positive.";
              return;
            }
            b = Math.sqrt(a * a + c * c - 2 * a * c * Math.cos(B));
            const s = (a + b + c) / 2;
            K = Math.sqrt(Math.max(0, s * (s - a) * (s - b) * (s - c)));
            A = Math.asin(Math.max(-1, Math.min(1, (a * Math.sin(B)) / b)));
            C = Math.PI - A - B;
            solved = true;
          }

          if (!solved && a != null && b != null && C != null) {
            if (a <= 0 || b <= 0) {
              status.textContent = "Sides must be positive.";
              return;
            }
            c = Math.sqrt(a * a + b * b - 2 * a * b * Math.cos(C));
            const s = (a + b + c) / 2;
            K = Math.sqrt(Math.max(0, s * (s - a) * (s - b) * (s - c)));
            A = Math.asin(Math.max(-1, Math.min(1, (a * Math.sin(C)) / c)));
            B = Math.PI - A - C;
            solved = true;
          }

          // 3) AAS / ASA: two angles + one side
          if (!solved) {
            const angles = [A, B, C].filter((x) => x != null);
            if (angles.length >= 2) {
              if (A == null && B != null && C != null) A = Math.PI - B - C;
              if (B == null && A != null && C != null) B = Math.PI - A - C;
              if (C == null && A != null && B != null) C = Math.PI - A - B;
              if (
                A <= 0 ||
                B <= 0 ||
                C <= 0 ||
                Math.abs(A + B + C - Math.PI) > 1e-6
              ) {
                status.textContent = "Angles must be positive and sum to 180°.";
                return;
              }
              if (a != null && a > 0) {
                b = (a * Math.sin(B)) / Math.sin(A);
                c = (a * Math.sin(C)) / Math.sin(A);
              } else if (b != null && b > 0) {
                a = (b * Math.sin(A)) / Math.sin(B);
                c = (b * Math.sin(C)) / Math.sin(B);
              } else if (c != null && c > 0) {
                a = (c * Math.sin(A)) / Math.sin(C);
                b = (c * Math.sin(B)) / Math.sin(C);
              } else {
                // No side known yet; try base + height or area
              }
              if (a != null && b != null && c != null) {
                const s = (a + b + c) / 2;
                K = Math.sqrt(Math.max(0, s * (s - a) * (s - b) * (s - c)));
                solved = true;
              }
            }
          }

          // 4) Base + height or area + side
          if (!solved) {
            if (a != null && ha != null && a > 0 && ha > 0) {
              K = 0.5 * a * ha;
              // Need more info to get b,c; if perimeter given, approximate isosceles around base
            } else if (b != null && hb != null && b > 0 && hb > 0) {
              K = 0.5 * b * hb;
            } else if (c != null && hc != null && c > 0 && hc > 0) {
              K = 0.5 * c * hc;
            } else if (K != null && a != null && a > 0) {
              ha = (2 * K) / a;
            } else if (K != null && b != null && b > 0) {
              hb = (2 * K) / b;
            } else if (K != null && c != null && c > 0) {
              hc = (2 * K) / c;
            }
          }

          // If still not fully solved, we require at least SSS or SAS or AAS/ASA
          if (
            a == null ||
            b == null ||
            c == null ||
            A == null ||
            B == null ||
            C == null ||
            K == null
          ) {
            status.textContent =
              "Not enough consistent information to determine a unique triangle. Try providing at least three independent values (e.g. 3 sides, or 2 sides + included angle, or 2 angles + 1 side).";
            return;
          }

          // Final triangle inequality check
          if (!(a + b > c && a + c > b && b + c > a)) {
            status.textContent =
              "Computed values do not satisfy the triangle inequality. Please check your inputs.";
            return;
          }
        }

        const s = (a + b + c) / 2;
        if (K == null || !isFinite(K)) {
          const tmp = Math.sqrt(Math.max(0, s * (s - a) * (s - b) * (s - c)));
          if (tmp === 0) {
            document.getElementById("status-message").textContent =
              "Degenerate triangle (area = 0).";
            return;
          }
          K = tmp;
        }

        // Heights
        ha = (2 * K) / a;
        hb = (2 * K) / b;
        hc = (2 * K) / c;

        // Radii
        const r = K / s;
        const R = (a * b * c) / (4 * K);

        // Output
        setText("res-a", a, lenUnit);
        setText("res-b", b, lenUnit);
        setText("res-c", c, lenUnit);

        setText("res-A", radToDeg(A), "°");
        setText("res-B", radToDeg(B), "°");
        setText("res-C", radToDeg(C), "°");

        setText("res-area", K, lenUnit ? lenUnit + "²" : "");
        setText("res-perimeter", a + b + c, lenUnit);
        setText("res-s", s, lenUnit);

        setText("res-ha", ha, lenUnit);
        setText("res-hb", hb, lenUnit);
        setText("res-hc", hc, lenUnit);

        setText("res-inradius", r, lenUnit);
        setText("res-circumradius", R, lenUnit);

        status.textContent = "";

        drawTriangle(a, b, c, A, B, C);
      }

      function clearAll() {
        const ids = [
          "side-a",
          "side-b",
          "side-c",
          "angle-A",
          "angle-B",
          "angle-C",
          "height-ha",
          "height-hb",
          "height-hc",
          "area-input",
          "perimeter-input",
        ];
        ids.forEach((id) => {
          document.getElementById(id).value = "";
        });
        const resIds = [
          "res-a",
          "res-b",
          "res-c",
          "res-A",
          "res-B",
          "res-C",
          "res-area",
          "res-perimeter",
          "res-s",
          "res-ha",
          "res-hb",
          "res-hc",
          "res-inradius",
          "res-circumradius",
        ];
        resIds.forEach((id) => {
          document.getElementById(id).textContent = "–";
        });
        document.getElementById("status-message").textContent = "";
        const canvas = document.getElementById("triangle-canvas");
        const ctx = canvas.getContext("2d");
        ctx.clearRect(0, 0, canvas.width, canvas.height);
      }

      function drawTriangle(a, b, c, A, B, C) {
        const canvas = document.getElementById("triangle-canvas");
        if (!canvas || !canvas.getContext) return;
        const ctx = canvas.getContext("2d");
        ctx.clearRect(0, 0, canvas.width, canvas.height);

        // Place A at (0,0), B at (c,0), C from sides b (AC) and a (BC)
        const Ax = 0,
          Ay = 0;
        const Bx = c,
          By = 0;
        const Cx = b * Math.cos(A);
        const Cy = b * Math.sin(A);

        // Compute bounding box
        const xs = [Ax, Bx, Cx];
        const ys = [Ay, By, Cy];
        const minX = Math.min(...xs),
          maxX = Math.max(...xs);
        const minY = Math.min(...ys),
          maxY = Math.max(...ys);

        const padding = 20;
        const width = canvas.width - 2 * padding;
        const height = canvas.height - 2 * padding;

        const scaleX = width / (maxX - minX || 1);
        const scaleY = height / (maxY - minY || 1);
        const scale = Math.min(scaleX, scaleY);

        function tx(x) {
          return padding + (x - minX) * scale;
        }
        function ty(y) {
          return canvas.height - (padding + (y - minY) * scale);
        }

        const Apx = tx(Ax),
          Apy = ty(Ay);
        const Bpx = tx(Bx),
          Bpy = ty(By);
        const Cpx = tx(Cx),
          Cpy = ty(Cy);

        ctx.lineWidth = 2;
        ctx.strokeStyle = "#2563eb";
        ctx.beginPath();
        ctx.moveTo(Apx, Apy);
        ctx.lineTo(Bpx, Bpy);
        ctx.lineTo(Cpx, Cpy);
        ctx.closePath();
        ctx.stroke();

        // Vertices
        ctx.fillStyle = "#1f2937";
        ctx.beginPath();
        ctx.arc(Apx, Apy, 3, 0, 2 * Math.PI);
        ctx.fill();
        ctx.beginPath();
        ctx.arc(Bpx, Bpy, 3, 0, 2 * Math.PI);
        ctx.fill();
        ctx.beginPath();
        ctx.arc(Cpx, Cpy, 3, 0, 2 * Math.PI);
        ctx.fill();

        ctx.font = "11px Inter, sans-serif";
        ctx.fillStyle = "#111827";
        ctx.fillText("A", Apx + 4, Apy - 4);
        ctx.fillText("B", Bpx + 4, Bpy - 4);
        ctx.fillText("C", Cpx + 4, Cpy - 4);

        // Side labels (approx midpoints)
        ctx.fillStyle = "#4b5563";
        ctx.fillText("c", (Apx + Bpx) / 2, (Apy + Bpy) / 2 - 4);
        ctx.fillText("a", (Bpx + Cpx) / 2, (Bpy + Cpy) / 2 - 4);
        ctx.fillText("b", (Apx + Cpx) / 2, (Apy + Cpy) / 2 - 4);
      }

      document
        .getElementById("solve-btn")
        .addEventListener("click", solveTriangle);
      document.getElementById("clear-btn").addEventListener("click", clearAll);
    </script>

