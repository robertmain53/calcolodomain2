    <script>
      // Utility formatting
      function formatCurrency(value, symbol) {
        if (!isFinite(value)) return "–";
        const abs = Math.abs(value);
        let formatted;
        if (abs >= 1e9) {
          formatted = (value / 1e9).toFixed(2) + "B";
        } else if (abs >= 1e6) {
          formatted = (value / 1e6).toFixed(2) + "M";
        } else if (abs >= 1e3) {
          formatted = (value / 1e3).toFixed(2) + "K";
        } else {
          formatted = value.toFixed(2);
        }
        return symbol + formatted;
      }

      function formatPercent(value) {
        if (!isFinite(value)) return "–";
        return value.toFixed(1) + "%";
      }

      function buildFcfeInputs() {
        const years = parseInt(document.getElementById("years").value, 10) || 1;
        const container = document.getElementById("fcfeYearInputs");
        container.innerHTML = "";
        for (let t = 1; t <= years; t++) {
          const div = document.createElement("div");
          div.innerHTML = `
        <label class="block text-xs font-medium text-gray-700 mb-1">FCFE Year ${t}</label>
        <input type="number" step="0.01" value="${
          500000 * Math.pow(1.08, t - 1).toFixed
            ? (500000 * Math.pow(1.08, t - 1)).toFixed(2)
            : 500000
        }" 
               class="w-full border border-gray-300 rounded-md px-2 py-1.5 fcfe-year-input" data-year="${t}">
      `;
          container.appendChild(div);
        }
      }

      document
        .getElementById("fcfeMode")
        .addEventListener("change", function () {
          const mode = this.value;
          const startBlock = document.getElementById("fcfeStartBlock");
          const explicitBlock = document.getElementById("fcfeExplicitBlock");
          if (mode === "start") {
            startBlock.classList.remove("hidden");
            explicitBlock.classList.add("hidden");
          } else {
            startBlock.classList.add("hidden");
            explicitBlock.classList.remove("hidden");
            buildFcfeInputs();
          }
        });

      document.getElementById("years").addEventListener("change", function () {
        const mode = document.getElementById("fcfeMode").value;
        if (mode === "explicit") {
          buildFcfeInputs();
        }
      });

      document
        .getElementById("resetBtn")
        .addEventListener("click", function () {
          document.getElementById("currency").value = "$";
          document.getElementById("shares").value = 1000000;
          document.getElementById("fcfeMode").value = "start";
          document.getElementById("years").value = 5;
          document.getElementById("fcfe0").value = 500000;
          document.getElementById("growth").value = 8;
          document.getElementById("terminalGrowth").value = 2;
          document.getElementById("discountRate").value = 10;
          document.getElementById("netDebt").value = 2000000;
          document.getElementById("currentPrice").value = 20;
          document.getElementById("fcfeStartBlock").classList.remove("hidden");
          document.getElementById("fcfeExplicitBlock").classList.add("hidden");
          document.getElementById("errorMsg").textContent = "";
          document.getElementById("warningMsg").classList.add("hidden");
          document.getElementById("cashflowTableBody").innerHTML =
            '<tr><td class="px-3 py-2 text-center text-gray-500" colspan="4">Run the calculation to see the cash flow schedule.</td></tr>';
          document.getElementById("pvFcfeSpan").textContent = "–";
          document.getElementById("pvTvSpan").textContent = "–";
          document.getElementById("equityValueSpan").textContent = "–";
          document.getElementById("valuePerShareSpan").textContent = "–";
          document.getElementById("currentPriceSpan").textContent = "–";
          document.getElementById("upsideSpan").textContent = "–";
          document.getElementById("tvRawCell").textContent = "–";
          document.getElementById("tvPvCell").textContent = "–";
        });

      document
        .getElementById("calculateBtn")
        .addEventListener("click", function () {
          const currency = document.getElementById("currency").value || "$";
          const years = Math.max(
            1,
            parseInt(document.getElementById("years").value, 10) || 1
          );
          const discountRate =
            parseFloat(document.getElementById("discountRate").value) / 100;
          const terminalGrowth =
            parseFloat(document.getElementById("terminalGrowth").value) / 100;
          const shares = parseFloat(document.getElementById("shares").value);
          const currentPrice = parseFloat(
            document.getElementById("currentPrice").value
          );
          const mode = document.getElementById("fcfeMode").value;

          const errorEl = document.getElementById("errorMsg");
          const warningEl = document.getElementById("warningMsg");
          errorEl.textContent = "";
          warningEl.classList.add("hidden");

          if (!isFinite(discountRate) || discountRate <= 0) {
            errorEl.textContent = "Please enter a positive cost of equity.";
            return;
          }
          if (!isFinite(terminalGrowth) || terminalGrowth < -0.5) {
            errorEl.textContent =
              "Please enter a reasonable terminal growth rate.";
            return;
          }
          if (terminalGrowth >= discountRate) {
            errorEl.textContent =
              "Terminal growth rate must be lower than the cost of equity (r > g).";
            return;
          }

          let fcfeArray = [];
          if (mode === "start") {
            const fcfe0 = parseFloat(document.getElementById("fcfe0").value);
            const growth =
              parseFloat(document.getElementById("growth").value) / 100;
            if (!isFinite(fcfe0)) {
              errorEl.textContent = "Please enter a valid current FCFE.";
              return;
            }
            if (!isFinite(growth) || growth < -0.9) {
              errorEl.textContent =
                "Please enter a reasonable FCFE growth rate.";
              return;
            }
            let fcfe = fcfe0;
            for (let t = 1; t <= years; t++) {
              fcfe = fcfe * (1 + growth);
              fcfeArray.push(fcfe);
            }
          } else {
            const inputs = document.querySelectorAll(".fcfe-year-input");
            if (!inputs.length) buildFcfeInputs();
            fcfeArray = [];
            for (let i = 0; i < years; i++) {
              const input = document.querySelector(
                '.fcfe-year-input[data-year="' + (i + 1) + '"]'
              );
              const val = input ? parseFloat(input.value) : NaN;
              if (!isFinite(val)) {
                errorEl.textContent =
                  "Please enter FCFE for all forecast years.";
                return;
              }
              fcfeArray.push(val);
            }
          }

          // DCF calculations
          let pvFcfe = 0;
          const rows = [];
          for (let t = 1; t <= years; t++) {
            const fcfe = fcfeArray[t - 1];
            const df = 1 / Math.pow(1 + discountRate, t);
            const pv = fcfe * df;
            pvFcfe += pv;
            rows.push({ year: t, fcfe, df, pv });
          }

          const lastFcfe = fcfeArray[fcfeArray.length - 1];
          const fcfeNext = lastFcfe * (1 + terminalGrowth);
          const tvRaw = fcfeNext / (discountRate - terminalGrowth);
          const dfTv = 1 / Math.pow(1 + discountRate, years);
          const tvPv = tvRaw * dfTv;

          const equityValue = pvFcfe + tvPv;
          let valuePerShare = NaN;
          if (isFinite(shares) && shares > 0) {
            valuePerShare = equityValue / shares;
          }

          // Populate table
          const tbody = document.getElementById("cashflowTableBody");
          tbody.innerHTML = "";
          rows.forEach((r) => {
            const tr = document.createElement("tr");
            tr.innerHTML = `
        <td class="px-3 py-2 border-b text-right">${r.year}</td>
        <td class="px-3 py-2 border-b text-right">${formatCurrency(
          r.fcfe,
          currency
        )}</td>
        <td class="px-3 py-2 border-b text-right">${r.df.toFixed(4)}</td>
        <td class="px-3 py-2 border-b text-right">${formatCurrency(
          r.pv,
          currency
        )}</td>
      `;
            tbody.appendChild(tr);
          });

          document.getElementById("tvRawCell").textContent = formatCurrency(
            tvRaw,
            currency
          );
          document.getElementById("tvPvCell").textContent = formatCurrency(
            tvPv,
            currency
          );

          document.getElementById("pvFcfeSpan").textContent = formatCurrency(
            pvFcfe,
            currency
          );
          document.getElementById("pvTvSpan").textContent = formatCurrency(
            tvPv,
            currency
          );
          document.getElementById("equityValueSpan").textContent =
            formatCurrency(equityValue, currency);
          document.getElementById("valuePerShareSpan").textContent = isFinite(
            valuePerShare
          )
            ? formatCurrency(valuePerShare, currency)
            : "–";

          if (
            isFinite(currentPrice) &&
            currentPrice > 0 &&
            isFinite(valuePerShare)
          ) {
            const upside = (valuePerShare / currentPrice - 1) * 100;
            document.getElementById("currentPriceSpan").textContent =
              formatCurrency(currentPrice, currency);
            document.getElementById("upsideSpan").textContent =
              formatPercent(upside);
          } else {
            document.getElementById("currentPriceSpan").textContent = "–";
            document.getElementById("upsideSpan").textContent = "–";
          }

          // Warnings
          const tvShare = tvPv / equityValue;
          if (tvShare > 0.7) {
            warningEl.textContent =
              "Warning: more than " +
              Math.round(tvShare * 100) +
              "% of the equity value comes from the terminal value. The valuation is highly sensitive to the terminal growth and discount rate assumptions.";
            warningEl.classList.remove("hidden");
          }
        });

      // Initialize explicit FCFE inputs once in case user switches mode
      buildFcfeInputs();
    </script>

