    <script>
      // ---------- Utility helpers ----------
      function skFormatNumber(value, decimals) {
        if (value === null || value === undefined || !isFinite(value))
          return "–";
        var d = Math.max(0, Math.min(10, decimals || 3));
        var absVal = Math.abs(value);
        if (absVal !== 0 && (absVal < 1e-4 || absVal > 1e7)) {
          return value.toExponential(6);
        }
        var s = value.toFixed(d);
        // Strip trailing zeros and possible trailing decimal point
        s = s.replace(/0+$/, "").replace(/\.$/, "");
        if (s === "-0") s = "0";
        return s;
      }

      function skParseData(raw) {
        if (!raw || !raw.trim()) {
          throw new Error(
            "Please paste at least 3 numeric values to compute skewness and kurtosis."
          );
        }
        var tokens = raw.split(/[\s,;]+/);
        var data = [];
        for (var i = 0; i < tokens.length; i++) {
          var t = tokens[i].trim();
          if (!t) continue;
          var v = parseFloat(t);
          if (!isFinite(v)) {
            throw new Error(
              'Non-numeric value encountered: "' +
                t +
                '". Please remove it or replace it with a number.'
            );
          }
          data.push(v);
        }
        if (data.length < 3) {
          throw new Error(
            "You need at least 3 numeric values to compute skewness (and 4 for kurtosis to be meaningful)."
          );
        }
        return data;
      }

      function skComputeMoments(data) {
        var n = data.length;
        var sum = 0;
        for (var i = 0; i < n; i++) sum += data[i];
        var mean = sum / n;

        var m2 = 0,
          m3 = 0,
          m4 = 0;
        for (var j = 0; j < n; j++) {
          var dx = data[j] - mean;
          var dx2 = dx * dx;
          m2 += dx2;
          m3 += dx2 * dx;
          m4 += dx2 * dx2;
        }
        m2 / n;
        m3 / n;
        m4 / n;

        var skew = null,
          kurt = null,
          excess = null;
        if (m2 > 0) {
          skew = m3 / Math.pow(m2, 1.5);
          kurt = m4 / (m2 * m2);
          excess = kurt - 3;
        } else {
          skew = 0;
          kurt = 0;
          excess = -3;
        }

        // population and sample variance / sd
        var popVar = m2;
        var popSd = Math.sqrt(popVar);
        var samVar = n > 1 ? (m2 * n) / (n - 1) : NaN;
        var samSd = n > 1 ? Math.sqrt(samVar) : NaN;

        return {
          n: n,
          mean: mean,
          m2: m2,
          m3: m3,
          m4: m4,
          skew: skew,
          kurt: kurt,
          excess: excess,
          popVar: popVar,
          popSd: popSd,
          samVar: samVar,
          samSd: samSd,
        };
      }

      function skInterpret(skew, kurt, excess) {
        if (
          skew === null ||
          kurt === null ||
          excess === null ||
          !isFinite(skew) ||
          !isFinite(kurt)
        ) {
          return "Unable to interpret results. Check that your data contain variation (not all values identical).";
        }

        var absSk = Math.abs(skew);
        var skewDesc;
        if (absSk < 0.5) {
          skewDesc = "approximately symmetric";
        } else if (absSk < 1.0) {
          skewDesc =
            "moderately " + (skew > 0 ? "right-skewed" : "left-skewed");
        } else {
          skewDesc = "strongly " + (skew > 0 ? "right-skewed" : "left-skewed");
        }

        var kurtDesc;
        if (!isFinite(excess)) {
          kurtDesc = "kurtosis could not be evaluated reliably";
        } else if (Math.abs(excess) < 0.5) {
          kurtDesc =
            "with tail behaviour similar to a normal distribution (mesokurtic)";
        } else if (excess > 0) {
          kurtDesc =
            "with heavier tails and more outliers than normal (leptokurtic)";
        } else {
          kurtDesc =
            "with lighter tails and fewer extreme values than normal (platykurtic)";
        }

        return (
          "Based on the estimates, the distribution appears " +
          skewDesc +
          ", " +
          kurtDesc +
          ". Use this as a descriptive guide and always confirm with plots and subject-matter context."
        );
      }

      function skUpdate() {
        var errorEl = document.getElementById("sk-error");
        var nEl = document.getElementById("sk-n");
        var meanEl = document.getElementById("sk-mean");
        var varEl = document.getElementById("sk-var");
        var sdEl = document.getElementById("sk-sd");
        var varNoteEl = document.getElementById("sk-var-note");
        var skewEl = document.getElementById("sk-skew");
        var kurtEl = document.getElementById("sk-kurt");
        var excessEl = document.getElementById("sk-excess");
        var interpEl = document.getElementById("sk-interpretation");

        errorEl.classList.add("hidden");
        errorEl.textContent = "";

        try {
          var raw = document.getElementById("sk-data").value;
          var data = skParseData(raw);
          var dec =
            parseInt(document.getElementById("sk-decimals").value, 10) || 3;
          var varMode =
            document.querySelector('input[name="sk-var-mode"]:checked')
              ?.value || "population";

          var res = skComputeMoments(data);

          nEl.textContent = res.n;
          meanEl.textContent = skFormatNumber(res.mean, dec);

          var varVal, sdVal, note;
          if (varMode === "sample" && res.n > 1) {
            varVal = res.samVar;
            sdVal = res.samSd;
            note =
              "Sample variance and standard deviation (denominator n − 1). Central moments for skewness and kurtosis still use denominator n.";
          } else {
            varVal = res.popVar;
            sdVal = res.popSd;
            note =
              "Population variance and standard deviation (denominator n). Skewness and kurtosis are computed from central moments m₂, m₃, and m₄ with denominator n.";
          }

          varEl.textContent = skFormatNumber(varVal, dec);
          sdEl.textContent = skFormatNumber(sdVal, dec);
          varNoteEl.textContent = note;

          skewEl.textContent = skFormatNumber(res.skew, dec);
          kurtEl.textContent = skFormatNumber(res.kurt, dec);
          excessEl.textContent = skFormatNumber(res.excess, dec);

          interpEl.textContent = skInterpret(res.skew, res.kurt, res.excess);
        } catch (err) {
          errorEl.textContent = err.message;
          errorEl.classList.remove("hidden");

          nEl.textContent = "–";
          meanEl.textContent = "–";
          varEl.textContent = "–";
          sdEl.textContent = "–";
          varNoteEl.textContent = "";
          skewEl.textContent = "–";
          kurtEl.textContent = "–";
          excessEl.textContent = "–";
          interpEl.textContent =
            "Results and a qualitative interpretation of skewness and kurtosis will appear here.";
        }
      }

      function skLoadExample() {
        var example = [12, 13, 14, 15, 16, 18, 19, 20, 25, 30, 32, 35, 40];
        document.getElementById("sk-data").value = example.join(", ");
        skUpdate();
      }

      function skClear() {
        document.getElementById("sk-data").value = "";
        document.getElementById("sk-error").classList.add("hidden");
        document.getElementById("sk-error").textContent = "";
        document.getElementById("sk-n").textContent = "–";
        document.getElementById("sk-mean").textContent = "–";
        document.getElementById("sk-var").textContent = "–";
        document.getElementById("sk-sd").textContent = "–";
        document.getElementById("sk-var-note").textContent = "";
        document.getElementById("sk-skew").textContent = "–";
        document.getElementById("sk-kurt").textContent = "–";
        document.getElementById("sk-excess").textContent = "–";
        document.getElementById("sk-interpretation").textContent =
          "Results and a qualitative interpretation of skewness and kurtosis will appear here.";
      }

      document.addEventListener("DOMContentLoaded", function () {
        document
          .getElementById("sk-calc-btn")
          ?.addEventListener("click", skUpdate);
        document
          .getElementById("sk-example-btn")
          ?.addEventListener("click", skLoadExample);
        document
          .getElementById("sk-clear-btn")
          ?.addEventListener("click", skClear);

        // Trigger calculation when Enter is pressed inside the textarea with Ctrl+Enter for power users
        document
          .getElementById("sk-data")
          ?.addEventListener("keydown", function (e) {
            if (e.key === "Enter" && (e.ctrlKey || e.metaKey)) {
              skUpdate();
            }
          });

        var faqButtons = document.querySelectorAll(
          "#sk-faq-container .sk-faq-question"
        );
        faqButtons.forEach(function (btn) {
          btn.addEventListener("click", function () {
            var answer = btn.parentElement.querySelector(".sk-faq-answer");
            var symbol = btn.querySelector("span:last-child");
            var isHidden = answer.classList.contains("hidden");

            document
              .querySelectorAll("#sk-faq-container .sk-faq-answer")
              .forEach(function (a) {
                a.classList.add("hidden");
              });
            document
              .querySelectorAll(
                "#sk-faq-container .sk-faq-question span:last-child"
              )
              .forEach(function (s) {
                s.textContent = "+";
              });

            if (isHidden) {
              answer.classList.remove("hidden");
              symbol.textContent = "−";
            }
          });
        });
      });
    </script>

