    <script>
      // Tab switching
      document.querySelectorAll(".tab-btn").forEach((btn) => {
        btn.addEventListener("click", () => {
          const target = btn.getAttribute("data-tab");
          document.querySelectorAll(".tab-btn").forEach((b) => {
            b.classList.remove(
              "border-blue-600",
              "text-blue-600",
              "bg-blue-50",
              "font-medium"
            );
            b.classList.add("border-gray-300", "text-gray-700");
          });
          btn.classList.add(
            "border-blue-600",
            "text-blue-600",
            "bg-blue-50",
            "font-medium"
          );
          btn.classList.remove("border-gray-300", "text-gray-700");

          document
            .querySelectorAll(".tab-panel")
            .forEach((p) => p.classList.add("hidden"));
          document.getElementById("tab-" + target).classList.remove("hidden");
        });
      });

      // Helper: parse positive number
      function parsePos(id) {
        const el = document.getElementById(id);
        const v = parseFloat(el.value);
        return isFinite(v) ? v : null;
      }

      // Ohm's Law
      document.getElementById("ohm-calc").addEventListener("click", () => {
        const V = parsePos("ohm-voltage");
        const I = parsePos("ohm-current");
        const R = parsePos("ohm-resistance");
        const P = parsePos("ohm-power");
        const vals = { V, I, R, P };
        const filled = Object.values(vals).filter((v) => v !== null).length;
        const errEl = document.getElementById("ohm-error");
        const resEl = document.getElementById("ohm-result");
        errEl.classList.add("hidden");
        resEl.textContent = "";

        if (filled < 2) {
          errEl.textContent = "Enter at least two values (V, I, R, or P).";
          errEl.classList.remove("hidden");
          return;
        }

        let v = V,
          i = I,
          r = R,
          p = P;

        try {
          if (v != null && i != null) {
            r = v / i;
            p = v * i;
          } else if (v != null && r != null) {
            i = v / r;
            p = v * i;
          } else if (v != null && p != null) {
            i = p / v;
            r = v / i;
          } else if (i != null && r != null) {
            v = i * r;
            p = v * i;
          } else if (i != null && p != null) {
            v = p / i;
            r = v / i;
          } else if (r != null && p != null) {
            i = Math.sqrt(p / r);
            v = i * r;
          } else {
            throw new Error("Inconsistent combination of inputs.");
          }

          if (!isFinite(v) || !isFinite(i) || !isFinite(r) || !isFinite(p)) {
            throw new Error("Calculation failed. Check your inputs.");
          }

          document.getElementById("ohm-voltage").value = v.toFixed(4);
          document.getElementById("ohm-current").value = i.toFixed(4);
          document.getElementById("ohm-resistance").value = r.toFixed(4);
          document.getElementById("ohm-power").value = p.toFixed(4);

          resEl.textContent = `Solved: V = ${v.toFixed(4)} V, I = ${i.toFixed(
            4
          )} A, R = ${r.toFixed(4)} Ω, P = ${p.toFixed(4)} W.`;
        } catch (e) {
          errEl.textContent = e.message;
          errEl.classList.remove("hidden");
        }
      });

      document.getElementById("ohm-clear").addEventListener("click", () => {
        ["ohm-voltage", "ohm-current", "ohm-resistance", "ohm-power"].forEach(
          (id) => (document.getElementById(id).value = "")
        );
        document.getElementById("ohm-error").classList.add("hidden");
        document.getElementById("ohm-result").textContent = "";
      });

      // Power & Energy
      document.getElementById("p-calc").addEventListener("click", () => {
        const V = parsePos("p-voltage");
        const I = parsePos("p-current");
        const t = parsePos("p-hours");
        const tariff = parsePos("p-tariff");
        const errEl = document.getElementById("p-error");
        const resEl = document.getElementById("p-result");
        errEl.classList.add("hidden");
        resEl.textContent = "";

        if (V == null || I == null) {
          errEl.textContent = "Enter both voltage and current.";
          errEl.classList.remove("hidden");
          return;
        }

        const P = V * I; // watts
        let msg = `Power: ${P.toFixed(2)} W`;

        if (t != null && t >= 0) {
          const kWh = (P * t) / 1000;
          msg += `, Energy: ${kWh.toFixed(3)} kWh over ${t.toFixed(2)} h`;
          if (tariff != null && tariff >= 0) {
            const cost = kWh * tariff;
            msg += `, Cost: ${cost.toFixed(2)} in your currency.`;
          } else {
            msg += ".";
          }
        } else {
          msg += ".";
        }

        resEl.textContent = msg;
      });

      document.getElementById("p-clear").addEventListener("click", () => {
        ["p-voltage", "p-current", "p-hours", "p-tariff"].forEach(
          (id) => (document.getElementById(id).value = "")
        );
        document.getElementById("p-error").classList.add("hidden");
        document.getElementById("p-result").textContent = "";
      });

      // Series / Parallel
      document.getElementById("sp-calc").addEventListener("click", () => {
        const inputs = Array.from(document.querySelectorAll(".sp-r"));
        const values = inputs
          .map((i) => parseFloat(i.value))
          .filter((v) => isFinite(v) && v > 0);
        const errEl = document.getElementById("sp-error");
        const resEl = document.getElementById("sp-result");
        errEl.classList.add("hidden");
        resEl.textContent = "";

        if (values.length === 0) {
          errEl.textContent = "Enter at least one positive resistance value.";
          errEl.classList.remove("hidden");
          return;
        }

        const mode = document.querySelector(
          'input[name="sp-mode"]:checked'
        ).value;
        let Req;
        if (mode === "series") {
          Req = values.reduce((a, b) => a + b, 0);
        } else {
          const inv = values.reduce((a, b) => a + 1 / b, 0);
          Req = 1 / inv;
        }
        resEl.textContent = `Equivalent resistance (${mode}): ${Req.toFixed(
          4
        )} Ω.`;
      });

      document.getElementById("sp-clear").addEventListener("click", () => {
        document.querySelectorAll(".sp-r").forEach((i) => (i.value = ""));
        document.getElementById("sp-error").classList.add("hidden");
        document.getElementById("sp-result").textContent = "";
      });

      // Voltage Drop
      document.getElementById("vd-calc").addEventListener("click", () => {
        const Vs = parsePos("vd-voltage");
        const I = parsePos("vd-current");
        const L = parsePos("vd-length");
        const A = parsePos("vd-area");
        const errEl = document.getElementById("vd-error");
        const resEl = document.getElementById("vd-result");
        errEl.classList.add("hidden");
        resEl.textContent = "";

        if (
          Vs == null ||
          I == null ||
          L == null ||
          A == null ||
          Vs <= 0 ||
          I < 0 ||
          L <= 0 ||
          A <= 0
        ) {
          errEl.textContent =
            "Enter positive values for voltage, current, length, and area.";
          errEl.classList.remove("hidden");
          return;
        }

        const rho = 1.72e-8; // ohm·m for copper
        const A_m2 = A * 1e-6; // mm² to m²
        const R = (rho * (2 * L)) / A_m2;
        const dV = I * R;
        const pct = (dV / Vs) * 100;
        const Vload = Vs - dV;

        resEl.textContent = `Voltage drop: ${dV.toFixed(3)} V (${pct.toFixed(
          2
        )}%), load voltage ≈ ${Vload.toFixed(3)} V.`;
      });

      document.getElementById("vd-clear").addEventListener("click", () => {
        ["vd-voltage", "vd-current", "vd-length", "vd-area"].forEach(
          (id) => (document.getElementById(id).value = "")
        );
        document.getElementById("vd-error").classList.add("hidden");
        document.getElementById("vd-result").textContent = "";
      });

      // Wire Size Suggestion
      const wireTable = [
        { awg: "18 AWG", area: 0.82, amp: 7 },
        { awg: "16 AWG", area: 1.31, amp: 10 },
        { awg: "14 AWG", area: 2.08, amp: 15 },
        { awg: "12 AWG", area: 3.31, amp: 20 },
        { awg: "10 AWG", area: 5.26, amp: 30 },
        { awg: "8 AWG", area: 8.37, amp: 40 },
        { awg: "6 AWG", area: 13.3, amp: 55 },
        { awg: "4 AWG", area: 21.1, amp: 70 },
      ];

      document.getElementById("ws-calc").addEventListener("click", () => {
        const V = parsePos("ws-voltage");
        const I = parsePos("ws-current");
        const L = parsePos("ws-length");
        const dropPct = parsePos("ws-drop");
        const errEl = document.getElementById("ws-error");
        const resEl = document.getElementById("ws-result");
        errEl.classList.add("hidden");
        resEl.textContent = "";

        if (
          V == null ||
          I == null ||
          L == null ||
          dropPct == null ||
          V <= 0 ||
          I <= 0 ||
          L <= 0 ||
          dropPct <= 0
        ) {
          errEl.textContent =
            "Enter positive values for voltage, current, length, and max drop.";
          errEl.classList.remove("hidden");
          return;
        }

        const maxDropV = (V * dropPct) / 100;
        const Rmax = maxDropV / I;
        const rho = 1.72e-8;
        const Amin_m2 = (rho * (2 * L)) / Rmax;
        const Amin_mm2 = Amin_m2 * 1e6;

        const candidates = wireTable.filter(
          (w) => w.area >= Amin_mm2 && w.amp >= I
        );
        if (candidates.length === 0) {
          resEl.textContent = `Required area ≈ ${Amin_mm2.toFixed(
            2
          )} mm². This exceeds the range of the quick table; use a detailed cable-sizing tool.`;
          return;
        }

        const best = candidates[0];
        resEl.textContent = `Estimated minimum: ${
          best.awg
        } (≈ ${best.area.toFixed(2)} mm²), ampacity ≈ ${
          best.amp
        } A. Computed minimum area for your drop limit: ${Amin_mm2.toFixed(
          2
        )} mm². Always verify against official ampacity tables and local code.`;
      });

      document.getElementById("ws-clear").addEventListener("click", () => {
        ["ws-voltage", "ws-current", "ws-length", "ws-drop"].forEach((id) => {
          if (id === "ws-drop") {
            document.getElementById(id).value = 3;
          } else {
            document.getElementById(id).value = "";
          }
        });
        document.getElementById("ws-error").classList.add("hidden");
        document.getElementById("ws-result").textContent = "";
      });

      // Mobile menu script (fallback if not in external file)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", () => {
          const menu = document.getElementById("mobile-menu");
          const expanded = menu.classList.toggle("hidden");
          document
            .getElementById("mobile-menu-toggle")
            .setAttribute("aria-expanded", !expanded);
        });
    </script>

