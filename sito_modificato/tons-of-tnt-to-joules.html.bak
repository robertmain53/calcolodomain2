    <script>
      (function () {
        const TNT_TO_J = 4.184e9; // 1 ton TNT in joules (conventional)
        const J_TO_TNT = 1 / TNT_TO_J;

        const tntInput = document.getElementById("tnt-tons-input");
        const tntScale = document.getElementById("tnt-scale-select");
        const joulesInput = document.getElementById("joules-input");

        const resultJ = document.getElementById("result-joules");
        const resultJSci = document.getElementById("result-joules-sci");
        const resultKJ = document.getElementById("result-kj");
        const resultMJ = document.getElementById("result-mj");
        const resultGJ = document.getElementById("result-gj");
        const resultTons = document.getElementById("result-tons");
        const resultKT = document.getElementById("result-kt");
        const resultMT = document.getElementById("result-mt");
        const resultKWh = document.getElementById("result-kwh");
        const resultCal = document.getElementById("result-cal");

        const swapBtn = document.getElementById("swap-direction-btn");
        const resetBtn = document.getElementById("reset-btn");
        const activeLabel = document.getElementById("active-input-label");

        let activeSide = "tnt"; // 'tnt' or 'joules'
        let isUpdating = false;

        function formatNumber(value) {
          if (!isFinite(value)) return "–";
          const abs = Math.abs(value);
          if (abs === 0) return "0";
          if (abs >= 1e9 || abs < 1e-3) {
            return value
              .toExponential(3)
              .replace("e+", "×10^")
              .replace("e-", "×10^-");
          }
          if (abs >= 1e6) return value.toFixed(0);
          if (abs >= 1e3) return value.toFixed(1);
          if (abs >= 1) return value.toFixed(3);
          return value.toPrecision(3);
        }

        function formatSci(value) {
          if (!isFinite(value) || value === 0) return "–";
          return value.toExponential(4);
        }

        function clearResults() {
          [
            resultJ,
            resultJSci,
            resultKJ,
            resultMJ,
            resultGJ,
            resultTons,
            resultKT,
            resultMT,
            resultKWh,
            resultCal,
          ].forEach((el) => (el.textContent = "–"));
        }

        function updateFromTNT() {
          const raw = parseFloat(tntInput.value);
          if (isNaN(raw)) {
            clearResults();
            joulesInput.value = "";
            return;
          }

          let tons;
          switch (tntScale.value) {
            case "kt":
              tons = raw * 1e3;
              break;
            case "mt":
              tons = raw * 1e6;
              break;
            default:
              tons = raw;
          }

          const joules = tons * TNT_TO_J;
          isUpdating = true;
          joulesInput.value = joules;
          isUpdating = false;
          updateResultsFromJoules(joules);
        }

        function updateFromJoules() {
          const j = parseFloat(joulesInput.value);
          if (isNaN(j)) {
            clearResults();
            tntInput.value = "";
            return;
          }

          const tons = j * J_TO_TNT;
          let display;
          switch (tntScale.value) {
            case "kt":
              display = tons / 1e3;
              break;
            case "mt":
              display = tons / 1e6;
              break;
            default:
              display = tons;
          }

          isUpdating = true;
          tntInput.value = isFinite(display) ? display : "";
          isUpdating = false;
          updateResultsFromJoules(j);
        }

        function updateResultsFromJoules(j) {
          if (!isFinite(j)) {
            clearResults();
            return;
          }

          const tons = j * J_TO_TNT;
          const kt = tons / 1e3;
          const mt = tons / 1e6;
          const kj = j / 1e3;
          const mj = j / 1e6;
          const gj = j / 1e9;
          const kwh = j / 3.6e6;
          const cal = j / 4.184;

          resultJ.textContent = formatNumber(j);
          resultJSci.textContent = formatSci(j);
          resultKJ.textContent = formatNumber(kj);
          resultMJ.textContent = formatNumber(mj);
          resultGJ.textContent = formatNumber(gj);
          resultTons.textContent = formatNumber(tons);
          resultKT.textContent = formatNumber(kt);
          resultMT.textContent = formatNumber(mt);
          resultKWh.textContent = formatNumber(kwh);
          resultCal.textContent = formatNumber(cal);
        }

        function setActiveSide(side) {
          activeSide = side;
          if (side === "tnt") {
            tntInput.classList.add("ring-2", "ring-blue-500");
            joulesInput.classList.remove("ring-2", "ring-blue-500");
            activeLabel.textContent = "Active: tons of TNT → joules";
          } else {
            joulesInput.classList.add("ring-2", "ring-blue-500");
            tntInput.classList.remove("ring-2", "ring-blue-500");
            activeLabel.textContent = "Active: joules → tons of TNT";
          }
        }

        tntInput.addEventListener("input", () => {
          if (isUpdating) return;
          setActiveSide("tnt");
          updateFromTNT();
        });

        tntScale.addEventListener("change", () => {
          if (activeSide === "tnt") {
            updateFromTNT();
          } else {
            updateFromJoules();
          }
        });

        joulesInput.addEventListener("input", () => {
          if (isUpdating) return;
          setActiveSide("joules");
          updateFromJoules();
        });

        swapBtn.addEventListener("click", () => {
          setActiveSide(activeSide === "tnt" ? "joules" : "tnt");
          if (activeSide === "tnt") {
            updateFromTNT();
          } else {
            updateFromJoules();
          }
        });

        resetBtn.addEventListener("click", () => {
          tntInput.value = "";
          joulesInput.value = "";
          clearResults();
        });

        // Initialize
        clearResults();
        setActiveSide("tnt");
      })();
    </script>

