    <script>
      // Utility: format currency
      function formatCurrency(value) {
        if (!isFinite(value)) return "$0";
        return value.toLocaleString("en-US", {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 0,
        });
      }

      function getStateRate() {
        const stateSelect = document.getElementById("stateRate");
        const customWrapper = document.getElementById("stateRateCustomWrapper");
        const customInput = document.getElementById("stateRateCustom");
        if (stateSelect.value === "custom") {
          customWrapper.classList.remove("hidden");
          const v = parseFloat(customInput.value) || 0;
          return v / 100;
        } else {
          customWrapper.classList.add("hidden");
          const v = parseFloat(stateSelect.value) || 0;
          return v / 100;
        }
      }

      function updateBadges() {
        const payoutType =
          document.querySelector('input[name="payoutType"]:checked')?.value ||
          "cash";
        const badgeCash = document.getElementById("badgeCash");
        const badgeAnnuity = document.getElementById("badgeAnnuity");
        if (payoutType === "cash") {
          badgeCash.classList.remove("hidden");
          badgeAnnuity.classList.add("hidden");
        } else {
          badgeAnnuity.classList.remove("hidden");
          badgeCash.classList.add("hidden");
        }
      }

      function calculateLottery() {
        const jackpot = Math.max(
          0,
          parseFloat(document.getElementById("jackpot").value) || 0
        );
        const cashPercent = Math.min(
          100,
          Math.max(
            0,
            parseFloat(document.getElementById("cashPercent").value) || 0
          )
        );
        const federalRate =
          Math.min(
            100,
            Math.max(
              0,
              parseFloat(document.getElementById("federalRate").value) || 0
            )
          ) / 100;
        const stateRate = getStateRate();
        const totalRate = Math.min(0.99, federalRate + stateRate); // cap to avoid >100%
        const numWinners = Math.max(
          1,
          parseInt(document.getElementById("numWinners").value, 10) || 1
        );
        const numPeople = Math.max(
          1,
          parseInt(document.getElementById("numPeople").value, 10) || 1
        );
        const annYears = Math.max(
          1,
          parseInt(document.getElementById("annuityYears").value, 10) || 30
        );
        const annGrowthPct = Math.max(
          0,
          parseFloat(document.getElementById("annuityGrowth").value) || 0
        );
        const g = annGrowthPct / 100;

        const cashOption = jackpot * (cashPercent / 100);

        const yourShareJackpot = jackpot / numWinners;
        const yourShareCash = cashOption / numWinners;

        // Lump sum taxes
        const cashTax = yourShareCash * totalRate;
        const cashNet = yourShareCash - cashTax;
        const cashNetPerPerson = cashNet / numPeople;

        // Annuity: assume jackpot share is total of nominal payments
        const annTotal = yourShareJackpot;

        // Approximate first payment using growing annuity factor
        let factor;
        if (g === 0) {
          factor = annYears;
        } else {
          factor = (Math.pow(1 + g, annYears) - 1) / g;
        }
        const firstPayment = annTotal / factor;

        // Taxes on annuity: apply same effective rate to total
        const annTax = annTotal * totalRate;
        const annNet = annTotal - annTax;
        const annNetPerPerson = annNet / numPeople;

        const firstPaymentAfterTax = firstPayment * (1 - totalRate);

        // Update UI
        document.getElementById("resJackpot").textContent =
          formatCurrency(jackpot);
        document.getElementById("resCashOption").textContent =
          formatCurrency(cashOption);
        document.getElementById("resYourShareJackpot").textContent =
          formatCurrency(yourShareJackpot);
        document.getElementById("resYourShareCash").textContent =
          formatCurrency(yourShareCash);

        document.getElementById("resCashGross").textContent =
          formatCurrency(yourShareCash);
        document.getElementById("resCashFedTax").textContent = formatCurrency(
          yourShareCash * federalRate
        );
        document.getElementById("resCashStateTax").textContent = formatCurrency(
          yourShareCash * stateRate
        );
        document.getElementById("resCashNet").textContent =
          formatCurrency(cashNet);
        document.getElementById("resCashNetPerPerson").textContent =
          formatCurrency(cashNetPerPerson);

        document.getElementById("resAnnTotal").textContent =
          formatCurrency(annTotal);
        document.getElementById("resAnnFedTax").textContent = formatCurrency(
          annTotal * federalRate
        );
        document.getElementById("resAnnStateTax").textContent = formatCurrency(
          annTotal * stateRate
        );
        document.getElementById("resAnnNet").textContent =
          formatCurrency(annNet);
        document.getElementById("resAnnNetPerPerson").textContent =
          formatCurrency(annNetPerPerson);
        document.getElementById("resAnnFirstPayment").textContent =
          formatCurrency(firstPaymentAfterTax) + " / year";

        updateBadges();
      }

      function applyPreset() {
        const preset = document.getElementById("gamePreset").value;
        const jackpotInput = document.getElementById("jackpot");
        const cashPercentInput = document.getElementById("cashPercent");
        const federalRateInput = document.getElementById("federalRate");
        const stateSelect = document.getElementById("stateRate");

        if (preset === "powerball") {
          if (!jackpotInput.value || parseFloat(jackpotInput.value) === 0) {
            jackpotInput.value = 100000000;
          }
          cashPercentInput.value = 60;
          federalRateInput.value = 37;
          stateSelect.value = "5";
        } else if (preset === "megamillions") {
          if (!jackpotInput.value || parseFloat(jackpotInput.value) === 0) {
            jackpotInput.value = 100000000;
          }
          cashPercentInput.value = 58;
          federalRateInput.value = 37;
          stateSelect.value = "5";
        }
        calculateLottery();
      }

      document.addEventListener("DOMContentLoaded", function () {
        document
          .getElementById("calculateBtn")
          .addEventListener("click", function (e) {
            e.preventDefault();
            calculateLottery();
          });

        document
          .getElementById("gamePreset")
          .addEventListener("change", applyPreset);
        document
          .getElementById("stateRate")
          .addEventListener("change", function () {
            getStateRate();
            calculateLottery();
          });
        document
          .getElementById("stateRateCustom")
          .addEventListener("input", calculateLottery);

        document
          .querySelectorAll('input[name="payoutType"]')
          .forEach(function (el) {
            el.addEventListener("change", updateBadges);
          });

        // Recalculate on key inputs blur for responsiveness
        [
          "jackpot",
          "cashPercent",
          "federalRate",
          "numWinners",
          "numPeople",
          "annuityYears",
          "annuityGrowth",
        ].forEach(function (id) {
          const el = document.getElementById(id);
          if (el) {
            el.addEventListener("change", calculateLottery);
            el.addEventListener("blur", calculateLottery);
          }
        });

        // Initial calculation
        calculateLottery();
      });

      // Mobile menu script (simple)
      document.addEventListener("DOMContentLoaded", function () {
        const toggle = document.getElementById("mobile-menu-toggle");
        const menu = document.getElementById("mobile-menu");
        if (toggle && menu) {
          toggle.addEventListener("click", () => {
            const isHidden = menu.classList.contains("hidden");
            menu.classList.toggle("hidden", !isHidden);
            toggle.setAttribute("aria-expanded", isHidden ? "true" : "false");
          });
        }
      });

      window.MathJax = {
        tex: {
          inlineMath: [
