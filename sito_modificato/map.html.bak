    <script>
      // Mobile menu toggle
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          this.setAttribute("aria-expanded", isHidden ? "true" : "false");
        });

      // Utility: clamp and validate number
      function parseBPValue(value, min, max) {
        if (value === "" || value === null || value === undefined) return null;
        const num = Number(value);
        if (isNaN(num)) return null;
        if (num < min || num > max) return null;
        return num;
      }

      function computeMAP(sbp, dbp) {
        return (sbp + 2 * dbp) / 3;
      }

      function classifyMAP(map) {
        if (map < 60) {
          return {
            label: "Low perfusion",
            badgeClass: "bg-red-100 text-red-800 border-red-200",
            text: "Your mean arterial pressure appears low. This may be insufficient for adequate organ perfusion and can be an emergency in the right clinical context. Seek urgent medical advice if this matches your actual situation.",
          };
        } else if (map < 70) {
          return {
            label: "Borderline low",
            badgeClass: "bg-amber-100 text-amber-800 border-amber-200",
            text: "Your MAP is on the low side. In some situations this may be acceptable, but in others it could be concerning, especially if you have symptoms such as dizziness, confusion, or fainting.",
          };
        } else if (map <= 100) {
          return {
            label: "Adequate perfusion",
            badgeClass: "bg-green-100 text-green-800 border-green-200",
            text: "Your MAP falls within a range that is commonly considered adequate for organ perfusion in many adults. Individual targets may differ based on age and medical conditions.",
          };
        } else {
          return {
            label: "Elevated",
            badgeClass: "bg-red-100 text-red-800 border-red-200",
            text: "Your MAP is elevated. Persistently high arterial pressure is often associated with hypertension and increased cardiovascular risk. Discuss this pattern with your healthcare provider.",
          };
        }
      }

      function heartRateNote(hr) {
        if (hr == null) return null;
        if (hr < 50) {
          return "Your heart rate is relatively low. In bradycardia, the simple MAP formula may be less accurate because the time spent in diastole versus systole changes.";
        } else if (hr > 120) {
          return "Your heart rate is relatively high. In tachycardia, the simple MAP formula may be less accurate because the heart spends proportionally less time in diastole.";
        }
        return null;
      }

      // UI elements
      const modeSingleBtn = document.getElementById("mode-single");
      const modeMultiBtn = document.getElementById("mode-multi");
      const singlePanel = document.getElementById("single-mode-panel");
      const multiPanel = document.getElementById("multi-mode-panel");

      modeSingleBtn.addEventListener("click", () => {
        modeSingleBtn.classList.add(
          "bg-blue-600",
          "text-white",
          "border-blue-600"
        );
        modeSingleBtn.classList.remove("border-gray-300", "text-gray-700");
        modeMultiBtn.classList.remove(
          "bg-blue-600",
          "text-white",
          "border-blue-600"
        );
        modeMultiBtn.classList.add("border-gray-300", "text-gray-700");
        singlePanel.classList.remove("hidden");
        multiPanel.classList.add("hidden");
      });

      modeMultiBtn.addEventListener("click", () => {
        modeMultiBtn.classList.add(
          "bg-blue-600",
          "text-white",
          "border-blue-600"
        );
        modeMultiBtn.classList.remove("border-gray-300", "text-gray-700");
        modeSingleBtn.classList.remove(
          "bg-blue-600",
          "text-white",
          "border-blue-600"
        );
        modeSingleBtn.classList.add("border-gray-300", "text-gray-700");
        singlePanel.classList.add("hidden");
        multiPanel.classList.remove("hidden");
      });

      const sbpInput = document.getElementById("sbp");
      const dbpInput = document.getElementById("dbp");
      const hrInput = document.getElementById("hr");
      const calcSingleBtn = document.getElementById("calc-single");
      const clearSingleBtn = document.getElementById("clear-single");
      const singleError = document.getElementById("single-error");

      const resultMain = document.getElementById("result-main");
      const mapValueEl = document.getElementById("map-value");
      const mapStatusBadge = document.getElementById("map-status-badge");
      const mapInterpretation = document.getElementById("map-interpretation");
      const mapNote = document.getElementById("map-note");

      function showResult(map, hr) {
        const cls = classifyMAP(map);
        mapValueEl.textContent = map.toFixed(1) + " mmHg";
        mapStatusBadge.textContent = cls.label;
        mapStatusBadge.className = "badge border " + cls.badgeClass;
        mapInterpretation.textContent = cls.text;

        const note = heartRateNote(hr);
        if (note) {
          mapNote.textContent =
            note +
            " This calculator still provides a useful estimate, but clinical decisions should rely on professional assessment.";
          mapNote.classList.remove("hidden");
        } else {
          mapNote.classList.add("hidden");
        }

        resultMain.classList.remove("hidden");
      }

      calcSingleBtn.addEventListener("click", () => {
        singleError.classList.add("hidden");
        const sbp = parseBPValue(sbpInput.value, 30, 300);
        const dbp = parseBPValue(dbpInput.value, 20, 200);
        const hr =
          hrInput.value === "" ? null : parseBPValue(hrInput.value, 20, 220);

        if (sbp == null || dbp == null) {
          singleError.textContent =
            "Please enter valid systolic and diastolic values within typical adult ranges.";
          singleError.classList.remove("hidden");
          return;
        }
        if (sbp <= dbp) {
          singleError.textContent =
            "Systolic pressure should be higher than diastolic pressure.";
          singleError.classList.remove("hidden");
          return;
        }

        const map = computeMAP(sbp, dbp);
        showResult(map, hr);
      });

      clearSingleBtn.addEventListener("click", () => {
        sbpInput.value = "";
        dbpInput.value = "";
        hrInput.value = "";
        singleError.classList.add("hidden");
      });

      // Multiple readings
      const sbpMultiInput = document.getElementById("sbp-multi");
      const dbpMultiInput = document.getElementById("dbp-multi");
      const addReadingBtn = document.getElementById("add-reading");
      const multiError = document.getElementById("multi-error");
      const readingsContainer = document.getElementById("readings-container");
      const readingsTableBody = document.getElementById("readings-table-body");
      const clearReadingsBtn = document.getElementById("clear-readings");
      const calcMultiBtn = document.getElementById("calc-multi");

      let readings = [];

      function renderReadings() {
        readingsTableBody.innerHTML = "";
        if (readings.length === 0) {
          readingsContainer.classList.add("hidden");
          return;
        }
        readingsContainer.classList.remove("hidden");
        readings.forEach((r, idx) => {
          const tr = document.createElement("tr");
          tr.className = "border-t";
          tr.innerHTML = `
        <td class="px-3 py-2">${idx + 1}</td>
        <td class="px-3 py-2">${r.sbp}</td>
        <td class="px-3 py-2">${r.dbp}</td>
        <td class="px-3 py-2">${r.map.toFixed(1)}</td>
        <td class="px-3 py-2">
          <button type="button" data-index="${idx}" class="text-xs text-red-600 hover:underline remove-reading">Remove</button>
        </td>
      `;
          readingsTableBody.appendChild(tr);
        });

        readingsTableBody.querySelectorAll(".remove-reading").forEach((btn) => {
          btn.addEventListener("click", (e) => {
            const index = Number(e.target.getAttribute("data-index"));
            readings.splice(index, 1);
            renderReadings();
          });
        });
      }

      addReadingBtn.addEventListener("click", () => {
        multiError.classList.add("hidden");
        const sbp = parseBPValue(sbpMultiInput.value, 30, 300);
        const dbp = parseBPValue(dbpMultiInput.value, 20, 200);

        if (sbp == null || dbp == null) {
          multiError.textContent =
            "Please enter valid systolic and diastolic values within typical adult ranges.";
          multiError.classList.remove("hidden");
          return;
        }
        if (sbp <= dbp) {
          multiError.textContent =
            "Systolic pressure should be higher than diastolic pressure.";
          multiError.classList.remove("hidden");
          return;
        }

        const map = computeMAP(sbp, dbp);
        readings.push({ sbp, dbp, map });
        sbpMultiInput.value = "";
        dbpMultiInput.value = "";
        renderReadings();
      });

      clearReadingsBtn.addEventListener("click", () => {
        readings = [];
        renderReadings();
      });

      calcMultiBtn.addEventListener("click", () => {
        multiError.classList.add("hidden");
        if (readings.length === 0) {
          multiError.textContent =
            "Add at least one blood pressure reading first.";
          multiError.classList.remove("hidden");
          return;
        }
        const avgMap =
          readings.reduce((sum, r) => sum + r.map, 0) / readings.length;
        showResult(avgMap, null);
      });

      // Initialize default example
      (function initExample() {
        const exampleMAP = computeMAP(120, 80);
        showResult(exampleMAP, 70);
      })();

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
