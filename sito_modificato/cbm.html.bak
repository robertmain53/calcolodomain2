    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
    
    <script>
      // Mobile menu toggle
      document.getElementById('mobile-menu-toggle')?.addEventListener('click', function () {
        const menu = document.getElementById('mobile-menu');
        if (!menu) return;
        const isHidden = menu.classList.contains('hidden');
        menu.classList.toggle('hidden', !isHidden);
        this.setAttribute('aria-expanded', String(isHidden));
      });

      document.addEventListener('DOMContentLoaded', function () {
        const lengthInput = document.getElementById('length-input');
        const widthInput = document.getElementById('width-input');
        const heightInput = document.getElementById('height-input');
        const quantityInput = document.getElementById('quantity-input');
        const cbmSingleOutput = document.getElementById('cbm-single-output');
        const cbmTotalOutput = document.getElementById('cbm-total-output');
        const cbmSummaryOutput = document.getElementById('cbm-summary-output');
        const unitRadios = document.querySelectorAll('.dim-unit-radio');
        const presetButtons = document.querySelectorAll('.preset-cbm-btn');
        const cbmClearBtn = document.getElementById('cbm-clear');

        const volumeValueInput = document.getElementById('volume-value');
        const volumeUnitSelect = document.getElementById('volume-unit');
        const volumeCbmOutput = document.getElementById('volume-cbm-output');

        let isUpdating = false;
        let currentUnit = 'cm';

        unitRadios.forEach(function (radio) {
          radio.addEventListener('change', function () {
            if (this.checked) {
              currentUnit = this.value;
              updateCbm();
            }
          });
        });

        function parseNumber(input) {
          const raw = input.value;
          if (raw === '' || isNaN(Number(raw))) return null;
          return Number(raw);
        }

        function formatNumber(value) {
          if (!isFinite(value)) return '';
          const absVal = Math.abs(value);
          if (absVal !== 0 && (absVal < 1e-4 || absVal >= 1e7)) {
            return value.toExponential(6);
          }
          return Number(value.toFixed(6)).toString();
        }

        function convertDimToMeters(value, unit) {
          if (unit === 'cm') return value / 100;
          if (unit === 'm') return value;
          if (unit === 'in') return value * 0.0254;
          return value;
        }

        function updateCbm() {
          if (isUpdating) return;
          isUpdating = true;

          const L = parseNumber(lengthInput);
          const W = parseNumber(widthInput);
          const H = parseNumber(heightInput);
          let qty = parseNumber(quantityInput);
          if (qty === null || qty <= 0) qty = 1;

          if (L === null || W === null || H === null) {
            cbmSingleOutput.value = '';
            cbmTotalOutput.value = '';
            cbmSummaryOutput.value = '';
            isUpdating = false;
            return;
          }

          const Lm = convertDimToMeters(L, currentUnit);
          const Wm = convertDimToMeters(W, currentUnit);
          const Hm = convertDimToMeters(H, currentUnit);

          const cbmSingle = Lm * Wm * Hm;
          const cbmTotal = cbmSingle * qty;

          cbmSingleOutput.value = formatNumber(cbmSingle);
          cbmTotalOutput.value = formatNumber(cbmTotal);

          cbmSummaryOutput.value =
            'Dimensions: ' + L + ' × ' + W + ' × ' + H + ' ' + currentUnit +
            ', quantity: ' + qty + '. CBM per piece: ' + formatNumber(cbmSingle) +
            ' m³, total CBM: ' + formatNumber(cbmTotal) + ' m³.';

          isUpdating = false;
        }

        [lengthInput, widthInput, heightInput, quantityInput].forEach(function (input) {
          input.addEventListener('input', updateCbm);
        });

        presetButtons.forEach(function (btn) {
          btn.addEventListener('click', function () {
            const L = this.getAttribute('data-length');
            const W = this.getAttribute('data-width');
            const H = this.getAttribute('data-height');
            const Q = this.getAttribute('data-qty');
            if (!L || !W || !H || !Q) return;

            // Presets are given in cm; ensure cm is selected
            unitRadios.forEach(function (r) {
              if (r.value === 'cm') {
                r.checked = true;
                currentUnit = 'cm';
              }
            });

            lengthInput.value = L;
            widthInput.value = W;
            heightInput.value = H;
            quantityInput.value = Q;
            updateCbm();
          });
        });

        if (cbmClearBtn) {
          cbmClearBtn.addEventListener('click', function () {
            lengthInput.value = '';
            widthInput.value = '';
            heightInput.value = '';
            quantityInput.value = '1';
            cbmSingleOutput.value = '';
            cbmTotalOutput.value = '';
            cbmSummaryOutput.value = '';
          });
        }

        function updateVolumeToCbm() {
          const val = parseNumber(volumeValueInput);
          if (val === null) {
            volumeCbmOutput.value = '';
            return;
          }
          const unit = volumeUnitSelect.value;
          let m3 = val;

          switch (unit) {
            case 'm3':
              m3 = val;
              break;
            case 'l':
              m3 = val / 1000; // 1000 L = 1 m³
              break;
            case 'cm3':
              m3 = val / 1e6; // 1,000,000 cm³ = 1 m³
              break;
            case 'ft3':
              m3 = val * 0.0283168466;
              break;
            case 'in3':
              m3 = val * 1.6387064e-5;
              break;
          }

          volumeCbmOutput.value = formatNumber(m3);
        }

        if (volumeValueInput && volumeUnitSelect && volumeCbmOutput) {
          volumeValueInput.addEventListener('input', updateVolumeToCbm);
          volumeUnitSelect.addEventListener('change', updateVolumeToCbm);
        }
      });
    </script>

