    <script>
      // --- Tax data (simplified, illustrative; update with actual IRS values as needed) ---
      const TAX_DATA = {
        2024: {
          standardDeduction: {
            single: 14600,
            mfj: 29200,
            mfs: 14600,
            hoh: 21900,
          },
          brackets: {
            single: [
              { upTo: 11600, rate: 0.1 },
              { upTo: 47150, rate: 0.12 },
              { upTo: 100525, rate: 0.22 },
              { upTo: 191950, rate: 0.24 },
              { upTo: 243725, rate: 0.32 },
              { upTo: 609350, rate: 0.35 },
              { upTo: Infinity, rate: 0.37 },
            ],
            mfj: [
              { upTo: 23200, rate: 0.1 },
              { upTo: 94300, rate: 0.12 },
              { upTo: 201050, rate: 0.22 },
              { upTo: 383900, rate: 0.24 },
              { upTo: 487450, rate: 0.32 },
              { upTo: 731200, rate: 0.35 },
              { upTo: Infinity, rate: 0.37 },
            ],
            mfs: [
              { upTo: 11600, rate: 0.1 },
              { upTo: 47150, rate: 0.12 },
              { upTo: 100525, rate: 0.22 },
              { upTo: 191950, rate: 0.24 },
              { upTo: 243725, rate: 0.32 },
              { upTo: 365600, rate: 0.35 },
              { upTo: Infinity, rate: 0.37 },
            ],
            hoh: [
              { upTo: 16550, rate: 0.1 },
              { upTo: 63100, rate: 0.12 },
              { upTo: 100500, rate: 0.22 },
              { upTo: 191950, rate: 0.24 },
              { upTo: 243700, rate: 0.32 },
              { upTo: 609350, rate: 0.35 },
              { upTo: Infinity, rate: 0.37 },
            ],
          },
        },
        2025: {
          // For now, assume same as 2024 (placeholder). Update when official brackets are available.
          standardDeduction: {
            single: 14600,
            mfj: 29200,
            mfs: 14600,
            hoh: 21900,
          },
          brackets: {},
        },
      };
      // Copy 2024 brackets into 2025 as a simple projection
      TAX_DATA[2025].brackets = JSON.parse(
        JSON.stringify(TAX_DATA[2024].brackets)
      );

      const COLORS = [
        "#1d4ed8",
        "#22c55e",
        "#f97316",
        "#a855f7",
        "#0ea5e9",
        "#e11d48",
        "#6b7280",
      ];

      const taxYearEl = document.getElementById("taxYear");
      const filingStatusEl = document.getElementById("filingStatus");
      const incomeSliderEl = document.getElementById("incomeSlider");
      const grossIncomeEl = document.getElementById("grossIncome");
      const itemizedDeductionsEl =
        document.getElementById("itemizedDeductions");
      const standardDeductionHintEl = document.getElementById(
        "standardDeductionHint"
      );
      const standardDeductionValueEl = document.getElementById(
        "standardDeductionValue"
      );
      const calculateBtn = document.getElementById("calculateBtn");

      const totalTaxEl = document.getElementById("totalTax");
      const marginalRateEl = document.getElementById("marginalRate");
      const effectiveRateEl = document.getElementById("effectiveRate");
      const taxableIncomeEl = document.getElementById("taxableIncome");
      const afterTaxIncomeEl = document.getElementById("afterTaxIncome");
      const resultsSectionEl = document.getElementById("resultsSection");
      const bracketTableBodyEl = document.getElementById("bracketTableBody");
      const taxBarEl = document.getElementById("taxBar");
      const taxBarLegendEl = document.getElementById("taxBarLegend");

      function formatCurrency(value) {
        return value.toLocaleString("en-US", {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 0,
        });
      }

      function formatPercent(value) {
        return (value * 100).toFixed(1) + "%";
      }

      function getSelectedDeductionType() {
        const radios = document.querySelectorAll("input.deduction-type");
        for (const r of radios) {
          if (r.checked) return r.value;
        }
        return "standard";
      }

      function updateStandardDeductionHint() {
        const year = parseInt(taxYearEl.value, 10);
        const status = filingStatusEl.value;
        const stdDed = TAX_DATA[year].standardDeduction[status] || 0;
        standardDeductionValueEl.textContent = formatCurrency(stdDed);
      }

      function syncIncomeFromSlider() {
        grossIncomeEl.value = incomeSliderEl.value;
      }

      function syncSliderFromIncome() {
        let val = parseFloat(grossIncomeEl.value) || 0;
        if (val < 0) val = 0;
        if (val > parseFloat(incomeSliderEl.max)) {
          incomeSliderEl.max = val;
        }
        incomeSliderEl.value = val;
      }

      function toggleDeductionInputs() {
        const type = getSelectedDeductionType();
        if (type === "standard") {
          itemizedDeductionsEl.disabled = true;
          itemizedDeductionsEl.classList.add("bg-gray-100");
        } else {
          itemizedDeductionsEl.disabled = false;
          itemizedDeductionsEl.classList.remove("bg-gray-100");
        }
      }

      function calculateTax() {
        const year = parseInt(taxYearEl.value, 10);
        const status = filingStatusEl.value;
        const income = Math.max(0, parseFloat(grossIncomeEl.value) || 0);
        const type = getSelectedDeductionType();
        const stdDed = TAX_DATA[year].standardDeduction[status] || 0;
        const itemized = Math.max(
          0,
          parseFloat(itemizedDeductionsEl.value) || 0
        );
        const deduction = type === "standard" ? stdDed : itemized;

        const taxableIncome = Math.max(0, income - deduction);
        const brackets = TAX_DATA[year].brackets[status];

        let remaining = taxableIncome;
        let lowerBound = 0;
        let totalTax = 0;
        const breakdown = [];

        for (let i = 0; i < brackets.length; i++) {
          const { upTo, rate } = brackets[i];
          if (remaining <= 0) break;
          const upperBound = upTo;
          const span = upperBound - lowerBound;
          const taxedHere = Math.max(0, Math.min(remaining, span));
          const taxHere = taxedHere * rate;
          if (taxedHere > 0) {
            breakdown.push({
              bracketLabel: `${formatCurrency(lowerBound)} – ${
                upTo === Infinity ? "and up" : formatCurrency(upperBound)
              }`,
              rate,
              taxedIncome: taxedHere,
              tax: taxHere,
            });
          }
          totalTax += taxHere;
          remaining -= taxedHere;
          lowerBound = upperBound;
        }

        const marginalRate =
          taxableIncome > 0 && breakdown.length
            ? breakdown[breakdown.length - 1].rate
            : 0;
        const effectiveRate = income > 0 ? totalTax / income : 0;
        const afterTaxIncome = income - totalTax;

        // Update UI
        taxableIncomeEl.textContent = formatCurrency(taxableIncome);
        totalTaxEl.textContent = formatCurrency(totalTax);
        afterTaxIncomeEl.textContent = formatCurrency(afterTaxIncome);
        marginalRateEl.textContent = formatPercent(marginalRate);
        effectiveRateEl.textContent = formatPercent(effectiveRate);

        // Table
        bracketTableBodyEl.innerHTML = "";
        if (breakdown.length === 0) {
          const tr = document.createElement("tr");
          tr.innerHTML =
            '<td colspan="4" class="px-3 py-2 text-center text-gray-500 text-sm">No taxable income in this scenario.</td>';
          bracketTableBodyEl.appendChild(tr);
        } else {
          breakdown.forEach((row, idx) => {
            const tr = document.createElement("tr");
            tr.className = idx % 2 === 0 ? "bg-white" : "bg-gray-50";
            tr.innerHTML = `
          <td class="px-3 py-2 border-t text-gray-700">${row.bracketLabel}</td>
          <td class="px-3 py-2 border-t text-right">${formatCurrency(
            row.taxedIncome
          )}</td>
          <td class="px-3 py-2 border-t text-right">${formatPercent(
            row.rate
          )}</td>
          <td class="px-3 py-2 border-t text-right">${formatCurrency(
            row.tax
          )}</td>
        `;
            bracketTableBodyEl.appendChild(tr);
          });
        }

        // Visual bar
        taxBarEl.innerHTML = "";
        taxBarLegendEl.innerHTML = "";
        const totalTaxedIncome = breakdown.reduce(
          (sum, b) => sum + b.taxedIncome,
          0
        );
        if (totalTaxedIncome > 0) {
          breakdown.forEach((row, idx) => {
            const widthPct = (row.taxedIncome / totalTaxedIncome) * 100;
            const color = COLORS[idx % COLORS.length];
            const segment = document.createElement("div");
            segment.style.width = widthPct + "%";
            segment.style.backgroundColor = color;
            segment.title = `${formatPercent(row.rate)} on ${formatCurrency(
              row.taxedIncome
            )}`;
            taxBarEl.appendChild(segment);

            const legendItem = document.createElement("div");
            legendItem.className = "flex items-center space-x-1";
            legendItem.innerHTML = `
          <span class="inline-block w-3 h-3 rounded-sm" style="background-color:${color}"></span>
          <span>${(row.rate * 100).toFixed(0)}% on ${formatCurrency(
              row.taxedIncome
            )}</span>
        `;
            taxBarLegendEl.appendChild(legendItem);
          });
        }

        resultsSectionEl.classList.remove("hidden");
      }

      // Event listeners
      incomeSliderEl.addEventListener("input", () => {
        syncIncomeFromSlider();
      });

      grossIncomeEl.addEventListener("input", () => {
        syncSliderFromIncome();
      });

      taxYearEl.addEventListener("change", () => {
        updateStandardDeductionHint();
      });

      filingStatusEl.addEventListener("change", () => {
        updateStandardDeductionHint();
      });

      document.querySelectorAll("input.deduction-type").forEach((radio) => {
        radio.addEventListener("change", () => {
          toggleDeductionInputs();
        });
      });

      calculateBtn.addEventListener("click", (e) => {
        e.preventDefault();
        calculateTax();
      });

      // Initialize
      updateStandardDeductionHint();
      toggleDeductionInputs();
      syncIncomeFromSlider();
    </script>

