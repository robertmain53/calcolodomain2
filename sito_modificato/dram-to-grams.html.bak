    <script>
      // Mobile menu toggle
      document.getElementById('mobile-menu-toggle')?.addEventListener('click', function () {
        const menu = document.getElementById('mobile-menu');
        if (!menu) return;
        const isHidden = menu.classList.contains('hidden');
        menu.classList.toggle('hidden', !isHidden);
        this.setAttribute('aria-expanded', String(isHidden));
      });

      document.addEventListener('DOMContentLoaded', function () {
        const valueInput = document.getElementById('value-input');
        const fromUnit = document.getElementById('from-unit');
        const toUnit = document.getElementById('to-unit');
        const resultOutput = document.getElementById('result-output');
        const statusOutput = document.getElementById('status-output');
        const swapBtn = document.getElementById('swap-units');
        const clearMainBtn = document.getElementById('clear-main');
        const presetMainButtons = document.querySelectorAll('.preset-main-btn');

        const syncG = document.getElementById('sync-g');
        const syncDrAv = document.getElementById('sync-dr-av');
        const syncDrAp = document.getElementById('sync-dr-ap');

        const DRAM_AV_G = 1.7718451953125;
        const DRAM_AP_G = 3.8879346;

        function parseNumber(input) {
          const raw = input.value;
          if (raw === '' || isNaN(Number(raw))) return null;
          return Number(raw);
        }

        function formatNumber(value) {
          if (!isFinite(value)) return '';
          const absVal = Math.abs(value);
          if (absVal !== 0 && (absVal < 1e-4 || absVal >= 1e7)) {
            return value.toExponential(6);
          }
          return Number(value.toFixed(6)).toString();
        }

        function toGrams(value, unit) {
          switch (unit) {
            case 'g':
              return value;
            case 'dr_av':
              return value * DRAM_AV_G;
            case 'dr_ap':
              return value * DRAM_AP_G;
            default:
              return value;
          }
        }

        function fromGrams(grams, unit) {
          switch (unit) {
            case 'g':
              return grams;
            case 'dr_av':
              return grams / DRAM_AV_G;
            case 'dr_ap':
              return grams / DRAM_AP_G;
            default:
              return grams;
          }
        }

        function updateSync(grams) {
          if (grams === null || !isFinite(grams)) {
            if (syncG) syncG.value = '';
            if (syncDrAv) syncDrAv.value = '';
            if (syncDrAp) syncDrAp.value = '';
            return;
          }
          if (syncG) syncG.value = formatNumber(grams);
          if (syncDrAv) syncDrAv.value = formatNumber(fromGrams(grams, 'dr_av'));
          if (syncDrAp) syncDrAp.value = formatNumber(fromGrams(grams, 'dr_ap'));
        }

        function getUnitLabel(unit) {
          if (unit === 'g') return 'g';
          if (unit === 'dr_av') return 'dr (avoirdupois)';
          if (unit === 'dr_ap') return 'dr (apothecaries)';
          return unit;
        }

        function updateConversion() {
          const val = parseNumber(valueInput);
          if (val === null) {
            resultOutput.value = '';
            statusOutput.value = '';
            updateSync(null);
            return;
          }

          const from = fromUnit.value;
          const to = toUnit.value;

          // Convert input to grams as pivot
          const grams = toGrams(val, from);
          const resultVal = fromGrams(grams, to);

          resultOutput.value = formatNumber(resultVal);

          const fromLabel = getUnitLabel(from);
          const toLabel = getUnitLabel(to);
          statusOutput.value =
            val + ' ' + fromLabel + ' = ' + formatNumber(resultVal) + ' ' + toLabel +
            ' (via ' + formatNumber(grams) + ' g as pivot).';

          updateSync(grams);
        }

        if (valueInput && fromUnit && toUnit) {
          valueInput.addEventListener('input', updateConversion);
          fromUnit.addEventListener('change', updateConversion);
          toUnit.addEventListener('change', updateConversion);
        }

        if (swapBtn) {
          swapBtn.addEventListener('click', function () {
            const oldFrom = fromUnit.value;
            fromUnit.value = toUnit.value;
            toUnit.value = oldFrom;
            updateConversion();
          });
        }

        if (clearMainBtn) {
          clearMainBtn.addEventListener('click', function () {
            valueInput.value = '';
            resultOutput.value = '';
            statusOutput.value = '';
            updateSync(null);
          });
        }

        presetMainButtons.forEach(function (btn) {
          btn.addEventListener('click', function () {
            const val = this.getAttribute('data-value');
            const from = this.getAttribute('data-from');
            const to = this.getAttribute('data-to');
            if (!val || !from || !to) return;
            valueInput.value = val;
            fromUnit.value = from;
            toUnit.value = to;
            updateConversion();
          });
        });
      });
    </script>

