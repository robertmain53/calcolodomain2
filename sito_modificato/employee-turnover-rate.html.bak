    <script>
      // Mobile menu toggle
      document
        .getElementById("mobile-menu-toggle")
        ?.addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          if (!menu) return;
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden");
          this.setAttribute("aria-expanded", String(isHidden));
        });

      // Calculator logic
      (function () {
        const periodEl = document.getElementById("period");
        const customDaysWrapper = document.getElementById(
          "custom-days-wrapper"
        );
        const customDaysEl = document.getElementById("custom-days");
        const startHeadcountEl = document.getElementById("start-headcount");
        const endHeadcountEl = document.getElementById("end-headcount");
        const totalSeparationsEl = document.getElementById("total-separations");
        const voluntarySeparationsEl = document.getElementById(
          "voluntary-separations"
        );
        const involuntarySeparationsEl = document.getElementById(
          "involuntary-separations"
        );
        const otherSeparationsEl = document.getElementById("other-separations");

        const turnoverRateEl = document.getElementById("turnover-rate");
        const turnoverNoteEl = document.getElementById("turnover-note");
        const retentionRateEl = document.getElementById("retention-rate");
        const annualizedTurnoverEl = document.getElementById(
          "annualized-turnover"
        );
        const voluntaryRateEl = document.getElementById("voluntary-rate");
        const involuntaryRateEl = document.getElementById("involuntary-rate");
        const totalSeparationsDisplayEl = document.getElementById(
          "total-separations-display"
        );

        const simpleModeBtn = document.getElementById("mode-simple");
        const advancedModeBtn = document.getElementById("mode-advanced");
        const simpleSeparationsBox =
          document.getElementById("simple-separations");
        const advancedSeparationsBox = document.getElementById(
          "advanced-separations"
        );
        const advancedResultsBox = document.getElementById("advanced-results");

        const calcBtn = document.getElementById("calculate-btn");
        const resetBtn = document.getElementById("reset-btn");

        let mode = "simple"; // 'simple' or 'advanced'

        function setMode(newMode) {
          mode = newMode;
          if (mode === "simple") {
            simpleModeBtn.classList.remove("bg-gray-100", "text-gray-700");
            simpleModeBtn.classList.add("bg-blue-600", "text-white");
            advancedModeBtn.classList.remove("bg-blue-600", "text-white");
            advancedModeBtn.classList.add("bg-gray-100", "text-gray-700");
            simpleSeparationsBox.classList.remove("hidden");
            advancedSeparationsBox.classList.add("hidden");
            advancedResultsBox.classList.add("hidden");
          } else {
            advancedModeBtn.classList.remove("bg-gray-100", "text-gray-700");
            advancedModeBtn.classList.add("bg-blue-600", "text-white");
            simpleModeBtn.classList.remove("bg-blue-600", "text-white");
            simpleModeBtn.classList.add("bg-gray-100", "text-gray-700");
            simpleSeparationsBox.classList.add("hidden");
            advancedSeparationsBox.classList.remove("hidden");
            advancedResultsBox.classList.remove("hidden");
          }
        }

        simpleModeBtn.addEventListener("click", () => setMode("simple"));
        advancedModeBtn.addEventListener("click", () => setMode("advanced"));

        periodEl.addEventListener("change", () => {
          if (periodEl.value === "custom") {
            customDaysWrapper.classList.remove("hidden");
          } else {
            customDaysWrapper.classList.add("hidden");
          }
        });

        function parseNumber(el) {
          const v = parseFloat(el.value);
          return isNaN(v) ? 0 : v;
        }

        function formatPercent(value) {
          if (!isFinite(value)) return "–";
          if (value === 0) return "0%";
          if (Math.abs(value) < 0.01) return value.toFixed(3) + "%";
          if (Math.abs(value) < 1) return value.toFixed(2) + "%";
          if (Math.abs(value) < 10) return value.toFixed(1) + "%";
          return value.toFixed(1) + "%";
        }

        function getPeriodMonths() {
          switch (periodEl.value) {
            case "monthly":
              return 1;
            case "quarterly":
              return 3;
            case "annual":
              return 12;
            case "custom":
              const days = parseNumber(customDaysEl);
              if (days <= 0) return null;
              return days / 30.4375; // average days per month
            default:
              return 1;
          }
        }

        function calculate() {
          const start = parseNumber(startHeadcountEl);
          const end = parseNumber(endHeadcountEl);
          const avg = (start + end) / 2;

          if (avg <= 0) {
            turnoverRateEl.textContent = "–";
            retentionRateEl.textContent = "–";
            annualizedTurnoverEl.textContent = "–";
            voluntaryRateEl.textContent = "–";
            involuntaryRateEl.textContent = "–";
            totalSeparationsDisplayEl.textContent = "–";
            turnoverNoteEl.textContent =
              "Average headcount is zero or missing. Please check your inputs.";
            return;
          }

          let totalSeparations;
          let voluntary = 0;
          let involuntary = 0;

          if (mode === "simple") {
            totalSeparations = parseNumber(totalSeparationsEl);
          } else {
            voluntary = parseNumber(voluntarySeparationsEl);
            involuntary = parseNumber(involuntarySeparationsEl);
            const other = parseNumber(otherSeparationsEl);
            totalSeparations = voluntary + involuntary + other;
          }

          if (totalSeparations < 0) totalSeparations = 0;

          const turnover = (totalSeparations / avg) * 100;
          const retention = 100 - turnover;

          turnoverRateEl.textContent = formatPercent(turnover);
          retentionRateEl.textContent = isFinite(retention)
            ? formatPercent(retention)
            : "–";

          // Advanced breakdown
          if (mode === "advanced") {
            const voluntaryRate = (voluntary / avg) * 100;
            const involuntaryRate = (involuntary / avg) * 100;
            voluntaryRateEl.textContent = formatPercent(voluntaryRate);
            involuntaryRateEl.textContent = formatPercent(involuntaryRate);
            totalSeparationsDisplayEl.textContent = totalSeparations.toFixed(0);
          }

          // Annualized estimate
          const months = getPeriodMonths();
          if (!months || months <= 0) {
            annualizedTurnoverEl.textContent = "–";
          } else {
            const annualized = turnover * (12 / months);
            annualizedTurnoverEl.textContent = formatPercent(annualized);
          }

          // Note
          let note = "Based on ";
          switch (periodEl.value) {
            case "monthly":
              note += "a one‑month period.";
              break;
            case "quarterly":
              note += "a three‑month (quarterly) period.";
              break;
            case "annual":
              note += "a 12‑month (annual) period.";
              break;
            case "custom":
              note += "a custom period.";
              break;
          }
          turnoverNoteEl.textContent = note;
        }

        calcBtn.addEventListener("click", calculate);

        resetBtn.addEventListener("click", () => {
          periodEl.value = "monthly";
          customDaysWrapper.classList.add("hidden");
          customDaysEl.value = 30;
          startHeadcountEl.value = 50;
          endHeadcountEl.value = 52;
          totalSeparationsEl.value = 3;
          voluntarySeparationsEl.value = 2;
          involuntarySeparationsEl.value = 1;
          otherSeparationsEl.value = 0;
          setMode("simple");
          turnoverRateEl.textContent = "–";
          retentionRateEl.textContent = "–";
          annualizedTurnoverEl.textContent = "–";
          voluntaryRateEl.textContent = "–";
          involuntaryRateEl.textContent = "–";
          totalSeparationsDisplayEl.textContent = "–";
          turnoverNoteEl.textContent = "";
        });

        // Initial calculation
        calculate();
      })();

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
