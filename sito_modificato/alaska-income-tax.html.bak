    <script>
      // Simple 2025 federal tax brackets approximation
      const FED_BRACKETS = {
        single: [
          { upTo: 11600, rate: 0.1 },
          { upTo: 47150, rate: 0.12 },
          { upTo: 100525, rate: 0.22 },
          { upTo: 191950, rate: 0.24 },
          { upTo: 243725, rate: 0.32 },
          { upTo: 609350, rate: 0.35 },
          { upTo: Infinity, rate: 0.37 },
        ],
        mfj: [
          { upTo: 23200, rate: 0.1 },
          { upTo: 94300, rate: 0.12 },
          { upTo: 201050, rate: 0.22 },
          { upTo: 383900, rate: 0.24 },
          { upTo: 487450, rate: 0.32 },
          { upTo: 731200, rate: 0.35 },
          { upTo: Infinity, rate: 0.37 },
        ],
        hoh: [
          { upTo: 16550, rate: 0.1 },
          { upTo: 63100, rate: 0.12 },
          { upTo: 100500, rate: 0.22 },
          { upTo: 191950, rate: 0.24 },
          { upTo: 243700, rate: 0.32 },
          { upTo: 609350, rate: 0.35 },
          { upTo: Infinity, rate: 0.37 },
        ],
      };

      const STANDARD_DEDUCTION = {
        single: 14600,
        mfj: 29200,
        hoh: 21900,
      };

      function formatCurrency(value) {
        if (!isFinite(value)) return "$0";
        return value.toLocaleString("en-US", {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 0,
        });
      }

      function formatPercent(value) {
        if (!isFinite(value)) return "0%";
        return value.toFixed(1).replace(/\.0$/, "") + "%";
      }

      function computeFederalTax(taxableIncome, filingStatus) {
        if (taxableIncome <= 0) return 0;
        const brackets = FED_BRACKETS[filingStatus] || FED_BRACKETS.single;
        let remaining = taxableIncome;
        let lastLimit = 0;
        let tax = 0;

        for (const bracket of brackets) {
          const amountInBracket = Math.min(remaining, bracket.upTo - lastLimit);
          if (amountInBracket <= 0) break;
          tax += amountInBracket * bracket.rate;
          remaining -= amountInBracket;
          lastLimit = bracket.upTo;
          if (remaining <= 0) break;
        }
        return tax;
      }

      function calculateAlaskaTax() {
        const filingStatus = document.getElementById("filing-status").value;

        const wages = parseFloat(document.getElementById("wages").value) || 0;
        const otherIncome =
          parseFloat(document.getElementById("other-income").value) || 0;
        const deductionsInput = document.getElementById("deductions").value;
        const itemizedDeductions =
          deductionsInput === "" ? null : parseFloat(deductionsInput) || 0;

        const salesRate =
          parseFloat(document.getElementById("sales-rate").value) || 0;
        const taxableSpending =
          parseFloat(document.getElementById("taxable-spending").value) || 0;
        const homeValue =
          parseFloat(document.getElementById("home-value").value) || 0;
        const propertyRate =
          parseFloat(document.getElementById("property-rate").value) || 0;

        const grossIncome = wages + otherIncome;

        const standardDeduction =
          STANDARD_DEDUCTION[filingStatus] || STANDARD_DEDUCTION.single;
        const deductionUsed =
          itemizedDeductions !== null ? itemizedDeductions : standardDeduction;

        const taxableIncome = Math.max(0, grossIncome - deductionUsed);
        const federalTax = computeFederalTax(taxableIncome, filingStatus);

        const stateTax = 0; // Alaska has no state income tax

        const salesTax = taxableSpending * (salesRate / 100);
        const propertyTax = homeValue * (propertyRate / 100);

        const totalTax = federalTax + stateTax + salesTax + propertyTax;
        const effectiveRate =
          grossIncome > 0 ? (totalTax / grossIncome) * 100 : 0;

        // Update DOM
        document.getElementById("taxable-income-output").textContent =
          formatCurrency(taxableIncome);
        document.getElementById("federal-tax-output").textContent =
          formatCurrency(federalTax);
        document.getElementById("state-tax-output").textContent =
          formatCurrency(stateTax);
        document.getElementById("sales-tax-output").textContent =
          formatCurrency(salesTax);
        document.getElementById("property-tax-output").textContent =
          formatCurrency(propertyTax);
        document.getElementById("total-tax-output").textContent =
          formatCurrency(totalTax);
        document.getElementById("effective-rate-output").textContent =
          formatPercent(effectiveRate);
      }

      document.addEventListener("DOMContentLoaded", function () {
        const btn = document.getElementById("calculate-btn");
        if (btn) {
          btn.addEventListener("click", calculateAlaskaTax);
          // Auto-run once on load
          calculateAlaskaTax();
        }

        // Simple FAQ toggle (in addition to global script_faq if present)
        document.querySelectorAll("#faq .faq-question").forEach(function (btn) {
          btn.addEventListener("click", function () {
            const answer = this.parentElement.querySelector(".faq-answer");
            const icon = this.querySelector("span:last-child");
            const isHidden = answer.classList.contains("hidden");
            document
              .querySelectorAll("#faq .faq-answer")
              .forEach((a) => a.classList.add("hidden"));
            document
              .querySelectorAll("#faq .faq-question span:last-child")
              .forEach((i) => (i.textContent = "+"));
            if (isHidden) {
              answer.classList.remove("hidden");
              icon.textContent = "−";
            }
          });
        });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
