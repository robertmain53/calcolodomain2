    <script>
      // Utility helpers
      function ftInToFeet(ft, inch) {
        ft = parseFloat(ft) || 0;
        inch = parseFloat(inch) || 0;
        return ft + inch / 12;
      }
      function roundTo(num, decimals) {
        const f = Math.pow(10, decimals);
        return Math.round(num * f) / f;
      }

      const modeEl = document.getElementById("mode");
      const unitsEl = document.getElementById("units");
      const sheetPresetEl = document.getElementById("sheetPreset");
      const calcBtn = document.getElementById("calcBtn");
      const resetBtn = document.getElementById("resetBtn");
      const errorMsg = document.getElementById("errorMsg");
      const resultsEl = document.getElementById("results");

      const areaImperialEl = document.getElementById("area-imperial");
      const areaMetricEl = document.getElementById("area-metric");
      const sheetSizeImperialEl = document.getElementById(
        "sheet-size-imperial"
      );
      const sheetSizeMetricEl = document.getElementById("sheet-size-metric");
      const roofExtraEl = document.getElementById("roof-extra");
      const heightWidthLabel = document.getElementById("heightWidthLabel");
      const heightWidthLabelIn = document.getElementById("heightWidthLabelIn");
      const heightWidthLabelMetric = document.getElementById(
        "heightWidthLabelMetric"
      );

      function syncLabels() {
        const mode = modeEl.value;
        const isWall = mode === "wall";
        const label = isWall ? "Height" : "Width";
        heightWidthLabel.textContent = label;
        heightWidthLabelIn.textContent = label;
        heightWidthLabelMetric.textContent = label;
        roofExtraEl.classList.toggle("hidden", mode !== "roof");
      }

      function syncUnits() {
        const units = unitsEl.value;
        const isImperial = units === "imperial";
        areaImperialEl.classList.toggle("hidden", !isImperial);
        areaMetricEl.classList.toggle("hidden", isImperial);
        sheetSizeImperialEl.classList.toggle("hidden", !isImperial);
        sheetSizeMetricEl.classList.toggle("hidden", isImperial);
      }

      function applySheetPreset() {
        const preset = sheetPresetEl.value;
        const units = unitsEl.value;
        if (units === "imperial") {
          const wFt = document.getElementById("sheetWidthFt");
          const lFt = document.getElementById("sheetLengthFt");
          if (preset === "4x8") {
            wFt.value = 4;
            lFt.value = 8;
          } else if (preset === "4x9") {
            wFt.value = 4;
            lFt.value = 9;
          } else if (preset === "4x10") {
            wFt.value = 4;
            lFt.value = 10;
          } else if (preset === "metric-1.2x2.4") {
            wFt.value = roundTo(1.2 * 3.28084, 3);
            lFt.value = roundTo(2.4 * 3.28084, 3);
          }
        } else {
          const wM = document.getElementById("sheetWidthM");
          const lM = document.getElementById("sheetLengthM");
          if (preset === "4x8") {
            wM.value = roundTo(4 * 0.3048, 3);
            lM.value = roundTo(8 * 0.3048, 3);
          } else if (preset === "4x9") {
            wM.value = roundTo(4 * 0.3048, 3);
            lM.value = roundTo(9 * 0.3048, 3);
          } else if (preset === "4x10") {
            wM.value = roundTo(4 * 0.3048, 3);
            lM.value = roundTo(10 * 0.3048, 3);
          } else if (preset === "metric-1.2x2.4") {
            wM.value = 1.2;
            lM.value = 2.4;
          }
        }
      }

      modeEl.addEventListener("change", () => {
        syncLabels();
      });
      unitsEl.addEventListener("change", () => {
        syncUnits();
        applySheetPreset();
      });
      sheetPresetEl.addEventListener("change", applySheetPreset);

      resetBtn.addEventListener("click", () => {
        document.getElementById("lengthFt").value = 40;
        document.getElementById("lengthIn").value = 0;
        document.getElementById("heightFt").value = 9;
        document.getElementById("heightIn").value = 0;
        document.getElementById("lengthM").value = "";
        document.getElementById("heightM").value = "";
        document.getElementById("roofRunFt").value = "";
        document.getElementById("roofRunIn").value = "";
        document.getElementById("roofRisePer12").value = 6;
        sheetPresetEl.value = "4x8";
        unitsEl.value = "imperial";
        modeEl.value = "wall";
        document.getElementById("sheetWidthFt").value = 4;
        document.getElementById("sheetLengthFt").value = 8;
        document.getElementById("sheetWidthM").value = "";
        document.getElementById("sheetLengthM").value = "";
        document.getElementById("wastePct").value = 10;
        document.getElementById("pricePerSheet").value = "";
        document.getElementById("edgeNailSpacing").value = 6;
        document.getElementById("spacing").value = "16";
        document.getElementById("orientation").value = "horizontal";
        document.getElementById("thickness").value = "";
        errorMsg.textContent = "";
        resultsEl.classList.add("hidden");
        syncUnits();
        syncLabels();
        applySheetPreset();
      });

      calcBtn.addEventListener("click", () => {
        errorMsg.textContent = "";
        const mode = modeEl.value;
        const units = unitsEl.value;

        let area; // in ft² or m² depending on units
        if (units === "imperial") {
          const L = ftInToFeet(
            document.getElementById("lengthFt").value,
            document.getElementById("lengthIn").value
          );
          const H = ftInToFeet(
            document.getElementById("heightFt").value,
            document.getElementById("heightIn").value
          );
          if (L <= 0 || H <= 0) {
            errorMsg.textContent = "Enter positive length and height/width.";
            resultsEl.classList.add("hidden");
            return;
          }
          let flatArea = L * H;
          if (mode === "roof") {
            const run = ftInToFeet(
              document.getElementById("roofRunFt").value,
              document.getElementById("roofRunIn").value
            );
            const risePer12 = parseFloat(
              document.getElementById("roofRisePer12").value
            );
            if (run > 0 && risePer12 >= 0) {
              const slope = risePer12 / 12;
              const factor = Math.sqrt(1 + slope * slope);
              flatArea = L * run;
              area = flatArea * factor;
            } else {
              area = flatArea;
            }
          } else {
            area = flatArea;
          }
        } else {
          const L = parseFloat(document.getElementById("lengthM").value);
          const H = parseFloat(document.getElementById("heightM").value);
          if (!L || !H || L <= 0 || H <= 0) {
            errorMsg.textContent = "Enter positive length and height/width.";
            resultsEl.classList.add("hidden");
            return;
          }
          area = L * H;
        }

        // Sheet size
        let sheetWidth, sheetLength;
        if (units === "imperial") {
          sheetWidth = parseFloat(
            document.getElementById("sheetWidthFt").value
          );
          sheetLength = parseFloat(
            document.getElementById("sheetLengthFt").value
          );
        } else {
          sheetWidth = parseFloat(document.getElementById("sheetWidthM").value);
          sheetLength = parseFloat(
            document.getElementById("sheetLengthM").value
          );
        }
        if (
          !sheetWidth ||
          !sheetLength ||
          sheetWidth <= 0 ||
          sheetLength <= 0
        ) {
          errorMsg.textContent = "Enter a valid sheet width and length.";
          resultsEl.classList.add("hidden");
          return;
        }

        const sheetArea = sheetWidth * sheetLength;
        const exactSheets = area / sheetArea;
        const sheetsRounded = Math.ceil(exactSheets);

        let wastePct = parseFloat(document.getElementById("wastePct").value);
        if (isNaN(wastePct) || wastePct < 0) wastePct = 0;
        const sheetsWithWaste = Math.ceil(sheetsRounded * (1 + wastePct / 100));

        const pricePerSheet = parseFloat(
          document.getElementById("pricePerSheet").value
        );
        const cost = pricePerSheet > 0 ? sheetsWithWaste * pricePerSheet : null;

        // Fastener estimate (imperial only, approximate)
        let fastenersPerSheet = 0;
        let fastenerRows = 0;
        const spacingIn = parseFloat(document.getElementById("spacing").value);
        const edgeSpacingIn = parseFloat(
          document.getElementById("edgeNailSpacing").value
        );
        if (
          units === "imperial" &&
          sheetWidth > 0 &&
          sheetLength > 0 &&
          edgeSpacingIn > 0
        ) {
          const widthIn = sheetWidth * 12;
          const lengthIn = sheetLength * 12;

          function countAlong(lengthInches, spacing) {
            if (!spacing || spacing <= 0) return 0;
            return Math.max(2, Math.floor(lengthInches / spacing) + 1);
          }

          const edgeLong = countAlong(lengthIn, edgeSpacingIn);
          const edgeShort = countAlong(widthIn, edgeSpacingIn);
          const edgeFasteners = 2 * (edgeLong + edgeShort);

          let fieldFasteners = 0;
          if (spacingIn && spacingIn > 0) {
            const rows = Math.max(0, Math.floor(widthIn / spacingIn) - 1);
            fastenerRows = rows;
            const perRow = countAlong(lengthIn, edgeSpacingIn * 2); // assume looser spacing in field
            fieldFasteners = rows * perRow;
          }

          fastenersPerSheet = edgeFasteners + fieldFasteners;
        }

        const totalFasteners = fastenersPerSheet
          ? Math.ceil(fastenersPerSheet * sheetsWithWaste)
          : 0;

        // Populate results
        const unitAreaLabel = units === "imperial" ? "ft²" : "m²";
        document.getElementById("resArea").textContent = `${roundTo(
          area,
          2
        )} ${unitAreaLabel}`;
        document.getElementById("resSheetArea").textContent = `${roundTo(
          sheetArea,
          2
        )} ${unitAreaLabel}`;
        document.getElementById("resExactSheets").textContent = roundTo(
          exactSheets,
          2
        );
        document.getElementById("resSheetsRounded").textContent = sheetsRounded;
        document.getElementById(
          "resSheetsWithWaste"
        ).textContent = `${sheetsWithWaste} (incl. waste)`;
        document.getElementById("resWastePct").textContent = `${roundTo(
          wastePct,
          1
        )}%`;

        document.getElementById("resCost").textContent =
          cost !== null
            ? `$${roundTo(cost, 2).toLocaleString()}`
            : "— (enter price per sheet)";
        document.getElementById("resFastenersPerSheet").textContent =
          fastenersPerSheet ? `${Math.ceil(fastenersPerSheet)} (approx.)` : "—";
        document.getElementById("resFastenersTotal").textContent =
          totalFasteners ? `${totalFasteners.toLocaleString()} (approx.)` : "—";
        document.getElementById("resFastenerRows").textContent = fastenerRows
          ? `${fastenerRows} interior rows (approx.)`
          : "—";

        const orientation = document.getElementById("orientation").value;
        const orientationText =
          orientation === "horizontal"
            ? "Install sheets with the long edge horizontal and stagger vertical joints where possible."
            : "Install sheets with the long edge vertical and align edges over framing.";
        const modeText =
          mode === "wall" ? "wall" : mode === "roof" ? "roof" : "floor/deck";
        document.getElementById("resLayout").textContent =
          `You will need approximately ${sheetsWithWaste} full sheets to sheath this ${modeText}. ` +
          `${orientationText} Trim sheets at corners and openings as required, and always follow local building codes for panel spacing and nailing patterns.`;

        resultsEl.classList.remove("hidden");
      });

      // Initial setup
      syncUnits();
      syncLabels();
      applySheetPreset();

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
