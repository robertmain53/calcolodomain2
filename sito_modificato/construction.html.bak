    <script>
      // Tab handling
      document.querySelectorAll(".tab-btn").forEach((btn) => {
        btn.addEventListener("click", () => {
          const target = btn.getAttribute("data-tab");
          document.querySelectorAll(".tab-btn").forEach((b) => {
            b.classList.remove("border-blue-600", "text-blue-600");
            b.classList.add("border-transparent", "text-gray-600");
          });
          btn.classList.add("border-blue-600", "text-blue-600");
          btn.classList.remove("border-transparent", "text-gray-600");

          document.querySelectorAll(".tab-panel").forEach((panel) => {
            panel.classList.add("hidden");
          });
          document.getElementById("tab-" + target).classList.remove("hidden");
        });
      });

      // Shared summary state
      const summaryState = {
        concreteCost: 0,
        lumberCost: 0,
        roofCost: 0,
        excavationCost: 0,
      };

      function updateSummary() {
        const fmt = (v) => "$" + v.toFixed(2);
        document.getElementById("summary-concrete-cost").textContent = fmt(
          summaryState.concreteCost
        );
        document.getElementById("summary-lumber-cost").textContent = fmt(
          summaryState.lumberCost
        );
        document.getElementById("summary-roof-cost").textContent = fmt(
          summaryState.roofCost
        );
        document.getElementById("summary-excavation-cost").textContent = fmt(
          summaryState.excavationCost
        );
        const total =
          summaryState.concreteCost +
          summaryState.lumberCost +
          summaryState.roofCost +
          summaryState.excavationCost;
        document.getElementById("summary-total-cost").textContent = fmt(total);
      }

      // Concrete logic
      const concreteUnitEl = document.getElementById("concrete-unit");
      const concreteLengthUnitEl = document.getElementById(
        "concrete-length-unit"
      );
      const concreteWidthUnitEl = document.getElementById(
        "concrete-width-unit"
      );
      const concreteThicknessUnitEl = document.getElementById(
        "concrete-thickness-unit"
      );
      const concretePriceUnitLabel = document.getElementById(
        "concrete-price-unit-label"
      );

      function updateConcreteUnits() {
        if (concreteUnitEl.value === "imperial") {
          concreteLengthUnitEl.textContent = "ft";
          concreteWidthUnitEl.textContent = "ft";
          concreteThicknessUnitEl.textContent = "ft";
          concretePriceUnitLabel.textContent = "per cubic yard";
        } else {
          concreteLengthUnitEl.textContent = "m";
          concreteWidthUnitEl.textContent = "m";
          concreteThicknessUnitEl.textContent = "m";
          concretePriceUnitLabel.textContent = "per cubic meter";
        }
      }
      concreteUnitEl.addEventListener("change", updateConcreteUnits);
      updateConcreteUnits();

      document.getElementById("concrete-calc").addEventListener("click", () => {
        const L =
          parseFloat(document.getElementById("concrete-length").value) || 0;
        const W =
          parseFloat(document.getElementById("concrete-width").value) || 0;
        const T =
          parseFloat(document.getElementById("concrete-thickness").value) || 0;
        const waste = Math.max(
          0,
          parseFloat(document.getElementById("concrete-waste").value) || 0
        );
        const price = Math.max(
          0,
          parseFloat(document.getElementById("concrete-price").value) || 0
        );

        let volNet = L * W * T;
        let volUnit, volNetDisplay, volGrossDisplay;
        if (concreteUnitEl.value === "imperial") {
          const yd3Net = volNet / 27;
          const yd3Gross = yd3Net * (1 + waste / 100);
          volNetDisplay = yd3Net.toFixed(2) + " yd³";
          volGrossDisplay = yd3Gross.toFixed(2) + " yd³";
          summaryState.concreteCost = yd3Gross * price;
        } else {
          const m3Net = volNet;
          const m3Gross = m3Net * (1 + waste / 100);
          volNetDisplay = m3Net.toFixed(2) + " m³";
          volGrossDisplay = m3Gross.toFixed(2) + " m³";
          summaryState.concreteCost = m3Gross * price;
        }

        document.getElementById("concrete-volume-net").textContent =
          volNetDisplay;
        document.getElementById("concrete-volume-gross").textContent =
          volGrossDisplay;
        document.getElementById("concrete-cost").textContent =
          "$" + summaryState.concreteCost.toFixed(2);
        document.getElementById("concrete-results").classList.remove("hidden");
        updateSummary();
      });

      // Lumber logic
      document.getElementById("lumber-calc").addEventListener("click", () => {
        const t =
          parseFloat(document.getElementById("lumber-thickness").value) || 0;
        const w =
          parseFloat(document.getElementById("lumber-width").value) || 0;
        const L =
          parseFloat(document.getElementById("lumber-length").value) || 0;
        const qty = Math.max(
          0,
          parseInt(document.getElementById("lumber-qty").value, 10) || 0
        );
        const priceBF = Math.max(
          0,
          parseFloat(document.getElementById("lumber-price").value) || 0
        );

        const bfEach = (t * w * L) / 12;
        const bfTotal = bfEach * qty;
        const cost = bfTotal * priceBF;

        document.getElementById("lumber-bf-each").textContent =
          bfEach.toFixed(2) + " bf";
        document.getElementById("lumber-bf-total").textContent =
          bfTotal.toFixed(2) + " bf";
        document.getElementById("lumber-cost").textContent =
          "$" + cost.toFixed(2);
        document.getElementById("lumber-results").classList.remove("hidden");

        summaryState.lumberCost = cost;
        updateSummary();
      });

      // Roofing logic
      document.getElementById("roof-calc").addEventListener("click", () => {
        const L = parseFloat(document.getElementById("roof-length").value) || 0;
        const W = parseFloat(document.getElementById("roof-width").value) || 0;
        const pitchRise =
          parseFloat(document.getElementById("roof-pitch").value) || 0;
        const waste = Math.max(
          0,
          parseFloat(document.getElementById("roof-waste").value) || 0
        );
        const priceSq = Math.max(
          0,
          parseFloat(document.getElementById("roof-price").value) || 0
        );

        const planArea = L * W; // ft²
        const pitchFactor = Math.sqrt(1 + Math.pow(pitchRise / 12, 2));
        const roofAreaNet = planArea * pitchFactor;
        const roofAreaGross = roofAreaNet * (1 + waste / 100);
        const squares = roofAreaGross / 100;
        const cost = squares * priceSq;

        document.getElementById("roof-plan-area").textContent =
          planArea.toFixed(1) + " ft²";
        document.getElementById("roof-area-net").textContent =
          roofAreaNet.toFixed(1) + " ft²";
        document.getElementById("roof-area-gross").textContent =
          roofAreaGross.toFixed(1) + " ft²";
        document.getElementById("roof-squares").textContent =
          squares.toFixed(2) + " squares";
        document.getElementById("roof-cost").textContent =
          "$" + cost.toFixed(2);
        document.getElementById("roof-results").classList.remove("hidden");

        summaryState.roofCost = cost;
        updateSummary();
      });

      // Excavation logic
      const excavationUnitEl = document.getElementById("excavation-unit");
      const excavationLengthUnitEl = document.getElementById(
        "excavation-length-unit"
      );
      const excavationWidthUnitEl = document.getElementById(
        "excavation-width-unit"
      );
      const excavationDepthUnitEl = document.getElementById(
        "excavation-depth-unit"
      );
      const excavationPriceUnitLabel = document.getElementById(
        "excavation-price-unit-label"
      );

      function updateExcavationUnits() {
        if (excavationUnitEl.value === "imperial") {
          excavationLengthUnitEl.textContent = "ft";
          excavationWidthUnitEl.textContent = "ft";
          excavationDepthUnitEl.textContent = "ft";
          excavationPriceUnitLabel.textContent = "per cubic yard (loose)";
        } else {
          excavationLengthUnitEl.textContent = "m";
          excavationWidthUnitEl.textContent = "m";
          excavationDepthUnitEl.textContent = "m";
          excavationPriceUnitLabel.textContent = "per cubic meter (loose)";
        }
      }
      excavationUnitEl.addEventListener("change", updateExcavationUnits);
      updateExcavationUnits();

      document
        .getElementById("excavation-calc")
        .addEventListener("click", () => {
          const L =
            parseFloat(document.getElementById("excavation-length").value) || 0;
          const W =
            parseFloat(document.getElementById("excavation-width").value) || 0;
          const D =
            parseFloat(document.getElementById("excavation-depth").value) || 0;
          const swell = Math.max(
            0,
            parseFloat(document.getElementById("excavation-swell").value) || 0
          );
          const price = Math.max(
            0,
            parseFloat(document.getElementById("excavation-price").value) || 0
          );

          let volNet = L * W * D;
          let volNetDisplay, volLooseDisplay, cost;

          if (excavationUnitEl.value === "imperial") {
            const yd3Net = volNet / 27;
            const yd3Loose = yd3Net * (1 + swell / 100);
            volNetDisplay = yd3Net.toFixed(2) + " yd³";
            volLooseDisplay = yd3Loose.toFixed(2) + " yd³";
            cost = yd3Loose * price;
          } else {
            const m3Net = volNet;
            const m3Loose = m3Net * (1 + swell / 100);
            volNetDisplay = m3Net.toFixed(2) + " m³";
            volLooseDisplay = m3Loose.toFixed(2) + " m³";
            cost = m3Loose * price;
          }

          document.getElementById("excavation-volume-net").textContent =
            volNetDisplay;
          document.getElementById("excavation-volume-loose").textContent =
            volLooseDisplay;
          document.getElementById("excavation-cost").textContent =
            "$" + cost.toFixed(2);
          document
            .getElementById("excavation-results")
            .classList.remove("hidden");

          summaryState.excavationCost = cost;
          updateSummary();
        });

      // Initialize summary
      updateSummary();

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
