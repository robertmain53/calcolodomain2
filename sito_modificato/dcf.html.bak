    <script>
      // Utility: format numbers as currency with symbol
      function formatCurrency(value, symbol) {
        if (!isFinite(value)) return "–";
        const abs = Math.abs(value);
        let formatted = abs.toLocaleString(undefined, {
          maximumFractionDigits: 2,
          minimumFractionDigits: 2,
        });
        if (value < 0) formatted = "-" + symbol + formatted;
        else formatted = symbol + formatted;
        return formatted;
      }

      function formatNumber(value) {
        if (!isFinite(value)) return "–";
        return value.toLocaleString(undefined, {
          maximumFractionDigits: 2,
          minimumFractionDigits: 2,
        });
      }

      const projectionBody = document.getElementById("projection-body");
      const projectionYearsSelect = document.getElementById("projection-years");

      function buildProjectionRows() {
        const years = parseInt(projectionYearsSelect.value, 10);
        projectionBody.innerHTML = "";
        const baseRevenue = 1000000; // 1M
        const growth = 0.05; // 5% default growth
        for (let i = 1; i <= years; i++) {
          const rev = baseRevenue * Math.pow(1 + growth, i - 1);
          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="border-b border-gray-100 text-xs text-gray-600 px-2 py-1">Year ${i}</td>
        <td class="border-b border-gray-100 px-2 py-1 text-right">
          <input type="number" step="0.01" class="border border-gray-200 rounded px-1 py-0.5 text-right text-xs w-28" value="${rev.toFixed(
            2
          )}">
        </td>
        <td class="border-b border-gray-100 px-2 py-1 text-right">
          <input type="number" step="0.01" class="border border-gray-200 rounded px-1 py-0.5 text-right text-xs w-20" value="15">
        </td>
        <td class="border-b border-gray-100 px-2 py-1 text-right">
          <input type="number" step="0.01" class="border border-gray-200 rounded px-1 py-0.5 text-right text-xs w-20" value="25">
        </td>
        <td class="border-b border-gray-100 px-2 py-1 text-right">
          <input type="number" step="0.01" class="border border-gray-200 rounded px-1 py-0.5 text-right text-xs w-24" value="4">
        </td>
        <td class="border-b border-gray-100 px-2 py-1 text-right">
          <input type="number" step="0.01" class="border border-gray-200 rounded px-1 py-0.5 text-right text-xs w-24" value="5">
        </td>
        <td class="border-b border-gray-100 px-2 py-1 text-right">
          <input type="number" step="0.01" class="border border-gray-200 rounded px-1 py-0.5 text-right text-xs w-24" value="1">
        </td>
      `;
          projectionBody.appendChild(tr);
        }
      }

      projectionYearsSelect.addEventListener("change", buildProjectionRows);
      buildProjectionRows();

      document
        .getElementById("btn-reset")
        .addEventListener("click", function () {
          document.getElementById("currency").value = "$";
          document.getElementById("wacc").value = 8;
          document.getElementById("terminal-growth").value = 2;
          document.getElementById("net-debt").value = 0;
          document.getElementById("shares-out").value = 1000000;
          projectionYearsSelect.value = 5;
          buildProjectionRows();
          document.getElementById("pv-fcff").textContent = "–";
          document.getElementById("pv-tv").textContent = "–";
          document.getElementById("ev").textContent = "–";
          document.getElementById("equity-value").textContent = "–";
          document.getElementById("fair-value").textContent = "–";
          document.getElementById("assump-years").textContent = "–";
          document.getElementById("assump-wacc").textContent = "–";
          document.getElementById("assump-g").textContent = "–";
          document.getElementById("assump-net-debt").textContent = "–";
          document.getElementById("assump-shares").textContent = "–";
          document
            .getElementById("table-results-wrapper")
            .classList.add("hidden");
        });

      document
        .getElementById("btn-calc")
        .addEventListener("click", function () {
          const currency = document.getElementById("currency").value || "$";
          const waccPct = parseFloat(document.getElementById("wacc").value);
          const gPct = parseFloat(
            document.getElementById("terminal-growth").value
          );
          const netDebt =
            parseFloat(document.getElementById("net-debt").value) || 0;
          const shares =
            parseFloat(document.getElementById("shares-out").value) || 0;

          const wacc = (waccPct || 0) / 100;
          const g = (gPct || 0) / 100;

          const rows = Array.from(projectionBody.querySelectorAll("tr"));
          const fcffArr = [];
          const revArr = [];
          const pvArr = [];
          const dfArr = [];

          let totalPvFcff = 0;

          rows.forEach((row, idx) => {
            const inputs = row.querySelectorAll("input");
            const revenue = parseFloat(inputs[0].value) || 0;
            const ebitMargin = (parseFloat(inputs[1].value) || 0) / 100;
            const taxRate = (parseFloat(inputs[2].value) || 0) / 100;
            const depPct = (parseFloat(inputs[3].value) || 0) / 100;
            const capexPct = (parseFloat(inputs[4].value) || 0) / 100;
            const wcPct = (parseFloat(inputs[5].value) || 0) / 100;

            const ebit = revenue * ebitMargin;
            const nopat = ebit * (1 - taxRate);
            const dep = revenue * depPct;
            const capex = revenue * capexPct;
            const deltaWc = revenue * wcPct;

            const fcff = nopat + dep - capex - deltaWc;
            const t = idx + 1;
            const df = 1 / Math.pow(1 + wacc, t);
            const pv = fcff * df;

            revArr.push(revenue);
            fcffArr.push(fcff);
            dfArr.push(df);
            pvArr.push(pv);
            totalPvFcff += pv;
          });

          const n = rows.length;
          const lastFcff = fcffArr[n - 1] || 0;
          const fcffNext = lastFcff * (1 + g);
          let tv = 0;
          let pvTv = 0;
          if (wacc > g) {
            tv = fcffNext / (wacc - g);
            const dfTv = 1 / Math.pow(1 + wacc, n);
            pvTv = tv * dfTv;
          }

          const ev = totalPvFcff + pvTv;
          const equityValue = ev - netDebt;
          const fairValuePerShare = shares > 0 ? equityValue / shares : NaN;

          document.getElementById("pv-fcff").textContent = formatCurrency(
            totalPvFcff,
            currency
          );
          document.getElementById("pv-tv").textContent = formatCurrency(
            pvTv,
            currency
          );
          document.getElementById("ev").textContent = formatCurrency(
            ev,
            currency
          );
          document.getElementById("equity-value").textContent = formatCurrency(
            equityValue,
            currency
          );
          document.getElementById("fair-value").textContent = isFinite(
            fairValuePerShare
          )
            ? formatCurrency(fairValuePerShare, currency)
            : "–";

          document.getElementById("assump-years").textContent = n.toString();
          document.getElementById("assump-wacc").textContent =
            (waccPct || 0).toFixed(2) + " %";
          document.getElementById("assump-g").textContent =
            (gPct || 0).toFixed(2) + " %";
          document.getElementById("assump-net-debt").textContent =
            formatCurrency(netDebt, currency);
          document.getElementById("assump-shares").textContent = shares
            ? shares.toLocaleString()
            : "–";

          const tbody = document.getElementById("table-results-body");
          tbody.innerHTML = "";
          revArr.forEach((rev, idx) => {
            const tr = document.createElement("tr");
            tr.innerHTML = `
        <td class="border-b border-gray-100 px-2 py-1 text-gray-600">Year ${
          idx + 1
        }</td>
        <td class="border-b border-gray-100 px-2 py-1 text-right">${formatCurrency(
          rev,
          currency
        )}</td>
        <td class="border-b border-gray-100 px-2 py-1 text-right">${formatCurrency(
          fcffArr[idx],
          currency
        )}</td>
        <td class="border-b border-gray-100 px-2 py-1 text-right">${formatNumber(
          dfArr[idx]
        )}</td>
        <td class="border-b border-gray-100 px-2 py-1 text-right">${formatCurrency(
          pvArr[idx],
          currency
        )}</td>
      `;
            tbody.appendChild(tr);
          });
          document.getElementById("table-total-pv").textContent =
            formatCurrency(totalPvFcff, currency);
          document
            .getElementById("table-results-wrapper")
            .classList.remove("hidden");
        });

      // Mobile menu script (simple)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          this.setAttribute("aria-expanded", isHidden ? "true" : "false");
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
