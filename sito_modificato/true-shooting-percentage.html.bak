    <script>
      // Mobile menu toggle
      document
        .getElementById("mobile-menu-toggle")
        ?.addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          if (!menu) return;
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          this.setAttribute("aria-expanded", String(isHidden));
        });

      (function () {
        const modeButtons = document.querySelectorAll(".mode-btn");
        const basicInputs = document.getElementById("basic-inputs");
        const detailedInputs = document.getElementById("detailed-inputs");
        const errorMsg = document.getElementById("error-msg");
        const tsOutput = document.getElementById("ts-output");
        const tsEffLabel = document.getElementById("ts-eff-label");
        const ppsOutput = document.getElementById("pps-output");
        const possOutput = document.getElementById("poss-output");
        const calcBtn = document.getElementById("calc-btn");
        const resetBtn = document.getElementById("reset-btn");

        let currentMode = "basic";

        function setMode(mode) {
          currentMode = mode;
          modeButtons.forEach((btn) => {
            if (btn.dataset.mode === mode) {
              btn.classList.remove("bg-blue-100", "text-blue-700");
              btn.classList.add("bg-blue-600", "text-white");
            } else {
              btn.classList.remove("bg-blue-600", "text-white");
              btn.classList.add("bg-blue-100", "text-blue-700");
            }
          });
          if (mode === "basic") {
            basicInputs.classList.remove("hidden");
            detailedInputs.classList.add("hidden");
          } else {
            basicInputs.classList.add("hidden");
            detailedInputs.classList.remove("hidden");
          }
          clearError();
        }

        function showError(msg) {
          if (!errorMsg) return;
          errorMsg.textContent = msg;
          errorMsg.classList.remove("hidden");
        }

        function clearError() {
          if (!errorMsg) return;
          errorMsg.textContent = "";
          errorMsg.classList.add("hidden");
        }

        function classifyTS(ts) {
          if (isNaN(ts)) return "";
          if (ts < 0.52) return "Inefficient scoring";
          if (ts < 0.57) return "Around league average efficiency";
          if (ts < 0.62) return "Efficient scorer";
          return "Elite scoring efficiency";
        }

        function formatPercent(value) {
          if (!isFinite(value)) return "–";
          return (value * 100).toFixed(1) + "%";
        }

        function calculateTS() {
          clearError();
          let pts, fga, fta;

          if (currentMode === "basic") {
            pts = parseFloat(document.getElementById("pts").value);
            fga = parseFloat(document.getElementById("fga").value);
            fta = parseFloat(document.getElementById("fta").value);
          } else {
            const fgm = parseFloat(document.getElementById("fgm").value);
            const fga_d = parseFloat(document.getElementById("fga_d").value);
            const three_pm = parseFloat(
              document.getElementById("three_pm").value
            );
            const ftm = parseFloat(document.getElementById("ftm").value);
            const fta_d = parseFloat(document.getElementById("fta_d").value);

            if (
              [fgm, fga_d, three_pm, ftm, fta_d].some((v) => isNaN(v) || v < 0)
            ) {
              showError(
                "Please enter non-negative numbers for all detailed stats."
              );
              return;
            }
            if (three_pm > fgm) {
              showError("3P made cannot exceed total field goals made.");
              return;
            }
            if (fgm > fga_d) {
              showError("FG made cannot exceed FG attempts.");
              return;
            }
            if (ftm > fta_d) {
              showError("FT made cannot exceed FT attempts.");
              return;
            }

            pts = 2 * (fgm - three_pm) + 3 * three_pm + ftm;
            fga = fga_d;
            fta = fta_d;
          }

          if ([pts, fga, fta].some((v) => isNaN(v) || v < 0)) {
            showError(
              "Please enter non-negative numbers for points, FGA, and FTA."
            );
            return;
          }

          const shootingPoss = fga + 0.44 * fta;
          if (shootingPoss <= 0) {
            showError(
              "Shooting possessions must be greater than zero (check FGA and FTA)."
            );
            return;
          }

          const ts = pts / (2 * shootingPoss);
          const pps = pts / shootingPoss;

          tsOutput.textContent = formatPercent(ts);
          tsEffLabel.textContent = classifyTS(ts);
          ppsOutput.textContent = pps.toFixed(3);
          possOutput.textContent = shootingPoss.toFixed(2);
        }

        function resetForm() {
          if (currentMode === "basic") {
            document.getElementById("pts").value = "";
            document.getElementById("fga").value = "";
            document.getElementById("fta").value = "";
          } else {
            ["fgm", "fga_d", "three_pm", "ftm", "fta_d"].forEach((id) => {
              const el = document.getElementById(id);
              if (el) el.value = "";
            });
          }
          tsOutput.textContent = "–";
          tsEffLabel.textContent = "Enter stats to see TS%";
          ppsOutput.textContent = "–";
          possOutput.textContent = "–";
          clearError();
        }

        modeButtons.forEach((btn) => {
          btn.addEventListener("click", () => setMode(btn.dataset.mode));
        });

        calcBtn?.addEventListener("click", calculateTS);
        resetBtn?.addEventListener("click", resetForm);

        // Initial calculation with default example values
        calculateTS();
      })();
    </script>

