    <script>
        (function() {
            'use strict';

            const xInput = document.getElementById('xInput');
            const yInput = document.getElementById('yInput');
            const zInput = document.getElementById('zInput');
            const rhoResult = document.getElementById('rhoResult');
            const phiResult = document.getElementById('phiResult');
            const zResult = document.getElementById('zResult');

            function convertCartesianToCylindrical() {
                const x = parseFloat(xInput.value) || 0;
                const y = parseFloat(yInput.value) || 0;
                const z = parseFloat(zInput.value) || 0;

                // Calculate rho (radial distance)
                const rho = Math.sqrt(x * x + y * y);

                // Calculate phi (angle in degrees)
                const phi = Math.atan2(y, x) * (180 / Math.PI);

                // Update results
                rhoResult.textContent = rho.toFixed(2);
                phiResult.textContent = phi.toFixed(2) + '°';
                zResult.textContent = z.toFixed(2);
            }

            // Add event listeners for instant conversion
            xInput.addEventListener('input', convertCartesianToCylindrical);
            yInput.addEventListener('input', convertCartesianToCylindrical);
            zInput.addEventListener('input', convertCartesianToCylindrical);

            // Mobile menu toggle
            const mobileToggle = document.getElementById('mobile-menu-toggle');
            const mobileMenu = document.getElementById('mobile-menu');
            if (mobileToggle && mobileMenu) {
                mobileToggle.addEventListener('click', () => {
                    mobileMenu.classList.toggle('hidden');
                });
            }

            // Initial calculation
            convertCartesianToCylindrical();
        })();
    </script>
