    <script>
      // --- Data model ---
      const state = {
        unit: "ft",
        room: { length: 16, width: 12 },
        scale: 2, // ft or m per grid square
        zoom: 1,
        furniture: [],
        selectedId: null,
      };

      const defaultFurniturePresets = {
        sofa: { label: "Sofa", lengthFt: 7, widthFt: 3, color: "#bfdbfe" },
        loveseat: {
          label: "Loveseat",
          lengthFt: 5,
          widthFt: 3,
          color: "#bfdbfe",
        },
        armchair: {
          label: "Armchair",
          lengthFt: 3,
          widthFt: 3,
          color: "#bfdbfe",
        },
        coffee_table: {
          label: "Coffee table",
          lengthFt: 4,
          widthFt: 2,
          color: "#bbf7d0",
        },
        tv_stand: {
          label: "TV stand",
          lengthFt: 5,
          widthFt: 1.5,
          color: "#bbf7d0",
        },
        dining_table: {
          label: "Dining table",
          lengthFt: 6,
          widthFt: 3,
          color: "#fef3c7",
        },
        bed_queen: {
          label: "Queen bed",
          lengthFt: 6.67,
          widthFt: 5,
          color: "#e9d5ff",
        },
        bed_king: {
          label: "King bed",
          lengthFt: 6.67,
          widthFt: 6.33,
          color: "#e9d5ff",
        },
        desk: { label: "Desk", lengthFt: 4, widthFt: 2, color: "#bae6fd" },
        wardrobe: {
          label: "Wardrobe",
          lengthFt: 5,
          widthFt: 2,
          color: "#e5e7eb",
        },
      };

      let canvasEl, selectionPanel, noSelectionLabel;
      let itemLabelInput,
        itemLengthInput,
        itemWidthInput,
        itemRotateInput,
        itemElevationInput;
      let roomLengthInput, roomWidthInput, unitSelect, scaleLabel;
      let clearanceSummaryEl;
      let layoutJsonEl;

      // Utility
      function generateId() {
        return "f_" + Math.random().toString(36).slice(2, 9);
      }

      function unitFactor() {
        // internal model uses feet; convert if meters selected
        return state.unit === "ft" ? 1 : 3.28084;
      }

      function toInternalFeet(valueInUnit) {
        return valueInUnit * unitFactor();
      }

      function fromInternalFeet(valueInFeet) {
        return valueInFeet / unitFactor();
      }

      // Room & scale
      function updateScale() {
        // Aim for ~10–14 grid squares across the longer side
        const maxDimFt = Math.max(state.room.length, state.room.width);
        const targetSquares = 12;
        const approxScale = maxDimFt / targetSquares;
        // Round scale to a "nice" value
        const niceScales = [1, 2, 3, 4, 5];
        let best = niceScales[0];
        let bestDiff = Math.abs(niceScales[0] - approxScale);
        for (const s of niceScales) {
          const d = Math.abs(s - approxScale);
          if (d < bestDiff) {
            bestDiff = d;
            best = s;
          }
        }
        state.scale = best;
        const unitLabel = state.unit === "ft" ? "ft" : "m";
        const scaleVal =
          state.unit === "ft" ? state.scale : state.scale / 3.28084;
        scaleLabel.textContent = `1 square = ${scaleVal.toFixed(
          1
        )} ${unitLabel}`;
      }

      function updateRoomFromInputs() {
        const len = parseFloat(roomLengthInput.value) || 0;
        const wid = parseFloat(roomWidthInput.value) || 0;
        state.room.length = toInternalFeet(len);
        state.room.width = toInternalFeet(wid);
        updateScale();
        renderCanvas();
      }

      // Canvas rendering
      function renderCanvas() {
        if (!canvasEl) return;
        const wrapper = document.getElementById("room-canvas-wrapper");
        const w = wrapper.clientWidth;
        const h = wrapper.clientHeight;
        const gridSizePx = 40 * state.zoom;
        const roomWidthPx = (state.room.length / state.scale) * gridSizePx;
        const roomHeightPx = (state.room.width / state.scale) * gridSizePx;

        canvasEl.style.width = roomWidthPx + "px";
        canvasEl.style.height = roomHeightPx + "px";
        canvasEl.style.transform = `scale(${1})`;

        // Clear existing furniture DOM
        canvasEl.innerHTML = "";

        // Draw room border
        const border = document.createElement("div");
        border.className =
          "absolute inset-0 border border-gray-400 pointer-events-none";
        canvasEl.appendChild(border);

        // Draw furniture
        state.furniture.forEach((item) => {
          const el = document.createElement("div");
          el.className =
            "absolute bg-white shadow-sm border border-gray-400 text-[10px] flex items-center justify-center select-none";
          el.dataset.id = item.id;

          const lengthPx = (item.length / state.scale) * gridSizePx;
          const widthPx = (item.width / state.scale) * gridSizePx;

          el.style.width = lengthPx + "px";
          el.style.height = widthPx + "px";
          el.style.backgroundColor = item.color || "#e5e7eb";

          const centerX = (item.x / state.scale) * gridSizePx;
          const centerY = (item.y / state.scale) * gridSizePx;

          el.style.left = centerX - lengthPx / 2 + "px";
          el.style.top = centerY - widthPx / 2 + "px";
          el.style.transformOrigin = "50% 50%";
          el.style.transform = `rotate(${item.rotation}deg)`;

          if (state.selectedId === item.id) {
            el.classList.add("furniture-selected");
            // rotation handle
            const handle = document.createElement("div");
            handle.className = "handle-rotate";
            handle.dataset.id = item.id;
            el.appendChild(handle);
          }

          const labelSpan = document.createElement("span");
          labelSpan.textContent = item.label || "Item";
          labelSpan.style.pointerEvents = "none";
          el.appendChild(labelSpan);

          canvasEl.appendChild(el);
        });
      }

      // Add furniture
      function addFurniture(type) {
        const preset = defaultFurniturePresets[type];
        if (!preset) return;
        const id = generateId();
        const lengthFt = preset.lengthFt;
        const widthFt = preset.widthFt;
        const item = {
          id,
          type,
          label: preset.label,
          length: lengthFt,
          width: widthFt,
          x: state.room.length / 2,
          y: state.room.width / 2,
          rotation: 0,
          color: preset.color,
          meta: {},
        };
        state.furniture.push(item);
        state.selectedId = id;
        syncSelectionPanel();
        updateClearanceSummary();
        renderCanvas();
      }

      // Selection & properties
      function getSelectedItem() {
        return state.furniture.find((f) => f.id === state.selectedId) || null;
      }

      function syncSelectionPanel() {
        const item = getSelectedItem();
        if (!item) {
          selectionPanel.classList.add("hidden");
          noSelectionLabel.classList.remove("hidden");
          return;
        }
        selectionPanel.classList.remove("hidden");
        noSelectionLabel.classList.add("hidden");

        itemLabelInput.value = item.label || "";
        itemLengthInput.value = fromInternalFeet(item.length).toFixed(2);
        itemWidthInput.value = fromInternalFeet(item.width).toFixed(2);
        itemRotateInput.value = item.rotation.toFixed(0);
        itemElevationInput.value =
          item.meta.fromWall != null
            ? fromInternalFeet(item.meta.fromWall).toFixed(2)
            : "";
      }

      function updateItemFromInputs() {
        const item = getSelectedItem();
        if (!item) return;
        item.label = itemLabelInput.value || item.label;
        const len = parseFloat(itemLengthInput.value);
        const wid = parseFloat(itemWidthInput.value);
        if (len > 0) item.length = toInternalFeet(len);
        if (wid > 0) item.width = toInternalFeet(wid);
        const rot = parseFloat(itemRotateInput.value);
        if (!isNaN(rot)) item.rotation = rot;
        const elev = parseFloat(itemElevationInput.value);
        if (!isNaN(elev)) item.meta.fromWall = toInternalFeet(elev);
        updateClearanceSummary();
        renderCanvas();
      }

      // Drag & rotate
      let dragState = null;
      let rotateState = null;

      function onCanvasMouseDown(e) {
        const target = e.target;
        const id =
          target.dataset.id ||
          (target.parentElement && target.parentElement.dataset.id);
        const isHandle = target.classList.contains("handle-rotate");
        if (!id) return;

        const item = state.furniture.find((f) => f.id === id);
        if (!item) return;

        state.selectedId = id;
        syncSelectionPanel();
        renderCanvas(); // to show selection outline & handle

        const wrapper = document.getElementById("room-canvas-wrapper");
        const rect = wrapper.getBoundingClientRect();
        const gridSizePx = 40 * state.zoom;

        if (isHandle) {
          // rotation
          const centerX = rect.left + (item.x / state.scale) * gridSizePx;
          const centerY = rect.top + (item.y / state.scale) * gridSizePx;
          rotateState = {
            id,
            centerX,
            centerY,
          };
          e.preventDefault();
          return;
        }

        // drag
        const roomRect = canvasEl.getBoundingClientRect();
        const clickX = e.clientX - roomRect.left;
        const clickY = e.clientY - roomRect.top;

        dragState = {
          id,
          offsetX: clickX - (item.x / state.scale) * gridSizePx,
          offsetY: clickY - (item.y / state.scale) * gridSizePx,
          constrainAxis: e.shiftKey ? "pending" : null,
        };
        e.preventDefault();
      }

      function onMouseMove(e) {
        const wrapper = document.getElementById("room-canvas-wrapper");
        const rect = wrapper.getBoundingClientRect();
        const gridSizePx = 40 * state.zoom;

        if (rotateState) {
          const item = state.furniture.find((f) => f.id === rotateState.id);
          if (!item) return;
          const dx = e.clientX - rotateState.centerX;
          const dy = e.clientY - rotateState.centerY;
          const angle = (Math.atan2(dy, dx) * 180) / Math.PI;
          item.rotation = angle;
          itemRotateInput.value = angle.toFixed(0);
          renderCanvas();
          return;
        }

        if (dragState) {
          const item = state.furniture.find((f) => f.id === dragState.id);
          if (!item) return;
          const roomRect = canvasEl.getBoundingClientRect();
          let xPx = e.clientX - roomRect.left - dragState.offsetX;
          let yPx = e.clientY - roomRect.top - dragState.offsetY;

          // Determine axis constraint after small movement
          if (dragState.constrainAxis === "pending") {
            const dx = Math.abs(
              xPx + dragState.offsetX - (item.x / state.scale) * gridSizePx
            );
            const dy = Math.abs(
              yPx + dragState.offsetY - (item.y / state.scale) * gridSizePx
            );
            dragState.constrainAxis = dx > dy ? "x" : "y";
          }

          if (dragState.constrainAxis === "x") {
            // lock y to original
            yPx = (item.y / state.scale) * gridSizePx - dragState.offsetY;
          } else if (dragState.constrainAxis === "y") {
            xPx = (item.x / state.scale) * gridSizePx - dragState.offsetX;
          }

          const centerX = xPx + ((item.length / state.scale) * gridSizePx) / 2;
          const centerY = yPx + ((item.width / state.scale) * gridSizePx) / 2;

          // convert back to internal feet
          item.x = (centerX / gridSizePx) * state.scale;
          item.y = (centerY / gridSizePx) * state.scale;

          renderCanvas();
        }
      }

      function onMouseUp() {
        dragState = null;
        rotateState = null;
      }

      // Zoom
      function onCanvasWheel(e) {
        if (!e.ctrlKey && !e.metaKey) return; // require ctrl/cmd to avoid accidental zoom
        e.preventDefault();
        const delta = e.deltaY < 0 ? 0.1 : -0.1;
        state.zoom = Math.min(2, Math.max(0.5, state.zoom + delta));
        renderCanvas();
      }

      // Clearance checker (simplified heuristics)
      function updateClearanceSummary() {
        const lines = [];
        const unitLabel = state.unit === "ft" ? "ft" : "m";

        const sofa = state.furniture.find(
          (f) => f.type === "sofa" || f.type === "loveseat"
        );
        const coffee = state.furniture.find((f) => f.type === "coffee_table");
        if (sofa && coffee) {
          const distFt = distanceBetweenItems(sofa, coffee);
          const distUnit = fromInternalFeet(distFt);
          const idealMinFt = 1.2; // 14 in
          const idealMaxFt = 1.5; // 18 in
          let status;
          if (distFt < idealMinFt)
            status = `Tight: ${distUnit.toFixed(
              2
            )} ${unitLabel} between sofa and coffee table (aim for 1.2–1.5 ft).`;
          else if (distFt > idealMaxFt * 1.5)
            status = `Far: ${distUnit.toFixed(
              2
            )} ${unitLabel} between sofa and coffee table (ideal 1.2–1.5 ft).`;
          else
            status = `Good: ${distUnit.toFixed(
              2
            )} ${unitLabel} between sofa and coffee table.`;
          lines.push(status);
        }

        const bed = state.furniture.find(
          (f) => f.type === "bed_queen" || f.type === "bed_king"
        );
        if (bed) {
          const sideClearFt = Math.min(bed.x, state.room.length - bed.x);
          const sideClearUnit = fromInternalFeet(sideClearFt);
          if (sideClearFt < 2) {
            lines.push(
              `Bed side clearance is tight (~${sideClearUnit.toFixed(
                2
              )} ${unitLabel}); aim for at least 2–2.5 ft on each accessible side.`
            );
          } else {
            lines.push(
              `Bed side clearance looks comfortable (~${sideClearUnit.toFixed(
                2
              )} ${unitLabel}).`
            );
          }
        }

        const dining = state.furniture.find((f) => f.type === "dining_table");
        if (dining) {
          const minToWallFt = Math.min(
            dining.x,
            state.room.length - dining.x,
            dining.y,
            state.room.width - dining.y
          );
          const minToWallUnit = fromInternalFeet(minToWallFt);
          if (minToWallFt < 3) {
            lines.push(
              `Dining table is close to a wall (~${minToWallUnit.toFixed(
                2
              )} ${unitLabel}); aim for 3–3.5 ft behind chairs for comfortable movement.`
            );
          } else {
            lines.push(
              `Dining table has about ${minToWallUnit.toFixed(
                2
              )} ${unitLabel} to the nearest wall, which is generally comfortable.`
            );
          }
        }

        if (!lines.length) {
          clearanceSummaryEl.innerHTML = `
      <li class="text-gray-500">Add a sofa and coffee table to see seating clearance.</li>
      <li class="text-gray-500">Add a bed to check side and foot clearances.</li>
      <li class="text-gray-500">Add a dining table to check chair space.</li>
    `;
          return;
        }

        clearanceSummaryEl.innerHTML = "";
        lines.forEach((text) => {
          const li = document.createElement("li");
          li.textContent = text;
          clearanceSummaryEl.appendChild(li);
        });
      }

      function distanceBetweenItems(a, b) {
        // approximate as distance between centers
        const dx = a.x - b.x;
        const dy = a.y - b.y;
        return Math.sqrt(dx * dx + dy * dy);
      }

      // Export / Import
      function exportLayout() {
        const data = {
          unit: state.unit,
          room: {
            length: fromInternalFeet(state.room.length),
            width: fromInternalFeet(state.room.width),
          },
          furniture: state.furniture.map((f) => ({
            id: f.id,
            type: f.type,
            label: f.label,
            length: fromInternalFeet(f.length),
            width: fromInternalFeet(f.width),
            x: fromInternalFeet(f.x),
            y: fromInternalFeet(f.y),
            rotation: f.rotation,
            color: f.color,
            meta: f.meta,
          })),
        };
        layoutJsonEl.value = JSON.stringify(data, null, 2);
      }

      function importLayout() {
        try {
          const data = JSON.parse(layoutJsonEl.value);
          if (!data || !data.room || !Array.isArray(data.furniture)) {
            alert("Invalid layout JSON.");
            return;
          }
          state.unit = data.unit === "m" ? "m" : "ft";
          unitSelect.value = state.unit;
          document.getElementById("room-length-unit-label").textContent =
            state.unit;
          document.getElementById("room-width-unit-label").textContent =
            state.unit;
          document.getElementById("item-length-unit-label").textContent =
            state.unit;
          document.getElementById("item-width-unit-label").textContent =
            state.unit;
          document.getElementById("item-elevation-unit-label").textContent =
            state.unit + " (note)";

          state.room.length = toInternalFeet(data.room.length);
          state.room.width = toInternalFeet(data.room.width);
          roomLengthInput.value = data.room.length;
          roomWidthInput.value = data.room.width;

          state.furniture = data.furniture.map((f) => ({
            id: f.id || generateId(),
            type: f.type,
            label: f.label,
            length: toInternalFeet(f.length),
            width: toInternalFeet(f.width),
            x: toInternalFeet(f.x),
            y: toInternalFeet(f.y),
            rotation: f.rotation || 0,
            color: f.color || "#e5e7eb",
            meta: f.meta || {},
          }));
          state.selectedId = null;
          updateScale();
          syncSelectionPanel();
          updateClearanceSummary();
          renderCanvas();
        } catch (err) {
          alert("Could not parse layout JSON.");
        }
      }

      // Reset
      function resetLayout() {
        state.furniture = [];
        state.selectedId = null;
        layoutJsonEl.value = "";
        syncSelectionPanel();
        updateClearanceSummary();
        renderCanvas();
      }

      // Init
      document.addEventListener("DOMContentLoaded", () => {
        canvasEl = document.getElementById("room-canvas");
        selectionPanel = document.getElementById("selection-panel");
        noSelectionLabel = document.getElementById("no-selection-label");

        itemLabelInput = document.getElementById("item-label-input");
        itemLengthInput = document.getElementById("item-length-input");
        itemWidthInput = document.getElementById("item-width-input");
        itemRotateInput = document.getElementById("item-rotate-input");
        itemElevationInput = document.getElementById("item-elevation-input");

        roomLengthInput = document.getElementById("room-length-input");
        roomWidthInput = document.getElementById("room-width-input");
        unitSelect = document.getElementById("unit-select");
        scaleLabel = document.getElementById("scale-label");
        clearanceSummaryEl = document.getElementById("clearance-summary");
        layoutJsonEl = document.getElementById("layout-json");

        document.getElementById("room-length-unit-label").textContent =
          state.unit;
        document.getElementById("room-width-unit-label").textContent =
          state.unit;
        document.getElementById("item-length-unit-label").textContent =
          state.unit;
        document.getElementById("item-width-unit-label").textContent =
          state.unit;
        document.getElementById("item-elevation-unit-label").textContent =
          state.unit + " (note)";

        updateScale();
        renderCanvas();

        // Event bindings
        unitSelect.addEventListener("change", () => {
          const newUnit = unitSelect.value;
          if (newUnit === state.unit) return;
          // convert room & furniture values
          if (newUnit === "m" && state.unit === "ft") {
            // convert internal ft to m in inputs
            roomLengthInput.value = (state.room.length / 3.28084).toFixed(2);
            roomWidthInput.value = (state.room.width / 3.28084).toFixed(2);
          } else if (newUnit === "ft" && state.unit === "m") {
            roomLengthInput.value = state.room.length.toFixed(2);
            roomWidthInput.value = state.room.width.toFixed(2);
          }
          state.unit = newUnit;
          document.getElementById("room-length-unit-label").textContent =
            state.unit;
          document.getElementById("room-width-unit-label").textContent =
            state.unit;
          document.getElementById("item-length-unit-label").textContent =
            state.unit;
          document.getElementById("item-width-unit-label").textContent =
            state.unit;
          document.getElementById("item-elevation-unit-label").textContent =
            state.unit + " (note)";
          updateRoomFromInputs();
          syncSelectionPanel();
        });

        roomLengthInput.addEventListener("input", updateRoomFromInputs);
        roomWidthInput.addEventListener("input", updateRoomFromInputs);

        document.querySelectorAll(".add-furniture-btn").forEach((btn) => {
          btn.addEventListener("click", () => addFurniture(btn.dataset.type));
        });

        itemLabelInput.addEventListener("input", updateItemFromInputs);
        itemLengthInput.addEventListener("input", updateItemFromInputs);
        itemWidthInput.addEventListener("input", updateItemFromInputs);
        itemRotateInput.addEventListener("input", updateItemFromInputs);
        itemElevationInput.addEventListener("input", updateItemFromInputs);

        document
          .getElementById("delete-item-btn")
          .addEventListener("click", () => {
            const item = getSelectedItem();
            if (!item) return;
            state.furniture = state.furniture.filter((f) => f.id !== item.id);
            state.selectedId = null;
            syncSelectionPanel();
            updateClearanceSummary();
            renderCanvas();
          });

        document
          .getElementById("export-layout-btn")
          .addEventListener("click", exportLayout);
        document
          .getElementById("import-layout-btn")
          .addEventListener("click", importLayout);
        document
          .getElementById("reset-layout-btn")
          .addEventListener("click", resetLayout);

        canvasEl.addEventListener("mousedown", onCanvasMouseDown);
        canvasEl.addEventListener("wheel", onCanvasWheel, { passive: false });
        window.addEventListener("mousemove", onMouseMove);
        window.addEventListener("mouseup", onMouseUp);
      });
    </script>
  
