    <script>
      // Utility functions
      function fmtCurrency(value) {
        if (!isFinite(value)) return "$0";
        const abs = Math.abs(value);
        const sign = value < 0 ? "-" : "";
        return (
          sign +
          abs.toLocaleString(undefined, {
            style: "currency",
            currency: "USD",
            maximumFractionDigits: 0,
          })
        );
      }

      function fmtPercent(value) {
        if (!isFinite(value)) return "0.0%";
        return (value * 100).toFixed(1) + "%";
      }

      function getNumber(id) {
        const el = document.getElementById(id);
        if (!el) return 0;
        const v = parseFloat(el.value);
        return isNaN(v) ? 0 : v;
      }

      function calculateCoC() {
        // Inputs
        const purchasePrice = getNumber("purchasePrice");
        const downPct = getNumber("downPaymentPct") / 100;
        const closingCosts = getNumber("closingCosts");
        const upfrontCapex = getNumber("upfrontCapex");

        const monthlyRent = getNumber("monthlyRent");
        const otherMonthly = getNumber("otherIncome");
        const vacancyPct = getNumber("vacancyPct") / 100;

        const annualTaxes = getNumber("annualTaxes");
        const annualInsurance = getNumber("annualInsurance");
        const annualRepairs = getNumber("annualRepairs");
        const annualMgmt = getNumber("annualMgmt");
        const otherOpex = getNumber("otherOpex");

        const rate = getNumber("interestRate") / 100;
        const termYears = getNumber("loanTerm");

        // Guard
        if (purchasePrice <= 0) {
          updateOutputs({
            totalCash: 0,
            loanAmount: 0,
            monthlyDebt: 0,
            annualDebt: 0,
            egi: 0,
            opex: 0,
            noi: 0,
            capRate: 0,
            cfLevered: 0,
            cfUnlevered: 0,
            cocLevered: 0,
            cocUnlevered: 0,
            cocStressed: 0,
          });
          return;
        }

        // Loan & equity
        const downPayment = purchasePrice * downPct;
        const loanAmount = Math.max(purchasePrice - downPayment, 0);
        const totalCash = downPayment + closingCosts + upfrontCapex;

        // Mortgage payment
        const n = termYears * 12;
        const i = rate / 12;
        let monthlyDebt = 0;
        if (loanAmount > 0 && rate > 0 && n > 0) {
          const pow = Math.pow(1 + i, n);
          monthlyDebt = (loanAmount * (i * pow)) / (pow - 1);
        } else if (loanAmount > 0 && rate === 0 && n > 0) {
          monthlyDebt = loanAmount / n;
        }
        const annualDebt = monthlyDebt * 12;

        // Income
        const gsi = (monthlyRent + otherMonthly) * 12;
        const egi = gsi * (1 - vacancyPct);

        // Expenses & NOI
        const opex =
          annualTaxes +
          annualInsurance +
          annualRepairs +
          annualMgmt +
          otherOpex;
        const noi = egi - opex;
        const capRate = purchasePrice > 0 ? noi / purchasePrice : 0;

        // Cash flows
        const cfLevered = noi - annualDebt;
        const cfUnlevered = noi; // no debt

        const cocLevered = totalCash > 0 ? cfLevered / totalCash : 0;
        const cocUnlevered =
          purchasePrice > 0 ? cfUnlevered / purchasePrice : 0;

        // Stress test
        const stressPct = getNumber("stressVacancy") / 100;
        const egiStressed = egi * (1 - stressPct);
        const noiStressed = egiStressed - opex;
        const cfLeveredStressed = noiStressed - annualDebt;
        const cocStressed = totalCash > 0 ? cfLeveredStressed / totalCash : 0;

        updateOutputs({
          totalCash,
          loanAmount,
          monthlyDebt,
          annualDebt,
          egi,
          opex,
          noi,
          capRate,
          cfLevered,
          cfUnlevered,
          cocLevered,
          cocUnlevered,
          cocStressed,
        });
      }

      function updateOutputs(o) {
        document.getElementById("outTotalCash").textContent = fmtCurrency(
          o.totalCash
        );
        document.getElementById("outLoanAmount").textContent = fmtCurrency(
          o.loanAmount
        );
        document.getElementById("outMonthlyDebt").textContent = fmtCurrency(
          o.monthlyDebt
        );
        document.getElementById("outAnnualDebt").textContent = fmtCurrency(
          o.annualDebt
        );

        document.getElementById("outEGI").textContent = fmtCurrency(o.egi);
        document.getElementById("outOpex").textContent = fmtCurrency(o.opex);
        document.getElementById("outNOI").textContent = fmtCurrency(o.noi);
        document.getElementById("outCapRate").textContent = fmtPercent(
          o.capRate
        );

        document.getElementById("outCashFlowLevered").textContent = fmtCurrency(
          o.cfLevered
        );
        document.getElementById("outCashFlowUnlevered").textContent =
          fmtCurrency(o.cfUnlevered);
        document.getElementById("outCoCLevered").textContent = fmtPercent(
          o.cocLevered
        );
        document.getElementById("outCoCUnlevered").textContent = fmtPercent(
          o.cocUnlevered
        );
        document.getElementById("outCoCStressed").textContent = fmtPercent(
          o.cocStressed
        );
      }

      function resetDefaults() {
        const defaults = {
          purchasePrice: 300000,
          downPaymentPct: 25,
          closingCosts: 6000,
          upfrontCapex: 15000,
          monthlyRent: 2600,
          otherIncome: 0,
          vacancyPct: 5,
          annualTaxes: 4200,
          annualInsurance: 1200,
          annualRepairs: 1800,
          annualMgmt: 2500,
          otherOpex: 1000,
          interestRate: 6.5,
          loanTerm: 30,
          stressVacancy: 0,
        };
        for (const [id, val] of Object.entries(defaults)) {
          const el = document.getElementById(id);
          if (el) el.value = val;
        }
        document.getElementById("downPaymentSlider").value =
          defaults.downPaymentPct;
        document.getElementById("vacancySlider").value = defaults.vacancyPct;
        document.getElementById("stressVacancyLabel").textContent =
          defaults.stressVacancy + "%";
        calculateCoC();
      }

      document.addEventListener("DOMContentLoaded", () => {
        const downPct = document.getElementById("downPaymentPct");
        const downSlider = document.getElementById("downPaymentSlider");
        const vacPct = document.getElementById("vacancyPct");
        const vacSlider = document.getElementById("vacancySlider");
        const stress = document.getElementById("stressVacancy");
        const stressLabel = document.getElementById("stressVacancyLabel");

        // Sync sliders and inputs
        downSlider.addEventListener("input", () => {
          downPct.value = downSlider.value;
          calculateCoC();
        });
        downPct.addEventListener("input", () => {
          const v = Math.min(Math.max(parseFloat(downPct.value) || 0, 0), 100);
          downPct.value = v;
          downSlider.value = Math.min(v, parseFloat(downSlider.max));
          calculateCoC();
        });

        vacSlider.addEventListener("input", () => {
          vacPct.value = vacSlider.value;
          calculateCoC();
        });
        vacPct.addEventListener("input", () => {
          const v = Math.min(Math.max(parseFloat(vacPct.value) || 0, 0), 50);
          vacPct.value = v;
          vacSlider.value = Math.min(v, parseFloat(vacSlider.max));
          calculateCoC();
        });

        stress.addEventListener("input", () => {
          stressLabel.textContent = stress.value + "%";
          calculateCoC();
        });

        document.getElementById("calcBtn").addEventListener("click", (e) => {
          e.preventDefault();
          calculateCoC();
        });

        document.getElementById("resetBtn").addEventListener("click", (e) => {
          e.preventDefault();
          resetDefaults();
        });

        // Auto-recalculate on change of key fields
        [
          "purchasePrice",
          "closingCosts",
          "upfrontCapex",
          "monthlyRent",
          "otherIncome",
          "annualTaxes",
          "annualInsurance",
          "annualRepairs",
          "annualMgmt",
          "otherOpex",
          "interestRate",
          "loanTerm",
        ].forEach((id) => {
          const el = document.getElementById(id);
          if (el) el.addEventListener("input", calculateCoC);
        });

        resetDefaults();
      });
    </script>

