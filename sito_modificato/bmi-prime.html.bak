    <script>
      // Unit toggle
      const unitMetricBtn = document.getElementById("unit-metric");
      const unitImperialBtn = document.getElementById("unit-imperial");
      const metricFields = document.getElementById("metric-fields");
      const imperialFields = document.getElementById("imperial-fields");

      function setUnitSystem(system) {
        if (system === "metric") {
          metricFields.classList.remove("hidden");
          imperialFields.classList.add("hidden");
          unitMetricBtn.classList.add(
            "bg-blue-600",
            "text-white",
            "border-blue-600"
          );
          unitMetricBtn.classList.remove("border-gray-300", "text-gray-700");
          unitImperialBtn.classList.remove(
            "bg-blue-600",
            "text-white",
            "border-blue-600"
          );
          unitImperialBtn.classList.add("border-gray-300", "text-gray-700");
        } else {
          metricFields.classList.add("hidden");
          imperialFields.classList.remove("hidden");
          unitImperialBtn.classList.add(
            "bg-blue-600",
            "text-white",
            "border-blue-600"
          );
          unitImperialBtn.classList.remove("border-gray-300", "text-gray-700");
          unitMetricBtn.classList.remove(
            "bg-blue-600",
            "text-white",
            "border-blue-600"
          );
          unitMetricBtn.classList.add("border-gray-300", "text-gray-700");
        }
      }

      unitMetricBtn.addEventListener("click", () => setUnitSystem("metric"));
      unitImperialBtn.addEventListener("click", () =>
        setUnitSystem("imperial")
      );

      // Calculation
      const calculateBtn = document.getElementById("calculate-btn");
      const errorEl = document.getElementById("input-error");

      const bmiValueEl = document.getElementById("bmi-value");
      const bmiPrimeValueEl = document.getElementById("bmi-prime-value");
      const bmiCategoryEl = document.getElementById("bmi-category");
      const primePercentageEl = document.getElementById("prime-percentage");
      const targetBmiRangeEl = document.getElementById("target-bmi-range");
      const targetWeightRangeEl = document.getElementById(
        "target-weight-range"
      );
      const weightDeltaEl = document.getElementById("weight-delta");
      const primeMarkerEl = document.getElementById("prime-marker");

      function classifyBMI(bmi) {
        if (bmi < 16) return "Severe underweight";
        if (bmi < 18.5) return "Underweight";
        if (bmi < 25) return "Normal weight";
        if (bmi < 30) return "Overweight";
        if (bmi < 35) return "Obesity class I";
        if (bmi < 40) return "Obesity class II";
        return "Obesity class III";
      }

      function updatePrimeMarker(prime) {
        // Map BMI Prime 0.6–1.4 to 0–100% of the bar
        const minPrime = 0.6;
        const maxPrime = 1.4;
        let p = (prime - minPrime) / (maxPrime - minPrime);
        if (isNaN(p)) {
          primeMarkerEl.style.left = "50%";
          primeMarkerEl.style.display = "none";
          return;
        }
        p = Math.max(0, Math.min(1, p));
        primeMarkerEl.style.left = p * 100 + "%";
        primeMarkerEl.style.display = "block";
      }

      calculateBtn.addEventListener("click", () => {
        errorEl.classList.add("hidden");
        errorEl.textContent = "";

        const usingMetric = !metricFields.classList.contains("hidden");

        let heightM, weightKg;

        if (usingMetric) {
          const hCm = parseFloat(document.getElementById("height-cm").value);
          const wKg = parseFloat(document.getElementById("weight-kg").value);
          if (!hCm || !wKg || hCm <= 0 || wKg <= 0) {
            errorEl.textContent = "Please enter a valid height and weight.";
            errorEl.classList.remove("hidden");
            return;
          }
          heightM = hCm / 100;
          weightKg = wKg;
        } else {
          const hFt = parseFloat(document.getElementById("height-ft").value);
          const hIn = parseFloat(
            document.getElementById("height-in").value || "0"
          );
          const wLb = parseFloat(document.getElementById("weight-lb").value);
          const totalIn = (hFt || 0) * 12 + (hIn || 0);
          if (!totalIn || !wLb || totalIn <= 0 || wLb <= 0) {
            errorEl.textContent = "Please enter a valid height and weight.";
            errorEl.classList.remove("hidden");
            return;
          }
          heightM = totalIn * 0.0254;
          weightKg = wLb * 0.45359237;
        }

        const bmi = weightKg / (heightM * heightM);
        if (!isFinite(bmi) || bmi <= 0) {
          errorEl.textContent = "Unable to compute BMI with the given values.";
          errorEl.classList.remove("hidden");
          return;
        }

        const bmiPrime = bmi / 25;
        const category = classifyBMI(bmi);

        bmiValueEl.textContent = bmi.toFixed(1);
        bmiPrimeValueEl.textContent = bmiPrime.toFixed(2);
        bmiCategoryEl.textContent = category;

        const percent = (bmiPrime * 100).toFixed(0);
        const rel =
          bmiPrime >= 1
            ? `${percent}% of upper normal limit`
            : `${percent}% of upper normal limit`;
        primePercentageEl.textContent = rel;

        // Target BMI range 18.5–25
        const bmiMin = 18.5;
        const bmiMax = 25;
        targetBmiRangeEl.textContent = `${bmiMin.toFixed(1)} – ${bmiMax.toFixed(
          1
        )}`;

        const weightMin = bmiMin * heightM * heightM;
        const weightMax = bmiMax * heightM * heightM;
        targetWeightRangeEl.textContent = `${weightMin.toFixed(
          1
        )} – ${weightMax.toFixed(1)} kg`;

        let deltaText = "";
        if (weightKg < weightMin) {
          const gain = weightMin - weightKg;
          deltaText = `You would need to gain about ${gain.toFixed(
            1
          )} kg to reach the lower end of the normal BMI range.`;
        } else if (weightKg > weightMax) {
          const lose = weightKg - weightMax;
          deltaText = `You would need to lose about ${lose.toFixed(
            1
          )} kg to reach the upper end of the normal BMI range.`;
        } else {
          deltaText =
            "Your current weight is already within the normal BMI range for your height.";
        }
        weightDeltaEl.textContent = deltaText;

        updatePrimeMarker(bmiPrime);
      });

      // Initialize marker as hidden
      primeMarkerEl.style.display = "none";
    </script>

