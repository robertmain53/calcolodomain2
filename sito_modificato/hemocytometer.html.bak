    <script>
      (function () {
        const chamberTypeEl = document.getElementById("chamber-type");
        const customWrapper = document.getElementById("custom-chamber-wrapper");
        const squareAreaEl = document.getElementById("square-area");
        const chamberDepthEl = document.getElementById("chamber-depth");
        const squaresCountedEl = document.getElementById("squares-counted");
        const dilutionFactorEl = document.getElementById("dilution-factor");
        const liveCellsEl = document.getElementById("live-cells");
        const deadCellsEl = document.getElementById("dead-cells");
        const sampleVolumeEl = document.getElementById("sample-volume");
        const sampleVolumeUnitEl =
          document.getElementById("sample-volume-unit");

        const avgLiveEl = document.getElementById("avg-live");
        const liveConcEl = document.getElementById("live-conc");
        const liveConcSciEl = document.getElementById("live-conc-sci");
        const totalConcEl = document.getElementById("total-conc");
        const totalConcSciEl = document.getElementById("total-conc-sci");
        const viabilityEl = document.getElementById("viability");
        const totalLiveCellsEl = document.getElementById("total-live-cells");
        const totalCellsEl = document.getElementById("total-cells");
        const resultNoteEl = document.getElementById("result-note");

        const calcBtn = document.getElementById("calculate-btn");
        const resetBtn = document.getElementById("reset-btn");

        function formatNumber(num) {
          if (!isFinite(num)) return "–";
          if (num === 0) return "0";
          if (Math.abs(num) >= 1e6 || Math.abs(num) < 1e-2) {
            return num
              .toExponential(2)
              .replace("e+", "×10^")
              .replace("e-", "×10^-");
          }
          if (Math.abs(num) >= 1e3) {
            return num.toLocaleString(undefined, { maximumFractionDigits: 0 });
          }
          return num.toLocaleString(undefined, { maximumFractionDigits: 2 });
        }

        function formatSci(num) {
          if (!isFinite(num) || num === 0) return "";
          const exp = num.toExponential(2);
          const parts = exp.split("e");
          const mant = parseFloat(parts[0]);
          const power = parseInt(parts[1], 10);
          return mant + " × 10^" + power;
        }

        function getVolumeFactor() {
          if (chamberTypeEl.value === "neubauer") {
            // 1 mm² × 0.1 mm = 0.1 mm³ = 1e-4 mL => factor 1 / 1e-4 = 1e4
            return 1e4;
          }
          const area = parseFloat(squareAreaEl.value);
          const depth = parseFloat(chamberDepthEl.value);
          if (!(area > 0) || !(depth > 0)) return NaN;
          const volume_mm3 = area * depth; // mm³
          const volume_mL = volume_mm3 * 1e-3; // 1 mm³ = 1e-3 µL = 1e-6 mL? Actually 1 cm³ = 1 mL, 1 cm = 10 mm, so 1 mm³ = 1e-3 cm³ = 1e-3 mL
          // Correct: 1 mm³ = 1e-3 mL
          const volume_mL_correct = volume_mm3 * 1e-3;
          return 1 / volume_mL_correct;
        }

        chamberTypeEl.addEventListener("change", () => {
          if (chamberTypeEl.value === "custom") {
            customWrapper.classList.remove("hidden");
          } else {
            customWrapper.classList.add("hidden");
          }
        });

        function calculate() {
          const nSquares = parseFloat(squaresCountedEl.value);
          const dilution = parseFloat(dilutionFactorEl.value);
          const live = parseFloat(liveCellsEl.value);
          const deadRaw = deadCellsEl.value.trim();
          const dead = deadRaw === "" ? NaN : parseFloat(deadRaw);

          if (!(nSquares > 0) || !(dilution > 0) || !(live >= 0)) {
            resultNoteEl.textContent =
              "Please enter valid values for squares counted, dilution factor, and live cells.";
            return;
          }

          const volFactor = getVolumeFactor();
          if (!isFinite(volFactor) || volFactor <= 0) {
            resultNoteEl.textContent =
              "Please enter valid custom chamber dimensions.";
            return;
          }

          const avgLive = live / nSquares;
          const totalCount = live + (isNaN(dead) ? 0 : dead);
          const avgTotal = totalCount / nSquares;

          const liveConc = avgLive * volFactor * dilution;
          const totalConc = avgTotal * volFactor * dilution;

          let viability = NaN;
          if (!isNaN(dead) && totalCount > 0) {
            viability = (live / totalCount) * 100;
          }

          // Sample volume
          let totalLiveCells = NaN;
          let totalCells = NaN;
          const volInput = sampleVolumeEl.value.trim();
          if (volInput !== "") {
            let V = parseFloat(volInput);
            if (V > 0) {
              if (sampleVolumeUnitEl.value === "uL") {
                V = V / 1000; // µL to mL
              }
              totalLiveCells = liveConc * V;
              totalCells = totalConc * V;
            }
          }

          avgLiveEl.textContent = isFinite(avgLive)
            ? formatNumber(avgLive) + " cells/square"
            : "–";
          liveConcEl.textContent = isFinite(liveConc)
            ? formatNumber(liveConc) + " cells/mL"
            : "–";
          liveConcSciEl.textContent = isFinite(liveConc)
            ? formatSci(liveConc) + " cells/mL"
            : "";
          totalConcEl.textContent = isFinite(totalConc)
            ? formatNumber(totalConc) + " cells/mL"
            : "–";
          totalConcSciEl.textContent = isFinite(totalConc)
            ? formatSci(totalConc) + " cells/mL"
            : "";
          viabilityEl.textContent = isFinite(viability)
            ? formatNumber(viability) + " %"
            : "–";
          totalLiveCellsEl.textContent = isFinite(totalLiveCells)
            ? formatNumber(totalLiveCells) + " cells"
            : "–";
          totalCellsEl.textContent = isFinite(totalCells)
            ? formatNumber(totalCells) + " cells"
            : "–";

          resultNoteEl.textContent =
            "Results are approximate and depend on proper technique and chamber calibration.";
        }

        function reset() {
          chamberTypeEl.value = "neubauer";
          customWrapper.classList.add("hidden");
          squareAreaEl.value = 1;
          chamberDepthEl.value = 0.1;
          squaresCountedEl.value = 4;
          dilutionFactorEl.value = 2;
          liveCellsEl.value = 320;
          deadCellsEl.value = 80;
          sampleVolumeEl.value = "";
          sampleVolumeUnitEl.value = "mL";

          avgLiveEl.textContent = "–";
          liveConcEl.textContent = "–";
          liveConcSciEl.textContent = "";
          totalConcEl.textContent = "–";
          totalConcSciEl.textContent = "";
          viabilityEl.textContent = "–";
          totalLiveCellsEl.textContent = "–";
          totalCellsEl.textContent = "–";
          resultNoteEl.textContent =
            "Enter your counts and click “Calculate” to see results.";
        }

        calcBtn.addEventListener("click", calculate);
        resetBtn.addEventListener("click", reset);

        // Allow Enter key to trigger calculation
        document.addEventListener("keydown", function (e) {
          if (
            e.key === "Enter" &&
            document.activeElement &&
            document.activeElement.tagName === "INPUT"
          ) {
            e.preventDefault();
            calculate();
          }
        });
      })();
    </script>
  
