<script>
  // Time zone list (common zones + browser default at top)
  const COMMON_TIMEZONES = [
    "UTC",
    "Europe/London",
    "Europe/Berlin",
    "Europe/Paris",
    "Europe/Madrid",
    "Europe/Rome",
    "Europe/Warsaw",
    "Europe/Moscow",
    "America/New_York",
    "America/Chicago",
    "America/Denver",
    "America/Los_Angeles",
    "America/Toronto",
    "America/Sao_Paulo",
    "America/Mexico_City",
    "America/Bogota",
    "America/Lima",
    "Asia/Dubai",
    "Asia/Kolkata",
    "Asia/Shanghai",
    "Asia/Tokyo",
    "Asia/Singapore",
    "Asia/Hong_Kong",
    "Asia/Bangkok",
    "Australia/Sydney",
    "Pacific/Auckland"
  ];

  function getBrowserTimeZone() {
    try {
      return Intl.DateTimeFormat().resolvedOptions().timeZone || "UTC";
    } catch (e) {
      return "UTC";
    }
  }

  function buildTimeZoneOptions(selectEl, browserTz) {
    const tzSet = new Set(COMMON_TIMEZONES);
    tzSet.add(browserTz);
    const tzList = Array.from(tzSet).sort();

    selectEl.innerHTML = "";
    tzList.forEach(tz => {
      const opt = document.createElement("option");
      opt.value = tz;
      opt.textContent = tz;
      if (tz === browserTz) opt.selected = true;
      selectEl.appendChild(opt);
    });
  }

  function getOffsetLabel(tz, date) {
    try {
      const dtf = new Intl.DateTimeFormat("en-US", {
        timeZone: tz,
        hour12: false,
        year: "numeric",
        month: "2-digit",
        day: "2-digit",
        hour: "2-digit",
        minute: "2-digit",
        second: "2-digit",
        timeZoneName: "shortOffset"
      });
      const parts = dtf.formatToParts(date);
      const offsetPart = parts.find(p => p.type === "timeZoneName");
      return offsetPart ? offsetPart.value : "";
    } catch (e) {
      return "";
    }
  }

  function updateOffsetBadges() {
    const now = new Date();
    const startTz = document.getElementById("start-tz").value;
    const endTz = document.getElementById("end-tz").value;
    document.getElementById("start-tz-offset").textContent =
      startTz ? `Current offset: ${getOffsetLabel(startTz, now)}` : "";
    document.getElementById("end-tz-offset").textContent =
      endTz ? `Current offset: ${getOffsetLabel(endTz, now)}` : "";
  }

  function pad2(n) {
    return n.toString().padStart(2, "0");
  }

  function setDateTimeInputsToNow(prefix) {
    const now = new Date();
    const dateEl = document.getElementById(prefix + "-date");
    const timeEl = document.getElementById(prefix + "-time");
    const ampmEl = document.getElementById(prefix + "-ampm");
    const is12 = document.getElementById("format-12").getAttribute("aria-pressed") === "true";

    dateEl.value = now.toISOString().slice(0, 10);

    let hours = now.getHours();
    let minutes = now.getMinutes();
    if (is12) {
      const ampm = hours >= 12 ? "PM" : "AM";
      let displayHour = hours % 12;
      if (displayHour === 0) displayHour = 12;
      timeEl.value = pad2(displayHour) + ":" + pad2(minutes);
      ampmEl.value = ampm;
    } else {
      timeEl.value = pad2(hours) + ":" + pad2(minutes);
    }
  }

  function parseLocalDateTime(dateStr, timeStr, ampm, is12) {
    if (!dateStr || !timeStr) return null;
    let [hourStr, minuteStr] = timeStr.split(":");
    let h = parseInt(hourStr, 10);
    const m = parseInt(minuteStr, 10);
    if (isNaN(h) || isNaN(m)) return null;

    if (is12) {
      if (ampm === "PM" && h < 12) h += 12;
      if (ampm === "AM" && h === 12) h = 0;
    }

    const [year, month, day] = dateStr.split("-").map(x => parseInt(x, 10));
    if (!year || !month || !day) return null;

    // Construct as ISO string in local time zone, then interpret in target tz
    const isoLocal = `${pad2(year)}-${pad2(month)}-${pad2(day)}T${pad2(h)}:${pad2(m)}:00`;
    return isoLocal;
  }

  function zonedToUtc(isoLocal, tz) {
    // Use Intl to get UTC timestamp for a local time in a given tz
    try {
      const dtf = new Intl.DateTimeFormat("en-US", {
        timeZone: tz,
        hour12: false,
        year: "numeric",
        month: "2-digit",
        day: "2-digit",
        hour: "2-digit",
        minute: "2-digit",
        second: "2-digit"
      });

      const parts = dtf.formatToParts(new Date(isoLocal));
      const map = {};
      for (const p of parts) {
        if (p.type !== "literal") map[p.type] = p.value;
      }
      // This gives us the same wall time but in tz; we need offset, so we instead:
      // Create Date from isoLocal and adjust by difference between its local and tz offset.
      const localDate = new Date(isoLocal);
      const tzOffsetMinutes = -localDate.getTimezoneOffset(); // local offset
      // But we actually want the timestamp that corresponds to isoLocal in tz.
      // Simpler: use Date.parse with fake offset via toLocaleString; fallback to approximate.
      // For browser compatibility and simplicity, we rely on Date with timeZoneName:
      const utcTimestamp = Date.parse(localDate.toLocaleString("en-US", { timeZone: tz }));
      return new Date(utcTimestamp);
    } catch (e) {
      return new Date(isoLocal);
    }
  }

  function calculateDifference() {
    const is12 = document.getElementById("format-12").getAttribute("aria-pressed") === "true";

    const startDateStr = document.getElementById("start-date").value;
    const startTimeStr = document.getElementById("start-time").value;
    const startAmpm = document.getElementById("start-ampm").value;
    const startTz = document.getElementById("start-tz").value;

    const endDateStr = document.getElementById("end-date").value;
    const endTimeStr = document.getElementById("end-time").value;
    const endAmpm = document.getElementById("end-ampm").value;
    const endTz = document.getElementById("end-tz").value;

    const errorEl = document.getElementById("error-message");
    errorEl.classList.add("hidden");
    errorEl.textContent = "";

    const startIsoLocal = parseLocalDateTime(startDateStr, startTimeStr, startAmpm, is12);
    const endIsoLocal = parseLocalDateTime(endDateStr, endTimeStr, endAmpm, is12);

    if (!startIsoLocal || !endIsoLocal || !startTz || !endTz) {
      errorEl.textContent = "Please enter valid dates, times, and time zones for both start and end.";
      errorEl.classList.remove("hidden");
      document.getElementById("results-section").classList.add("hidden");
      return;
    }

    const startUtc = zonedToUtc(startIsoLocal, startTz);
    const endUtc = zonedToUtc(endIsoLocal, endTz);

    if (isNaN(startUtc.getTime()) || isNaN(endUtc.getTime())) {
      errorEl.textContent = "One of the date/time values is invalid. Please check your inputs.";
      errorEl.classList.remove("hidden");
      document.getElementById("results-section").classList.add("hidden");
      return;
    }

    let diffMs = endUtc.getTime() - startUtc.getTime();
    const sign = diffMs >= 0 ? 1 : -1;
    const absMs = Math.abs(diffMs);
    const totalSeconds = Math.floor(absMs / 1000);
    const totalMinutes = Math.floor(totalSeconds / 60);
    const totalHours = Math.floor(totalMinutes / 60);
    const totalDays = Math.floor(totalHours / 24);

    let remainingSeconds = totalSeconds;
    const days = Math.floor(remainingSeconds / 86400);
    remainingSeconds %= 86400;
    const hours = Math.floor(remainingSeconds / 3600);
    remainingSeconds %= 3600;
    const minutes = Math.floor(remainingSeconds / 60);
    const seconds = remainingSeconds % 60;

    const parts = [];
    if (days) parts.push(days + " day" + (days !== 1 ? "s" : ""));
    if (hours) parts.push(hours + " hour" + (hours !== 1 ? "s" : ""));
    if (minutes) parts.push(minutes + " minute" + (minutes !== 1 ? "s" : ""));
    if (seconds || parts.length === 0) parts.push(seconds + " second" + (seconds !== 1 ? "s" : ""));

    const mainText = (sign < 0 ? "−" : "") + parts.join(", ");

    document.getElementById("result-main").textContent = mainText;
    document.getElementById("result-alt").textContent =
      `Total: ${totalHours} hour${totalHours !== 1 ? "s" : ""} = ${totalMinutes} minute${totalMinutes !== 1 ? "s" : ""} = ${totalSeconds} second${totalSeconds !== 1 ? "s" : ""}`;
    document.getElementById("result-sign").textContent =
      sign < 0
        ? "Note: End moment is earlier than start moment (negative interval)."
        : "End moment is later than or equal to start moment (non‑negative interval).";

    const breakdownEl = document.getElementById("result-breakdown");
    breakdownEl.innerHTML = "";
    breakdownEl.innerHTML += `<li><strong>Days:</strong> ${days}</li>`;
    breakdownEl.innerHTML += `<li><strong>Hours:</strong> ${hours}</li>`;
    breakdownEl.innerHTML += `<li><strong>Minutes:</strong> ${minutes}</li>`;
    breakdownEl.innerHTML += `<li><strong>Seconds:</strong> ${seconds}</li>`;

    const unitsEl = document.getElementById("result-units");
    unitsEl.innerHTML = "";
    unitsEl.innerHTML += `<li><strong>Total days:</strong> ${ (sign < 0 ? "-" : "") + totalDays }</li>`;
    unitsEl.innerHTML += `<li><strong>Total hours:</strong> ${ (sign < 0 ? "-" : "") + totalHours }</li>`;
    unitsEl.innerHTML += `<li><strong>Total minutes:</strong> ${ (sign < 0 ? "-" : "") + totalMinutes }</li>`;
    unitsEl.innerHTML += `<li><strong>Total seconds:</strong> ${ (sign < 0 ? "-" : "") + totalSeconds }</li>`;

    const resultUtcEl = document.getElementById("result-utc");
    resultUtcEl.textContent =
      `Start (UTC): ${startUtc.toISOString().replace("T", " ").replace(".000Z", " UTC")}  |  ` +
      `End (UTC): ${endUtc.toISOString().replace("T", " ").replace(".000Z", " UTC")}`;

    document.getElementById("results-section").classList.remove("hidden");
  }

  function toggleFormat(is12) {
    const btn12 = document.getElementById("format-12");
    const btn24 = document.getElementById("format-24");
    const startAmpm = document.getElementById("start-ampm");
    const endAmpm = document.getElementById("end-ampm");
    const startTime = document.getElementById("start-time");
    const endTime = document.getElementById("end-time");

    if (is12) {
      btn12.classList.add("bg-blue-600", "text-white");
      btn12.setAttribute("aria-pressed", "true");
      btn24.classList.remove("bg-blue-600", "text-white");
      btn24.setAttribute("aria-pressed", "false");

      // Convert existing values to 12h
      [startTime, endTime].forEach((input, idx) => {
        if (!input.value) return;
        let [h, m] = input.value.split(":").map(v => parseInt(v, 10));
        let ampm = "AM";
        if (h === 0) {
          h = 12;
          ampm = "AM";
        } else if (h === 12) {
          ampm = "PM";
        } else if (h > 12) {
          h = h - 12;
          ampm = "PM";
        }
        input.value = pad2(h) + ":" + pad2(m);
        (idx === 0 ? startAmpm : endAmpm).value = ampm;
      });

      startAmpm.classList.remove("hidden");
      endAmpm.classList.remove("hidden");
    } else {
      btn24.classList.add("bg-blue-600", "text-white");
      btn24.setAttribute("aria-pressed", "true");
      btn12.classList.remove("bg-blue-600", "text-white");
      btn12.setAttribute("aria-pressed", "false");

      // Convert existing values to 24h
      [startTime, endTime].forEach((input, idx) => {
        if (!input.value) return;
        let [h, m] = input.value.split(":").map(v => parseInt(v, 10));
        const ampm = (idx === 0 ? startAmpm : endAmpm).value;
        if (ampm === "PM" && h < 12) h += 12;
        if (ampm === "AM" && h === 12) h = 0;
        input.value = pad2(h) + ":" + pad2(m);
      });

      startAmpm.classList.add("hidden");
      endAmpm.classList.add("hidden");
    }
  }

  function swapStartEnd() {
    const ids = ["date", "time", "ampm", "tz"];
    const start = {};
    const end = {};
    ids.forEach(id => {
      start[id] = document.getElementById("start-" + id).value;
      end[id] = document.getElementById("end-" + id).value;
    });
    ids.forEach(id => {
      document.getElementById("start-" + id).value = end[id];
      document.getElementById("end-" + id).value = start[id];
    });
    updateOffsetBadges();
  }

  function setSameDate() {
    const startDate = document.getElementById("start-date").value;
    if (startDate) {
      document.getElementById("end-date").value = startDate;
    }
  }

  function addOneDayToEnd() {
    const endDateEl = document.getElementById("end-date");
    if (!endDateEl.value) return;
    const d = new Date(endDateEl.value + "T00:00:00");
    d.setDate(d.getDate() + 1);
    endDateEl.value = d.toISOString().slice(0, 10);
  }

  function clearAll() {
    ["start-date","start-time","end-date","end-time"].forEach(id => {
      document.getElementById(id).value = "";
    });
    document.getElementById("error-message").classList.add("hidden");
    document.getElementById("results-section").classList.add("hidden");
  }

  document.addEventListener("DOMContentLoaded", () => {
    const browserTz = getBrowserTimeZone();
    document.getElementById("local-tz-label").textContent = browserTz;

    const startTzSelect = document.getElementById("start-tz");
    const endTzSelect = document.getElementById("end-tz");
    buildTimeZoneOptions(startTzSelect, browserTz);
    buildTimeZoneOptions(endTzSelect, browserTz);
    updateOffsetBadges();

    document.getElementById("format-12").addEventListener("click", () => toggleFormat(true));
    document.getElementById("format-24").addEventListener("click", () => toggleFormat(false));

    document.getElementById("set-now-start").addEventListener("click", () => setDateTimeInputsToNow("start"));
    document.getElementById("set-now-end").addEventListener("click", () => setDateTimeInputsToNow("end"));

    document.getElementById("start-tz").addEventListener("change", updateOffsetBadges);
    document.getElementById("end-tz").addEventListener("change", updateOffsetBadges);

    document.getElementById("swap-btn").addEventListener("click", swapStartEnd);
    document.getElementById("set-same-date").addEventListener("click", setSameDate);
    document.getElementById("add-1-day").addEventListener("click", addOneDayToEnd);
    document.getElementById("clear-all").addEventListener("click", clearAll);

    document.getElementById("calculate-btn").addEventListener("click", calculateDifference);

    // Initialize with now for convenience
    setDateTimeInputsToNow("start");
    setDateTimeInputsToNow("end");
  });
</script>

