    <script>
      // --- Tax configuration (illustrative, based on recent SK & federal data) ---
      const TAX_CONFIG = {
        2024: {
          federal: {
            brackets: [
              { upTo: 55867, rate: 0.15 },
              { upTo: 111733, rate: 0.205 },
              { upTo: 173205, rate: 0.26 },
              { upTo: 246752, rate: 0.29 },
              { upTo: Infinity, rate: 0.33 },
            ],
            basicPersonalAmount: 15305,
            creditRate: 0.15,
          },
          sk: {
            brackets: [
              { upTo: 53438, rate: 0.105 },
              { upTo: 122728, rate: 0.125 },
              { upTo: Infinity, rate: 0.145 },
            ],
            basicPersonalAmountSingle: 18225,
            basicPersonalAmountMarried: 36450,
            creditRate: 0.105,
          },
          pstRate: 0.06,
        },
      };

      function formatCurrency(value) {
        if (!isFinite(value)) return "$0";
        return value.toLocaleString("en-CA", {
          style: "currency",
          currency: "CAD",
          maximumFractionDigits: 0,
        });
      }

      function formatPercent(value) {
        if (!isFinite(value)) return "0%";
        return (value * 100).toFixed(1) + "%";
      }

      function computeBracketTax(taxable, brackets) {
        let remaining = taxable;
        let tax = 0;
        let marginalRate = 0;
        let lastThreshold = 0;

        for (const bracket of brackets) {
          const span = Math.min(remaining, bracket.upTo - lastThreshold);
          if (span > 0) {
            tax += span * bracket.rate;
            remaining -= span;
            marginalRate = bracket.rate;
          }
          lastThreshold = bracket.upTo;
          if (remaining <= 0) break;
        }
        return { tax, marginalRate };
      }

      function calculateTax() {
        const year = document.getElementById("tax-year").value;
        const cfg = TAX_CONFIG[year];
        const filingStatus = document.getElementById("filing-status").value;

        const income = Math.max(
          0,
          Number(document.getElementById("employment-income").value) || 0
        );
        const rrsp = Math.max(
          0,
          Number(document.getElementById("rrsp").value) || 0
        );
        const otherDed = Math.max(
          0,
          Number(document.getElementById("other-deductions").value) || 0
        );

        const taxableIncome = Math.max(0, income - rrsp - otherDed);

        // Federal
        const fed = computeBracketTax(taxableIncome, cfg.federal.brackets);
        const fedCredit =
          cfg.federal.basicPersonalAmount * cfg.federal.creditRate;
        const netFederalTax = Math.max(0, fed.tax - fedCredit);

        // Saskatchewan
        const skBasic =
          filingStatus === "married"
            ? cfg.sk.basicPersonalAmountMarried
            : cfg.sk.basicPersonalAmountSingle;
        const sk = computeBracketTax(taxableIncome, cfg.sk.brackets);
        const skCredit = skBasic * cfg.sk.creditRate;
        const netSkTax = Math.max(0, sk.tax - skCredit);

        const totalIncomeTax = netFederalTax + netSkTax;
        const afterTaxIncome = income - totalIncomeTax;

        const avgRate = taxableIncome > 0 ? totalIncomeTax / taxableIncome : 0;
        const marginalRate = fed.marginalRate + sk.marginalRate;

        // PST
        const includePst = document.getElementById("include-pst").checked;
        const pstAmountInput = Math.max(
          0,
          Number(document.getElementById("pst-amount").value) || 0
        );
        const pst = includePst ? pstAmountInput * cfg.pstRate : 0;
        const pstTotal = includePst ? pstAmountInput + pst : 0;

        // Update outputs
        document.getElementById("out-taxable-income").textContent =
          formatCurrency(taxableIncome);
        document.getElementById("out-federal-tax").textContent =
          formatCurrency(netFederalTax);
        document.getElementById("out-sk-tax").textContent =
          formatCurrency(netSkTax);
        document.getElementById("out-total-tax").textContent =
          formatCurrency(totalIncomeTax);
        document.getElementById("out-after-tax").textContent =
          formatCurrency(afterTaxIncome);
        document.getElementById("out-average-rate").textContent =
          formatPercent(avgRate);
        document.getElementById("out-marginal-rate").textContent =
          formatPercent(marginalRate);

        document.getElementById("out-pst").textContent = formatCurrency(pst);
        document.getElementById("out-pst-total").textContent = pstTotal
          ? formatCurrency(pstTotal)
          : "$0";
      }

      function updatePstVisibility() {
        const includePst = document.getElementById("include-pst").checked;
        const section = document.getElementById("pst-section");
        if (includePst) {
          section.classList.remove("hidden");
        } else {
          section.classList.add("hidden");
        }
      }

      function buildBracketTable() {
        const year = document.getElementById("tax-year").value;
        const cfg = TAX_CONFIG[year];
        const tbody = document.getElementById("bracket-table-body");
        tbody.innerHTML = "";

        function addRows(systemName, brackets) {
          let prev = 0;
          brackets.forEach((b, idx) => {
            const tr = document.createElement("tr");
            tr.className = idx % 2 === 0 ? "bg-white" : "bg-gray-50";
            const range =
              b.upTo === Infinity
                ? formatCurrency(prev + 1) + " and up"
                : formatCurrency(prev + 1) + " – " + formatCurrency(b.upTo);
            tr.innerHTML = `
          <td class="px-3 py-2 border-b">${systemName}</td>
          <td class="px-3 py-2 border-b">${range}</td>
          <td class="px-3 py-2 border-b">${(b.rate * 100).toFixed(1)}%</td>
        `;
            tbody.appendChild(tr);
            prev = b.upTo === Infinity ? prev : b.upTo;
          });
        }

        addRows("Federal", cfg.federal.brackets);
        addRows("Saskatchewan", cfg.sk.brackets);
      }

      document.addEventListener("DOMContentLoaded", () => {
        buildBracketTable();
        updatePstVisibility();
        calculateTax();

        document
          .getElementById("calculate-btn")
          .addEventListener("click", calculateTax);
        document
          .getElementById("include-pst")
          .addEventListener("change", () => {
            updatePstVisibility();
            calculateTax();
          });

        [
          "tax-year",
          "filing-status",
          "employment-income",
          "rrsp",
          "other-deductions",
          "pst-amount",
        ].forEach((id) => {
          const el = document.getElementById(id);
          if (el) {
            el.addEventListener("change", calculateTax);
            el.addEventListener("input", () => {
              // lightweight live update for sliders/typing
              calculateTax();
            });
          }
        });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
