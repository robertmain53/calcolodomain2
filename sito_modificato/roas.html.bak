    <script>
      // Utility functions
      function parseNumber(val) {
        if (val === "" || val === null || typeof val === "undefined") return 0;
        const n = parseFloat(val.toString().replace(/,/g, ""));
        return isNaN(n) ? 0 : n;
      }

      function formatCurrency(n, symbol) {
        if (n === null || typeof n === "undefined" || isNaN(n))
          return symbol + "0.00";
        const sign = n < 0 ? "-" : "";
        const abs = Math.abs(n);
        return (
          sign +
          symbol +
          abs.toLocaleString(undefined, {
            minimumFractionDigits: 2,
            maximumFractionDigits: 2,
          })
        );
      }

      function formatPercent(n) {
        if (n === null || typeof n === "undefined" || isNaN(n)) return "–";
        return n.toFixed(1).replace(/\.0$/, "") + "%";
      }

      const channelRowsTbody = document.getElementById("channel-rows");
      const currencySelect = document.getElementById("currency");
      const targetRoasInput = document.getElementById("target-roas");
      const grossMarginInput = document.getElementById("gross-margin");

      const totalSpendCell = document.getElementById("total-spend-cell");
      const totalRevenueCell = document.getElementById("total-revenue-cell");
      const totalCogsCell = document.getElementById("total-cogs-cell");
      const totalRoasCell = document.getElementById("total-roas-cell");
      const totalProfitCell = document.getElementById("total-profit-cell");
      const totalPoasCell = document.getElementById("total-poas-cell");

      const summaryRoas = document.getElementById("summary-roas");
      const summaryRoasStatus = document.getElementById("summary-roas-status");
      const summaryProfit = document.getElementById("summary-profit");
      const summaryBreakeven = document.getElementById("summary-breakeven");

      function createRow(name = "") {
        const tr = document.createElement("tr");
        tr.className = "border-t border-gray-200";

        tr.innerHTML = `
      <td class="px-3 py-2">
        <input type="text" class="input-field text-sm" placeholder="e.g. Google Search" value="${name}">
      </td>
      <td class="px-3 py-2 text-right">
        <input type="number" min="0" step="0.01" class="input-field text-right text-sm spend-input" placeholder="0.00">
      </td>
      <td class="px-3 py-2 text-right">
        <input type="number" min="0" step="0.01" class="input-field text-right text-sm revenue-input" placeholder="0.00">
      </td>
      <td class="px-3 py-2 text-right">
        <input type="number" min="0" step="0.01" class="input-field text-right text-sm cogs-input" placeholder="0.00">
      </td>
      <td class="px-3 py-2 text-right">
        <span class="text-sm roas-output">–</span>
      </td>
      <td class="px-3 py-2 text-right">
        <span class="text-sm profit-output">–</span>
      </td>
      <td class="px-3 py-2 text-right">
        <span class="text-sm poas-output">–</span>
      </td>
      <td class="px-3 py-2 text-center">
        <span class="text-xs px-2 py-1 rounded-full status-badge bg-gray-100 text-gray-600 border border-gray-200">n/a</span>
      </td>
      <td class="px-3 py-2 text-center">
        <button type="button" class="text-xs text-red-600 hover:underline remove-row-btn">Remove</button>
      </td>
    `;

        channelRowsTbody.appendChild(tr);
      }

      function recalc() {
        const currency = currencySelect.value || "$";
        const targetRoasPct = parseNumber(targetRoasInput.value);
        const targetRoas = targetRoasPct > 0 ? targetRoasPct / 100 : null;
        const grossMarginPct = parseNumber(grossMarginInput.value);
        const grossMargin = grossMarginPct > 0 ? grossMarginPct / 100 : null;

        let totalSpend = 0;
        let totalRevenue = 0;
        let totalCogs = 0;
        let totalProfit = 0;

        const rows = Array.from(channelRowsTbody.querySelectorAll("tr"));

        rows.forEach((row) => {
          const spendInput = row.querySelector(".spend-input");
          const revenueInput = row.querySelector(".revenue-input");
          const cogsInput = row.querySelector(".cogs-input");
          const roasOutput = row.querySelector(".roas-output");
          const profitOutput = row.querySelector(".profit-output");
          const poasOutput = row.querySelector(".poas-output");
          const statusBadge = row.querySelector(".status-badge");

          const spend = parseNumber(spendInput.value);
          const revenue = parseNumber(revenueInput.value);
          let cogs = parseNumber(cogsInput.value);

          // Estimate COGS from margin if not provided
          if (!cogs && grossMargin && revenue) {
            cogs = revenue * (1 - grossMargin);
          }

          const profit = revenue - spend - cogs;
          const roas = spend > 0 ? revenue / spend : null;
          const poas = spend > 0 ? profit / spend : null;

          totalSpend += spend;
          totalRevenue += revenue;
          totalCogs += cogs;
          totalProfit += profit;

          // Update row outputs
          roasOutput.textContent =
            roas !== null ? formatPercent(roas * 100) : "–";
          profitOutput.textContent =
            revenue || cogs || spend ? formatCurrency(profit, currency) : "–";
          poasOutput.textContent =
            poas !== null ? formatPercent(poas * 100) : "–";

          // Status badge
          statusBadge.className =
            "text-xs px-2 py-1 rounded-full status-badge border";
          if (roas === null || !targetRoas) {
            statusBadge.textContent = "n/a";
            statusBadge.classList.add(
              "bg-gray-100",
              "text-gray-600",
              "border-gray-200"
            );
          } else if (roas >= targetRoas) {
            statusBadge.textContent = "Meets target";
            statusBadge.classList.add(
              "bg-green-100",
              "text-green-700",
              "border-green-200"
            );
          } else {
            statusBadge.textContent = "Below target";
            statusBadge.classList.add(
              "bg-red-100",
              "text-red-700",
              "border-red-200"
            );
          }
        });

        // Totals
        totalSpendCell.textContent = formatCurrency(totalSpend, currency);
        totalRevenueCell.textContent = formatCurrency(totalRevenue, currency);
        totalCogsCell.textContent = formatCurrency(totalCogs, currency);
        totalProfitCell.textContent = formatCurrency(totalProfit, currency);

        const totalRoas = totalSpend > 0 ? totalRevenue / totalSpend : null;
        const totalPoas = totalSpend > 0 ? totalProfit / totalSpend : null;

        totalRoasCell.textContent =
          totalRoas !== null ? formatPercent(totalRoas * 100) : "–";
        totalPoasCell.textContent =
          totalPoas !== null ? formatPercent(totalPoas * 100) : "–";

        // Summary cards
        if (totalRoas !== null) {
          summaryRoas.textContent = formatPercent(totalRoas * 100);
          if (targetRoas) {
            if (totalRoas >= targetRoas) {
              summaryRoasStatus.textContent =
                "Overall ROAS meets or exceeds your target.";
              summaryRoasStatus.className = "text-xs mt-1 text-green-700";
            } else {
              summaryRoasStatus.textContent =
                "Overall ROAS is below your target. Consider optimizing bids, creatives, or audiences.";
              summaryRoasStatus.className = "text-xs mt-1 text-red-700";
            }
          } else {
            summaryRoasStatus.textContent =
              "Set a target ROAS to see how you compare.";
            summaryRoasStatus.className = "text-xs mt-1 text-gray-500";
          }
        } else {
          summaryRoas.textContent = "–";
          summaryRoasStatus.textContent =
            "Enter spend and revenue to see ROAS.";
          summaryRoasStatus.className = "text-xs mt-1 text-gray-500";
        }

        summaryProfit.textContent =
          totalRevenue || totalSpend || totalCogs
            ? formatCurrency(totalProfit, currency)
            : "–";

        if (grossMargin) {
          const breakevenRoas = 1 / grossMargin;
          summaryBreakeven.textContent = formatPercent(breakevenRoas * 100);
        } else {
          summaryBreakeven.textContent = "–";
        }
      }

      document.getElementById("add-row-btn").addEventListener("click", () => {
        createRow();
        recalc();
      });

      document.getElementById("reset-btn").addEventListener("click", () => {
        channelRowsTbody.innerHTML = "";
        createDefaultRows();
        targetRoasInput.value = 400;
        grossMarginInput.value = "";
        recalc();
      });

      channelRowsTbody.addEventListener("input", (e) => {
        if (e.target.matches("input")) {
          recalc();
        }
      });

      channelRowsTbody.addEventListener("click", (e) => {
        if (e.target.classList.contains("remove-row-btn")) {
          const row = e.target.closest("tr");
          if (row && channelRowsTbody.children.length > 1) {
            row.remove();
            recalc();
          }
        }
      });

      [currencySelect, targetRoasInput, grossMarginInput].forEach((el) => {
        el.addEventListener("input", recalc);
        el.addEventListener("change", recalc);
      });

      function createDefaultRows() {
        createRow("Google Ads");
        createRow("Meta / Facebook Ads");
        createRow("Other / Display");
      }

      // Initialize
      createDefaultRows();
      recalc();
    </script>

