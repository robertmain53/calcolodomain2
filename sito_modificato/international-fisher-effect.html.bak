    <script>
      // Update labels when currencies change
      function updateCurrencyLabels() {
        const dom =
          document.getElementById("domesticCurrency").value.trim() ||
          "Domestic";
        const forCur =
          document.getElementById("foreignCurrency").value.trim() || "Foreign";
        const pair = `${dom.toUpperCase()} per 1 ${forCur.toUpperCase()}`;
        document.getElementById("spotLabel").textContent = pair;
        document.getElementById("expectedSpotLabel").textContent = pair;
      }

      document
        .getElementById("domesticCurrency")
        .addEventListener("input", updateCurrencyLabels);
      document
        .getElementById("foreignCurrency")
        .addEventListener("input", updateCurrencyLabels);
      updateCurrencyLabels();

      function showError(msg) {
        const el = document.getElementById("errorMsg");
        el.textContent = msg;
        el.classList.remove("hidden");
      }

      function clearError() {
        const el = document.getElementById("errorMsg");
        el.textContent = "";
        el.classList.add("hidden");
      }

      document.getElementById("swapBtn").addEventListener("click", () => {
        const domInput = document.getElementById("domesticCurrency");
        const forInput = document.getElementById("foreignCurrency");
        const domRate = document.getElementById("domesticRate");
        const forRate = document.getElementById("foreignRate");
        const spot = document.getElementById("spotRate");

        const tmpCur = domInput.value;
        domInput.value = forInput.value;
        forInput.value = tmpCur;

        const tmpRate = domRate.value;
        domRate.value = forRate.value;
        forRate.value = tmpRate;

        const spotVal = parseFloat(spot.value);
        if (!isNaN(spotVal) && spotVal !== 0) {
          spot.value = (1 / spotVal).toFixed(4);
        }

        updateCurrencyLabels();
        clearError();
      });

      document.getElementById("resetBtn").addEventListener("click", () => {
        document.getElementById("domesticCurrency").value = "USD";
        document.getElementById("foreignCurrency").value = "EUR";
        document.getElementById("spotRate").value = "1.1000";
        document.getElementById("horizonYears").value = "1";
        document.getElementById("domesticRate").value = "5";
        document.getElementById("foreignRate").value = "2";
        updateCurrencyLabels();
        clearError();
        document.getElementById("resultsSection").classList.add("hidden");
      });

      document.getElementById("calculateBtn").addEventListener("click", () => {
        clearError();
        const S0 = parseFloat(document.getElementById("spotRate").value);
        const idPct = parseFloat(document.getElementById("domesticRate").value);
        const ifPct = parseFloat(document.getElementById("foreignRate").value);

        if (isNaN(S0) || S0 <= 0) {
          showError("Please enter a valid positive current spot rate.");
          return;
        }
        if (isNaN(idPct) || isNaN(ifPct)) {
          showError("Please enter valid interest rates for both countries.");
          return;
        }

        const id = idPct / 100;
        const iff = ifPct / 100;

        const denom = 1 + iff;
        if (denom === 0) {
          showError("Foreign interest rate of -100% is not allowed.");
          return;
        }

        const expectedSpot = (S0 * (1 + id)) / denom;
        const pctChange = ((expectedSpot - S0) / S0) * 100;
        const approxDiff = (id - iff) * 100;

        document.getElementById("expectedSpotDisplay").textContent =
          expectedSpot.toFixed(4);
        document.getElementById("pctChangeDisplay").textContent =
          pctChange.toFixed(3) + " %";
        document.getElementById("approxDiffDisplay").textContent =
          approxDiff.toFixed(3) + " %";
        document.getElementById("exactPctDisplay").textContent =
          pctChange.toFixed(3) + " %";

        const dom = (
          document.getElementById("domesticCurrency").value.trim() || "Domestic"
        ).toUpperCase();
        const forCur = (
          document.getElementById("foreignCurrency").value.trim() || "Foreign"
        ).toUpperCase();

        const directionText = document.getElementById("directionText");
        const badge = document.getElementById("directionBadge");
        badge.classList.remove("hidden");

        if (pctChange > 0) {
          directionText.textContent = `The ${forCur} is expected to appreciate versus the ${dom}.`;
          badge.textContent = "Foreign appreciation";
          badge.className =
            "result-badge bg-green-100 text-green-800 border border-green-200";
        } else if (pctChange < 0) {
          directionText.textContent = `The ${forCur} is expected to depreciate versus the ${dom}.`;
          badge.textContent = "Foreign depreciation";
          badge.className =
            "result-badge bg-red-100 text-red-800 border border-red-200";
        } else {
          directionText.textContent = `No expected change in the ${dom}/${forCur} rate under IFE.`;
          badge.textContent = "No change";
          badge.className =
            "result-badge bg-gray-100 text-gray-800 border border-gray-200";
        }

        const interp = document.getElementById("interpretationText");
        interp.textContent =
          `Under the International Fisher Effect, with a domestic rate of ${idPct.toFixed(
            2
          )}% and a ` +
          `foreign rate of ${ifPct.toFixed(
            2
          )}%, the expected future spot rate is ` +
          `${expectedSpot.toFixed(
            4
          )} ${dom} per 1 ${forCur}, implying a ${pctChange.toFixed(3)}% ` +
          `change in the ${dom}/${forCur} rate.`;

        document.getElementById("resultsSection").classList.remove("hidden");
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
