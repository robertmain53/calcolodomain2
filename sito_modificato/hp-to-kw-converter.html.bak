    <script>
      (function () {
        const modeHpToKwBtn = document.getElementById("mode-hp-to-kw");
        const modeKwToHpBtn = document.getElementById("mode-kw-to-hp");
        const inputValueEl = document.getElementById("input-value");
        const outputValueEl = document.getElementById("output-value");
        const inputUnitLabel = document.getElementById("input-unit-label");
        const outputUnitLabel = document.getElementById("output-unit-label");
        const hpTypeEl = document.getElementById("hp-type");
        const noteEl = document.getElementById("converter-note");
        const btnConvert = document.getElementById("btn-convert");
        const btnSwap = document.getElementById("btn-swap");
        const btnClear = document.getElementById("btn-clear");
        const torqueValueEl = document.getElementById("torque-value");
        const torqueUnitEl = document.getElementById("torque-unit");
        const rpmValueEl = document.getElementById("rpm-value");
        const btnTorqueToHp = document.getElementById("btn-torque-to-hp");
        const quickTableBody = document.getElementById("quick-table-body");

        let mode = "hp-to-kw"; // or 'kw-to-hp'

        function getFactor() {
          const type = hpTypeEl.value;
          if (type === "metric") return 0.7355;
          if (type === "electrical") return 0.746;
          return 0.7457; // mechanical default
        }

        function updateNote() {
          const type = hpTypeEl.value;
          let txt = "";
          if (type === "metric") {
            txt = "Using metric horsepower (1 hp = 0.7355 kW).";
          } else if (type === "electrical") {
            txt = "Using electrical horsepower (1 hp = 0.7460 kW).";
          } else {
            txt = "Using mechanical horsepower (1 hp = 0.7457 kW).";
          }
          noteEl.textContent =
            txt + " Change the horsepower type above if needed.";
        }

        function convert() {
          const factor = getFactor();
          const val = parseFloat(inputValueEl.value);
          if (isNaN(val)) {
            outputValueEl.value = "";
            return;
          }
          let result;
          if (mode === "hp-to-kw") {
            result = val * factor;
          } else {
            result = val / factor;
          }
          // Format with up to 6 significant digits
          const absRes = Math.abs(result);
          let formatted;
          if (absRes === 0) {
            formatted = "0";
          } else if (absRes >= 0.01 && absRes < 100000) {
            formatted = result.toFixed(4).replace(/\.?0+$/, "");
          } else {
            formatted = result.toExponential(4);
          }
          outputValueEl.value = formatted;
        }

        function setMode(newMode) {
          mode = newMode;
          if (mode === "hp-to-kw") {
            modeHpToKwBtn.classList.add("toggle-btn-active");
            modeKwToHpBtn.classList.remove("toggle-btn-active");
            inputUnitLabel.textContent = "hp";
            outputUnitLabel.textContent = "kW";
          } else {
            modeKwToHpBtn.classList.add("toggle-btn-active");
            modeHpToKwBtn.classList.remove("toggle-btn-active");
            inputUnitLabel.textContent = "kW";
            outputUnitLabel.textContent = "hp";
          }
          convert();
        }

        function swapDirection() {
          setMode(mode === "hp-to-kw" ? "kw-to-hp" : "hp-to-kw");
          // Also swap values if both present
          if (inputValueEl.value && outputValueEl.value) {
            const temp = inputValueEl.value;
            inputValueEl.value = outputValueEl.value;
            outputValueEl.value = temp;
          }
          convert();
        }

        function clearAll() {
          inputValueEl.value = "";
          outputValueEl.value = "";
          torqueValueEl.value = "";
          rpmValueEl.value = "";
        }

        function torqueToHp() {
          const torque = parseFloat(torqueValueEl.value);
          const rpm = parseFloat(rpmValueEl.value);
          if (isNaN(torque) || isNaN(rpm)) return;
          let hp;
          if (torqueUnitEl.value === "lbft") {
            hp = (torque * rpm) / 5252;
          } else {
            // N·m
            hp = (torque * rpm) / 7127;
          }
          inputValueEl.value = hp.toFixed(3).replace(/\.?0+$/, "");
          setMode("hp-to-kw");
          convert();
        }

        function buildQuickTable() {
          const mechanicalFactor = 0.7457;
          const metricFactor = 0.7355;
          const hpValues = [25, 50, 75, 100, 150, 200, 250, 300];
          quickTableBody.innerHTML = "";
          hpValues.forEach((hp) => {
            const kwMech = hp * mechanicalFactor;
            const kwMetric = hp * metricFactor;
            const row = document.createElement("tr");
            row.innerHTML = `
          <td class="px-3 py-2">${hp}</td>
          <td class="px-3 py-2">${kwMech.toFixed(1)}</td>
          <td class="px-3 py-2">${kwMetric.toFixed(1)}</td>
        `;
            quickTableBody.appendChild(row);
          });
        }

        // Event listeners
        modeHpToKwBtn.addEventListener("click", () => setMode("hp-to-kw"));
        modeKwToHpBtn.addEventListener("click", () => setMode("kw-to-hp"));
        btnConvert.addEventListener("click", convert);
        btnSwap.addEventListener("click", swapDirection);
        btnClear.addEventListener("click", clearAll);
        hpTypeEl.addEventListener("change", () => {
          updateNote();
          convert();
        });
        inputValueEl.addEventListener("input", convert);
        btnTorqueToHp.addEventListener("click", torqueToHp);

        // Initialize
        updateNote();
        buildQuickTable();
      })();
    </script>

