    <script>
      // Utility: unit conversions
      const GPa_TO_Pa = 1e9;
      const MPa_TO_Pa = 1e6;
      const PSI_TO_Pa = 6894.757293168;
      const M_TO_MM = 1000;
      const FT_TO_M = 0.3048;
      const IN_TO_M = 0.0254;
      const N_TO_KN = 0.001;
      const N_TO_LB = 0.2248089431;
      const N_TO_KIP = 0.0002248089431;

      function lengthToMeters(value, unit) {
        if (unit === "m") return value;
        if (unit === "mm") return value / 1000;
        if (unit === "ft") return value * FT_TO_M;
        if (unit === "in") return value * IN_TO_M;
        return value;
      }

      function EToPa(value, unit) {
        if (unit === "GPa") return value * GPa_TO_Pa;
        if (unit === "MPa") return value * MPa_TO_Pa;
        if (unit === "psi") return value * PSI_TO_Pa;
        if (unit === "Pa") return value;
        return value;
      }

      function IToM4(value, unit) {
        if (unit === "m4") return value;
        if (unit === "mm4") return value * Math.pow(1e-3, 4);
        if (unit === "in4") return value * Math.pow(IN_TO_M, 4);
        return value;
      }

      function forceToPreferredUnits(N) {
        // Return object with kN and lb
        return {
          kN: N * N_TO_KN,
          lb: N * N_TO_LB,
        };
      }

      function formatNumber(x, decimals = 3) {
        if (!isFinite(x)) return "–";
        if (Math.abs(x) >= 1e4 || (Math.abs(x) <= 1e-3 && x !== 0)) {
          return x.toExponential(decimals);
        }
        return x.toFixed(decimals);
      }

      document.addEventListener("DOMContentLoaded", () => {
        const materialSelect = document.getElementById("material-select");
        const customEWrapper = document.getElementById("custom-e-wrapper");
        const EInput = document.getElementById("E");
        const EUnit = document.getElementById("E-unit");

        const endCondition = document.getElementById("end-condition");
        const customKWrapper = document.getElementById("custom-k-wrapper");
        const KInput = document.getElementById("K");

        const sectionType = document.getElementById("section-type");
        const sectionInputs = document.querySelectorAll(".section-inputs");

        const calcBtn = document.getElementById("calculate-btn");
        const resetBtn = document.getElementById("reset-btn");
        const errorSpan = document.getElementById("calc-error");
        const resultsSection = document.getElementById("results-section");

        const resultPcrMain = document.getElementById("result-pcr-main");
        const resultPcrAlt = document.getElementById("result-pcr-alt");
        const resultLeff = document.getElementById("result-leff");
        const resultSlenderness = document.getElementById("result-slenderness");
        const resultDesignLoad = document.getElementById("result-design-load");
        const resultAppliedCheck = document.getElementById(
          "result-applied-check"
        );

        // Material selection behavior
        materialSelect.addEventListener("change", () => {
          const val = materialSelect.value;
          if (val === "custom") {
            customEWrapper.classList.remove("hidden");
            EInput.value = "";
          } else {
            customEWrapper.classList.remove("hidden");
            // Pre-fill E in GPa
            EInput.value = val;
            EUnit.value = "GPa";
          }
        });

        // End condition behavior
        endCondition.addEventListener("change", () => {
          if (endCondition.value === "custom") {
            customKWrapper.classList.remove("hidden");
          } else {
            customKWrapper.classList.add("hidden");
          }
        });

        // Section type behavior
        sectionType.addEventListener("change", () => {
          sectionInputs.forEach((el) => el.classList.add("hidden"));
          const type = sectionType.value;
          if (type === "rect") {
            document
              .getElementById("section-inputs-rect")
              .classList.remove("hidden");
          } else if (type === "circular") {
            document
              .getElementById("section-inputs-circular")
              .classList.remove("hidden");
          } else if (type === "tube") {
            document
              .getElementById("section-inputs-tube")
              .classList.remove("hidden");
          } else if (type === "custom") {
            document
              .getElementById("section-inputs-custom")
              .classList.remove("hidden");
          }
        });

        function computeIandA() {
          const type = sectionType.value;
          let I = null;
          let A = null;

          if (type === "rect") {
            const bVal = parseFloat(document.getElementById("b-rect").value);
            const bUnit = document.getElementById("b-rect-unit").value;
            const hVal = parseFloat(document.getElementById("h-rect").value);
            const hUnit = document.getElementById("h-rect-unit").value;
            if (bVal > 0 && hVal > 0) {
              const b = lengthToMeters(bVal, bUnit);
              const h = lengthToMeters(hVal, hUnit);
              A = b * h;
              I = (b * Math.pow(h, 3)) / 12;
            }
          } else if (type === "circular") {
            const dVal = parseFloat(document.getElementById("d-circ").value);
            const dUnit = document.getElementById("d-circ-unit").value;
            if (dVal > 0) {
              const d = lengthToMeters(dVal, dUnit);
              A = (Math.PI * Math.pow(d, 2)) / 4;
              I = (Math.PI * Math.pow(d, 4)) / 64;
            }
          } else if (type === "tube") {
            const doVal = parseFloat(document.getElementById("do-tube").value);
            const doUnit = document.getElementById("do-tube-unit").value;
            const diVal = parseFloat(document.getElementById("di-tube").value);
            const diUnit = document.getElementById("di-tube-unit").value;
            if (doVal > 0 && diVal >= 0) {
              const d_o = lengthToMeters(doVal, doUnit);
              const d_i = lengthToMeters(diVal, diUnit);
              if (d_i >= d_o) {
                throw new Error(
                  "Inner diameter must be smaller than outer diameter."
                );
              }
              A = (Math.PI * (Math.pow(d_o, 2) - Math.pow(d_i, 2))) / 4;
              I = (Math.PI * (Math.pow(d_o, 4) - Math.pow(d_i, 4))) / 64;
            }
          } else if (type === "custom") {
            const IVal = parseFloat(document.getElementById("I-custom").value);
            const IUnit = document.getElementById("I-custom-unit").value;
            if (IVal > 0) {
              I = IToM4(IVal, IUnit);
            }
            // A is unknown for custom I; slenderness ratio will be omitted
          }

          return { I, A };
        }

        function calculate() {
          errorSpan.textContent = "";
          try {
            // Length and K
            const LVal = parseFloat(document.getElementById("length").value);
            const LUnit = document.getElementById("length-unit").value;
            if (!(LVal > 0))
              throw new Error("Please enter a positive column length.");
            const L = lengthToMeters(LVal, LUnit);

            let K;
            if (endCondition.value === "custom") {
              const KVal = parseFloat(KInput.value);
              if (!(KVal > 0))
                throw new Error(
                  "Please enter a positive effective length factor K."
                );
              K = KVal;
            } else {
              K = parseFloat(endCondition.value);
            }

            // Material E
            let EVal;
            let Eunit;
            if (materialSelect.value === "custom") {
              EVal = parseFloat(EInput.value);
              Eunit = EUnit.value;
              if (!(EVal > 0))
                throw new Error("Please enter a positive Young’s modulus E.");
            } else {
              // Predefined material in GPa
              EVal = parseFloat(materialSelect.value);
              Eunit = "GPa";
            }
            const E = EToPa(EVal, Eunit);

            // Section properties
            const { I, A } = computeIandA();
            if (!(I > 0))
              throw new Error(
                "Please provide valid cross-section dimensions or I."
              );

            const Le = K * L;
            const Pcr = (Math.PI * Math.PI * E * I) / (Le * Le); // in N

            // Slenderness ratio if A known
            let slendernessText = "–";
            if (A && A > 0) {
              const r = Math.sqrt(I / A);
              const slenderness = Le / r;
              slendernessText = formatNumber(slenderness, 1);
            }

            // Format results
            const forces = forceToPreferredUnits(Pcr);
            resultPcrMain.textContent = formatNumber(forces.kN, 3) + " kN";
            resultPcrAlt.textContent =
              "(" + formatNumber(forces.lb, 1) + " lb)";

            resultLeff.textContent = formatNumber(Le, 3) + " m";
            resultSlenderness.textContent = slendernessText;

            // Safety factor & design load
            const sfVal = parseFloat(
              document.getElementById("safety-factor").value
            );
            if (sfVal && sfVal > 1) {
              const Pdesign = Pcr / sfVal;
              const f = forceToPreferredUnits(Pdesign);
              resultDesignLoad.textContent =
                formatNumber(f.kN, 3) +
                " kN (" +
                formatNumber(f.lb, 1) +
                " lb) with SF = " +
                formatNumber(sfVal, 2);
            } else {
              resultDesignLoad.textContent =
                "Provide a safety factor to compute design load.";
            }

            // Applied load check
            const appliedVal = parseFloat(
              document.getElementById("applied-load").value
            );
            const appliedUnit =
              document.getElementById("applied-load-unit").value;
            if (appliedVal && appliedVal > 0) {
              let PappN;
              if (appliedUnit === "N") PappN = appliedVal;
              else if (appliedUnit === "kN") PappN = appliedVal / N_TO_KN;
              else if (appliedUnit === "lb") PappN = appliedVal / N_TO_LB;
              else if (appliedUnit === "kip") PappN = appliedVal / N_TO_KIP;
              const sfBuckling = Pcr / PappN;
              let status;
              if (sfBuckling < 1) {
                status = "Warning: applied load exceeds Euler buckling load.";
              } else if (sfBuckling < 2) {
                status = "Caution: low buckling safety factor.";
              } else {
                status = "OK: applied load is below Euler buckling load.";
              }
              resultAppliedCheck.textContent =
                "Buckling safety factor = " +
                formatNumber(sfBuckling, 2) +
                ". " +
                status;
            } else {
              resultAppliedCheck.textContent =
                "Enter an applied load to check buckling safety factor.";
            }

            resultsSection.classList.remove("hidden");
          } catch (err) {
            errorSpan.textContent =
              err.message ||
              "Unable to compute buckling load. Check your inputs.";
            resultsSection.classList.add("hidden");
          }
        }

        calcBtn.addEventListener("click", calculate);

        resetBtn.addEventListener("click", () => {
          document.getElementById("buckling-form").reset();
          errorSpan.textContent = "";
          resultsSection.classList.add("hidden");
          // Reset visibility
          customEWrapper.classList.add("hidden");
          customKWrapper.classList.add("hidden");
          sectionInputs.forEach((el) => el.classList.add("hidden"));
          document
            .getElementById("section-inputs-rect")
            .classList.remove("hidden");
          // Restore defaults
          document.getElementById("length").value = 3;
          document.getElementById("length-unit").value = "m";
          materialSelect.value = "200";
        });
      });
    </script>

