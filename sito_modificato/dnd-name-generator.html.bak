      function choice(arr) {
        return arr[Math.floor(Math.random() * arr.length)];
      }

      function buildBaseName(raceKey, genderKey) {
        const race = nameData[raceKey] || nameData.generic;
        let pool = [];
        if (genderKey === "male") pool = race.male;
        else if (genderKey === "female") pool = race.female;
        else if (genderKey === "unisex") pool = [...race.male, ...race.female];
        else pool = [...race.male, ...race.female];
        return choice(pool);
      }

      function buildSurname(raceKey) {
        const race = nameData[raceKey] || nameData.generic;
        if (!race.surname) return "";
        return choice(race.surname);
      }

      function applyTheme(name, theme) {
        if (!theme || theme === "any") return name;
        const mods = themeModifiers[theme];
        if (!mods) return name;
        // 50% chance to prepend, 50% to append
        if (Math.random() < 0.5) {
          return choice(mods) + name;
        } else {
          return name + " " + choice(mods);
        }
      }

      function tweakForClass(name, cls) {
        if (!cls || cls === "any") return name;
        // Very light-touch: add apostrophes or extra consonants for some classes
        if (cls === "rogue" || cls === "npc" || cls === "druid") return name;
        if (cls === "wizard" || cls === "warlock") {
          return name
            .replace(/a/g, "ae")
            .replace(/o/g, "o'")
            .replace(/ii/g, "i");
        }
        if (cls === "fighter") {
          return name.replace(/th/g, "dr").replace(/l/g, "rk");
        }
        if (cls === "cleric" || cls === "bard") {
          return name + (Math.random() < 0.5 ? "us" : "iel");
        }
        return name;
      }

      function generateSingleName(options) {
        const { race, gender, cls, theme, includeSurname } = options;
        const raceKey = race === "any" ? choice(Object.keys(nameData)) : race;
        const genderKey = gender || "any";

        let first = buildBaseName(raceKey, genderKey);
        first = applyTheme(first, theme);
        first = tweakForClass(first, cls);

        let surname = "";
        if (includeSurname) {
          surname = buildSurname(raceKey);
        }

        return surname ? `${first} ${surname}` : first;
      }

      function generateNames(count, options) {
        const results = new Set();
        let safety = 0;
        while (results.size < count && safety < count * 10) {
          results.add(generateSingleName(options));
          safety++;
        }
        return Array.from(results);
      }

      function updateResultsList(names) {
        const container = document.getElementById("results-container");
        const countLabel = document.getElementById("result-count");
        container.innerHTML = "";
        if (!names.length) {
          container.textContent = "No names generated yet.";
          countLabel.textContent = "";
          return;
        }
        const ul = document.createElement("ul");
        ul.className = "space-y-1";
        names.forEach((name, idx) => {
          const li = document.createElement("li");
          li.className = "flex items-center justify-between gap-2";
          const span = document.createElement("span");
          span.textContent = `${idx + 1}. ${name}`;
          const copyBtn = document.createElement("button");
          copyBtn.textContent = "Copy";
          copyBtn.className =
            "text-xs px-2 py-0.5 border border-gray-300 rounded hover:bg-gray-100";
          copyBtn.addEventListener("click", () => {
            navigator.clipboard.writeText(name).catch(() => {});
          });
          li.appendChild(span);
          li.appendChild(copyBtn);
          ul.appendChild(li);
        });
        container.appendChild(ul);
        countLabel.textContent = `${names.length} name${
          names.length === 1 ? "" : "s"
        } generated`;
      }

      document.addEventListener("DOMContentLoaded", () => {
        const raceEl = document.getElementById("race");
        const genderEl = document.getElementById("gender");
        const classEl = document.getElementById("class");
        const themeEl = document.getElementById("theme");
        const countEl = document.getElementById("count");
        const countLabelEl = document.getElementById("count-label");
        const includeSurnameEl = document.getElementById("include-surname");
        const generateBtn = document.getElementById("generate-btn");
        const copyAllBtn = document.getElementById("copy-all-btn");
        const downloadBtn = document.getElementById("download-btn");
        const presetButtons = document.querySelectorAll("button[data-preset]");

        let lastNames = [];

        countEl.addEventListener("input", () => {
          countLabelEl.textContent = countEl.value;
        });

        generateBtn.addEventListener("click", () => {
          const options = {
            race: raceEl.value,
            gender: genderEl.value,
            cls: classEl.value,
            theme: themeEl.value,
            includeSurname: includeSurnameEl.checked,
          };
          const count = parseInt(countEl.value, 10) || 10;
          lastNames = generateNames(count, options);
          updateResultsList(lastNames);
        });

        copyAllBtn.addEventListener("click", () => {
          if (!lastNames.length) return;
          const text = lastNames.join("\n");
          navigator.clipboard.writeText(text).catch(() => {});
        });

        downloadBtn.addEventListener("click", () => {
          if (!lastNames.length) return;
          const blob = new Blob([lastNames.join("\n")], { type: "text/plain" });
          const url = URL.createObjectURL(blob);
          const a = document.createElement("a");
          a.href = url;
          a.download = "dnd-names.txt";
          document.body.appendChild(a);
          a.click();
          document.body.removeChild(a);
          URL.revokeObjectURL(url);
        });

        presetButtons.forEach((btn) => {
          btn.addEventListener("click", () => {
            const preset = btn.getAttribute("data-preset");
            switch (preset) {
              case "elf-hero":
                raceEl.value = "elf";
                genderEl.value = "any";
                classEl.value = "wizard";
                themeEl.value = "heroic";
                break;
              case "dwarf-cleric":
                raceEl.value = "dwarf";
                genderEl.value = "any";
                classEl.value = "cleric";
                themeEl.value = "heroic";
                break;
              case "tiefling-warlock":
                raceEl.value = "tiefling";
                genderEl.value = "any";
                classEl.value = "warlock";
                themeEl.value = "dark";
                break;
              case "human-rogue":
                raceEl.value = "human";
                genderEl.value = "any";
                classEl.value = "rogue";
                themeEl.value = "gritty";
                break;
              case "orc-villain":
                raceEl.value = "orc";
                genderEl.value = "any";
                classEl.value = "fighter";
                themeEl.value = "dark";
                break;
              case "npc-town":
                raceEl.value = "human";
                genderEl.value = "any";
                classEl.value = "npc";
                themeEl.value = "any";
                break;
            }
            countEl.value = 15;
            countLabelEl.textContent = "15";
            includeSurnameEl.checked = true;
            generateBtn.click();
          });
        });
      });
    </script>

