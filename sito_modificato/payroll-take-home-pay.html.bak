    <script>
      function fmt(n) {
        if (!isFinite(n)) return "$0.00";
        return n.toLocaleString("en-US", {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 2,
        });
      }
      function pct(n) {
        if (!isFinite(n)) return "0.0%";
        return (n * 100).toFixed(1) + "%";
      }

      const payTypeSalaryBtn = document.getElementById("payTypeSalary");
      const payTypeHourlyBtn = document.getElementById("payTypeHourly");
      const salaryContainer = document.getElementById("salaryContainer");
      const hourlyContainer = document.getElementById("hourlyContainer");

      let payType = "salary";

      function setPayType(type) {
        payType = type;
        if (type === "salary") {
          payTypeSalaryBtn.classList.add(
            "border-blue-500",
            "bg-blue-50",
            "text-blue-700",
            "font-medium"
          );
          payTypeSalaryBtn.classList.remove("border-gray-300", "text-gray-700");
          payTypeHourlyBtn.classList.remove(
            "border-blue-500",
            "bg-blue-50",
            "text-blue-700",
            "font-medium"
          );
          payTypeHourlyBtn.classList.add("border-gray-300", "text-gray-700");
          salaryContainer.classList.remove("hidden");
          hourlyContainer.classList.add("hidden");
        } else {
          payTypeHourlyBtn.classList.add(
            "border-blue-500",
            "bg-blue-50",
            "text-blue-700",
            "font-medium"
          );
          payTypeHourlyBtn.classList.remove("border-gray-300", "text-gray-700");
          payTypeSalaryBtn.classList.remove(
            "border-blue-500",
            "bg-blue-50",
            "text-blue-700",
            "font-medium"
          );
          payTypeSalaryBtn.classList.add("border-gray-300", "text-gray-700");
          salaryContainer.classList.add("hidden");
          hourlyContainer.classList.remove("hidden");
        }
      }

      payTypeSalaryBtn.addEventListener("click", () => setPayType("salary"));
      payTypeHourlyBtn.addEventListener("click", () => setPayType("hourly"));

      function getPeriodsPerYear(freq) {
        switch (freq) {
          case "weekly":
            return 52;
          case "biweekly":
            return 26;
          case "semimonthly":
            return 24;
          case "monthly":
            return 12;
          case "annual":
            return 1;
          default:
            return 26;
        }
      }

      function getTaxRates(country, filingStatus, annualIncome) {
        // Simplified assumptions for demonstration
        let federal = 0.12;
        let state = 0.04;

        if (country === "generic") {
          federal = 0.15;
          state = 0.05;
        } else if (country === "us") {
          if (filingStatus === "married") {
            if (annualIncome > 200000) federal = 0.22;
            else if (annualIncome > 90000) federal = 0.18;
            else if (annualIncome > 40000) federal = 0.14;
            else federal = 0.1;
            state = 0.04;
          } else if (filingStatus === "head") {
            if (annualIncome > 150000) federal = 0.22;
            else if (annualIncome > 70000) federal = 0.18;
            else if (annualIncome > 30000) federal = 0.14;
            else federal = 0.1;
            state = 0.045;
          } else {
            if (annualIncome > 150000) federal = 0.24;
            else if (annualIncome > 80000) federal = 0.18;
            else if (annualIncome > 40000) federal = 0.14;
            else federal = 0.1;
            state = 0.045;
          }
        }

        const socialSecurity = 0.062;
        const medicare = 0.0145;

        return { federal, state, socialSecurity, medicare };
      }

      function calculate() {
        const country = document.getElementById("country").value;
        const frequency = document.getElementById("frequency").value;
        const filingStatus = document.getElementById("filingStatus").value;
        const errorEl = document.getElementById("calcError");

        const salaryAmount =
          parseFloat(document.getElementById("salaryAmount").value) || 0;
        const hourlyRate =
          parseFloat(document.getElementById("hourlyRate").value) || 0;
        const hoursPerWeek =
          parseFloat(document.getElementById("hoursPerWeek").value) || 0;

        const pretaxRetirement =
          parseFloat(document.getElementById("pretaxRetirement").value) || 0;
        const pretaxHealth =
          parseFloat(document.getElementById("pretaxHealth").value) || 0;
        const pretaxHsa =
          parseFloat(document.getElementById("pretaxHsa").value) || 0;
        const otherPretax =
          parseFloat(document.getElementById("otherPretax").value) || 0;

        const posttaxRoth =
          parseFloat(document.getElementById("posttaxRoth").value) || 0;
        const posttaxOther =
          parseFloat(document.getElementById("posttaxOther").value) || 0;

        const periodsPerYear = getPeriodsPerYear(frequency);

        let annualIncome = 0;
        if (payType === "salary") {
          annualIncome = salaryAmount;
        } else {
          annualIncome = hourlyRate * hoursPerWeek * 52;
        }

        if (!annualIncome || annualIncome <= 0) {
          errorEl.classList.remove("hidden");
          return;
        } else {
          errorEl.classList.add("hidden");
        }

        const grossPerPeriod = annualIncome / periodsPerYear;

        const totalPretaxPerPeriod =
          pretaxRetirement + pretaxHealth + pretaxHsa + otherPretax;
        const totalPosttaxPerPeriod = posttaxRoth + posttaxOther;

        const taxableIncomePerPeriod = Math.max(
          0,
          grossPerPeriod - totalPretaxPerPeriod
        );

        const rates = getTaxRates(country, filingStatus, annualIncome);
        const federalTaxPerPeriod = taxableIncomePerPeriod * rates.federal;
        const stateTaxPerPeriod = taxableIncomePerPeriod * rates.state;
        const ssTaxPerPeriod = taxableIncomePerPeriod * rates.socialSecurity;
        const medicareTaxPerPeriod = taxableIncomePerPeriod * rates.medicare;

        const totalTaxPerPeriod =
          federalTaxPerPeriod +
          stateTaxPerPeriod +
          ssTaxPerPeriod +
          medicareTaxPerPeriod;
        const totalDeductionsPerPeriod =
          totalPretaxPerPeriod + totalPosttaxPerPeriod + totalTaxPerPeriod;

        const netPerPeriod = Math.max(
          0,
          grossPerPeriod - totalDeductionsPerPeriod
        );
        const netPerYear = netPerPeriod * periodsPerYear;

        const effectiveTaxRate =
          grossPerPeriod > 0 ? totalTaxPerPeriod / grossPerPeriod : 0;

        document.getElementById("grossPerPeriod").textContent =
          fmt(grossPerPeriod);
        document.getElementById("netPerPeriod").textContent = fmt(netPerPeriod);
        document.getElementById("netPerYear").textContent = fmt(netPerYear);

        document.getElementById("federalTax").textContent =
          fmt(federalTaxPerPeriod);
        document.getElementById("stateTax").textContent =
          fmt(stateTaxPerPeriod);
        document.getElementById("ssTax").textContent = fmt(ssTaxPerPeriod);
        document.getElementById("medicareTax").textContent =
          fmt(medicareTaxPerPeriod);
        document.getElementById("totalTax").textContent =
          fmt(totalTaxPerPeriod);

        document.getElementById("totalPretax").textContent =
          fmt(totalPretaxPerPeriod);
        document.getElementById("totalPosttax").textContent = fmt(
          totalPosttaxPerPeriod
        );
        document.getElementById("totalDeductions").textContent = fmt(
          totalDeductionsPerPeriod
        );
        document.getElementById("effectiveRate").textContent =
          pct(effectiveTaxRate);
      }

      document
        .getElementById("calculateBtn")
        .addEventListener("click", calculate);

      document.getElementById("resetBtn").addEventListener("click", () => {
        document.getElementById("country").value = "us";
        document.getElementById("frequency").value = "biweekly";
        document.getElementById("filingStatus").value = "single";
        document.getElementById("salaryAmount").value = 60000;
        document.getElementById("hourlyRate").value = 25;
        document.getElementById("hoursPerWeek").value = 40;
        document.getElementById("pretaxRetirement").value = "";
        document.getElementById("pretaxHealth").value = "";
        document.getElementById("pretaxHsa").value = "";
        document.getElementById("otherPretax").value = "";
        document.getElementById("posttaxRoth").value = "";
        document.getElementById("posttaxOther").value = "";
        setPayType("salary");
        document.getElementById("calcError").classList.add("hidden");
        calculate();
      });

      [
        "country",
        "frequency",
        "filingStatus",
        "salaryAmount",
        "hourlyRate",
        "hoursPerWeek",
        "pretaxRetirement",
        "pretaxHealth",
        "pretaxHsa",
        "otherPretax",
        "posttaxRoth",
        "posttaxOther",
      ].forEach((id) => {
        const el = document.getElementById(id);
        if (el) {
          el.addEventListener("change", () => {
            if (
              id === "salaryAmount" ||
              id === "hourlyRate" ||
              id === "hoursPerWeek"
            ) {
              document.getElementById("calcError").classList.add("hidden");
            }
            calculate();
          });
          el.addEventListener("input", () => {
            document.getElementById("calcError").classList.add("hidden");
          });
        }
      });

      window.addEventListener("DOMContentLoaded", () => {
        setPayType("salary");
        calculate();
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
