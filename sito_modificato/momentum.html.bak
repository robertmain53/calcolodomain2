    <script>
      // Tab switching
      document.querySelectorAll(".mode-tab").forEach((btn) => {
        btn.addEventListener("click", () => {
          const mode = btn.getAttribute("data-mode");
          document.querySelectorAll(".mode-tab").forEach((b) => {
            b.classList.remove("border-blue-600", "text-blue-600");
            b.classList.add("border-transparent", "text-gray-600");
          });
          btn.classList.add("border-blue-600", "text-blue-600");
          btn.classList.remove("border-transparent", "text-gray-600");

          document
            .querySelectorAll(".mode-panel")
            .forEach((panel) => panel.classList.add("hidden"));
          const active = document.getElementById("mode-" + mode);
          if (active) active.classList.remove("hidden");
        });
      });

      // Unit conversion helpers
      function massToKg(value, unit) {
        if (!isFinite(value)) return NaN;
        switch (unit) {
          case "kg":
            return value;
          case "g":
            return value / 1000;
          case "lb":
            return value * 0.45359237;
          case "tonne":
            return value * 1000;
          default:
            return NaN;
        }
      }

      function velocityToMs(value, unit) {
        if (!isFinite(value)) return NaN;
        switch (unit) {
          case "m_s":
            return value;
          case "km_h":
            return value * (1000 / 3600);
          case "mph":
            return value * 0.44704;
          case "ft_s":
            return value * 0.3048;
          case "c_fraction":
            return value * 299792458;
          default:
            return NaN;
        }
      }

      function formatNumber(x, digits = 4) {
        if (!isFinite(x)) return "NaN";
        const abs = Math.abs(x);
        if (abs !== 0 && (abs >= 1e6 || abs < 1e-3)) {
          return x.toExponential(digits - 1);
        }
        return x.toLocaleString(undefined, { maximumFractionDigits: digits });
      }

      // Classical momentum
      document
        .getElementById("calcClassicalBtn")
        .addEventListener("click", () => {
          const mVal = parseFloat(document.getElementById("mass").value);
          const mUnit = document.getElementById("massUnit").value;
          const vVal = parseFloat(document.getElementById("velocity").value);
          const vUnit = document.getElementById("velocityUnit").value;

          const mKg = massToKg(mVal, mUnit);
          const vMs = velocityToMs(vVal, vUnit);

          if (!isFinite(mKg) || !isFinite(vMs) || mKg < 0) {
            alert(
              "Please enter valid, non-negative values for mass and velocity."
            );
            return;
          }

          const p = mKg * vMs;
          document.getElementById("classicalP").textContent = formatNumber(
            p,
            5
          );

          const interp = [];
          if (p === 0) {
            interp.push(
              "The object has zero momentum because either its mass or its velocity is zero."
            );
          } else {
            interp.push(
              `The object has a momentum of approximately ${formatNumber(
                p,
                4
              )} kg·m/s.`
            );
            if (mKg > 0 && vMs > 0) {
              interp.push(
                `Both its mass (${formatNumber(
                  mKg,
                  4
                )} kg) and speed (${formatNumber(
                  vMs,
                  4
                )} m/s) contribute linearly to this value.`
              );
            }
          }
          document.getElementById("classicalInterpretation").textContent =
            interp.join(" ");

          const steps = `1. Convert mass to kilograms: m = ${formatNumber(
            mKg,
            6
          )} kg.
2. Convert velocity to meters per second: v = ${formatNumber(vMs, 6)} m/s.
3. Apply p = m·v = ${formatNumber(mKg, 6)} × ${formatNumber(
            vMs,
            6
          )} ≈ ${formatNumber(p, 6)} kg·m/s.`;
          document.getElementById("classicalSteps").textContent = steps;

          document.getElementById("classicalResult").classList.remove("hidden");
        });

      document
        .getElementById("resetClassicalBtn")
        .addEventListener("click", () => {
          document.getElementById("mass").value = "";
          document.getElementById("velocity").value = "";
          document.getElementById("classicalResult").classList.add("hidden");
        });

      // Relativistic momentum
      document.getElementById("calcRelBtn").addEventListener("click", () => {
        const c = 299792458; // m/s
        const mVal = parseFloat(document.getElementById("relMass").value);
        const mUnit = document.getElementById("relMassUnit").value;
        const vVal = parseFloat(document.getElementById("relVelocity").value);
        const vUnit = document.getElementById("relVelocityUnit").value;

        const mKg = massToKg(mVal, mUnit);
        const vMs = velocityToMs(vVal, vUnit);

        if (!isFinite(mKg) || !isFinite(vMs) || mKg < 0 || vMs < 0) {
          alert(
            "Please enter valid, non-negative values for mass and velocity."
          );
          return;
        }
        if (vMs >= c) {
          alert("Velocity must be less than the speed of light.");
          return;
        }

        const beta = vMs / c;
        const gamma = 1 / Math.sqrt(1 - beta * beta);
        const p = gamma * mKg * vMs;

        document.getElementById("relP").textContent = formatNumber(p, 5);
        document.getElementById("gammaVal").textContent = formatNumber(
          gamma,
          5
        );
        document.getElementById("betaVal").textContent = formatNumber(beta, 5);

        let interp = `At v/c = ${formatNumber(
          beta,
          3
        )}, the Lorentz factor is γ ≈ ${formatNumber(gamma, 3)}. `;
        if (beta < 0.1) {
          interp +=
            "Relativistic effects are very small here; classical momentum would give almost the same result.";
        } else if (beta < 0.5) {
          interp +=
            "Relativistic effects are noticeable; momentum is moderately larger than the classical value m·v.";
        } else {
          interp +=
            "Relativistic effects are strong; momentum is much larger than the classical value m·v.";
        }
        document.getElementById("relInterpretation").textContent = interp;

        const steps = `1. Convert mass to kilograms: m = ${formatNumber(
          mKg,
          6
        )} kg.
2. Convert velocity to meters per second: v = ${formatNumber(vMs, 6)} m/s.
3. Compute β = v/c = ${formatNumber(beta, 6)}.
4. Compute γ = 1 / √(1 − β²) ≈ ${formatNumber(gamma, 6)}.
5. Compute p = γ·m·v ≈ ${formatNumber(gamma, 6)} × ${formatNumber(
          mKg,
          6
        )} × ${formatNumber(vMs, 6)} ≈ ${formatNumber(p, 6)} kg·m/s.`;
        document.getElementById("relSteps").textContent = steps;

        document.getElementById("relResult").classList.remove("hidden");
      });

      document.getElementById("resetRelBtn").addEventListener("click", () => {
        document.getElementById("relMass").value = "";
        document.getElementById("relVelocity").value = "";
        document.getElementById("relResult").classList.add("hidden");
      });

      // Compare two objects
      document
        .getElementById("calcCompareBtn")
        .addEventListener("click", () => {
          const mA = massToKg(
            parseFloat(document.getElementById("massA").value),
            document.getElementById("massAUnit").value
          );
          const vA = velocityToMs(
            parseFloat(document.getElementById("velA").value),
            document.getElementById("velAUnit").value
          );
          const mB = massToKg(
            parseFloat(document.getElementById("massB").value),
            document.getElementById("massBUnit").value
          );
          const vB = velocityToMs(
            parseFloat(document.getElementById("velB").value),
            document.getElementById("velBUnit").value
          );

          if (
            !isFinite(mA) ||
            !isFinite(vA) ||
            !isFinite(mB) ||
            !isFinite(vB) ||
            mA < 0 ||
            mB < 0
          ) {
            alert("Please enter valid, non-negative values for both objects.");
            return;
          }

          const pA = mA * vA;
          const pB = mB * vB;

          document.getElementById("pA").textContent = formatNumber(pA, 5);
          document.getElementById("pB").textContent = formatNumber(pB, 5);

          let interp = "";
          if (pA === pB) {
            interp = "Both objects have the same momentum.";
          } else if (Math.abs(pA) > Math.abs(pB)) {
            interp = "Object A has greater momentum than Object B.";
          } else {
            interp = "Object B has greater momentum than Object A.";
          }
          document.getElementById("compareInterpretation").textContent = interp;

          if (pA !== 0 && pB !== 0) {
            const ratio =
              Math.abs(pA) > Math.abs(pB)
                ? Math.abs(pA / pB)
                : Math.abs(pB / pA);
            const bigger = Math.abs(pA) > Math.abs(pB) ? "A" : "B";
            const smaller = bigger === "A" ? "B" : "A";
            document.getElementById(
              "compareRatio"
            ).textContent = `Object ${bigger} has about ${formatNumber(
              ratio,
              3
            )} times the momentum of Object ${smaller}.`;
          } else {
            document.getElementById("compareRatio").textContent = "";
          }

          document.getElementById("compareResult").classList.remove("hidden");
        });

      document
        .getElementById("resetCompareBtn")
        .addEventListener("click", () => {
          ["massA", "velA", "massB", "velB"].forEach(
            (id) => (document.getElementById(id).value = "")
          );
          document.getElementById("compareResult").classList.add("hidden");
        });

      // Simple FAQ toggle (in addition to global script_faq if present)
      document
        .querySelectorAll("#faq-container .faq-question")
        .forEach((btn) => {
          btn.addEventListener("click", () => {
            const answer = btn.parentElement.querySelector(".faq-answer");
            const isHidden = answer.classList.contains("hidden");
            document
              .querySelectorAll("#faq-container .faq-answer")
              .forEach((a) => a.classList.add("hidden"));
            document
              .querySelectorAll("#faq-container .faq-question span:last-child")
              .forEach((icon) => (icon.textContent = "+"));
            if (isHidden) {
              answer.classList.remove("hidden");
              btn.querySelector("span:last-child").textContent = "−";
            }
          });
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
