    <script>
      (function () {
        const FACTOR = 35.3146667; // 1 m³ = 35.3146667 ft³

        const m3Input = document.getElementById("cubicMeters");
        const ft3Input = document.getElementById("cubicFeet");
        const swapBtn = document.getElementById("swapBtn");
        const clearBtn = document.getElementById("clearBtn");
        const resultSummary = document.getElementById("resultSummary");

        let activeField = null;
        let suppress = false;

        function formatNumber(value) {
          if (!isFinite(value)) return "";
          const abs = Math.abs(value);
          if (abs !== 0 && (abs < 0.0001 || abs >= 1e7)) {
            return value.toExponential(6);
          }
          return value.toLocaleString(undefined, { maximumFractionDigits: 6 });
        }

        function updateSummary(from, to, direction) {
          if (!isFinite(from) || !isFinite(to)) {
            resultSummary.classList.add("hidden");
            return;
          }
          let text = "";
          if (direction === "m3-to-ft3") {
            text = `${formatNumber(from)} m³ = ${formatNumber(to)} ft³`;
          } else {
            text = `${formatNumber(from)} ft³ = ${formatNumber(to)} m³`;
          }
          resultSummary.textContent = text;
          resultSummary.classList.remove("hidden");
        }

        function handleM3Input() {
          if (suppress) return;
          activeField = "m3";
          const val = parseFloat(m3Input.value);
          if (isNaN(val)) {
            suppress = true;
            ft3Input.value = "";
            suppress = false;
            resultSummary.classList.add("hidden");
            return;
          }
          const converted = val * FACTOR;
          suppress = true;
          ft3Input.value = converted;
          suppress = false;
          updateSummary(val, converted, "m3-to-ft3");
        }

        function handleFt3Input() {
          if (suppress) return;
          activeField = "ft3";
          const val = parseFloat(ft3Input.value);
          if (isNaN(val)) {
            suppress = true;
            m3Input.value = "";
            suppress = false;
            resultSummary.classList.add("hidden");
            return;
          }
          const converted = val / FACTOR;
          suppress = true;
          m3Input.value = converted;
          suppress = false;
          updateSummary(val, converted, "ft3-to-m3");
        }

        function swapValues() {
          const m3Val = m3Input.value;
          const ft3Val = ft3Input.value;
          m3Input.value = ft3Val;
          ft3Input.value = m3Val;

          if (ft3Val && !isNaN(parseFloat(ft3Val))) {
            handleM3Input();
          } else if (m3Val && !isNaN(parseFloat(m3Val))) {
            handleFt3Input();
          } else {
            resultSummary.classList.add("hidden");
          }
        }

        function clearValues() {
          m3Input.value = "";
          ft3Input.value = "";
          resultSummary.classList.add("hidden");
          activeField = null;
        }

        m3Input.addEventListener("input", handleM3Input);
        ft3Input.addEventListener("input", handleFt3Input);
        swapBtn.addEventListener("click", swapValues);
        clearBtn.addEventListener("click", clearValues);
      })();
    </script>

