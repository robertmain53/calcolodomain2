          <h2 class="text-xl font-semibold mb-3">Beaufort scale (0–12)</h2>
          <div class="scroll-x mb-6">
            <table>
              <thead>
                <tr>
                  <th>Force</th>
                  <th>Name</th>
                  <th>m/s</th>
                  <th>km/h</th>
                  <th>knots</th>
                  <th>Sea / Land effects (short)</th>
                </tr>
              </thead>
              <tbody id="bf-table-body">
                <!-- filled by JS -->
              </tbody>
            </table>
          </div>

          <div class="prose">
            <h2>How conversion works</h2>
            <p>The Beaufort scale is defined by ranges. For example, 6–11 km/h → Beaufort 2. So to convert, we:</p>
            <ol>
              <li>Normalize your input to m/s.</li>
              <li>Find the scale entry whose range contains that m/s value.</li>
              <li>Return the number, name, and typical range in your original unit.</li>
            </ol>
            <div class="formula-box">
              <p><strong>Example:</strong> 10 km/h</p>
              <p>10 km/h ÷ 3.6 = 2.78 m/s, which is in the range 1.6–3.3 m/s → Beaufort 2.</p>
            </div>
            <h3>Standard ranges used</h3>
            <p>We follow common international tables (WMO, Met Office, national met services). Minor rounding may occur between knots and km/h, but the Beaufort number stays the same.</p>
          </div>

          <script>
            (function(){
              const errorBox = document.getElementById('error-box');
              const errorMsg = document.getElementById('error-msg');
              const windInput = document.getElementById('wind-value');
              const unitSelect = document.getElementById('wind-unit');
              const bfSelect = document.getElementById('bf-select');
              const convertBtn = document.getElementById('convert-btn');
              const bfNumber = document.getElementById('bf-number');
              const bfDesc = document.getElementById('bf-desc');
              const bfRange = document.getElementById('bf-range');
              const lastConv = document.getElementById('last-conv');
              const tableBody = document.getElementById('bf-table-body');

              // data (km/h ranges for convenience)
              const BEAUFORT = [
                {b:0, name:"Calm", kmhMin:0, kmhMax:1, mpsMin:0, mpsMax:0.2, knotsMin:0, knotsMax:1, sea:"Sea like a mirror", land:"Smoke rises vertically"},
                {b:1, name:"Light air", kmhMin:1, kmhMax:5, mpsMin:0.3, mpsMax:1.5, knotsMin:1, knotsMax:3, sea:"Ripples", land:"Smoke drift"},
                {b:2, name:"Light breeze", kmhMin:6, kmhMax:11, mpsMin:1.6, mpsMax:3.3, knotsMin:4, knotsMax:6, sea:"Small wavelets", land:"Leaves rustle"},
                {b:3, name:"Gentle breeze", kmhMin:12, kmhMax:19, mpsMin:3.4, mpsMax:5.4, knotsMin:7, knotsMax:10, sea:"Large wavelets", land:"Flags extended"},
                {b:4, name:"Moderate breeze", kmhMin:20, kmhMax:28, mpsMin:5.5, mpsMax:7.9, knotsMin:11, knotsMax:16, sea:"Small waves", land:"Raises dust"},
                {b:5, name:"Fresh breeze", kmhMin:29, kmhMax:38, mpsMin:8.0, mpsMax:10.7, knotsMin:17, knotsMax:21, sea:"Moderate waves", land:"Small trees sway"},
                {b:6, name:"Strong breeze", kmhMin:39, kmhMax:49, mpsMin:10.8, mpsMax:13.8, knotsMin:22, knotsMax:27, sea:"Large waves", land:"Wires whistling"},
                {b:7, name:"Near gale", kmhMin:50, kmhMax:61, mpsMin:13.9, mpsMax:17.1, knotsMin:28, knotsMax:33, sea:"Sea heaps up", land:"Whole trees in motion"},
                {b:8, name:"Gale", kmhMin:62, kmhMax:74, mpsMin:17.2, mpsMax:20.7, knotsMin:34, knotsMax:40, sea:"Moderately high waves", land:"Twigs break"},
                {b:9, name:"Strong gale", kmhMin:75, kmhMax:88, mpsMin:20.8, mpsMax:24.4, knotsMin:41, knotsMax:47, sea:"High waves", land:"Slight structural damage"},
                {b:10, name:"Storm", kmhMin:89, kmhMax:102, mpsMin:24.5, mpsMax:28.4, knotsMin:48, knotsMax:55, sea:"Very high waves", land:"Trees uprooted"},
                {b:11, name:"Violent storm", kmhMin:103, kmhMax:117, mpsMin:28.5, mpsMax:32.6, knotsMin:56, knotsMax:63, sea:"Exceptionally high waves", land:"Widespread damage"},
                {b:12, name:"Hurricane", kmhMin:118, kmhMax:999, mpsMin:32.7, mpsMax:999, knotsMin:64, knotsMax:999, sea:"Air filled with foam", land:"Severe, devastation"}
              ];

              // populate table
              BEAUFORT.forEach(e => {
                const tr = document.createElement('tr');
                tr.innerHTML = `
                  <td><span class="scale-pill ${e.b>=10?'scale-10':(e.b>=6?'scale-6':'scale-0')}">${e.b}</span></td>
                  <td>${e.name}</td>
                  <td>${e.mpsMin.toFixed(1)} – ${e.mpsMax === 999 ? 'up' : e.mpsMax.toFixed(1)}</td>
                  <td>${e.kmhMin} – ${e.kmhMax === 999 ? 'up' : e.kmhMax}</td>
                  <td>${e.knotsMin} – ${e.knotsMax === 999 ? 'up' : e.knotsMax}</td>
                  <td>${e.sea}</td>
                `;
                tableBody.appendChild(tr);
              });

              // populate Beaufort select
              BEAUFORT.forEach(e => {
                const opt = document.createElement('option');
                opt.value = e.b;
                opt.textContent = `${e.b} – ${e.name}`;
                bfSelect.appendChild(opt);
              });

              function toKmh(val, unit){
                switch(unit){
                  case 'mps': return val * 3.6;
                  case 'kmh': return val;
                  case 'kn': return val * 1.852;
                  case 'mph': return val * 1.60934;
                  default: return val;
                }
              }
              function fromKmh(kmh, unit){
                switch(unit){
                  case 'mps': return kmh / 3.6;
                  case 'kmh': return kmh;
                  case 'kn': return kmh / 1.852;
                  case 'mph': return kmh / 1.60934;
                  default: return kmh;
                }
              }

              function findBeaufortFromKmh(kmh){
                for (let i=0;i<BEAUFORT.length;i++){
                  const e = BEAUFORT[i];
                  if (kmh >= e.kmhMin && kmh <= e.kmhMax) return e;
                }
                return BEAUFORT[BEAUFORT.length-1];
              }

              function setOutput(entry, originalUnit){
                bfNumber.innerHTML = `<span class="scale-pill ${entry.b>=10?'scale-10':(entry.b>=6?'scale-6':'scale-0')}">${entry.b}</span>`;
                bfDesc.textContent = entry.name;
                // show range in original unit
                const min = fromKmh(entry.kmhMin, originalUnit);
                const max = entry.kmhMax === 999 ? null : fromKmh(entry.kmhMax, originalUnit);
                bfRange.textContent = max ? `${min.toFixed(1)} – ${max.toFixed(1)} ${originalUnit}` : `${min.toFixed(1)}+ ${originalUnit}`;
              }

              function clearError(){ errorBox.classList.add('hidden'); }
              function showError(msg){ errorMsg.textContent = msg; errorBox.classList.remove('hidden'); }

              function convertFromSpeed(){
                clearError();
                const val = parseFloat(windInput.value);
                if (isNaN(val)) { showError("Wind speed must be numeric."); return; }
                const unit = unitSelect.value;
                const kmh = toKmh(val, unit);
                const entry = findBeaufortFromKmh(kmh);
                setOutput(entry, unit);
                lastConv.textContent = `Converted ${val} ${unit} → Beaufort ${entry.b} (${entry.name})`;
                bfSelect.value = entry.b;
              }

              convertBtn.addEventListener('click', convertFromSpeed);

              bfSelect.addEventListener('change', () => {
                clearError();
                const bf = bfSelect.value;
                if (bf === "") return;
                const entry = BEAUFORT.find(e => e.b == bf);
                const unit = unitSelect.value;
                setOutput(entry, unit);
                // set input to middle of range in that unit
                const midKmh = (entry.kmhMin + (entry.kmhMax === 999 ? entry.kmhMin : entry.kmhMax)) / 2;
                const midInUnit = fromKmh(midKmh, unit);
                windInput.value = midInUnit.toFixed(1);
                lastConv.textContent = `Selected Beaufort ${entry.b} → ${entry.name}`;
              });

              // init with default
              convertFromSpeed();
            })();
          </script>
        </div>



