    <script>
      // State
      const state = {
        unit: "metric", // 'metric' or 'imperial'
        activeTab: "strip",
      };

      function qs(id) {
        return document.getElementById(id);
      }

      function setUnit(unit) {
        state.unit = unit;
        const metric = qs("unitMetric");
        const imperial = qs("unitImperial");
        if (unit === "metric") {
          metric.classList.add("tab-active");
          imperial.classList.remove("tab-active");
          // Update unit labels
          qs("stripLengthUnit").textContent = "m";
          qs("stripWidthUnit").textContent = "m";
          qs("stripDepthUnit").textContent = "m";
          qs("stripWallThicknessUnit").textContent = "m";
          qs("padLengthUnit").textContent = "m";
          qs("padWidthUnit").textContent = "m";
          qs("padDepthUnit").textContent = "m";
          qs("slabLengthUnit").textContent = "m";
          qs("slabWidthUnit").textContent = "m";
          qs("slabThicknessUnit").textContent = "m";
          qs("blockLengthUnit").textContent = "m";
          qs("blockHeightUnit").textContent = "m";
          qs("concreteCostUnit").textContent = "/m³";
        } else {
          metric.classList.remove("tab-active");
          imperial.classList.add("tab-active");
          qs("stripLengthUnit").textContent = "ft";
          qs("stripWidthUnit").textContent = "ft";
          qs("stripDepthUnit").textContent = "ft";
          qs("stripWallThicknessUnit").textContent = "ft";
          qs("padLengthUnit").textContent = "ft";
          qs("padWidthUnit").textContent = "ft";
          qs("padDepthUnit").textContent = "ft";
          qs("slabLengthUnit").textContent = "ft";
          qs("slabWidthUnit").textContent = "ft";
          qs("slabThicknessUnit").textContent = "ft";
          qs("blockLengthUnit").textContent = "ft";
          qs("blockHeightUnit").textContent = "ft";
          qs("concreteCostUnit").textContent = "/yd³";
        }
      }

      function setTab(tab) {
        state.activeTab = tab;
        document.querySelectorAll(".foundation-tab").forEach((btn) => {
          if (btn.dataset.tab === tab) {
            btn.classList.add("tab-active");
            btn.classList.remove("bg-white");
          } else {
            btn.classList.remove("tab-active");
            btn.classList.add("bg-white");
          }
        });
        document.querySelectorAll(".foundation-panel").forEach((panel) => {
          panel.classList.add("hidden");
        });
        const activePanel = qs("tab-" + tab);
        if (activePanel) activePanel.classList.remove("hidden");
      }

      function parsePositive(id) {
        const el = qs(id);
        const v = parseFloat(el.value);
        if (isNaN(v) || v <= 0) return null;
        return v;
      }

      function calculate() {
        // Clear previous error styles
        document
          .querySelectorAll("input")
          .forEach((i) => i.classList.remove("input-error"));

        let volume = 0; // in m³
        let description = "";
        const unit = state.unit;

        if (state.activeTab === "strip") {
          const L = parsePositive("stripLength");
          const B = parsePositive("stripWidth");
          const D = parsePositive("stripDepth");
          if (!L || !B || !D) {
            ["stripLength", "stripWidth", "stripDepth"].forEach((id) => {
              if (!parsePositive(id)) qs(id).classList.add("input-error");
            });
            showError(
              "Please enter positive values for footing length, width, and depth."
            );
            return;
          }
          let Lm = L,
            Bm = B,
            Dm = D;
          if (unit === "imperial") {
            // ft to m
            Lm = L * 0.3048;
            Bm = B * 0.3048;
            Dm = D * 0.3048;
          }
          volume = Lm * Bm * Dm;
          description = "Strip / continuous footing";
        } else if (state.activeTab === "pad") {
          const n = parsePositive("padCount");
          const L = parsePositive("padLength");
          const B = parsePositive("padWidth");
          const D = parsePositive("padDepth");
          if (!n || !L || !B || !D) {
            ["padCount", "padLength", "padWidth", "padDepth"].forEach((id) => {
              if (!parsePositive(id)) qs(id).classList.add("input-error");
            });
            showError(
              "Please enter positive values for pad count and dimensions."
            );
            return;
          }
          let Lm = L,
            Bm = B,
            Dm = D;
          if (unit === "imperial") {
            Lm = L * 0.3048;
            Bm = B * 0.3048;
            Dm = D * 0.3048;
          }
          volume = n * Lm * Bm * Dm;
          description = "Pad / isolated footings";
        } else if (state.activeTab === "slab") {
          const L = parsePositive("slabLength");
          const W = parsePositive("slabWidth");
          const T = parsePositive("slabThickness");
          if (!L || !W || !T) {
            ["slabLength", "slabWidth", "slabThickness"].forEach((id) => {
              if (!parsePositive(id)) qs(id).classList.add("input-error");
            });
            showError(
              "Please enter positive values for slab length, width, and thickness."
            );
            return;
          }
          let Lm = L,
            Wm = W,
            Tm = T;
          if (unit === "imperial") {
            Lm = L * 0.3048;
            Wm = W * 0.3048;
            Tm = T * 0.3048;
          }
          volume = Lm * Wm * Tm;
          description = "Slab-on-grade";
        }

        const waste = parseFloat(qs("wastePercent").value);
        const wasteFactor = isNaN(waste)
          ? 0
          : Math.max(0, Math.min(30, waste)) / 100;
        const volumeWithWaste = volume * (1 + wasteFactor);

        const volumeM3 = volume;
        const volumeM3Waste = volumeWithWaste;
        const volumeYd3 = volumeM3 * 1.30795;
        const volumeYd3Waste = volumeM3Waste * 1.30795;

        // Bag estimate (80 lb ~ 0.6 ft³)
        const ft3 = volumeM3 * 35.3147;
        const bags80 = ft3 / 0.6;

        // Block estimate (only for strip footing with wall thickness & block size)
        let blockInfo = null;
        const wallT = parsePositive("stripWallThickness");
        const blockL = parsePositive("blockLength");
        const blockH = parsePositive("blockHeight");
        if (state.activeTab === "strip" && wallT && blockL && blockH) {
          let length = parsePositive("stripLength");
          if (length) {
            if (unit === "imperial") {
              length = length * 0.3048;
            }
            let bL = blockL;
            let bH = blockH;
            if (unit === "imperial") {
              bL = blockL * 0.3048;
              bH = blockH * 0.3048;
            }
            const blocksPerCourse = length / bL;
            const courses = wallT / bH;
            const totalBlocks = blocksPerCourse * courses;
            const blockCost = parseFloat(qs("blockCost").value);
            const blockCostTotal =
              !isNaN(blockCost) && blockCost > 0
                ? totalBlocks * blockCost
                : null;
            blockInfo = {
              blocksPerCourse,
              courses,
              totalBlocks,
              blockCostTotal,
            };
          }
        }

        // Cost estimate
        const concreteCost = parseFloat(qs("concreteCost").value);
        let concreteCostTotal = null;
        if (!isNaN(concreteCost) && concreteCost > 0) {
          if (unit === "metric") {
            concreteCostTotal = volumeM3Waste * concreteCost;
          } else {
            concreteCostTotal = volumeYd3Waste * concreteCost;
          }
        }

        renderResults({
          description,
          volumeM3,
          volumeM3Waste,
          volumeYd3,
          volumeYd3Waste,
          bags80,
          wastePercent: wasteFactor * 100,
          blockInfo,
          concreteCostTotal,
          unit,
        });
      }

      function showError(msg) {
        const container = qs("resultsContainer");
        container.innerHTML =
          '<div class="border border-red-200 bg-red-50 text-red-700 rounded-md p-4 text-sm">' +
          msg +
          "</div>";
      }

      function renderResults(data) {
        const {
          description,
          volumeM3,
          volumeM3Waste,
          volumeYd3,
          volumeYd3Waste,
          bags80,
          wastePercent,
          blockInfo,
          concreteCostTotal,
          unit,
        } = data;

        const container = qs("resultsContainer");
        let html = "";

        html +=
          '<div class="border border-gray-200 rounded-md p-4 bg-gray-50 text-sm">';
        html += `<p class="font-semibold mb-1">${description} – concrete volume</p>`;
        html += '<ul class="space-y-1">';
        html += `<li>Theoretical volume: <strong>${volumeM3.toFixed(
          3
        )}</strong> m³ (${volumeYd3.toFixed(3)} yd³)</li>`;
        html += `<li>With ${wastePercent.toFixed(
          0
        )}% waste: <strong>${volumeM3Waste.toFixed(
          3
        )}</strong> m³ (${volumeYd3Waste.toFixed(3)} yd³)</li>`;
        html += `<li>Approx. 80 lb bags: <strong>${Math.ceil(
          bags80
        )}</strong> bags (estimate)</li>`;
        html += "</ul>";
        html += "</div>";

        if (concreteCostTotal !== null) {
          html +=
            '<div class="border border-green-200 rounded-md p-4 bg-green-50 text-sm">';
          html +=
            '<p class="font-semibold mb-1">Concrete material cost (with waste)</p>';
          html += `<p>Estimated concrete cost: <strong>$${concreteCostTotal.toFixed(
            2
          )}</strong></p>`;
          html +=
            '<p class="text-xs text-gray-700 mt-1">Excludes labor, formwork, reinforcement, excavation, and equipment.</p>';
          html += "</div>";
        }

        if (blockInfo) {
          html +=
            '<div class="border border-blue-200 rounded-md p-4 bg-blue-50 text-sm">';
          html += '<p class="font-semibold mb-1">Block wall estimate</p>';
          html += `<ul class="space-y-1">
        <li>Blocks per course (approx.): <strong>${blockInfo.blocksPerCourse.toFixed(
          1
        )}</strong></li>
        <li>Number of courses (approx.): <strong>${blockInfo.courses.toFixed(
          1
        )}</strong></li>
        <li>Total blocks (approx.): <strong>${Math.ceil(
          blockInfo.totalBlocks
        )}</strong></li>
      </ul>`;
          if (blockInfo.blockCostTotal !== null) {
            html += `<p class="mt-1">Estimated block cost: <strong>$${blockInfo.blockCostTotal.toFixed(
              2
            )}</strong></p>`;
          }
          html +=
            '<p class="text-xs text-gray-700 mt-1">Block count is approximate and does not include openings, corners, or waste.</p>';
          html += "</div>";
        }

        html +=
          '<div class="text-xs text-gray-500 mt-2">This calculator is for estimating only and does not replace a structural design by a licensed engineer.</div>';

        container.innerHTML = html;
      }

      function resetAll() {
        document.querySelectorAll("input").forEach((i) => {
          i.value = "";
          i.classList.remove("input-error");
        });
        qs("wastePercent").value = 5;
        setUnit("metric");
        setTab("strip");
        qs("resultsContainer").innerHTML =
          '<div class="border border-dashed border-gray-300 rounded-md p-4 text-sm text-gray-500">Enter your foundation dimensions and click “Calculate foundation” to see concrete volume, block count, and cost.</div>';
      }

      document.addEventListener("DOMContentLoaded", () => {
        // Unit toggle
        qs("unitMetric").addEventListener("click", () => setUnit("metric"));
        qs("unitImperial").addEventListener("click", () => setUnit("imperial"));

        // Tabs
        document.querySelectorAll(".foundation-tab").forEach((btn) => {
          btn.addEventListener("click", () => setTab(btn.dataset.tab));
        });

        // Buttons
        qs("calcBtn").addEventListener("click", calculate);
        qs("resetBtn").addEventListener("click", resetAll);
      });

      // Mobile menu script (simple)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const expanded = this.getAttribute("aria-expanded") === "true";
          this.setAttribute("aria-expanded", String(!expanded));
          menu.classList.toggle("hidden");
        });
    </script>

