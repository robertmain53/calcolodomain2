    <script>
      // Mobile menu toggle
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          this.setAttribute("aria-expanded", String(isHidden));
        });

      // k-factor custom handling
      const kSelect = document.getElementById("k-factor");
      const kCustomInput = document.getElementById("k-factor-custom");
      kSelect.addEventListener("change", () => {
        if (kSelect.value === "custom") {
          kCustomInput.classList.remove("hidden");
        } else {
          kCustomInput.classList.add("hidden");
        }
      });

      function getImperfectionFactor(curve) {
        switch (curve) {
          case "a0":
            return 0.13;
          case "a":
            return 0.21;
          case "b":
            return 0.34;
          case "c":
            return 0.49;
          case "d":
            return 0.76;
          default:
            return 0.34;
        }
      }

      function formatNumber(value, decimals) {
        if (!isFinite(value)) return "-";
        return Number(value).toFixed(decimals);
      }

      function createResultRow(label, value, unit, highlight) {
        const color =
          highlight === "ok"
            ? "bg-green-100 text-green-800"
            : highlight === "bad"
            ? "bg-red-100 text-red-800"
            : "bg-gray-100 text-gray-800";
        return `
      <div class="flex items-center justify-between py-1 border-b border-gray-100 last:border-b-0">
        <div class="text-sm text-gray-700">${label}</div>
        <div class="flex items-center gap-2">
          <span class="text-sm font-semibold">${value}</span>
          ${unit ? `<span class="text-xs text-gray-500">${unit}</span>` : ""}
          ${
            highlight
              ? `<span class="result-badge ${color}">${
                  highlight === "ok" ? "OK" : "Not OK"
                }</span>`
              : ""
          }
        </div>
      </div>
    `;
      }

      document.getElementById("calculate-btn").addEventListener("click", () => {
        const fy = parseFloat(document.getElementById("steel-grade").value); // MPa
        const gammaM0 = parseFloat(document.getElementById("gamma-m0").value);
        const gammaM1 = parseFloat(document.getElementById("gamma-m1").value);
        const L = parseFloat(document.getElementById("length").value); // m
        let k =
          kSelect.value === "custom"
            ? parseFloat(kCustomInput.value)
            : parseFloat(kSelect.value);
        const axis = document.getElementById("axis").value;
        const A = parseFloat(document.getElementById("area").value); // mm2
        const Iy = parseFloat(document.getElementById("iy").value);
        const Iz = parseFloat(document.getElementById("iz").value);
        const curve = document.getElementById("buckling-curve").value;
        const Ned_kN = parseFloat(document.getElementById("ned").value);

        const resultsDiv = document.getElementById("results-container");

        if (
          [fy, gammaM0, gammaM1, L, k, A, Iy, Iz, Ned_kN].some(
            (v) => !isFinite(v) || v <= 0
          )
        ) {
          resultsDiv.innerHTML =
            '<p class="text-sm text-red-600">Please enter valid positive values for all inputs.</p>';
          return;
        }

        const E = 210000; // MPa
        const Ned = Ned_kN * 1000; // kN -> N

        const Npl_Rd_N = (A * fy) / gammaM0; // N
        const Npl_Rd_kN = Npl_Rd_N / 1000;

        const Lcr_mm = k * L * 1000; // m -> mm
        const I = axis === "y" ? Iy : Iz;

        const Ncr_N = (Math.PI * Math.PI * E * I) / (Lcr_mm * Lcr_mm);
        const Ncr_kN = Ncr_N / 1000;

        const lambda = Math.sqrt((A * fy) / Ncr_N);

        const alpha = getImperfectionFactor(curve);
        const phi = 0.5 * (1 + alpha * (lambda - 0.2) + lambda * lambda);
        const sqrtTerm = Math.max(phi * phi - lambda * lambda, 0);
        let chi = 1 / (phi + Math.sqrt(sqrtTerm));
        if (chi > 1) chi = 1;

        const Nb_Rd_N = (chi * A * fy) / gammaM1;
        const Nb_Rd_kN = Nb_Rd_N / 1000;

        const eta = Ned_kN / Nb_Rd_kN;

        const status = eta <= 1 ? "ok" : "bad";

        resultsDiv.innerHTML = `
      <div class="border border-gray-200 rounded-lg p-4">
        <h3 class="text-lg font-semibold mb-2">Summary</h3>
        <div class="mb-3">
          ${createResultRow(
            "Design axial load N\u2091\u1d05",
            formatNumber(Ned_kN, 1),
            "kN"
          )}
          ${createResultRow(
            "Plastic resistance N\u209a\u2097,Rd",
            formatNumber(Npl_Rd_kN, 1),
            "kN"
          )}
          ${createResultRow(
            "Buckling resistance N\u1d3d,Rd",
            formatNumber(Nb_Rd_kN, 1),
            "kN"
          )}
          ${createResultRow(
            "Utilization \u03b7 = N\u2091\u1d05 / N\u1d3d,Rd",
            formatNumber(eta, 2),
            "",
            status
          )}
        </div>
        <h4 class="text-md font-semibold mb-1">Slenderness &amp; buckling</h4>
        <div>
          ${createResultRow(
            "Effective length L\u2097\u209b",
            formatNumber(Lcr_mm, 0),
            "mm"
          )}
          ${createResultRow(
            "Euler critical load N\u1d04\u1d3d",
            formatNumber(Ncr_kN, 1),
            "kN"
          )}
          ${createResultRow(
            "Non‑dimensional slenderness \u03bb",
            formatNumber(lambda, 3),
            ""
          )}
          ${createResultRow(
            "Buckling curve / \u03b1",
            `${curve.toUpperCase()} / ${formatNumber(alpha, 2)}`,
            ""
          )}
          ${createResultRow(
            "Reduction factor \u03c7",
            formatNumber(chi, 3),
            ""
          )}
        </div>
        <p class="mt-3 text-xs text-gray-500">
          Note: Checks are for pure axial compression only. For combined bending and compression, interaction formulas from EN 1993‑1‑1, 6.3.3 must be used.
        </p>
      </div>
    `;
      });

      document.getElementById("reset-btn").addEventListener("click", () => {
        document.getElementById("steel-grade").value = "355";
        document.getElementById("gamma-m0").value = "1.0";
        document.getElementById("gamma-m1").value = "1.0";
        document.getElementById("length").value = "3.0";
        document.getElementById("k-factor").value = "1.0";
        document.getElementById("k-factor-custom").value = "1.0";
        document.getElementById("k-factor-custom").classList.add("hidden");
        document.getElementById("axis").value = "y";
        document.getElementById("area").value = "6500";
        document.getElementById("iy").value = "8.5e8";
        document.getElementById("iz").value = "2.6e8";
        document.getElementById("buckling-curve").value = "b";
        document.getElementById("ned").value = "800";
        document.getElementById("results-container").innerHTML =
          '<div class="text-sm text-gray-500">Enter data and click “Calculate” to see Eurocode 3 column checks.</div>';
      });
    </script>
  
