    <script>
      // Core assumptions (update as laws change)
      const KY_FLAT_RATE = 0.045; // example 4.5% flat rate; update to current KY rate
      const KY_STANDARD_DEDUCTION = {
        single: 3100,
        married: 3100 * 2, // simple doubling for joint; adjust if KY rules differ
        hoh: 3100,
      };

      // Simple federal model (for planning only, not exact)
      const FED_STANDARD_DEDUCTION = {
        single: 14600,
        married: 29200,
        hoh: 21900,
      };

      // Example 2025-ish brackets (placeholder, not official)
      const FED_BRACKETS = {
        single: [
          { upTo: 11600, rate: 0.1 },
          { upTo: 47150, rate: 0.12 },
          { upTo: 100525, rate: 0.22 },
          { upTo: 191950, rate: 0.24 },
          { upTo: 243725, rate: 0.32 },
          { upTo: 609350, rate: 0.35 },
          { upTo: Infinity, rate: 0.37 },
        ],
        married: [
          { upTo: 23200, rate: 0.1 },
          { upTo: 94300, rate: 0.12 },
          { upTo: 201050, rate: 0.22 },
          { upTo: 383900, rate: 0.24 },
          { upTo: 487450, rate: 0.32 },
          { upTo: 731200, rate: 0.35 },
          { upTo: Infinity, rate: 0.37 },
        ],
        hoh: [
          { upTo: 16550, rate: 0.1 },
          { upTo: 63100, rate: 0.12 },
          { upTo: 100500, rate: 0.22 },
          { upTo: 191950, rate: 0.24 },
          { upTo: 243700, rate: 0.32 },
          { upTo: 609350, rate: 0.35 },
          { upTo: Infinity, rate: 0.37 },
        ],
      };

      function formatMoney(value) {
        if (!isFinite(value)) return "$0";
        return value.toLocaleString("en-US", {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 0,
        });
      }

      function formatPercent(value) {
        if (!isFinite(value)) return "0%";
        return (value * 100).toFixed(1).replace(/\.0$/, "") + "%";
      }

      function annualize(amount, freq) {
        switch (freq) {
          case "monthly":
            return amount * 12;
          case "biweekly":
            return amount * 26;
          case "weekly":
            return amount * 52;
          default:
            return amount;
        }
      }

      function perPeriod(amount, freq) {
        switch (freq) {
          case "monthly":
            return amount / 12;
          case "biweekly":
            return amount / 26;
          case "weekly":
            return amount / 52;
          default:
            return amount;
        }
      }

      function calcFederalTax(taxable, statusKey) {
        if (taxable <= 0) return 0;
        const brackets = FED_BRACKETS[statusKey];
        let remaining = taxable;
        let lastLimit = 0;
        let tax = 0;
        for (const b of brackets) {
          const band = Math.min(remaining, b.upTo - lastLimit);
          if (band <= 0) break;
          tax += band * b.rate;
          remaining -= band;
          lastLimit = b.upTo;
          if (remaining <= 0) break;
        }
        return Math.max(0, tax);
      }

      function calcSelfEmploymentTax(seIncomeAnnual) {
        if (seIncomeAnnual <= 0) return 0;
        const seBase = seIncomeAnnual * 0.9235;
        const ssWageBase = 168600; // placeholder
        const ssTax = Math.min(seBase, ssWageBase) * 0.124;
        const medicareTax = seBase * 0.029;
        return Math.max(0, ssTax + medicareTax);
      }

      document.addEventListener("DOMContentLoaded", () => {
        const selfEmploymentShareInput = document.getElementById(
          "selfEmploymentShare"
        );
        const selfEmploymentShareLabel = document.getElementById(
          "selfEmploymentShareLabel"
        );
        const calculateBtn = document.getElementById("calculateBtn");
        const resultsSection = document.getElementById("resultsSection");

        selfEmploymentShareInput.addEventListener("input", () => {
          selfEmploymentShareLabel.textContent =
            selfEmploymentShareInput.value + "%";
        });

        calculateBtn.addEventListener("click", () => {
          const filingStatus = document.getElementById("filingStatus").value;
          const payFrequency = document.getElementById("payFrequency").value;
          const includeFederal =
            document.getElementById("includeFederal").checked;

          const grossInput =
            parseFloat(document.getElementById("grossIncome").value) || 0;
          const preTaxRet =
            parseFloat(document.getElementById("preTaxRetirement").value) || 0;
          const preTaxOther =
            parseFloat(document.getElementById("preTaxOther").value) || 0;
          const localRate =
            (parseFloat(document.getElementById("localRate").value) || 0) / 100;
          const seShare =
            (parseFloat(selfEmploymentShareInput.value) || 0) / 100;

          const grossAnnual = annualize(grossInput, payFrequency);
          const preTaxAnnual = annualize(preTaxRet + preTaxOther, payFrequency);
          const kyStdDed =
            KY_STANDARD_DEDUCTION[filingStatus] ?? KY_STANDARD_DEDUCTION.single;

          const kyTaxable = Math.max(0, grossAnnual - preTaxAnnual - kyStdDed);
          const kyTax = kyTaxable * KY_FLAT_RATE;

          const localTax = Math.max(
            0,
            (grossAnnual - preTaxAnnual) * localRate
          );

          let federalTax = 0;
          let seTax = 0;
          if (includeFederal) {
            const fedStdDed =
              FED_STANDARD_DEDUCTION[filingStatus] ??
              FED_STANDARD_DEDUCTION.single;
            const fedTaxable = Math.max(
              0,
              grossAnnual - preTaxAnnual - fedStdDed
            );
            const statusKey =
              filingStatus === "married"
                ? "married"
                : filingStatus === "hoh"
                ? "hoh"
                : "single";
            federalTax = calcFederalTax(fedTaxable, statusKey);

            const seIncomeAnnual = grossAnnual * seShare;
            seTax = calcSelfEmploymentTax(seIncomeAnnual);
          }

          const totalTax = kyTax + localTax + federalTax + seTax;
          const takeHomeAnnual = Math.max(
            0,
            grossAnnual - preTaxAnnual - totalTax
          );

          const grossPerPeriod = perPeriod(grossAnnual, payFrequency);
          const preTaxPerPeriod = perPeriod(preTaxAnnual, payFrequency);
          const kyTaxPerPeriod = perPeriod(kyTax, payFrequency);
          const localTaxPerPeriod = perPeriod(localTax, payFrequency);
          const federalTaxPerPeriod = perPeriod(federalTax, payFrequency);
          const seTaxPerPeriod = perPeriod(seTax, payFrequency);
          const totalTaxPerPeriod = perPeriod(totalTax, payFrequency);
          const takeHomePerPeriod = perPeriod(takeHomeAnnual, payFrequency);

          const stateEffectiveRate = grossAnnual > 0 ? kyTax / grossAnnual : 0;
          const totalEffectiveRate =
            grossAnnual > 0 ? totalTax / grossAnnual : 0;

          // Update summary cards
          document.getElementById("stateTaxAnnual").textContent =
            formatMoney(kyTax);
          document.getElementById("stateEffectiveRate").textContent =
            "Effective rate: " + formatPercent(stateEffectiveRate);
          document.getElementById("totalTaxAnnual").textContent =
            formatMoney(totalTax);
          document.getElementById("totalEffectiveRate").textContent =
            "Effective rate: " + formatPercent(totalEffectiveRate);
          document.getElementById("takeHomeAnnual").textContent =
            formatMoney(takeHomeAnnual);
          document.getElementById("takeHomePerPeriod").textContent =
            "Per paycheck: " + formatMoney(takeHomePerPeriod);

          // Update table
          document.getElementById("tblGrossAnnual").textContent =
            formatMoney(grossAnnual);
          document.getElementById("tblGrossPerPeriod").textContent =
            formatMoney(grossPerPeriod);
          document.getElementById("tblPreTaxAnnual").textContent =
            formatMoney(preTaxAnnual);
          document.getElementById("tblPreTaxPerPeriod").textContent =
            formatMoney(preTaxPerPeriod);
          document.getElementById("tblKyTaxable").textContent =
            formatMoney(kyTaxable);
          document.getElementById("tblKyTax").textContent = formatMoney(kyTax);
          document.getElementById("tblKyTaxPerPeriod").textContent =
            formatMoney(kyTaxPerPeriod);
          document.getElementById("tblLocalTax").textContent =
            formatMoney(localTax);
          document.getElementById("tblLocalTaxPerPeriod").textContent =
            formatMoney(localTaxPerPeriod);
          document.getElementById("tblFederalTax").textContent =
            formatMoney(federalTax);
          document.getElementById("tblFederalTaxPerPeriod").textContent =
            formatMoney(federalTaxPerPeriod);
          document.getElementById("tblSeTax").textContent = formatMoney(seTax);
          document.getElementById("tblSeTaxPerPeriod").textContent =
            formatMoney(seTaxPerPeriod);
          document.getElementById("tblTotalTax").textContent =
            formatMoney(totalTax);
          document.getElementById("tblTotalTaxPerPeriod").textContent =
            formatMoney(totalTaxPerPeriod);
          document.getElementById("tblTakeHome").textContent =
            formatMoney(takeHomeAnnual);
          document.getElementById("tblTakeHomePerPeriod").textContent =
            formatMoney(takeHomePerPeriod);

          resultsSection.classList.remove("hidden");
          resultsSection.scrollIntoView({ behavior: "smooth", block: "start" });
        });

        // Auto-run once with defaults
        calculateBtn.click();
      });
    </script>

