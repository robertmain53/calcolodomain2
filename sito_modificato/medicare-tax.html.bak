    <script>
      // Medicare tax calculator logic
      (function () {
        const filingThresholds = {
          single: 200000,
          mfj: 250000,
          mfs: 125000,
          hoh: 200000,
          qw: 200000,
        };

        const wageRate = 0.0145; // 1.45% employee + 1.45% employer separately
        const seRate = 0.029; // 2.9% combined for self-employed
        const addlRate = 0.009; // 0.9% Additional Medicare Tax

        const $ = (id) => document.getElementById(id);

        function formatMoney(v) {
          if (!isFinite(v)) return "$0.00";
          return v.toLocaleString("en-US", {
            style: "currency",
            currency: "USD",
          });
        }

        function formatPercent(v) {
          if (!isFinite(v)) return "0.00%";
          return (v * 100).toFixed(2) + "%";
        }

        function calculate() {
          const filingStatus = $("filing-status").value;
          const wages = Math.max(0, parseFloat($("wage-income").value) || 0);
          const seIncome = Math.max(0, parseFloat($("se-income").value) || 0);
          const threshold = filingThresholds[filingStatus] || 200000;

          const totalEarned = wages + seIncome;

          // Regular Medicare
          const employeeRegular = wages * wageRate;
          const employerRegular = wages * wageRate;
          const seRegular = seIncome * seRate;

          // Additional Medicare Tax
          const excess = Math.max(0, totalEarned - threshold);
          let addlOnWages = 0;
          let addlOnSE = 0;

          if (excess > 0 && totalEarned > 0) {
            const wageShare = wages / totalEarned;
            const seShare = seIncome / totalEarned;
            addlOnWages = excess * wageShare * addlRate;
            addlOnSE = excess * seShare * addlRate;
          }

          const employeeAdditional = addlOnWages; // only employee/self-employed pays
          const seAdditional = addlOnSE;

          const employeeTotal = employeeRegular + employeeAdditional;
          const seTotal = seRegular + seAdditional;
          const totalMedicare = employeeTotal + employerRegular + seTotal;

          const effectiveRate =
            totalEarned > 0 ? totalMedicare / totalEarned : 0;

          const seDeduction = 0.5 * seTotal;

          // Update UI
          $("employee-regular").textContent =
            "Regular Medicare tax: " + formatMoney(employeeRegular);
          $("employee-additional").textContent =
            "Additional Medicare Tax: " + formatMoney(employeeAdditional);
          $("employee-total").textContent =
            "Total employee Medicare tax: " + formatMoney(employeeTotal);

          $("employer-regular").textContent =
            "Employer Medicare tax: " + formatMoney(employerRegular);

          $("se-regular").textContent =
            "Regular Medicare (2.9% on SE income): " + formatMoney(seRegular);
          $("se-additional").textContent =
            "Additional Medicare Tax on SE income: " +
            formatMoney(seAdditional);
          $("se-total").textContent =
            "Total Medicare portion of SE tax: " + formatMoney(seTotal);
          $("se-deduction").textContent =
            "Estimated SE tax deduction (employer-equivalent half): " +
            formatMoney(seDeduction);

          $("total-medicare-tax").textContent = formatMoney(totalMedicare);
          $("effective-rate").textContent =
            "Effective Medicare tax rate: " +
            formatPercent(effectiveRate) +
            " of total Medicare-taxable income";

          const details = [];
          details.push("Filing status threshold: " + formatMoney(threshold));
          details.push(
            "Total Medicare-taxable earned income: " + formatMoney(totalEarned)
          );
          details.push(
            "Excess over threshold for Additional Medicare Tax: " +
              formatMoney(excess)
          );
          details.push("");
          details.push(
            "Employee regular Medicare (1.45% of wages): " +
              formatMoney(employeeRegular)
          );
          details.push(
            "Employer Medicare (1.45% of wages): " +
              formatMoney(employerRegular)
          );
          details.push(
            "Self-employment regular Medicare (2.9% of SE income): " +
              formatMoney(seRegular)
          );
          details.push("");
          details.push(
            "Additional Medicare Tax on wages: " + formatMoney(addlOnWages)
          );
          details.push(
            "Additional Medicare Tax on SE income: " + formatMoney(addlOnSE)
          );
          details.push(
            "Total Additional Medicare Tax: " +
              formatMoney(employeeAdditional + seAdditional)
          );

          $("calc-details").innerText = details.join("\n");
        }

        document.addEventListener("DOMContentLoaded", () => {
          $("calculate-btn").addEventListener("click", calculate);
          ["filing-status", "wage-income", "se-income", "year-select"].forEach(
            (id) => {
              $(id).addEventListener("change", calculate);
              $(id).addEventListener("input", () => {
                // debounce-like quick update
                clearTimeout(window._medicareTimer);
                window._medicareTimer = setTimeout(calculate, 200);
              });
            }
          );
          calculate();
        });
      })();
    </script>

