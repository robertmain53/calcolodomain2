    <script>
      // Utility: format currency with symbol
      function formatCurrency(value, symbol) {
        if (isNaN(value)) return symbol + "0";
        const absVal = Math.abs(value);
        let formatted;
        if (absVal >= 1_000_000_000) {
          formatted = (value / 1_000_000_000).toFixed(2) + "B";
        } else if (absVal >= 1_000_000) {
          formatted = (value / 1_000_000).toFixed(2) + "M";
        } else if (absVal >= 1_000) {
          formatted = (value / 1_000).toFixed(2) + "K";
        } else {
          formatted = value.toFixed(0);
        }
        return symbol + formatted;
      }

      function formatCurrencyFull(value, symbol) {
        if (isNaN(value)) return symbol + "0";
        return (
          symbol + value.toLocaleString(undefined, { maximumFractionDigits: 0 })
        );
      }

      function getNumber(id) {
        const el = document.getElementById(id);
        if (!el) return 0;
        const v = parseFloat(el.value);
        return isNaN(v) ? 0 : v;
      }

      function updateCurrencyPrefixes() {
        const symbol = document.getElementById("currency").value || "$";
        const ids = [
          "rev-currency-prefix",
          "ebitda-currency-prefix",
          "owner-currency-prefix",
          "profit-currency-prefix",
          "assets-currency-prefix",
          "liab-currency-prefix",
          "cf-currency-prefix",
        ];
        ids.forEach((id) => {
          const el = document.getElementById(id);
          if (el) el.textContent = symbol;
        });
      }

      document
        .getElementById("currency")
        .addEventListener("change", updateCurrencyPrefixes);
      updateCurrencyPrefixes();

      document
        .getElementById("calculate-btn")
        .addEventListener("click", function () {
          const symbol = document.getElementById("currency").value || "$";

          // Inputs
          const revenue = getNumber("annual-revenue");
          const ebitda = getNumber("ebitda");
          const ownerAddBack = getNumber("owner-salary");
          const assets = getNumber("total-assets");
          const liabilities = getNumber("total-liabilities");

          const ebitdaMultLow = getNumber("ebitda-mult-low");
          const ebitdaMultMid = getNumber("ebitda-mult-mid");
          const ebitdaMultHigh = getNumber("ebitda-mult-high");

          const revMultLow = getNumber("rev-mult-low");
          const revMultMid = getNumber("rev-mult-mid");
          const revMultHigh = getNumber("rev-mult-high");

          const cf0 = getNumber("starting-cashflow");
          const g = getNumber("growth-rate") / 100;
          const r = getNumber("discount-rate") / 100;
          const n =
            parseInt(document.getElementById("forecast-years").value, 10) || 5;
          const gTerminal = getNumber("terminal-growth") / 100;

          const resultsSection = document.getElementById("results-section");
          const snapshotList = document.getElementById("snapshot-list");
          snapshotList.innerHTML = "";

          // EBITDA multiple valuation
          const ebitdaUsed = ebitda + ownerAddBack;
          let ebitdaValLow = 0,
            ebitdaValMid = 0,
            ebitdaValHigh = 0;
          if (ebitdaUsed > 0 && ebitdaMultLow > 0 && ebitdaMultHigh > 0) {
            ebitdaValLow = ebitdaUsed * ebitdaMultLow;
            ebitdaValMid =
              ebitdaUsed *
              (ebitdaMultMid || (ebitdaMultLow + ebitdaMultHigh) / 2);
            ebitdaValHigh = ebitdaUsed * ebitdaMultHigh;
            document.getElementById("ebitda-used").textContent =
              formatCurrencyFull(ebitdaUsed, symbol);
            document.getElementById("ebitda-multiple-range").textContent =
              ebitdaMultLow.toFixed(1) +
              "x – " +
              ebitdaMultHigh.toFixed(1) +
              "x";
            document.getElementById("ebitda-value-range").textContent =
              formatCurrencyFull(ebitdaValLow, symbol) +
              " – " +
              formatCurrencyFull(ebitdaValHigh, symbol);

            const li = document.createElement("li");
            li.textContent =
              "EBITDA multiple (mid): " +
              formatCurrencyFull(ebitdaValMid, symbol);
            snapshotList.appendChild(li);
          } else {
            document.getElementById("ebitda-used").textContent = "—";
            document.getElementById("ebitda-multiple-range").textContent = "—";
            document.getElementById("ebitda-value-range").textContent = "—";
          }

          // Revenue multiple valuation
          let revValLow = 0,
            revValMid = 0,
            revValHigh = 0;
          if (revenue > 0 && revMultLow > 0 && revMultHigh > 0) {
            revValLow = revenue * revMultLow;
            revValMid =
              revenue * (revMultMid || (revMultLow + revMultHigh) / 2);
            revValHigh = revenue * revMultHigh;
            document.getElementById("revenue-used").textContent =
              formatCurrencyFull(revenue, symbol);
            document.getElementById("revenue-multiple-range").textContent =
              revMultLow.toFixed(1) + "x – " + revMultHigh.toFixed(1) + "x";
            document.getElementById("revenue-value-range").textContent =
              formatCurrencyFull(revValLow, symbol) +
              " – " +
              formatCurrencyFull(revValHigh, symbol);

            const li = document.createElement("li");
            li.textContent =
              "Revenue multiple (mid): " +
              formatCurrencyFull(revValMid, symbol);
            snapshotList.appendChild(li);
          } else {
            document.getElementById("revenue-used").textContent = "—";
            document.getElementById("revenue-multiple-range").textContent = "—";
            document.getElementById("revenue-value-range").textContent = "—";
          }

          // Asset-based valuation
          let assetBasedValue = null;
          if (assets > 0 || liabilities > 0) {
            assetBasedValue = assets - liabilities;
            document.getElementById("assets-used").textContent =
              formatCurrencyFull(assets, symbol);
            document.getElementById("liabilities-used").textContent =
              formatCurrencyFull(liabilities, symbol);
            document.getElementById("asset-based-value").textContent =
              formatCurrencyFull(assetBasedValue, symbol);

            const li = document.createElement("li");
            li.textContent =
              "Net asset value: " + formatCurrencyFull(assetBasedValue, symbol);
            snapshotList.appendChild(li);
          } else {
            document.getElementById("assets-used").textContent = "—";
            document.getElementById("liabilities-used").textContent = "—";
            document.getElementById("asset-based-value").textContent = "—";
          }

          // DCF valuation
          let dcfPVcashflows = 0;
          let dcfPVterminal = 0;
          let dcfTotal = null;
          const dcfTableDiv = document.getElementById("dcf-table");
          dcfTableDiv.innerHTML = "";

          if (cf0 > 0 && r > 0 && r > gTerminal) {
            let cf = cf0;
            const rows = [];
            for (let t = 1; t <= n; t++) {
              cf = cf0 * Math.pow(1 + g, t);
              const pv = cf / Math.pow(1 + r, t);
              dcfPVcashflows += pv;
              rows.push({ year: t, cf, pv });
            }
            const cfTerminalYear = cf0 * Math.pow(1 + g, n + 1);
            const tv = cfTerminalYear / (r - gTerminal);
            dcfPVterminal = tv / Math.pow(1 + r, n);
            dcfTotal = dcfPVcashflows + dcfPVterminal;

            document.getElementById("dcf-pv-cashflows").textContent =
              formatCurrencyFull(dcfPVcashflows, symbol);
            document.getElementById("dcf-pv-terminal").textContent =
              formatCurrencyFull(dcfPVterminal, symbol);
            document.getElementById("dcf-total-value").textContent =
              formatCurrencyFull(dcfTotal, symbol);

            const list = document.createElement("ul");
            rows.forEach((row) => {
              const li = document.createElement("li");
              li.textContent =
                "Year " +
                row.year +
                ": CF " +
                formatCurrencyFull(row.cf, symbol) +
                " → PV " +
                formatCurrencyFull(row.pv, symbol);
              list.appendChild(li);
            });
            dcfTableDiv.appendChild(list);

            const li = document.createElement("li");
            li.textContent =
              "DCF value: " + formatCurrencyFull(dcfTotal, symbol);
            snapshotList.appendChild(li);
          } else {
            document.getElementById("dcf-pv-cashflows").textContent = "—";
            document.getElementById("dcf-pv-terminal").textContent = "—";
            document.getElementById("dcf-total-value").textContent = "—";
          }

          // Overall range: use midpoints from available methods
          const midValues = [];
          const labels = [];

          if (ebitdaValMid > 0) {
            midValues.push(ebitdaValMid);
            labels.push("EBITDA multiple");
          }
          if (revValMid > 0) {
            midValues.push(revValMid);
            labels.push("Revenue multiple");
          }
          if (dcfTotal && dcfTotal > 0) {
            midValues.push(dcfTotal);
            labels.push("DCF");
          }
          if (assetBasedValue !== null && !isNaN(assetBasedValue)) {
            midValues.push(assetBasedValue);
            labels.push("Asset-based");
          }

          if (midValues.length === 0) {
            document.getElementById("overall-range").textContent =
              "Please enter at least revenue or EBITDA to see a valuation.";
            document.getElementById("overall-methods-used").textContent = "";
            resultsSection.classList.remove("hidden");
            return;
          }

          const low = Math.min(...midValues);
          const high = Math.max(...midValues);
          const median = midValues.slice().sort((a, b) => a - b)[
            Math.floor(midValues.length / 2)
          ];

          document.getElementById("overall-range").textContent =
            formatCurrencyFull(low, symbol) +
            " – " +
            formatCurrencyFull(high, symbol) +
            " (median " +
            formatCurrencyFull(median, symbol) +
            ")";

          document.getElementById("overall-methods-used").textContent =
            "Based on: " + labels.join(", ") + ".";

          resultsSection.classList.remove("hidden");
          resultsSection.scrollIntoView({ behavior: "smooth", block: "start" });
        });

      // Mobile menu toggle
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          this.setAttribute("aria-expanded", isHidden ? "true" : "false");
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
