    <script>
      // Tab switching
      const tabs = {
        basic: document.getElementById("tab-basic"),
        compare: document.getElementById("tab-compare"),
        advanced: document.getElementById("tab-advanced"),
      };
      const panels = {
        basic: document.getElementById("panel-basic"),
        compare: document.getElementById("panel-compare"),
        advanced: document.getElementById("panel-advanced"),
      };

      function activateTab(name) {
        Object.keys(tabs).forEach((key) => {
          if (key === name) {
            tabs[key].classList.add("border-blue-600", "text-blue-600");
            tabs[key].classList.remove("border-transparent", "text-gray-500");
            panels[key].classList.remove("hidden");
          } else {
            tabs[key].classList.remove("border-blue-600", "text-blue-600");
            tabs[key].classList.add("border-transparent", "text-gray-500");
            panels[key].classList.add("hidden");
          }
        });
      }

      tabs.basic.addEventListener("click", () => activateTab("basic"));
      tabs.compare.addEventListener("click", () => activateTab("compare"));
      tabs.advanced.addEventListener("click", () => activateTab("advanced"));

      // Dividend period handling
      const dividendPeriodEl = document.getElementById("dividend-period");
      const dividendInputWrapper = document.getElementById(
        "dividend-input-wrapper"
      );
      const dividendInput = document.getElementById("dividend");

      function updateDividendLabel() {
        const label = dividendInputWrapper.querySelector("label");
        switch (dividendPeriodEl.value) {
          case "annual":
            label.textContent = "Annual dividend per share";
            dividendInput.placeholder = "e.g. 2";
            break;
          case "quarterly":
            label.textContent = "Most recent quarterly dividend per share";
            dividendInput.placeholder = "e.g. 0.50";
            break;
          case "monthly":
            label.textContent = "Most recent monthly dividend per share";
            dividendInput.placeholder = "e.g. 0.10";
            break;
          case "custom":
            label.textContent = "Total dividends per share (last 12 months)";
            dividendInput.placeholder = "e.g. 1.85";
            break;
        }
      }

      dividendPeriodEl.addEventListener("change", updateDividendLabel);
      updateDividendLabel();

      // Core calculation
      function annualizeDividend(dividend, period) {
        if (!isFinite(dividend) || dividend < 0) return NaN;
        switch (period) {
          case "annual":
            return dividend;
          case "quarterly":
            return dividend * 4;
          case "monthly":
            return dividend * 12;
          case "custom":
            return dividend;
          default:
            return dividend;
        }
      }

      function formatPercent(value) {
        if (!isFinite(value)) return "–";
        if (Math.abs(value) >= 10) return value.toFixed(2) + "%";
        return value.toFixed(2) + "%";
      }

      function formatCurrencyPer1000(value) {
        if (!isFinite(value)) return "–";
        // generic formatting without currency symbol
        return value.toFixed(2);
      }

      const priceEl = document.getElementById("price");
      const taxRateEl = document.getElementById("tax-rate");
      const yieldGrossEl = document.getElementById("yield-gross");
      const yieldNetEl = document.getElementById("yield-net");
      const income1000El = document.getElementById("income-1000");

      document.getElementById("calc-btn").addEventListener("click", () => {
        const price = parseFloat(priceEl.value);
        const divInput = parseFloat(dividendInput.value);
        const period = dividendPeriodEl.value;
        const taxRate = parseFloat(taxRateEl.value);

        if (
          !isFinite(price) ||
          price <= 0 ||
          !isFinite(divInput) ||
          divInput < 0
        ) {
          yieldGrossEl.textContent = "–";
          yieldNetEl.textContent = "–";
          income1000El.textContent = "–";
          return;
        }

        const annualDividend = annualizeDividend(divInput, period);
        const grossYield = (annualDividend / price) * 100;
        const netDividend =
          isFinite(taxRate) && taxRate >= 0 && taxRate <= 100
            ? annualDividend * (1 - taxRate / 100)
            : annualDividend;
        const netYield = (netDividend / price) * 100;
        const incomePer1000 = (1000 / price) * netDividend;

        yieldGrossEl.textContent = formatPercent(grossYield);
        yieldNetEl.textContent = formatPercent(netYield);
        income1000El.textContent = formatCurrencyPer1000(incomePer1000);
      });

      document.getElementById("reset-btn").addEventListener("click", () => {
        priceEl.value = "";
        dividendInput.value = "";
        taxRateEl.value = "";
        yieldGrossEl.textContent = "–";
        yieldNetEl.textContent = "–";
        income1000El.textContent = "–";
      });

      // Comparison table
      const compareBody = document.getElementById("compare-body");
      const compareAddRowBtn = document.getElementById("compare-add-row");
      const compareCalcBtn = document.getElementById("compare-calc");

      function createCompareRow(index) {
        const tr = document.createElement("tr");
        tr.className = index % 2 === 0 ? "bg-white" : "bg-gray-50";
        tr.innerHTML = `
      <td class="px-3 py-2">
        <input type="text" class="w-full border border-gray-200 rounded-md px-2 py-1 text-sm" placeholder="e.g. TICKER">
      </td>
      <td class="px-3 py-2 text-right">
        <input type="number" min="0" step="0.0001" class="w-full border border-gray-200 rounded-md px-2 py-1 text-sm text-right" placeholder="Price">
      </td>
      <td class="px-3 py-2 text-right">
        <input type="number" min="0" step="0.0001" class="w-full border border-gray-200 rounded-md px-2 py-1 text-sm text-right" placeholder="Annual dividend">
      </td>
      <td class="px-3 py-2 text-right text-sm font-semibold text-gray-800">–</td>
    `;
        return tr;
      }

      function ensureRows(count) {
        while (compareBody.children.length < count) {
          compareBody.appendChild(
            createCompareRow(compareBody.children.length)
          );
        }
      }

      ensureRows(3);

      compareAddRowBtn.addEventListener("click", () => {
        if (compareBody.children.length >= 5) return;
        compareBody.appendChild(createCompareRow(compareBody.children.length));
      });

      compareCalcBtn.addEventListener("click", () => {
        let bestYield = -Infinity;
        let bestRow = null;

        Array.from(compareBody.children).forEach((tr) => {
          const priceInput = tr.children[1].querySelector("input");
          const divInput = tr.children[2].querySelector("input");
          const resultCell = tr.children[3];

          const price = parseFloat(priceInput.value);
          const dividend = parseFloat(divInput.value);

          if (
            !isFinite(price) ||
            price <= 0 ||
            !isFinite(dividend) ||
            dividend < 0
          ) {
            resultCell.textContent = "–";
            resultCell.classList.remove("text-blue-700");
            tr.classList.remove("ring-2", "ring-blue-300");
            return;
          }

          const y = (dividend / price) * 100;
          resultCell.textContent = formatPercent(y);

          if (y > bestYield) {
            bestYield = y;
            bestRow = tr;
          }
          resultCell.classList.remove("text-blue-700");
          tr.classList.remove("ring-2", "ring-blue-300");
        });

        if (isFinite(bestYield) && bestRow) {
          const bestCell = bestRow.children[3];
          bestCell.classList.add("text-blue-700");
          bestRow.classList.add("ring-2", "ring-blue-300");
        }
      });

      // Yield on cost
      const purchasePriceEl = document.getElementById("purchase-price");
      const currentDividendEl = document.getElementById("current-dividend");
      const yieldOnCostEl = document.getElementById("yield-on-cost");

      document
        .getElementById("yield-on-cost-btn")
        .addEventListener("click", () => {
          const purchasePrice = parseFloat(purchasePriceEl.value);
          const currentDividend = parseFloat(currentDividendEl.value);
          if (
            !isFinite(purchasePrice) ||
            purchasePrice <= 0 ||
            !isFinite(currentDividend) ||
            currentDividend < 0
          ) {
            yieldOnCostEl.textContent = "–";
            return;
          }
          const yoc = (currentDividend / purchasePrice) * 100;
          yieldOnCostEl.textContent = formatPercent(yoc);
        });

      // Payout ratio
      const epsEl = document.getElementById("eps");
      const annualDividendPayoutEl = document.getElementById(
        "annual-dividend-payout"
      );
      const payoutRatioEl = document.getElementById("payout-ratio");
      const payoutInterpretationEl = document.getElementById(
        "payout-interpretation"
      );

      document.getElementById("payout-btn").addEventListener("click", () => {
        const eps = parseFloat(epsEl.value);
        const div = parseFloat(annualDividendPayoutEl.value);
        if (!isFinite(eps) || eps <= 0 || !isFinite(div) || div < 0) {
          payoutRatioEl.textContent = "–";
          payoutInterpretationEl.textContent = "";
          return;
        }
        const ratio = (div / eps) * 100;
        payoutRatioEl.textContent = formatPercent(ratio);

        let msg = "";
        if (ratio < 40) {
          msg =
            "Low payout ratio: generally conservative and likely sustainable, leaving room for reinvestment and dividend growth.";
        } else if (ratio < 70) {
          msg =
            "Moderate payout ratio: typical for many mature dividend payers. Check earnings stability and cash flow.";
        } else if (ratio < 90) {
          msg =
            "High payout ratio: income-focused policy. More sensitive to earnings declines.";
        } else {
          msg =
            "Very high payout ratio: dividend may be at risk if earnings fall. Investigate carefully.";
        }
        payoutInterpretationEl.textContent = msg;
      });

      // Mobile menu script (basic)
      const mobileToggle = document.getElementById("mobile-menu-toggle");
      const mobileMenu = document.getElementById("mobile-menu");
      if (mobileToggle && mobileMenu) {
        mobileToggle.addEventListener("click", () => {
          const isHidden = mobileMenu.classList.contains("hidden");
          mobileMenu.classList.toggle("hidden", !isHidden);
          mobileToggle.setAttribute(
            "aria-expanded",
            isHidden ? "true" : "false"
          );
        });
      }

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
