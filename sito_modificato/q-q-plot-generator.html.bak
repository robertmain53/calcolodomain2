    const dataInput = document.getElementById('data-input');
    const distSelect = document.getElementById('dist-select');
    const showLineSelect = document.getElementById('show-line');
    const generateBtn = document.getElementById('generate-btn');
    const inputError = document.getElementById('input-error');
    const canvas = document.getElementById('qq-canvas');
    const ctx = canvas.getContext('2d');
    const quantileTableBody = document.querySelector('#quantile-table tbody');
    const summaryLine = document.getElementById('summary-line');

    function parseData(str) {
      const parts = str.split(/[\s,;]+/).filter(Boolean);
      const nums = parts.map(Number).filter(v => !isNaN(v));
      return nums;
    }

    function mean(arr) {
      return arr.reduce((a,b) => a+b, 0) / (arr.length || 1);
    }

    function stdDev(arr, m) {
      if (arr.length < 2) return 0;
      const v = arr.reduce((acc, x) => acc + (x - m) * (x - m), 0) / (arr.length - 1);
      return Math.sqrt(v);
    }

    // Acklam's approximation for inverse normal CDF
    function normInv(p) {
      if (p <= 0 || p >= 1) {
        return p === 0 ? -Infinity : Infinity;
      }
      const a = [ -3.969683028665376e+01,  2.209460984245205e+02,
                  -2.759285104469687e+02,  1.383577518672690e+02,
                  -3.066479806614716e+01,  2.506628277459239e+00 ];

      const b = [ -5.447609879822406e+01,  1.615858368580409e+02,
                  -1.556989798598866e+02,  6.680131188771972e+01,
                  -1.328068155288572e+01 ];

      const c = [ -7.784894002430293e-03, -3.223964580411365e-01,
                  -2.400758277161838e+00, -2.549732539343734e+00,
                   4.374664141464968e+00,  2.938163982698783e+00 ];

      const d = [ 7.784695709041462e-03,  3.224671290700398e-01,
                  2.445134137142996e+00,  3.754408661907416e+00 ];

      const plow  = 0.02425;
      const phigh = 1 - plow;
      let q, r;
      if (p < plow) {
        q = Math.sqrt(-2*Math.log(p));
        return (((((c[0]*q + c[1])*q + c[2])*q + c[3])*q + c[4])*q + c[5]) /
               ((((d[0]*q + d[1])*q + d[2])*q + d[3])*q + 1);
      }
      if (phigh < p) {
        q = Math.sqrt(-2*Math.log(1-p));
        return -(((((c[0]*q + c[1])*q + c[2])*q + c[3])*q + c[4])*q + c[5]) /
                  ((((d[0]*q + d[1])*q + d[2])*q + d[3])*q + 1);
      }
      q = p - 0.5;
      r = q*q;
      return (((((a[0]*r + a[1])*r + a[2])*r + a[3])*r + a[4])*r + a[5])*q /
             (((((b[0]*r + b[1])*r + b[2])*r + b[3])*r + b[4])*r + 1);
    }

    function buildTheoretical(dist, p, sampleMean, sampleSD) {
      if (dist === 'normal') {
        return sampleMean + sampleSD * normInv(p);
      }
      if (dist === 'uniform') {
        return p;
      }
      if (dist === 'exponential') {
        return -Math.log(1 - p);
      }
      return p;
    }

    function computeQQ() {
      inputError.textContent = '';
      summaryLine.textContent = '—';
      quantileTableBody.innerHTML = '';
      const data = parseData(dataInput.value);
      if (!data.length) {
        inputError.textContent = 'Please enter some numeric data.';
        return;
      }
      const sorted = data.slice().sort((a,b) => a - b);
      const n = sorted.length;
      const m = mean(sorted);
      const sd = stdDev(sorted, m) || 1;

      const dist = distSelect.value;
      const points = [];
      for (let i = 1; i <= n; i++) {
        const p = (i - 0.5) / n;
        const tq = buildTheoretical(dist, p, m, sd);
        const sq = sorted[i-1];
        points.push({i, sq, tq});
      }

      // Draw plot
      drawQQ(points);

      // Fill table
      points.forEach(pt => {
        const tr = document.createElement('tr');
        const td1 = document.createElement('td');
        td1.textContent = pt.i;
        const td2 = document.createElement('td');
        td2.textContent = pt.sq.toFixed(4);
        const td3 = document.createElement('td');
        td3.textContent = pt.tq.toFixed(4);
        const td4 = document.createElement('td');
        td4.textContent = (pt.sq - pt.tq).toFixed(4);
        tr.appendChild(td1);
        tr.appendChild(td2);
        tr.appendChild(td3);
        tr.appendChild(td4);
        quantileTableBody.appendChild(tr);
      });

      // correlation for goodness
      const corr = pearson(points.map(p=>p.tq), points.map(p=>p.sq));
      summaryLine.textContent = "Points: " + n + " | Pearson r (theoretical vs sample) = " + corr.toFixed(4);
    }

    function pearson(xs, ys) {
      const n = xs.length;
      const mx = xs.reduce((a,b)=>a+b,0)/n;
      const my = ys.reduce((a,b)=>a+b,0)/n;
      let num = 0, dx = 0, dy = 0;
      for (let i = 0; i < n; i++) {
        const vx = xs[i] - mx;
        const vy = ys[i] - my;
        num += vx*vy;
        dx += vx*vx;
        dy += vy*vy;
      }
      const den = Math.sqrt(dx*dy);
      if (den === 0) return 0;
      return num / den;
    }

    function drawQQ(points) {
      const pad = 50;
      ctx.clearRect(0,0,canvas.width,canvas.height);
      const xs = points.map(p=>p.tq);
      const ys = points.map(p=>p.sq);
      const minX = Math.min(...xs);
      const maxX = Math.max(...xs);
      const minY = Math.min(...ys);
      const maxY = Math.max(...ys);
      const xRange = maxX - minX || 1;
      const yRange = maxY - minY || 1;

      // axes
      ctx.strokeStyle = "#e5e7eb";
      ctx.lineWidth = 1;
      ctx.beginPath();
      ctx.moveTo(pad, pad);
      ctx.lineTo(pad, canvas.height - pad);
      ctx.lineTo(canvas.width - pad, canvas.height - pad);
      ctx.stroke();

      function xScale(x) {
        return pad + ( (x - minX) / xRange ) * (canvas.width - 2*pad);
      }
      function yScale(y) {
        return (canvas.height - pad) - ( (y - minY) / yRange ) * (canvas.height - 2*pad);
      }

      // line (fitted)
      const lineMode = showLineSelect.value;
      if (lineMode !== 'none') {
        let a = 0, b = 1;
        if (lineMode === 'fit') {
          // least squares: y = a + b x
          const n = points.length;
          const mx = xs.reduce((s,v)=>s+v,0)/n;
          const my = ys.reduce((s,v)=>s+v,0)/n;
          let num = 0, den = 0;
          for (let i = 0; i < n; i++) {
            num += (xs[i] - mx)*(ys[i] - my);
            den += (xs[i] - mx)*(xs[i] - mx);
          }
          b = den ? num/den : 1;
          a = my - b*mx;
        } else if (lineMode === 'quartiles') {
          const q1 = points[Math.floor(points.length*0.25)].tq;
          const q3 = points[Math.floor(points.length*0.75)].tq;
          const s1 = points[Math.floor(points.length*0.25)].sq;
          const s3 = points[Math.floor(points.length*0.75)].sq;
          b = (s3 - s1) / (q3 - q1);
          a = s1 - b*q1;
        }
        const x1 = minX;
        const y1 = a + b*x1;
        const x2 = maxX;
        const y2 = a + b*x2;
        ctx.strokeStyle = "#93c5fd";
        ctx.lineWidth = 2;
        ctx.beginPath();
        ctx.moveTo(xScale(x1), yScale(y1));
        ctx.lineTo(xScale(x2), yScale(y2));
        ctx.stroke();
      }

      // points
      ctx.fillStyle = "#2563eb";
      points.forEach(pt => {
        const x = xScale(pt.tq);
        const y = yScale(pt.sq);
        ctx.beginPath();
        ctx.arc(x, y, 3, 0, Math.PI*2);
        ctx.fill();
      });

      // labels
      ctx.fillStyle = "#4b5563";
      ctx.font = "12px Inter, system-ui, sans-serif";
      ctx.fillText("Theoretical quantiles", pad, pad - 15);
      ctx.save();
      ctx.translate(15, canvas.height/2);
      ctx.rotate(-Math.PI/2);
      ctx.fillText("Sample quantiles", 0, 0);
      ctx.restore();
    }

    generateBtn.addEventListener('click', computeQQ);
    // initial draw
    computeQQ();
  </script>

