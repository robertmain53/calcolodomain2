    <script>
      // PSI <-> kPa converter logic
      (function () {
        const FACTOR = 6.894757;
        const psiInput = document.getElementById("psiInput");
        const kpaInput = document.getElementById("kpaInput");
        const summaryBox = document.getElementById("summaryBox");
        const tableBody = document.getElementById("psiKpaTableBody");

        function formatNumber(value, decimals) {
          if (isNaN(value) || !isFinite(value)) return "";
          return value.toFixed(decimals);
        }

        function updateSummary(fromUnit) {
          const psiVal = parseFloat(psiInput.value);
          const kpaVal = parseFloat(kpaInput.value);
          if (isNaN(psiVal) || isNaN(kpaVal)) {
            summaryBox.classList.add("hidden");
            summaryBox.textContent = "";
            return;
          }
          if (fromUnit === "psi") {
            summaryBox.textContent = `${formatNumber(
              psiVal,
              2
            )} psi is equal to ${formatNumber(kpaVal, 2)} kPa.`;
          } else {
            summaryBox.textContent = `${formatNumber(
              kpaVal,
              2
            )} kPa is equal to ${formatNumber(psiVal, 2)} psi.`;
          }
          summaryBox.classList.remove("hidden");
        }

        function onPsiInput() {
          const psiVal = parseFloat(psiInput.value);
          if (isNaN(psiVal)) {
            kpaInput.value = "";
            updateSummary();
            return;
          }
          const kpaVal = psiVal * FACTOR;
          kpaInput.value = formatNumber(kpaVal, 4);
          updateSummary("psi");
        }

        function onKpaInput() {
          const kpaVal = parseFloat(kpaInput.value);
          if (isNaN(kpaVal)) {
            psiInput.value = "";
            updateSummary();
            return;
          }
          const psiVal = kpaVal / FACTOR;
          psiInput.value = formatNumber(psiVal, 4);
          updateSummary("kPa");
        }

        psiInput.addEventListener("input", onPsiInput);
        kpaInput.addEventListener("input", onKpaInput);

        // Preset buttons
        document.querySelectorAll(".preset-btn").forEach((btn) => {
          btn.addEventListener("click", () => {
            const psiVal = parseFloat(btn.getAttribute("data-psi"));
            if (!isNaN(psiVal)) {
              psiInput.value = psiVal;
              onPsiInput();
              psiInput.focus();
            }
          });
        });

        // Build conversion table
        const rows = [
          { psi: 20, note: "Low car tire / light trailer" },
          { psi: 25, note: "Mountain bike (low end)" },
          { psi: 30, note: "Typical car tire (low end)" },
          { psi: 32, note: "Common car tire setting" },
          { psi: 35, note: "Typical car tire (high end)" },
          { psi: 40, note: "SUV / light truck" },
          { psi: 45, note: "SUV / light truck (loaded)" },
          { psi: 50, note: "Heavy-duty / trailer tire" },
          { psi: 60, note: "Light truck / van (max load)" },
          { psi: 80, note: "Road bike (low end)" },
          { psi: 100, note: "Road bike (typical)" },
          { psi: 120, note: "Road bike (high end)" },
        ];

        rows.forEach((r) => {
          const kpa = r.psi * FACTOR;
          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="px-3 py-2 whitespace-nowrap">${formatNumber(
          r.psi,
          0
        )} psi</td>
        <td class="px-3 py-2 whitespace-nowrap">${formatNumber(kpa, 1)} kPa</td>
        <td class="px-3 py-2 text-gray-600">${r.note}</td>
      `;
          tableBody.appendChild(tr);
        });

        // Initialize with a common value
        psiInput.value = 32;
        onPsiInput();
      })();
    </script>

