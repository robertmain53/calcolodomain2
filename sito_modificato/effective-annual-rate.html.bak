    <script>
      // EAR calculator logic
      (function () {
        const modeNominalBtn = document.getElementById("mode-nominal-to-ear");
        const modeEarBtn = document.getElementById("mode-ear-to-nominal");
        const sectionNominal = document.getElementById(
          "section-nominal-to-ear"
        );
        const sectionEar = document.getElementById("section-ear-to-nominal");

        const compNominal = document.getElementById("compounding-nominal");
        const compEar = document.getElementById("compounding-ear");
        const customWrapNominal = document.getElementById(
          "custom-m-wrapper-nominal"
        );
        const customWrapEar = document.getElementById("custom-m-wrapper-ear");
        const customMNominal = document.getElementById("custom-m-nominal");
        const customMEar = document.getElementById("custom-m-ear");

        const nominalRateInput = document.getElementById("nominal-rate");
        const earRateInput = document.getElementById("ear-rate");

        const btnCalc = document.getElementById("btn-calc");
        const btnClear = document.getElementById("btn-clear");
        const errorMsg = document.getElementById("error-msg");

        const resultsCard = document.getElementById("results-card");
        const resultMain = document.getElementById("result-main");
        const resultSecondary = document.getElementById("result-secondary");
        const resultGrowth = document.getElementById("result-growth");

        let currentMode = "nominal-to-ear";

        function setMode(mode) {
          currentMode = mode;
          if (mode === "nominal-to-ear") {
            sectionNominal.classList.remove("hidden");
            sectionEar.classList.add("hidden");
            modeNominalBtn.classList.add(
              "bg-blue-600",
              "text-white",
              "border-blue-600"
            );
            modeNominalBtn.classList.remove(
              "bg-white",
              "text-gray-700",
              "border-gray-300"
            );
            modeEarBtn.classList.remove(
              "bg-blue-600",
              "text-white",
              "border-blue-600"
            );
            modeEarBtn.classList.add(
              "bg-white",
              "text-gray-700",
              "border-gray-300"
            );
          } else {
            sectionNominal.classList.add("hidden");
            sectionEar.classList.remove("hidden");
            modeEarBtn.classList.add(
              "bg-blue-600",
              "text-white",
              "border-blue-600"
            );
            modeEarBtn.classList.remove(
              "bg-white",
              "text-gray-700",
              "border-gray-300"
            );
            modeNominalBtn.classList.remove(
              "bg-blue-600",
              "text-white",
              "border-blue-600"
            );
            modeNominalBtn.classList.add(
              "bg-white",
              "text-gray-700",
              "border-gray-300"
            );
          }
          clearResults();
          errorMsg.textContent = "";
        }

        function handleCompChange(selectEl, wrapperEl) {
          if (selectEl.value === "custom") {
            wrapperEl.classList.remove("hidden");
          } else {
            wrapperEl.classList.add("hidden");
          }
        }

        function parseRate(inputEl) {
          const v = parseFloat(inputEl.value);
          if (isNaN(v)) return null;
          return v / 100.0;
        }

        function getM(selectEl, customInput) {
          const val = selectEl.value;
          if (val === "continuous") return "continuous";
          if (val === "custom") {
            const m = parseFloat(customInput.value);
            if (isNaN(m) || m <= 0) return null;
            return m;
          }
          const m = parseFloat(val);
          if (isNaN(m) || m <= 0) return null;
          return m;
        }

        function formatPercent(x) {
          if (!isFinite(x)) return "";
          return (x * 100).toFixed(4).replace(/\.?0+$/, "") + "%";
        }

        function clearResults() {
          resultsCard.classList.add("hidden");
          resultMain.textContent = "";
          resultSecondary.textContent = "";
          resultGrowth.textContent = "";
        }

        modeNominalBtn.addEventListener("click", () =>
          setMode("nominal-to-ear")
        );
        modeEarBtn.addEventListener("click", () => setMode("ear-to-nominal"));

        compNominal.addEventListener("change", () =>
          handleCompChange(compNominal, customWrapNominal)
        );
        compEar.addEventListener("change", () =>
          handleCompChange(compEar, customWrapEar)
        );

        btnCalc.addEventListener("click", () => {
          errorMsg.textContent = "";
          clearResults();

          try {
            if (currentMode === "nominal-to-ear") {
              const r = parseRate(nominalRateInput);
              if (r === null) {
                errorMsg.textContent =
                  "Please enter a valid nominal annual rate.";
                return;
              }
              const m = getM(compNominal, customMNominal);
              if (m === null) {
                errorMsg.textContent =
                  "Please enter a valid compounding frequency.";
                return;
              }

              let ear;
              if (m === "continuous") {
                ear = Math.exp(r) - 1;
              } else {
                ear = Math.pow(1 + r / m, m) - 1;
              }

              resultMain.textContent =
                "Effective annual rate (EAR): " + formatPercent(ear);
              const nominalStr = formatPercent(r);
              const compLabel =
                compNominal.options[compNominal.selectedIndex].text;
              resultSecondary.textContent = `Equivalent to a nominal rate of ${nominalStr} with ${compLabel}.`;
              resultGrowth.textContent = `A balance of $1.00 would grow to $${(
                1 + ear
              ).toFixed(6)} after one year.`;
              resultsCard.classList.remove("hidden");
            } else {
              const ear = parseRate(earRateInput);
              if (ear === null) {
                errorMsg.textContent =
                  "Please enter a valid effective annual rate.";
                return;
              }
              const m = getM(compEar, customMEar);
              if (m === null) {
                errorMsg.textContent =
                  "Please enter a valid compounding frequency.";
                return;
              }

              let r;
              if (m === "continuous") {
                if (1 + ear <= 0) {
                  errorMsg.textContent =
                    "EAR must be greater than -100% for continuous compounding.";
                  return;
                }
                r = Math.log(1 + ear);
              } else {
                if (1 + ear <= 0) {
                  errorMsg.textContent = "EAR must be greater than -100%.";
                  return;
                }
                r = m * (Math.pow(1 + ear, 1 / m) - 1);
              }

              resultMain.textContent =
                "Nominal annual rate (APR): " + formatPercent(r);
              const earStr = formatPercent(ear);
              const compLabel = compEar.options[compEar.selectedIndex].text;
              resultSecondary.textContent = `This nominal rate with ${compLabel} compounding has an effective annual rate of ${earStr}.`;
              resultGrowth.textContent = `A balance of $1.00 would grow to $${(
                1 + ear
              ).toFixed(6)} after one year.`;
              resultsCard.classList.remove("hidden");
            }
          } catch (e) {
            console.error(e);
            errorMsg.textContent =
              "An unexpected error occurred. Please check your inputs.";
          }
        });

        btnClear.addEventListener("click", () => {
          nominalRateInput.value = "";
          earRateInput.value = "";
          customMNominal.value = "";
          customMEar.value = "";
          compNominal.value = "12";
          compEar.value = "12";
          customWrapNominal.classList.add("hidden");
          customWrapEar.classList.add("hidden");
          errorMsg.textContent = "";
          clearResults();
        });
      })();
    </script>

