    <script>
      // 2024-ish standard deductions (approx; update as needed)
      const STANDARD_DEDUCTION = {
        single: 14600,
        married_joint: 29200,
        married_separate: 14600,
        head: 21900,
      };

      // 2024-ish federal tax brackets (approx; update as needed)
      // Each status: array of { upTo, rate } where upTo is taxable income cap for that bracket
      const FEDERAL_BRACKETS = {
        single: [
          { upTo: 11600, rate: 0.1 },
          { upTo: 47150, rate: 0.12 },
          { upTo: 100525, rate: 0.22 },
          { upTo: 191950, rate: 0.24 },
          { upTo: 243725, rate: 0.32 },
          { upTo: 609350, rate: 0.35 },
          { upTo: Infinity, rate: 0.37 },
        ],
        married_joint: [
          { upTo: 23200, rate: 0.1 },
          { upTo: 94300, rate: 0.12 },
          { upTo: 201050, rate: 0.22 },
          { upTo: 383900, rate: 0.24 },
          { upTo: 487450, rate: 0.32 },
          { upTo: 731200, rate: 0.35 },
          { upTo: Infinity, rate: 0.37 },
        ],
        married_separate: [
          { upTo: 11600, rate: 0.1 },
          { upTo: 47150, rate: 0.12 },
          { upTo: 100525, rate: 0.22 },
          { upTo: 191950, rate: 0.24 },
          { upTo: 243725, rate: 0.32 },
          { upTo: 365600, rate: 0.35 },
          { upTo: Infinity, rate: 0.37 },
        ],
        head: [
          { upTo: 16550, rate: 0.1 },
          { upTo: 63100, rate: 0.12 },
          { upTo: 100500, rate: 0.22 },
          { upTo: 191950, rate: 0.24 },
          { upTo: 243700, rate: 0.32 },
          { upTo: 609350, rate: 0.35 },
          { upTo: Infinity, rate: 0.37 },
        ],
      };

      function formatMoney(value) {
        if (!isFinite(value)) return "$0";
        return value.toLocaleString("en-US", {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 0,
        });
      }

      function formatPercent(value) {
        if (!isFinite(value)) return "0%";
        return (value * 100).toFixed(1).replace(/\.0$/, "") + "%";
      }

      function computeFederalTax(taxableIncome, status) {
        if (taxableIncome <= 0) return 0;
        const brackets = FEDERAL_BRACKETS[status] || FEDERAL_BRACKETS.single;
        let remaining = taxableIncome;
        let lastCap = 0;
        let tax = 0;

        for (const bracket of brackets) {
          const cap = bracket.upTo;
          const amountInBracket = Math.max(
            0,
            Math.min(remaining, cap - lastCap)
          );
          tax += amountInBracket * bracket.rate;
          remaining -= amountInBracket;
          lastCap = cap;
          if (remaining <= 0) break;
        }
        return tax;
      }

      function calculateTexasIncomeTax() {
        const status = document.getElementById("filing-status").value;
        const income = parseFloat(document.getElementById("income").value) || 0;
        const pretax = Math.max(
          0,
          parseFloat(document.getElementById("pretax-deductions").value) || 0
        );
        const credits = Math.max(
          0,
          parseFloat(document.getElementById("tax-credits").value) || 0
        );
        const freq = document.getElementById("pay-frequency").value;

        const gross = Math.max(0, income);
        const adjGross = Math.max(0, gross - pretax);
        const stdDed = STANDARD_DEDUCTION[status] || STANDARD_DEDUCTION.single;
        const taxable = Math.max(0, adjGross - stdDed);

        const taxBeforeCredits = computeFederalTax(taxable, status);
        const creditsApplied = Math.min(credits, taxBeforeCredits);
        const federalTax = Math.max(0, taxBeforeCredits - creditsApplied);

        const afterTax = Math.max(0, gross - federalTax);
        let divisor = 1;
        if (freq === "monthly") divisor = 12;
        else if (freq === "biweekly") divisor = 26;
        else if (freq === "weekly") divisor = 52;

        const perPay = afterTax / divisor;
        const effectiveRate = gross > 0 ? federalTax / gross : 0;

        // Update outputs
        document.getElementById("out-federal-tax").textContent =
          formatMoney(federalTax);
        document.getElementById("out-effective-rate").textContent =
          formatPercent(effectiveRate);
        document.getElementById("out-after-tax").textContent =
          formatMoney(afterTax);
        document.getElementById("out-per-pay").textContent =
          formatMoney(perPay);

        document.getElementById("out-gross").textContent = formatMoney(gross);
        document.getElementById("out-pretax").textContent =
          "-" + formatMoney(pretax);
        document.getElementById("out-standard-deduction").textContent =
          "-" + formatMoney(stdDed);
        document.getElementById("out-taxable").textContent =
          formatMoney(taxable);
        document.getElementById("out-tax-before-credits").textContent =
          formatMoney(taxBeforeCredits);
        document.getElementById("out-credits-applied").textContent =
          "-" + formatMoney(creditsApplied);
      }

      document.addEventListener("DOMContentLoaded", () => {
        document
          .getElementById("calc-btn")
          .addEventListener("click", calculateTexasIncomeTax);

        // Auto-calc on input changes for better UX
        [
          "filing-status",
          "income",
          "pretax-deductions",
          "tax-credits",
          "pay-frequency",
        ].forEach((id) => {
          const el = document.getElementById(id);
          if (!el) return;
          el.addEventListener("change", calculateTexasIncomeTax);
          el.addEventListener("input", () => {
            // Debounce slightly
            clearTimeout(el._txTimer);
            el._txTimer = setTimeout(calculateTexasIncomeTax, 250);
          });
        });

        calculateTexasIncomeTax();
      });
    </script>

