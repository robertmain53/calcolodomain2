    <script>
      // Helper functions
      function cmFromImperial(ft, inch) {
        return ft * 30.48 + inch * 2.54;
      }
      function kgFromLbs(lbs) {
        return lbs * 0.45359237;
      }
      function round(value, decimals) {
        const factor = Math.pow(10, decimals);
        return Math.round(value * factor) / factor;
      }

      document.addEventListener("DOMContentLoaded", function () {
        const unitsButtons = document.querySelectorAll(".units-toggle");
        const genderButtons = document.querySelectorAll(".gender-toggle");
        let currentUnits = "metric";
        let currentGender = "male";

        unitsButtons.forEach((btn) => {
          btn.addEventListener("click", () => {
            currentUnits = btn.dataset.units;
            unitsButtons.forEach((b) => {
              if (b === btn) {
                b.classList.add("bg-blue-50", "text-blue-700");
                b.classList.remove("text-gray-600");
              } else {
                b.classList.remove("bg-blue-50", "text-blue-700");
                b.classList.add("text-gray-600");
              }
            });
            document
              .getElementById("height-metric")
              .classList.toggle("hidden", currentUnits !== "metric");
            document
              .getElementById("weight-metric")
              .classList.toggle("hidden", currentUnits !== "metric");
            document
              .getElementById("height-imperial")
              .classList.toggle("hidden", currentUnits !== "imperial");
            document
              .getElementById("weight-imperial")
              .classList.toggle("hidden", currentUnits !== "imperial");
          });
        });

        genderButtons.forEach((btn) => {
          btn.addEventListener("click", () => {
            currentGender = btn.dataset.gender;
            genderButtons.forEach((b) => {
              if (b === btn) {
                b.classList.add("bg-blue-50", "text-blue-700");
                b.classList.remove("text-gray-600");
              } else {
                b.classList.remove("bg-blue-50", "text-blue-700");
                b.classList.add("text-gray-600");
              }
            });
          });
        });

        document
          .getElementById("calculateBtn")
          .addEventListener("click", () => {
            const age = parseInt(document.getElementById("age").value, 10);
            let heightCm, weightKg;

            if (currentUnits === "metric") {
              heightCm = parseFloat(document.getElementById("heightCm").value);
              weightKg = parseFloat(document.getElementById("weightKg").value);
            } else {
              const ft = parseFloat(document.getElementById("heightFt").value);
              const inch = parseFloat(
                document.getElementById("heightIn").value
              );
              const lbs = parseFloat(
                document.getElementById("weightLbs").value
              );
              heightCm = cmFromImperial(ft, inch);
              weightKg = kgFromLbs(lbs);
            }

            const activity = parseFloat(
              document.getElementById("activity").value
            );
            const goalPercent = parseFloat(
              document.getElementById("goal").value
            );
            const carbLimit = parseFloat(
              document.getElementById("ketoType").value
            );
            const proteinPerKg = parseFloat(
              document.getElementById("proteinPerKg").value
            );

            if (!age || !heightCm || !weightKg) {
              alert("Please enter valid age, height, and weight.");
              return;
            }

            // BMR (Mifflin–St Jeor)
            let bmr;
            if (currentGender === "male") {
              bmr = 10 * weightKg + 6.25 * heightCm - 5 * age + 5;
            } else {
              bmr = 10 * weightKg + 6.25 * heightCm - 5 * age - 161;
            }

            const tdee = bmr * activity;
            const goalCalories = tdee * (1 + goalPercent / 100);

            // Protein
            const proteinG = weightKg * proteinPerKg;
            const proteinKcal = proteinG * 4;

            // Carbs
            const carbG = carbLimit;
            const carbKcal = carbG * 4;

            // Fat
            let fatKcal = goalCalories - (proteinKcal + carbKcal);
            if (fatKcal < 0) fatKcal = 0;
            const fatG = fatKcal / 9;

            const totalKcal = proteinKcal + carbKcal + fatKcal || 1; // avoid division by zero

            const fatPct = (fatKcal / totalKcal) * 100;
            const proteinPct = (proteinKcal / totalKcal) * 100;
            const carbPct = (carbKcal / totalKcal) * 100;

            // Update outputs
            document.getElementById("bmrOut").textContent = Math.round(bmr);
            document.getElementById("caloriesOut").textContent =
              Math.round(goalCalories);

            document.getElementById("fatG").textContent = Math.round(fatG);
            document.getElementById("fatKcal").textContent =
              Math.round(fatKcal);
            document.getElementById("fatPct").textContent = Math.round(fatPct);

            document.getElementById("proteinG").textContent =
              Math.round(proteinG);
            document.getElementById("proteinKcal").textContent =
              Math.round(proteinKcal);
            document.getElementById("proteinPct").textContent =
              Math.round(proteinPct);

            document.getElementById("carbG").textContent = Math.round(carbG);
            document.getElementById("carbKcal").textContent =
              Math.round(carbKcal);
            document.getElementById("carbPct").textContent =
              Math.round(carbPct);

            // Per meal (3 meals)
            document.getElementById("fatPerMeal").textContent = Math.round(
              fatG / 3
            );
            document.getElementById("proteinPerMeal").textContent = Math.round(
              proteinG / 3
            );
            document.getElementById("carbPerMeal").textContent = Math.round(
              carbG / 3
            );

            // Per meal (2 meals)
            document.getElementById("fatPerMeal2").textContent = Math.round(
              fatG / 2
            );
            document.getElementById("proteinPerMeal2").textContent = Math.round(
              proteinG / 2
            );
            document.getElementById("carbPerMeal2").textContent = Math.round(
              carbG / 2
            );

            // Macro bar widths
            document.getElementById("fatBar").style.width = fatPct + "%";
            document.getElementById("proteinBar").style.width =
              proteinPct + "%";
            document.getElementById("carbBar").style.width = carbPct + "%";

            document
              .getElementById("resultsSection")
              .classList.remove("hidden");
            document
              .getElementById("resultsSection")
              .scrollIntoView({ behavior: "smooth", block: "start" });
          });
      });
    </script>

