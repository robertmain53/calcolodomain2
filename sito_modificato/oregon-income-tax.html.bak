    <script>
      // --- Oregon tax logic (simplified, educational only) ---

      // Approximate 2024-2025 Oregon brackets (fictionalized but realistic structure)
      const OR_BRACKETS = {
        single: [
          { upTo: 4150, rate: 0.0475 },
          { upTo: 10400, rate: 0.0675 },
          { upTo: 125000, rate: 0.0875 },
          { upTo: Infinity, rate: 0.099 },
        ],
        mfj: [
          { upTo: 8300, rate: 0.0475 },
          { upTo: 20800, rate: 0.0675 },
          { upTo: 250000, rate: 0.0875 },
          { upTo: Infinity, rate: 0.099 },
        ],
        mfs: [
          { upTo: 4150, rate: 0.0475 },
          { upTo: 10400, rate: 0.0675 },
          { upTo: 125000, rate: 0.0875 },
          { upTo: Infinity, rate: 0.099 },
        ],
        hoh: [
          { upTo: 8300, rate: 0.0475 },
          { upTo: 20800, rate: 0.0675 },
          { upTo: 200000, rate: 0.0875 },
          { upTo: Infinity, rate: 0.099 },
        ],
      };

      // Rough standard deduction estimates for Oregon (educational only)
      const OR_STD_DED = {
        single: 2500,
        mfj: 5000,
        mfs: 2500,
        hoh: 3800,
      };

      function formatMoney(value) {
        if (!isFinite(value)) return "$0";
        return value.toLocaleString("en-US", {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 0,
        });
      }

      function formatPercent(value) {
        if (!isFinite(value)) return "0%";
        return value.toFixed(1).replace(/\.0$/, "") + "%";
      }

      function estimateDeductions(status, income, itemize) {
        const baseStd = OR_STD_DED[status] || 0;
        if (!itemize) return baseStd;

        // Simple heuristic: itemized ≈ 10% of income, capped between std and 25k
        const est = Math.min(Math.max(baseStd, income * 0.1), 25000);
        return est;
      }

      function computeOrTax(status, taxableIncome) {
        const brackets = OR_BRACKETS[status] || OR_BRACKETS.single;
        let remaining = taxableIncome;
        let lastCap = 0;
        let tax = 0;
        let marginalRate = 0;

        for (const bracket of brackets) {
          if (remaining <= 0) break;
          const band = Math.min(remaining, bracket.upTo - lastCap);
          if (band > 0) {
            tax += band * bracket.rate;
            marginalRate = bracket.rate;
            remaining -= band;
          }
          lastCap = bracket.upTo;
        }

        return { tax, marginalRate };
      }

      function populateBracketTable() {
        const tbody = document.getElementById("or-brackets-table");
        if (!tbody) return;
        tbody.innerHTML = "";

        const labels = {
          single: "Single",
          mfj: "Married filing jointly",
          mfs: "Married filing separately",
          hoh: "Head of household",
        };

        Object.keys(OR_BRACKETS).forEach((status) => {
          const rows = OR_BRACKETS[status];
          rows.forEach((b, idx) => {
            const prevCap = idx === 0 ? 0 : rows[idx - 1].upTo;
            const tr = document.createElement("tr");

            const statusCell = document.createElement("td");
            statusCell.className = "px-3 py-2 border-b align-top";
            statusCell.textContent = idx === 0 ? labels[status] : "";
            tr.appendChild(statusCell);

            const bandCell = document.createElement("td");
            bandCell.className = "px-3 py-2 border-b";
            const from = prevCap;
            const to = b.upTo;
            if (!isFinite(to)) {
              bandCell.textContent = `$${from.toLocaleString()} and above`;
            } else {
              bandCell.textContent = `$${from.toLocaleString()} – $${(
                to - 0.01
              ).toLocaleString()}`;
            }
            tr.appendChild(bandCell);

            const rateCell = document.createElement("td");
            rateCell.className = "px-3 py-2 border-b";
            rateCell.textContent =
              (b.rate * 100).toFixed(2).replace(/\.00$/, "") + "%";
            tr.appendChild(rateCell);

            tbody.appendChild(tr);
          });
        });
      }

      function calculateOregonTax() {
        const status = document.getElementById("or-filing-status").value;
        const income =
          parseFloat(document.getElementById("or-income").value) || 0;
        const preTax =
          parseFloat(document.getElementById("or-pre-tax").value) || 0;
        const fedRate =
          parseFloat(document.getElementById("or-fed-rate").value) || 0;
        const itemize = document.getElementById("or-itemize").checked;
        const errorEl = document.getElementById("or-error");

        errorEl.classList.add("hidden");
        errorEl.textContent = "";

        if (income < 0 || preTax < 0) {
          errorEl.textContent =
            "Income and pre-tax deductions must be zero or positive.";
          errorEl.classList.remove("hidden");
          return;
        }
        if (preTax > income) {
          errorEl.textContent =
            "Pre-tax deductions cannot exceed gross income.";
          errorEl.classList.remove("hidden");
          return;
        }

        const adjIncome = Math.max(0, income - preTax);
        const deductions = estimateDeductions(status, adjIncome, itemize);
        const taxable = Math.max(0, adjIncome - deductions);

        const { tax, marginalRate } = computeOrTax(status, taxable);
        const effectiveRate = income > 0 ? (tax / income) * 100 : 0;

        const estFederal = adjIncome * (fedRate / 100);
        const net = Math.max(0, income - preTax - estFederal - tax);

        // Update summary
        document.getElementById("or-taxable-income").textContent =
          formatMoney(taxable);
        document.getElementById("or-state-tax").textContent = formatMoney(tax);
        document.getElementById("or-effective-rate").textContent =
          formatPercent(effectiveRate);
        document.getElementById("or-marginal-rate").textContent = formatPercent(
          marginalRate * 100
        );

        document.getElementById("or-summary-gross").textContent =
          formatMoney(income);
        document.getElementById("or-summary-pretax").textContent =
          "-" + formatMoney(preTax);
        document.getElementById("or-summary-federal").textContent =
          "-" + formatMoney(estFederal);
        document.getElementById("or-summary-state").textContent =
          "-" + formatMoney(tax);
        document.getElementById("or-summary-net").textContent =
          formatMoney(net);
        document.getElementById("or-summary-monthly").textContent = formatMoney(
          net / 12
        );
        document.getElementById("or-summary-biweekly").textContent =
          formatMoney(net / 26);
      }

      document.addEventListener("DOMContentLoaded", () => {
        populateBracketTable();

        const fedSlider = document.getElementById("or-fed-rate");
        const fedOutput = document.getElementById("or-fed-rate-output");
        if (fedSlider && fedOutput) {
          fedOutput.textContent = fedSlider.value + "%";
          fedSlider.addEventListener("input", () => {
            fedOutput.textContent = fedSlider.value + "%";
          });
        }

        document
          .getElementById("or-calc-btn")
          .addEventListener("click", calculateOregonTax);

        // Recalculate on key inputs change for a smoother UX
        [
          "or-filing-status",
          "or-income",
          "or-pre-tax",
          "or-fed-rate",
          "or-itemize",
        ].forEach((id) => {
          const el = document.getElementById(id);
          if (!el) return;
          el.addEventListener("change", () => {
            // small debounce
            setTimeout(calculateOregonTax, 50);
          });
        });

        // Initial calculation
        calculateOregonTax();
      });

      // MathJax config
      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
