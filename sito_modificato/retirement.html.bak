    <script>
      // Helper: format currency
      function formatCurrency(value) {
        if (!isFinite(value)) return "$0";
        const abs = Math.abs(value);
        const sign = value < 0 ? "-" : "";
        if (abs >= 1e9) return sign + "$" + (abs / 1e9).toFixed(1) + "B";
        if (abs >= 1e6) return sign + "$" + (abs / 1e6).toFixed(1) + "M";
        if (abs >= 1e3) return sign + "$" + (abs / 1e3).toFixed(1) + "k";
        return sign + "$" + abs.toFixed(0);
      }

      function formatPercent(value) {
        if (!isFinite(value)) return "0%";
        return value.toFixed(1) + "%";
      }

      function clamp(value, min, max) {
        return Math.min(Math.max(value, min), max);
      }

      // Sync range labels
      function syncRangeLabel(id, labelId, suffix = "%") {
        const el = document.getElementById(id);
        const label = document.getElementById(labelId);
        if (!el || !label) return;
        label.textContent = parseFloat(el.value).toFixed(1) + suffix;
        el.addEventListener("input", () => {
          label.textContent = parseFloat(el.value).toFixed(1) + suffix;
        });
      }

      document.addEventListener("DOMContentLoaded", () => {
        syncRangeLabel("expected-return", "expected-return-value");
        syncRangeLabel("retirement-return", "retirement-return-value");
        syncRangeLabel("inflation-rate", "inflation-rate-value");
        syncRangeLabel("withdrawal-rate", "withdrawal-rate-value");

        const plannedYearsEl = document.getElementById("planned-years");
        const retirementYearsInput =
          document.getElementById("retirement-years");
        if (plannedYearsEl && retirementYearsInput) {
          plannedYearsEl.textContent = retirementYearsInput.value;
          retirementYearsInput.addEventListener("input", () => {
            plannedYearsEl.textContent = retirementYearsInput.value;
          });
        }

        const btn = document.getElementById("calculate-btn");
        if (btn) btn.addEventListener("click", runRetirementCalc);

        document
          .querySelectorAll("#faq-container .faq-question")
          .forEach((btn) => {
            btn.addEventListener("click", () => {
              const answer = btn.parentElement.querySelector(".faq-answer");
              const icon = btn.querySelector("span:last-child");
              const isHidden = answer.classList.contains("hidden");
              document
                .querySelectorAll("#faq-container .faq-answer")
                .forEach((a) => a.classList.add("hidden"));
              document
                .querySelectorAll(
                  "#faq-container .faq-question span:last-child"
                )
                .forEach((i) => (i.textContent = "+"));
              if (isHidden) {
                answer.classList.remove("hidden");
                icon.textContent = "−";
              }
            });
          });
      });

      function runRetirementCalc() {
        const currentAge =
          parseFloat(document.getElementById("current-age").value) || 0;
        const retirementAge =
          parseFloat(document.getElementById("retirement-age").value) || 0;
        const currentSavings = Math.max(
          0,
          parseFloat(document.getElementById("current-savings").value) || 0
        );
        const annualContribution = Math.max(
          0,
          parseFloat(document.getElementById("annual-contribution").value) || 0
        );
        const expectedReturn =
          (parseFloat(document.getElementById("expected-return").value) || 0) /
          100;
        const retirementReturn =
          (parseFloat(document.getElementById("retirement-return").value) ||
            0) / 100;
        const desiredSpending = Math.max(
          0,
          parseFloat(document.getElementById("retirement-spending").value) || 0
        );
        const monthlyBenefits = Math.max(
          0,
          parseFloat(document.getElementById("social-security").value) || 0
        );
        const inflationRate =
          (parseFloat(document.getElementById("inflation-rate").value) || 0) /
          100;
        const retirementYears = Math.max(
          1,
          parseFloat(document.getElementById("retirement-years").value) || 30
        );
        const withdrawalRate =
          (parseFloat(document.getElementById("withdrawal-rate").value) || 0) /
          100;

        const yearsToRetirement = Math.max(0, retirementAge - currentAge);

        // Future value of current savings
        const fvCurrent =
          currentSavings * Math.pow(1 + expectedReturn, yearsToRetirement);

        // Future value of contributions (ordinary annuity, contributions at end of year)
        let fvContrib = 0;
        if (expectedReturn > 0 && yearsToRetirement > 0) {
          fvContrib =
            annualContribution *
            ((Math.pow(1 + expectedReturn, yearsToRetirement) - 1) /
              expectedReturn);
        } else {
          fvContrib = annualContribution * yearsToRetirement;
        }

        const projectedNestEgg = fvCurrent + fvContrib;

        // Net annual spending from portfolio (today's dollars)
        const netAnnualFromPortfolioToday = Math.max(
          0,
          desiredSpending - 12 * monthlyBenefits
        );

        // Adjust net spending to retirement start in nominal terms
        const netAnnualFromPortfolioAtRetirement =
          netAnnualFromPortfolioToday *
          Math.pow(1 + inflationRate, yearsToRetirement);

        // Required nest egg using withdrawal rate
        const requiredNestEgg =
          withdrawalRate > 0
            ? netAnnualFromPortfolioAtRetirement / withdrawalRate
            : 0;

        // Implied withdrawal rate on projected nest egg
        const impliedWithdrawalRate =
          projectedNestEgg > 0
            ? netAnnualFromPortfolioAtRetirement / projectedNestEgg
            : 0;

        // Estimate years sustainable with inflation-adjusted withdrawals
        // Approximate using real return (retirementReturn - inflationRate)
        const realReturn = retirementReturn - inflationRate;
        let yearsSustainable;
        if (projectedNestEgg <= 0 || netAnnualFromPortfolioAtRetirement <= 0) {
          yearsSustainable = Infinity;
        } else if (Math.abs(realReturn) < 1e-6) {
          // Real return ~ 0: portfolio roughly flat in real terms
          yearsSustainable =
            projectedNestEgg / netAnnualFromPortfolioAtRetirement;
        } else {
          // Use annuity formula inverted: PV = Pmt * (1 - (1+r)^-n)/r  => solve for n
          const r = realReturn;
          const ratio =
            1 - (projectedNestEgg * r) / netAnnualFromPortfolioAtRetirement;
          if (ratio <= 0) {
            yearsSustainable = Infinity; // withdrawals small relative to return
          } else {
            yearsSustainable = -Math.log(ratio) / Math.log(1 + r);
          }
        }

        // Update UI
        document.getElementById("projected-nest-egg").textContent =
          formatCurrency(projectedNestEgg);
        document.getElementById("required-nest-egg").textContent =
          formatCurrency(requiredNestEgg);
        const gap = projectedNestEgg - requiredNestEgg;
        document.getElementById("savings-gap").textContent =
          formatCurrency(gap);
        document.getElementById("implied-withdrawal-rate").textContent =
          formatPercent(impliedWithdrawalRate * 100);

        const yearsSustainableDisplay = !isFinite(yearsSustainable)
          ? "Not expected to run out"
          : yearsSustainable.toFixed(1) + " years";
        document.getElementById("years-sustainable").textContent =
          yearsSustainableDisplay;
        document.getElementById("planned-years").textContent = retirementYears;

        let riskText;
        if (!isFinite(yearsSustainable)) {
          riskText =
            "Low (withdrawals are small relative to portfolio and assumed return)";
        } else if (yearsSustainable >= retirementYears + 5) {
          riskText =
            "Low (portfolio is projected to last beyond your planning horizon)";
        } else if (yearsSustainable >= retirementYears - 5) {
          riskText =
            "Moderate (close to your planning horizon; consider a margin of safety)";
        } else {
          riskText =
            "Elevated (portfolio may run out before your planning horizon)";
        }
        document.getElementById("runout-risk").textContent = riskText;

        // Readiness label
        const readinessLabel = document.getElementById("readiness-label");
        const shortfallRatio = requiredNestEgg > 0 ? gap / requiredNestEgg : 0;
        readinessLabel.classList.remove(
          "bg-green-100",
          "text-green-800",
          "bg-yellow-100",
          "text-yellow-800",
          "bg-red-100",
          "text-red-800"
        );

        if (
          !isFinite(yearsSustainable) ||
          (gap >= 0 && yearsSustainable >= retirementYears)
        ) {
          readinessLabel.textContent =
            "On track (based on current assumptions)";
          readinessLabel.classList.add("bg-green-100", "text-green-800");
        } else if (shortfallRatio > -0.2) {
          readinessLabel.textContent = "Needs attention (small gap)";
          readinessLabel.classList.add("bg-yellow-100", "text-yellow-800");
        } else {
          readinessLabel.textContent = "At risk (significant shortfall)";
          readinessLabel.classList.add("bg-red-100", "text-red-800");
        }

        // What-if suggestions
        const whatIfSaveMore = document.getElementById("whatif-save-more");
        const whatIfRetireLater = document.getElementById(
          "whatif-retire-later"
        );
        const whatIfSpendLess = document.getElementById("whatif-spend-less");

        if (
          requiredNestEgg <= 0 ||
          projectedNestEgg <= 0 ||
          yearsToRetirement <= 0
        ) {
          whatIfSaveMore.textContent =
            "Enter realistic ages, savings, and spending to see personalized suggestions.";
          whatIfRetireLater.textContent =
            "Adjust your retirement age to understand the impact on your plan.";
          whatIfSpendLess.textContent =
            "Lower your desired retirement spending to see how it changes your target nest egg.";
          return;
        }

        // 1) Extra annual savings needed to close gap (approximate, using same expectedReturn)
        let extraAnnual = 0;
        if (gap < 0 && yearsToRetirement > 0) {
          const neededExtraFV = -gap;
          if (expectedReturn > 0) {
            extraAnnual =
              (neededExtraFV * expectedReturn) /
              (Math.pow(1 + expectedReturn, yearsToRetirement) - 1);
          } else {
            extraAnnual = neededExtraFV / yearsToRetirement;
          }
          whatIfSaveMore.textContent =
            "Increase annual savings by about " +
            formatCurrency(extraAnnual) +
            " to fully close the projected gap by retirement (at the same return).";
        } else {
          whatIfSaveMore.textContent =
            "Your current savings rate appears sufficient under these assumptions; extra savings can add more safety or allow earlier retirement.";
        }

        // 2) Delay retirement: estimate extra years needed to close gap
        if (gap < 0) {
          let extraYears = 0;
          let fvTotal = projectedNestEgg;
          let n = yearsToRetirement;
          while (fvTotal < requiredNestEgg && extraYears < 15) {
            n += 1;
            extraYears += 1;
            fvTotal = currentSavings * Math.pow(1 + expectedReturn, n);
            if (expectedReturn > 0) {
              fvTotal +=
                annualContribution *
                ((Math.pow(1 + expectedReturn, n) - 1) / expectedReturn);
            } else {
              fvTotal += annualContribution * n;
            }
          }
          if (fvTotal >= requiredNestEgg) {
            whatIfRetireLater.textContent =
              "Delaying retirement by about " +
              extraYears +
              " year" +
              (extraYears > 1 ? "s" : "") +
              " could close the gap, assuming you keep saving at the current rate.";
          } else {
            whatIfRetireLater.textContent =
              "Even delaying retirement by many years may not fully close the gap at your current savings rate; consider combining a later retirement with higher savings and/or lower spending.";
          }
        } else {
          whatIfRetireLater.textContent =
            "You could consider retiring earlier or building a larger safety margin; try lowering the retirement age or increasing spending to test scenarios.";
        }

        // 3) Reduce spending: how much lower spending to match projected nest egg at chosen withdrawal rate
        const maxNetAnnualFromPortfolioAtRetirement =
          projectedNestEgg * withdrawalRate;
        const maxNetAnnualFromPortfolioToday =
          maxNetAnnualFromPortfolioAtRetirement /
          Math.pow(1 + inflationRate, yearsToRetirement);
        const maxDesiredSpendingToday =
          maxNetAnnualFromPortfolioToday + 12 * monthlyBenefits;

        if (maxDesiredSpendingToday < desiredSpending) {
          const reduction = desiredSpending - maxDesiredSpendingToday;
          whatIfSpendLess.textContent =
            "Reducing desired retirement spending by about " +
            formatCurrency(reduction) +
            " per year (in today's dollars) would align your plan with a " +
            formatPercent(withdrawalRate * 100) +
            " withdrawal rate.";
        } else {
          whatIfSpendLess.textContent =
            "At your current assumptions, your projected nest egg can support your desired spending at the chosen withdrawal rate. You could modestly increase spending or build more cushion.";
        }
      }

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
