    <script>
      // DPO calculator logic
      function formatNumber(num, decimals = 1) {
        if (!isFinite(num)) return "–";
        return Number(num).toLocaleString(undefined, {
          minimumFractionDigits: decimals,
          maximumFractionDigits: decimals,
        });
      }

      function calculateDPO() {
        const method = document.getElementById("dpo-method").value;
        const denomChoice = document.getElementById("dpo-denominator").value;

        const apBeg = parseFloat(document.getElementById("ap-beginning").value);
        const apEnd = parseFloat(document.getElementById("ap-ending").value);
        const cogs = parseFloat(document.getElementById("cogs").value);
        const purchases = parseFloat(
          document.getElementById("purchases").value
        );
        const periodDays =
          parseFloat(document.getElementById("period-days").value) || 0;

        let avgAP;

        if (method === "average") {
          if (isNaN(apBeg) || isNaN(apEnd)) {
            alert(
              "Please enter both beginning and ending accounts payable to use average payables."
            );
            return;
          }
          avgAP = (apBeg + apEnd) / 2;
        } else if (method === "ending") {
          if (isNaN(apEnd)) {
            alert("Please enter ending accounts payable.");
            return;
          }
          avgAP = apEnd;
        } else if (method === "beginning") {
          if (isNaN(apBeg)) {
            alert("Please enter beginning accounts payable.");
            return;
          }
          avgAP = apBeg;
        }

        let denominatorValue;
        let denominatorLabel;

        if (denomChoice === "purchases") {
          if (isNaN(purchases)) {
            alert(
              "Please enter Purchases for the period or switch denominator to COGS."
            );
            return;
          }
          denominatorValue = purchases;
          denominatorLabel = "Purchases";
        } else {
          if (isNaN(cogs)) {
            alert(
              "Please enter COGS for the period or switch denominator to Purchases."
            );
            return;
          }
          denominatorValue = cogs;
          denominatorLabel = "COGS";
        }

        if (denominatorValue <= 0 || avgAP < 0 || periodDays <= 0) {
          alert(
            "Please ensure all inputs are positive and period days is greater than zero."
          );
          return;
        }

        const dpo = (avgAP / denominatorValue) * periodDays;

        // Update results
        document.getElementById("dpo-value").textContent = formatNumber(dpo, 1);
        document.getElementById("avg-ap-detail").textContent =
          avgAP.toLocaleString(undefined, { maximumFractionDigits: 2 });
        document.getElementById(
          "denominator-detail"
        ).textContent = `${denominatorLabel} = ${denominatorValue.toLocaleString(
          undefined,
          { maximumFractionDigits: 2 }
        )}`;
        document.getElementById(
          "formula-detail"
        ).textContent = `DPO = (Average A/P ÷ ${denominatorLabel}) × ${periodDays} days`;

        // Qualitative interpretation
        let qualitative;
        if (dpo < 15) {
          qualitative =
            "Very low DPO – you are paying suppliers very quickly. This is supplier‑friendly but may tie up cash.";
        } else if (dpo < 45) {
          qualitative =
            "Moderate DPO – broadly consistent with common 30–45 day terms in many industries.";
        } else if (dpo < 75) {
          qualitative =
            "High DPO – you are holding cash longer. Check whether this aligns with supplier terms and industry norms.";
        } else {
          qualitative =
            "Very high DPO – investigate whether invoices are being paid late or if there are supply‑chain risks.";
        }
        document.getElementById("dpo-qualitative").textContent = qualitative;

        // Scenario comparison
        document.getElementById("scenario-current").textContent = formatNumber(
          dpo,
          1
        );
        const change =
          parseFloat(document.getElementById("scenario-change").value) || 0;
        const adjusted = dpo + change;
        document.getElementById("scenario-adjusted").textContent = formatNumber(
          adjusted,
          1
        );
        let scenarioComment = "";
        if (change > 0) {
          scenarioComment = `You would pay suppliers about ${formatNumber(
            change,
            1
          )} days later.`;
        } else if (change < 0) {
          scenarioComment = `You would pay suppliers about ${formatNumber(
            Math.abs(change),
            1
          )} days sooner.`;
        } else {
          scenarioComment = "No change in DPO.";
        }
        document.getElementById("scenario-comment").textContent =
          scenarioComment;

        document.getElementById("dpo-results").classList.remove("hidden");
      }

      document
        .getElementById("calculate-dpo")
        .addEventListener("click", calculateDPO);

      document.getElementById("reset-dpo").addEventListener("click", () => {
        [
          "ap-beginning",
          "ap-ending",
          "cogs",
          "purchases",
          "period-days",
          "scenario-change",
        ].forEach((id) => {
          const el = document.getElementById(id);
          if (id === "period-days") {
            el.value = 365;
          } else if (id === "scenario-change") {
            el.value = 5;
          } else {
            el.value = "";
          }
        });
        document.getElementById("dpo-results").classList.add("hidden");
      });

      document
        .getElementById("scenario-change")
        .addEventListener("input", () => {
          const currentText =
            document.getElementById("scenario-current").textContent;
          const current = parseFloat(currentText.replace(/,/g, ""));
          if (!isFinite(current)) return;
          const change =
            parseFloat(document.getElementById("scenario-change").value) || 0;
          const adjusted = current + change;
          document.getElementById("scenario-adjusted").textContent =
            formatNumber(adjusted, 1);
          let scenarioComment = "";
          if (change > 0) {
            scenarioComment = `You would pay suppliers about ${formatNumber(
              change,
              1
            )} days later.`;
          } else if (change < 0) {
            scenarioComment = `You would pay suppliers about ${formatNumber(
              Math.abs(change),
              1
            )} days sooner.`;
          } else {
            scenarioComment = "No change in DPO.";
          }
          document.getElementById("scenario-comment").textContent =
            scenarioComment;
        });

      // Simple FAQ open/close enhancement (optional, works with native <details>)
      document.querySelectorAll("#faq-container details").forEach((d) => {
        d.addEventListener("toggle", () => {
          if (d.open) {
            document
              .querySelectorAll("#faq-container details")
              .forEach((other) => {
                if (other !== d) other.open = false;
              });
          }
        });
      });

      // MathJax config
      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
