    <script>
      // Mobile menu toggle
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const expanded = this.getAttribute("aria-expanded") === "true";
          this.setAttribute("aria-expanded", String(!expanded));
          menu.classList.toggle("hidden");
        });

      const acidTypeEl = document.getElementById("acidType");
      const pKaContainer = document.getElementById("pKaContainer");
      acidTypeEl.addEventListener("change", () => {
        pKaContainer.style.display =
          acidTypeEl.value === "weak" ? "block" : "none";
      });

      const errorMsg = document.getElementById("errorMsg");
      let chart;

      function showError(msg) {
        errorMsg.textContent = msg;
        errorMsg.classList.remove("hidden");
      }

      function clearError() {
        errorMsg.classList.add("hidden");
        errorMsg.textContent = "";
      }

      function safeLog10(x) {
        return Math.log(x) / Math.LN10;
      }

      function computePhStrongAcidStrongBase(Ca, Va, Cb, Vb) {
        const na = Ca * Va;
        const nb = Cb * Vb;
        const Vtot = Va + Vb;
        if (Vtot <= 0) return NaN;

        const Kw = 1e-14;

        if (nb < na) {
          const H = (na - nb) / Vtot;
          return -safeLog10(H);
        } else if (Math.abs(nb - na) < 1e-12) {
          // Neutral solution, include water autoionization
          const H = Math.sqrt(Kw);
          return -safeLog10(H);
        } else {
          const OH = (nb - na) / Vtot;
          const pOH = -safeLog10(OH);
          return 14 - pOH;
        }
      }

      function computePhWeakAcidStrongBase(Ca, Va, Cb, Vb, pKa) {
        const na = Ca * Va;
        const nb = Cb * Vb;
        const Vtot = Va + Vb;
        if (Vtot <= 0) return NaN;

        const Ka = Math.pow(10, -pKa);
        const Kw = 1e-14;

        if (nb < 0 || na <= 0) return NaN;

        if (nb < na && nb > 0) {
          // Buffer region: Henderson–Hasselbalch
          const ratio = nb / (na - nb);
          if (ratio <= 0) {
            // fallback to weak acid equilibrium
            const C = na / Vtot;
            const H = (-Ka + Math.sqrt(Ka * Ka + 4 * Ka * C)) / 2;
            return -safeLog10(H);
          }
          return pKa + safeLog10(ratio);
        } else if (Math.abs(nb - na) < 1e-12) {
          // Equivalence: solution of A- only, weak base
          const Cb_conj = na / Vtot;
          const Kb = Kw / Ka;
          // Solve x^2 / (Cb_conj - x) = Kb  -> approximate quadratic
          const a = 1;
          const b = Kb;
          const c = -Kb * Cb_conj;
          const disc = b * b - 4 * a * c;
          const x = (-b + Math.sqrt(Math.max(disc, 0))) / (2 * a);
          const OH = x;
          const pOH = -safeLog10(OH);
          return 14 - pOH;
        } else if (nb < na && nb === 0) {
          // Initial weak acid only
          const C = na / Vtot;
          const H = (-Ka + Math.sqrt(Ka * Ka + 4 * Ka * C)) / 2;
          return -safeLog10(H);
        } else if (nb > na) {
          // After equivalence: excess strong base
          const OH = (nb - na) / Vtot;
          const pOH = -safeLog10(OH);
          return 14 - pOH;
        } else {
          // Very start or numerical edge
          const C = na / Vtot;
          const H = (-Ka + Math.sqrt(Ka * Ka + 4 * Ka * C)) / 2;
          return -safeLog10(H);
        }
      }

      function generateCurve() {
        clearError();

        const acidType = acidTypeEl.value;
        const Ca = parseFloat(document.getElementById("acidConc").value);
        const Va_ml = parseFloat(document.getElementById("acidVol").value);
        const Cb = parseFloat(document.getElementById("baseConc").value);
        const maxV_ml = parseFloat(document.getElementById("maxVol").value);
        const step_ml = parseFloat(document.getElementById("stepSize").value);
        const pKa = parseFloat(document.getElementById("acidPka").value);

        if (
          ![Ca, Va_ml, Cb, maxV_ml, step_ml].every((v) => isFinite(v) && v > 0)
        ) {
          showError(
            "Please enter positive numeric values for all concentrations, volumes and step size."
          );
          return null;
        }
        if (acidType === "weak" && (!isFinite(pKa) || pKa <= 0 || pKa > 14)) {
          showError(
            "Please enter a realistic pKa value between 0 and 14 for the weak acid."
          );
          return null;
        }
        if (maxV_ml <= step_ml) {
          showError("Max added volume must be greater than the step size.");
          return null;
        }

        const Va = Va_ml / 1000;
        const maxV = maxV_ml / 1000;
        const step = step_ml / 1000;

        const na = Ca * Va;
        const Ve = na / Cb; // L
        const Ve_ml = Ve * 1000;

        const volumes = [];
        const phs = [];

        for (let Vb = 0; Vb <= maxV + 1e-9; Vb += step) {
          const Vb_ml = Vb * 1000;
          let ph;
          if (acidType === "strong") {
            ph = computePhStrongAcidStrongBase(Ca, Va, Cb, Vb);
          } else {
            ph = computePhWeakAcidStrongBase(Ca, Va, Cb, Vb, pKa);
          }
          if (!isFinite(ph)) ph = NaN;
          volumes.push(Vb_ml);
          phs.push(ph);
        }

        return {
          volumes,
          phs,
          Ve_ml,
          na,
          Ca,
          Va_ml,
          Cb,
          maxV_ml,
          pKa,
          acidType,
        };
      }

      function updateOutputs(data) {
        if (!data) return;
        const {
          volumes,
          phs,
          Ve_ml,
          na,
          Ca,
          Va_ml,
          Cb,
          maxV_ml,
          pKa,
          acidType,
        } = data;

        const outAcidMoles = document.getElementById("outAcidMoles");
        const outBaseMolesEq = document.getElementById("outBaseMolesEq");
        const outVe = document.getElementById("outVe");
        const outInitialPh = document.getElementById("outInitialPh");
        const outHalfEqPh = document.getElementById("outHalfEqPh");
        const outEqPh = document.getElementById("outEqPh");
        const outFinalPh = document.getElementById("outFinalPh");

        outAcidMoles.textContent = na.toExponential(3) + " mol";
        outBaseMolesEq.textContent = na.toExponential(3) + " mol";
        outVe.textContent = Ve_ml.toFixed(2) + " mL";

        const initialPh = phs[0];
        outInitialPh.textContent = isFinite(initialPh)
          ? initialPh.toFixed(2)
          : "–";

        // Find pH at half-equivalence and equivalence and final
        function interpPhAtVolume(targetMl) {
          if (targetMl < 0 || targetMl > maxV_ml) return NaN;
          for (let i = 0; i < volumes.length - 1; i++) {
            const v1 = volumes[i],
              v2 = volumes[i + 1];
            if (targetMl >= v1 && targetMl <= v2) {
              const t = (targetMl - v1) / (v2 - v1 || 1);
              const p1 = phs[i],
                p2 = phs[i + 1];
              if (!isFinite(p1) || !isFinite(p2)) return NaN;
              return p1 + t * (p2 - p1);
            }
          }
          return NaN;
        }

        const halfVe_ml = Ve_ml / 2;
        const halfPh = interpPhAtVolume(halfVe_ml);
        outHalfEqPh.textContent = isFinite(halfPh)
          ? halfPh.toFixed(2) + (acidType === "weak" ? " (≈ pKa)" : "")
          : "–";

        const eqPh = interpPhAtVolume(Ve_ml);
        outEqPh.textContent = isFinite(eqPh) ? eqPh.toFixed(2) : "–";

        const finalPh = phs[phs.length - 1];
        outFinalPh.textContent = isFinite(finalPh) ? finalPh.toFixed(2) : "–";
      }

      function plotChart(data) {
        if (!data) return;
        const { volumes, phs, Ve_ml, pKa, acidType, maxV_ml } = data;
        const ctx = document.getElementById("titrationChart").getContext("2d");

        const points = volumes.map((v, i) => ({ x: v, y: phs[i] }));

        // Equivalence and half-equivalence lines
        const eqLine = [
          { x: Ve_ml, y: 0 },
          { x: Ve_ml, y: 14 },
        ];
        const halfVe_ml = Ve_ml / 2;
        const halfEqLine = [
          { x: halfVe_ml, y: 0 },
          { x: halfVe_ml, y: 14 },
        ];

        // Buffer region band for weak acid
        let bufferRegion = null;
        if (acidType === "weak" && isFinite(pKa)) {
          const yMin = Math.max(pKa - 1, 0);
          const yMax = Math.min(pKa + 1, 14);
          bufferRegion = {
            yMin,
            yMax,
          };
        }

        if (chart) {
          chart.destroy();
        }

        chart = new Chart(ctx, {
          type: "scatter",
          data: {
            datasets: [
              {
                label: "pH",
                data: points,
                showLine: true,
                borderColor: "#2563eb",
                backgroundColor: "#2563eb",
                pointRadius: 0,
                borderWidth: 2,
                tension: 0.1,
              },
              {
                label: "Equivalence point",
                data: eqLine,
                type: "line",
                borderColor: "#f97316",
                borderWidth: 1.5,
                borderDash: [6, 4],
                pointRadius: 0,
              },
              {
                label: "Half-equivalence",
                data: halfEqLine,
                type: "line",
                borderColor: "#6b7280",
                borderWidth: 1,
                borderDash: [3, 4],
                pointRadius: 0,
              },
            ],
          },
          options: {
            responsive: true,
            maintainAspectRatio: false,
            interaction: {
              mode: "nearest",
              intersect: false,
            },
            plugins: {
              legend: {
                display: true,
              },
              tooltip: {
                callbacks: {
                  label: function (ctx) {
                    const x = ctx.parsed.x;
                    const y = ctx.parsed.y;
                    return `Vb = ${x.toFixed(2)} mL, pH = ${y.toFixed(2)}`;
                  },
                },
              },
            },
            scales: {
              x: {
                type: "linear",
                title: { display: true, text: "Volume of base added (mL)" },
                min: 0,
                max: maxV_ml,
                ticks: { maxTicksLimit: 10 },
              },
              y: {
                title: { display: true, text: "pH" },
                min: 0,
                max: 14,
              },
            },
          },
        });

        // Add buffer region shading using plugin
        if (bufferRegion) {
          const plugin = {
            id: "bufferRegion",
            beforeDraw(chartInstance, args, opts) {
              const { ctx, chartArea, scales } = chartInstance;
              const yMinPx = scales.y.getPixelForValue(bufferRegion.yMax);
              const yMaxPx = scales.y.getPixelForValue(bufferRegion.yMin);
              ctx.save();
              ctx.fillStyle = "rgba(34,197,94,0.08)";
              ctx.fillRect(
                chartArea.left,
                yMinPx,
                chartArea.right - chartArea.left,
                yMaxPx - yMinPx
              );
              ctx.restore();
            },
          };
          chart.options.plugins.bufferRegion = true;
          chart.$bufferPlugin = plugin;
          Chart.register(plugin);
          chart.update();
        }
      }

      document.getElementById("plotBtn").addEventListener("click", () => {
        const data = generateCurve();
        if (!data) return;
        updateOutputs(data);
        plotChart(data);
      });

      document.getElementById("resetBtn").addEventListener("click", () => {
        document.getElementById("acidType").value = "strong";
        document.getElementById("acidConc").value = "0.10";
        document.getElementById("acidVol").value = "25.0";
        document.getElementById("baseConc").value = "0.10";
        document.getElementById("maxVol").value = "50.0";
        document.getElementById("stepSize").value = "0.25";
        document.getElementById("acidPka").value = "4.76";
        pKaContainer.style.display = "none";
        clearError();
        if (chart) {
          chart.destroy();
          chart = null;
        }
        [
          "outAcidMoles",
          "outBaseMolesEq",
          "outVe",
          "outInitialPh",
          "outHalfEqPh",
          "outEqPh",
          "outFinalPh",
        ].forEach((id) => {
          document.getElementById(id).textContent = "–";
        });
      });

      document
        .getElementById("downloadCsvBtn")
        .addEventListener("click", () => {
          const data = generateCurve();
          if (!data) return;
          const { volumes, phs } = data;
          let csv = "Volume_mL,pH\n";
          for (let i = 0; i < volumes.length; i++) {
            if (!isFinite(phs[i])) continue;
            csv += volumes[i].toFixed(4) + "," + phs[i].toFixed(4) + "\n";
          }
          const blob = new Blob([csv], { type: "text/csv;charset=utf-8;" });
          const url = URL.createObjectURL(blob);
          const a = document.createElement("a");
          a.href = url;
          a.download = "titration-curve.csv";
          document.body.appendChild(a);
          a.click();
          document.body.removeChild(a);
          URL.revokeObjectURL(url);
        });

      // Auto-plot on first load with defaults
      window.addEventListener("load", () => {
        const data = generateCurve();
        if (data) {
          updateOutputs(data);
          plotChart(data);
        }
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
