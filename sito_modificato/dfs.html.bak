    function parseAdjacency(text, directed) {
      const lines = text.split(/\r?\n/);
      const graph = {};
      lines.forEach(line => {
        const trimmed = line.trim();
        if (!trimmed) return;
        const parts = trimmed.split(':');
        const node = parts[0].trim();
        const neighbors = (parts[1] ? parts[1].trim() : '').split(/\s+/).filter(Boolean);
        if (!graph[node]) graph[node] = [];
        neighbors.forEach(nb => {
          if (!graph[node].includes(nb)) graph[node].push(nb);
          if (!directed) {
            if (!graph[nb]) graph[nb] = [];
            if (!graph[nb].includes(node)) graph[nb].push(node);
          }
        });
      });
      return graph;
    }

    function dfs(graph, start, directed) {
      // collect all nodes
      const nodes = Object.keys(graph).sort();
      const color = {};
      const d = {};
      const f = {};
      const parent = {};
      const edges = [];
      nodes.forEach(v => { color[v] = 'white'; parent[v] = null; });
      let time = 0;
      const visitOrder = [];

      function visit(u) {
        color[u] = 'gray';
        time += 1;
        d[u] = time;
        visitOrder.push(u);
        const neighbors = graph[u] || [];
        for (const v of neighbors) {
          if (!color.hasOwnProperty(v)) {
            // unseen node from adjacency
            color[v] = 'white';
            parent[v] = null;
          }
          if (color[v] === 'white') {
            edges.push({ from: u, to: v, type: 'tree' });
            parent[v] = u;
            visit(v);
          } else if (directed) {
            if (color[v] === 'gray') {
              edges.push({ from: u, to: v, type: 'back' });
            } else {
              // color[v] === 'black'
              // classify forward or cross
              if (d[u] < d[v]) {
                edges.push({ from: u, to: v, type: 'forward' });
              } else {
                edges.push({ from: u, to: v, type: 'cross' });
              }
            }
          } else {
            // undirected: ignore edge back to parent
            if (v !== parent[u]) {
              edges.push({ from: u, to: v, type: 'back' });
            }
          }
        }
        color[u] = 'black';
        time += 1;
        f[u] = time;
      }

      // if start specified, visit it first (if present)
      if (start && graph[start]) {
        visit(start);
      }

      // visit remaining connected components
      nodes.forEach(v => {
        if (color[v] === 'white') visit(v);
      });

      return { order: visitOrder, d, f, parent, edges };
    }

    const runBtn = document.getElementById('run-dfs');
    const clearBtn = document.getElementById('clear-output');
    const adjTextarea = document.getElementById('adj-input');
    const startNodeEl = document.getElementById('start-node');
    const directedFlag = document.getElementById('directed-flag');
    const errEl = document.getElementById('input-error');
    const visitOut = document.getElementById('visit-order');
    const tbody = document.getElementById('timestamps-body');
    const edgeList = document.getElementById('edge-list');

    runBtn.addEventListener('click', () => {
      errEl.textContent = '';
      const text = adjTextarea.value;
      const directed = directedFlag.checked;
      const graph = parseAdjacency(text, directed);
      if (Object.keys(graph).length === 0) {
        errEl.textContent = 'Graph is empty. Please enter at least one node.';
        return;
      }
      const start = startNodeEl.value.trim();
      const { order, d, f, parent, edges } = dfs(graph, start, directed);

      visitOut.textContent = order.join(' → ');

      // timestamps table
      tbody.innerHTML = '';
      const nodes = Object.keys(d).sort((a, b) => d[a] - d[b]);
      nodes.forEach(node => {
        const tr = document.createElement('tr');
        tr.innerHTML = `
          <td class="border px-2 py-1">${node}</td>
          <td class="border px-2 py-1">${d[node]}</td>
          <td class="border px-2 py-1">${f[node]}</td>
          <td class="border px-2 py-1">${parent[node] ? parent[node] : '—'}</td>
        `;
        tbody.appendChild(tr);
      });

      // edges
      if (edges.length === 0) {
        edgeList.textContent = 'No edges found.';
      } else {
        edgeList.innerHTML = edges.map(e => `${e.from} → ${e.to} (${e.type})`).join('<br>');
      }
    });

    clearBtn.addEventListener('click', () => {
      visitOut.textContent = '—';
      tbody.innerHTML = '<tr><td class="border px-2 py-1 text-gray-400" colspan="4">Run DFS to populate</td></tr>';
      edgeList.textContent = '—';
      errEl.textContent = '';
    });

    // initial demo run
    runBtn.click();
  </script>

