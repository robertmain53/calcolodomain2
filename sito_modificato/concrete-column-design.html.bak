    <script>
      // Unit system handling
      const state = {
        system: "us", // 'us' or 'metric'
      };

      const unitLabels = {
        us: {
          b: "in",
          h: "in",
          diam: "in",
          cover: "in",
          length: "ft",
          fc: "ksi",
          fy: "ksi",
          bar: "in²",
          tie: "in",
          Pu: "kips",
          Mu: "kip-ft",
        },
        metric: {
          b: "mm",
          h: "mm",
          diam: "mm",
          cover: "mm",
          length: "m",
          fc: "MPa",
          fy: "MPa",
          bar: "cm²",
          tie: "mm",
          Pu: "kN",
          Mu: "kN-m",
        },
      };

      function updateUnitLabels() {
        const u = unitLabels[state.system];
        document.getElementById("label-b-unit").textContent = `(${u.b})`;
        document.getElementById("label-h-unit").textContent = `(${u.h})`;
        document.getElementById("label-diam-unit").textContent = `(${u.diam})`;
        document.getElementById(
          "label-cover-unit"
        ).textContent = `(${u.cover})`;
        document.getElementById(
          "label-length-unit"
        ).textContent = `(${u.length})`;
        document.getElementById("label-fc-unit").textContent = `(${u.fc})`;
        document.getElementById("label-fy-unit").textContent = `(${u.fy})`;
        document.getElementById("label-bar-unit").textContent = `(${u.bar})`;
        document.getElementById("label-tie-unit").textContent = `(${u.tie})`;
        document.getElementById("label-pu-unit").textContent = `(${u.Pu})`;
        document.getElementById("label-mux-unit").textContent = `(${u.Mu})`;
        document.getElementById("label-muy-unit").textContent = `(${u.Mu})`;
      }

      document.getElementById("btn-us").addEventListener("click", () => {
        state.system = "us";
        document
          .getElementById("btn-us")
          .classList.add("bg-blue-600", "text-white");
        document
          .getElementById("btn-us")
          .classList.remove("bg-white", "text-gray-700");
        document
          .getElementById("btn-metric")
          .classList.remove("bg-blue-600", "text-white");
        document
          .getElementById("btn-metric")
          .classList.add("bg-white", "text-gray-700");
        updateUnitLabels();
      });

      document.getElementById("btn-metric").addEventListener("click", () => {
        state.system = "metric";
        document
          .getElementById("btn-metric")
          .classList.add("bg-blue-600", "text-white");
        document
          .getElementById("btn-metric")
          .classList.remove("bg-white", "text-gray-700");
        document
          .getElementById("btn-us")
          .classList.remove("bg-blue-600", "text-white");
        document
          .getElementById("btn-us")
          .classList.add("bg-white", "text-gray-700");
        updateUnitLabels();
      });

      // Shape toggle
      document
        .getElementById("section-shape")
        .addEventListener("change", (e) => {
          const val = e.target.value;
          document
            .getElementById("rect-geom")
            .classList.toggle("hidden", val !== "rect");
          document
            .getElementById("circ-geom")
            .classList.toggle("hidden", val !== "circ");
        });

      // Reset
      document.getElementById("btn-reset").addEventListener("click", () => {
        document.getElementById("column-form").reset();
        document.getElementById("section-shape").value = "rect";
        document.getElementById("rect-geom").classList.remove("hidden");
        document.getElementById("circ-geom").classList.add("hidden");
        document.getElementById("summary-card").classList.add("hidden");
        document.getElementById("strength-tables").classList.add("hidden");
        document.getElementById("slenderness-warning").classList.add("hidden");
        document.getElementById("calc-status").textContent = "";
      });

      // Core calculation
      function runCalculation() {
        const statusEl = document.getElementById("calc-status");
        statusEl.textContent = "Calculating...";

        const shape = document.getElementById("section-shape").value;
        const fc = parseFloat(document.getElementById("fc").value);
        const fy = parseFloat(document.getElementById("fy").value);
        const type = document.getElementById("column-type").value;
        const nBars = parseInt(document.getElementById("num-bars").value, 10);
        const AbarInput = parseFloat(document.getElementById("bar-area").value);
        const cover = parseFloat(document.getElementById("tie-clear").value);
        const PuInput = parseFloat(document.getElementById("pu").value);
        const MuxInput = parseFloat(document.getElementById("mux").value);
        const MuyInput = parseFloat(document.getElementById("muy").value);
        const LuInput = parseFloat(document.getElementById("length").value);

        if (!fc || !fy || !nBars || !AbarInput || !PuInput) {
          statusEl.textContent = "Please fill in all required fields.";
          return;
        }

        // Convert to consistent internal units: in, ksi, kips
        let b, h, D, Lu;
        let Abar = AbarInput;
        let Pu = PuInput;
        let Mux = MuxInput;
        let Muy = MuyInput;

        if (state.system === "metric") {
          // geometry: mm -> in, m -> ft
          if (shape === "rect") {
            b = parseFloat(document.getElementById("b").value) / 25.4;
            h = parseFloat(document.getElementById("h").value) / 25.4;
          } else {
            D = parseFloat(document.getElementById("diam").value) / 25.4;
          }
          const coverMetric = cover / 25.4;
          // length m -> ft
          Lu = LuInput * 3.28084;
          // fc, fy: MPa -> ksi (1 MPa ≈ 0.145038 ksi)
          const fcKsi = fc * 0.145038;
          const fyKsi = fy * 0.145038;
          // bar area: cm² -> in² (1 cm² ≈ 0.1550 in²)
          Abar = AbarInput * 0.155;
          // loads: kN -> kips (1 kN ≈ 0.224809 kips), kN-m -> kip-ft (1 kN-m ≈ 0.737562 kip-ft)
          Pu = PuInput * 0.224809;
          Mux = MuxInput * 0.737562;
          Muy = MuyInput * 0.737562;
          // override fc, fy in ksi
          fcVal = fcKsi;
          fyVal = fyKsi;
        } else {
          if (shape === "rect") {
            b = parseFloat(document.getElementById("b").value);
            h = parseFloat(document.getElementById("h").value);
          } else {
            D = parseFloat(document.getElementById("diam").value);
          }
          Lu = LuInput * 12; // ft -> in for slenderness
          fcVal = fc;
          fyVal = fy;
        }

        const coverIn = state.system === "metric" ? cover / 25.4 : cover;

        // Basic geometry
        let Ag, dimX, dimY;
        if (shape === "rect") {
          Ag = b * h;
          dimX = b;
          dimY = h;
        } else {
          Ag = (Math.PI * D * D) / 4;
          dimX = D;
          dimY = D;
        }

        const As = nBars * Abar;
        const rho = As / Ag;

        // φ factors
        const phi_c = type === "spiral" ? 0.75 : 0.65;
        const phi_f = 0.9;

        // Nominal axial strength
        const Pn = 0.85 * fcVal * (Ag - As) + fyVal * As; // kips
        const phiPn = phi_c * Pn;

        // Approximate flexural capacities about x and y
        // Assume symmetric reinforcement and effective depth ~ 0.8*dim
        const dX = 0.8 * dimY;
        const dY = 0.8 * dimX;
        const MnX = (0.9 * As * fyVal * (dX - (0.5 * dimY) / 2)) / 12; // kip-ft approx
        const MnY = (0.9 * As * fyVal * (dY - (0.5 * dimX) / 2)) / 12; // kip-ft approx
        const phiMnX = phi_f * MnX;
        const phiMnY = phi_f * MnY;

        // Utilization ratios
        const rP = Pu / phiPn;
        const rMx = phiMnX > 0 ? Math.abs(Mux) / phiMnX : 0;
        const rMy = phiMnY > 0 ? Math.abs(Muy) / phiMnY : 0;

        // Biaxial interaction approximation
        let rBiax = 0;
        if (phiMnX > 0 && phiMnY > 0 && phiPn > 0 && Pu < phiPn) {
          const ex = Math.abs(Mux) / phiMnX;
          const ey = Math.abs(Muy) / phiMnY;
          const r = 1.5;
          const rhs = Math.pow(Math.max(1 - rP, 0), r);
          const lhs = Math.pow(ex, r) + Math.pow(ey, r);
          rBiax = rhs > 0 ? lhs / rhs : 0;
        }

        const utilization = Math.max(rP || 0, rMx || 0, rMy || 0, rBiax || 0);

        // Slenderness check (very rough)
        const k = 1.0;
        const Ix =
          shape === "rect"
            ? (b * Math.pow(h, 3)) / 12
            : (Math.PI * Math.pow(D, 4)) / 64;
        const Iy = shape === "rect" ? (h * Math.pow(b, 3)) / 12 : Ix;
        const rx = Math.sqrt(Ix / Ag);
        const ry = Math.sqrt(Iy / Ag);
        const rMin = Math.min(rx, ry);
        const slendernessRatio = rMin > 0 ? (k * Lu) / rMin : 0;

        // Build outputs
        const summaryCard = document.getElementById("summary-card");
        const statusText = document.getElementById("status-text");
        const statusNote = document.getElementById("status-note");
        const utilText = document.getElementById("utilization-text");

        if (!isFinite(utilization) || utilization <= 0) {
          statusText.textContent = "Invalid input";
          statusText.className = "font-bold text-red-600";
          statusNote.textContent =
            "Please check that all geometry, material, and load values are positive.";
          utilText.textContent = "—";
        } else if (utilization <= 1.0) {
          statusText.textContent = "PASS";
          statusText.className = "font-bold text-green-600";
          statusNote.textContent =
            "Section satisfies the simplified ACI 318 strength check for the given factored load combination.";
          utilText.textContent = (utilization * 100).toFixed(1) + " %";
        } else {
          statusText.textContent = "FAIL";
          statusText.className = "font-bold text-red-600";
          statusNote.textContent =
            "Demand exceeds design strength in at least one mode. Increase size/reinforcement or refine analysis.";
          utilText.textContent = (utilization * 100).toFixed(1) + " %";
        }

        summaryCard.classList.remove("hidden");

        // Section summary table
        const secBody = document.getElementById("section-summary-body");
        secBody.innerHTML = "";
        function addRowSec(label, value, unit) {
          const tr = document.createElement("tr");
          tr.innerHTML = `<td class="px-2 py-1 text-gray-700">${label}</td>
                      <td class="px-2 py-1 text-right text-gray-900">${value}</td>
                      <td class="px-2 py-1 text-gray-500">${unit || ""}</td>`;
          secBody.appendChild(tr);
        }

        addRowSec("Shape", shape === "rect" ? "Rectangular" : "Circular", "");
        if (shape === "rect") {
          addRowSec("b", b.toFixed(2), "in");
          addRowSec("h", h.toFixed(2), "in");
        } else {
          addRowSec("D", D.toFixed(2), "in");
        }
        addRowSec("Gross area A\u2095", Ag.toFixed(2), "in²");
        addRowSec("Steel area A\u209B", As.toFixed(2), "in²");
        addRowSec("Reinf. ratio ρ", (rho * 100).toFixed(2), "%");
        addRowSec("f'c", fcVal.toFixed(2), "ksi");
        addRowSec("f\u2099", fyVal.toFixed(2), "ksi");
        addRowSec("Column type", type === "spiral" ? "Spiral" : "Tied", "");
        addRowSec("Unsupported length ℓ\u2095", (Lu / 12).toFixed(2), "ft");

        // Strength summary table
        const strBody = document.getElementById("strength-summary-body");
        strBody.innerHTML = "";
        function addRowStr(label, value, unit) {
          const tr = document.createElement("tr");
          tr.innerHTML = `<td class="px-2 py-1 text-gray-700">${label}</td>
                      <td class="px-2 py-1 text-right text-gray-900">${value} ${
            unit || ""
          }</td>`;
          strBody.appendChild(tr);
        }

        addRowStr("P\u2093 (factored)", Pu.toFixed(1), "kips");
        addRowStr("φP\u2099 (design axial)", phiPn.toFixed(1), "kips");
        addRowStr("P utilization", (rP * 100).toFixed(1), "%");

        addRowStr("M\u2098\u2093 (factored)", Mux.toFixed(1), "kip-ft");
        addRowStr("φM\u2099\u2098 (design)", phiMnX.toFixed(1), "kip-ft");
        addRowStr("M\u2098\u2093 utilization", (rMx * 100).toFixed(1), "%");

        addRowStr("M\u2098\u2099 (factored)", Muy.toFixed(1), "kip-ft");
        addRowStr("φM\u2099\u2099 (design)", phiMnY.toFixed(1), "kip-ft");
        addRowStr("M\u2098\u2099 utilization", (rMy * 100).toFixed(1), "%");

        addRowStr("Biaxial interaction ratio", (rBiax * 100).toFixed(1), "%");
        addRowStr(
          "Critical utilization U",
          (utilization * 100).toFixed(1),
          "%"
        );

        document.getElementById("strength-tables").classList.remove("hidden");

        // Slenderness warning
        const slenderEl = document.getElementById("slenderness-warning");
        const slenderText = document.getElementById("slenderness-text");
        if (slendernessRatio > 0 && slendernessRatio > 34) {
          slenderEl.classList.remove("hidden");
          slenderText.textContent = `Estimated slenderness ratio kℓu/r ≈ ${slendernessRatio.toFixed(
            1
          )}. This suggests a slender column; ACI 318 requires moment magnification or second-order analysis. The current check assumes a short column.`;
        } else if (slendernessRatio > 0 && slendernessRatio > 22) {
          slenderEl.classList.remove("hidden");
          slenderText.textContent = `Estimated slenderness ratio kℓu/r ≈ ${slendernessRatio.toFixed(
            1
          )}. Slenderness effects may be significant depending on frame type (sway vs. non-sway). Consider performing a more detailed analysis.`;
        } else {
          slenderEl.classList.add("hidden");
        }

        statusEl.textContent = "Done.";
      }

      document
        .getElementById("btn-calc")
        .addEventListener("click", runCalculation);

      // Initialize labels
      updateUnitLabels();
    </script>

