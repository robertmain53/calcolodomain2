    <script>
      // Utility: convert glucose to mg/dL if needed
      function glucoseToMgDl(value, unit) {
        if (!isFinite(value)) return NaN;
        if (unit === "mgdl") return value;
        // mmol/L to mg/dL (approx for glucose)
        return value * 18;
      }

      function roundTo(value, decimals) {
        const factor = Math.pow(10, decimals);
        return Math.round(value * factor) / factor;
      }

      function classifyNa(na) {
        if (!isFinite(na)) return "";
        if (na < 135)
          return "Corrected sodium is in the hyponatremic range (< 135 mEq/L). Consider true hypotonic hyponatremia in addition to hyperglycemia.";
        if (na <= 145)
          return "Corrected sodium is within the usual reference range (135–145 mEq/L). Apparent hyponatremia is largely due to hyperglycemia-induced water shifts.";
        return "Corrected sodium is in the hypernatremic range (> 145 mEq/L). Use caution with fluid selection and rate of correction.";
      }

      function buildResultsTable(measuredNa, glucoseMgDl, patientType) {
        const deltaG = Math.max(glucoseMgDl - 100, 0);
        const rows = [];

        // Katz (adult)
        const katzFactor = 1.6;
        const katzNa = measuredNa + katzFactor * (deltaG / 100);
        rows.push({
          id: "katz",
          name: "Katz (classic adult)",
          factor: "+1.6 mEq/L per 100 mg/dL above 100",
          value: katzNa,
        });

        // Hillier (adult)
        const hillierFactor = 2.4;
        const hillierNa = measuredNa + hillierFactor * (deltaG / 100);
        rows.push({
          id: "hillier",
          name: "Hillier (higher correction)",
          factor: "+2.4 mEq/L per 100 mg/dL above 100",
          value: hillierNa,
        });

        // Neonatal / pediatric (approx)
        const neoFactor = 1.6;
        const neoNa = measuredNa + neoFactor * (deltaG / 100);
        rows.push({
          id: "neonatal",
          name: "Neonatal / Pediatric (approximation)",
          factor: "+1.6 mEq/L per 100 mg/dL above 100",
          value: neoNa,
        });

        return rows;
      }

      document.addEventListener("DOMContentLoaded", function () {
        const naInput = document.getElementById("na-measured");
        const glucoseInput = document.getElementById("glucose");
        const glucoseUnitSelect = document.getElementById("glucose-unit");
        const primaryFormulaSelect = document.getElementById("primary-formula");
        const calculateBtn = document.getElementById("calculate-btn");
        const resetBtn = document.getElementById("reset-btn");
        const errorMsg = document.getElementById("error-msg");
        const resultsContainer = document.getElementById("results-container");
        const primaryCorrectedNaSpan = document.getElementById(
          "primary-corrected-na"
        );
        const primaryFormulaLabel = document.getElementById(
          "primary-formula-label"
        );
        const primaryInterpretation = document.getElementById(
          "primary-interpretation"
        );
        const measuredNaDisplay = document.getElementById(
          "measured-na-display"
        );
        const glucoseDisplay = document.getElementById("glucose-display");
        const resultsTableBody = document.getElementById("results-table-body");

        function getPatientType() {
          const radios = document.querySelectorAll(
            'input[name="patient-type"]'
          );
          for (const r of radios) {
            if (r.checked) return r.value;
          }
          return "adult";
        }

        function updatePrimaryLabel(id) {
          if (id === "katz") {
            primaryFormulaLabel.textContent = "Katz";
          } else if (id === "hillier") {
            primaryFormulaLabel.textContent = "Hillier";
          } else {
            primaryFormulaLabel.textContent = "Neonatal/Peds";
          }
        }

        calculateBtn.addEventListener("click", function () {
          errorMsg.textContent = "";
          resultsContainer.classList.add("hidden");
          resultsTableBody.innerHTML = "";

          const naVal = parseFloat(naInput.value);
          const glucoseVal = parseFloat(glucoseInput.value);
          const glucoseUnit = glucoseUnitSelect.value;

          if (!isFinite(naVal) || naVal <= 0) {
            errorMsg.textContent =
              "Please enter a valid measured sodium value.";
            return;
          }
          if (!isFinite(glucoseVal) || glucoseVal <= 0) {
            errorMsg.textContent = "Please enter a valid serum glucose value.";
            return;
          }

          const glucoseMgDl = glucoseToMgDl(glucoseVal, glucoseUnit);
          if (!isFinite(glucoseMgDl)) {
            errorMsg.textContent =
              "Unable to convert glucose value. Please check your input.";
            return;
          }

          const patientType = getPatientType();
          const rows = buildResultsTable(naVal, glucoseMgDl, patientType);

          // Determine primary row
          let primaryId = primaryFormulaSelect.value;
          if (patientType === "neonate" && primaryId === "katz") {
            primaryId = "neonatal";
          }

          const primaryRow = rows.find((r) => r.id === primaryId) || rows[0];

          // Update primary display
          const primaryNaRounded = roundTo(primaryRow.value, 1);
          primaryCorrectedNaSpan.textContent = isFinite(primaryNaRounded)
            ? primaryNaRounded.toFixed(1)
            : "–";
          updatePrimaryLabel(primaryRow.id);
          primaryInterpretation.textContent = classifyNa(primaryRow.value);

          measuredNaDisplay.textContent = roundTo(naVal, 1).toFixed(1);
          glucoseDisplay.textContent = roundTo(glucoseMgDl, 1).toFixed(1);

          // Build table rows
          rows.forEach((row) => {
            const tr = document.createElement("tr");
            if (row.id === primaryRow.id) {
              tr.className = "bg-blue-50";
            } else {
              tr.className = "bg-white";
            }

            const nameTd = document.createElement("td");
            nameTd.className = "px-3 py-2 text-gray-800";
            nameTd.textContent = row.name;

            const factorTd = document.createElement("td");
            factorTd.className = "px-3 py-2 text-gray-600";
            factorTd.textContent = row.factor;

            const valueTd = document.createElement("td");
            valueTd.className = "px-3 py-2 text-gray-900 font-semibold";
            valueTd.textContent = roundTo(row.value, 1).toFixed(1);

            tr.appendChild(nameTd);
            tr.appendChild(factorTd);
            tr.appendChild(valueTd);
            resultsTableBody.appendChild(tr);
          });

          resultsContainer.classList.remove("hidden");
        });

        resetBtn.addEventListener("click", function () {
          naInput.value = "";
          glucoseInput.value = "";
          glucoseUnitSelect.value = "mgdl";
          primaryFormulaSelect.value = "katz";
          errorMsg.textContent = "";
          resultsContainer.classList.add("hidden");
          resultsTableBody.innerHTML = "";
          primaryCorrectedNaSpan.textContent = "–";
          primaryInterpretation.textContent = "";
          measuredNaDisplay.textContent = "";
          glucoseDisplay.textContent = "";
          updatePrimaryLabel("katz");
        });
      });
    </script>

