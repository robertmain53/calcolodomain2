    <script>
      // --- Utility: parse user expression as plain number (supports scientific notation) ---
      function parseInputNumber(str) {
        if (str == null) return NaN;
        const trimmed = String(str).trim();
        if (!trimmed) return NaN;
        const val = Number(trimmed);
        return val;
      }

      function formatRounded(value) {
        if (value === 0) return "0";
        if (!isFinite(value)) return String(value);
        const abs = Math.abs(value);
        if (abs >= 1e6 || abs < 1e-4) {
          return value
            .toExponential(8)
            .replace(/0+e/, "e")
            .replace(/(\.\d*[1-9])0+e/, "$1e");
        }
        return value.toFixed(10).replace(/0+$/, "").replace(/\.$/, "");
      }

      function formatPercentage(p) {
        if (!isFinite(p)) return "–";
        return p.toFixed(4).replace(/0+$/, "").replace(/\.$/, "") + "%";
      }

      // --- Rounding helpers ---
      function roundHalfUp(x, decimals) {
        const factor = Math.pow(10, decimals);
        return Math.round(x * factor) / factor;
      }

      function roundBankers(x, decimals) {
        const factor = Math.pow(10, decimals);
        const scaled = x * factor;
        const floorVal = Math.floor(scaled);
        let diff = scaled - floorVal;
        // Correct for floating point noise around .5
        if (Math.abs(diff - 0.5) < 1e-10) diff = 0.5;
        if (diff < 0.5) {
          return floorVal / factor;
        } else if (diff > 0.5) {
          return (floorVal + 1) / factor;
        } else {
          // exactly midpoint: round to even
          const candidate = floorVal % 2 === 0 ? floorVal : floorVal + 1;
          return candidate / factor;
        }
      }

      function roundUp(x, decimals) {
        const factor = Math.pow(10, decimals);
        return Math.ceil(x * factor) / factor;
      }

      function roundDown(x, decimals) {
        const factor = Math.pow(10, decimals);
        return Math.floor(x * factor) / factor;
      }

      function roundTowardZero(x, decimals) {
        const factor = Math.pow(10, decimals);
        const scaled = x * factor;
        const r = scaled > 0 ? Math.floor(scaled) : Math.ceil(scaled);
        return r / factor;
      }

      function roundAwayFromZero(x, decimals) {
        const factor = Math.pow(10, decimals);
        const scaled = x * factor;
        const r = scaled > 0 ? Math.ceil(scaled) : Math.floor(scaled);
        return r / factor;
      }

      function roundToMultiple(x, multiple) {
        if (multiple === 0) return x;
        const scaled = x / multiple;
        const r = Math.round(scaled);
        return r * multiple;
      }

      function roundToSigFigs(x, sig) {
        if (x === 0) return 0;
        const sign = x < 0 ? -1 : 1;
        const abs = Math.abs(x);
        const exponent = Math.floor(Math.log10(abs));
        const factor = Math.pow(10, exponent - sig + 1);
        const rounded = Math.round(abs / factor) * factor;
        return sign * rounded;
      }

      function computeRounding() {
        const rawInput = document.getElementById("round-input").value;
        const mode = document.getElementById("round-mode").value;
        const decimalsInput = document.getElementById("round-decimals").value;
        const multipleInput = document.getElementById("round-multiple").value;
        const sigfigInput = document.getElementById("round-sigfigs").value;
        const out = document.getElementById("round-result");
        const quick = document.getElementById("round-quick-table");

        try {
          const x = parseInputNumber(rawInput);
          if (!isFinite(x)) {
            throw new Error(
              "Please enter a valid number (decimals or scientific notation)."
            );
          }

          let rounded;
          let methodLabel = "";
          let detailLabel = "";

          if (
            mode === "nearest-decimals" ||
            mode === "nearest-even" ||
            mode === "up" ||
            mode === "down" ||
            mode === "toward-zero" ||
            mode === "away-zero"
          ) {
            let decimals = parseInt(decimalsInput, 10);
            if (!isFinite(decimals) || decimals < 0) decimals = 0;
            if (decimals > 20) decimals = 20;

            switch (mode) {
              case "nearest-decimals":
                rounded = roundHalfUp(x, decimals);
                methodLabel =
                  "Nearest (half up) to " +
                  decimals +
                  " decimal place" +
                  (decimals === 1 ? "" : "s");
                break;
              case "nearest-even":
                rounded = roundBankers(x, decimals);
                methodLabel =
                  "Nearest (bankers, half to even) to " +
                  decimals +
                  " decimal place" +
                  (decimals === 1 ? "" : "s");
                break;
              case "up":
                rounded = roundUp(x, decimals);
                methodLabel =
                  "Always up (ceiling), to " +
                  decimals +
                  " decimal place" +
                  (decimals === 1 ? "" : "s");
                break;
              case "down":
                rounded = roundDown(x, decimals);
                methodLabel =
                  "Always down (floor), to " +
                  decimals +
                  " decimal place" +
                  (decimals === 1 ? "" : "s");
                break;
              case "toward-zero":
                rounded = roundTowardZero(x, decimals);
                methodLabel =
                  "Toward zero, to " +
                  decimals +
                  " decimal place" +
                  (decimals === 1 ? "" : "s");
                break;
              case "away-zero":
                rounded = roundAwayFromZero(x, decimals);
                methodLabel =
                  "Away from zero, to " +
                  decimals +
                  " decimal place" +
                  (decimals === 1 ? "" : "s");
                break;
            }
            detailLabel = "Decimal places: " + decimals;
          } else if (mode === "nearest-multiple") {
            let multiple = parseFloat(multipleInput);
            if (!isFinite(multiple) || multiple <= 0) {
              throw new Error("Please enter a positive multiple to round to.");
            }
            rounded = roundToMultiple(x, multiple);
            methodLabel = "Nearest multiple";
            detailLabel = "Multiple: " + multiple;
          } else if (mode === "significant-figures") {
            let sigfigs = parseInt(sigfigInput, 10);
            if (!isFinite(sigfigs) || sigfigs < 1) {
              throw new Error("Please enter at least 1 significant figure.");
            }
            if (sigfigs > 15) sigfigs = 15;
            rounded = roundToSigFigs(x, sigfigs);
            methodLabel = "Significant figures";
            detailLabel = "Significant figures: " + sigfigs;
          } else {
            throw new Error("Unsupported rounding mode.");
          }

          const absError = rounded - x;
          const relError = x !== 0 ? (absError / x) * 100 : NaN;

          out.innerHTML =
            '<p class="mb-1"><span class="text-gray-600">Original:</span> ' +
            '<span class="font-mono">' +
            formatRounded(x) +
            "</span></p>" +
            '<p class="mb-1"><span class="text-gray-600">Rounded:</span> ' +
            '<span class="font-mono font-semibold">' +
            formatRounded(rounded) +
            "</span></p>" +
            '<p class="mb-1 text-xs text-gray-600">Method: ' +
            methodLabel +
            "</p>" +
            '<p class="mb-1 text-xs text-gray-600">' +
            detailLabel +
            "</p>" +
            '<p class="mt-1 text-xs text-gray-500">Absolute error: ' +
            '<span class="font-mono">' +
            formatRounded(absError) +
            "</span> " +
            ' | Relative error: <span class="font-mono">' +
            formatPercentage(relError) +
            "</span></p>";

          // Quick comparison table for common methods at 0 decimal places (nearest integer)
          const d = 0;
          const nearest = roundHalfUp(x, d);
          const up = roundUp(x, d);
          const down = roundDown(x, d);
          const toward = roundTowardZero(x, d);
          const away = roundAwayFromZero(x, d);
          const bankers = roundBankers(x, d);

          quick.innerHTML =
            '<div class="grid grid-cols-2 text-gray-700">' +
            '<div class="px-3 py-2 border-r border-gray-100">Method</div>' +
            '<div class="px-3 py-2">Result (0 decimals)</div>' +
            "</div>" +
            '<div class="grid grid-cols-2 text-gray-700 border-t border-gray-100">' +
            '<div class="px-3 py-1">Nearest (half up)</div>' +
            '<div class="px-3 py-1 font-mono">' +
            formatRounded(nearest) +
            "</div>" +
            "</div>" +
            '<div class="grid grid-cols-2 text-gray-700 border-t border-gray-100">' +
            '<div class="px-3 py-1">Nearest (bankers)</div>' +
            '<div class="px-3 py-1 font-mono">' +
            formatRounded(bankers) +
            "</div>" +
            "</div>" +
            '<div class="grid grid-cols-2 text-gray-700 border-t border-gray-100">' +
            '<div class="px-3 py-1">Up (ceiling)</div>' +
            '<div class="px-3 py-1 font-mono">' +
            formatRounded(up) +
            "</div>" +
            "</div>" +
            '<div class="grid grid-cols-2 text-gray-700 border-t border-gray-100">' +
            '<div class="px-3 py-1">Down (floor)</div>' +
            '<div class="px-3 py-1 font-mono">' +
            formatRounded(down) +
            "</div>" +
            "</div>" +
            '<div class="grid grid-cols-2 text-gray-700 border-t border-gray-100">' +
            '<div class="px-3 py-1">Toward zero</div>' +
            '<div class="px-3 py-1 font-mono">' +
            formatRounded(toward) +
            "</div>" +
            "</div>" +
            '<div class="grid grid-cols-2 text-gray-700 border-t border-gray-100">' +
            '<div class="px-3 py-1">Away from zero</div>' +
            '<div class="px-3 py-1 font-mono">' +
            formatRounded(away) +
            "</div>" +
            "</div>";
        } catch (err) {
          out.innerHTML =
            '<p class="text-red-600 text-sm">' + err.message + "</p>";
          document.getElementById("round-quick-table").innerHTML =
            '<div class="px-3 py-2 text-gray-500 text-xs">No quick comparison available due to invalid input.</div>';
        }
      }

      function updateParamVisibility() {
        const mode = document.getElementById("round-mode").value;
        const groups = [
          document.getElementById("param-decimals-group"),
          document.getElementById("param-multiple-group"),
          document.getElementById("param-sigfig-group"),
        ];

        groups.forEach((g) => g && g.classList.add("hidden"));

        if (
          mode === "nearest-decimals" ||
          mode === "nearest-even" ||
          mode === "up" ||
          mode === "down" ||
          mode === "toward-zero" ||
          mode === "away-zero"
        ) {
          document
            .getElementById("param-decimals-group")
            ?.classList.remove("hidden");
        } else if (mode === "nearest-multiple") {
          document
            .getElementById("param-multiple-group")
            ?.classList.remove("hidden");
        } else if (mode === "significant-figures") {
          document
            .getElementById("param-sigfig-group")
            ?.classList.remove("hidden");
        }
      }

      document.addEventListener("DOMContentLoaded", function () {
        updateParamVisibility();
        document
          .getElementById("round-mode")
          ?.addEventListener("change", updateParamVisibility);
        document
          .getElementById("round-calc-btn")
          ?.addEventListener("click", computeRounding);
      });

      // MathJax config
      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
