    <script>
      (function () {
        const amountEl = document.getElementById("purchaseAmount");
        const stateRateEl = document.getElementById("stateRate");
        const localRateEl = document.getElementById("localRate");
        const presetEl = document.getElementById("locationPreset");
        const errorEl = document.getElementById("calcError");

        const stateTaxDisplay = document.getElementById("stateTaxDisplay");
        const localTaxDisplay = document.getElementById("localTaxDisplay");
        const totalTaxDisplay = document.getElementById("totalTaxDisplay");
        const totalPriceDisplay = document.getElementById("totalPriceDisplay");
        const effectiveRateDisplay = document.getElementById(
          "effectiveRateDisplay"
        );
        const stateShareDisplay = document.getElementById("stateShareDisplay");
        const localShareDisplay = document.getElementById("localShareDisplay");

        function formatMoney(value) {
          if (!isFinite(value)) return "$0.00";
          return "$" + value.toFixed(2);
        }

        function formatPercent(value) {
          if (!isFinite(value)) return "0.00%";
          return value.toFixed(2) + "%";
        }

        function calculate() {
          errorEl.classList.add("hidden");
          errorEl.textContent = "";

          const amount = parseFloat(amountEl.value);
          const stateRate = parseFloat(stateRateEl.value);
          const localRate = parseFloat(localRateEl.value || "0");

          if (isNaN(amount) || amount < 0) {
            errorEl.textContent =
              "Please enter a valid, non-negative purchase amount.";
            errorEl.classList.remove("hidden");
            return;
          }
          if (isNaN(stateRate) || stateRate < 0 || stateRate > 20) {
            errorEl.textContent =
              "Please enter a valid state rate between 0% and 20%.";
            errorEl.classList.remove("hidden");
            return;
          }
          if (isNaN(localRate) || localRate < 0 || localRate > 20) {
            errorEl.textContent =
              "Please enter a valid local rate between 0% and 20%.";
            errorEl.classList.remove("hidden");
            return;
          }

          const stateTax = amount * (stateRate / 100);
          const localTax = amount * (localRate / 100);
          const totalTax = stateTax + localTax;
          const totalPrice = amount + totalTax;
          const effectiveRate = amount > 0 ? (totalTax / amount) * 100 : 0;

          const stateShare = totalTax > 0 ? (stateTax / totalTax) * 100 : 0;
          const localShare = totalTax > 0 ? (localTax / totalTax) * 100 : 0;

          stateTaxDisplay.textContent = formatMoney(stateTax);
          localTaxDisplay.textContent = formatMoney(localTax);
          totalTaxDisplay.textContent = formatMoney(totalTax);
          totalPriceDisplay.textContent = formatMoney(totalPrice);
          effectiveRateDisplay.textContent = formatPercent(effectiveRate);
          stateShareDisplay.textContent = stateShare.toFixed(1) + "%";
          localShareDisplay.textContent = localShare.toFixed(1) + "%";
        }

        function resetCalculator() {
          amountEl.value = "";
          stateRateEl.value = "4.45";
          localRateEl.value = "";
          presetEl.value = "";
          errorEl.classList.add("hidden");
          errorEl.textContent = "";

          stateTaxDisplay.textContent = "$0.00";
          localTaxDisplay.textContent = "$0.00";
          totalTaxDisplay.textContent = "$0.00";
          totalPriceDisplay.textContent = "$0.00";
          effectiveRateDisplay.textContent = "0.00%";
          stateShareDisplay.textContent = "0%";
          localShareDisplay.textContent = "0%";
        }

        document.getElementById("calcBtn").addEventListener("click", calculate);
        document
          .getElementById("resetBtn")
          .addEventListener("click", resetCalculator);

        // Recalculate on Enter in amount or rate fields
        [amountEl, stateRateEl, localRateEl].forEach((el) => {
          el.addEventListener("keydown", function (e) {
            if (e.key === "Enter") {
              e.preventDefault();
              calculate();
            }
          });
        });

        // Apply preset local rate when location is selected
        presetEl.addEventListener("change", function () {
          const val = this.value;
          if (!val) return;
          const parts = val.split("|");
          if (parts.length === 2) {
            const rate = parseFloat(parts[1]);
            if (!isNaN(rate)) {
              localRateEl.value = rate.toFixed(2);
            }
          }
        });
      })();
    </script>

