    <script>
      function formatCurrency(value) {
        if (isNaN(value)) return "–";
        const abs = Math.abs(value);
        const sign = value < 0 ? "-" : "";
        let formatted;
        if (abs >= 1_000_000_000) {
          formatted = (abs / 1_000_000_000).toFixed(2) + "B";
        } else if (abs >= 1_000_000) {
          formatted = (abs / 1_000_000).toFixed(2) + "M";
        } else if (abs >= 1_000) {
          formatted = (abs / 1_000).toFixed(2) + "k";
        } else {
          formatted = abs.toFixed(2);
        }
        return "$" + sign + formatted;
      }

      function formatNumber(value, decimals = 2) {
        if (isNaN(value)) return "–";
        return value.toFixed(decimals);
      }

      function classifyBadge(value, goodAbove, warnRange) {
        if (isNaN(value)) return "";
        if (value >= goodAbove) return "badge-good";
        if (value >= warnRange) return "badge-warn";
        return "badge-bad";
      }

      document.addEventListener("DOMContentLoaded", () => {
        const eacMethodSelect = document.getElementById("eac-method");
        const pfWrapper = document.getElementById("pf-wrapper");
        const calculateBtn = document.getElementById("calculate-btn");

        eacMethodSelect.addEventListener("change", () => {
          pfWrapper.classList.toggle(
            "hidden",
            eacMethodSelect.value !== "custom"
          );
        });

        calculateBtn.addEventListener("click", () => {
          const bac = parseFloat(document.getElementById("bac").value);
          const plannedPercent = parseFloat(
            document.getElementById("planned-percent").value
          );
          const actualPercent = parseFloat(
            document.getElementById("actual-percent").value
          );
          const ac = parseFloat(document.getElementById("ac").value);
          const pvOverride = parseFloat(
            document.getElementById("pv-override").value
          );
          const evOverride = parseFloat(
            document.getElementById("ev-override").value
          );
          const eacMethod = document.getElementById("eac-method").value;
          const pf = parseFloat(document.getElementById("pf").value);

          if (isNaN(bac) || bac <= 0 || isNaN(ac) || ac < 0) {
            alert("Please enter at least BAC (> 0) and AC (≥ 0).");
            return;
          }

          let pv, ev;
          if (!isNaN(pvOverride)) {
            pv = pvOverride;
          } else if (!isNaN(plannedPercent)) {
            pv = bac * (plannedPercent / 100);
          }

          if (!isNaN(evOverride)) {
            ev = evOverride;
          } else if (!isNaN(actualPercent)) {
            ev = bac * (actualPercent / 100);
          }

          // Core outputs
          document.getElementById("pv-output").textContent =
            pv != null ? formatCurrency(pv) : "–";
          document.getElementById("ev-output").textContent =
            ev != null ? formatCurrency(ev) : "–";
          document.getElementById("ac-output").textContent = formatCurrency(ac);

          let cv = NaN,
            sv = NaN,
            cpi = NaN,
            spi = NaN;
          if (ev != null) {
            cv = ev - ac;
            cpi = ac !== 0 ? ev / ac : NaN;
          }
          if (ev != null && pv != null) {
            sv = ev - pv;
            spi = pv !== 0 ? ev / pv : NaN;
          }

          document.getElementById("cv-output").textContent = isNaN(cv)
            ? "–"
            : formatCurrency(cv);
          document.getElementById("sv-output").textContent = isNaN(sv)
            ? "–"
            : formatCurrency(sv);

          const cpiEl = document.getElementById("cpi-output");
          const spiEl = document.getElementById("spi-output");
          cpiEl.classList.remove("badge-good", "badge-warn", "badge-bad");
          spiEl.classList.remove("badge-good", "badge-warn", "badge-bad");

          cpiEl.textContent = formatNumber(cpi, 2);
          spiEl.textContent = formatNumber(spi, 2);

          if (!isNaN(cpi)) cpiEl.classList.add(classifyBadge(cpi, 1.0, 0.9));
          if (!isNaN(spi)) spiEl.classList.add(classifyBadge(spi, 1.0, 0.9));

          // EAC, ETC, VAC
          let eac = NaN;
          if (ev != null) {
            const remaining = bac - ev;
            switch (eacMethod) {
              case "cpi":
                eac = !isNaN(cpi) && cpi !== 0 ? bac / cpi : NaN;
                break;
              case "ac_plus_remaining":
                eac = ac + remaining;
                break;
              case "cpi_spi":
                const perf = !isNaN(cpi) && !isNaN(spi) ? cpi * spi : NaN;
                eac = !isNaN(perf) && perf !== 0 ? ac + remaining / perf : NaN;
                break;
              case "custom":
                eac = !isNaN(pf) && pf !== 0 ? ac + remaining / pf : NaN;
                break;
            }
          }

          const etc = !isNaN(eac) ? eac - ac : NaN;
          const vac = !isNaN(eac) ? bac - eac : NaN;

          document.getElementById("eac-output").textContent =
            formatCurrency(eac);
          document.getElementById("etc-output").textContent =
            formatCurrency(etc);
          document.getElementById("vac-output").textContent =
            formatCurrency(vac);

          // Mini chart
          const barPv = document.getElementById("bar-pv");
          const barEv = document.getElementById("bar-ev");
          const barAc = document.getElementById("bar-ac");

          const safeBac = bac > 0 ? bac : 1;
          const pvPct =
            pv != null ? Math.max(0, Math.min(100, (pv / safeBac) * 100)) : 0;
          const evPct =
            ev != null ? Math.max(0, Math.min(100, (ev / safeBac) * 100)) : 0;
          const acPct = Math.max(0, Math.min(100, (ac / safeBac) * 100));

          barPv.style.width = pvPct + "%";
          barEv.style.width = evPct + "%";
          barAc.style.width = acPct + "%";

          // Health summary
          const summaryEl = document.getElementById("health-summary");
          let messages = [];

          if (!isNaN(cpi)) {
            if (cpi > 1.05)
              messages.push(
                `Cost performance is strong (CPI ≈ ${cpi.toFixed(
                  2
                )}: under budget).`
              );
            else if (cpi >= 0.95)
              messages.push(
                `Cost performance is close to plan (CPI ≈ ${cpi.toFixed(2)}).`
              );
            else
              messages.push(
                `Cost performance is weak (CPI ≈ ${cpi.toFixed(
                  2
                )}: over budget).`
              );
          }

          if (!isNaN(spi)) {
            if (spi > 1.05)
              messages.push(
                `Schedule performance is strong (SPI ≈ ${spi.toFixed(
                  2
                )}: ahead of schedule).`
              );
            else if (spi >= 0.95)
              messages.push(
                `Schedule performance is close to plan (SPI ≈ ${spi.toFixed(
                  2
                )}).`
              );
            else
              messages.push(
                `Schedule performance is weak (SPI ≈ ${spi.toFixed(
                  2
                )}: behind schedule).`
              );
          }

          if (!isNaN(vac)) {
            if (vac > 0)
              messages.push(
                `Forecast to finish under budget by about ${formatCurrency(
                  vac
                )}.`
              );
            else if (vac < 0)
              messages.push(
                `Forecast to exceed budget by about ${formatCurrency(
                  Math.abs(vac)
                )}.`
              );
          }

          summaryEl.textContent = messages.length
            ? messages.join(" ")
            : "Not enough information to diagnose project health. Please provide BAC, progress and AC.";
        });
      });
    </script>

