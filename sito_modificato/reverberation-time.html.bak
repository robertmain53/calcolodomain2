    <script>
      (function () {
        const unitMetricBtn = document.getElementById("unit-metric");
        const unitImperialBtn = document.getElementById("unit-imperial");
        const lengthInput = document.getElementById("length");
        const widthInput = document.getElementById("width");
        const heightInput = document.getElementById("height");
        const freqSelect = document.getElementById("freq");
        const targetUseSelect = document.getElementById("target-use");
        const targetRangeText = document.getElementById("target-range-text");
        const errorMsg = document.getElementById("error-msg");
        const calcBtn = document.getElementById("calc-btn");
        const resetBtn = document.getElementById("reset-btn");
        const resultsSection = document.getElementById("results-section");
        const resVolume = document.getElementById("res-volume");
        const resArea = document.getElementById("res-area");
        const resAlphaAvg = document.getElementById("res-alpha-avg");
        const resTarget = document.getElementById("res-target");
        const resultsTableBody = document.getElementById("results-table-body");
        const resultsWarning = document.getElementById("results-warning");

        const labelLengthUnit = document.getElementById("label-length-unit");
        const labelWidthUnit = document.getElementById("label-width-unit");
        const labelHeightUnit = document.getElementById("label-height-unit");
        const labelAreaUnit = document.getElementById("label-area-unit");

        const surfaces = [
          { key: "floor", auto: true },
          { key: "ceiling", auto: true },
          { key: "long", auto: true },
          { key: "short", auto: true },
          { key: "extra", auto: false },
        ];

        let currentUnit = "metric"; // 'metric' or 'imperial'

        function updateUnitLabels() {
          if (currentUnit === "metric") {
            labelLengthUnit.textContent = "(m)";
            labelWidthUnit.textContent = "(m)";
            labelHeightUnit.textContent = "(m)";
            labelAreaUnit.textContent = "(m²)";
          } else {
            labelLengthUnit.textContent = "(ft)";
            labelWidthUnit.textContent = "(ft)";
            labelHeightUnit.textContent = "(ft)";
            labelAreaUnit.textContent = "(ft²)";
          }
        }

        function convertValue(val, from, to) {
          if (isNaN(val)) return val;
          if (from === to) return val;
          if (from === "metric" && to === "imperial") {
            return val * 3.28084; // m -> ft
          }
          if (from === "imperial" && to === "metric") {
            return val / 3.28084; // ft -> m
          }
          return val;
        }

        function convertAllDimensions(toUnit) {
          if (currentUnit === toUnit) return;
          const fromUnit = currentUnit;
          [lengthInput, widthInput, heightInput].forEach((input) => {
            const v = parseFloat(input.value);
            if (!isNaN(v)) {
              input.value = convertValue(v, fromUnit, toUnit).toFixed(2);
            }
          });

          // Convert areas for surfaces that have values
          surfaces.forEach((s) => {
            const areaInput = document.getElementById("area-" + s.key);
            const v = parseFloat(areaInput.value);
            if (!isNaN(v)) {
              let val = v;
              if (fromUnit === "metric" && toUnit === "imperial") {
                val = v * 10.7639; // m² -> ft²
              } else if (fromUnit === "imperial" && toUnit === "metric") {
                val = v / 10.7639; // ft² -> m²
              }
              areaInput.value = val.toFixed(2);
            }
          });

          currentUnit = toUnit;
          updateUnitLabels();
        }

        unitMetricBtn.addEventListener("click", () => {
          unitMetricBtn.classList.add("bg-blue-600", "text-white");
          unitMetricBtn.classList.remove(
            "bg-white",
            "text-gray-700",
            "border",
            "border-gray-300"
          );
          unitImperialBtn.classList.remove("bg-blue-600", "text-white");
          unitImperialBtn.classList.add(
            "bg-white",
            "text-gray-700",
            "border",
            "border-gray-300"
          );
          convertAllDimensions("metric");
        });

        unitImperialBtn.addEventListener("click", () => {
          unitImperialBtn.classList.add("bg-blue-600", "text-white");
          unitImperialBtn.classList.remove(
            "bg-white",
            "text-gray-700",
            "border",
            "border-gray-300"
          );
          unitMetricBtn.classList.remove("bg-blue-600", "text-white");
          unitMetricBtn.classList.add(
            "bg-white",
            "text-gray-700",
            "border",
            "border-gray-300"
          );
          convertAllDimensions("imperial");
        });

        // Preset handlers: when preset changes, set alpha if not empty
        surfaces.forEach((s) => {
          const preset = document.getElementById("preset-" + s.key);
          const alpha = document.getElementById("alpha-" + s.key);
          if (preset) {
            preset.addEventListener("change", () => {
              if (preset.value !== "") {
                alpha.value = preset.value;
              }
            });
          }
        });

        // Target use text
        const targetRanges = {
          office: [0.4, 0.7],
          classroom: [0.5, 0.8],
          "speech-hall": [0.6, 1.0],
          "music-room": [0.8, 1.3],
          "small-hall": [1.2, 1.8],
          studio: [0.2, 0.4],
        };

        function updateTargetText() {
          const val = targetUseSelect.value;
          if (!val || !targetRanges[val]) {
            targetRangeText.textContent = "";
            return;
          }
          const [min, max] = targetRanges[val];
          targetRangeText.textContent = `Typical RT60 target range: ${min.toFixed(
            1
          )}–${max.toFixed(1)} s`;
        }

        targetUseSelect.addEventListener("change", updateTargetText);
        updateTargetText();

        function autoFillAreasIfEmpty(L, W, H) {
          const floorArea = L * W;
          const ceilingArea = floorArea;
          const longWalls = 2 * L * H;
          const shortWalls = 2 * W * H;

          const floorInput = document.getElementById("area-floor");
          const ceilingInput = document.getElementById("area-ceiling");
          const longInput = document.getElementById("area-long");
          const shortInput = document.getElementById("area-short");

          if (!floorInput.value) floorInput.value = floorArea.toFixed(2);
          if (!ceilingInput.value) ceilingInput.value = ceilingArea.toFixed(2);
          if (!longInput.value) longInput.value = longWalls.toFixed(2);
          if (!shortInput.value) shortInput.value = shortWalls.toFixed(2);
        }

        function getTargetFeedback(rtValues) {
          const val = targetUseSelect.value;
          if (!val || !targetRanges[val]) {
            return "No target selected. Compare RT60 values with your own design goals.";
          }
          const [min, max] = targetRanges[val];
          const sabine = rtValues.sabine;
          if (!isFinite(sabine)) {
            return "Target selected, but Sabine RT60 could not be computed (check absorption values).";
          }
          if (sabine < min) {
            return `Sabine RT60 is ${sabine.toFixed(
              2
            )} s, which is below the typical range (${min.toFixed(
              1
            )}–${max.toFixed(1)} s). The room may sound too dry for this use.`;
          } else if (sabine > max) {
            return `Sabine RT60 is ${sabine.toFixed(
              2
            )} s, which is above the typical range (${min.toFixed(
              1
            )}–${max.toFixed(1)} s). Additional absorption may be needed.`;
          } else {
            return `Sabine RT60 is ${sabine.toFixed(
              2
            )} s, which is within the typical range (${min.toFixed(
              1
            )}–${max.toFixed(1)} s) for this room use.`;
          }
        }

        function calculate() {
          errorMsg.textContent = "";
          resultsWarning.classList.add("hidden");
          resultsSection.classList.add("hidden");
          resultsTableBody.innerHTML = "";

          const L = parseFloat(lengthInput.value);
          const W = parseFloat(widthInput.value);
          const H = parseFloat(heightInput.value);

          if ([L, W, H].some((v) => isNaN(v) || v <= 0)) {
            errorMsg.textContent =
              "Please enter valid positive room dimensions.";
            return;
          }

          // Auto-fill areas if empty
          autoFillAreasIfEmpty(L, W, H);

          // Collect areas and alphas
          let totalArea = 0;
          let totalAbsorption = 0;
          let sumMinusLog = 0; // for Millington
          let anyAlphaHigh = false;

          for (const s of surfaces) {
            const areaInput = document.getElementById("area-" + s.key);
            const alphaInput = document.getElementById("alpha-" + s.key);
            const area = parseFloat(areaInput.value) || 0;
            let alpha = parseFloat(alphaInput.value);
            if (isNaN(alpha) || alpha < 0) alpha = 0;
            if (alpha > 0.99) alpha = 0.99; // avoid log(0)
            alphaInput.value = alpha.toFixed(2);

            if (area < 0) {
              errorMsg.textContent = "Areas must be non-negative.";
              return;
            }

            totalArea += area;
            totalAbsorption += area * alpha;

            if (alpha > 0 && alpha < 0.99) {
              sumMinusLog += area * -Math.log(1 - alpha);
            }
            if (alpha > 0.6) anyAlphaHigh = true;
          }

          if (totalArea <= 0 || totalAbsorption <= 0) {
            errorMsg.textContent =
              "Total area and absorption must be greater than zero. Check your inputs.";
            return;
          }

          // Convert to metric for core formulas
          let V_m3, S_m2;
          if (currentUnit === "metric") {
            V_m3 = L * W * H;
            S_m2 = totalArea;
          } else {
            const Lm = L / 3.28084;
            const Wm = W / 3.28084;
            const Hm = H / 3.28084;
            V_m3 = Lm * Wm * Hm;
            S_m2 = totalArea / 10.7639;
          }

          const A = totalAbsorption / (currentUnit === "metric" ? 1 : 10.7639); // convert ft² Sabins to m² Sabins if needed
          const alphaAvg = A / S_m2;

          // Sabine
          const T_sabine = (0.161 * V_m3) / A;

          // Eyring / Norris-Eyring
          let T_eyring = NaN;
          if (alphaAvg > 0 && alphaAvg < 0.99) {
            T_eyring = (0.161 * V_m3) / (-S_m2 * Math.log(1 - alphaAvg));
          }

          // Millington–Sette
          let T_millington = NaN;
          if (sumMinusLog > 0) {
            const sumMinusLog_m2 =
              sumMinusLog / (currentUnit === "metric" ? 1 : 10.7639);
            T_millington = (0.161 * V_m3) / sumMinusLog_m2;
          }

          const rtValues = {
            sabine: T_sabine,
            eyring: T_eyring,
            millington: T_millington,
            norris: T_eyring, // same numeric value here
          };

          // Populate summary
          const volDisplay =
            currentUnit === "metric"
              ? `${V_m3.toFixed(1)} m³`
              : `${(V_m3 * 35.3147).toFixed(0)} ft³`;
          const areaDisplay =
            currentUnit === "metric"
              ? `${S_m2.toFixed(1)} m²`
              : `${(S_m2 * 10.7639).toFixed(1)} ft²`;

          resVolume.textContent = volDisplay;
          resArea.textContent = areaDisplay;
          resAlphaAvg.textContent = alphaAvg.toFixed(3);

          resTarget.textContent = getTargetFeedback(rtValues);

          function addRow(name, value, note) {
            const tr = document.createElement("tr");
            const valText = isFinite(value) ? value.toFixed(2) + " s" : "—";
            tr.innerHTML = `
        <td class="px-3 py-2 text-gray-800">${name}</td>
        <td class="px-3 py-2 font-mono">${valText}</td>
        <td class="px-3 py-2 text-gray-600 text-xs">${note}</td>
      `;
            resultsTableBody.appendChild(tr);
          }

          addRow(
            "Sabine",
            T_sabine,
            "Classical formula, good for moderate absorption."
          );
          addRow(
            "Eyring",
            T_eyring,
            "More accurate for high average absorption."
          );
          addRow(
            "Millington–Sette",
            T_millington,
            "Handles different surface absorptions separately."
          );
          addRow(
            "Norris–Eyring",
            T_eyring,
            "Variant of Eyring; same numeric result here."
          );

          // Warnings
          let warnings = [];
          if (alphaAvg < 0.05) {
            warnings.push(
              "Average absorption is very low; RT60 may be impractically long and sensitive to small changes."
            );
          } else if (alphaAvg > 0.5) {
            warnings.push(
              "Average absorption is high; Sabine may underestimate RT60. Prefer Eyring or Millington–Sette."
            );
          }
          if (anyAlphaHigh) {
            warnings.push(
              "Some surfaces have very high absorption (α > 0.6). Real materials may behave differently across frequency bands."
            );
          }
          if (warnings.length) {
            resultsWarning.textContent = warnings.join(" ");
            resultsWarning.classList.remove("hidden");
          }

          resultsSection.classList.remove("hidden");
        }

        calcBtn.addEventListener("click", calculate);

        resetBtn.addEventListener("click", () => {
          lengthInput.value = currentUnit === "metric" ? 6 : 20;
          widthInput.value = currentUnit === "metric" ? 4 : 13;
          heightInput.value = currentUnit === "metric" ? 2.8 : 9;
          freqSelect.value = "500";
          targetUseSelect.value = "";
          updateTargetText();
          surfaces.forEach((s) => {
            const areaInput = document.getElementById("area-" + s.key);
            const preset = document.getElementById("preset-" + s.key);
            const alpha = document.getElementById("alpha-" + s.key);
            areaInput.value = s.key === "extra" ? "0" : "";
            if (preset) preset.value = "";
            if (s.key === "floor") alpha.value = "0.06";
            else if (s.key === "ceiling") alpha.value = "0.05";
            else if (s.key === "long" || s.key === "short")
              alpha.value = "0.03";
            else if (s.key === "extra") alpha.value = "0.15";
          });
          errorMsg.textContent = "";
          resultsSection.classList.add("hidden");
          resultsWarning.classList.add("hidden");
        });

        // Initial setup
        updateUnitLabels();
      })();
    </script>
  
