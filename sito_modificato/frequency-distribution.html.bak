        document.getElementById('calculateBtn').addEventListener('click', function() {
            const dataInput = document.getElementById('dataInput').value;
            const dataArray = dataInput.split(',').map(Number).filter(n => !isNaN(n));
            const frequencyDistribution = dataArray.reduce((acc, val) => {
                acc[val] = acc[val] ? acc[val] + 1 : 1;
                return acc;
            }, {});

            document.getElementById('frequencyDistribution').textContent = JSON.stringify(frequencyDistribution, null, 2);
        });
    </script>


