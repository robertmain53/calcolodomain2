    <script>
      // Tab switching
      const tabRaw = document.getElementById("tab-raw");
      const tabFreq = document.getElementById("tab-frequency");
      const panelRaw = document.getElementById("panel-raw");
      const panelFreq = document.getElementById("panel-frequency");

      function activateTab(tab) {
        if (tab === "raw") {
          tabRaw.classList.add("border-blue-600", "text-blue-600");
          tabRaw.classList.remove("border-transparent", "text-gray-600");
          tabFreq.classList.remove("border-blue-600", "text-blue-600");
          tabFreq.classList.add("border-transparent", "text-gray-600");
          panelRaw.classList.remove("hidden");
          panelFreq.classList.add("hidden");
        } else {
          tabFreq.classList.add("border-blue-600", "text-blue-600");
          tabFreq.classList.remove("border-transparent", "text-gray-600");
          tabRaw.classList.remove("border-blue-600", "text-blue-600");
          tabRaw.classList.add("border-transparent", "text-gray-600");
          panelFreq.classList.remove("hidden");
          panelRaw.classList.add("hidden");
        }
      }

      tabRaw.addEventListener("click", () => activateTab("raw"));
      tabFreq.addEventListener("click", () => activateTab("freq"));

      // Frequency table dynamic rows
      const freqTbody = document.getElementById("freq-tbody");
      const btnAddRow = document.getElementById("btn-add-row");

      function addFreqRow(value = "", freq = "") {
        const tr = document.createElement("tr");
        tr.innerHTML = `
      <td class="border-t border-gray-200">
        <input type="text" class="freq-value w-full border border-gray-200 rounded px-2 py-1 text-sm" value="${value}">
      </td>
      <td class="border-t border-gray-200">
        <input type="number" min="0" step="1" class="freq-count w-full border border-gray-200 rounded px-2 py-1 text-sm" value="${freq}">
      </td>
      <td class="border-t border-gray-200 text-right">
        <button type="button" class="btn-remove-row text-xs text-red-600 hover:underline">Remove</button>
      </td>
    `;
        freqTbody.appendChild(tr);
      }

      btnAddRow.addEventListener("click", () => addFreqRow());

      freqTbody.addEventListener("click", (e) => {
        if (e.target.classList.contains("btn-remove-row")) {
          const rows = freqTbody.querySelectorAll("tr");
          if (rows.length > 1) {
            e.target.closest("tr").remove();
          } else {
            // clear last row instead of removing
            const inputs = e.target.closest("tr").querySelectorAll("input");
            inputs.forEach((inp) => (inp.value = ""));
          }
        }
      });

      // Initialize with 3 empty rows
      for (let i = 0; i < 3; i++) addFreqRow();

      // Parsing helpers
      function parseRawInput(text) {
        if (!text) return [];
        // Split on commas, semicolons, whitespace
        const tokens = text.split(/[\s,;]+/).filter(Boolean);
        const nums = [];
        for (const t of tokens) {
          const v = Number(t);
          if (!Number.isNaN(v) && Number.isFinite(v)) {
            nums.push(v);
          }
        }
        return nums;
      }

      function parseFreqTable() {
        const rows = freqTbody.querySelectorAll("tr");
        const data = [];
        rows.forEach((row) => {
          const valStr = row.querySelector(".freq-value").value.trim();
          const freqStr = row.querySelector(".freq-count").value.trim();
          if (!valStr || !freqStr) return;
          const v = Number(valStr);
          const f = Number(freqStr);
          if (
            !Number.isNaN(v) &&
            Number.isFinite(v) &&
            f > 0 &&
            Number.isInteger(f)
          ) {
            for (let i = 0; i < f; i++) data.push(v);
          }
        });
        return data;
      }

      // Statistics functions
      function computeStats(values, usePopulation) {
        const n = values.length;
        if (n === 0) return null;
        const sorted = [...values].sort((a, b) => a - b);
        const sum = values.reduce((a, b) => a + b, 0);
        const mean = sum / n;

        let median;
        if (n % 2 === 1) {
          median = sorted[(n - 1) / 2];
        } else {
          median = (sorted[n / 2 - 1] + sorted[n / 2]) / 2;
        }

        // Mode
        const freqMap = new Map();
        for (const v of values) {
          freqMap.set(v, (freqMap.get(v) || 0) + 1);
        }
        let maxFreq = 0;
        freqMap.forEach((c) => {
          if (c > maxFreq) maxFreq = c;
        });
        let modes = [];
        if (maxFreq <= 1) {
          modes = []; // no mode
        } else {
          freqMap.forEach((c, v) => {
            if (c === maxFreq) modes.push(v);
          });
          modes.sort((a, b) => a - b);
        }

        const min = sorted[0];
        const max = sorted[n - 1];
        const range = max - min;

        // Variance & SD
        let variance = 0;
        if (n > 1) {
          const denom = usePopulation ? n : n - 1;
          const sqSum = values.reduce((acc, v) => {
            const d = v - mean;
            return acc + d * d;
          }, 0);
          variance = sqSum / denom;
        } else {
          variance = 0;
        }
        const sd = Math.sqrt(variance);

        return {
          n,
          sum,
          mean,
          median,
          modes,
          min,
          max,
          range,
          variance,
          sd,
          sorted,
        };
      }

      function formatNumber(x) {
        if (x === null || x === undefined || Number.isNaN(x)) return "–";
        if (Number.isInteger(x)) return x.toString();
        return x.toFixed(4).replace(/\.?0+$/, "");
      }

      // UI update
      const resCount = document.getElementById("res-count");
      const resMean = document.getElementById("res-mean");
      const resMedian = document.getElementById("res-median");
      const resMode = document.getElementById("res-mode");
      const resRange = document.getElementById("res-range");
      const resMin = document.getElementById("res-min");
      const resMax = document.getElementById("res-max");
      const resSum = document.getElementById("res-sum");
      const resVar = document.getElementById("res-variance");
      const resSd = document.getElementById("res-sd");
      const resVarType = document.getElementById("res-var-type");
      const resSdType = document.getElementById("res-sd-type");
      const statusMessage = document.getElementById("status-message");
      const stepsContainer = document.getElementById("steps-container");
      const stepsContent = document.getElementById("steps-content");
      const chkPopulation = document.getElementById("chk-population");

      function resetResults() {
        resCount.textContent = "–";
        resMean.textContent = "–";
        resMedian.textContent = "–";
        resMode.textContent = "–";
        resRange.textContent = "–";
        resMin.textContent = "–";
        resMax.textContent = "–";
        resSum.textContent = "–";
        resVar.textContent = "–";
        resSd.textContent = "–";
        statusMessage.textContent = "";
        stepsContainer.classList.add("hidden");
        stepsContent.innerHTML = "";
      }

      function updateResults(stats, usePopulation, source) {
        if (!stats) {
          resetResults();
          statusMessage.textContent =
            "No valid numeric data found. Please check your input.";
          return;
        }
        resCount.textContent = stats.n;
        resMean.textContent = formatNumber(stats.mean);
        resMedian.textContent = formatNumber(stats.median);
        if (!stats.modes || stats.modes.length === 0) {
          resMode.textContent = "no mode";
        } else {
          resMode.textContent = stats.modes.map(formatNumber).join(", ");
        }
        resRange.textContent = formatNumber(stats.range);
        resMin.textContent = formatNumber(stats.min);
        resMax.textContent = formatNumber(stats.max);
        resSum.textContent = formatNumber(stats.sum);
        resVar.textContent = formatNumber(stats.variance);
        resSd.textContent = formatNumber(stats.sd);
        const label = usePopulation ? "population" : "sample";
        resVarType.textContent = label;
        resSdType.textContent = label;

        statusMessage.textContent = `Computed from ${stats.n} value${
          stats.n !== 1 ? "s" : ""
        } (${source}).`;

        // Step-by-step explanation (for reasonably small n)
        if (stats.n <= 50) {
          const sortedStr = stats.sorted.map(formatNumber).join(", ");
          const sumStr = stats.sorted.map(formatNumber).join(" + ");
          let html = "";
          html += `<p><strong>1. Sort the data</strong>: [${sortedStr}]</p>`;
          html += `<p><strong>2. Mean</strong>: add all values and divide by n = ${stats.n}:<br>`;
          html += `\\( \\bar{x} = (${sumStr}) / ${stats.n} = ${formatNumber(
            stats.mean
          )} \\)</p>`;

          html += `<p><strong>3. Median</strong>: `;
          if (stats.n % 2 === 1) {
            const k = (stats.n + 1) / 2;
            html += `n is odd, so the median is the ${k}<sup>th</sup> value in the ordered list: ${formatNumber(
              stats.median
            )}.`;
          } else {
            const k1 = stats.n / 2;
            const k2 = k1 + 1;
            html += `n is even, so the median is the average of the ${k1}<sup>th</sup> and ${k2}<sup>th</sup> values: ${formatNumber(
              stats.median
            )}.`;
          }
          html += `</p>`;

          html += `<p><strong>4. Mode</strong>: `;
          if (!stats.modes || stats.modes.length === 0) {
            html += `all values occur with the same frequency → no mode.`;
          } else if (stats.modes.length === 1) {
            html += `the most frequent value is ${formatNumber(
              stats.modes[0]
            )}.`;
          } else {
            html += `the most frequent values are ${stats.modes
              .map(formatNumber)
              .join(", ")} (multimodal).`;
          }
          html += `</p>`;

          html += `<p><strong>5. Range</strong>: max − min = ${formatNumber(
            stats.max
          )} − ${formatNumber(stats.min)} = ${formatNumber(stats.range)}.</p>`;

          stepsContent.innerHTML = html;
          stepsContainer.classList.remove("hidden");
          if (window.MathJax && window.MathJax.typesetPromise) {
            window.MathJax.typesetPromise([stepsContent]);
          }
        } else {
          stepsContainer.classList.add("hidden");
          stepsContent.innerHTML = "";
        }
      }

      // Button handlers
      const btnCalcRaw = document.getElementById("btn-calc-raw");
      const btnClearRaw = document.getElementById("btn-clear-raw");
      const rawInput = document.getElementById("raw-input");

      btnCalcRaw.addEventListener("click", () => {
        const values = parseRawInput(rawInput.value);
        const usePopulation = chkPopulation.checked;
        const stats = computeStats(values, usePopulation);
        updateResults(stats, usePopulation, "raw data");
      });

      btnClearRaw.addEventListener("click", () => {
        rawInput.value = "";
        resetResults();
      });

      const btnCalcFreq = document.getElementById("btn-calc-freq");
      const btnClearFreq = document.getElementById("btn-clear-freq");

      btnCalcFreq.addEventListener("click", () => {
        const values = parseFreqTable();
        const usePopulation = chkPopulation.checked;
        const stats = computeStats(values, usePopulation);
        updateResults(stats, usePopulation, "frequency table");
      });

      btnClearFreq.addEventListener("click", () => {
        freqTbody.innerHTML = "";
        for (let i = 0; i < 3; i++) addFreqRow();
        resetResults();
      });

      // Auto-calc on input blur for convenience (raw mode)
      rawInput.addEventListener("blur", () => {
        if (!rawInput.value.trim()) return;
        const values = parseRawInput(rawInput.value);
        if (values.length === 0) return;
        const usePopulation = chkPopulation.checked;
        const stats = computeStats(values, usePopulation);
        updateResults(stats, usePopulation, "raw data");
      });
    </script>

