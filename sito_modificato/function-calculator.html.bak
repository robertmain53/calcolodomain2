    <script>
      // Function calculator logic
      const fnInput = document.getElementById('function-input');
      const xValueInput = document.getElementById('x-value');
      const fromXInput = document.getElementById('from-x');
      const toXInput = document.getElementById('to-x');

      const btnEvaluate = document.getElementById('btn-evaluate');
      const btnTable = document.getElementById('btn-table');
      const btnPlot = document.getElementById('btn-plot');
      const btnDerivative = document.getElementById('btn-derivative');

      const evalResult = document.getElementById('eval-result');
      const evalContent = document.getElementById('eval-content');

      const derivativeResult = document.getElementById('derivative-result');
      const derivativeContent = document.getElementById('derivative-content');

      const tableResult = document.getElementById('table-result');
      const valuesTable = document.getElementById('values-table').querySelector('tbody');

      let chartInstance = null;

      function safeEvaluate(expr, xVal) {
        try {
          const scope = { x: xVal, pi: Math.PI, e: Math.E };
          return math.evaluate(expr, scope);
        } catch (err) {
          return NaN;
        }
      }

      function evaluateAtPoint() {
        const expr = fnInput.value.trim();
        const xVal = parseFloat(xValueInput.value);
        if (!expr) return;
        const val = safeEvaluate(expr, xVal);
        evalResult.classList.remove('hidden');
        if (isNaN(val)) {
          evalContent.textContent = 'Function could not be evaluated at x = ' + xVal + '. Check your syntax or domain (e.g. log of negative numbers).';
        } else {
          evalContent.innerHTML = '<strong>f(' + xVal + ')</strong> = ' + val;
        }
      }

      function generateTable() {
        const expr = fnInput.value.trim();
        const from = parseFloat(fromXInput.value);
        const to = parseFloat(toXInput.value);
        if (!expr || isNaN(from) || isNaN(to)) return;
        const step = (to - from) / 20; // 21 points
        valuesTable.innerHTML = '';
        for (let x = from; x <= to + step/2; x += step) {
          const y = safeEvaluate(expr, x);
          if (isNaN(y)) continue;
          const row = document.createElement('tr');
          const tdX = document.createElement('td');
          const tdY = document.createElement('td');
          tdX.textContent = x.toFixed(3);
          tdY.textContent = Math.abs(y) > 1e6 ? y.toExponential(4) : y.toFixed(6).replace(/\.?0+$/, '');
          row.appendChild(tdX);
          row.appendChild(tdY);
          valuesTable.appendChild(row);
        }
        tableResult.classList.remove('hidden');
      }

      function plotFunction() {
        const expr = fnInput.value.trim();
        const from = parseFloat(fromXInput.value);
        const to = parseFloat(toXInput.value);
        if (!expr || isNaN(from) || isNaN(to)) return;
        const points = 200;
        const xs = [];
        const ys = [];
        const step = (to - from) / points;
        for (let i = 0; i <= points; i++) {
          const x = from + i * step;
          const y = safeEvaluate(expr, x);
          if (isNaN(y) || Math.abs(y) > 1e6) {
            xs.push(x);
            ys.push(null);
          } else {
            xs.push(x);
            ys.push(y);
          }
        }
        const ctx = document.getElementById('function-chart').getContext('2d');
        if (chartInstance) {
          chartInstance.destroy();
        }
        chartInstance = new Chart(ctx, {
          type: 'line',
          data: {
            labels: xs,
            datasets: [{
              label: 'f(x)',
              data: ys,
              borderWidth: 1,
              pointRadius: 0
            }]
          },
          options: {
            responsive: true,
            scales: {
              x: {
                title: { display: true, text: 'x' }
              },
              y: {
                title: { display: true, text: 'f(x)' }
              }
            },
            plugins: {
              legend: { display: false }
            }
          }
        });
      }

      function showDerivative() {
        const expr = fnInput.value.trim();
        if (!expr) return;
        try {
          const derivative = math.derivative(expr, 'x').toString();
          const xVal = parseFloat(xValueInput.value);
          const slope = safeEvaluate(derivative, xVal);
          derivativeResult.classList.remove('hidden');
          derivativeContent.innerHTML = '<p><strong>f\'(x) = </strong><code>' + derivative + '</code></p>' +
            (isNaN(slope) ? '' : '<p class="mt-2">At x = ' + xVal + ', slope of tangent: <strong>' + slope + '</strong></p>');
        } catch (err) {
          derivativeResult.classList.remove('hidden');
          derivativeContent.textContent = 'Could not compute symbolic derivative for this function.';
        }
      }

      btnEvaluate.addEventListener('click', evaluateAtPoint);
      btnTable.addEventListener('click', generateTable);
      btnPlot.addEventListener('click', plotFunction);
      btnDerivative.addEventListener('click', showDerivative);

      // initial draw
      plotFunction();
    </script>

