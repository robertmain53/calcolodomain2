  <script>
    // mobile menu
    const mobileToggle = document.getElementById('mobile-menu-toggle');
    const mobileMenu = document.getElementById('mobile-menu');
    if (mobileToggle && mobileMenu) {
      mobileToggle.addEventListener('click', () => {
        const isHidden = mobileMenu.classList.contains('hidden');
        mobileMenu.classList.toggle('hidden', !isHidden);
        mobileToggle.setAttribute('aria-expanded', String(isHidden));
      });
    }

    const wordsInput = document.getElementById('words-input');
    const convertBtn = document.getElementById('convert-btn');
    const inputError = document.getElementById('input-error');
    const resultNumber = document.getElementById('result-number');
    const resultDetail = document.getElementById('result-detail');
    const exampleItems = document.querySelectorAll('.example-item');

    exampleItems.forEach(item => {
      item.addEventListener('click', () => {
        wordsInput.value = item.textContent.trim();
        convertWords();
      });
    });

    function normalizeText(str) {
      return str
        .toLowerCase()
        .replace(/-/g, ' ')
        .replace(/[^a-z0-9\s\.]/g, ' ')
        .replace(/\s+/g, ' ')
        .trim();
    }

    // Core conversion
    function wordsToNumber(text) {
      const smalls = {
        "zero":0,"one":1,"two":2,"three":3,"four":4,"five":5,"six":6,"seven":7,"eight":8,"nine":9,
        "ten":10,"eleven":11,"twelve":12,"thirteen":13,"fourteen":14,"fifteen":15,"sixteen":16,
        "seventeen":17,"eighteen":18,"nineteen":19
      };
      const tens = {
        "twenty":20,"thirty":30,"forty":40,"fifty":50,"sixty":60,"seventy":70,"eighty":80,"ninety":90
      };
      const scales = {
        "hundred":100,
        "thousand":1000,
        "million":1000000,
        "billion":1000000000
      };
      const digitWords = {
        "zero":"0","one":"1","two":"2","three":"3","four":"4","five":"5","six":"6","seven":"7","eight":"8","nine":"9"
      };

      let negative = false;
      let tokens = normalizeText(text).split(' ').filter(Boolean);

      if (tokens[0] === 'negative' || tokens[0] === 'minus') {
        negative = true;
        tokens.shift();
      }

      // Detect decimal part
      let decimalIndex = tokens.indexOf('point');
      let integerTokens = decimalIndex >= 0 ? tokens.slice(0, decimalIndex) : tokens;
      let decimalTokens = decimalIndex >= 0 ? tokens.slice(decimalIndex + 1) : [];

      let current = 0;
      let total = 0;

      for (let i = 0; i < integerTokens.length; i++) {
        const w = integerTokens[i];
        if (w === 'and') {
          continue;
        } else if (smalls.hasOwnProperty(w)) {
          current += smalls[w];
        } else if (tens.hasOwnProperty(w)) {
          current += tens[w];
        } else if (scales.hasOwnProperty(w)) {
          if (w === 'hundred') {
            current = current === 0 ? 100 : current * 100;
          } else {
            current *= scales[w];
            total += current;
            current = 0;
          }
        } else if (!isNaN(parseFloat(w))) {
          // numeric token
          current += parseFloat(w);
        } else {
          // unrecognized word, ignore (to allow sentences)
        }
      }

      total += current;

      // decimal
      let decimalStr = '';
      if (decimalTokens.length > 0) {
        for (let i = 0; i < decimalTokens.length; i++) {
          const w = decimalTokens[i];
          if (digitWords[w]) {
            decimalStr += digitWords[w];
          } else if (!isNaN(parseInt(w))) {
            decimalStr += String(parseInt(w));
          }
        }
      }

      let finalNumber = decimalStr ? parseFloat(total + '.' + decimalStr) : total;
      if (negative) finalNumber = -finalNumber;
      return finalNumber;
    }

    function convertWords() {
      inputError.textContent = "";
      const raw = wordsInput.value.trim();
      if (!raw) {
        inputError.textContent = "Please enter some words to convert.";
        resultNumber.textContent = "—";
        resultDetail.textContent = "";
        return;
      }
      const num = wordsToNumber(raw);
      if (isNaN(num)) {
        inputError.textContent = "Could not parse the number words. Try a simpler phrasing.";
        resultNumber.textContent = "—";
        resultDetail.textContent = "";
        return;
      }
      resultNumber.textContent = num.toString();
      resultDetail.textContent = "Parsed successfully from: “" + raw + "”.";
    }

    convertBtn.addEventListener('click', convertWords);
    // initial
    convertWords();
  </script>

