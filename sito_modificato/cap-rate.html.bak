    <script>
      // Utility formatting
      function formatCurrency(value) {
        if (!isFinite(value)) return "–";
        return (
          "$" + value.toLocaleString(undefined, { maximumFractionDigits: 0 })
        );
      }
      function formatPercent(value, digits = 2) {
        if (!isFinite(value)) return "–";
        return value.toFixed(digits) + "%";
      }

      let currentMode = "caprate"; // 'caprate' | 'value' | 'noi'

      function setMode(mode) {
        currentMode = mode;
        const tabCap = document.getElementById("tab-caprate");
        const tabVal = document.getElementById("tab-value");
        const tabNoi = document.getElementById("tab-noi");

        [tabCap, tabVal, tabNoi].forEach((btn) => {
          btn.classList.remove("bg-blue-600", "text-white");
          btn.classList.add("bg-white", "text-gray-700");
        });

        if (mode === "caprate") {
          tabCap.classList.add("bg-blue-600", "text-white");
          tabCap.classList.remove("bg-white", "text-gray-700");
        } else if (mode === "value") {
          tabVal.classList.add("bg-blue-600", "text-white");
          tabVal.classList.remove("bg-white", "text-gray-700");
        } else {
          tabNoi.classList.add("bg-blue-600", "text-white");
          tabNoi.classList.remove("bg-white", "text-gray-700");
        }
      }

      function recalcNOIFromDetails() {
        const gross =
          parseFloat(document.getElementById("grossIncome").value) || 0;
        const vac =
          parseFloat(document.getElementById("vacancyRate").value) || 0;
        const opex =
          parseFloat(document.getElementById("operatingExpenses").value) || 0;

        const vacancyLoss = gross * (vac / 100);
        const effectiveGross = gross - vacancyLoss;
        const noi = effectiveGross - opex;

        document.getElementById("noi").value =
          noi > 0 ? noi.toFixed(2) : noi.toFixed(2);
      }

      function calculate() {
        const price = parseFloat(
          document.getElementById("purchasePrice").value
        );
        const noiInput = parseFloat(document.getElementById("noi").value);
        const capRateInput = parseFloat(
          document.getElementById("capRateInput").value
        );
        const gross = parseFloat(document.getElementById("grossIncome").value);
        const vac = parseFloat(document.getElementById("vacancyRate").value);
        const opex = parseFloat(
          document.getElementById("operatingExpenses").value
        );

        let noi = noiInput;
        // If NOI not provided but details are, build NOI
        if (!noiInput && (gross || opex)) {
          const vacancyLoss = (gross || 0) * ((vac || 0) / 100);
          const effectiveGross = (gross || 0) - vacancyLoss;
          noi = effectiveGross - (opex || 0);
        }

        let capRate = NaN;
        let value = price;

        if (currentMode === "caprate") {
          if (noi > 0 && price > 0) {
            capRate = (noi / price) * 100;
          }
        } else if (currentMode === "value") {
          if (noi > 0 && capRateInput > 0) {
            value = noi / (capRateInput / 100);
            capRate = capRateInput;
          }
        } else if (currentMode === "noi") {
          if (price > 0 && capRateInput > 0) {
            noi = price * (capRateInput / 100);
            capRate = capRateInput;
          }
        }

        // Fallback: if capRate not set but we have noi & value, compute
        if (!isFinite(capRate) && noi > 0 && value > 0) {
          capRate = (noi / value) * 100;
        }

        const resultsEl = document.getElementById("results");
        resultsEl.classList.remove("hidden");

        document.getElementById("result-caprate").textContent =
          formatPercent(capRate);
        document.getElementById("result-noi").textContent = formatCurrency(noi);
        document.getElementById("result-value").textContent =
          formatCurrency(value);

        let noiMargin = NaN;
        if (gross && gross > 0 && isFinite(noi)) {
          noiMargin = (noi / gross) * 100;
        }
        document.getElementById("result-noi-margin").textContent = isFinite(
          noiMargin
        )
          ? formatPercent(noiMargin, 1)
          : "–";

        // Interpretation text
        const interp = document.getElementById("result-interpretation");
        if (isFinite(capRate)) {
          let quality = "";
          if (capRate < 4)
            quality =
              "This is a very low cap rate, typical of prime, low-risk markets or aggressively priced assets.";
          else if (capRate < 6)
            quality =
              "This cap rate is on the lower side, often associated with strong locations and stable income.";
          else if (capRate < 8)
            quality =
              "This cap rate is in a mid-range that many investors consider a balance of risk and return.";
          else
            quality =
              "This is a relatively high cap rate, which may reflect higher risk, weaker location, or significant value-add potential.";

          interp.textContent = `At a cap rate of ${formatPercent(
            capRate
          )}, the property’s unlevered annual return (based on current NOI and value) is approximately ${capRate.toFixed(
            2
          )}% before financing and taxes. ${quality}`;
        } else {
          interp.textContent =
            "Enter at least two of the three variables (NOI, property value, cap rate) to compute the missing one.";
        }

        // Update scenario table defaults
        if (isFinite(noi)) {
          document.getElementById("sc1-noi").value = noi.toFixed(0);
        }
        if (isFinite(value)) {
          document.getElementById("sc1-price").value = value.toFixed(0);
        }
        updateScenarios();
      }

      function clearAll() {
        [
          "purchasePrice",
          "noi",
          "capRateInput",
          "grossIncome",
          "vacancyRate",
          "operatingExpenses",
          "sc1-noi",
          "sc1-price",
          "sc2-noi",
          "sc2-price",
        ].forEach((id) => {
          const el = document.getElementById(id);
          if (el) el.value = "";
        });
        document.getElementById("results").classList.add("hidden");
        document.getElementById("result-interpretation").textContent = "";
        document.getElementById("sc1-cap").textContent = "–";
        document.getElementById("sc2-cap").textContent = "–";
      }

      function updateScenarios(fromMain = false) {
        if (fromMain) {
          const noi = parseFloat(document.getElementById("noi").value);
          const price = parseFloat(
            document.getElementById("purchasePrice").value
          );
          if (noi) document.getElementById("sc1-noi").value = noi.toFixed(0);
          if (price)
            document.getElementById("sc1-price").value = price.toFixed(0);
        }
        const sc1Noi = parseFloat(document.getElementById("sc1-noi").value);
        const sc1Price = parseFloat(document.getElementById("sc1-price").value);
        const sc2Noi = parseFloat(document.getElementById("sc2-noi").value);
        const sc2Price = parseFloat(document.getElementById("sc2-price").value);

        const sc1Cap =
          sc1Noi > 0 && sc1Price > 0 ? (sc1Noi / sc1Price) * 100 : NaN;
        const sc2Cap =
          sc2Noi > 0 && sc2Price > 0 ? (sc2Noi / sc2Price) * 100 : NaN;

        document.getElementById("sc1-cap").textContent = isFinite(sc1Cap)
          ? formatPercent(sc1Cap, 2)
          : "–";
        document.getElementById("sc2-cap").textContent = isFinite(sc2Cap)
          ? formatPercent(sc2Cap, 2)
          : "–";
      }

      document.addEventListener("DOMContentLoaded", function () {
        document
          .getElementById("tab-caprate")
          .addEventListener("click", () => setMode("caprate"));
        document
          .getElementById("tab-value")
          .addEventListener("click", () => setMode("value"));
        document
          .getElementById("tab-noi")
          .addEventListener("click", () => setMode("noi"));

        document
          .getElementById("btn-build-noi")
          .addEventListener("click", recalcNOIFromDetails);
        document
          .getElementById("btn-calc")
          .addEventListener("click", calculate);
        document
          .getElementById("btn-clear")
          .addEventListener("click", clearAll);
        document
          .getElementById("btn-scenarios")
          .addEventListener("click", () => updateScenarios(true));

        ["sc1-noi", "sc1-price", "sc2-noi", "sc2-price"].forEach((id) => {
          document
            .getElementById(id)
            .addEventListener("input", () => updateScenarios(false));
        });
      });
    </script>

    <script>
      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
