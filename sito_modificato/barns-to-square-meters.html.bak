  <script>
    (function() {
      const barnInput = document.getElementById('barnInput');
      const barnUnit = document.getElementById('barnUnit');
      const outM2 = document.getElementById('outM2');
      const outBarn = document.getElementById('outBarn');
      const outPb = document.getElementById('outPb');
      const outFb = document.getElementById('outFb');
      const tableBody = document.getElementById('tableBody');

      // base: 1 barn = 1e-28 m^2
      const BARN_TO_M2 = 1e-28;

      function toBarns(value, unit) {
        if (unit === 'b') return value;
        if (unit === 'mb') return value * 1e-3;
        if (unit === 'ub') return value * 1e-6;
        if (unit === 'nb') return value * 1e-9;
        if (unit === 'pb') return value * 1e-12;
        if (unit === 'fb') return value * 1e-15;
        if (unit === 'am2') {
          // value is already m² → convert to barns
          return value / BARN_TO_M2;
        }
        return value;
      }

      function formatSci(num) {
        if (!isFinite(num)) return '—';
        if (num === 0) return '0';
        return num.toExponential(6);
      }

      function update() {
        const val = parseFloat(barnInput.value);
        if (isNaN(val)) {
          outM2.textContent = '—';
          outBarn.textContent = '—';
          outPb.textContent = '—';
          outFb.textContent = '—';
          return;
        }
        // convert input to barns
        const barns = toBarns(val, barnUnit.value);
        // m2
        const m2 = barns * BARN_TO_M2;
        // picobarns & femtobarns for HEP
        const pb = barns * 1e12;
        const fb = barns * 1e15;

        outM2.textContent = formatSci(m2);
        outBarn.textContent = barns.toExponential(6);
        outPb.textContent = pb.toExponential(6);
        outFb.textContent = fb.toExponential(6);
      }

      barnInput.addEventListener('input', update);
      barnUnit.addEventListener('change', update);

      // fill table
      const samples = [1, 10, 100, 1e3, 1e6];
      samples.forEach(b => {
        const m2 = b * BARN_TO_M2;
        const pb = b * 1e12;
        const tr = document.createElement('tr');
        tr.innerHTML = `
          <td>${b.toExponential ? b.toExponential(1) : b}</td>
          <td>${m2.toExponential(6)}</td>
          <td>${pb.toExponential(6)}</td>
        `;
        tableBody.appendChild(tr);
      });

      // init
      barnInput.value = 5;
      update();
    })();
  </script>

