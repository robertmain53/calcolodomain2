    <script>
      // Utility: normal CDF approximation
      function normCdf(x) {
        // Abramowitz & Stegun approximation
        var sign = x < 0 ? -1 : 1;
        x = Math.abs(x) / Math.sqrt(2);
        var t = 1 / (1 + 0.3275911 * x);
        var a1 = 0.254829592,
          a2 = -0.284496736,
          a3 = 1.421413741,
          a4 = -1.453152027,
          a5 = 1.061405429;
        var erf =
          1 -
          ((((a5 * t + a4) * t + a3) * t + a2) * t + a1) * t * Math.exp(-x * x);
        return 0.5 * (1 + sign * erf);
      }

      function formatCurrency(value) {
        if (!isFinite(value)) return "–";
        return value.toLocaleString(undefined, {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 2,
        });
      }

      function formatPercent(value) {
        if (!isFinite(value)) return "–";
        return (value * 100).toFixed(2) + "%";
      }

      function calculateCashSecuredPut() {
        const stockPrice =
          parseFloat(document.getElementById("stockPrice").value) || 0;
        const strikePrice =
          parseFloat(document.getElementById("strikePrice").value) || 0;
        const premium =
          parseFloat(document.getElementById("premium").value) || 0;
        const contracts = Math.max(
          1,
          parseInt(document.getElementById("contracts").value) || 1
        );
        const daysToExp = Math.max(
          1,
          parseInt(document.getElementById("daysToExpiration").value) || 1
        );
        const rfRatePct = parseFloat(
          document.getElementById("riskFreeRate").value
        );
        const ivPct = parseFloat(document.getElementById("iv").value);
        const targetPrice =
          parseFloat(document.getElementById("targetPrice").value) || 0;

        const shares = contracts * 100;
        const totalPremium = premium * shares;
        const cashReserved = strikePrice * shares;

        const simpleYield = cashReserved > 0 ? totalPremium / cashReserved : 0;
        const annualizedYield = simpleYield * (365 / daysToExp);
        const rfAnnual = isFinite(rfRatePct) ? rfRatePct / 100 : 0;
        const excessYield = annualizedYield - rfAnnual;

        const breakeven = strikePrice - premium;
        const maxProfit = totalPremium;
        const maxLoss = (strikePrice - premium) * shares; // if stock -> 0

        // Probability estimates using Black-Scholes-like lognormal model
        let probOTM = NaN;
        let probITM = NaN;
        if (
          stockPrice > 0 &&
          strikePrice > 0 &&
          isFinite(ivPct) &&
          ivPct > 0 &&
          daysToExp > 0
        ) {
          const T = daysToExp / 365;
          const sigma = ivPct / 100;
          const r = rfAnnual;
          const d2 =
            (Math.log(stockPrice / strikePrice) +
              (r - 0.5 * sigma * sigma) * T) /
            (sigma * Math.sqrt(T));
          // For a put, P(OTM) = P(S_T > K) = N(d2)
          probOTM = normCdf(d2);
          probITM = 1 - probOTM;
        }

        // Update key results
        document.getElementById("resPremiumPerShare").textContent =
          formatCurrency(premium);
        document.getElementById("resTotalPremium").textContent =
          formatCurrency(totalPremium);
        document.getElementById("resCashReserved").textContent =
          formatCurrency(cashReserved);
        document.getElementById("resSimpleYield").textContent =
          formatPercent(simpleYield);
        document.getElementById("resAnnualizedYield").textContent =
          formatPercent(annualizedYield);
        document.getElementById("resExcessYield").textContent = isFinite(
          excessYield
        )
          ? (excessYield * 100).toFixed(2) + "%"
          : "–";

        document.getElementById("resBreakeven").textContent =
          formatCurrency(breakeven);
        document.getElementById("resMaxProfit").textContent =
          formatCurrency(maxProfit);
        document.getElementById("resMaxLoss").textContent =
          formatCurrency(maxLoss);

        document.getElementById("resProbOTM").textContent = isFinite(probOTM)
          ? (probOTM * 100).toFixed(1) + "%"
          : "–";
        document.getElementById("resProbITM").textContent = isFinite(probITM)
          ? (probITM * 100).toFixed(1) + "%"
          : "–";

        // Scenario analysis at targetPrice
        const scenarioPrice = targetPrice;
        document.getElementById("scenarioPriceLabel").textContent =
          formatCurrency(scenarioPrice);

        let assigned = scenarioPrice < strikePrice;
        let scenarioPL;
        let scenarioReturnOnCash;
        let sharePL;
        let effectiveCost = breakeven;

        if (!assigned) {
          scenarioPL = totalPremium;
          scenarioReturnOnCash =
            cashReserved > 0 ? scenarioPL / cashReserved : 0;
          sharePL = 0;
        } else {
          // Buy at strike, own shares
          sharePL = (scenarioPrice - strikePrice) * shares;
          scenarioPL = totalPremium + sharePL;
          scenarioReturnOnCash =
            cashReserved > 0 ? scenarioPL / cashReserved : 0;
        }

        document.getElementById("scenarioAssignment").textContent = assigned
          ? "Yes (put exercised)"
          : "No (put expires worthless)";
        document.getElementById("scenarioShares").textContent = assigned
          ? shares + " shares long"
          : "0 shares";
        document.getElementById("scenarioPL").textContent =
          formatCurrency(scenarioPL);
        document.getElementById("scenarioEffectiveCost").textContent =
          formatCurrency(effectiveCost);
        document.getElementById("scenarioSharePL").textContent =
          formatCurrency(sharePL);
        document.getElementById("scenarioReturnOnCash").textContent =
          formatPercent(scenarioReturnOnCash);

        // Quick P/L table: 5 prices around strike and breakeven
        const plBody = document.getElementById("plTableBody");
        plBody.innerHTML = "";
        const prices = [
          Math.max(0, breakeven - (strikePrice - breakeven)), // deep ITM
          breakeven,
          strikePrice - (strikePrice - breakeven) / 2,
          strikePrice,
          strikePrice + (stockPrice - strikePrice || 5),
        ];

        prices.forEach((p) => {
          const price = Math.max(0, p);
          const isAssigned = price < strikePrice;
          let pl;
          if (!isAssigned) {
            pl = totalPremium;
          } else {
            pl = totalPremium + (price - strikePrice) * shares;
          }
          const retOnCash = cashReserved > 0 ? pl / cashReserved : 0;

          const tr = document.createElement("tr");
          tr.className = "border-t";

          const tdPrice = document.createElement("td");
          tdPrice.className = "px-3 py-2 border-r whitespace-nowrap";
          tdPrice.textContent = formatCurrency(price);

          const tdAssign = document.createElement("td");
          tdAssign.className = "px-3 py-2 border-r text-right";
          tdAssign.textContent = isAssigned ? "Yes" : "No";

          const tdPL = document.createElement("td");
          tdPL.className = "px-3 py-2 border-r text-right";
          tdPL.textContent = formatCurrency(pl);

          const tdRet = document.createElement("td");
          tdRet.className = "px-3 py-2 text-right";
          tdRet.textContent = formatPercent(retOnCash);

          tr.appendChild(tdPrice);
          tr.appendChild(tdAssign);
          tr.appendChild(tdPL);
          tr.appendChild(tdRet);
          plBody.appendChild(tr);
        });
      }

      document
        .getElementById("calculateBtn")
        .addEventListener("click", function (e) {
          e.preventDefault();
          calculateCashSecuredPut();
        });

      document
        .getElementById("resetBtn")
        .addEventListener("click", function (e) {
          e.preventDefault();
          document.getElementById("stockPrice").value = 100;
          document.getElementById("strikePrice").value = 95;
          document.getElementById("premium").value = 3;
          document.getElementById("contracts").value = 1;
          document.getElementById("daysToExpiration").value = 30;
          document.getElementById("riskFreeRate").value = 4;
          document.getElementById("iv").value = 25;
          document.getElementById("targetPrice").value = 100;
          calculateCashSecuredPut();
        });

      // Initial calculation
      document.addEventListener("DOMContentLoaded", calculateCashSecuredPut);

      // Mobile menu script (simple)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const expanded = this.getAttribute("aria-expanded") === "true";
          this.setAttribute("aria-expanded", String(!expanded));
          menu.classList.toggle("hidden");
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
