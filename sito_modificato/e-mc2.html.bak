    <script>
      // Core constants
      const C = 299792458; // m/s exact
      const C2 = C * C; // m^2/s^2
      const EV_TO_J = 1.602176634e-19;
      const J_TO_EV = 1 / EV_TO_J;
      const TON_TNT_J = 4.184e9; // metric ton TNT
      const MEGATON_TNT_J = TON_TNT_J * 1e6;
      const U_TO_KG = 1.6605390666e-27;

      function formatNumber(value) {
        if (!isFinite(value)) return "NaN";
        const abs = Math.abs(value);
        if (abs === 0) return "0";
        if (abs >= 1e-3 && abs < 1e6) {
          return value.toLocaleString(undefined, { maximumFractionDigits: 8 });
        }
        return value.toExponential(6);
      }

      function massToKg(mass, unit) {
        switch (unit) {
          case "kg":
            return mass;
          case "g":
            return mass * 1e-3;
          case "mg":
            return mass * 1e-6;
          case "ug":
            return mass * 1e-9;
          case "tonne":
            return mass * 1e3;
          case "u":
            return mass * U_TO_KG;
          default:
            return NaN;
        }
      }

      function kgToMass(kg, unit) {
        switch (unit) {
          case "kg":
            return kg;
          case "g":
            return kg * 1e3;
          case "mg":
            return kg * 1e6;
          case "ug":
            return kg * 1e9;
          case "tonne":
            return kg / 1e3;
          case "u":
            return kg / U_TO_KG;
          default:
            return NaN;
        }
      }

      function energyToJ(E, unit) {
        switch (unit) {
          case "J":
            return E;
          case "kJ":
            return E * 1e3;
          case "MJ":
            return E * 1e6;
          case "kWh":
            return E * 3.6e6;
          case "eV":
            return E * EV_TO_J;
          case "keV":
            return E * 1e3 * EV_TO_J;
          case "MeV":
            return E * 1e6 * EV_TO_J;
          case "GeV":
            return E * 1e9 * EV_TO_J;
          case "TNT":
            return E * TON_TNT_J;
          case "MT":
            return E * MEGATON_TNT_J;
          default:
            return NaN;
        }
      }

      function JToEnergy(J, unit) {
        switch (unit) {
          case "J":
            return J;
          case "kJ":
            return J / 1e3;
          case "MJ":
            return J / 1e6;
          case "kWh":
            return J / 3.6e6;
          case "eV":
            return J * J_TO_EV;
          case "keV":
            return (J * J_TO_EV) / 1e3;
          case "MeV":
            return (J * J_TO_EV) / 1e6;
          case "GeV":
            return (J * J_TO_EV) / 1e9;
          case "TNT":
            return J / TON_TNT_J;
          case "MT":
            return J / MEGATON_TNT_J;
          default:
            return NaN;
        }
      }

      // Mode toggle
      const modeMassToEnergyBtn = document.getElementById(
        "mode-mass-to-energy"
      );
      const modeEnergyToMassBtn = document.getElementById(
        "mode-energy-to-mass"
      );
      const formMassToEnergy = document.getElementById("form-mass-to-energy");
      const formEnergyToMass = document.getElementById("form-energy-to-mass");
      const errorEl = document.getElementById("calc-error");

      function setMode(mode) {
        if (mode === "m2e") {
          formMassToEnergy.classList.remove("hidden");
          formEnergyToMass.classList.add("hidden");
          modeMassToEnergyBtn.classList.add("toggle-btn-active");
          modeEnergyToMassBtn.classList.remove("toggle-btn-active");
          modeMassToEnergyBtn.setAttribute("aria-selected", "true");
          modeEnergyToMassBtn.setAttribute("aria-selected", "false");
        } else {
          formMassToEnergy.classList.add("hidden");
          formEnergyToMass.classList.remove("hidden");
          modeMassToEnergyBtn.classList.remove("toggle-btn-active");
          modeEnergyToMassBtn.classList.add("toggle-btn-active");
          modeMassToEnergyBtn.setAttribute("aria-selected", "false");
          modeEnergyToMassBtn.setAttribute("aria-selected", "true");
        }
        errorEl.classList.add("hidden");
      }

      modeMassToEnergyBtn.addEventListener("click", () => setMode("m2e"));
      modeEnergyToMassBtn.addEventListener("click", () => setMode("e2m"));

      // Mass -> Energy calculation
      document
        .getElementById("calc-mass-to-energy")
        .addEventListener("click", () => {
          const massVal = parseFloat(
            document.getElementById("mass-value").value
          );
          const massUnit = document.getElementById("mass-unit").value;
          const energyUnit = document.getElementById("energy-unit").value;
          const resultBox = document.getElementById("mass-to-energy-result");
          const mainEl = document.getElementById("mass-to-energy-main");
          const secondaryEl = document.getElementById(
            "mass-to-energy-secondary"
          );

          if (isNaN(massVal)) {
            errorEl.textContent = "Please enter a valid mass value.";
            errorEl.classList.remove("hidden");
            resultBox.classList.add("hidden");
            return;
          }
          if (massVal < 0) {
            errorEl.textContent = "Mass must be non-negative.";
            errorEl.classList.remove("hidden");
            resultBox.classList.add("hidden");
            return;
          }
          errorEl.classList.add("hidden");

          const massKg = massToKg(massVal, massUnit);
          const energyJ = massKg * C2;
          const energyOut = JToEnergy(energyJ, energyUnit);

          mainEl.textContent = `${formatNumber(energyOut)} ${energyUnit}`;
          secondaryEl.textContent = `Equivalent to ${formatNumber(
            energyJ
          )} J using E = mc² with c = 299,792,458 m/s.`;
          resultBox.classList.remove("hidden");
        });

      document
        .getElementById("reset-mass-to-energy")
        .addEventListener("click", () => {
          document.getElementById("mass-value").value = "";
          document.getElementById("mass-unit").value = "kg";
          document.getElementById("energy-unit").value = "J";
          document
            .getElementById("mass-to-energy-result")
            .classList.add("hidden");
          errorEl.classList.add("hidden");
        });

      // Energy -> Mass calculation
      document
        .getElementById("calc-energy-to-mass")
        .addEventListener("click", () => {
          const energyVal = parseFloat(
            document.getElementById("energy-value").value
          );
          const energyUnitIn =
            document.getElementById("energy-input-unit").value;
          const massOutUnit = document.getElementById("mass-output-unit").value;
          const resultBox = document.getElementById("energy-to-mass-result");
          const mainEl = document.getElementById("energy-to-mass-main");
          const secondaryEl = document.getElementById(
            "energy-to-mass-secondary"
          );

          if (isNaN(energyVal)) {
            errorEl.textContent = "Please enter a valid energy value.";
            errorEl.classList.remove("hidden");
            resultBox.classList.add("hidden");
            return;
          }
          if (energyVal < 0) {
            errorEl.textContent = "Energy must be non-negative.";
            errorEl.classList.remove("hidden");
            resultBox.classList.add("hidden");
            return;
          }
          errorEl.classList.add("hidden");

          const energyJ = energyToJ(energyVal, energyUnitIn);
          const massKg = energyJ / C2;
          const massOut = kgToMass(massKg, massOutUnit);

          mainEl.textContent = `${formatNumber(massOut)} ${massOutUnit}`;
          secondaryEl.textContent = `Equivalent to ${formatNumber(
            massKg
          )} kg using m = E / c² with c = 299,792,458 m/s.`;
          resultBox.classList.remove("hidden");
        });

      document
        .getElementById("reset-energy-to-mass")
        .addEventListener("click", () => {
          document.getElementById("energy-value").value = "";
          document.getElementById("energy-input-unit").value = "J";
          document.getElementById("mass-output-unit").value = "kg";
          document
            .getElementById("energy-to-mass-result")
            .classList.add("hidden");
          errorEl.classList.add("hidden");
        });

      // Initialize default mode
      setMode("m2e");

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
