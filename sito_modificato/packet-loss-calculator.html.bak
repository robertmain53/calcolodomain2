    <script>
      // Mode switching
      const modeButtons = document.querySelectorAll(".mode-btn");
      const forms = {
        counts: document.getElementById("form-counts"),
        bitrate: document.getElementById("form-bitrate"),
        ping: document.getElementById("form-ping"),
      };
      let currentMode = "counts";

      modeButtons.forEach((btn) => {
        btn.addEventListener("click", () => {
          currentMode = btn.dataset.mode;
          modeButtons.forEach((b) => {
            if (b === btn) {
              b.classList.remove(
                "border-gray-300",
                "bg-white",
                "text-gray-700"
              );
              b.classList.add(
                "border-blue-600",
                "bg-blue-50",
                "text-blue-700",
                "font-medium"
              );
            } else {
              b.classList.add("border-gray-300", "bg-white", "text-gray-700");
              b.classList.remove(
                "border-blue-600",
                "bg-blue-50",
                "text-blue-700",
                "font-medium"
              );
            }
          });
          Object.keys(forms).forEach((mode) => {
            forms[mode].classList.toggle("hidden", mode !== currentMode);
          });
        });
      });

      function formatNumber(num, decimals = 2) {
        if (!isFinite(num)) return "–";
        return num.toLocaleString(undefined, {
          maximumFractionDigits: decimals,
        });
      }

      function classifyQuality(lossPercent) {
        if (!isFinite(lossPercent))
          return { label: "Unknown", badgeClass: "", hint: "" };
        if (lossPercent < 0.5) {
          return {
            label: "Excellent",
            badgeClass: "badge-good",
            hint: "Excellent connection. Suitable for VoIP, HD video calls, and competitive gaming.",
          };
        } else if (lossPercent < 1) {
          return {
            label: "Good",
            badgeClass: "badge-good",
            hint: "Good connection. Real-time apps should work well with only occasional minor glitches.",
          };
        } else if (lossPercent < 3) {
          return {
            label: "Fair",
            badgeClass: "badge-ok",
            hint: "Fair connection. Web and downloads are fine; VoIP and gaming may show noticeable issues.",
          };
        } else {
          return {
            label: "Poor",
            badgeClass: "badge-bad",
            hint: "Poor connection. Expect choppy calls, lag in games, and slower transfers. Investigate your network.",
          };
        }
      }

      function parsePingOutput(text) {
        const result = {
          sent: null,
          received: null,
          lost: null,
          lossPercent: null,
          avgRtt: null,
        };

        // Windows-style: Sent = X, Received = Y, Lost = Z (P% loss)
        let m = text.match(
          /Sent\s*=\s*(\d+)[^\d]+Received\s*=\s*(\d+)[^\d]+Lost\s*=\s*(\d+)/i
        );
        if (m) {
          result.sent = parseInt(m[1], 10);
          result.received = parseInt(m[2], 10);
          result.lost = parseInt(m[3], 10);
        }

        // Generic "X% packet loss"
        m = text.match(/(\d+(?:\.\d+)?)\s*%[^\n]*loss/i);
        if (m) {
          result.lossPercent = parseFloat(m[1]);
        }

        // Average RTT
        m = text.match(/Average\s*=\s*(\d+(?:\.\d+)?)\s*ms/i);
        if (!m) {
          m = text.match(/avg[=/\s]*([\d.]+)\s*ms/i);
        }
        if (m) {
          result.avgRtt = parseFloat(m[1]);
        }

        // If we have sent & received but not lossPercent, compute it
        if (result.sent != null && result.received != null) {
          result.lost =
            result.lost != null
              ? result.lost
              : Math.max(result.sent - result.received, 0);
          result.lossPercent = (result.lost / result.sent) * 100;
        }

        return result;
      }

      const calcBtn = document.getElementById("calcBtn");
      const resetBtn = document.getElementById("resetBtn");

      const resSent = document.getElementById("resSent");
      const resReceived = document.getElementById("resReceived");
      const resLost = document.getElementById("resLost");
      const resLossPercent = document.getElementById("resLossPercent");
      const resThroughput = document.getElementById("resThroughput");
      const resDuration = document.getElementById("resDuration");
      const resRtt = document.getElementById("resRtt");
      const resQuality = document.getElementById("resQuality");
      const qualityHint = document.getElementById("qualityHint");

      function resetResults() {
        resSent.textContent = "–";
        resReceived.textContent = "–";
        resLost.textContent = "–";
        resLossPercent.textContent = "–";
        resThroughput.textContent = "–";
        resDuration.textContent = "–";
        resRtt.textContent = "–";
        resQuality.textContent = "–";
        resQuality.className = "font-semibold text-gray-900";
        qualityHint.textContent = "";
        qualityHint.classList.add("hidden");
      }

      resetBtn.addEventListener("click", () => {
        document.getElementById("sentPackets").value = "";
        document.getElementById("receivedPackets").value = "";
        document.getElementById("packetSize").value = "";
        document.getElementById("duration").value = "";
        document.getElementById("rtt").value = "";
        document.getElementById("bitrate").value = "";
        document.getElementById("lossPercentInput").value = "";
        document.getElementById("bitrateDuration").value = "";
        document.getElementById("pingOutput").value = "";
        resetResults();
      });

      calcBtn.addEventListener("click", () => {
        resetResults();

        let sent = null,
          received = null,
          lost = null,
          lossPercent = null;
        let durationSec = null;
        let throughputMbps = null;
        let rttMs = null;

        if (currentMode === "counts") {
          const sentVal = parseFloat(
            document.getElementById("sentPackets").value
          );
          const recvVal = parseFloat(
            document.getElementById("receivedPackets").value
          );
          const pktSize = parseFloat(
            document.getElementById("packetSize").value
          );
          const dur = parseFloat(document.getElementById("duration").value);
          const rttVal = parseFloat(document.getElementById("rtt").value);

          if (
            !isFinite(sentVal) ||
            !isFinite(recvVal) ||
            sentVal < 0 ||
            recvVal < 0
          ) {
            alert(
              "Please enter valid non-negative values for packets sent and received."
            );
            return;
          }
          if (recvVal > sentVal) {
            alert("Packets received cannot be greater than packets sent.");
            return;
          }

          sent = sentVal;
          received = recvVal;
          lost = sent - received;
          lossPercent = sent > 0 ? (lost / sent) * 100 : 0;

          if (isFinite(dur) && dur > 0 && isFinite(pktSize) && pktSize > 0) {
            durationSec = dur;
            const bitsReceived = received * pktSize * 8;
            throughputMbps = bitsReceived / dur / 1e6;
          } else if (isFinite(dur) && dur > 0) {
            durationSec = dur;
          }

          if (isFinite(rttVal) && rttVal >= 0) {
            rttMs = rttVal;
          }
        } else if (currentMode === "bitrate") {
          const bitrateVal = parseFloat(
            document.getElementById("bitrate").value
          );
          const bitrateUnit = document.getElementById("bitrateUnit").value;
          const lossVal = parseFloat(
            document.getElementById("lossPercentInput").value
          );
          const dur = parseFloat(
            document.getElementById("bitrateDuration").value
          );

          if (!isFinite(bitrateVal) || bitrateVal < 0) {
            alert("Please enter a valid nominal bitrate.");
            return;
          }
          if (!isFinite(lossVal) || lossVal < 0 || lossVal > 100) {
            alert(
              "Please enter a valid packet loss percentage between 0 and 100."
            );
            return;
          }

          // Convert bitrate to Mbps
          let bitrateMbps = bitrateVal;
          if (bitrateUnit === "Kbps") bitrateMbps = bitrateVal / 1000;
          if (bitrateUnit === "Gbps") bitrateMbps = bitrateVal * 1000;

          lossPercent = lossVal;
          throughputMbps = bitrateMbps * (1 - lossPercent / 100);

          if (isFinite(dur) && dur > 0) {
            durationSec = dur;
            // Approximate packet counts assuming 1500-byte packets
            const bitsTotal = bitrateMbps * 1e6 * dur;
            const bitsPerPacket = 1500 * 8;
            sent = bitsTotal / bitsPerPacket;
            received = sent * (1 - lossPercent / 100);
            lost = sent - received;
          }
        } else if (currentMode === "ping") {
          const text = document.getElementById("pingOutput").value.trim();
          if (!text) {
            alert("Please paste ping or mtr output first.");
            return;
          }
          const parsed = parsePingOutput(text);
          if (parsed.sent == null && parsed.lossPercent == null) {
            alert(
              "Could not detect packet statistics in the pasted output. Please include the summary lines with Sent/Received/Lost or % loss."
            );
            return;
          }
          sent = parsed.sent;
          received = parsed.received;
          lost = parsed.lost;
          lossPercent = parsed.lossPercent;
          rttMs = parsed.avgRtt;
        }

        // Update results
        if (sent != null) resSent.textContent = formatNumber(sent, 0);
        if (received != null)
          resReceived.textContent = formatNumber(received, 0);
        if (lost != null) resLost.textContent = formatNumber(lost, 0);
        if (lossPercent != null)
          resLossPercent.textContent = formatNumber(lossPercent, 3) + " %";
        if (throughputMbps != null)
          resThroughput.textContent = formatNumber(throughputMbps, 3) + " Mbps";
        if (durationSec != null)
          resDuration.textContent = formatNumber(durationSec, 3) + " s";
        if (rttMs != null) resRtt.textContent = formatNumber(rttMs, 2) + " ms";

        if (lossPercent != null) {
          const q = classifyQuality(lossPercent);
          resQuality.textContent = q.label;
          resQuality.className =
            "font-semibold px-2 py-0.5 rounded-full text-xs " + q.badgeClass;
          if (q.hint) {
            qualityHint.textContent = q.hint;
            qualityHint.classList.remove("hidden");
          }
        }
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
