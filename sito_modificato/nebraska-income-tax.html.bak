    <script>
      // Simplified Nebraska tax brackets (illustrative only, not official)
      // Structure: { year: { status: [ {limit, rate}, ... ] } }
      const NE_BRACKETS = {
        2024: {
          single: [
            { limit: 3500, rate: 0.0246 },
            { limit: 21000, rate: 0.0351 },
            { limit: 34000, rate: 0.0501 },
            { limit: Infinity, rate: 0.0584 },
          ],
          mfj: [
            { limit: 7000, rate: 0.0246 },
            { limit: 42000, rate: 0.0351 },
            { limit: 68000, rate: 0.0501 },
            { limit: Infinity, rate: 0.0584 },
          ],
          mfs: [
            { limit: 3500, rate: 0.0246 },
            { limit: 21000, rate: 0.0351 },
            { limit: 34000, rate: 0.0501 },
            { limit: Infinity, rate: 0.0584 },
          ],
          hoh: [
            { limit: 3500, rate: 0.0246 },
            { limit: 21000, rate: 0.0351 },
            { limit: 34000, rate: 0.0501 },
            { limit: Infinity, rate: 0.0584 },
          ],
        },
        2023: {
          single: [
            { limit: 3500, rate: 0.0246 },
            { limit: 21000, rate: 0.0351 },
            { limit: 34000, rate: 0.0501 },
            { limit: Infinity, rate: 0.0684 },
          ],
          mfj: [
            { limit: 7000, rate: 0.0246 },
            { limit: 42000, rate: 0.0351 },
            { limit: 68000, rate: 0.0501 },
            { limit: Infinity, rate: 0.0684 },
          ],
          mfs: [
            { limit: 3500, rate: 0.0246 },
            { limit: 21000, rate: 0.0351 },
            { limit: 34000, rate: 0.0501 },
            { limit: Infinity, rate: 0.0684 },
          ],
          hoh: [
            { limit: 3500, rate: 0.0246 },
            { limit: 21000, rate: 0.0351 },
            { limit: 34000, rate: 0.0501 },
            { limit: Infinity, rate: 0.0684 },
          ],
        },
      };

      function formatCurrency(value) {
        if (!isFinite(value)) return "$0";
        return value.toLocaleString("en-US", {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 0,
        });
      }

      function formatPercent(value) {
        if (!isFinite(value)) return "0%";
        return (value * 100).toFixed(1) + "%";
      }

      function calculateNebraskaTax(income, year, status) {
        const brackets = NE_BRACKETS[year][status];
        let remaining = income;
        let lower = 0;
        let tax = 0;
        const details = [];

        for (const bracket of brackets) {
          if (remaining <= 0) break;
          const upper = bracket.limit;
          const taxableInBracket = Math.max(
            0,
            Math.min(remaining, upper - lower)
          );
          const taxForBracket = taxableInBracket * bracket.rate;

          details.push({
            range: [lower, upper],
            rate: bracket.rate,
            taxable: taxableInBracket,
            tax: taxForBracket,
          });

          tax += taxForBracket;
          remaining -= taxableInBracket;
          lower = upper;
        }

        const marginalBracket = details.findLast
          ? details.findLast((d) => d.taxable > 0)
          : [...details].reverse().find((d) => d.taxable > 0);

        return {
          tax,
          details,
          marginalRate: marginalBracket ? marginalBracket.rate : 0,
        };
      }

      function renderBreakdown(details) {
        if (!details || !details.length) {
          return '<p class="text-gray-500">No tax due at this income level.</p>';
        }

        let html =
          '<table class="min-w-full text-xs border border-gray-200 rounded-md overflow-hidden"><thead class="bg-gray-100"><tr>';
        html += '<th class="px-2 py-1 text-left">Bracket range</th>';
        html += '<th class="px-2 py-1 text-right">Rate</th>';
        html += '<th class="px-2 py-1 text-right">Taxable</th>';
        html += '<th class="px-2 py-1 text-right">Tax</th>';
        html += "</tr></thead><tbody>";

        details.forEach((row) => {
          if (row.taxable <= 0) return;
          const [low, high] = row.range;
          const rangeLabel =
            high === Infinity
              ? `${formatCurrency(low)}+`
              : `${formatCurrency(low)} – ${formatCurrency(high)}`;
          html += `<tr class="border-t border-gray-200">
        <td class="px-2 py-1">${rangeLabel}</td>
        <td class="px-2 py-1 text-right">${formatPercent(row.rate)}</td>
        <td class="px-2 py-1 text-right">${formatCurrency(row.taxable)}</td>
        <td class="px-2 py-1 text-right">${formatCurrency(row.tax)}</td>
      </tr>`;
        });

        html += "</tbody></table>";
        return html;
      }

      function runCalculator() {
        const year = parseInt(document.getElementById("tax-year").value, 10);
        const status = document.getElementById("filing-status").value;
        const income = Math.max(
          0,
          Number(document.getElementById("ne-income").value) || 0
        );
        const credits = Math.max(
          0,
          Number(document.getElementById("credits").value) || 0
        );

        const { tax, details, marginalRate } = calculateNebraskaTax(
          income,
          year,
          status
        );
        const taxAfterCredits = Math.max(0, tax - credits);
        const effectiveRate = income > 0 ? taxAfterCredits / income : 0;

        document.getElementById("result-tax-before").textContent =
          formatCurrency(tax);
        document.getElementById("result-tax-after").textContent =
          formatCurrency(taxAfterCredits);
        document.getElementById("result-effective-rate").textContent =
          formatPercent(effectiveRate);
        document.getElementById("result-bracket-label").textContent =
          marginalRate ? formatPercent(marginalRate) : "–";
        document.getElementById("result-takehome").textContent = formatCurrency(
          income - taxAfterCredits
        );
        document.getElementById("breakdown-container").innerHTML =
          renderBreakdown(details);
      }

      document.addEventListener("DOMContentLoaded", () => {
        document
          .getElementById("calc-btn")
          .addEventListener("click", runCalculator);
        document.getElementById("reset-btn").addEventListener("click", () => {
          document.getElementById("tax-year").value = "2024";
          document.getElementById("filing-status").value = "mfj";
          document.getElementById("ne-income").value = 75000;
          document.getElementById("credits").value = 0;
          runCalculator();
        });

        ["tax-year", "filing-status", "ne-income", "credits"].forEach((id) => {
          document.getElementById(id).addEventListener("change", runCalculator);
          document.getElementById(id).addEventListener("input", () => {
            if (id === "ne-income" || id === "credits") {
              // debounce-like quick update
              clearTimeout(window.__neTaxTimer);
              window.__neTaxTimer = setTimeout(runCalculator, 250);
            }
          });
        });

        runCalculator();
      });

      // Mobile menu script (from template)
      document.addEventListener("DOMContentLoaded", () => {
        const toggle = document.getElementById("mobile-menu-toggle");
        const menu = document.getElementById("mobile-menu");
        if (toggle && menu) {
          toggle.addEventListener("click", () => {
            const isHidden = menu.classList.contains("hidden");
            menu.classList.toggle("hidden", !isHidden);
            toggle.setAttribute("aria-expanded", String(isHidden));
          });
        }
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
