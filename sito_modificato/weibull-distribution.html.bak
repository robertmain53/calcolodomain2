        function calculateWeibull() {
            // Placeholder logic for calculation
            const shape = document.getElementById('shape-parameter').value;
            const scale = document.getElementById('scale-parameter').value;
            if (shape && scale) {
                // Example calculation, replace with actual formula
                const probability = (1 - Math.exp(-1 * Math.pow((1 / scale), shape))).toFixed(4);
                document.getElementById('failure-probability').innerText = probability;
            } else {
                alert('Please enter valid values for both parameters.');
            }
        }
    </script>


