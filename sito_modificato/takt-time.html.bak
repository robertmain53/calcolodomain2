    <script>
      (function () {
        const periodLengthEl = document.getElementById("period-length");
        const periodUnitEl = document.getElementById("period-unit");
        const plannedBreaksEl = document.getElementById("planned-breaks");
        const breaksUnitEl = document.getElementById("breaks-unit");
        const demandEl = document.getElementById("demand");
        const cycleTimeEl = document.getElementById("cycle-time");
        const cycleTimeUnitEl = document.getElementById("cycle-time-unit");
        const workContentEl = document.getElementById("work-content");
        const workContentUnitEl = document.getElementById("work-content-unit");

        const taktTimeDisplay = document.getElementById("takt-time-display");
        const taktTimePerHour = document.getElementById("takt-time-per-hour");
        const netTimeDisplay = document.getElementById("net-time-display");
        const netTimeMinutes = document.getElementById("net-time-minutes");
        const demandRateDisplay = document.getElementById(
          "demand-rate-display"
        );
        const cycleVsTakt = document.getElementById("cycle-vs-takt");
        const capacityBadge = document.getElementById("capacity-badge");
        const operatorsDisplay = document.getElementById("operators-display");
        const resetBtn = document.getElementById("reset-btn");
        const copyBtn = document.getElementById("copy-summary");
        const copyFeedback = document.getElementById("copy-feedback");

        function toSeconds(value, unit) {
          const v = Number(value);
          if (!isFinite(v) || v < 0) return 0;
          switch (unit) {
            case "hours":
              return v * 3600;
            case "minutes":
              return v * 60;
            case "seconds":
              return v;
            default:
              return v;
          }
        }

        function fromSecondsToBestUnit(seconds) {
          if (!isFinite(seconds) || seconds <= 0)
            return { value: 0, unit: "s" };
          if (seconds >= 3600) return { value: seconds / 3600, unit: "h" };
          if (seconds >= 60) return { value: seconds / 60, unit: "min" };
          return { value: seconds, unit: "s" };
        }

        function formatNumber(num, decimals = 2) {
          if (!isFinite(num)) return "–";
          return num.toLocaleString(undefined, {
            maximumFractionDigits: decimals,
            minimumFractionDigits: num < 10 ? Math.min(decimals, 2) : 0,
          });
        }

        function calculate() {
          const periodSeconds = toSeconds(
            periodLengthEl.value,
            periodUnitEl.value
          );
          const breaksSeconds = toSeconds(
            plannedBreaksEl.value,
            breaksUnitEl.value
          );
          const demand = Number(demandEl.value);

          const netSeconds = Math.max(periodSeconds - breaksSeconds, 0);
          const netMinutesVal = netSeconds / 60;

          // Update net time
          const bestNet = fromSecondsToBestUnit(netSeconds);
          netTimeDisplay.textContent =
            isFinite(netSeconds) && netSeconds > 0
              ? `${formatNumber(bestNet.value)} ${bestNet.unit}`
              : "–";
          netTimeMinutes.textContent =
            isFinite(netMinutesVal) && netMinutesVal > 0
              ? `${formatNumber(netMinutesVal)} minutes`
              : "– minutes";

          // Demand rate & takt time
          if (!isFinite(demand) || demand <= 0 || netSeconds <= 0) {
            taktTimeDisplay.textContent = "–";
            taktTimePerHour.textContent = "– units per hour";
            demandRateDisplay.textContent = "–";
            capacityBadge.textContent = "–";
            capacityBadge.className = "badge bg-gray-200 text-gray-700";
            cycleVsTakt.textContent =
              "Enter valid demand and times to see capacity status.";
            operatorsDisplay.textContent =
              "Enter work content to estimate required operators.";
            return;
          }

          const taktSeconds = netSeconds / demand;
          const taktBest = fromSecondsToBestUnit(taktSeconds);
          taktTimeDisplay.textContent = `${formatNumber(taktBest.value)} ${
            taktBest.unit
          } / unit`;

          const unitsPerHour = (demand / netSeconds) * 3600;
          taktTimePerHour.textContent = `${formatNumber(
            unitsPerHour
          )} units per hour`;

          demandRateDisplay.textContent = formatNumber(unitsPerHour);

          // Cycle vs takt
          const cycleVal = Number(cycleTimeEl.value);
          const cycleSeconds = toSeconds(cycleVal, cycleTimeUnitEl.value);
          if (isFinite(cycleSeconds) && cycleSeconds > 0) {
            const ratio = cycleSeconds / taktSeconds;
            let statusText = "";
            let badgeText = "";
            let badgeClass = "badge ";

            if (ratio <= 0.8) {
              statusText =
                "Cycle time is comfortably below takt time. You have capacity buffer.";
              badgeText = "Comfortable";
              badgeClass += "bg-green-100 text-green-700";
            } else if (ratio <= 1.0) {
              statusText =
                "Cycle time is close to takt time. Line is balanced but has limited buffer.";
              badgeText = "Balanced";
              badgeClass += "bg-yellow-100 text-yellow-700";
            } else {
              statusText =
                "Cycle time is above takt time. You cannot meet demand with current capacity.";
              badgeText = "Overloaded";
              badgeClass += "bg-red-100 text-red-700";
            }

            capacityBadge.textContent = badgeText;
            capacityBadge.className = badgeClass;
            cycleVsTakt.textContent =
              statusText + ` (Cycle/Takt = ${formatNumber(ratio, 2)}).`;
          } else {
            capacityBadge.textContent = "–";
            capacityBadge.className = "badge bg-gray-200 text-gray-700";
            cycleVsTakt.textContent =
              "Enter cycle time to see capacity status.";
          }

          // Operators estimate
          const workContentVal = Number(workContentEl.value);
          const workContentSeconds = toSeconds(
            workContentVal,
            workContentUnitEl.value
          );
          if (isFinite(workContentSeconds) && workContentSeconds > 0) {
            const operators = workContentSeconds / taktSeconds;
            operatorsDisplay.textContent = `Estimated operators or machines required: ${formatNumber(
              operators,
              2
            )} (work content ÷ takt time).`;
          } else {
            operatorsDisplay.textContent =
              "Enter work content to estimate required operators.";
          }
        }

        function resetDefaults() {
          periodLengthEl.value = 8;
          periodUnitEl.value = "hours";
          plannedBreaksEl.value = 60;
          breaksUnitEl.value = "minutes";
          demandEl.value = 400;
          cycleTimeEl.value = 60;
          cycleTimeUnitEl.value = "seconds";
          workContentEl.value = 180;
          workContentUnitEl.value = "seconds";
          calculate();
        }

        function copySummaryToClipboard() {
          const summaryParts = [];

          summaryParts.push(`Takt Time Analysis Summary`);
          summaryParts.push(`--------------------------`);

          const period = `${periodLengthEl.value} ${periodUnitEl.value}`;
          const breaks = `${plannedBreaksEl.value} ${breaksUnitEl.value}`;
          const demand = `${demandEl.value} units`;

          summaryParts.push(`Planning period: ${period}`);
          summaryParts.push(`Planned non-productive time: ${breaks}`);
          summaryParts.push(`Customer demand: ${demand}`);
          summaryParts.push(
            `Net available time: ${netTimeDisplay.textContent} (${netTimeMinutes.textContent})`
          );
          summaryParts.push(
            `Required takt time: ${taktTimeDisplay.textContent} (${taktTimePerHour.textContent})`
          );
          summaryParts.push(
            `Demand rate: ${demandRateDisplay.textContent} units/hour`
          );

          const cycleVal = cycleTimeEl.value;
          if (cycleVal && Number(cycleVal) > 0) {
            summaryParts.push(
              `Cycle time: ${cycleVal} ${cycleTimeUnitEl.value} per unit`
            );
            summaryParts.push(`Takt vs. cycle: ${cycleVsTakt.textContent}`);
          }

          const opsText = operatorsDisplay.textContent;
          if (opsText && opsText.startsWith("Estimated")) {
            summaryParts.push(opsText);
          }

          const text = summaryParts.join("\n");

          if (navigator.clipboard && navigator.clipboard.writeText) {
            navigator.clipboard.writeText(text).then(function () {
              copyFeedback.classList.remove("hidden");
              setTimeout(() => copyFeedback.classList.add("hidden"), 2000);
            });
          }
        }

        ["input", "change"].forEach((evt) => {
          periodLengthEl.addEventListener(evt, calculate);
          periodUnitEl.addEventListener(evt, calculate);
          plannedBreaksEl.addEventListener(evt, calculate);
          breaksUnitEl.addEventListener(evt, calculate);
          demandEl.addEventListener(evt, calculate);
          cycleTimeEl.addEventListener(evt, calculate);
          cycleTimeUnitEl.addEventListener(evt, calculate);
          workContentEl.addEventListener(evt, calculate);
          workContentUnitEl.addEventListener(evt, calculate);
        });

        resetBtn.addEventListener("click", resetDefaults);
        copyBtn.addEventListener("click", copySummaryToClipboard);

        // Initial calculation
        calculate();
      })();
    </script>
  
