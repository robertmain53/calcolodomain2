    <script src="https://cdn.jsdelivr.net/npm/chart.js@4.4.1/dist/chart.umd.min.js"></script>
    <script>
      (function () {
        const nInput = document.getElementById("n");
        const pInput = document.getElementById("p");
        const kInput = document.getElementById("k");
        const modeType = document.getElementById("modeType");
        const betweenContainer = document.getElementById("between-container");
        const k1Input = document.getElementById("k1");
        const k2Input = document.getElementById("k2");
        const computeBtn = document.getElementById("computeBtn");
        const resetBtn = document.getElementById("resetBtn");
        const showCdfCheckbox = document.getElementById("showCdf");
        const errorMsg = document.getElementById("errorMsg");

        const probResult = document.getElementById("probabilityResult");
        const probPercent = document.getElementById("probabilityPercent");
        const meanResult = document.getElementById("meanResult");
        const varianceResult = document.getElementById("varianceResult");
        const sdResult = document.getElementById("sdResult");
        const modeResult = document.getElementById("modeResult");
        const chartLabel = document.getElementById("chartLabel");

        const ctx = document.getElementById("binomialChart").getContext("2d");
        let chart;

        function factorialLog(n) {
          // log(n!) using Stirling for large n
          if (n < 0) return NaN;
          if (n === 0 || n === 1) return 0;
          if (n < 50) {
            let res = 0;
            for (let i = 2; i <= n; i++) res += Math.log(i);
            return res;
          }
          const x = n + 1;
          return (x - 0.5) * Math.log(x) - x + 0.5 * Math.log(2 * Math.PI);
        }

        function logChoose(n, k) {
          if (k < 0 || k > n) return -Infinity;
          return factorialLog(n) - factorialLog(k) - factorialLog(n - k);
        }

        function binomialPmf(n, p, k) {
          if (k < 0 || k > n) return 0;
          if (p === 0) return k === 0 ? 1 : 0;
          if (p === 1) return k === n ? 1 : 0;
          const logProb =
            logChoose(n, k) + k * Math.log(p) + (n - k) * Math.log(1 - p);
          return Math.exp(logProb);
        }

        function computeStats(n, p) {
          const mean = n * p;
          const variance = n * p * (1 - p);
          const sd = Math.sqrt(variance);
          let mode = Math.floor((n + 1) * p);
          if (mode < 0) mode = 0;
          if (mode > n) mode = n;
          return { mean, variance, sd, mode };
        }

        function validateInputs() {
          const n = parseInt(nInput.value, 10);
          const p = parseFloat(pInput.value);
          const k = parseInt(kInput.value, 10);

          if (!Number.isFinite(n) || n < 1 || n > 1000) {
            return {
              ok: false,
              message: "n must be an integer between 1 and 1000.",
            };
          }
          if (!Number.isFinite(p) || p < 0 || p > 1) {
            return {
              ok: false,
              message: "p must be a probability between 0 and 1.",
            };
          }
          if (!Number.isFinite(k) || k < 0 || k > n) {
            return {
              ok: false,
              message: "k must be an integer between 0 and n.",
            };
          }

          if (modeType.value === "between") {
            const k1 = parseInt(k1Input.value, 10);
            const k2 = parseInt(k2Input.value, 10);
            if (!Number.isFinite(k1) || !Number.isFinite(k2)) {
              return { ok: false, message: "k1 and k2 must be integers." };
            }
            if (k1 < 0 || k2 < 0 || k1 > n || k2 > n) {
              return {
                ok: false,
                message: "k1 and k2 must be between 0 and n.",
              };
            }
            if (k1 > k2) {
              return {
                ok: false,
                message: "k1 must be less than or equal to k2.",
              };
            }
          }

          return { ok: true, n, p, k };
        }

        function updateChart(n, p, highlight) {
          const labels = [];
          const pmf = [];
          const cdf = [];
          const highlightMask = [];

          let cumulative = 0;
          for (let k = 0; k <= n; k++) {
            const pk = binomialPmf(n, p, k);
            cumulative += pk;
            labels.push(k);
            pmf.push(pk);
            cdf.push(cumulative);
            highlightMask.push(highlight(k) ? 1 : 0);
          }

          const dataToShow = showCdfCheckbox.checked ? cdf : pmf;
          chartLabel.textContent = showCdfCheckbox.checked
            ? "CDF: P(X ≤ k)"
            : "PMF: P(X = k)";

          const baseColor = "rgba(37, 99, 235, 0.5)";
          const highlightColor = "rgba(239, 68, 68, 0.8)";

          const backgroundColors = dataToShow.map((_, idx) =>
            highlightMask[idx] ? highlightColor : baseColor
          );

          if (chart) {
            chart.data.labels = labels;
            chart.data.datasets[0].data = dataToShow;
            chart.data.datasets[0].backgroundColor = backgroundColors;
            chart.update();
          } else {
            chart = new Chart(ctx, {
              type: "bar",
              data: {
                labels,
                datasets: [
                  {
                    label: "Probability",
                    data: dataToShow,
                    backgroundColor: backgroundColors,
                    borderWidth: 0,
                  },
                ],
              },
              options: {
                responsive: true,
                maintainAspectRatio: false,
                scales: {
                  x: {
                    title: { display: true, text: "k (number of successes)" },
                    ticks: { autoSkip: true, maxTicksLimit: 20 },
                  },
                  y: {
                    title: { display: true, text: "Probability" },
                    beginAtZero: true,
                  },
                },
                plugins: {
                  legend: { display: false },
                  tooltip: {
                    callbacks: {
                      label: function (context) {
                        const val = context.parsed.y;
                        return (
                          "P = " +
                          val.toFixed(6) +
                          " (" +
                          (val * 100).toFixed(3) +
                          "%)"
                        );
                      },
                    },
                  },
                },
              },
            });
          }
        }

        function compute() {
          const v = validateInputs();
          if (!v.ok) {
            errorMsg.textContent = v.message;
            errorMsg.classList.remove("hidden");
            return;
          }
          errorMsg.classList.add("hidden");

          const { n, p, k } = v;
          const { mean, variance, sd, mode } = computeStats(n, p);

          let prob = 0;
          let description = "";

          if (modeType.value === "exact") {
            prob = binomialPmf(n, p, k);
            description = `P(X = ${k}) = ${prob.toFixed(6)}`;
            updateChart(n, p, (kk) => kk === k);
          } else if (modeType.value === "leq") {
            for (let i = 0; i <= k; i++) prob += binomialPmf(n, p, i);
            description = `P(X ≤ ${k}) = ${prob.toFixed(6)}`;
            updateChart(n, p, (kk) => kk <= k);
          } else if (modeType.value === "geq") {
            for (let i = k; i <= n; i++) prob += binomialPmf(n, p, i);
            description = `P(X ≥ ${k}) = ${prob.toFixed(6)}`;
            updateChart(n, p, (kk) => kk >= k);
          } else if (modeType.value === "between") {
            const k1 = parseInt(k1Input.value, 10);
            const k2 = parseInt(k2Input.value, 10);
            for (let i = k1; i <= k2; i++) prob += binomialPmf(n, p, i);
            description = `P(${k1} ≤ X ≤ ${k2}) = ${prob.toFixed(6)}`;
            updateChart(n, p, (kk) => kk >= k1 && kk <= k2);
          }

          probResult.textContent = description;
          probPercent.textContent = (prob * 100).toFixed(4) + "%";

          meanResult.textContent = mean.toFixed(4).replace(/\.?0+$/, "");
          varianceResult.textContent = variance
            .toFixed(4)
            .replace(/\.?0+$/, "");
          sdResult.textContent = sd.toFixed(4).replace(/\.?0+$/, "");
          modeResult.textContent = mode;
        }

        function reset() {
          nInput.value = 10;
          pInput.value = 0.5;
          kInput.value = 3;
          modeType.value = "exact";
          k1Input.value = 2;
          k2Input.value = 5;
          betweenContainer.classList.add("hidden");
          showCdfCheckbox.checked = false;
          errorMsg.classList.add("hidden");
          compute();
        }

        modeType.addEventListener("change", () => {
          if (modeType.value === "between") {
            betweenContainer.classList.remove("hidden");
          } else {
            betweenContainer.classList.add("hidden");
          }
          compute();
        });

        showCdfCheckbox.addEventListener("change", compute);
        computeBtn.addEventListener("click", compute);
        resetBtn.addEventListener("click", reset);

        // Initial render
        compute();
      })();
    </script>
  
