                                solutionSet.push(resolveTriangle(a, b, c, alpha, beta, gamma, steps, 1));
                            } else if (oppositeSide > height && oppositeSide < adjacentSide) {
                                steps.push(`<p>3. <strong>Two Solutions:</strong> $h < a < b$ (${formatNumber(height)} < ${formatNumber(oppositeSide)} < ${formatNumber(adjacentSide)}$). Two triangles are possible (Ambiguous Case).</p>`);
                                
                                // Solve for the first acute angle
                                const angle1 = Math.asin(sinVal) * R_TO_D;
                                // Solve for the second obtuse angle
                                const angle2 = 180 - angle1;
                                
                                // Solution 1 (using angle1)
                                let sol1 = { a, b, c, alpha, beta, gamma };
                                if (alpha !== null) sol1.beta = angle1; else if (beta !== null) sol1.alpha = angle1; else sol1.alpha = angle1; // Assume solving for the angle opposite side B
                                solutionSet.push(resolveTriangle(a, b, c, alpha, sol1.beta, gamma, steps, 1));
                                
                                // Solution 2 (using angle2)
                                let sol2 = { a, b, c, alpha, beta, gamma };
                                if (alpha !== null) sol2.beta = angle2; else if (beta !== null) sol2.alpha = angle2; else sol2.alpha = angle2;
                                solutionSet.push(resolveTriangle(a, b, c, alpha, sol2.beta, gamma, steps, 2));
                            } else {
                                throw new Error('Input resulted in an undefined state during SSA check.');
                            }

                        } else {
                            // AAS/ASA (One Solution)
                            solutionSet.push(resolveTriangle(a, b, c, alpha, beta, gamma, steps, 1));
                        }


                        // 4. Output Results
                        if (solutionSet.length === 0) {
                             caseResult.textContent = 'Result: No Triangle Exists.';
                             stepByStep.innerHTML = steps.join('');
                             throw new Error('No Solution.');
                        }
                        
                        caseResult.textContent = `Result: ${solutionSet.length} Triangle Solution${solutionSet.length > 1 ? 's' : ''} Found (Case: ${caseType})`;

                        // Output Solution 1
                        const sol1 = solutionSet[0];
                        document.getElementById('result-sides-1').innerHTML = `a = ${formatNumber(sol1.a)}<br>b = ${formatNumber(sol1.b)}<br>c = ${formatNumber(sol1.c)}`;
                        document.getElementById('result-angles-1').innerHTML = `$\\alpha = ${formatAngle(sol1.alpha)}$<br>$\\beta = ${formatAngle(sol1.beta)}$<br>$\\gamma = ${formatAngle(sol1.gamma)}$`;
                        document.getElementById('solution-title-1').textContent = `Solution 1: ${caseType} Triangle`;
                        
                        // Output Solution 2 (if present)
                        if (solutionSet.length > 1) {
                            const sol2 = solutionSet[1];
                            solution2Box.classList.remove('hidden');
                            document.getElementById('result-sides-2').innerHTML = `a = ${formatNumber(sol2.a)}<br>b = ${formatNumber(sol2.b)}<br>c = ${formatNumber(sol2.c)}`;
                            document.getElementById('result-angles-2').innerHTML = `$\\alpha = ${formatAngle(sol2.alpha)}$<br>$\\beta = ${formatAngle(sol2.beta)}$<br>$\\gamma = ${formatAngle(sol2.gamma)}$`;
                        } else {
                            solution2Box.classList.add('hidden');
                        }
                        
                        stepByStep.innerHTML = steps.join('');
                        resultsContainer.classList.remove('hidden');
                        
                        // Re-render MathJax
                        if (window.MathJax) {
                            window.MathJax.typesetPromise([solutionContainer, stepByStep]);
                        }

                    } catch (e) {
                        showError(e.message);
                    }
                });

                // Initial calculation (AAS example)
                calculateBtn.click();
            </script>

