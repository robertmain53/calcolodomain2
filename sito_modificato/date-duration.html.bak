    <script>
      // Tab handling
      const tabDiff = document.getElementById("tab-diff");
      const tabAddSub = document.getElementById("tab-addsub");
      const panelDiff = document.getElementById("panel-diff");
      const panelAddSub = document.getElementById("panel-addsub");

      function activateTab(tab) {
        if (tab === "diff") {
          tabDiff.classList.add("border-blue-600", "text-blue-600");
          tabDiff.classList.remove("border-transparent", "text-gray-500");
          tabAddSub.classList.remove("border-blue-600", "text-blue-600");
          tabAddSub.classList.add("border-transparent", "text-gray-500");
          panelDiff.classList.remove("hidden");
          panelAddSub.classList.add("hidden");
        } else {
          tabAddSub.classList.add("border-blue-600", "text-blue-600");
          tabAddSub.classList.remove("border-transparent", "text-gray-500");
          tabDiff.classList.remove("border-blue-600", "text-blue-600");
          tabDiff.classList.add("border-transparent", "text-gray-500");
          panelAddSub.classList.remove("hidden");
          panelDiff.classList.add("hidden");
        }
      }

      tabDiff.addEventListener("click", () => activateTab("diff"));
      tabAddSub.addEventListener("click", () => activateTab("addsub"));

      // Utility functions
      function parseDateInput(id) {
        const val = document.getElementById(id).value;
        if (!val) return null;
        const d = new Date(val + "T00:00:00");
        return isNaN(d.getTime()) ? null : d;
      }

      function formatDateISO(d) {
        return d.toISOString().slice(0, 10);
      }

      function weekdayName(d) {
        return d.toLocaleDateString(undefined, { weekday: "long" });
      }

      function diffInDays(start, end) {
        const msPerDay = 24 * 60 * 60 * 1000;
        const utcStart = Date.UTC(
          start.getFullYear(),
          start.getMonth(),
          start.getDate()
        );
        const utcEnd = Date.UTC(
          end.getFullYear(),
          end.getMonth(),
          end.getDate()
        );
        return Math.round((utcEnd - utcStart) / msPerDay);
      }

      function calendarDiff(start, end) {
        // Ensure start <= end
        let s = new Date(start.getTime());
        let e = new Date(end.getTime());
        if (s > e) {
          const tmp = s;
          s = e;
          e = tmp;
        }

        let years = e.getFullYear() - s.getFullYear();
        let months = e.getMonth() - s.getMonth();
        let days = e.getDate() - s.getDate();

        if (days < 0) {
          months -= 1;
          const prevMonth = new Date(e.getFullYear(), e.getMonth(), 0);
          days += prevMonth.getDate();
        }
        if (months < 0) {
          years -= 1;
          months += 12;
        }
        return { years, months, days };
      }

      function parseHolidaySet(text) {
        const set = new Set();
        text.split(/\r?\n/).forEach((line) => {
          const t = line.trim();
          if (!t) return;
          // basic YYYY-MM-DD check
          if (/^\d{4}-\d{2}-\d{2}$/.test(t)) {
            set.add(t);
          }
        });
        return set;
      }

      function countBusinessDays(start, end, includeEnd, holidaysSet) {
        if (start > end) {
          const tmp = start;
          start = end;
          end = tmp;
        }
        let days = diffInDays(start, end);
        if (includeEnd) days += 1;
        if (days <= 0) return 0;

        let count = 0;
        const cur = new Date(
          start.getFullYear(),
          start.getMonth(),
          start.getDate()
        );
        for (let i = 0; i < days; i++) {
          const day = cur.getDay(); // 0=Sun..6=Sat
          const iso = formatDateISO(cur);
          if (day !== 0 && day !== 6 && !holidaysSet.has(iso)) {
            count++;
          }
          cur.setDate(cur.getDate() + 1);
        }
        return count;
      }

      // Between two dates calculation
      const btnCalcDiff = document.getElementById("btn-calc-diff");
      const btnSwap = document.getElementById("btn-swap");
      const btnTodayRange = document.getElementById("btn-today-range");
      const diffError = document.getElementById("diff-error");
      const diffResults = document.getElementById("diff-results");

      btnCalcDiff.addEventListener("click", () => {
        diffError.classList.add("hidden");
        diffError.textContent = "";

        const start = parseDateInput("start-date");
        const end = parseDateInput("end-date");
        const includeEnd = document.getElementById("include-end").checked;
        const businessOnly =
          document.getElementById("business-days-only").checked;
        const holidaysText = document.getElementById("holidays").value;
        const holidaysSet = parseHolidaySet(holidaysText);

        if (!start || !end) {
          diffError.textContent =
            "Please enter both a valid start date and end date.";
          diffError.classList.remove("hidden");
          diffResults.classList.add("hidden");
          return;
        }

        let s = start;
        let e = end;
        let swapped = false;
        if (s > e) {
          swapped = true;
          const tmp = s;
          s = e;
          e = tmp;
        }

        let totalDays = diffInDays(s, e);
        if (includeEnd) totalDays += 1;

        if (totalDays < 0) {
          diffError.textContent =
            "The end date must be on or after the start date.";
          diffError.classList.remove("hidden");
          diffResults.classList.add("hidden");
          return;
        }

        const cal = calendarDiff(s, e);
        const businessDays = businessOnly
          ? countBusinessDays(s, e, includeEnd, holidaysSet)
          : "—";

        const totalWeeks = (totalDays / 7).toFixed(2);
        const totalHours = totalDays * 24;
        const totalMinutes = totalHours * 60;

        document.getElementById("diff-years").textContent = cal.years;
        document.getElementById("diff-months").textContent = cal.months;
        document.getElementById("diff-days").textContent = cal.days;
        document.getElementById("diff-total-days").textContent = totalDays;
        document.getElementById("diff-business-days").textContent =
          businessDays;
        document.getElementById("diff-weeks").textContent = totalWeeks;
        document.getElementById("diff-hours").textContent =
          totalHours.toLocaleString();
        document.getElementById("diff-minutes").textContent =
          totalMinutes.toLocaleString();

        const sLabel = formatDateISO(start) + " (" + weekdayName(start) + ")";
        const eLabel = formatDateISO(end) + " (" + weekdayName(end) + ")";
        const direction = swapped
          ? " (dates were swapped so the earlier date is treated as the start)"
          : "";

        document.getElementById(
          "diff-summary"
        ).textContent = `There are ${totalDays} day(s) between ${sLabel} and ${eLabel}${
          includeEnd ? " (end date included)" : ""
        }.`;

        document.getElementById("diff-weekday-summary").textContent =
          businessOnly
            ? `Business days counted (Mon–Fri, excluding listed holidays): ${businessDays}.${direction}`
            : `Calendar days only (weekends included).${direction}`;

        diffResults.classList.remove("hidden");
      });

      btnSwap.addEventListener("click", () => {
        const s = document.getElementById("start-date").value;
        const e = document.getElementById("end-date").value;
        document.getElementById("start-date").value = e;
        document.getElementById("end-date").value = s;
      });

      btnTodayRange.addEventListener("click", () => {
        const today = new Date();
        const iso = formatDateISO(today);
        document.getElementById("start-date").value = iso;
        document.getElementById("end-date").value = iso;
      });

      // Add / subtract
      const btnCalcAddSub = document.getElementById("btn-calc-addsub");
      const btnAsClear = document.getElementById("btn-as-clear");
      const asError = document.getElementById("as-error");
      const asResults = document.getElementById("as-results");

      btnCalcAddSub.addEventListener("click", () => {
        asError.classList.add("hidden");
        asError.textContent = "";

        const base = parseDateInput("as-base-date");
        if (!base) {
          asError.textContent = "Please enter a valid start date.";
          asError.classList.remove("hidden");
          asResults.classList.add("hidden");
          return;
        }

        const op = document.querySelector('input[name="as-op"]:checked').value;
        const sign = op === "add" ? 1 : -1;

        const years = parseInt(
          document.getElementById("as-years").value || "0",
          10
        );
        const months = parseInt(
          document.getElementById("as-months").value || "0",
          10
        );
        const weeks = parseInt(
          document.getElementById("as-weeks").value || "0",
          10
        );
        const days = parseInt(
          document.getElementById("as-days").value || "0",
          10
        );

        if (years < 0 || months < 0 || weeks < 0 || days < 0) {
          asError.textContent =
            "Years, months, weeks, and days must be zero or positive.";
          asError.classList.remove("hidden");
          asResults.classList.add("hidden");
          return;
        }

        if (years === 0 && months === 0 && weeks === 0 && days === 0) {
          asError.textContent =
            "Please enter at least one non-zero duration value.";
          asError.classList.remove("hidden");
          asResults.classList.add("hidden");
          return;
        }

        const result = new Date(base.getTime());
        // Apply years and months using calendar arithmetic
        result.setFullYear(result.getFullYear() + sign * years);
        result.setMonth(result.getMonth() + sign * months);
        // Weeks and days as day offsets
        const totalDaysOffset = sign * (weeks * 7 + days);
        result.setDate(result.getDate() + totalDaysOffset);

        const baseLabel = formatDateISO(base) + " (" + weekdayName(base) + ")";
        const resultLabel =
          formatDateISO(result) + " (" + weekdayName(result) + ")";

        document.getElementById("as-summary").textContent = `${
          op === "add" ? "Adding" : "Subtracting"
        } ${years} year(s), ${months} month(s), ${weeks} week(s), and ${days} day(s) from ${baseLabel} gives ${resultLabel}.`;

        document.getElementById(
          "as-weekday"
        ).textContent = `Resulting date: ${formatDateISO(
          result
        )} – ${weekdayName(result)}.`;

        asResults.classList.remove("hidden");
      });

      btnAsClear.addEventListener("click", () => {
        document.getElementById("as-years").value = 0;
        document.getElementById("as-months").value = 0;
        document.getElementById("as-weeks").value = 0;
        document.getElementById("as-days").value = 0;
        asResults.classList.add("hidden");
        asError.classList.add("hidden");
      });

      // Initialize defaults
      (function initDefaults() {
        const today = new Date();
        const iso = formatDateISO(today);
        document.getElementById("start-date").value = iso;
        document.getElementById("end-date").value = iso;
        document.getElementById("as-base-date").value = iso;
      })();
    </script>

