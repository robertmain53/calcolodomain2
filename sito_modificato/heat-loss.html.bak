    <script>
      // Tab handling
      const tabs = document.querySelectorAll(".heat-tab");
      const panels = {
        conduction: document.getElementById("panel-conduction"),
        convection: document.getElementById("panel-convection"),
        radiation: document.getElementById("panel-radiation"),
      };
      tabs.forEach((btn) => {
        btn.addEventListener("click", () => {
          const target = btn.getAttribute("data-tab");
          tabs.forEach((b) => {
            b.classList.remove(
              "border-blue-600",
              "bg-blue-50",
              "text-blue-700",
              "font-medium"
            );
            b.classList.add("border-gray-300", "text-gray-700");
          });
          btn.classList.add(
            "border-blue-600",
            "bg-blue-50",
            "text-blue-700",
            "font-medium"
          );
          btn.classList.remove("border-gray-300", "text-gray-700");
          Object.keys(panels).forEach((k) => {
            panels[k].classList.toggle("hidden", k !== target);
          });
        });
      });

      // Conduction mode toggle
      const condModeRadios = document.querySelectorAll(
        'input[name="condMode"]'
      );
      const condUWrapper = document.getElementById("cond-u-wrapper");
      const condKWrapper = document.getElementById("cond-k-wrapper");
      condModeRadios.forEach((r) => {
        r.addEventListener("change", () => {
          if (r.checked && r.value === "u") {
            condUWrapper.classList.remove("hidden");
            condKWrapper.classList.add("hidden");
          } else if (r.checked && r.value === "k") {
            condUWrapper.classList.add("hidden");
            condKWrapper.classList.remove("hidden");
          }
        });
      });

      // Unit system handling
      const unitRadios = document.querySelectorAll('input[name="unitSystem"]');
      const tHotUnit = document.getElementById("tHotUnit");
      const tColdUnit = document.getElementById("tColdUnit");
      const areaUnit = document.getElementById("areaUnit");
      const uUnit = document.getElementById("uUnit");
      const kUnit = document.getElementById("kUnit");
      const thicknessUnit = document.getElementById("thicknessUnit");
      const hUnit = document.getElementById("hUnit");

      function updateUnitLabels() {
        const system = document.querySelector(
          'input[name="unitSystem"]:checked'
        ).value;
        if (system === "si") {
          tHotUnit.textContent = "°C";
          tColdUnit.textContent = "°C";
          areaUnit.textContent = "m²";
          uUnit.textContent = "W/(m²·K)";
          kUnit.textContent = "W/(m·K)";
          thicknessUnit.textContent = "m";
          hUnit.textContent = "W/(m²·K)";
        } else {
          tHotUnit.textContent = "°F";
          tColdUnit.textContent = "°F";
          areaUnit.textContent = "ft²";
          uUnit.textContent = "BTU/(hr·ft²·°F)";
          kUnit.textContent = "BTU/(hr·ft·°F)";
          thicknessUnit.textContent = "ft";
          hUnit.textContent = "BTU/(hr·ft²·°F)";
        }
      }
      unitRadios.forEach((r) => r.addEventListener("change", updateUnitLabels));
      updateUnitLabels();

      // Helpers
      function toNumber(id) {
        const v = parseFloat(document.getElementById(id).value);
        return isNaN(v) ? NaN : v;
      }
      function round(val, digits) {
        const p = Math.pow(10, digits);
        return Math.round(val * p) / p;
      }

      // Calculation
      const calcBtn = document.getElementById("calcBtn");
      const resetBtn = document.getElementById("resetBtn");
      const errorMsg = document.getElementById("errorMsg");
      const resultsCard = document.getElementById("resultsCard");
      const qTotalW = document.getElementById("qTotalW");
      const qTotalBtu = document.getElementById("qTotalBtu");
      const deltaTOut = document.getElementById("deltaTOut");
      const qCondOut = document.getElementById("qCond");
      const qConvOut = document.getElementById("qConv");
      const qRadOut = document.getElementById("qRad");
      const dominantMode = document.getElementById("dominantMode");

      calcBtn.addEventListener("click", () => {
        errorMsg.textContent = "";
        dominantMode.textContent = "";
        const system = document.querySelector(
          'input[name="unitSystem"]:checked'
        ).value;

        const tHot = toNumber("tHot");
        const tCold = toNumber("tCold");
        const area = toNumber("area");
        const emissivity = toNumber("emissivity");
        const h = toNumber("hValue");

        if (isNaN(tHot) || isNaN(tCold) || isNaN(area) || area <= 0) {
          errorMsg.textContent =
            "Please enter valid temperatures and a positive surface area.";
          resultsCard.classList.add("hidden");
          return;
        }
        if (emissivity < 0 || emissivity > 1) {
          errorMsg.textContent = "Emissivity must be between 0 and 1.";
          resultsCard.classList.add("hidden");
          return;
        }

        const dT = tHot - tCold;
        if (dT === 0) {
          errorMsg.textContent =
            "Temperature difference is zero – heat loss will be zero.";
        }

        // Convert inputs to SI
        let dT_K;
        let area_m2;
        if (system === "si") {
          dT_K = dT; // K and °C difference are same
          area_m2 = area;
        } else {
          dT_K = (dT * 5) / 9; // °F difference to K
          area_m2 = area * 0.092903; // ft² to m²
        }

        // Conduction
        let qCond = 0;
        const condMode = document.querySelector(
          'input[name="condMode"]:checked'
        ).value;
        if (condMode === "u") {
          const U = toNumber("uValue");
          if (!isNaN(U) && U > 0) {
            let U_SI;
            if (system === "si") {
              U_SI = U;
            } else {
              // BTU/(hr·ft²·°F) to W/(m²·K)
              U_SI = U * 5.678263;
            }
            qCond = U_SI * area_m2 * dT_K;
          }
        } else {
          const k = toNumber("kValue");
          const L = toNumber("thickness");
          if (!isNaN(k) && !isNaN(L) && k > 0 && L > 0) {
            let k_SI;
            let L_m;
            if (system === "si") {
              k_SI = k;
              L_m = L;
            } else {
              // BTU/(hr·ft·°F) to W/(m·K)
              k_SI = k * 1.730735;
              L_m = L * 0.3048;
            }
            qCond = (k_SI * area_m2 * dT_K) / L_m;
          }
        }

        // Convection
        let qConv = 0;
        if (!isNaN(h) && h > 0) {
          let h_SI;
          if (system === "si") {
            h_SI = h;
          } else {
            // BTU/(hr·ft²·°F) to W/(m²·K)
            h_SI = h * 5.678263;
          }
          qConv = h_SI * area_m2 * dT_K;
        }

        // Radiation
        let qRad = 0;
        if (emissivity > 0 && dT !== 0) {
          const sigma = 5.670374419e-8;
          let Th_K, Tc_K;
          if (system === "si") {
            Th_K = tHot + 273.15;
            Tc_K = tCold + 273.15;
          } else {
            Th_K = ((tHot - 32) * 5) / 9 + 273.15;
            Tc_K = ((tCold - 32) * 5) / 9 + 273.15;
          }
          qRad =
            emissivity *
            sigma *
            area_m2 *
            (Math.pow(Th_K, 4) - Math.pow(Tc_K, 4));
        }

        const qTotal = qCond + qConv + qRad;
        const qTotal_BTUhr = qTotal * 3.412141633; // W to BTU/hr

        qTotalW.textContent = isFinite(qTotal)
          ? `${round(qTotal, 2)} W`
          : "– W";
        qTotalBtu.textContent = isFinite(qTotal_BTUhr)
          ? `${round(qTotal_BTUhr, 2)} BTU/hr`
          : "– BTU/hr";

        const dTLabel =
          system === "si" ? `${round(dT, 2)} °C` : `${round(dT, 2)} °F`;
        deltaTOut.textContent = dTLabel;

        qCondOut.textContent = `${round(qCond, 2)} W`;
        qConvOut.textContent = `${round(qConv, 2)} W`;
        qRadOut.textContent = `${round(qRad, 2)} W`;

        // Dominant mode
        const modes = [
          { name: "conduction", value: Math.abs(qCond) },
          { name: "convection", value: Math.abs(qConv) },
          { name: "radiation", value: Math.abs(qRad) },
        ];
        modes.sort((a, b) => b.value - a.value);
        if (modes[0].value > 0 && qTotal !== 0) {
          const frac = (modes[0].value / Math.abs(qTotal)) * 100;
          dominantMode.textContent = `Dominant mechanism: ${
            modes[0].name
          } (~${round(frac, 1)}% of total heat loss).`;
        }

        resultsCard.classList.remove("hidden");
      });

      resetBtn.addEventListener("click", () => {
        document.querySelector(
          'input[name="unitSystem"][value="si"]'
        ).checked = true;
        document.getElementById("tHot").value = 60;
        document.getElementById("tCold").value = 20;
        document.getElementById("area").value = 10;
        document.getElementById("uValue").value = 0.5;
        document.getElementById("kValue").value = 0.04;
        document.getElementById("thickness").value = 0.2;
        document.getElementById("hValue").value = 8;
        document.getElementById("emissivity").value = 0.9;
        document.querySelector(
          'input[name="condMode"][value="u"]'
        ).checked = true;
        condUWrapper.classList.remove("hidden");
        condKWrapper.classList.add("hidden");
        updateUnitLabels();
        errorMsg.textContent = "";
        resultsCard.classList.add("hidden");
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
