    <script>
      // Exact conversion factor (IAU 2015, Julian year)
      const LY_TO_M = 9460730472580800; // 9,460,730,472,580,800 m
      const M_TO_LY = 1 / LY_TO_M;

      const lyInput = document.getElementById("lyInput");
      const mInput = document.getElementById("mInput");
      const resultStandard = document.getElementById("resultStandard");
      const resultSci = document.getElementById("resultSci");
      const swapBtn = document.getElementById("swapBtn");
      const clearBtn = document.getElementById("clearBtn");

      let primary = "ly"; // which field user is primarily editing

      function formatStandard(value) {
        if (!isFinite(value)) return "–";
        if (value === 0) return "0";
        const abs = Math.abs(value);
        if (abs >= 1e6 || abs <= 1e-3) {
          return value.toExponential(6).replace(/e\+?(-?\d+)/, " × 10^$1");
        }
        return value.toLocaleString("en-US", { maximumFractionDigits: 6 });
      }

      function formatScientific(value) {
        if (!isFinite(value) || value === 0) return "–";
        const exp = value.toExponential(10);
        const [coeff, ePart] = exp.split("e");
        const exponent = parseInt(ePart, 10);
        return coeff.replace(/\.?0+$/, "") + " × 10^" + exponent;
      }

      function updateFromLy() {
        const ly = parseFloat(lyInput.value);
        if (isNaN(ly)) {
          mInput.value = "";
          resultStandard.textContent = "Enter a value to see the result.";
          resultSci.textContent = "–";
          return;
        }
        const meters = ly * LY_TO_M;
        mInput.value = meters;
        resultStandard.innerHTML = `<span>${formatStandard(
          ly
        )} ly = <strong>${formatStandard(meters)} m</strong></span>`;
        resultSci.innerHTML = `<span>${formatScientific(
          ly
        )} ly = <strong>${formatScientific(meters)} m</strong></span>`;
      }

      function updateFromM() {
        const meters = parseFloat(mInput.value);
        if (isNaN(meters)) {
          lyInput.value = "";
          resultStandard.textContent = "Enter a value to see the result.";
          resultSci.textContent = "–";
          return;
        }
        const ly = meters * M_TO_LY;
        lyInput.value = ly;
        resultStandard.innerHTML = `<span>${formatStandard(
          meters
        )} m = <strong>${formatStandard(ly)} ly</strong></span>`;
        resultSci.innerHTML = `<span>${formatScientific(
          meters
        )} m = <strong>${formatScientific(ly)} ly</strong></span>`;
      }

      lyInput.addEventListener("input", () => {
        primary = "ly";
        updateFromLy();
      });

      mInput.addEventListener("input", () => {
        primary = "m";
        updateFromM();
      });

      swapBtn.addEventListener("click", () => {
        if (primary === "ly") {
          primary = "m";
          if (lyInput.value !== "") updateFromLy();
        } else {
          primary = "ly";
          if (mInput.value !== "") updateFromM();
        }
      });

      clearBtn.addEventListener("click", () => {
        lyInput.value = "";
        mInput.value = "";
        resultStandard.textContent = "Enter a value to see the result.";
        resultSci.textContent = "–";
        primary = "ly";
      });
    </script>

