            <script>
              document.addEventListener('DOMContentLoaded', () => {
                const atmInput = document.getElementById('atm-input');
                const psiInput = document.getElementById('psi-input');
                const barInput = document.getElementById('bar-input');
                const kpaInput = document.getElementById('kpa-input');
                
                const inputs = [atmInput, psiInput, barInput, kpaInput];
                
                // Conversion factors TO 1 PASCAL (Pa) - La nostra base scientifica
                const PA_TO_ATM = 1 / 101325;
                const PA_TO_PSI = 1 / 6894.75729;
                const PA_TO_BAR = 1 / 100000;
                const PA_TO_KPA = 1 / 1000;

                const factors = {
                    'atm': PA_TO_ATM,
                    'psi': PA_TO_PSI,
                    'bar': PA_TO_BAR,
                    'kpa': PA_TO_KPA
                };

                function formatNum(num) {
                    if (isNaN(num) || num === 0) return '';
                    return parseFloat(num.toPrecision(7)).toString();
                }

                function update(sourceUnit, value) {
                    if (isNaN(value) || value === 0) {
                        inputs.forEach(input => input.value = '');
                        return;
                    }

                    let pa = 0; // Base unit

                    // 1. Converti l'input sorgente in Pascals (Pa)
                    pa = value / factors[sourceUnit];
                    
                    // 2. Aggiorna tutti gli altri campi
                    for (const input of inputs) {
                        const unit = input.dataset.unit;
                        if (unit !== sourceUnit) {
                            input.value = formatNum(pa * factors[unit]);
                        }
                    }
                }

                inputs.forEach(input => {
                  input.addEventListener('input', (e) => {
                    const sourceUnit = e.target.dataset.unit;
                    const value = parseFloat(e.target.value);
                    update(sourceUnit, value);
                  });
                });
              });
            </script>
            
