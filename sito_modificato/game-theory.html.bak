    <script>
      // ---------- Utility formatting ----------
      function gtFormatNumber(value, decimals) {
        if (!isFinite(value)) return String(value);
        var d = Math.max(0, Math.min(10, decimals || 3));
        var abs = Math.abs(value);
        if (abs !== 0 && (abs >= 1e6 || abs < Math.pow(10, -d))) {
          return value
            .toExponential(d)
            .replace(/0+e/, "e")
            .replace(/(\.\d*[1-9])0+e/, "$1e");
        }
        var s = value.toFixed(d);
        s = s.replace(/0+$/, "").replace(/\.$/, "");
        if (s === "-0") s = "0";
        return s;
      }

      function gtReadNumber(id) {
        var el = document.getElementById(id);
        if (!el) {
          throw new Error("Internal error: missing input " + id);
        }
        var v = parseFloat(el.value);
        if (!isFinite(v)) {
          throw new Error(
            "All payoffs must be finite numbers. Check cell " + id + "."
          );
        }
        return v;
      }

      function gtSolve() {
        var resultOut = document.getElementById("gt-result");
        var stepsOut = document.getElementById("gt-steps");
        var dec =
          parseInt(document.getElementById("gt-decimals").value, 10) || 3;
        var showSteps = document.getElementById("gt-show-steps").checked;

        try {
          var p1Name = (
            document.getElementById("gt-player1-name").value || "Row player"
          ).trim();
          var p2Name = (
            document.getElementById("gt-player2-name").value || "Column player"
          ).trim();
          var r1Name = (
            document.getElementById("gt-row1-name").value || "Top"
          ).trim();
          var r2Name = (
            document.getElementById("gt-row2-name").value || "Bottom"
          ).trim();
          var c1Name = (
            document.getElementById("gt-col1-name").value || "Left"
          ).trim();
          var c2Name = (
            document.getElementById("gt-col2-name").value || "Right"
          ).trim();

          // keep header labels in sync
          document.getElementById("gt-row1-label-head").textContent = r1Name;
          document.getElementById("gt-row2-label-head").textContent = r2Name;
          document.getElementById("gt-col1-label-head").textContent = c1Name;
          document.getElementById("gt-col2-label-head").textContent = c2Name;

          // Payoffs a_ij for row, b_ij for column
          var a11 = gtReadNumber("gt-a-11"),
            a12 = gtReadNumber("gt-a-12"),
            a21 = gtReadNumber("gt-a-21"),
            a22 = gtReadNumber("gt-a-22");
          var b11 = gtReadNumber("gt-b-11"),
            b12 = gtReadNumber("gt-b-12"),
            b21 = gtReadNumber("gt-b-21"),
            b22 = gtReadNumber("gt-b-22");

          // Best responses
          // For each column, best responses of row player
          var rowBestForC1 = [];
          var maxC1 = Math.max(a11, a21);
          if (a11 === maxC1) rowBestForC1.push("r1");
          if (a21 === maxC1) rowBestForC1.push("r2");

          var rowBestForC2 = [];
          var maxC2 = Math.max(a12, a22);
          if (a12 === maxC2) rowBestForC2.push("r1");
          if (a22 === maxC2) rowBestForC2.push("r2");

          // For each row, best responses of column player
          var colBestForR1 = [];
          var maxR1 = Math.max(b11, b12);
          if (b11 === maxR1) colBestForR1.push("c1");
          if (b12 === maxR1) colBestForR1.push("c2");

          var colBestForR2 = [];
          var maxR2 = Math.max(b21, b22);
          if (b21 === maxR2) colBestForR2.push("c1");
          if (b22 === maxR2) colBestForR2.push("c2");

          // Pure Nash equilibria: cells that are best responses for both players
          var pureNE = [];
          if (
            rowBestForC1.indexOf("r1") !== -1 &&
            colBestForR1.indexOf("c1") !== -1
          ) {
            pureNE.push({
              row: "r1",
              col: "c1",
              rName: r1Name,
              cName: c1Name,
              a: a11,
              b: b11,
            });
          }
          if (
            rowBestForC2.indexOf("r1") !== -1 &&
            colBestForR1.indexOf("c2") !== -1
          ) {
            pureNE.push({
              row: "r1",
              col: "c2",
              rName: r1Name,
              cName: c2Name,
              a: a12,
              b: b12,
            });
          }
          if (
            rowBestForC1.indexOf("r2") !== -1 &&
            colBestForR2.indexOf("c1") !== -1
          ) {
            pureNE.push({
              row: "r2",
              col: "c1",
              rName: r2Name,
              cName: c1Name,
              a: a21,
              b: b21,
            });
          }
          if (
            rowBestForC2.indexOf("r2") !== -1 &&
            colBestForR2.indexOf("c2") !== -1
          ) {
            pureNE.push({
              row: "r2",
              col: "c2",
              rName: r2Name,
              cName: c2Name,
              a: a22,
              b: b22,
            });
          }

          // Dominant strategies
          function dominanceInfo() {
            var info = {
              p1Strict: null, // "r1"/"r2"
              p1Weak: null,
              p2Strict: null, // "c1"/"c2"
              p2Weak: null,
            };

            // Row player dominance
            var r1WeaklyDominatesR2 = a11 >= a21 && a12 >= a22;
            var r2WeaklyDominatesR1 = a21 >= a11 && a22 >= a12;
            var r1Strict = a11 > a21 && a12 > a22;
            var r2Strict = a21 > a11 && a22 > a12;

            if (r1Strict) info.p1Strict = "r1";
            else if (r2Strict) info.p1Strict = "r2";
            else if (
              r1WeaklyDominatesR2 &&
              !(r2WeaklyDominatesR1 && !r1WeaklyDominatesR2)
            )
              info.p1Weak = "r1";
            else if (
              r2WeaklyDominatesR1 &&
              !(r1WeaklyDominatesR2 && !r2WeaklyDominatesR1)
            )
              info.p1Weak = "r2";

            // Column player dominance
            var c1WeaklyDominatesC2 = b11 >= b12 && b21 >= b22;
            var c2WeaklyDominatesC1 = b12 >= b11 && b22 >= b21;
            var c1Strict = b11 > b12 && b21 > b22;
            var c2Strict = b12 > b11 && b22 > b21;

            if (c1Strict) info.p2Strict = "c1";
            else if (c2Strict) info.p2Strict = "c2";
            else if (
              c1WeaklyDominatesC2 &&
              !(c2WeaklyDominatesC1 && !c1WeaklyDominatesC2)
            )
              info.p2Weak = "c1";
            else if (
              c2WeaklyDominatesC1 &&
              !(c1WeaklyDominatesC2 && !c2WeaklyDominatesC1)
            )
              info.p2Weak = "c2";

            return info;
          }

          var dom = dominanceInfo();

          // Mixed strategy equilibrium (if interior)
          var mixed = null;
          var denomQ = a11 - a12 - a21 + a22;
          var denomP = b11 - b21 - b12 + b22;
          var degenerate = false;

          if (denomQ === 0 || denomP === 0) {
            degenerate = true;
          } else {
            var q = (a22 - a12) / denomQ; // probability Column plays Left making Row indifferent
            var p = (b22 - b21) / denomP; // probability Row plays Top making Column indifferent
            if (
              isFinite(p) &&
              isFinite(q) &&
              p > 0 &&
              p < 1 &&
              q > 0 &&
              q < 1
            ) {
              mixed = { p: p, q: q };
            }
          }

          // Build result HTML
          var html = "";
          html +=
            '<p class="mb-1 text-sm font-medium">Equilibrium summary:</p>';
          html += '<ul class="list-disc ml-5 text-sm mb-2">';

          if (pureNE.length > 0) {
            // List pure NE
            for (var i = 0; i < pureNE.length; i++) {
              var ne = pureNE[i];
              html +=
                "<li>Pure Nash equilibrium at (" +
                ne.rName +
                ", " +
                ne.cName +
                ") with payoffs (" +
                gtFormatNumber(ne.a, dec) +
                ", " +
                gtFormatNumber(ne.b, dec) +
                ").</li>";
            }
          } else {
            html += "<li>No pure-strategy Nash equilibrium found.</li>";
          }

          // Dominance
          if (dom.p1Strict || dom.p1Weak || dom.p2Strict || dom.p2Weak) {
            var parts = [];
            if (dom.p1Strict === "r1")
              parts.push(
                p1Name +
                  " strictly prefers " +
                  r1Name +
                  " to " +
                  r2Name +
                  " (dominant strategy)."
              );
            if (dom.p1Strict === "r2")
              parts.push(
                p1Name +
                  " strictly prefers " +
                  r2Name +
                  " to " +
                  r1Name +
                  " (dominant strategy)."
              );
            if (!dom.p1Strict && dom.p1Weak === "r1")
              parts.push(
                p1Name + " has a weakly dominant strategy " + r1Name + "."
              );
            if (!dom.p1Strict && dom.p1Weak === "r2")
              parts.push(
                p1Name + " has a weakly dominant strategy " + r2Name + "."
              );

            if (dom.p2Strict === "c1")
              parts.push(
                p2Name +
                  " strictly prefers " +
                  c1Name +
                  " to " +
                  c2Name +
                  " (dominant strategy)."
              );
            if (dom.p2Strict === "c2")
              parts.push(
                p2Name +
                  " strictly prefers " +
                  c2Name +
                  " to " +
                  c1Name +
                  " (dominant strategy)."
              );
            if (!dom.p2Strict && dom.p2Weak === "c1")
              parts.push(
                p2Name + " has a weakly dominant strategy " + c1Name + "."
              );
            if (!dom.p2Strict && dom.p2Weak === "c2")
              parts.push(
                p2Name + " has a weakly dominant strategy " + c2Name + "."
              );

            html += "<li>" + parts.join(" ") + "</li>";
          } else {
            html +=
              "<li>No global dominant strategy detected for either player.</li>";
          }

          if (mixed) {
            html +=
              "<li>Interior mixed-strategy equilibrium: " +
              p1Name +
              " plays " +
              r1Name +
              " with probability " +
              gtFormatNumber(mixed.p, dec) +
              " and " +
              r2Name +
              " with " +
              gtFormatNumber(1 - mixed.p, dec) +
              "; " +
              p2Name +
              " plays " +
              c1Name +
              " with probability " +
              gtFormatNumber(mixed.q, dec) +
              " and " +
              c2Name +
              " with " +
              gtFormatNumber(1 - mixed.q, dec) +
              ".</li>";
          } else if (!degenerate) {
            html +=
              "<li>No interior mixed-strategy equilibrium with probabilities strictly between 0 and 1.</li>";
          } else {
            html +=
              "<li>The mixed-strategy indifference equations are degenerate (denominator zero). The game may have only pure equilibria or a continuum of borderline mixed strategies.</li>";
          }

          // Some very light classification
          if (pureNE.length === 2) {
            html +=
              "<li>The game has two pure equilibria, which is typical of coordination-type games (multiple stable outcomes).</li>";
          } else if (pureNE.length === 0 && mixed) {
            html +=
              "<li>Zero pure equilibria but one interior mixed equilibrium: this pattern is typical of strictly competitive or anti-coordination games such as Matching Pennies.</li>";
          }

          html += "</ul>";

          resultOut.innerHTML = html;

          // Steps / explanation
          if (showSteps) {
            var steps = "";
            steps +=
              '<p class="text-sm mb-1 font-medium">How the solver works (high level):</p>';
            steps += '<ol class="list-decimal ml-5 text-sm mb-2">';
            steps +=
              "<li>Reads the four payoff pairs (Row, Column) for each cell of the 2×2 matrix.</li>";
            steps +=
              "<li>For each column, finds which row(s) give the row player the highest payoff (row best responses).</li>";
            steps +=
              "<li>For each row, finds which column(s) give the column player the highest payoff (column best responses).</li>";
            steps +=
              "<li>Marks any cell that is a best response for both players as a pure-strategy Nash equilibrium.</li>";
            steps +=
              "<li>Checks simple dominance inequalities to identify strictly or weakly dominant strategies.</li>";
            steps +=
              "<li>Builds and solves linear indifference equations to identify a potential interior mixed equilibrium, and verifies that the probabilities are between 0 and 1.</li>";
            steps += "</ol>";

            steps +=
              '<p class="text-xs text-gray-600 mb-1">Mixed-equilibrium equations (schematic):</p>';
            steps += '<div class="formula-box text-xs">\\[';
            steps += "q a_{11} + (1-q)a_{12} = q a_{21} + (1-q)a_{22},\\\\";
            steps += "p b_{11} + (1-p)b_{21} = p b_{12} + (1-p)b_{22}.";
            steps += "\\]</div>";
            steps +=
              '<p class="text-xs text-gray-600 mt-1">The solution (p, q) gives the probabilities with which each player mixes between their two strategies so that the opponent is indifferent.</p>';

            stepsOut.innerHTML = steps;
          } else {
            stepsOut.innerHTML =
              '<span class="text-gray-500 text-sm">Steps are hidden. Enable "Show detailed steps" to see the underlying logic.</span>';
          }

          if (window.MathJax && window.MathJax.typeset) {
            window.MathJax.typeset();
          }
        } catch (err) {
          resultOut.innerHTML =
            '<p class="text-red-600 text-sm">' + err.message + "</p>";
          stepsOut.innerHTML =
            '<span class="text-gray-500 text-sm">No steps available due to invalid input.</span>';
        }
      }

      function gtReset() {
        document.getElementById("gt-player1-name").value = "Row player";
        document.getElementById("gt-player2-name").value = "Column player";
        document.getElementById("gt-row1-name").value = "Top";
        document.getElementById("gt-row2-name").value = "Bottom";
        document.getElementById("gt-col1-name").value = "Left";
        document.getElementById("gt-col2-name").value = "Right";

        document.getElementById("gt-a-11").value = 3;
        document.getElementById("gt-b-11").value = 3;
        document.getElementById("gt-a-12").value = 0;
        document.getElementById("gt-b-12").value = 5;
        document.getElementById("gt-a-21").value = 5;
        document.getElementById("gt-b-21").value = 0;
        document.getElementById("gt-a-22").value = 1;
        document.getElementById("gt-b-22").value = 1;

        document.getElementById("gt-decimals").value = "3";
        document.getElementById("gt-show-steps").checked = true;

        document.getElementById("gt-row1-label-head").textContent = "Top";
        document.getElementById("gt-row2-label-head").textContent = "Bottom";
        document.getElementById("gt-col1-label-head").textContent = "Left";
        document.getElementById("gt-col2-label-head").textContent = "Right";

        document.getElementById("gt-result").innerHTML =
          '<span class="text-gray-500">Equilibria and strategic summary will appear here.</span>';
        document.getElementById("gt-steps").innerHTML =
          '<span class="text-gray-500">Step-by-step explanation of best responses and mixed strategies will appear here.</span>';
      }

      document.addEventListener("DOMContentLoaded", function () {
        gtReset();
        document
          .getElementById("gt-solve-btn")
          ?.addEventListener("click", gtSolve);
        document
          .getElementById("gt-reset-btn")
          ?.addEventListener("click", gtReset);
      });
    </script>

