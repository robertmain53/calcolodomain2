    <script>
      // --- Utility functions ---
      function grayCode(n) {
        return n ^ (n >> 1);
      }
      function graySequence(bits) {
        const len = 1 << bits;
        const seq = [];
        for (let i = 0; i < len; i++) seq.push(grayCode(i));
        return seq;
      }
      function countBits(x) {
        let c = 0;
        while (x) {
          x &= x - 1;
          c++;
        }
        return c;
      }
      function parseRangeList(str, maxIndex) {
        const result = new Set();
        if (!str.trim()) return result;
        const parts = str.split(/[,;\s]+/).filter(Boolean);
        for (const p of parts) {
          if (p.includes("-")) {
            const [a, b] = p.split("-").map(Number);
            if (Number.isInteger(a) && Number.isInteger(b)) {
              const start = Math.min(a, b);
              const end = Math.max(a, b);
              for (let i = start; i <= end && i <= maxIndex; i++) {
                if (i >= 0) result.add(i);
              }
            }
          } else {
            const v = Number(p);
            if (Number.isInteger(v) && v >= 0 && v <= maxIndex) result.add(v);
          }
        }
        return result;
      }

      // --- K-map state ---
      const state = {
        vars: 4,
        values: [], // 0,1,2 (2 = don't-care)
        planes: 1,
      };

      function initState() {
        const n = state.vars;
        const size = 1 << n;
        state.values = new Array(size).fill(0);
        state.planes = n <= 4 ? 1 : n === 5 ? 2 : 4; // for 6 vars, 4 planes (AB)
      }

      function buildFromInputs() {
        const n = state.vars;
        const size = 1 << n;
        state.values = new Array(size).fill(0);
        const inputMode = document.getElementById("input-mode").value;
        if (inputMode === "truth-table") {
          const raw = document.getElementById("truth-input").value.trim();
          if (!raw) return;
          const tokens = raw.split(/[,;\s]+/).filter(Boolean);
          for (let i = 0; i < size && i < tokens.length; i++) {
            const t = tokens[i].toUpperCase();
            if (t === "1") state.values[i] = 1;
            else if (t === "X" || t === "D") state.values[i] = 2;
            else state.values[i] = 0;
          }
        } else {
          const maxIndex = size - 1;
          const termStr = document.getElementById("term-input").value || "";
          const indices = parseRangeList(termStr, maxIndex);
          const isMinterm = inputMode === "minterms";
          if (isMinterm) {
            indices.forEach((i) => (state.values[i] = 1));
          } else {
            // maxterms: indices where F=0; others default 1
            state.values.fill(1);
            indices.forEach((i) => (state.values[i] = 0));
          }
        }
      }

      // --- K-map layout ---
      function buildKMapGrid() {
        const container = document.getElementById("kmap-container");
        container.innerHTML = "";
        const n = state.vars;

        const rowBits = n <= 2 ? 1 : 2;
        const colBits = n - rowBits - (n > 4 ? (n === 5 ? 1 : 2) : 0);
        const rowSeq = graySequence(rowBits);
        const colSeq = graySequence(colBits);

        const planeBits = n > 4 ? (n === 5 ? 1 : 2) : 0;
        const planeSeq = graySequence(planeBits);

        const planes = state.planes;
        const wrapper = document.createElement("div");
        wrapper.className = "flex flex-wrap gap-4";

        for (let p = 0; p < planes; p++) {
          const planeMask = planeBits ? planeSeq[p] : 0;
          const planeDiv = document.createElement("div");
          planeDiv.className = "inline-block";

          const title = document.createElement("div");
          title.className =
            "text-xs font-medium text-gray-600 mb-1 text-center";
          if (n === 5) {
            const aVal = planeMask & 1 ? 1 : 0;
            title.textContent = `Plane A=${aVal}`;
          } else if (n === 6) {
            const aVal = (planeMask >> 1) & 1;
            const bVal = planeMask & 1;
            title.textContent = `Plane A=${aVal}, B=${bVal}`;
          } else {
            title.textContent = "";
          }
          planeDiv.appendChild(title);

          const table = document.createElement("table");
          table.className = "kmap-grid text-xs";

          const thead = document.createElement("thead");
          const headRow = document.createElement("tr");
          headRow.appendChild(document.createElement("th"));
          const colHeader = document.createElement("th");
          colHeader.colSpan = colSeq.length;
          colHeader.textContent =
            colBits > 0 ? `Columns (${colBits} bits Gray code)` : "";
          headRow.appendChild(colHeader);
          thead.appendChild(headRow);

          const labelRow = document.createElement("tr");
          const rowHeader = document.createElement("th");
          rowHeader.textContent =
            rowBits > 0 ? `Rows (${rowBits} bits Gray code)` : "";
          labelRow.appendChild(rowHeader);
          for (let c = 0; c < colSeq.length; c++) {
            const th = document.createElement("th");
            th.textContent = colBits
              ? colSeq[c].toString(2).padStart(colBits, "0")
              : "";
            labelRow.appendChild(th);
          }
          thead.appendChild(labelRow);
          table.appendChild(thead);

          const tbody = document.createElement("tbody");
          for (let r = 0; r < rowSeq.length; r++) {
            const tr = document.createElement("tr");
            const th = document.createElement("th");
            th.textContent = rowBits
              ? rowSeq[r].toString(2).padStart(rowBits, "0")
              : "";
            tr.appendChild(th);
            for (let c = 0; c < colSeq.length; c++) {
              const td = document.createElement("td");
              td.className = "kmap-cell";
              const index = computeIndexFromCoords(
                n,
                rowBits,
                colBits,
                planeBits,
                rowSeq[r],
                colSeq[c],
                planeMask
              );
              td.dataset.index = index;
              td.textContent = displayValue(state.values[index]);
              applyCellClass(td, state.values[index]);
              td.addEventListener("click", () => {
                cycleCellValue(index);
                td.textContent = displayValue(state.values[index]);
                applyCellClass(td, state.values[index]);
              });
              tr.appendChild(td);
            }
            tbody.appendChild(tr);
          }
          table.appendChild(tbody);
          planeDiv.appendChild(table);
          wrapper.appendChild(planeDiv);
        }
        container.appendChild(wrapper);
      }

      function computeIndexFromCoords(
        n,
        rowBits,
        colBits,
        planeBits,
        rowVal,
        colVal,
        planeVal
      ) {
        // Variable order: for n<=4, [rowBits][colBits]; for 5: [A][rowBits][colBits]; for 6: [A,B][rowBits][colBits]
        let index = 0;
        if (n <= 4) {
          index = (rowVal << colBits) | colVal;
        } else if (n === 5) {
          const a = planeVal & 1;
          index = (a << (rowBits + colBits)) | (rowVal << colBits) | colVal;
        } else {
          // 6 vars
          const a = (planeVal >> 1) & 1;
          const b = planeVal & 1;
          index =
            (a << (1 + rowBits + colBits)) |
            (b << (rowBits + colBits)) |
            (rowVal << colBits) |
            colVal;
        }
        return index;
      }

      function displayValue(v) {
        return v === 2 ? "X" : String(v);
      }
      function applyCellClass(td, v) {
        td.classList.remove("on", "dc");
        if (v === 1) td.classList.add("on");
        else if (v === 2) td.classList.add("dc");
      }
      function cycleCellValue(index) {
        const v = state.values[index];
        state.values[index] = (v + 1) % 3; // 0->1->2->0
      }

      // --- Simplification algorithm (SOP) ---
      function solveKMap() {
        const n = state.vars;
        const size = 1 << n;
        const ones = [];
        const dcs = [];
        for (let i = 0; i < size; i++) {
          if (state.values[i] === 1) ones.push(i);
          else if (state.values[i] === 2) dcs.push(i);
        }
        if (!ones.length) {
          return {
            sop: "F = 0",
            pos: "F = Π(0.." + (size - 1) + ")",
            groups: [],
          };
        }
        if (ones.length + dcs.length === size) {
          return {
            sop: "F = 1",
            pos: "F = 1",
            groups: [],
          };
        }

        const usedSet = new Set([...ones, ...dcs]);
        const groups = generateGroups(n, usedSet);
        const primeImplicants = filterPrimeImplicants(groups);
        const cover = selectCover(
          primeImplicants,
          ones,
          document.getElementById("grouping-mode").value
        );

        const sopExpr = buildExpressionFromGroups(cover, n, true);
        const posExpr = buildPOSFromSOP(cover, n);

        return {
          sop: "F(" + variableNames(n).join(",") + ") = " + sopExpr,
          pos: "F(" + variableNames(n).join(",") + ") = " + posExpr,
          groups: cover,
        };
      }

      function generateGroups(n, usedSet) {
        const size = 1 << n;
        const groups = [];
        const maxGroupSize = 1 << n;
        for (let gSize = maxGroupSize; gSize >= 1; gSize >>= 1) {
          for (let start = 0; start < size; start++) {
            const mask = gSize - 1;
            if ((start & mask) !== 0) continue;
            const indices = [];
            let valid = true;
            for (let k = 0; k < gSize; k++) {
              const idx = start ^ k; // not exactly K-map adjacency, but we filter later via usedSet
              if (!usedSet.has(idx)) {
                valid = false;
                break;
              }
              indices.push(idx);
            }
            if (valid) {
              const uniq = Array.from(new Set(indices)).sort((a, b) => a - b);
              const key = uniq.join(",");
              if (!groups.some((g) => g.key === key)) {
                groups.push({ indices: uniq, size: uniq.length, key });
              }
            }
          }
        }
        // Fallback: if above heuristic misses some shapes, also add singletons
        for (const idx of usedSet) {
          const key = String(idx);
          if (!groups.some((g) => g.key === key)) {
            groups.push({ indices: [idx], size: 1, key });
          }
        }
        return groups;
      }

      function filterPrimeImplicants(groups) {
        // Remove groups that are strict subsets of others
        const primes = [];
        for (let i = 0; i < groups.length; i++) {
          let isSubset = false;
          for (let j = 0; j < groups.length; j++) {
            if (i === j) continue;
            const gi = groups[i].indices;
            const gj = groups[j].indices;
            if (gi.length >= gj.length) continue;
            if (gi.every((x) => gj.includes(x))) {
              isSubset = true;
              break;
            }
          }
          if (!isSubset) primes.push(groups[i]);
        }
        return primes;
      }

      function selectCover(primes, ones, mode) {
        const onesSet = new Set(ones);
        const covered = new Set();
        const cover = [];

        // Essential prime implicants
        for (const pi of primes) {
          let countCoveredOnes = 0;
          for (const m of pi.indices) {
            if (onesSet.has(m)) countCoveredOnes++;
          }
          pi.coveredOnes = pi.indices.filter((m) => onesSet.has(m));
        }

        for (const one of ones) {
          const covering = primes.filter((pi) => pi.indices.includes(one));
          if (covering.length === 1) {
            const pi = covering[0];
            if (!cover.includes(pi)) {
              cover.push(pi);
              pi.coveredOnes.forEach((m) => covered.add(m));
            }
          }
        }

        // Greedy cover for remaining ones
        while (covered.size < onesSet.size) {
          let best = null;
          let bestScore = -1;
          for (const pi of primes) {
            if (cover.includes(pi)) continue;
            let newOnes = 0;
            for (const m of pi.coveredOnes) {
              if (!covered.has(m)) newOnes++;
            }
            if (newOnes === 0) continue;
            const score = mode === "fewest-terms" ? newOnes : newOnes / pi.size;
            if (score > bestScore) {
              bestScore = score;
              best = pi;
            }
          }
          if (!best) break;
          cover.push(best);
          best.coveredOnes.forEach((m) => covered.add(m));
        }

        return cover;
      }

      function variableNames(n) {
        const base = ["A", "B", "C", "D", "E", "F"];
        return base.slice(0, n);
      }

      function buildExpressionFromGroups(groups, n, isSOP) {
        const vars = variableNames(n);
        const terms = [];
        const notation = document.getElementById("output-notation").value;

        if (notation === "indices") {
          const allMinterms = new Set();
          groups.forEach((g) => g.indices.forEach((i) => allMinterms.add(i)));
          const sorted = Array.from(allMinterms).sort((a, b) => a - b);
          const prefix = isSOP ? "m" : "M";
          return prefix + "(" + sorted.join(",") + ")";
        }

        for (const g of groups) {
          let mask = (1 << n) - 1;
          let value = g.indices[0];
          for (const idx of g.indices) {
            mask &= ~(value ^ idx);
          }
          let term = "";
          for (let bit = n - 1; bit >= 0; bit--) {
            const bitMask = 1 << bit;
            if (mask & bitMask) {
              const v = value & bitMask ? "" : "'";
              term += vars[n - 1 - bit] + v;
            }
          }
          if (!term) term = "1";
          terms.push(term);
        }
        return terms.length ? terms.join(" + ") : "0";
      }

      function buildPOSFromSOP(groups, n) {
        // Simple dual: compute zeros and build product of sums by De Morgan
        const size = 1 << n;
        const zeros = [];
        for (let i = 0; i < size; i++) {
          if (state.values[i] === 0) zeros.push(i);
        }
        if (!zeros.length) return "1";
        const vars = variableNames(n);
        const sums = [];
        for (const idx of zeros) {
          let clause = "";
          for (let bit = n - 1; bit >= 0; bit--) {
            const bitMask = 1 << bit;
            const isOne = idx & bitMask;
            clause += isOne ? vars[n - 1 - bit] + "'" : vars[n - 1 - bit];
            if (bit > 0) clause += " + ";
          }
          sums.push("(" + clause + ")");
        }
        return sums.join(" · ");
      }

      function renderResults(res) {
        document.getElementById("sop-output").textContent = res.sop;
        document.getElementById("pos-output").textContent = res.pos;
        const ul = document.getElementById("groups-output");
        ul.innerHTML = "";
        const vars = variableNames(state.vars);
        res.groups.forEach((g, idx) => {
          const li = document.createElement("li");
          li.textContent = `Group ${idx + 1}: {${g.indices.join(", ")}}`;
          li.className = "kmap-group-" + (idx % 8);
          ul.appendChild(li);
        });

        // Color groups on map
        const cells = document.querySelectorAll(".kmap-cell");
        cells.forEach((td) => {
          for (let k = 0; k < 8; k++) td.classList.remove("kmap-group-" + k);
        });
        res.groups.forEach((g, idx) => {
          const cls = "kmap-group-" + (idx % 8);
          g.indices.forEach((i) => {
            const cell = document.querySelector(
              '.kmap-cell[data-index="' + i + '"]'
            );
            if (cell) cell.classList.add(cls);
          });
        });
      }

      // --- Event listeners ---
      document.addEventListener("DOMContentLoaded", () => {
        const varSelect = document.getElementById("variable-count");
        const inputModeSelect = document.getElementById("input-mode");

        function updateVarCount() {
          state.vars = parseInt(varSelect.value, 10);
          initState();
          buildKMapGrid();
          document.getElementById("sop-output").textContent = "";
          document.getElementById("pos-output").textContent = "";
          document.getElementById("groups-output").innerHTML = "";
        }

        varSelect.addEventListener("change", updateVarCount);
        updateVarCount();

        inputModeSelect.addEventListener("change", () => {
          const mode = inputModeSelect.value;
          document
            .getElementById("minterm-maxterm-input")
            .classList.toggle("hidden", mode === "truth-table");
          document
            .getElementById("truth-table-input")
            .classList.toggle("hidden", mode !== "truth-table");
        });

        document
          .getElementById("build-kmap-btn")
          .addEventListener("click", () => {
            buildFromInputs();
            buildKMapGrid();
          });

        document
          .getElementById("clear-all-btn")
          .addEventListener("click", () => {
            initState();
            buildKMapGrid();
            document.getElementById("term-input").value = "";
            document.getElementById("truth-input").value = "";
            document.getElementById("sop-output").textContent = "";
            document.getElementById("pos-output").textContent = "";
            document.getElementById("groups-output").innerHTML = "";
          });

        document.getElementById("solve-btn").addEventListener("click", () => {
          const res = solveKMap();
          renderResults(res);
        });
      });

      // Mobile menu script (simple)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const expanded = this.getAttribute("aria-expanded") === "true";
          this.setAttribute("aria-expanded", String(!expanded));
          menu.classList.toggle("hidden");
        });
    </script>

