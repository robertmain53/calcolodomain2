    <script>
      // Tab handling
      const tabs = {
        "tab-basic": "panel-basic",
        "tab-multi": "panel-multi",
        "tab-vehicle": "panel-vehicle",
      };
      Object.keys(tabs).forEach((tabId) => {
        document.getElementById(tabId).addEventListener("click", () => {
          // buttons
          Object.keys(tabs).forEach((id) => {
            const btn = document.getElementById(id);
            if (id === tabId) {
              btn.classList.add("tab-active");
            } else {
              btn.classList.remove("tab-active");
            }
          });
          // panels
          Object.values(tabs).forEach((pid) => {
            document.getElementById(pid).classList.add("hidden");
          });
          document.getElementById(tabs[tabId]).classList.remove("hidden");
        });
      });

      // Helper: format ratio as X:1
      function formatRatio(r) {
        if (!isFinite(r) || r <= 0) return "—";
        const rounded = Math.round(r * 1000) / 1000;
        return rounded.toFixed(3) + ":1";
      }

      // BASIC CALCULATOR
      document.getElementById("basic-calc").addEventListener("click", () => {
        const Td = parseFloat(
          document.getElementById("basic-driving-teeth").value
        );
        const To = parseFloat(
          document.getElementById("basic-driven-teeth").value
        );
        const Dd = parseFloat(
          document.getElementById("basic-driving-diam").value
        );
        const Do = parseFloat(
          document.getElementById("basic-driven-diam").value
        );
        const nIn = parseFloat(
          document.getElementById("basic-input-rpm").value
        );
        const tauIn = parseFloat(
          document.getElementById("basic-input-torque").value
        );
        const eff =
          parseFloat(document.getElementById("basic-eff").value) / 100;

        let R = null;
        if (Td > 0 && To > 0) {
          R = To / Td;
        } else if (Dd > 0 && Do > 0) {
          R = Do / Dd;
        }

        const ratioTextEl = document.getElementById("basic-ratio-text");
        const ratioNumEl = document.getElementById("basic-ratio-num");
        const speedRatioEl = document.getElementById("basic-speed-ratio");
        const outRpmEl = document.getElementById("basic-output-rpm");
        const outTorqueEl = document.getElementById("basic-output-torque");

        if (!R || !isFinite(R) || R <= 0) {
          ratioTextEl.textContent = "Please enter valid teeth or diameters.";
          ratioNumEl.textContent = "—";
          speedRatioEl.textContent = "—";
          outRpmEl.textContent = "—";
          outTorqueEl.textContent = "—";
          document.getElementById("basic-results").classList.remove("hidden");
          return;
        }

        ratioTextEl.textContent = formatRatio(R);
        ratioNumEl.textContent = R.toFixed(4);
        speedRatioEl.textContent = (1 / R).toFixed(4);

        if (nIn > 0) {
          const nOut = nIn / R;
          outRpmEl.textContent = nOut.toFixed(2) + " RPM";
        } else {
          outRpmEl.textContent = "—";
        }

        if (tauIn > 0 && eff > 0) {
          const tauOut = tauIn * R * eff;
          const unit =
            document.getElementById("basic-torque-unit").value === "lbft"
              ? "lb·ft"
              : "N·m";
          outTorqueEl.textContent = tauOut.toFixed(2) + " " + unit;
        } else {
          outTorqueEl.textContent = "—";
        }

        document.getElementById("basic-results").classList.remove("hidden");
      });

      document.getElementById("basic-clear").addEventListener("click", () => {
        [
          "basic-driving-teeth",
          "basic-driven-teeth",
          "basic-driving-diam",
          "basic-driven-diam",
          "basic-input-rpm",
          "basic-input-torque",
        ].forEach((id) => {
          document.getElementById(id).value = "";
        });
        document.getElementById("basic-eff").value = 95;
        document.getElementById("basic-results").classList.add("hidden");
      });

      // MULTI-STAGE
      const multiContainer = document.getElementById("multi-stages");
      function createStageRow(index) {
        const div = document.createElement("div");
        div.className = "border rounded p-3 bg-gray-50";
        div.dataset.index = index;
        div.innerHTML = `
      <div class="flex justify-between items-center mb-2">
        <span class="font-medium text-sm">Stage ${index + 1}</span>
        <span class="text-xs text-gray-500">Ratio = driven ÷ driving</span>
      </div>
      <div class="grid md:grid-cols-3 gap-3">
        <div>
          <label class="block text-xs font-medium mb-1">Driving teeth</label>
          <input type="number" min="0" step="1" class="w-full border rounded px-2 py-1.5 multi-driving">
        </div>
        <div>
          <label class="block text-xs font-medium mb-1">Driven teeth</label>
          <input type="number" min="0" step="1" class="w-full border rounded px-2 py-1.5 multi-driven">
        </div>
        <div>
          <label class="block text-xs font-medium mb-1">Or direct ratio</label>
          <input type="number" min="0" step="0.001" class="w-full border rounded px-2 py-1.5 multi-ratio" placeholder="e.g. 3.73">
        </div>
      </div>
    `;
        return div;
      }

      function initMultiStages() {
        multiContainer.innerHTML = "";
        for (let i = 0; i < 2; i++) {
          multiContainer.appendChild(createStageRow(i));
        }
      }
      initMultiStages();

      document
        .getElementById("multi-add-stage")
        .addEventListener("click", () => {
          const count = multiContainer.children.length;
          if (count >= 4) return;
          multiContainer.appendChild(createStageRow(count));
        });

      document
        .getElementById("multi-remove-stage")
        .addEventListener("click", () => {
          const count = multiContainer.children.length;
          if (count > 1) {
            multiContainer.removeChild(multiContainer.lastElementChild);
          }
        });

      document.getElementById("multi-calc").addEventListener("click", () => {
        const stages = Array.from(multiContainer.children);
        let Roverall = 1;
        let valid = false;

        stages.forEach((row) => {
          const Td = parseFloat(row.querySelector(".multi-driving").value);
          const To = parseFloat(row.querySelector(".multi-driven").value);
          const Rdir = parseFloat(row.querySelector(".multi-ratio").value);
          let R = null;
          if (To > 0 && Td > 0) {
            R = To / Td;
          } else if (Rdir > 0) {
            R = Rdir;
          }
          if (R && isFinite(R) && R > 0) {
            Roverall *= R;
            valid = true;
          }
        });

        const ratioTextEl = document.getElementById("multi-ratio-text");
        const ratioNumEl = document.getElementById("multi-ratio-num");
        const outRpmEl = document.getElementById("multi-output-rpm");
        const outTorqueEl = document.getElementById("multi-output-torque");

        if (!valid) {
          ratioTextEl.textContent = "Please enter at least one valid stage.";
          ratioNumEl.textContent = "—";
          outRpmEl.textContent = "—";
          outTorqueEl.textContent = "—";
          document.getElementById("multi-results").classList.remove("hidden");
          return;
        }

        ratioTextEl.textContent = formatRatio(Roverall);
        ratioNumEl.textContent = Roverall.toFixed(4);

        const nIn = parseFloat(
          document.getElementById("multi-input-rpm").value
        );
        const tauIn = parseFloat(
          document.getElementById("multi-input-torque").value
        );
        const eff =
          parseFloat(document.getElementById("multi-eff").value) / 100;

        if (nIn > 0) {
          const nOut = nIn / Roverall;
          outRpmEl.textContent = nOut.toFixed(2) + " RPM";
        } else {
          outRpmEl.textContent = "—";
        }

        if (tauIn > 0 && eff > 0) {
          const tauOut = tauIn * Roverall * eff;
          const unit =
            document.getElementById("multi-torque-unit").value === "lbft"
              ? "lb·ft"
              : "N·m";
          outTorqueEl.textContent = tauOut.toFixed(2) + " " + unit;
        } else {
          outTorqueEl.textContent = "—";
        }

        document.getElementById("multi-results").classList.remove("hidden");
      });

      document.getElementById("multi-clear").addEventListener("click", () => {
        initMultiStages();
        document.getElementById("multi-input-rpm").value = "";
        document.getElementById("multi-input-torque").value = "";
        document.getElementById("multi-eff").value = 90;
        document.getElementById("multi-results").classList.add("hidden");
      });

      // VEHICLE SPEED
      function tireDiameterToMeters(d, unit) {
        if (!(d > 0)) return null;
        if (unit === "in") {
          return d * 0.0254;
        } else {
          return d / 1000;
        }
      }

      document.getElementById("veh-calc").addEventListener("click", () => {
        const nEngine = parseFloat(
          document.getElementById("veh-engine-rpm").value
        );
        const Rgear = parseFloat(
          document.getElementById("veh-gear-ratio").value
        );
        const Rfinal = parseFloat(
          document.getElementById("veh-final-drive").value
        );
        const dTire = parseFloat(
          document.getElementById("veh-tire-diam").value
        );
        const unit = document.getElementById("veh-tire-unit").value;

        const ratioEl = document.getElementById("veh-overall-ratio");
        const wheelRpmEl = document.getElementById("veh-wheel-rpm");
        const mphEl = document.getElementById("veh-speed-mph");
        const kphEl = document.getElementById("veh-speed-kph");

        if (!(nEngine > 0 && Rgear > 0 && Rfinal > 0 && dTire > 0)) {
          ratioEl.textContent = "Please fill all fields with positive values.";
          wheelRpmEl.textContent = "—";
          mphEl.textContent = "—";
          kphEl.textContent = "—";
          document.getElementById("veh-results").classList.remove("hidden");
          return;
        }

        const Roverall = Rgear * Rfinal;
        const nWheel = nEngine / Roverall;
        const dMeters = tireDiameterToMeters(dTire, unit);
        const C = Math.PI * dMeters;
        const v_ms = (nWheel * C) / 60;
        const v_kph = v_ms * 3.6;
        const v_mph = v_ms * 2.23693629;

        ratioEl.textContent = formatRatio(Roverall);
        wheelRpmEl.textContent = nWheel.toFixed(2) + " RPM";
        mphEl.textContent = v_mph.toFixed(2) + " mph";
        kphEl.textContent = v_kph.toFixed(2) + " km/h";

        document.getElementById("veh-results").classList.remove("hidden");
      });

      document.getElementById("veh-clear").addEventListener("click", () => {
        [
          "veh-engine-rpm",
          "veh-gear-ratio",
          "veh-final-drive",
          "veh-tire-diam",
        ].forEach((id) => {
          document.getElementById(id).value = "";
        });
        document.getElementById("veh-results").classList.add("hidden");
      });

      document
        .querySelectorAll("#faq-container .faq-question")
        .forEach((btn) => {
          btn.addEventListener("click", () => {
            const answer = btn.nextElementSibling;
            const isHidden = answer.classList.contains("hidden");
            answer.classList.toggle("hidden", !isHidden);
            btn.querySelector("span:last-child").textContent = isHidden
              ? "−"
              : "+";
          });
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
