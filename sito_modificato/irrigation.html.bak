    <script>
      // Unit handling and calculator logic
      (function () {
        const unitMetricBtn = document.getElementById("unit-metric");
        const unitUsBtn = document.getElementById("unit-us");

        const etoInput = document.getElementById("eto");
        const kcInput = document.getElementById("kc");
        const rainInput = document.getElementById("rain");
        const effInput = document.getElementById("eff");
        const areaInput = document.getElementById("area");
        const seasonInput = document.getElementById("season-days");

        const etoUnitLabel = document.getElementById("eto-unit-label");
        const rainUnitLabel = document.getElementById("rain-unit-label");
        const areaUnitLabel = document.getElementById("area-unit-label");

        const calcBtn = document.getElementById("calc-btn");
        const resetBtn = document.getElementById("reset-btn");
        const errorEl = document.getElementById("calc-error");
        const resultsContainer = document.getElementById("results-container");

        const netDepthEl = document.getElementById("net-depth");
        const grossDepthEl = document.getElementById("gross-depth");
        const netDepthUnitEl = document.getElementById("net-depth-unit");
        const grossDepthUnitEl = document.getElementById("gross-depth-unit");

        const volM3DayEl = document.getElementById("vol-m3-day");
        const volLDayEl = document.getElementById("vol-l-day");
        const volAfDayEl = document.getElementById("vol-af-day");
        const volGalDayEl = document.getElementById("vol-gal-day");

        const volM3SeasonEl = document.getElementById("vol-m3-season");
        const volLSeasonEl = document.getElementById("vol-l-season");
        const volAfSeasonEl = document.getElementById("vol-af-season");
        const volGalSeasonEl = document.getElementById("vol-gal-season");

        let unitSystem = "metric"; // 'metric' or 'us'

        function setUnits(system) {
          unitSystem = system;
          if (system === "metric") {
            etoUnitLabel.textContent = "(mm/day)";
            rainUnitLabel.textContent = "(mm/day)";
            areaUnitLabel.textContent = "(ha)";
            unitMetricBtn.classList.add("bg-blue-600", "text-white");
            unitMetricBtn.classList.remove("bg-white", "text-blue-600");
            unitUsBtn.classList.remove("bg-blue-600", "text-white");
            unitUsBtn.classList.add("bg-white", "text-blue-600");
          } else {
            etoUnitLabel.textContent = "(in/day)";
            rainUnitLabel.textContent = "(in/day)";
            areaUnitLabel.textContent = "(acres)";
            unitUsBtn.classList.add("bg-blue-600", "text-white");
            unitUsBtn.classList.remove("bg-white", "text-blue-600");
            unitMetricBtn.classList.remove("bg-blue-600", "text-white");
            unitMetricBtn.classList.add("bg-white", "text-blue-600");
          }
          // Update depth unit labels
          netDepthUnitEl.textContent =
            system === "metric" ? "mm/day" : "in/day";
          grossDepthUnitEl.textContent =
            system === "metric" ? "mm/day" : "in/day";
        }

        unitMetricBtn.addEventListener("click", () => setUnits("metric"));
        unitUsBtn.addEventListener("click", () => setUnits("us"));

        function parsePositive(value) {
          const v = parseFloat(value);
          return isNaN(v) ? null : v;
        }

        function formatNumber(value, decimals) {
          if (!isFinite(value)) return "-";
          return value.toLocaleString(undefined, {
            minimumFractionDigits: decimals,
            maximumFractionDigits: decimals,
          });
        }

        function calculate() {
          errorEl.textContent = "";
          resultsContainer.classList.add("hidden");

          const eto = parsePositive(etoInput.value);
          const kc = parsePositive(kcInput.value);
          const rain = parsePositive(rainInput.value) || 0;
          const eff = parsePositive(effInput.value);
          const area = parsePositive(areaInput.value);
          const seasonDays = parsePositive(seasonInput.value) || 1;

          if (eto === null || eto < 0) {
            errorEl.textContent = "Please enter a valid reference ET (ETo).";
            return;
          }
          if (kc === null || kc <= 0) {
            errorEl.textContent = "Please enter a valid crop coefficient (Kc).";
            return;
          }
          if (eff === null || eff <= 0 || eff > 1) {
            errorEl.textContent =
              "Irrigation efficiency must be between 0 and 1.";
            return;
          }
          if (area === null || area <= 0) {
            errorEl.textContent = "Please enter a positive irrigated area.";
            return;
          }
          if (seasonDays <= 0) {
            errorEl.textContent = "Season length must be greater than zero.";
            return;
          }

          // 1) Crop ET
          let etc = eto * kc; // same units as eto

          // 2) Net irrigation depth (cannot be negative)
          let dNet = Math.max(0, etc - rain);

          // 3) Gross irrigation depth
          let dGross = dNet / eff;

          // Convert depth & area to metric base for volume
          let depthMmPerDay;
          let areaHa;

          if (unitSystem === "metric") {
            depthMmPerDay = dGross; // already mm/day
            areaHa = area; // already ha
          } else {
            // inches to mm, acres to ha
            depthMmPerDay = dGross * 25.4;
            areaHa = area * 0.40468564224;
          }

          // Volume: 1 mm over 1 ha = 10 m³
          const volM3Day = depthMmPerDay * areaHa * 10;
          const volLDay = volM3Day * 1000;
          const volAfDay = volM3Day / 1233.48184; // 1 acre-foot = 1233.48184 m³
          const volGalDay = volM3Day * 264.172052; // 1 m³ = 264.172052 US gal

          const volM3Season = volM3Day * seasonDays;
          const volLSeason = volLDay * seasonDays;
          const volAfSeason = volAfDay * seasonDays;
          const volGalSeason = volGalDay * seasonDays;

          // Update depth results
          netDepthEl.textContent = formatNumber(dNet, 2);
          grossDepthEl.textContent = formatNumber(dGross, 2);
          netDepthUnitEl.textContent =
            unitSystem === "metric" ? "mm/day" : "in/day";
          grossDepthUnitEl.textContent =
            unitSystem === "metric" ? "mm/day" : "in/day";

          // Update daily volumes
          volM3DayEl.textContent = formatNumber(volM3Day, 2);
          volLDayEl.textContent = formatNumber(volLDay, 0);
          volAfDayEl.textContent = formatNumber(volAfDay, 4);
          volGalDayEl.textContent = formatNumber(volGalDay, 0);

          // Update seasonal volumes
          volM3SeasonEl.textContent = formatNumber(volM3Season, 2);
          volLSeasonEl.textContent = formatNumber(volLSeason, 0);
          volAfSeasonEl.textContent = formatNumber(volAfSeason, 4);
          volGalSeasonEl.textContent = formatNumber(volGalSeason, 0);

          resultsContainer.classList.remove("hidden");
        }

        function reset() {
          etoInput.value = "";
          kcInput.value = "";
          rainInput.value = "";
          effInput.value = "";
          areaInput.value = "";
          seasonInput.value = "";
          errorEl.textContent = "";
          resultsContainer.classList.add("hidden");
        }

        calcBtn.addEventListener("click", calculate);
        resetBtn.addEventListener("click", reset);

        // Default units and example values
        setUnits("metric");
        etoInput.value = "5";
        kcInput.value = "0.9";
        rainInput.value = "1";
        effInput.value = "0.85";
        areaInput.value = "2";
        seasonInput.value = "120";
      })();
    </script>

