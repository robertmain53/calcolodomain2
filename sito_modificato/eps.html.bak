    <script>
      // EPS calculator logic
      (function () {
        const netIncomeEl = document.getElementById("netIncome");
        const preferredDividendsEl =
          document.getElementById("preferredDividends");
        const weightedSharesEl = document.getElementById("weightedShares");
        const includeDilutedEl = document.getElementById("includeDiluted");
        const dilutedSectionEl = document.getElementById("dilutedSection");
        const dilutiveSharesEl = document.getElementById("dilutiveShares");
        const includeExtrasEl = document.getElementById("includeExtras");
        const extrasSectionEl = document.getElementById("extrasSection");
        const previousEPSEl = document.getElementById("previousEPS");
        const sharePriceEl = document.getElementById("sharePrice");

        const basicEpsResultEl = document.getElementById("basicEpsResult");
        const dilutedResultCardEl =
          document.getElementById("dilutedResultCard");
        const dilutedEpsResultEl = document.getElementById("dilutedEpsResult");
        const growthResultCardEl = document.getElementById("growthResultCard");
        const epsGrowthResultEl = document.getElementById("epsGrowthResult");
        const peResultCardEl = document.getElementById("peResultCard");
        const peRatioResultEl = document.getElementById("peRatioResult");
        const interpretationTextEl =
          document.getElementById("interpretationText");

        const calculateBtn = document.getElementById("calculateBtn");
        const resetBtn = document.getElementById("resetBtn");

        includeDilutedEl.addEventListener("change", () => {
          dilutedSectionEl.classList.toggle(
            "hidden",
            !includeDilutedEl.checked
          );
          if (!includeDilutedEl.checked) {
            dilutedResultCardEl.classList.add("hidden");
            dilutiveSharesEl.value = "";
          }
        });

        includeExtrasEl.addEventListener("change", () => {
          extrasSectionEl.classList.toggle("hidden", !includeExtrasEl.checked);
          if (!includeExtrasEl.checked) {
            growthResultCardEl.classList.add("hidden");
            peResultCardEl.classList.add("hidden");
            previousEPSEl.value = "";
            sharePriceEl.value = "";
          }
        });

        function parseNumber(el) {
          const v = parseFloat(el.value);
          return isNaN(v) ? 0 : v;
        }

        function formatNumber(value, decimals) {
          if (!isFinite(value)) return "—";
          return value.toLocaleString(undefined, {
            minimumFractionDigits: decimals,
            maximumFractionDigits: decimals,
          });
        }

        function calculate() {
          const netIncome = parseNumber(netIncomeEl);
          const preferredDividends = parseNumber(preferredDividendsEl);
          const weightedShares = parseFloat(weightedSharesEl.value);

          if (
            !weightedShares ||
            !isFinite(weightedShares) ||
            weightedShares <= 0
          ) {
            alert(
              "Please enter a valid positive number for weighted average common shares."
            );
            return;
          }

          const earningsToCommon = netIncome - preferredDividends;
          const basicEPS = earningsToCommon / weightedShares;

          basicEpsResultEl.textContent = isFinite(basicEPS)
            ? formatNumber(basicEPS, 4)
            : "—";

          // Diluted EPS
          let dilutedEPS = null;
          if (includeDilutedEl.checked) {
            const dilutiveShares = parseFloat(dilutiveSharesEl.value);
            if (dilutiveShares && dilutiveShares > 0) {
              const totalDilutedShares = weightedShares + dilutiveShares;
              dilutedEPS = earningsToCommon / totalDilutedShares;
              dilutedEpsResultEl.textContent = formatNumber(dilutedEPS, 4);
              dilutedResultCardEl.classList.remove("hidden");
            } else {
              dilutedResultCardEl.classList.add("hidden");
            }
          } else {
            dilutedResultCardEl.classList.add("hidden");
          }

          // EPS growth
          let growthPct = null;
          if (includeExtrasEl.checked) {
            const prevEPS = parseFloat(previousEPSEl.value);
            if (prevEPS && isFinite(prevEPS) && prevEPS !== 0) {
              growthPct = ((basicEPS - prevEPS) / Math.abs(prevEPS)) * 100;
              epsGrowthResultEl.textContent = formatNumber(growthPct, 2) + " %";
              growthResultCardEl.classList.remove("hidden");
            } else {
              growthResultCardEl.classList.add("hidden");
            }

            // P/E ratio
            const sharePrice = parseFloat(sharePriceEl.value);
            if (sharePrice && isFinite(sharePrice) && basicEPS !== 0) {
              const pe = sharePrice / basicEPS;
              peRatioResultEl.textContent = formatNumber(pe, 2) + "×";
              peResultCardEl.classList.remove("hidden");
            } else {
              peResultCardEl.classList.add("hidden");
            }
          } else {
            growthResultCardEl.classList.add("hidden");
            peResultCardEl.classList.add("hidden");
          }

          // Interpretation
          let interp = "";
          if (!isFinite(basicEPS)) {
            interp =
              "Unable to compute EPS. Please check that your inputs are valid numbers.";
          } else if (basicEPS < 0) {
            interp =
              "Basic EPS is negative, which means the company reported a net loss attributable to common shareholders for this period.";
          } else {
            interp = `Basic EPS is ${formatNumber(basicEPS, 4)}. `;
            if (dilutedEPS !== null && isFinite(dilutedEPS)) {
              const diffPct = ((basicEPS - dilutedEPS) / basicEPS) * 100;
              interp += `Diluted EPS is ${formatNumber(
                dilutedEPS,
                4
              )}, about ${formatNumber(
                diffPct,
                2
              )}% lower due to potential dilution. `;
            }
            if (growthPct !== null && isFinite(growthPct)) {
              interp += `Compared with the previous period, EPS growth is ${formatNumber(
                growthPct,
                2
              )}%. `;
            }
            const sharePrice = parseFloat(sharePriceEl.value);
            if (sharePrice && isFinite(sharePrice) && basicEPS !== 0) {
              const pe = sharePrice / basicEPS;
              interp += `At a share price of $${formatNumber(
                sharePrice,
                2
              )}, the stock trades at a P/E of ${formatNumber(pe, 2)}×.`;
            } else {
              interp +=
                "Add share price and previous EPS to see growth and P/E interpretation.";
            }
          }
          interpretationTextEl.textContent = interp;
        }

        function reset() {
          netIncomeEl.value = "";
          preferredDividendsEl.value = "";
          weightedSharesEl.value = "";
          includeDilutedEl.checked = false;
          dilutedSectionEl.classList.add("hidden");
          dilutiveSharesEl.value = "";
          includeExtrasEl.checked = false;
          extrasSectionEl.classList.add("hidden");
          previousEPSEl.value = "";
          sharePriceEl.value = "";

          basicEpsResultEl.textContent = "—";
          dilutedResultCardEl.classList.add("hidden");
          growthResultCardEl.classList.add("hidden");
          peResultCardEl.classList.add("hidden");
          interpretationTextEl.textContent =
            "Enter your data and click “Calculate EPS” to see results and a short interpretation here.";
        }

        calculateBtn.addEventListener("click", calculate);
        resetBtn.addEventListener("click", reset);
      })();
    </script>

