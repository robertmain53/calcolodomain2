<script>
  // Basic state
  const state = {
    unit: 'imperial', // 'imperial' or 'metric'
    items: [],
    selectedId: null,
    nextId: 1
  };

  const unitConfig = {
    imperial: {
      lengthLabel: 'ft',
      areaLabel: 'sq ft',
      heightDefault: 8,
      boardWidthDefault: 6,
      boardWidthUnit: 'in'
    },
    metric: {
      lengthLabel: 'm',
      areaLabel: 'sq m',
      heightDefault: 2.4,
      boardWidthDefault: 0.15,
      boardWidthUnit: 'm'
    }
  };

  // Helpers
  function getUnitCfg() {
    return unitConfig[state.unit];
  }

  function recalcRoomArea() {
    const L = parseFloat(document.getElementById('room-length').value) || 0;
    const W = parseFloat(document.getElementById('room-width').value) || 0;
    const area = L * W;
    const perimeter = 2 * (L + W);

    document.getElementById('room-area').textContent = area.toFixed(1);
    document.getElementById('sum-room-length').textContent = L.toFixed(2);
    document.getElementById('sum-room-width').textContent = W.toFixed(2);
    document.getElementById('sum-room-area').textContent = area.toFixed(1);
    document.getElementById('sum-wall-perimeter').textContent = perimeter.toFixed(1);

    recalcPaintAndBoards();
  }

  function recalcPaintAndBoards() {
    const cfg = getUnitCfg();
    const perimeter = parseFloat(document.getElementById('sum-wall-perimeter').textContent) || 0;
    let height = parseFloat(document.getElementById('paint-height').value) || 0;
    const areaEl = document.getElementById('paint-area');
    const galEl = document.getElementById('paint-gallons');

    let wallArea;
    if (state.unit === 'imperial') {
      wallArea = perimeter * height;
      areaEl.textContent = wallArea.toFixed(0);
      galEl.textContent = (wallArea / 350).toFixed(1);
    } else {
      // convert m to ft for paint coverage approximation
      const perimeterFt = perimeter * 3.28084;
      const heightFt = height * 3.28084;
      wallArea = perimeterFt * heightFt;
      areaEl.textContent = (wallArea * 0.092903).toFixed(1); // back to m2
      galEl.textContent = (wallArea / 350).toFixed(1);
    }

    // Boards across width
    const W = parseFloat(document.getElementById('room-width').value) || 0;
    let boardWidth = parseFloat(document.getElementById('board-width').value) || 1;
    let boards;
    if (state.unit === 'imperial') {
      // width in ft, board width in inches
      boards = (W * 12) / boardWidth;
    } else {
      // both in meters
      boards = W / boardWidth;
    }
    document.getElementById('board-count').textContent = boards > 0 ? boards.toFixed(0) : '0';
  }

  function updateUnitLabels() {
    const cfg = getUnitCfg();
    document.getElementById('room-length-unit').textContent = cfg.lengthLabel;
    document.getElementById('room-width-unit').textContent = cfg.lengthLabel;
    document.getElementById('room-area-unit').textContent = cfg.areaLabel;
    document.getElementById('sum-room-length-unit').textContent = cfg.lengthLabel;
    document.getElementById('sum-room-width-unit').textContent = cfg.lengthLabel;
    document.getElementById('sum-room-area-unit').textContent = cfg.areaLabel;
    document.getElementById('sum-wall-perimeter-unit').textContent = cfg.lengthLabel;
    document.getElementById('paint-height-unit').textContent = cfg.lengthLabel;
    document.getElementById('paint-area-unit').textContent = cfg.areaLabel;
    document.getElementById('board-width-unit').textContent = cfg.boardWidthUnit;
    document.getElementById('prop-length-unit').textContent = cfg.lengthLabel;
    document.getElementById('prop-width-unit').textContent = cfg.lengthLabel;

    // Defaults for quick estimators
    document.getElementById('paint-height').value = cfg.heightDefault;
    document.getElementById('board-width').value = cfg.boardWidthDefault;

    recalcRoomArea();
  }

  // Canvas rendering
  function renderCanvas() {
    const canvas = document.getElementById('layout-canvas');
    canvas.innerHTML = '';
    const snap = document.getElementById('snap-grid').checked;
    const rect = canvas.getBoundingClientRect();
    const width = rect.width;
    const height = rect.height;

    const roomL = parseFloat(document.getElementById('room-length').value) || 1;
    const roomW = parseFloat(document.getElementById('room-width').value) || 1;

    const scaleX = width / roomL;
    const scaleY = height / roomW;
    const scale = Math.min(scaleX, scaleY);

    state.items.forEach(item => {
      const el = document.createElement('div');
      el.className = 'absolute bg-blue-100 border border-blue-400 rounded-sm text-[10px] text-blue-900 flex items-center justify-center handle';
      el.dataset.id = item.id;

      const pxW = item.length * scale;
      const pxH = item.width * scale;

      el.style.width = pxW + 'px';
      el.style.height = pxH + 'px';
      el.style.left = (item.x * scale) + 'px';
      el.style.top = (item.y * scale) + 'px';
      el.style.transform = `rotate(${item.rotation || 0}deg)`;
      el.style.transformOrigin = 'center center';

      if (item.type === 'wall') {
        el.style.backgroundColor = '#bfdbfe';
      } else if (item.type === 'zone') {
        el.style.backgroundColor = '#dcfce7';
        el.style.borderColor = '#16a34a';
        el.style.color = '#166534';
      } else if (item.type === 'cabinet') {
        el.style.backgroundColor = '#fef3c7';
        el.style.borderColor = '#f59e0b';
        el.style.color = '#92400e';
      } else if (item.type === 'clearance') {
        el.style.backgroundColor = 'rgba(248, 250, 252, 0.6)';
        el.style.borderStyle = 'dashed';
        el.style.borderColor = '#9ca3af';
        el.style.color = '#4b5563';
      }

      if (state.selectedId === item.id) {
        el.classList.add('canvas-item-selected');
      }

      el.textContent = item.label || item.type;

      // Dragging
      let dragStart = null;
      el.addEventListener('mousedown', (e) => {
        e.preventDefault();
        selectItem(item.id);
        const startX = e.clientX;
        const startY = e.clientY;
        dragStart = { startX, startY, origX: item.x, origY: item.y };

        function onMove(ev) {
          if (!dragStart) return;
          const dx = (ev.clientX - dragStart.startX) / scale;
          const dy = (ev.clientY - dragStart.startY) / scale;
          let newX = dragStart.origX + dx;
          let newY = dragStart.origY + dy;
          if (snap) {
            const grid = 0.5; // 0.5 unit grid
            newX = Math.round(newX / grid) * grid;
            newY = Math.round(newY / grid) * grid;
          }
          item.x = Math.max(0, newX);
          item.y = Math.max(0, newY);
          renderCanvas();
          renderMaterialsTable();
        }

        function onUp() {
          dragStart = null;
          document.removeEventListener('mousemove', onMove);
          document.removeEventListener('mouseup', onUp);
        }

        document.addEventListener('mousemove', onMove);
        document.addEventListener('mouseup', onUp);
      });

      el.addEventListener('click', (e) => {
        e.stopPropagation();
        selectItem(item.id);
      });

      canvas.appendChild(el);
    });
  }

  function addItem(type) {
    const roomL = parseFloat(document.getElementById('room-length').value) || 10;
    const roomW = parseFloat(document.getElementById('room-width').value) || 10;
    let length = roomL / 4;
    let width = roomW / 8;

    if (type === 'wall') {
      width = roomW / 40;
    } else if (type === 'clearance') {
      length = roomL / 6;
      width = roomW / 6;
    }

    const item = {
      id: state.nextId++,
      type,
      label: type.charAt(0).toUpperCase() + type.slice(1),
      length,
      width,
      x: (roomL - length) / 2,
      y: (roomW - width) / 2,
      rotation: 0
    };
    state.items.push(item);
    state.selectedId = item.id;
    renderCanvas();
    renderSelectionPanel();
    renderMaterialsTable();
  }

  function selectItem(id) {
    state.selectedId = id;
    renderCanvas();
    renderSelectionPanel();
  }

  function getSelectedItem() {
    return state.items.find(i => i.id === state.selectedId) || null;
  }

  function renderSelectionPanel() {
    const panel = document.getElementById('selection-panel');
    const noSel = document.getElementById('no-selection');
    const item = getSelectedItem();
    if (!item) {
      panel.classList.add('hidden');
      noSel.classList.remove('hidden');
      return;
    }
    panel.classList.remove('hidden');
    noSel.classList.add('hidden');

    document.getElementById('prop-label').value = item.label || '';
    document.getElementById('prop-length').value = item.length.toFixed(2);
    document.getElementById('prop-width').value = item.width.toFixed(2);
    document.getElementById('prop-rotation').value = item.rotation || 0;
  }

  function renderMaterialsTable() {
    const tbody = document.getElementById('materials-table-body');
    tbody.innerHTML = '';
    const cfg = getUnitCfg();

    state.items.forEach(item => {
      const tr = document.createElement('tr');
      tr.className = 'border-t border-gray-100';

      const area = item.length * item.width;

      tr.innerHTML = `
        <td class="px-3 py-1 text-left">${item.label || item.type}</td>
        <td class="px-3 py-1 text-right">${item.length.toFixed(2)} ${cfg.lengthLabel}</td>
        <td class="px-3 py-1 text-right">${item.width.toFixed(2)} ${cfg.lengthLabel}</td>
        <td class="px-3 py-1 text-right">${area.toFixed(2)} ${cfg.areaLabel}</td>
        <td class="px-3 py-1 text-left capitalize">${item.type}</td>
      `;
      tbody.appendChild(tr);
    });
  }

  function resetLayout() {
    state.items = [];
    state.selectedId = null;
    renderCanvas();
    renderSelectionPanel();
    renderMaterialsTable();
  }

  // Event wiring
  document.addEventListener('DOMContentLoaded', () => {
    // Tabs
    const tabLayout = document.getElementById('tab-layout');
    const tabMaterials = document.getElementById('tab-materials');
    const tabChecklist = document.getElementById('tab-checklist');
    const panelLayout = document.getElementById('panel-layout');
    const panelMaterials = document.getElementById('panel-materials');
    const panelChecklist = document.getElementById('panel-checklist');

    function activateTab(tab) {
      [tabLayout, tabMaterials, tabChecklist].forEach(t => {
        t.classList.remove('border-blue-600', 'text-blue-600');
        t.classList.add('border-transparent', 'text-gray-600');
      });
      [panelLayout, panelMaterials, panelChecklist].forEach(p => p.classList.add('hidden'));

      if (tab === 'layout') {
        tabLayout.classList.add('border-blue-600', 'text-blue-600');
        panelLayout.classList.remove('hidden');
      } else if (tab === 'materials') {
        tabMaterials.classList.add('border-blue-600', 'text-blue-600');
        panelMaterials.classList.remove('hidden');
      } else {
        tabChecklist.classList.add('border-blue-600', 'text-blue-600');
        panelChecklist.classList.remove('hidden');
      }
    }

    tabLayout.addEventListener('click', () => activateTab('layout'));
    tabMaterials.addEventListener('click', () => {
      activateTab('materials');
      renderMaterialsTable();
    });
    tabChecklist.addEventListener('click', () => activateTab('checklist'));

    // Unit system
    document.getElementById('unit-system').addEventListener('change', (e) => {
      state.unit = e.target.value;
      updateUnitLabels();
    });

    // Room dimensions
    document.getElementById('room-length').addEventListener('input', () => {
      recalcRoomArea();
      renderCanvas();
    });
    document.getElementById('room-width').addEventListener('input', () => {
      recalcRoomArea();
      renderCanvas();
    });

    // Snap toggle
    document.getElementById('snap-grid').addEventListener('change', () => {
      renderCanvas();
    });

    // Palette buttons
    document.querySelectorAll('button[data-type]').forEach(btn => {
      btn.addEventListener('click', () => {
        addItem(btn.getAttribute('data-type'));
      });
    });

    // Canvas click to deselect
    document.getElementById('layout-canvas').addEventListener('click', () => {
      state.selectedId = null;
      renderCanvas();
      renderSelectionPanel();
    });

    // Selection panel inputs
    document.getElementById('prop-label').addEventListener('input', (e) => {
      const item = getSelectedItem();
      if (!item) return;
      item.label = e.target.value;
      renderCanvas();
      renderMaterialsTable();
    });
    document.getElementById('prop-length').addEventListener('input', (e) => {
      const item = getSelectedItem();
      if (!item) return;
      const v = parseFloat(e.target.value);
      if (!isNaN(v) && v >= 0) {
        item.length = v;
        renderCanvas();
        renderMaterialsTable();
      }
    });
    document.getElementById('prop-width').addEventListener('input', (e) => {
      const item = getSelectedItem();
      if (!item) return;
      const v = parseFloat(e.target.value);
      if (!isNaN(v) && v >= 0) {
        item.width = v;
        renderCanvas();
        renderMaterialsTable();
      }
    });
    document.getElementById('prop-rotation').addEventListener('input', (e) => {
      const item = getSelectedItem();
      if (!item) return;
      const v = parseFloat(e.target.value);
      if (!isNaN(v)) {
        item.rotation = v;
        renderCanvas();
      }
    });

    // Duplicate & delete
    document.getElementById('btn-duplicate').addEventListener('click', () => {
      const item = getSelectedItem();
      if (!item) return;
      const copy = { ...item, id: state.nextId++, x: item.x + item.length * 0.1, y: item.y + item.width * 0.1 };
      state.items.push(copy);
      state.selectedId = copy.id;
      renderCanvas();
      renderSelectionPanel();
      renderMaterialsTable();
    });

    document.getElementById('btn-delete').addEventListener('click', () => {
      const item = getSelectedItem();
      if (!item) return;
      state.items = state.items.filter(i => i.id !== item.id);
      state.selectedId = null;
      renderCanvas();
      renderSelectionPanel();
      renderMaterialsTable();
    });

    // Reset layout
    document.getElementById('btn-reset-layout').addEventListener('click', resetLayout);

    // Quick estimators
    document.getElementById('paint-height').addEventListener('input', recalcPaintAndBoards);
    document.getElementById('board-width').addEventListener('input', recalcPaintAndBoards);

    // FAQ toggles (simple, in addition to global script_faq)
    document.querySelectorAll('#faq-container .faq-question').forEach(btn => {
      btn.addEventListener('click', () => {
        const answer = btn.parentElement.querySelector('.faq-answer');
        const icon = btn.querySelector('span:last-child');
        const isHidden = answer.classList.contains('hidden');
        answer.classList.toggle('hidden', !isHidden);
        icon.textContent = isHidden ? '−' : '+';
      });
    });

    // Initial
    updateUnitLabels();
    recalcRoomArea();
    renderCanvas();
    renderMaterialsTable();
  });
</script>

