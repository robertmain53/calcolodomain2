    <script>
      (function () {
        const SQRT3 = Math.sqrt(3);

        const modeButtons = {
          ampsToWatts: document.getElementById("mode-amps-to-watts"),
          wattsToAmps: document.getElementById("mode-watts-to-amps"),
        };
        const sysButtons = document.querySelectorAll(".sys-btn");
        const presetButtons = document.querySelectorAll(".preset-btn");

        const ampsGroup = document.getElementById("input-amps-group");
        const wattsGroup = document.getElementById("input-watts-group");
        const ampsInput = document.getElementById("amps-input");
        const wattsInput = document.getElementById("watts-input");
        const voltsInput = document.getElementById("volts-input");
        const pfInput = document.getElementById("pf-input");
        const pfGroup = document.getElementById("pf-group");
        const voltageHelp = document.getElementById("voltage-help");

        const calcBtn = document.getElementById("calc-btn");
        const clearBtn = document.getElementById("clear-btn");
        const errorMsg = document.getElementById("error-msg");
        const resultsCard = document.getElementById("results-card");
        const resultsMain = document.getElementById("results-main");
        const resultsSecondary = document.getElementById("results-secondary");

        let mode = "ampsToWatts"; // or 'wattsToAmps'
        let system = "dc"; // 'dc', 'ac1', 'ac3'

        function setMode(newMode) {
          mode = newMode;
          // Toggle button styles
          Object.keys(modeButtons).forEach((key) => {
            if (key === newMode) {
              modeButtons[key].classList.add("toggle-btn-active");
              modeButtons[key].setAttribute("aria-selected", "true");
            } else {
              modeButtons[key].classList.remove("toggle-btn-active");
              modeButtons[key].setAttribute("aria-selected", "false");
            }
          });
          // Toggle input visibility
          if (mode === "ampsToWatts") {
            ampsGroup.classList.remove("hidden");
            wattsGroup.classList.add("hidden");
          } else {
            ampsGroup.classList.add("hidden");
            wattsGroup.classList.remove("hidden");
          }
          clearResults();
        }

        function setSystem(newSystem) {
          system = newSystem;
          sysButtons.forEach((btn) => {
            if (btn.dataset.system === newSystem) {
              btn.classList.add("toggle-btn-active");
            } else {
              btn.classList.remove("toggle-btn-active");
            }
          });

          if (system === "dc") {
            pfGroup.classList.add("opacity-60");
            pfInput.placeholder = "1.0";
            voltageHelp.textContent = "DC voltage.";
          } else if (system === "ac1") {
            pfGroup.classList.remove("opacity-60");
            pfInput.placeholder = "e.g. 0.8";
            voltageHelp.textContent = "Single‑phase RMS voltage.";
          } else {
            pfGroup.classList.remove("opacity-60");
            pfInput.placeholder = "e.g. 0.9";
            voltageHelp.textContent = "Line‑to‑line voltage for 3‑phase.";
          }
          clearResults();
        }

        function showError(msg) {
          errorMsg.textContent = msg;
          errorMsg.classList.remove("hidden");
          resultsCard.classList.add("hidden");
        }

        function clearResults() {
          errorMsg.classList.add("hidden");
          resultsCard.classList.add("hidden");
          resultsMain.textContent = "";
          resultsSecondary.innerHTML = "";
        }

        function parsePositive(value) {
          const num = parseFloat(value);
          return isFinite(num) && num > 0 ? num : null;
        }

        function calculate() {
          clearResults();

          const V = parsePositive(voltsInput.value);
          if (!V) {
            showError("Please enter a valid positive voltage.");
            return;
          }

          let PF;
          if (system === "dc") {
            PF = 1;
          } else {
            PF = pfInput.value === "" ? null : parseFloat(pfInput.value);
            if (PF === null || !isFinite(PF) || PF <= 0 || PF > 1) {
              showError("Please enter a valid power factor between 0 and 1.");
              return;
            }
          }

          let A = null;
          let W = null;

          if (mode === "ampsToWatts") {
            A = parsePositive(ampsInput.value);
            if (!A) {
              showError("Please enter a valid positive current in amps.");
              return;
            }
            if (system === "dc" || PF === 1) {
              W = V * A;
            } else if (system === "ac1") {
              W = V * A * PF;
            } else {
              W = SQRT3 * V * A * PF;
            }
          } else {
            W = parsePositive(wattsInput.value);
            if (!W) {
              showError("Please enter a valid positive power in watts.");
              return;
            }
            if (system === "dc" || PF === 1) {
              A = W / V;
            } else if (system === "ac1") {
              A = W / (V * PF);
            } else {
              A = W / (SQRT3 * V * PF);
            }
          }

          if (!isFinite(W) || !isFinite(A)) {
            showError("Calculation error. Please check your inputs.");
            return;
          }

          const kW = W / 1000;
          const kVA = system === "dc" ? kW : kW / PF;
          const VA = kVA * 1000;

          resultsMain.textContent =
            mode === "ampsToWatts"
              ? `${A.toFixed(3)} A ≈ ${W.toFixed(1)} W`
              : `${W.toFixed(1)} W ≈ ${A.toFixed(3)} A`;

          const lines = [];
          lines.push(`Real power: ${W.toFixed(1)} W (${kW.toFixed(3)} kW)`);
          if (system !== "dc") {
            lines.push(
              `Apparent power: ${VA.toFixed(1)} VA (${kVA.toFixed(3)} kVA)`
            );
            lines.push(`Power factor: ${PF.toFixed(3)}`);
          }
          lines.push(`Voltage: ${V.toFixed(1)} V`);
          lines.push(`Current: ${A.toFixed(3)} A`);

          resultsSecondary.innerHTML = lines
            .map((line) => `<div>${line}</div>`)
            .join("");

          resultsCard.classList.remove("hidden");
        }

        function clearAll() {
          ampsInput.value = "";
          wattsInput.value = "";
          voltsInput.value = "";
          pfInput.value = "";
          clearResults();
        }

        // Event listeners
        modeButtons.ampsToWatts.addEventListener("click", () =>
          setMode("ampsToWatts")
        );
        modeButtons.wattsToAmps.addEventListener("click", () =>
          setMode("wattsToAmps")
        );

        sysButtons.forEach((btn) => {
          btn.addEventListener("click", () => setSystem(btn.dataset.system));
        });

        presetButtons.forEach((btn) => {
          btn.addEventListener("click", () => {
            const v = btn.dataset.volts;
            const pf = btn.dataset.pf;
            if (v) voltsInput.value = v;
            if (pf) pfInput.value = pf;
            clearResults();
          });
        });

        calcBtn.addEventListener("click", calculate);
        clearBtn.addEventListener("click", clearAll);

        // Enter key triggers calculate
        [ampsInput, wattsInput, voltsInput, pfInput].forEach((input) => {
          input.addEventListener("keydown", (e) => {
            if (e.key === "Enter") {
              e.preventDefault();
              calculate();
            }
          });
        });

        // Initialize defaults
        setMode("ampsToWatts");
        setSystem("dc");
      })();
    </script>

