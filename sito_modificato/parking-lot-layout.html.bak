    <script>
      // Core calculation logic for parking lot layout
      (function () {
        const unitSystemEl = document.getElementById("unitSystem");
        const layoutAngleEl = document.getElementById("layoutAngle");
        const moduleTypeEl = document.getElementById("moduleType");
        const stallPresetEl = document.getElementById("stallPreset");

        const stallWidthEl = document.getElementById("stallWidth");
        const stallDepthEl = document.getElementById("stallDepth");
        const aisleWidthEl = document.getElementById("aisleWidth");
        const lotLengthEl = document.getElementById("lotLength");
        const lotDepthEl = document.getElementById("lotDepth");

        const stallWidthUnitEl = document.getElementById("stallWidthUnit");
        const stallDepthUnitEl = document.getElementById("stallDepthUnit");
        const aisleWidthUnitEl = document.getElementById("aisleWidthUnit");
        const lotLengthUnitEl = document.getElementById("lotLengthUnit");
        const lotDepthUnitEl = document.getElementById("lotDepthUnit");

        const resultStallsEl = document.getElementById("resultStalls");
        const resultAreaEl = document.getElementById("resultArea");
        const resultEfficiencyEl = document.getElementById("resultEfficiency");
        const resultEffectiveWidthEl = document.getElementById(
          "resultEffectiveWidth"
        );
        const resultStallsPerRowEl =
          document.getElementById("resultStallsPerRow");
        const resultRowsEl = document.getElementById("resultRows");
        const resultModuleDepthEl =
          document.getElementById("resultModuleDepth");
        const dimensionTableBodyEl =
          document.getElementById("dimensionTableBody");

        const calculateBtn = document.getElementById("calculateBtn");
        const resetBtn = document.getElementById("resetBtn");

        const DEG_TO_RAD = Math.PI / 180;

        const presets = {
          imperial: {
            standard: {
              width: 9,
              depth: 18,
              aisle90: 24,
              aisle60: 18,
              aisle45: 16,
            },
            compact: {
              width: 8.5,
              depth: 16,
              aisle90: 22,
              aisle60: 16,
              aisle45: 14,
            },
            accessible: {
              width: 8,
              depth: 18,
              aisle90: 24,
              aisle60: 18,
              aisle45: 16,
              accessAisle: 5,
            },
          },
          metric: {
            standard: {
              width: 2.7,
              depth: 5.5,
              aisle90: 7.0,
              aisle60: 5.5,
              aisle45: 4.5,
            },
            compact: {
              width: 2.5,
              depth: 5.0,
              aisle90: 6.5,
              aisle60: 5.0,
              aisle45: 4.0,
            },
            accessible: {
              width: 2.4,
              depth: 5.5,
              aisle90: 7.0,
              aisle60: 5.5,
              aisle45: 4.5,
              accessAisle: 1.5,
            },
          },
        };

        function setUnitLabels() {
          const isMetric = unitSystemEl.value === "metric";
          const unit = isMetric ? "m" : "ft";
          stallWidthUnitEl.textContent = unit;
          stallDepthUnitEl.textContent = unit;
          aisleWidthUnitEl.textContent = unit;
          lotLengthUnitEl.textContent = unit;
          lotDepthUnitEl.textContent = unit;
        }

        function applyStallPreset() {
          const system = unitSystemEl.value;
          const presetKey = stallPresetEl.value;
          const angle = parseInt(layoutAngleEl.value, 10);
          const p = presets[system][presetKey];

          if (!p) return;

          let width = p.width;
          if (presetKey === "accessible" && p.accessAisle) {
            width = p.width + p.accessAisle;
          }

          stallWidthEl.value = width.toFixed(2);
          stallDepthEl.value = p.depth.toFixed(2);

          let aisle;
          if (angle === 90) aisle = p.aisle90;
          else if (angle === 60) aisle = p.aisle60;
          else aisle = p.aisle45;

          aisleWidthEl.value = aisle.toFixed(2);
        }

        function populateDimensionTable() {
          const angle = parseInt(layoutAngleEl.value, 10);
          const system = unitSystemEl.value;
          const isMetric = system === "metric";
          const unit = isMetric ? "m" : "ft";

          const rows = [
            {
              angle: 90,
              depth: isMetric ? 5.5 : 18,
              aisle: isMetric ? 7.0 : 24,
            },
            {
              angle: 60,
              depth: isMetric ? 5.5 : 18,
              aisle: isMetric ? 5.5 : 18,
            },
            {
              angle: 45,
              depth: isMetric ? 5.5 : 18,
              aisle: isMetric ? 4.5 : 16,
            },
          ];

          const moduleType = moduleTypeEl.value;
          dimensionTableBodyEl.innerHTML = "";
          rows.forEach((r) => {
            const moduleDepth =
              moduleType === "double"
                ? 2 * r.depth + r.aisle
                : r.depth + r.aisle;
            const tr = document.createElement("tr");
            if (r.angle === angle) {
              tr.className = "bg-blue-50";
            }
            tr.innerHTML = `
          <td class="border border-gray-200 px-2 py-1">${r.angle}°</td>
          <td class="border border-gray-200 px-2 py-1 text-right">${r.depth.toFixed(
            1
          )} ${unit}</td>
          <td class="border border-gray-200 px-2 py-1 text-right">${r.aisle.toFixed(
            1
          )} ${unit}</td>
          <td class="border border-gray-200 px-2 py-1 text-right">${moduleDepth.toFixed(
            1
          )} ${unit}</td>
        `;
            dimensionTableBodyEl.appendChild(tr);
          });
        }

        function parsePositive(value) {
          const v = parseFloat(value);
          return isFinite(v) && v > 0 ? v : null;
        }

        function formatNumber(value, decimals = 2) {
          if (!isFinite(value)) return "–";
          return value.toLocaleString(undefined, {
            minimumFractionDigits: decimals,
            maximumFractionDigits: decimals,
          });
        }

        function calculateLayout() {
          const angleDeg = parseInt(layoutAngleEl.value, 10);
          const theta = angleDeg * DEG_TO_RAD;

          const W = parsePositive(stallWidthEl.value);
          const D = parsePositive(stallDepthEl.value);
          const A = parsePositive(aisleWidthEl.value);
          const L = parsePositive(lotLengthEl.value);
          const H = parsePositive(lotDepthEl.value);

          if (!W || !D || !A || !L || !H) {
            alert(
              "Please enter positive values for stall width, stall depth, aisle width, lot length, and lot depth."
            );
            return;
          }

          const sinTheta = Math.sin(theta) || 1;
          const W_eff = W / sinTheta;

          const stallsPerRow = Math.floor(L / W_eff);
          if (stallsPerRow <= 0) {
            alert(
              "Lot length is too small for even a single stall at this angle and width."
            );
            return;
          }

          const moduleType = moduleTypeEl.value;
          const moduleDepth = moduleType === "double" ? 2 * D + A : D + A;
          const modules = Math.floor(H / moduleDepth);
          if (modules <= 0) {
            alert(
              "Lot depth is too small for a single parking module with the current stall and aisle dimensions."
            );
            return;
          }

          const rowsPerModule = moduleType === "double" ? 2 : 1;
          const totalRows = modules * rowsPerModule;
          const totalStalls = stallsPerRow * totalRows;

          const area = L * H;
          const isMetric = unitSystemEl.value === "metric";
          const areaUnit = isMetric ? "m²" : "ft²";
          const efficiencyFactor = isMetric ? 100 : 1000;
          const efficiency = (totalStalls / area) * efficiencyFactor;

          resultStallsEl.textContent = totalStalls.toLocaleString();
          resultAreaEl.textContent = `${formatNumber(area, 1)} ${areaUnit}`;
          resultEfficiencyEl.textContent = isFinite(efficiency)
            ? `${formatNumber(efficiency, 1)} stalls / ${
                isMetric ? "100 m²" : "1,000 ft²"
              }`
            : "–";

          const unit = isMetric ? "m" : "ft";
          resultEffectiveWidthEl.textContent = `${formatNumber(
            W_eff,
            2
          )} ${unit}`;
          resultStallsPerRowEl.textContent = stallsPerRow.toString();
          resultRowsEl.textContent = totalRows.toString();
          resultModuleDepthEl.textContent = `${formatNumber(
            moduleDepth,
            2
          )} ${unit}`;

          populateDimensionTable();
        }

        function resetForm() {
          unitSystemEl.value = "imperial";
          layoutAngleEl.value = "90";
          moduleTypeEl.value = "double";
          stallPresetEl.value = "standard";
          setUnitLabels();
          applyStallPreset();
          lotLengthEl.value = "";
          lotDepthEl.value = "";
          resultStallsEl.textContent = "–";
          resultAreaEl.textContent = "–";
          resultEfficiencyEl.textContent = "–";
          resultEffectiveWidthEl.textContent = "–";
          resultStallsPerRowEl.textContent = "–";
          resultRowsEl.textContent = "–";
          resultModuleDepthEl.textContent = "–";
          populateDimensionTable();
        }

        unitSystemEl.addEventListener("change", () => {
          setUnitLabels();
          applyStallPreset();
          populateDimensionTable();
        });

        layoutAngleEl.addEventListener("change", () => {
          if (stallPresetEl.value !== "custom") {
            applyStallPreset();
          }
          populateDimensionTable();
        });

        moduleTypeEl.addEventListener("change", populateDimensionTable);
        stallPresetEl.addEventListener("change", () => {
          if (stallPresetEl.value === "custom") return;
          applyStallPreset();
          populateDimensionTable();
        });

        calculateBtn.addEventListener("click", calculateLayout);
        resetBtn.addEventListener("click", resetForm);

        // Initialize defaults
        setUnitLabels();
        applyStallPreset();
        populateDimensionTable();
      })();
    </script>

