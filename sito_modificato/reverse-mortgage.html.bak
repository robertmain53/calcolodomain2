    <script>
      // Utility formatting
      function fmt(n) {
        if (!isFinite(n)) return "-";
        return n.toLocaleString("en-US", {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 0,
        });
      }
      function fmtSmall(n) {
        if (!isFinite(n)) return "-";
        return n.toLocaleString("en-US", {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 0,
        });
      }

      function calculateReverseMortgage() {
        const homeValue = parseFloat(
          document.getElementById("homeValue").value
        );
        const existingMortgage = parseFloat(
          document.getElementById("existingMortgage").value
        );
        const age = parseFloat(document.getElementById("age").value);
        const interestRate =
          parseFloat(document.getElementById("interestRate").value) / 100;
        const upfrontCosts = parseFloat(
          document.getElementById("upfrontCosts").value
        );
        const annualCosts = parseFloat(
          document.getElementById("annualCosts").value
        );
        const homeGrowth =
          parseFloat(document.getElementById("homeGrowth").value) / 100;
        const years = parseInt(document.getElementById("years").value, 10);
        const payoutType =
          document.querySelector('input[name="payoutType"]:checked')?.value ||
          "lump";
        const errorMsg = document.getElementById("errorMsg");
        errorMsg.textContent = "";

        if (isNaN(homeValue) || homeValue <= 0) {
          errorMsg.textContent = "Enter a valid home value.";
          return;
        }
        if (isNaN(age) || age < 55 || age > 100) {
          errorMsg.textContent = "Enter a realistic age between 55 and 100.";
          return;
        }
        if (isNaN(interestRate) || interestRate <= 0 || interestRate > 0.2) {
          errorMsg.textContent = "Enter an interest rate between 2% and 20%.";
          return;
        }
        if (isNaN(years) || years < 1 || years > 40) {
          errorMsg.textContent = "Projection years must be between 1 and 40.";
          return;
        }

        // Approximate LTV factor based on age (simplified)
        let ltv = 0.2 + 0.01 * (age - 62);
        if (ltv < 0.2) ltv = 0.2;
        if (ltv > 0.6) ltv = 0.6;

        const principalLimit = homeValue * ltv;
        const netProceeds = principalLimit - existingMortgage - upfrontCosts;

        // Initial balance: existing mortgage + upfront costs + (lump sum or 0)
        let initialDraw = Math.max(0, netProceeds);
        let initialBalance;
        let annualDraw = 0;

        if (payoutType === "lump") {
          initialBalance =
            existingMortgage + upfrontCosts + Math.max(0, netProceeds);
        } else {
          // Line of credit modeled as equal annual draws over the period
          annualDraw = netProceeds > 0 ? netProceeds / years : 0;
          initialBalance = existingMortgage + upfrontCosts;
        }

        // Projection
        let balance = initialBalance;
        let value = homeValue;
        const timelineBody = document.getElementById("timelineBody");
        timelineBody.innerHTML = "";

        for (let t = 0; t <= years; t++) {
          const currentAge = age + t;
          let drawThisYear = 0;
          let interestFees = 0;

          if (t === 0) {
            // At closing: initial draw for lump sum
            drawThisYear = payoutType === "lump" ? initialDraw : 0;
            interestFees = 0;
          } else {
            // Grow balance by interest
            const balanceBefore = balance;
            balance = balance * (1 + interestRate) + annualCosts;

            // Add line-of-credit draw
            if (payoutType === "line" && annualDraw > 0) {
              balance += annualDraw;
              drawThisYear = annualDraw;
            }

            interestFees = balance - balanceBefore - drawThisYear;
            // Grow home value
            value = value * (1 + homeGrowth);
          }

          const equity = Math.max(0, value - balance);

          const row = document.createElement("tr");
          row.className = t % 2 === 0 ? "bg-white" : "bg-gray-50";
          row.innerHTML = `
        <td class="px-3 py-2 text-left">${t}</td>
        <td class="px-3 py-2 text-right">${Math.round(currentAge)}</td>
        <td class="px-3 py-2 text-right">${fmtSmall(value)}</td>
        <td class="px-3 py-2 text-right">${fmtSmall(balance)}</td>
        <td class="px-3 py-2 text-right">${fmtSmall(equity)}</td>
        <td class="px-3 py-2 text-right">${
          drawThisYear > 0 ? fmtSmall(drawThisYear) : "-"
        }</td>
        <td class="px-3 py-2 text-right">${
          t === 0 ? "-" : fmtSmall(interestFees)
        }</td>
      `;
          timelineBody.appendChild(row);
        }

        const endingRow = timelineBody.lastElementChild;
        const endingBalanceText = endingRow
          ? endingRow.children[3].textContent
          : fmt(0);
        const endingHomeValueText = endingRow
          ? endingRow.children[2].textContent
          : fmt(0);
        const endingEquityText = endingRow
          ? endingRow.children[4].textContent
          : fmt(0);

        document.getElementById("principalLimit").textContent =
          fmt(principalLimit);
        document.getElementById("netProceeds").textContent =
          netProceeds > 0 ? fmt(netProceeds) : "$0";
        document.getElementById("endingBalance").textContent =
          endingBalanceText;
        document.getElementById("endingHomeValue").textContent =
          endingHomeValueText;
        document.getElementById("endingEquity").textContent = endingEquityText;
      }

      document
        .getElementById("calcBtn")
        .addEventListener("click", calculateReverseMortgage);

      document.getElementById("resetBtn").addEventListener("click", () => {
        document.getElementById("homeValue").value = 500000;
        document.getElementById("existingMortgage").value = 0;
        document.getElementById("age").value = 72;
        document.getElementById("interestRate").value = 6.0;
        document.getElementById("upfrontCosts").value = 12000;
        document.getElementById("annualCosts").value = 600;
        document.getElementById("homeGrowth").value = 2.5;
        document.getElementById("years").value = 20;
        document.querySelector(
          'input[name="payoutType"][value="lump"]'
        ).checked = true;
        document.getElementById("errorMsg").textContent = "";
        document.getElementById("timelineBody").innerHTML =
          '<tr><td colspan="7" class="px-3 py-4 text-center text-gray-500">Run the calculator to see a projection.</td></tr>';
        document.getElementById("principalLimit").textContent = "$0";
        document.getElementById("netProceeds").textContent = "$0";
        document.getElementById("endingBalance").textContent = "$0";
        document.getElementById("endingHomeValue").textContent = "$0";
        document.getElementById("endingEquity").textContent = "$0";
      });

      // Auto-run once on load for initial example
      window.addEventListener("load", calculateReverseMortgage);
    </script>

    <script>
      window.MathJax = {
