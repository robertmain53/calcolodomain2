  <script>
    (function() {
      const poiseInput = document.getElementById('poiseInput');
      const pasInput = document.getElementById('pasInput');
      const cpInput = document.getElementById('cpInput');
      const tableBody = document.getElementById('viscTableBody');

      const P_TO_PA_S = 0.1;      // 1 P = 0.1 Pa·s
      const CP_TO_PA_S = 0.001;   // 1 cP = 0.001 Pa·s

      let updating = false;

      function round(val, decimals) {
        if (!isFinite(val)) return '';
        const f = Math.pow(10, decimals);
        return Math.round(val * f) / f;
      }

      function updateFrom(source) {
        if (updating) return;
        updating = true;

        let P, PaS, cP;

        if (source === 'poise') {
          P = parseFloat(poiseInput.value);
          if (isNaN(P)) {
            pasInput.value = cpInput.value = '';
            updating = false;
            return;
          }
          PaS = P * P_TO_PA_S;
          cP = P * 100;
        } else if (source === 'pas') {
          PaS = parseFloat(pasInput.value);
          if (isNaN(PaS)) {
            poiseInput.value = cpInput.value = '';
            updating = false;
            return;
          }
          P = PaS / P_TO_PA_S;
          cP = PaS * 1000;
        } else if (source === 'cp') {
          cP = parseFloat(cpInput.value);
          if (isNaN(cP)) {
            poiseInput.value = pasInput.value = '';
            updating = false;
            return;
          }
          PaS = cP * CP_TO_PA_S;
          P = cP / 100;
        }

        poiseInput.value = typeof P === 'number' ? round(P, 6) : '';
        pasInput.value = typeof PaS === 'number' ? round(PaS, 6) : '';
        cpInput.value = typeof cP === 'number' ? round(cP, 6) : '';

        updating = false;
      }

      if (poiseInput) poiseInput.addEventListener('input', () => updateFrom('poise'));
      if (pasInput) pasInput.addEventListener('input', () => updateFrom('pas'));
      if (cpInput) cpInput.addEventListener('input', () => updateFrom('cp'));

      // Fill table with common values
      const sampleP = [0.01, 0.1, 0.5, 1, 2, 5, 10, 20, 50];
      if (tableBody) {
        sampleP.forEach(val => {
          const pa = val * P_TO_PA_S;
          const cp = val * 100;
          const tr = document.createElement('tr');
          tr.innerHTML = `
            <td class="text-right">${val}</td>
            <td class="text-right">${round(pa, 4)}</td>
            <td class="text-right">${round(cp, 2)}</td>
          `;
          tableBody.appendChild(tr);
        });
      }
    })();
  </script>

