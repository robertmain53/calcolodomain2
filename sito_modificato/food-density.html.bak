  <script>
    (function() {
      const CUP_TO_ML = 240;
      const OZ_PER_G = 1 / 28.3495;

      const foodSelect = document.getElementById('foodSelect');
      const volumeMl = document.getElementById('volumeMl');
      const cupsInput = document.getElementById('cupsInput');
      const densityInput = document.getElementById('densityInput');
      const sgInput = document.getElementById('sgInput');
      const gramsOutput = document.getElementById('gramsOutput');
      const ozOutput = document.getElementById('ozOutput');
      const foodTableBody = document.getElementById('foodTableBody');

      let updating = false;

      const foods = [
        { name: 'Water', density: 1.0 },
        { name: 'Whole milk', density: 1.03 },
        { name: 'Olive oil', density: 0.92 },
        { name: 'Honey', density: 1.42 },
        { name: 'Flour, AP, sifted', density: 0.60 },
        { name: 'Granulated sugar', density: 0.85 },
        { name: 'Yogurt (plain)', density: 0.95 },
        { name: 'Cooked rice', density: 0.65 }
      ];

      function round(val, decimals) {
        if (!isFinite(val)) return '';
        const f = Math.pow(10, decimals);
        return Math.round(val * f) / f;
      }

      function ensureDensityFromSG() {
        const sg = parseFloat(sgInput.value);
        if (!isNaN(sg)) {
          densityInput.value = round(sg * 1.0, 4);
        }
      }

      function ensureSGFromDensity() {
        const dens = parseFloat(densityInput.value);
        if (!isNaN(dens)) {
          sgInput.value = round(dens / 1.0, 4);
        }
      }

      function recalc() {
        if (updating) return;
        updating = true;

        let volMl = parseFloat(volumeMl.value);
        let cups = parseFloat(cupsInput.value);

        if (isNaN(volMl) && !isNaN(cups)) {
          volMl = cups * CUP_TO_ML;
          volumeMl.value = round(volMl, 3);
        } else if (!isNaN(volMl) && isNaN(cups)) {
          cupsInput.value = round(volMl / CUP_TO_ML, 4);
        }

        const density = parseFloat(densityInput.value);
        let grams = '';
        let oz = '';
        if (!isNaN(volMl) && !isNaN(density)) {
          grams = volMl * density;
          oz = grams * OZ_PER_G;
        }

        gramsOutput.value = grams !== '' ? round(grams, 3) : '';
        ozOutput.value = oz !== '' ? round(oz, 3) : '';

        updating = false;
      }

      if (foodSelect) {
        foodSelect.addEventListener('change', function() {
          const val = this.value;
          if (val) {
            densityInput.value = val;
            sgInput.value = round(parseFloat(val), 4);
            recalc();
          }
        });
      }

      if (volumeMl) volumeMl.addEventListener('input', recalc);
      if (cupsInput) cupsInput.addEventListener('input', recalc);
      if (densityInput) densityInput.addEventListener('input', function() {
        ensureSGFromDensity();
        recalc();
      });
      if (sgInput) sgInput.addEventListener('input', function() {
        ensureDensityFromSG();
        recalc();
      });

      // populate table
      if (foodTableBody) {
        foods.forEach(f => {
          const cupWeight = f.density * CUP_TO_ML;
          const tr = document.createElement('tr');
          tr.innerHTML = `
            <td>${f.name}</td>
            <td>${round(f.density, 4)}</td>
            <td>${round(f.density, 4)}</td>
            <td>${round(cupWeight, 1)}</td>
          `;
          foodTableBody.appendChild(tr);
        });
      }
    })();
  </script>

