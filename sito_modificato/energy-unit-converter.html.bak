    <script>
      // Mobile menu toggle
      document.getElementById('mobile-menu-toggle')?.addEventListener('click', function () {
        const menu = document.getElementById('mobile-menu');
        if (!menu) return;
        const isHidden = menu.classList.contains('hidden');
        menu.classList.toggle('hidden', !isHidden);
        this.setAttribute('aria-expanded', String(isHidden));
      });

      document.addEventListener('DOMContentLoaded', function () {
        const energyInputValue = document.getElementById('energy-input-value');
        const energyInputUnit = document.getElementById('energy-input-unit');

        const outJ = document.getElementById('out-J');
        const outKJ = document.getElementById('out-kJ');
        const outMJ = document.getElementById('out-MJ');
        const outCal = document.getElementById('out-cal');
        const outKcal = document.getElementById('out-kcal');
        const outBTU = document.getElementById('out-BTU');
        const outWh = document.getElementById('out-Wh');
        const outKWh = document.getElementById('out-kWh');
        const outEv = document.getElementById('out-eV');
        const outHartree = document.getElementById('out-Hartree');
        const energySummary = document.getElementById('energy-summary');

        const presetButtons = document.querySelectorAll('.energy-preset-btn');
        const clearBtn = document.getElementById('energy-clear');

        // Constants in joules
        const FACTORS_TO_J = {
          J: 1,
          kJ: 1000,
          cal: 4.184,           // thermochemical
          kcal: 4184,
          Wh: 3600,
          kWh: 3.6e6,
          BTU: 1055.056,        // IT BTU
          eV: 1.602176634e-19,  // exact
          Hartree: 4.3597447222071e-18 // CODATA-type value
        };

        function parseNumber(input) {
          const raw = input.value;
          if (raw === '' || isNaN(Number(raw))) return null;
          return Number(raw);
        }

        function formatNumber(value) {
          if (!isFinite(value)) return '';
          const absVal = Math.abs(value);
          if (absVal !== 0 && (absVal < 1e-4 || absVal >= 1e9)) {
            return value.toExponential(6);
          }
          return Number(value.toFixed(6)).toString();
        }

        function fromJoules(J, unit) {
          const factor = FACTORS_TO_J[unit];
          if (!factor) return NaN;
          return J / factor;
        }

        function updateEnergyOutputs() {
          const val = parseNumber(energyInputValue);
          if (val === null) {
            [outJ, outKJ, outMJ, outCal, outKcal, outBTU, outWh, outKWh, outEv, outHartree].forEach(function (field) {
              if (field) field.value = '';
            });
            if (energySummary) energySummary.value = '';
            return;
          }

          const unit = energyInputUnit.value;
          const factor = FACTORS_TO_J[unit] || 1;
          const J = val * factor;

          if (outJ) outJ.value = formatNumber(J);
          if (outKJ) outKJ.value = formatNumber(J / 1000);
          if (outMJ) outMJ.value = formatNumber(J / 1e6);
          if (outCal) outCal.value = formatNumber(fromJoules(J, 'cal'));
          if (outKcal) outKcal.value = formatNumber(fromJoules(J, 'kcal'));
          if (outBTU) outBTU.value = formatNumber(fromJoules(J, 'BTU'));
          if (outWh) outWh.value = formatNumber(fromJoules(J, 'Wh'));
          if (outKWh) outKWh.value = formatNumber(fromJoules(J, 'kWh'));
          if (outEv) outEv.value = formatNumber(fromJoules(J, 'eV'));
          if (outHartree) outHartree.value = formatNumber(fromJoules(J, 'Hartree'));

          if (energySummary) {
            energySummary.value =
              val + ' ' + unit + ' = ' +
              formatNumber(J) + ' J, ' +
              formatNumber(J / 1000) + ' kJ, ' +
              formatNumber(fromJoules(J, 'kWh')) + ' kWh, ' +
              formatNumber(fromJoules(J, 'BTU')) + ' BTU (IT), ' +
              formatNumber(fromJoules(J, 'kcal')) + ' kcal.';
          }
        }

        if (energyInputValue && energyInputUnit) {
          energyInputValue.addEventListener('input', updateEnergyOutputs);
          energyInputUnit.addEventListener('change', updateEnergyOutputs);
        }

        presetButtons.forEach(function (btn) {
          btn.addEventListener('click', function () {
            const value = this.getAttribute('data-value');
            const unit = this.getAttribute('data-unit');
            if (!value || !unit) return;
            energyInputValue.value = value;
            energyInputUnit.value = unit;
            updateEnergyOutputs();
          });
        });

        if (clearBtn) {
          clearBtn.addEventListener('click', function () {
            energyInputValue.value = '';
            [outJ, outKJ, outMJ, outCal, outKcal, outBTU, outWh, outKWh, outEv, outHartree].forEach(function (field) {
              if (field) field.value = '';
            });
            if (energySummary) energySummary.value = '';
          });
        }
      });
    </script>

