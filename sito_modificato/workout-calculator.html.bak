    <script>
      document.addEventListener("DOMContentLoaded", function () {
        document
          .querySelectorAll("#faq-container .faq-question")
          .forEach(function (btn) {
            btn.addEventListener("click", function () {
              const answer = btn.parentElement.querySelector(".faq-answer");
              const sign = btn.querySelector("span:last-child");
              const isHidden = answer.classList.contains("hidden");
              document
                .querySelectorAll("#faq-container .faq-answer")
                .forEach((a) => a.classList.add("hidden"));
              document
                .querySelectorAll(
                  "#faq-container .faq-question span:last-child"
                )
                .forEach((s) => (s.textContent = "+"));
              if (isHidden) {
                answer.classList.remove("hidden");
                sign.textContent = "−";
              }
            });
          });
      });

      // Workout calculator logic
      const MET_TABLE = {
        cardio: [
          {
            id: "walk_slow",
            name: "Walking – easy (2 mph / 3.2 km/h)",
            met: 2.5,
          },
          {
            id: "walk_brisk",
            name: "Walking – brisk (3.5 mph / 5.6 km/h)",
            met: 4.3,
          },
          {
            id: "run_jog",
            name: "Running – easy jog (5 mph / 8 km/h)",
            met: 8.0,
          },
          {
            id: "run_moderate",
            name: "Running – moderate (6 mph / 9.7 km/h)",
            met: 9.8,
          },
          {
            id: "run_fast",
            name: "Running – fast (7.5 mph / 12 km/h)",
            met: 11.5,
          },
          {
            id: "cycle_easy",
            name: "Cycling – easy (10–12 mph / 16–19 km/h)",
            met: 6.8,
          },
          {
            id: "cycle_fast",
            name: "Cycling – fast (14–16 mph / 22–26 km/h)",
            met: 10.0,
          },
          { id: "swim_easy", name: "Swimming – easy laps", met: 6.0 },
          { id: "swim_fast", name: "Swimming – vigorous laps", met: 9.8 },
          { id: "elliptical", name: "Elliptical trainer", met: 5.0 },
          {
            id: "stair_climber",
            name: "Stair climber / step machine",
            met: 8.8,
          },
          { id: "hiit", name: "HIIT / interval training (average)", met: 9.0 },
        ],
        strength: [
          {
            id: "weights_light",
            name: "Weight lifting – light effort",
            met: 3.0,
          },
          {
            id: "weights_moderate",
            name: "Weight lifting – moderate effort",
            met: 4.0,
          },
          {
            id: "weights_vigorous",
            name: "Weight lifting – vigorous / powerlifting",
            met: 6.0,
          },
          { id: "circuit_light", name: "Circuit training – light", met: 4.5 },
          {
            id: "circuit_vigorous",
            name: "Circuit training – vigorous",
            met: 8.0,
          },
          { id: "crossfit", name: "CrossFit-style workout", met: 9.0 },
          {
            id: "bodyweight",
            name: "Bodyweight training (push-ups, squats, etc.)",
            met: 4.0,
          },
        ],
        sports: [
          { id: "soccer", name: "Football / Soccer – game play", met: 7.0 },
          { id: "basketball", name: "Basketball – game play", met: 8.0 },
          { id: "tennis_singles", name: "Tennis – singles", met: 8.0 },
          { id: "tennis_doubles", name: "Tennis – doubles", met: 5.0 },
          { id: "dance", name: "Dancing – aerobic / Zumba", met: 7.5 },
          {
            id: "martial_arts",
            name: "Martial arts / boxing (sparring)",
            met: 9.0,
          },
        ],
        lifestyle: [
          { id: "yoga", name: "Yoga – general", met: 3.0 },
          { id: "pilates", name: "Pilates", met: 3.0 },
          {
            id: "housework",
            name: "Housework – cleaning, vacuuming",
            met: 3.5,
          },
          { id: "gardening", name: "Gardening / yard work", met: 4.0 },
          { id: "stairs_house", name: "Climbing stairs at home", met: 4.0 },
        ],
      };

      const intensityFactors = {
        light: 0.7,
        moderate: 1.0,
        vigorous: 1.3,
      };

      let workoutItems = [];

      function kgFromInput() {
        const w = parseFloat(document.getElementById("weight").value);
        const unit = document.getElementById("weight-unit").value;
        if (!w || w <= 0) return null;
        return unit === "lb" ? w * 0.45359237 : w;
      }

      function populateActivities() {
        const category = document.getElementById("activity-category").value;
        const select = document.getElementById("activity");
        select.innerHTML = "";
        (MET_TABLE[category] || []).forEach((act) => {
          const opt = document.createElement("option");
          opt.value = act.id;
          opt.textContent = act.name;
          opt.dataset.met = act.met;
          select.appendChild(opt);
        });
        updateMetInfo();
      }

      function getSelectedActivity() {
        const category = document.getElementById("activity-category").value;
        const id = document.getElementById("activity").value;
        return (MET_TABLE[category] || []).find((a) => a.id === id) || null;
      }

      function adjustedMet(baseMet, intensity) {
        if (!intensity || intensity === "auto") return baseMet;
        const factor = intensityFactors[intensity] || 1.0;
        return +(baseMet * factor).toFixed(1);
      }

      function updateMetInfo() {
        const act = getSelectedActivity();
        const intensity = document.getElementById("intensity").value;
        const info = document.getElementById("activity-met-info");
        if (!act) {
          info.textContent = "";
          return;
        }
        const metAdj = adjustedMet(act.met, intensity);
        info.textContent = `Estimated intensity: ${metAdj} METs`;
      }

      function formatDuration(mins) {
        if (!mins) return "0 min";
        if (mins < 60) return `${mins.toFixed(0)} min`;
        const h = Math.floor(mins / 60);
        const m = Math.round(mins % 60);
        if (m === 0) return `${h} h`;
        return `${h} h ${m} min`;
      }

      function recalcTotals() {
        const weightKg = kgFromInput();
        let totalCalories = 0;
        let totalMinutes = 0;
        let totalMetMinutes = 0;
        let totalDistanceKm = 0;

        workoutItems.forEach((item) => {
          totalMinutes += item.minutes;
          totalMetMinutes += item.met * item.minutes;
          if (item.distanceKm) totalDistanceKm += item.distanceKm;
          if (weightKg) {
            const cals = ((item.met * 3.5 * weightKg) / 200) * item.minutes;
            item.calories = cals;
            totalCalories += cals;
          } else {
            item.calories = null;
          }
        });

        const totalCaloriesEl = document.getElementById("total-calories");
        const caloriesPerHourEl = document.getElementById("calories-per-hour");
        const totalTimeEl = document.getElementById("total-time");
        const avgIntensityEl = document.getElementById("average-intensity");
        const distanceSummaryEl = document.getElementById("distance-summary");

        if (weightKg && totalCalories > 0) {
          totalCaloriesEl.textContent = `${totalCalories.toFixed(0)} kcal`;
          const perHour =
            totalMinutes > 0 ? totalCalories / (totalMinutes / 60) : 0;
          caloriesPerHourEl.textContent = `${perHour.toFixed(0)} kcal/hour`;
        } else {
          totalCaloriesEl.textContent = "0 kcal";
          caloriesPerHourEl.textContent =
            "Enter your weight to see calories burned.";
        }

        totalTimeEl.textContent = formatDuration(totalMinutes);

        if (totalMinutes > 0) {
          const avgMet = totalMetMinutes / totalMinutes;
          let label = "light";
          if (avgMet >= 6) label = "vigorous";
          else if (avgMet >= 3) label = "moderate";
          avgIntensityEl.textContent = `Average intensity: ${avgMet.toFixed(
            1
          )} METs (${label})`;
        } else {
          avgIntensityEl.textContent = "Average intensity: –";
        }

        if (totalDistanceKm > 0 && totalMinutes > 0) {
          const paceMinPerKm = totalMinutes / totalDistanceKm;
          const paceMin = Math.floor(paceMinPerKm);
          const paceSec = Math.round((paceMinPerKm - paceMin) * 60);
          const paceStr = `${paceMin}:${paceSec
            .toString()
            .padStart(2, "0")} min/km`;
          const kcalPerKm =
            weightKg && totalCalories > 0
              ? totalCalories / totalDistanceKm
              : null;
          distanceSummaryEl.textContent = kcalPerKm
            ? `Total distance: ${totalDistanceKm.toFixed(
                2
              )} km • Pace: ${paceStr} • ~${kcalPerKm.toFixed(0)} kcal/km`
            : `Total distance: ${totalDistanceKm.toFixed(
                2
              )} km • Pace: ${paceStr}`;
        } else {
          distanceSummaryEl.textContent =
            "Add distance-based activities to see pace and kcal/km or kcal/mi.";
        }

        renderWorkoutTable();
      }

      function renderWorkoutTable() {
        const tbody = document.getElementById("workout-body");
        tbody.innerHTML = "";
        if (workoutItems.length === 0) {
          const tr = document.createElement("tr");
          tr.id = "workout-empty-row";
          tr.innerHTML =
            '<td colspan="6" class="px-3 py-3 text-center text-gray-400">No exercises added yet. Add your first activity above.</td>';
          tbody.appendChild(tr);
          return;
        }

        workoutItems.forEach((item, index) => {
          const tr = document.createElement("tr");
          tr.className = index % 2 === 0 ? "bg-white" : "bg-gray-50";
          tr.innerHTML = `
        <td class="px-3 py-2 align-top">
          <div class="font-medium text-gray-800">${item.name}</div>
          <div class="text-xs text-gray-500">${item.notes || ""}</div>
        </td>
        <td class="px-3 py-2 text-right align-top">${item.met.toFixed(1)}</td>
        <td class="px-3 py-2 text-right align-top">${item.minutes.toFixed(
          0
        )} min</td>
        <td class="px-3 py-2 text-right align-top">
          ${item.distanceKm ? item.distanceKm.toFixed(2) + " km" : "–"}
        </td>
        <td class="px-3 py-2 text-right align-top">
          ${item.calories != null ? item.calories.toFixed(0) + " kcal" : "–"}
        </td>
        <td class="px-3 py-2 text-center align-top">
          <button data-index="${index}" class="text-xs text-red-600 hover:underline remove-item">Remove</button>
        </td>
      `;
          tbody.appendChild(tr);
        });

        tbody.querySelectorAll(".remove-item").forEach((btn) => {
          btn.addEventListener("click", function () {
            const idx = parseInt(this.getAttribute("data-index"), 10);
            if (!isNaN(idx)) {
              workoutItems.splice(idx, 1);
              recalcTotals();
            }
          });
        });
      }

      function addActivityToWorkout() {
        const act = getSelectedActivity();
        if (!act) return;

        const durationVal = parseFloat(
          document.getElementById("duration").value
        );
        const durationUnit = document.getElementById("duration-unit").value;
        if (!durationVal || durationVal <= 0) {
          alert("Please enter a valid duration.");
          return;
        }
        const minutes = durationUnit === "hr" ? durationVal * 60 : durationVal;

        const distanceVal = parseFloat(
          document.getElementById("distance").value
        );
        const distanceUnit = document.getElementById("distance-unit").value;
        let distanceKm = null;
        if (distanceVal && distanceVal > 0) {
          distanceKm =
            distanceUnit === "mi" ? distanceVal * 1.60934 : distanceVal;
        }

        const sets = parseFloat(document.getElementById("sets").value);
        const reps = parseFloat(document.getElementById("reps").value);
        let notes = "";
        if (sets && reps) {
          notes = `${sets} × ${reps} reps`;
        }

        const intensity = document.getElementById("intensity").value;
        const metAdj = adjustedMet(act.met, intensity);

        workoutItems.push({
          name: act.name,
          met: metAdj,
          minutes,
          distanceKm,
          notes,
          calories: null,
        });

        recalcTotals();
      }

      function clearWorkout() {
        workoutItems = [];
        recalcTotals();
      }

      function applyPreset(presetId) {
        clearWorkout();
        const weightKg = kgFromInput();
        switch (presetId) {
          case "30min-run":
            workoutItems.push({
              name: "Running – moderate (6 mph / 9.7 km/h)",
              met: 9.8,
              minutes: 30,
              distanceKm: 4.8,
              notes: "",
              calories: weightKg ? ((9.8 * 3.5 * weightKg) / 200) * 30 : null,
            });
            break;
          case "45min-walk":
            workoutItems.push({
              name: "Walking – brisk (3.5 mph / 5.6 km/h)",
              met: 4.3,
              minutes: 45,
              distanceKm: 4.0,
              notes: "",
              calories: weightKg ? ((4.3 * 3.5 * weightKg) / 200) * 45 : null,
            });
            break;
          case "full-body-strength":
            workoutItems.push(
              {
                name: "Weight lifting – moderate effort",
                met: 4.0,
                minutes: 40,
                distanceKm: null,
                notes: "Full-body routine",
                calories: weightKg ? ((4.0 * 3.5 * weightKg) / 200) * 40 : null,
              },
              {
                name: "Walking – easy (2 mph / 3.2 km/h)",
                met: 2.5,
                minutes: 20,
                distanceKm: 1.5,
                notes: "Warm-up & cool-down",
                calories: weightKg ? ((2.5 * 3.5 * weightKg) / 200) * 20 : null,
              }
            );
            break;
          case "hiit-20":
            workoutItems.push({
              name: "HIIT / interval training (average)",
              met: 9.0,
              minutes: 20,
              distanceKm: null,
              notes: "Intervals of work and rest",
              calories: weightKg ? ((9.0 * 3.5 * weightKg) / 200) * 20 : null,
            });
            break;
          default:
            return;
        }
        recalcTotals();
      }

      document.addEventListener("DOMContentLoaded", function () {
        populateActivities();
        document
          .getElementById("activity-category")
          .addEventListener("change", populateActivities);
        document
          .getElementById("activity")
          .addEventListener("change", updateMetInfo);
        document
          .getElementById("intensity")
          .addEventListener("change", updateMetInfo);

        document
          .getElementById("add-activity")
          .addEventListener("click", addActivityToWorkout);
        document
          .getElementById("clear-workout")
          .addEventListener("click", clearWorkout);

        ["weight", "weight-unit"].forEach((id) => {
          document.getElementById(id).addEventListener("input", recalcTotals);
          document.getElementById(id).addEventListener("change", recalcTotals);
        });

        document.querySelectorAll("[data-preset]").forEach((btn) => {
          btn.addEventListener("click", () =>
            applyPreset(btn.getAttribute("data-preset"))
          );
        });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
