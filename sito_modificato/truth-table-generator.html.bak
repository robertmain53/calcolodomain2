    <script>
      // --- Truth table generator logic ---

      const exprInput = document.getElementById("expression-input");
      const generateBtn = document.getElementById("generate-btn");
      const clearBtn = document.getElementById("clear-btn");
      const errorMessage = document.getElementById("error-message");
      const tableEl = document.getElementById("truth-table");
      const varsListEl = document.getElementById("vars-list");
      const rowCountEl = document.getElementById("row-count");
      const classificationEl = document.getElementById("classification");
      const truthCountsEl = document.getElementById("truth-counts");
      const dnfEl = document.getElementById("dnf-output");
      const cnfEl = document.getElementById("cnf-output");
      const mintermsEl = document.getElementById("minterms-output");
      const maxtermsEl = document.getElementById("maxterms-output");
      const sortVarsToggle = document.getElementById("sort-vars-toggle");
      const showStepsToggle = document.getElementById("show-steps-toggle");
      const compactToggle = document.getElementById("compact-toggle");
      const limitRowsToggle = document.getElementById("limit-rows-toggle");
      const rowLimitNote = document.getElementById("row-limit-note");
      const copyTableBtn = document.getElementById("copy-table-btn");
      const downloadCsvBtn = document.getElementById("download-csv-btn");

      const example1Btn = document.getElementById("example1-btn");
      const example2Btn = document.getElementById("example2-btn");

      // Insert operator buttons
      document.querySelectorAll("button[data-insert]").forEach((btn) => {
        btn.addEventListener("click", () => {
          const insert = btn.getAttribute("data-insert");
          insertAtCursor(exprInput, insert);
          exprInput.focus();
        });
      });

      function insertAtCursor(textarea, text) {
        const start = textarea.selectionStart ?? textarea.value.length;
        const end = textarea.selectionEnd ?? textarea.value.length;
        const before = textarea.value.slice(0, start);
        const after = textarea.value.slice(end);
        textarea.value = before + text + after;
        const newPos = start + text.length;
        textarea.selectionStart = textarea.selectionEnd = newPos;
      }

      example1Btn.addEventListener("click", () => {
        exprInput.value = "(P -> Q) & !R";
      });
      example2Btn.addEventListener("click", () => {
        exprInput.value = "(A or B) xor C";
      });

      clearBtn.addEventListener("click", () => {
        exprInput.value = "";
        errorMessage.classList.add("hidden");
        tableEl.innerHTML = "";
        varsListEl.textContent = "–";
        rowCountEl.textContent = "Rows: –";
        classificationEl.textContent = "–";
        truthCountsEl.textContent = "True rows: – · False rows: –";
        dnfEl.textContent = "–";
        cnfEl.textContent = "–";
        mintermsEl.textContent = "–";
        maxtermsEl.textContent = "–";
        rowLimitNote.classList.add("hidden");
      });

      generateBtn.addEventListener("click", () => {
        try {
          errorMessage.classList.add("hidden");
          const raw = exprInput.value.trim();
          if (!raw) {
            throw new Error("Please enter a logical expression.");
          }
          const parsed = parseExpression(raw);
          const vars = Array.from(parsed.variables);
          if (sortVarsToggle.checked) vars.sort((a, b) => a.localeCompare(b));
          const maxRows = limitRowsToggle.checked ? 256 : Infinity;
          const { rows, steps } = buildTruthTable(parsed.ast, vars, maxRows);
          renderTable(vars, rows, steps);
          updateSummary(vars, rows);
          updateNormalForms(vars, rows);
        } catch (e) {
          console.error(e);
          errorMessage.textContent = e.message || "Invalid expression.";
          errorMessage.classList.remove("hidden");
          tableEl.innerHTML = "";
        }
      });

      // Tokenizer and parser (shunting-yard to RPN, then AST)
      const TOKEN_TYPES = {
        VAR: "VAR",
        CONST: "CONST",
        NOT: "NOT",
        AND: "AND",
        OR: "OR",
        XOR: "XOR",
        IMP: "IMP",
        IFF: "IFF",
        LPAREN: "LPAREN",
        RPAREN: "RPAREN",
      };

      const PRECEDENCE = {
        [TOKEN_TYPES.NOT]: 5,
        [TOKEN_TYPES.AND]: 4,
        [TOKEN_TYPES.OR]: 3,
        [TOKEN_TYPES.XOR]: 3,
        [TOKEN_TYPES.IMP]: 2,
        [TOKEN_TYPES.IFF]: 1,
      };

      const RIGHT_ASSOC = new Set([
        TOKEN_TYPES.NOT,
        TOKEN_TYPES.IMP,
        TOKEN_TYPES.IFF,
      ]);

      function tokenize(input) {
        const tokens = [];
        const variables = new Set();
        const src = input.replace(/\s+/g, " ");
        let i = 0;
        while (i < src.length) {
          const ch = src[i];
          if (ch === " ") {
            i++;
            continue;
          }

          // Parentheses
          if (ch === "(") {
            tokens.push({ type: TOKEN_TYPES.LPAREN, value: "(" });
            i++;
            continue;
          }
          if (ch === ")") {
            tokens.push({ type: TOKEN_TYPES.RPAREN, value: ")" });
            i++;
            continue;
          }

          // Multi-char operators
          if (src.startsWith("<->", i)) {
            tokens.push({ type: TOKEN_TYPES.IFF, value: "<->" });
            i += 3;
            continue;
          }
          if (src.startsWith("↔", i)) {
            tokens.push({ type: TOKEN_TYPES.IFF, value: "↔" });
            i += 1;
            continue;
          }
          if (src.startsWith("->", i)) {
            tokens.push({ type: TOKEN_TYPES.IMP, value: "->" });
            i += 2;
            continue;
          }
          if (src.startsWith("→", i)) {
            tokens.push({ type: TOKEN_TYPES.IMP, value: "→" });
            i += 1;
            continue;
          }

          // Single-char operators
          if (ch === "!") {
            tokens.push({ type: TOKEN_TYPES.NOT, value: "!" });
            i++;
            continue;
          }
          if (ch === "¬") {
            tokens.push({ type: TOKEN_TYPES.NOT, value: "¬" });
            i++;
            continue;
          }
          if (ch === "&" || ch === "∧") {
            tokens.push({ type: TOKEN_TYPES.AND, value: ch });
            i++;
            continue;
          }
          if (ch === "|" || ch === "∨") {
            tokens.push({ type: TOKEN_TYPES.OR, value: ch });
            i++;
            continue;
          }
          if (ch === "^") {
            tokens.push({ type: TOKEN_TYPES.XOR, value: "^" });
            i++;
            continue;
          }

          // Identifiers / keywords / constants
          if (/[A-Za-z_]/.test(ch)) {
            let j = i + 1;
            while (j < src.length && /[A-Za-z0-9_]/.test(src[j])) j++;
            const word = src.slice(i, j);
            const lower = word.toLowerCase();
            if (lower === "and")
              tokens.push({ type: TOKEN_TYPES.AND, value: word });
            else if (lower === "or")
              tokens.push({ type: TOKEN_TYPES.OR, value: word });
            else if (lower === "not")
              tokens.push({ type: TOKEN_TYPES.NOT, value: word });
            else if (lower === "xor")
              tokens.push({ type: TOKEN_TYPES.XOR, value: word });
            else if (["true", "t"].includes(lower))
              tokens.push({ type: TOKEN_TYPES.CONST, value: true });
            else if (["false", "f"].includes(lower))
              tokens.push({ type: TOKEN_TYPES.CONST, value: false });
            else {
              tokens.push({ type: TOKEN_TYPES.VAR, value: word });
              variables.add(word);
            }
            i = j;
            continue;
          }

          // Numeric constants 0/1
          if (ch === "0" || ch === "1") {
            tokens.push({ type: TOKEN_TYPES.CONST, value: ch === "1" });
            i++;
            continue;
          }

          throw new Error(`Unexpected character "${ch}" at position ${i + 1}.`);
        }
        if (!tokens.length) throw new Error("Expression is empty.");
        return { tokens, variables };
      }

      function toRPN(tokens) {
        const output = [];
        const stack = [];
        for (const tok of tokens) {
          switch (tok.type) {
            case TOKEN_TYPES.VAR:
            case TOKEN_TYPES.CONST:
              output.push(tok);
              break;
            case TOKEN_TYPES.NOT:
            case TOKEN_TYPES.AND:
            case TOKEN_TYPES.OR:
            case TOKEN_TYPES.XOR:
            case TOKEN_TYPES.IMP:
            case TOKEN_TYPES.IFF: {
              while (stack.length) {
                const top = stack[stack.length - 1];
                if (top.type === TOKEN_TYPES.LPAREN) break;
                const precTop = PRECEDENCE[top.type] || 0;
                const precTok = PRECEDENCE[tok.type] || 0;
                if (
                  precTop > precTok ||
                  (precTop === precTok && !RIGHT_ASSOC.has(tok.type))
                ) {
                  output.push(stack.pop());
                } else break;
              }
              stack.push(tok);
              break;
            }
            case TOKEN_TYPES.LPAREN:
              stack.push(tok);
              break;
            case TOKEN_TYPES.RPAREN:
              let found = false;
              while (stack.length) {
                const top = stack.pop();
                if (top.type === TOKEN_TYPES.LPAREN) {
                  found = true;
                  break;
                }
                output.push(top);
              }
              if (!found) throw new Error("Mismatched parentheses.");
              break;
            default:
              throw new Error("Unknown token type.");
          }
        }
        while (stack.length) {
          const top = stack.pop();
          if (
            top.type === TOKEN_TYPES.LPAREN ||
            top.type === TOKEN_TYPES.RPAREN
          ) {
            throw new Error("Mismatched parentheses.");
          }
          output.push(top);
        }
        return output;
      }

      function rpnToAST(rpn) {
        const stack = [];
        for (const tok of rpn) {
          if (tok.type === TOKEN_TYPES.VAR || tok.type === TOKEN_TYPES.CONST) {
            stack.push({ type: tok.type, value: tok.value });
          } else if (tok.type === TOKEN_TYPES.NOT) {
            const arg = stack.pop();
            if (!arg) throw new Error("NOT operator missing operand.");
            stack.push({ type: "NOT", arg });
          } else {
            const right = stack.pop();
            const left = stack.pop();
            if (!left || !right)
              throw new Error("Binary operator missing operand.");
            stack.push({ type: tok.type, left, right });
          }
        }
        if (stack.length !== 1)
          throw new Error("Invalid expression structure.");
        return stack[0];
      }

      function parseExpression(input) {
        const { tokens, variables } = tokenize(input);
        const rpn = toRPN(tokens);
        const ast = rpnToAST(rpn);
        return { ast, variables };
      }

      function evalAST(ast, env) {
        switch (ast.type) {
          case TOKEN_TYPES.CONST:
            return !!ast.value;
          case TOKEN_TYPES.VAR:
            return !!env[ast.value];
          case "NOT":
            return !evalAST(ast.arg, env);
          case TOKEN_TYPES.AND:
            return evalAST(ast.left, env) && evalAST(ast.right, env);
          case TOKEN_TYPES.OR:
            return evalAST(ast.left, env) || evalAST(ast.right, env);
          case TOKEN_TYPES.XOR:
            return !!(evalAST(ast.left, env) ^ evalAST(ast.right, env));
          case TOKEN_TYPES.IMP: {
            const p = evalAST(ast.left, env);
            const q = evalAST(ast.right, env);
            return !p || q;
          }
          case TOKEN_TYPES.IFF: {
            const p = evalAST(ast.left, env);
            const q = evalAST(ast.right, env);
            return p === q;
          }
          default:
            throw new Error("Unknown AST node type.");
        }
      }

      function buildTruthTable(ast, vars, maxRows) {
        const n = vars.length;
        if (n === 0) {
          // No variables: single row
          const val = evalAST(ast, {});
          return { rows: [{ assignment: {}, result: val }], steps: [] };
        }
        if (n > 12 && !limitRowsToggle.checked) {
          throw new Error(
            "Expression has more than 12 variables; enable row limiting or simplify the formula."
          );
        }
        const totalRows = 1 << n;
        const rows = [];
        const steps = []; // currently only final result; placeholder for future intermediate steps

        const rowsToGenerate = Math.min(totalRows, maxRows);
        for (let i = 0; i < rowsToGenerate; i++) {
          const env = {};
          for (let bit = 0; bit < n; bit++) {
            env[vars[bit]] = !!(i & (1 << (n - bit - 1)));
          }
          const result = evalAST(ast, env);
          rows.push({ assignment: env, result });
        }
        return { rows, steps };
      }

      function renderTable(vars, rows, steps) {
        const showSteps = showStepsToggle.checked && !compactToggle.checked;
        const headers = [...vars];
        if (showSteps && steps.length) {
          headers.push(...steps.map((_, idx) => `Step ${idx + 1}`));
        }
        headers.push("F");

        let thead = "<thead><tr>";
        for (const h of headers) {
          thead += `<th class="px-2 py-1 border-b border-gray-200 font-semibold text-xs bg-gray-50">${escapeHtml(
            h
          )}</th>`;
        }
        thead += "</tr></thead>";

        let tbody = "<tbody>";
        rows.forEach((row, idx) => {
          tbody += '<tr class="odd:bg-white even:bg-gray-50">';
          for (const v of vars) {
            tbody += `<td class="px-2 py-1 border-b border-gray-100">${
              row.assignment[v] ? "1" : "0"
            }</td>`;
          }
          if (showSteps && steps.length) {
            steps.forEach(() => {
              tbody += `<td class="px-2 py-1 border-b border-gray-100 text-gray-400">–</td>`;
            });
          }
          tbody += `<td class="px-2 py-1 border-b border-gray-100 font-semibold ${
            row.result ? "text-green-700" : "text-red-700"
          }">${row.result ? "1" : "0"}</td>`;
          tbody += "</tr>";
        });
        tbody += "</tbody>";

        tableEl.innerHTML = thead + tbody;

        const totalRows = 1 << vars.length;
        if (limitRowsToggle.checked && rows.length < totalRows) {
          rowLimitNote.classList.remove("hidden");
        } else {
          rowLimitNote.classList.add("hidden");
        }
      }

      function updateSummary(vars, rows) {
        if (!rows.length) return;
        varsListEl.textContent = vars.length
          ? vars.join(", ")
          : "None (constant expression)";
        const totalRows = 1 << vars.length;
        rowCountEl.textContent = `Rows: ${rows.length}${
          rows.length < totalRows ? ` of ${totalRows}` : ""
        }`;

        let trueCount = 0;
        let falseCount = 0;
        rows.forEach((r) => (r.result ? trueCount++ : falseCount++));

        let classification =
          "Contingent formula (sometimes true, sometimes false).";
        if (
          trueCount === rows.length &&
          (rows.length === totalRows || limitRowsToggle.checked)
        ) {
          classification = "Tautology (always true for all combinations).";
        } else if (
          falseCount === rows.length &&
          (rows.length === totalRows || limitRowsToggle.checked)
        ) {
          classification = "Contradiction (always false for all combinations).";
        }
        classificationEl.textContent = classification;
        truthCountsEl.textContent = `True rows: ${trueCount} · False rows: ${falseCount}`;
      }

      function updateNormalForms(vars, rows) {
        if (!rows.length) {
          dnfEl.textContent =
            cnfEl.textContent =
            mintermsEl.textContent =
            maxtermsEl.textContent =
              "–";
          return;
        }
        const n = vars.length;
        const totalRows = 1 << n;
        const fullTable = rows.length === totalRows && !limitRowsToggle.checked;

        const minterms = [];
        const maxterms = [];
        const dnfClauses = [];
        const cnfClauses = [];

        rows.forEach((row, idx) => {
          const index = idx; // assuming rows in binary order
          if (row.result) {
            minterms.push(index);
            const literals = vars.map((v, i) =>
              row.assignment[v] ? v : `¬${v}`
            );
            dnfClauses.push("(" + literals.join(" ∧ ") + ")");
          } else {
            maxterms.push(index);
            const literals = vars.map((v, i) =>
              row.assignment[v] ? `¬${v}` : v
            );
            cnfClauses.push("(" + literals.join(" ∨ ") + ")");
          }
        });

        dnfEl.textContent = dnfClauses.length
          ? dnfClauses.join(" ∨ ")
          : fullTable
          ? "Contradiction: no rows where F = 1."
          : "DNF based on partial table only (row limit enabled).";

        cnfEl.textContent = cnfClauses.length
          ? cnfClauses.join(" ∧ ")
          : fullTable
          ? "Tautology: no rows where F = 0."
          : "CNF based on partial table only (row limit enabled).";

        mintermsEl.textContent = minterms.length ? minterms.join(", ") : "None";
        maxtermsEl.textContent = maxterms.length ? maxterms.join(", ") : "None";
      }

      function escapeHtml(str) {
        return String(str)
          .replace(/&/g, "&amp;")
          .replace(/</g, "&lt;")
          .replace(/>/g, "&gt;");
      }

      copyTableBtn.addEventListener("click", () => {
        if (!tableEl.rows.length) return;
        let text = "";
        for (let i = 0; i < tableEl.rows.length; i++) {
          const cells = Array.from(tableEl.rows[i].cells).map((td) =>
            td.textContent.trim()
          );
          text += cells.join("\t") + "\n";
        }
        navigator.clipboard
          .writeText(text)
          .then(() => {
            copyTableBtn.textContent = "Copied!";
            setTimeout(() => (copyTableBtn.textContent = "Copy as text"), 1500);
          })
          .catch(() => {
            alert("Unable to copy to clipboard in this browser.");
          });
      });

      downloadCsvBtn.addEventListener("click", () => {
        if (!tableEl.rows.length) return;
        let csv = "";
        for (let i = 0; i < tableEl.rows.length; i++) {
          const cells = Array.from(tableEl.rows[i].cells).map((td) => {
            const val = td.textContent.trim().replace(/"/g, '""');
            return `"${val}"`;
          });
          csv += cells.join(",") + "\n";
        }
        const blob = new Blob([csv], { type: "text/csv;charset=utf-8;" });
        const url = URL.createObjectURL(blob);
        const a = document.createElement("a");
        a.href = url;
        a.download = "truth-table.csv";
        document.body.appendChild(a);
        a.click();
        document.body.removeChild(a);
        URL.revokeObjectURL(url);
      });

      // Auto-generate on Ctrl+Enter
      exprInput.addEventListener("keydown", (e) => {
        if ((e.ctrlKey || e.metaKey) && e.key === "Enter") {
          e.preventDefault();
          generateBtn.click();
        }
      });
    </script>

