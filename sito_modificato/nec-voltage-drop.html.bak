    <script>
      // Conductor resistance data (ohms per 1000 ft at 75°C, approximate)
      const RESISTANCE_DATA = {
        cu: {
          14: 3.14,
          12: 1.98,
          10: 1.24,
          8: 0.778,
          6: 0.491,
          4: 0.308,
          3: 0.245,
          2: 0.194,
          1: 0.154,
          "1/0": 0.122,
          "2/0": 0.0967,
          "3/0": 0.0766,
          "4/0": 0.0608,
          250: 0.051,
          300: 0.0425,
          350: 0.0366,
          400: 0.0321,
          500: 0.0258,
          600: 0.0215,
          750: 0.0172,
        },
        al: {
          14: 5.0,
          12: 3.31,
          10: 2.08,
          8: 1.31,
          6: 0.828,
          4: 0.522,
          3: 0.414,
          2: 0.329,
          1: 0.261,
          "1/0": 0.207,
          "2/0": 0.164,
          "3/0": 0.13,
          "4/0": 0.103,
          250: 0.089,
          300: 0.074,
          350: 0.0632,
          400: 0.055,
          500: 0.044,
          600: 0.037,
          750: 0.03,
        },
      };

      function adjustResistance(baseR, material, tempC) {
        // Linear temperature coefficient relative to 75°C
        const alpha = material === "cu" ? 0.00393 : 0.00403;
        const deltaT = tempC - 75;
        return baseR * (1 + alpha * deltaT);
      }

      function formatLengthLabel(lengthFt, unit) {
        const ft = lengthFt;
        if (unit === "m") {
          const m = ft * 0.3048;
          return ft.toFixed(1) + " ft (" + m.toFixed(1) + " m) round-trip";
        }
        return ft.toFixed(1) + " ft round-trip";
      }

      function evaluateNECStatus(percentVD, circuitType) {
        const limit = circuitType === "feeder+branch" ? 5 : 3;
        const diff = percentVD - limit;
        if (diff <= 0) {
          return {
            level: "ok",
            text: "Within recommended " + limit + "% limit",
          };
        } else if (diff <= 1) {
          return {
            level: "warn",
            text: "Slightly above recommended " + limit + "% limit",
          };
        } else {
          return {
            level: "bad",
            text: "Above recommended " + limit + "% limit",
          };
        }
      }

      function setStatusBadge(status) {
        const badge = document.getElementById("nec-status-badge");
        badge.classList.remove(
          "result-badge-ok",
          "result-badge-warn",
          "result-badge-bad"
        );
        if (status.level === "ok") {
          badge.classList.add("result-badge-ok");
          badge.textContent = "Within NEC recommendation";
        } else if (status.level === "warn") {
          badge.classList.add("result-badge-warn");
          badge.textContent = "Slightly above NEC recommendation";
        } else {
          badge.classList.add("result-badge-bad");
          badge.textContent = "Above NEC recommendation";
        }
        document.getElementById("nec-status-text").textContent = status.text;
      }

      document.addEventListener("DOMContentLoaded", () => {
        const form = document.getElementById("vd-form");
        const errorEl = document.getElementById("calc-error");
        const resultsSection = document.getElementById("results-section");

        document.getElementById("calc-btn").addEventListener("click", () => {
          errorEl.textContent = "";

          const systemType = document.getElementById("system-type").value;
          const voltage = parseFloat(document.getElementById("voltage").value);
          const current = parseFloat(document.getElementById("current").value);
          let length = parseFloat(document.getElementById("length").value);
          const lengthUnit = document.getElementById("length-unit").value;
          const material = document.getElementById("material").value;
          const awg = document.getElementById("awg").value;
          const temp = parseFloat(document.getElementById("temp").value);
          let pf = parseFloat(document.getElementById("pf").value);
          const circuitType = document.getElementById("circuit-type").value;

          if (!isFinite(voltage) || voltage <= 0) {
            errorEl.textContent = "Enter a valid system voltage.";
            return;
          }
          if (!isFinite(current) || current < 0) {
            errorEl.textContent = "Enter a valid load current.";
            return;
          }
          if (!isFinite(length) || length < 0) {
            errorEl.textContent = "Enter a valid length.";
            return;
          }
          if (
            !RESISTANCE_DATA[material] ||
            RESISTANCE_DATA[material][awg] == null
          ) {
            errorEl.textContent = "Conductor size not supported.";
            return;
          }
          if (!isFinite(temp)) {
            errorEl.textContent = "Enter a valid temperature.";
            return;
          }
          if (!isFinite(pf) || pf <= 0 || pf > 1) {
            pf = 1.0;
          }

          // Convert length to feet if needed
          if (lengthUnit === "m") {
            length = length / 0.3048;
          }

          const baseRper1000ft = RESISTANCE_DATA[material][awg];
          const adjRper1000ft = adjustResistance(
            baseRper1000ft,
            material,
            temp
          );
          const RperFt = adjRper1000ft / 1000;

          let vdVolts = 0;
          let roundTripFt = 0;

          if (systemType === "1ph") {
            roundTripFt = 2 * length;
            vdVolts = current * RperFt * roundTripFt * pf;
          } else {
            roundTripFt = 2 * length; // for reporting only
            vdVolts = Math.sqrt(3) * current * RperFt * length * pf;
          }

          const vdPercent = voltage > 0 ? (vdVolts / voltage) * 100 : 0;
          const vLoad = voltage - vdVolts;

          document.getElementById("vd-volts").textContent = vdVolts.toFixed(2);
          document.getElementById("vd-percent").textContent =
            vdPercent.toFixed(2);
          document.getElementById("v-load").textContent = vLoad.toFixed(2);
          document.getElementById("v-source-label").textContent =
            voltage.toFixed(1);

          const status = evaluateNECStatus(vdPercent, circuitType);
          setStatusBadge(status);

          document.getElementById("res-material-label").textContent =
            material === "cu" ? "Copper" : "Aluminum";
          document.getElementById("res-size-label").textContent =
            awg + (isNaN(parseFloat(awg)) ? "" : " AWG");
          document.getElementById("resistance-label").textContent =
            adjRper1000ft.toFixed(3);
          document.getElementById("res-temp-label").textContent =
            temp.toFixed(0);
          document.getElementById("res-length-label").textContent =
            formatLengthLabel(roundTripFt, "ft");

          resultsSection.classList.remove("hidden");
          resultsSection.scrollIntoView({ behavior: "smooth", block: "start" });
        });

        document.getElementById("reset-btn").addEventListener("click", () => {
          form.reset();
          document.getElementById("voltage").value = 120;
          document.getElementById("current").value = 20;
          document.getElementById("length").value = 100;
          document.getElementById("length-unit").value = "ft";
          document.getElementById("material").value = "cu";
          document.getElementById("awg").value = "12";
          document.getElementById("temp").value = 75;
          document.getElementById("pf").value = 1.0;
          document.getElementById("system-type").value = "1ph";
          document.getElementById("circuit-type").value = "branch";
          document.getElementById("calc-error").textContent = "";
          document.getElementById("results-section").classList.add("hidden");
        });

        // Quick presets
        document.querySelectorAll("button[data-preset]").forEach((btn) => {
          btn.addEventListener("click", () => {
            const v = parseFloat(btn.getAttribute("data-preset"));
            document.getElementById("voltage").value = v;
            if (v === 208 || v === 480) {
              document.getElementById("system-type").value = "3ph";
            } else {
              document.getElementById("system-type").value = "1ph";
            }
          });
        });
      });
    </script>

