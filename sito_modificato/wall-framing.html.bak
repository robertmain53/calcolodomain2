    <script>
      function ftInToInches(ft, inch) {
        ft = parseFloat(ft) || 0;
        inch = parseFloat(inch) || 0;
        return ft * 12 + inch;
      }

      function inchesToFtIn(inches) {
        if (!isFinite(inches)) return { ft: 0, inch: 0 };
        const ft = Math.floor(inches / 12);
        const inch = Math.round((inches - ft * 12) * 100) / 100;
        return { ft, inch };
      }

      function formatFtIn(inches) {
        const v = inchesToFtIn(inches);
        return `${v.ft}' ${v.inch.toFixed(0)}"`;
      }

      function updateSummary() {
        const Lft = document.getElementById("wall-length").value;
        const Lin = document.getElementById("wall-length-in").value;
        const Hft = document.getElementById("wall-height").value;
        const Hin = document.getElementById("wall-height-in").value;
        const spacing = document.getElementById("stud-spacing").value;
        const hasOpening = document.getElementById("has-opening").value;
        const oW = document.getElementById("opening-width").value;
        const oOff = document.getElementById("opening-offset").value;

        document.getElementById("summary-length").textContent = formatFtIn(
          ftInToInches(Lft, Lin)
        );
        document.getElementById("summary-height").textContent = formatFtIn(
          ftInToInches(Hft, Hin)
        );
        document.getElementById(
          "summary-spacing"
        ).textContent = `${spacing}" o.c.`;
        if (hasOpening === "yes") {
          document.getElementById(
            "summary-opening"
          ).textContent = `${parseFloat(oW || 0)
            .toFixed(2)
            .replace(/\.00$/, "")}\' wide @ ${parseFloat(oOff || 0)
            .toFixed(2)
            .replace(/\.00$/, "")}\'`;
        } else {
          document.getElementById("summary-opening").textContent = "None";
        }
      }

      function calculateWallFraming() {
        const Lft =
          parseFloat(document.getElementById("wall-length").value) || 0;
        const Lin =
          parseFloat(document.getElementById("wall-length-in").value) || 0;
        const Hft =
          parseFloat(document.getElementById("wall-height").value) || 0;
        const Hin =
          parseFloat(document.getElementById("wall-height-in").value) || 0;
        const spacing =
          parseFloat(document.getElementById("stud-spacing").value) || 16;
        const plateConfig =
          parseInt(document.getElementById("plate-config").value, 10) || 2;
        const cornerConfig =
          parseInt(document.getElementById("corner-studs").value, 10) || 3;
        const intersections =
          parseInt(document.getElementById("intersections").value, 10) || 0;
        const blockingRows =
          parseInt(document.getElementById("blocking-rows").value, 10) || 0;
        const hasOpening =
          document.getElementById("has-opening").value === "yes";
        const oWft =
          parseFloat(document.getElementById("opening-width").value) || 0;
        const oHft =
          parseFloat(document.getElementById("opening-height").value) || 0;
        const oOffft =
          parseFloat(document.getElementById("opening-offset").value) || 0;
        const advanced = document.getElementById("advanced-mode").checked;
        const studLengthCustom = parseFloat(
          document.getElementById("stud-length").value
        );
        const wasteFactor = advanced
          ? parseFloat(document.getElementById("waste-factor").value) || 0
          : 0;

        const wallLenIn = ftInToInches(Lft, Lin);
        const wallHeightIn = ftInToInches(Hft, Hin);

        // Basic validation
        if (wallLenIn <= 0 || wallHeightIn <= 0) {
          alert("Please enter a positive wall length and height.");
          return;
        }

        // Stud layout
        const nSpaces = Math.floor(wallLenIn / spacing);
        let commonStuds = nSpaces + 1; // include both ends

        // Opening adjustments
        let kingStuds = 0;
        let jackStuds = 0;
        let headerLengthIn = 0;
        let cripplesAbove = 0;
        let cripplesBelow = 0;

        if (hasOpening && oWft > 0) {
          const oWIn = oWft * 12;
          const oOffIn = oOffft * 12;
          const oEndIn = oOffIn + oWIn;

          if (oEndIn > wallLenIn) {
            // Opening extends beyond wall; clamp
            headerLengthIn = Math.max(0, wallLenIn - oOffIn);
          } else {
            headerLengthIn = oWIn + 3; // simple rule: opening + 3"
          }

          kingStuds = 2;
          jackStuds = 2;

          // Cripples above: approximate number of stud bays over header
          const headerCenter = oOffIn + headerLengthIn / 2;
          const leftSpaceIndex = Math.ceil(oOffIn / spacing);
          const rightSpaceIndex = Math.floor(oEndIn / spacing);
          const baysOverHeader = Math.max(0, rightSpaceIndex - leftSpaceIndex);
          cripplesAbove = baysOverHeader;

          // Cripples below: assume window if opening height < wall height - 24"
          const oHIn = oHft * 12;
          if (oHIn < wallHeightIn - 24) {
            cripplesBelow = baysOverHeader;
          }
        }

        // Corner and intersection studs
        const extraCornerPerEnd = Math.max(0, cornerConfig - 1);
        const cornerStuds = extraCornerPerEnd * 2;
        const intersectionStuds = intersections * 2;

        // Blocking
        const studBays = Math.max(0, commonStuds - 1);
        const blockingPieces = studBays * blockingRows;

        // Total studs
        const totalStuds =
          commonStuds +
          kingStuds +
          jackStuds +
          cornerStuds +
          intersectionStuds +
          cripplesAbove +
          cripplesBelow;

        // Plates
        const bottomPlatesLF = wallLenIn / 12;
        const topPlatesLF = bottomPlatesLF * plateConfig;

        // Stud length
        const plateThickness = (plateConfig + 1) * 1.5; // top + bottom
        let studLengthIn =
          studLengthCustom && studLengthCustom > 0
            ? studLengthCustom
            : wallHeightIn - plateThickness;

        // Apply waste
        const wasteMultiplier = 1 + wasteFactor / 100;
        const totalStudsWithWaste = Math.ceil(totalStuds * wasteMultiplier);
        const bottomPlatesLFWithWaste = bottomPlatesLF * wasteMultiplier;
        const topPlatesLFWithWaste = topPlatesLF * wasteMultiplier;

        // Update results
        document.getElementById("result-common-studs").textContent =
          commonStuds.toString();
        document.getElementById("result-king-studs").textContent =
          kingStuds.toString();
        document.getElementById("result-jack-studs").textContent =
          jackStuds.toString();
        document.getElementById("result-corner-studs").textContent = (
          cornerStuds + intersectionStuds
        ).toString();
        document.getElementById("result-total-studs").textContent =
          totalStudsWithWaste.toString();
        document.getElementById("result-top-plates").textContent =
          topPlatesLFWithWaste.toFixed(1) + " ft";
        document.getElementById("result-bottom-plates").textContent =
          bottomPlatesLFWithWaste.toFixed(1) + " ft";
        document.getElementById("result-header-length").textContent =
          hasOpening && headerLengthIn > 0
            ? formatFtIn(headerLengthIn)
            : "0' 0\"";
        document.getElementById("result-cripple-above").textContent =
          cripplesAbove.toString();
        document.getElementById("result-cripple-below").textContent =
          cripplesBelow.toString();
        document.getElementById("result-blocking").textContent =
          blockingPieces.toString();

        // Cut list
        const cutListBody = document.getElementById("cut-list-body");
        cutListBody.innerHTML = "";

        function addRow(name, qty, lengthIn, stockHint) {
          if (qty <= 0) return;
          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="px-3 py-2 border-b text-gray-700">${name}</td>
        <td class="px-3 py-2 border-b text-right">${qty}</td>
        <td class="px-3 py-2 border-b text-right">${
          lengthIn > 0 ? formatFtIn(lengthIn) : "-"
        }</td>
        <td class="px-3 py-2 border-b text-right text-gray-600">${
          stockHint || ""
        }</td>
      `;
          cutListBody.appendChild(tr);
        }

        // Suggest common stock lengths
        function suggestStock(lenIn) {
          const lenFt = lenIn / 12;
          const stock = [8, 9, 10, 12, 14, 16];
          for (let s of stock) {
            if (lenFt <= s) return s + "'";
          }
          return Math.ceil(lenFt) + "'";
        }

        addRow(
          "Common studs",
          commonStuds,
          studLengthIn,
          suggestStock(studLengthIn)
        );
        addRow(
          "King studs",
          kingStuds,
          studLengthIn,
          suggestStock(studLengthIn)
        );
        addRow(
          "Jack studs",
          jackStuds,
          studLengthIn - (wallHeightIn - (oHft * 12 || wallHeightIn)),
          suggestStock(studLengthIn)
        );
        addRow(
          "Cripple studs above opening",
          cripplesAbove,
          wallHeightIn - (oHft * 12 || wallHeightIn),
          suggestStock(wallHeightIn - (oHft * 12 || wallHeightIn))
        );
        addRow(
          "Cripple studs below opening",
          cripplesBelow,
          oHft * 12 || 0,
          suggestStock(oHft * 12 || 0)
        );
        addRow(
          "Corner/intersection studs",
          cornerStuds + intersectionStuds,
          studLengthIn,
          suggestStock(studLengthIn)
        );
        addRow(
          "Top plates (total)",
          Math.ceil(topPlatesLFWithWaste / 8),
          8 * 12,
          "8'–16' as available"
        );
        addRow(
          "Bottom plates (total)",
          Math.ceil(bottomPlatesLFWithWaste / 8),
          8 * 12,
          "8'–16' as available"
        );
        if (hasOpening && headerLengthIn > 0) {
          addRow(
            "Header members",
            2,
            headerLengthIn,
            suggestStock(headerLengthIn)
          );
        }
        if (blockingPieces > 0) {
          addRow(
            "Blocking pieces",
            blockingPieces,
            studLengthIn / 4,
            "Cut from offcuts"
          );
        }

        if (!cutListBody.children.length) {
          const tr = document.createElement("tr");
          tr.innerHTML = `<td class="px-3 py-2 text-gray-500" colspan="4">No members calculated. Check your inputs.</td>`;
          cutListBody.appendChild(tr);
        }

        updateSummary();
      }

      document
        .getElementById("calculate-btn")
        .addEventListener("click", calculateWallFraming);

      [
        "wall-length",
        "wall-length-in",
        "wall-height",
        "wall-height-in",
        "stud-spacing",
        "has-opening",
        "opening-width",
        "opening-offset",
      ].forEach((id) => {
        const el = document.getElementById(id);
        if (el) el.addEventListener("input", updateSummary);
      });

      document
        .getElementById("advanced-mode")
        .addEventListener("change", function () {
          document
            .getElementById("advanced-options")
            .classList.toggle("hidden", !this.checked);
        });

      // Initialize summary on load
      updateSummary();
    </script>

