    <script>
      // Utility functions
      function clamp(num, min, max) {
        return Math.min(Math.max(num, min), max);
      }

      function formatPercent(value) {
        if (isNaN(value)) return "–";
        return value.toFixed(2).replace(/\.00$/, "") + "%";
      }

      function percentageToLetter(pct, scale, customCutoffs) {
        if (isNaN(pct)) return "-";
        if (scale === "plusminus") {
          if (pct >= 97) return "A+";
          if (pct >= 93) return "A";
          if (pct >= 90) return "A-";
          if (pct >= 87) return "B+";
          if (pct >= 83) return "B";
          if (pct >= 80) return "B-";
          if (pct >= 77) return "C+";
          if (pct >= 73) return "C";
          if (pct >= 70) return "C-";
          if (pct >= 67) return "D+";
          if (pct >= 63) return "D";
          if (pct >= 60) return "D-";
          return "F";
        } else if (scale === "custom" && customCutoffs) {
          const A = customCutoffs.A ?? 90;
          const B = customCutoffs.B ?? 80;
          const C = customCutoffs.C ?? 70;
          const D = customCutoffs.D ?? 60;
          if (pct >= A) return "A";
          if (pct >= B) return "B";
          if (pct >= C) return "C";
          if (pct >= D) return "D";
          return "F";
        } else {
          if (pct >= 90) return "A";
          if (pct >= 80) return "B";
          if (pct >= 70) return "C";
          if (pct >= 60) return "D";
          return "F";
        }
      }

      function letterToGPA(letter) {
        switch (letter) {
          case "A+":
            return 4.0;
          case "A":
            return 4.0;
          case "A-":
            return 3.7;
          case "B+":
            return 3.3;
          case "B":
            return 3.0;
          case "B-":
            return 2.7;
          case "C+":
            return 2.3;
          case "C":
            return 2.0;
          case "C-":
            return 1.7;
          case "D+":
            return 1.3;
          case "D":
            return 1.0;
          case "D-":
            return 0.7;
          case "F":
            return 0.0;
          default:
            return NaN;
        }
      }

      function estimatePercentile(pct) {
        if (isNaN(pct)) return "–";
        // Rough mapping assuming normal distribution centered at 75 with sd ~10
        const z = (pct - 75) / 10;
        const percentile = 0.5 * (1 + erf(z / Math.SQRT2)) * 100;
        const clamped = clamp(percentile, 1, 99);
        return clamped.toFixed(0) + "th percentile (approx.)";
      }

      // Approximate error function
      function erf(x) {
        // Abramowitz and Stegun approximation
        const sign = x >= 0 ? 1 : -1;
        x = Math.abs(x);
        const a1 = 0.254829592,
          a2 = -0.284496736,
          a3 = 1.421413741;
        const a4 = -1.453152027,
          a5 = 1.061405429,
          p = 0.3275911;
        const t = 1 / (1 + p * x);
        const y =
          1 -
          ((((a5 * t + a4) * t + a3) * t + a2) * t + a1) * t * Math.exp(-x * x);
        return sign * y;
      }

      // Mode switching
      const modeButtons = document.querySelectorAll(".mode-btn");
      const singlePanel = document.getElementById("single-test-panel");
      const multiPanel = document.getElementById("multi-test-panel");

      modeButtons.forEach((btn) => {
        btn.addEventListener("click", () => {
          modeButtons.forEach((b) => {
            b.classList.remove(
              "border-blue-600",
              "text-blue-600",
              "bg-blue-50"
            );
            b.classList.add("border-gray-300", "text-gray-700", "bg-white");
          });
          btn.classList.add("border-blue-600", "text-blue-600", "bg-blue-50");
          btn.classList.remove("border-gray-300", "text-gray-700", "bg-white");

          const mode = btn.getAttribute("data-mode");
          if (mode === "single") {
            singlePanel.classList.remove("hidden");
            multiPanel.classList.add("hidden");
          } else {
            singlePanel.classList.add("hidden");
            multiPanel.classList.remove("hidden");
          }
        });
      });

      // Grading scale selection
      let currentScale = "simple";
      const scaleButtons = document.querySelectorAll(".scale-btn");
      const customScalePanel = document.getElementById("custom-scale-panel");

      scaleButtons.forEach((btn) => {
        btn.addEventListener("click", () => {
          scaleButtons.forEach((b) => {
            b.classList.remove(
              "border-blue-600",
              "text-blue-600",
              "bg-blue-50"
            );
            b.classList.add("border-gray-300", "text-gray-700", "bg-white");
          });
          btn.classList.add("border-blue-600", "text-blue-600", "bg-blue-50");
          btn.classList.remove("border-gray-300", "text-gray-700", "bg-white");
          currentScale = btn.getAttribute("data-scale");
          if (currentScale === "custom") {
            customScalePanel.classList.remove("hidden");
          } else {
            customScalePanel.classList.add("hidden");
          }
        });
      });

      // Sections handling
      const sectionsContainer = document.getElementById("sections-container");
      const addSectionBtn = document.getElementById("add-section-btn");

      function createSectionRow(index) {
        const row = document.createElement("div");
        row.className = "grid md:grid-cols-4 gap-2 items-end";
        row.innerHTML = `
      <div>
        <label class="block text-xs text-gray-600 mb-1">Section name</label>
        <input type="text" class="section-name w-full border rounded px-2 py-1 text-xs" placeholder="e.g. Reading ${
          index + 1
        }">
      </div>
      <div>
        <label class="block text-xs text-gray-600 mb-1">Points earned</label>
        <input type="number" min="0" step="0.01" class="section-earned w-full border rounded px-2 py-1 text-xs">
      </div>
      <div>
        <label class="block text-xs text-gray-600 mb-1">Total points</label>
        <input type="number" min="0.01" step="0.01" class="section-total w-full border rounded px-2 py-1 text-xs">
      </div>
      <div>
        <label class="block text-xs text-gray-600 mb-1">Weight %</label>
        <div class="flex gap-1">
          <input type="number" min="0" max="100" step="0.1" class="section-weight w-full border rounded px-2 py-1 text-xs">
          <button type="button" class="remove-section text-red-500 text-xs px-2 py-1 border border-red-200 rounded">×</button>
        </div>
      </div>
    `;
        row.querySelector(".remove-section").addEventListener("click", () => {
          row.remove();
        });
        return row;
      }

      addSectionBtn.addEventListener("click", () => {
        const index = sectionsContainer.children.length;
        sectionsContainer.appendChild(createSectionRow(index));
      });

      // Tests handling (multi-test)
      const testsContainer = document.getElementById("tests-container");
      const addTestBtn = document.getElementById("add-test-btn");

      function createTestRow(index) {
        const row = document.createElement("div");
        row.className = "grid md:grid-cols-4 gap-2 items-end";
        row.innerHTML = `
      <div>
        <label class="block text-xs text-gray-600 mb-1">Test name</label>
        <input type="text" class="test-name w-full border rounded px-2 py-1 text-xs" placeholder="e.g. Quiz ${
          index + 1
        }">
      </div>
      <div>
        <label class="block text-xs text-gray-600 mb-1">Points earned</label>
        <input type="number" min="0" step="0.01" class="test-earned w-full border rounded px-2 py-1 text-xs">
      </div>
      <div>
        <label class="block text-xs text-gray-600 mb-1">Total points</label>
        <input type="number" min="0.01" step="0.01" class="test-total w-full border rounded px-2 py-1 text-xs">
      </div>
      <div>
        <label class="block text-xs text-gray-600 mb-1">Weight %</label>
        <div class="flex gap-1">
          <input type="number" min="0" max="100" step="0.1" class="test-weight w-full border rounded px-2 py-1 text-xs">
          <button type="button" class="remove-test text-red-500 text-xs px-2 py-1 border border-red-200 rounded">×</button>
        </div>
      </div>
    `;
        row.querySelector(".remove-test").addEventListener("click", () => {
          row.remove();
        });
        return row;
      }

      // Initialize with one test row
      testsContainer.appendChild(createTestRow(0));

      addTestBtn.addEventListener("click", () => {
        const index = testsContainer.children.length;
        testsContainer.appendChild(createTestRow(index));
      });

      // Single test calculation
      const calcSingleBtn = document.getElementById("calc-single-btn");
      const singleResults = document.getElementById("single-results");
      const singlePercentageEl = document.getElementById("single-percentage");
      const singlePointsSummaryEl = document.getElementById(
        "single-points-summary"
      );
      const singleLetterEl = document.getElementById("single-letter");
      const singleGpaEl = document.getElementById("single-gpa");
      const singleContributionEl = document.getElementById(
        "single-contribution"
      );
      const singlePercentileEl = document.getElementById("single-percentile");
      const scaleMaxInput = document.getElementById("scale-max");
      const scaledScoreEl = document.getElementById("scaled-score");

      function getCustomCutoffs() {
        return {
          A: parseFloat(document.getElementById("grade-A").value),
          B: parseFloat(document.getElementById("grade-B").value),
          C: parseFloat(document.getElementById("grade-C").value),
          D: parseFloat(document.getElementById("grade-D").value),
        };
      }

      function computeSectionWeightedScore() {
        const rows = sectionsContainer.querySelectorAll("div.grid");
        if (!rows.length) return null;

        let totalWeight = 0;
        let weightedSum = 0;
        rows.forEach((row) => {
          const earned = parseFloat(row.querySelector(".section-earned").value);
          const total = parseFloat(row.querySelector(".section-total").value);
          const weight = parseFloat(row.querySelector(".section-weight").value);
          if (
            !isNaN(earned) &&
            !isNaN(total) &&
            total > 0 &&
            !isNaN(weight) &&
            weight > 0
          ) {
            const pct = (earned / total) * 100;
            weightedSum += pct * weight;
            totalWeight += weight;
          }
        });
        if (totalWeight === 0) return null;
        return weightedSum / totalWeight;
      }

      function applyCurve(rawScore, totalPoints) {
        let score = rawScore;
        const add = parseFloat(document.getElementById("curve-add").value);
        const top = parseFloat(document.getElementById("curve-top").value);

        if (!isNaN(add)) {
          score += add;
        }
        if (!isNaN(top) && top > 0 && score > 0 && totalPoints > 0) {
          const factor = totalPoints / top;
          score *= factor;
        }
        return clamp(score, 0, totalPoints);
      }

      function updateScaledScore(pct) {
        const max = parseFloat(scaleMaxInput.value);
        if (isNaN(pct) || isNaN(max) || max <= 0) {
          scaledScoreEl.textContent = "–";
          return;
        }
        const scaled = (pct / 100) * max;
        scaledScoreEl.textContent = scaled.toFixed(2).replace(/\.00$/, "");
      }

      calcSingleBtn.addEventListener("click", () => {
        const earnedInput = document.getElementById("single-earned");
        const totalInput = document.getElementById("single-total");
        const weightInput = document.getElementById("single-weight");

        let earned = parseFloat(earnedInput.value);
        let total = parseFloat(totalInput.value);

        if (isNaN(total) || total <= 0) {
          alert("Please enter a valid total points value greater than 0.");
          return;
        }

        // If sections are defined and valid, override earned/total with section-based percentage
        const sectionPct = computeSectionWeightedScore();
        let pct;
        if (sectionPct !== null) {
          pct = sectionPct;
          // Convert back to equivalent earned points for display
          earned = (pct / 100) * total;
        } else {
          if (isNaN(earned) || earned < 0) {
            alert("Please enter a valid points earned value.");
            return;
          }
          // Apply curve on raw points
          const curvedEarned = applyCurve(earned, total);
          pct = (curvedEarned / total) * 100;
          earned = curvedEarned;
        }

        pct = clamp(pct, 0, 150); // allow some extra for strong curves
        const pctText = formatPercent(pct);
        singlePercentageEl.textContent = pctText;
        singlePointsSummaryEl.textContent = `${earned
          .toFixed(2)
          .replace(/\.00$/, "")} / ${total
          .toFixed(2)
          .replace(/\.00$/, "")} points`;

        const letter = percentageToLetter(
          pct,
          currentScale,
          currentScale === "custom" ? getCustomCutoffs() : null
        );
        singleLetterEl.textContent = letter;
        const gpa = letterToGPA(letter);
        singleGpaEl.textContent = isNaN(gpa)
          ? ""
          : `Approx. GPA for this test: ${gpa.toFixed(2)}`;

        const weight = parseFloat(weightInput.value);
        if (!isNaN(weight) && weight > 0) {
          const contribution = pct * (weight / 100);
          singleContributionEl.textContent = `${contribution.toFixed(
            2
          )} percentage points toward your final grade (weight ${weight}%).`;
        } else {
          singleContributionEl.textContent =
            "Enter a weight to see how much this test affects your final grade.";
        }

        singlePercentileEl.textContent = estimatePercentile(pct);
        updateScaledScore(pct);

        singleResults.classList.remove("hidden");
      });

      scaleMaxInput.addEventListener("input", () => {
        const pctText = singlePercentageEl.textContent;
        if (!pctText || pctText === "–") {
          scaledScoreEl.textContent = "–";
          return;
        }
        const pct = parseFloat(pctText);
        updateScaledScore(pct);
      });

      // Multi-test calculation
      const calcMultiBtn = document.getElementById("calc-multi-btn");
      const multiResults = document.getElementById("multi-results");
      const multiPercentageEl = document.getElementById("multi-percentage");
      const multiLetterEl = document.getElementById("multi-letter");
      const multiGpaEl = document.getElementById("multi-gpa");
      const multiBreakdownEl = document.getElementById("multi-breakdown");

      calcMultiBtn.addEventListener("click", () => {
        const rows = testsContainer.querySelectorAll("div.grid");
        if (!rows.length) {
          alert("Please add at least one test.");
          return;
        }

        let totalWeight = 0;
        let weightedSum = 0;
        const breakdownItems = [];

        rows.forEach((row) => {
          const name = row.querySelector(".test-name").value || "Test";
          const earned = parseFloat(row.querySelector(".test-earned").value);
          const total = parseFloat(row.querySelector(".test-total").value);
          const weight = parseFloat(row.querySelector(".test-weight").value);

          if (
            !isNaN(earned) &&
            !isNaN(total) &&
            total > 0 &&
            !isNaN(weight) &&
            weight > 0
          ) {
            const pct = (earned / total) * 100;
            weightedSum += pct * weight;
            totalWeight += weight;
            breakdownItems.push({
              name,
              pct,
              weight,
            });
          }
        });

        if (totalWeight === 0) {
          alert("Please enter valid points and weights for at least one test.");
          return;
        }

        const overallPct = weightedSum / totalWeight;
        const pctText = formatPercent(overallPct);
        multiPercentageEl.textContent = pctText;

        const letter = percentageToLetter(
          overallPct,
          currentScale,
          currentScale === "custom" ? getCustomCutoffs() : null
        );
        multiLetterEl.textContent = letter;
        const gpa = letterToGPA(letter);
        multiGpaEl.textContent = isNaN(gpa) ? "–" : gpa.toFixed(2);

        multiBreakdownEl.innerHTML = "";
        breakdownItems.forEach((item) => {
          const li = document.createElement("li");
          li.textContent = `${item.name}: ${formatPercent(item.pct)} (weight ${
            item.weight
          }%)`;
          multiBreakdownEl.appendChild(li);
        });

        multiResults.classList.remove("hidden");
      });

      // Mobile menu script (basic)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const expanded = this.getAttribute("aria-expanded") === "true";
          this.setAttribute("aria-expanded", String(!expanded));
          menu.classList.toggle("hidden");
        });

      // Initialize one default section row (optional but helpful)
      // Commented out to keep UI clean by default
      // sectionsContainer.appendChild(createSectionRow(0));

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
