    <script>
      // Utility helpers
      function parseInput(id, fallback) {
        const el = document.getElementById(id);
        if (!el) return fallback;
        const v = parseFloat(el.value);
        return isFinite(v) && v > 0 ? v : fallback;
      }

      function setText(id, value, decimals = 2) {
        const el = document.getElementById(id);
        if (!el) return;
        if (value === null || !isFinite(value)) {
          el.textContent = "–";
        } else {
          el.textContent = value.toFixed(decimals);
        }
      }

      function setStatus(id, ok, warnText = "Check") {
        const el = document.getElementById(id);
        if (!el) return;
        el.classList.remove(
          "bg-green-100",
          "text-green-700",
          "bg-red-100",
          "text-red-700",
          "bg-yellow-100",
          "text-yellow-700",
          "bg-gray-100",
          "text-gray-600"
        );
        if (ok === null) {
          el.textContent = "–";
          el.classList.add("bg-gray-100", "text-gray-600");
        } else if (ok === true) {
          el.textContent = "OK";
          el.classList.add("bg-green-100", "text-green-700");
        } else if (ok === "warn") {
          el.textContent = warnText;
          el.classList.add("bg-yellow-100", "text-yellow-700");
        } else {
          el.textContent = "NG";
          el.classList.add("bg-red-100", "text-red-700");
        }
      }

      function calculateFoundation() {
        const status = document.getElementById("statusMessage");
        if (status) {
          status.textContent = "Calculating...";
        }

        const mode = document.getElementById("designMode")?.value || "gravity";

        // Inputs
        const hubHeight = parseInput("hubHeight", 90);
        const rotorDiameter = parseInput("rotorDiameter", 120);
        const V = parseInput("designWindSpeed", 50);
        const mTurbine = parseInput("turbineMass", 120); // tonnes
        const mTower = parseInput("towerMass", 200); // tonnes
        const towerCgHeight = parseInput("towerCgHeight", 45);
        const gammaF = parseInput("partialSafetyLoad", 1.35);

        const soilBearing = parseInput("soilBearing", 250); // kPa
        const mu = parseInput("soilFriction", 0.6);
        const B = parseInput("footingDiameter", 18); // m
        const h = parseInput("footingThickness", 2.5);
        const Df = parseInput("embedmentDepth", 3);
        const gammaC = parseInput("concreteDensity", 24); // kN/m3
        const fck = parseInput("concreteStrength", 35); // MPa
        const fy = parseInput("steelYield", 500); // MPa

        // Constants
        const rhoAir = 1.25; // kg/m3
        const Cd = 1.2; // drag coefficient
        const g = 9.81; // m/s2

        // 1. Wind load at hub
        const Arotor = Math.PI * Math.pow(rotorDiameter / 2, 2); // m2
        const Fwind = 0.5 * rhoAir * Cd * Arotor * V * V; // N
        const Hchar = Fwind / 1000; // kN
        const Hd = Hchar * gammaF; // design horizontal load (kN)

        // Lever arm: hub height + embedment (simplified)
        let lever = hubHeight + Df;
        if (mode === "monopile") {
          // Slightly different assumption for monopile equivalent
          lever = hubHeight + 0.5 * Df;
        }

        // 2. Vertical loads
        const Wturbine = ((mTurbine + mTower) * g) / 1000; // kN
        const Afooting = B * B; // m2 (square approx)
        const Vfooting = Afooting * h; // m3
        const Wfooting = gammaC * Vfooting; // kN
        const Nchar = Wturbine + Wfooting; // kN
        const Nd = Nchar * gammaF; // design vertical load (kN)

        // 3. Overturning
        const Mot = Hd * lever; // kNm
        const Mr = Nd * (B / 2); // kNm
        const SFot = Mr / Mot;

        // 4. Sliding
        const Rfric = mu * Nd; // kN
        const SFsl = Rfric / Hd;

        // 5. Bearing
        const e = Mot / Nd; // m
        const qAvg = Nd / Afooting; // kPa (since kN/m2)
        let qMax = qAvg * (1 + (6 * e) / B);
        let qMin = qAvg * (1 - (6 * e) / B);

        // 6. Reinforcement (very simplified)
        const d = Math.max(h - 0.1, 0.2); // effective depth (m)
        const z = 0.9 * d;
        const Med_per_m = Mot / B; // kNm/m
        const fyd = fy / 1.15; // MPa ~ N/mm2
        // Convert units: 1 kNm = 10^6 Nmm, 1 m width = 1000 mm
        const Med_Nmm = Med_per_m * 1e6;
        const z_mm = z * 1000;
        const fyd_Nmm2 = fyd;
        const As_req_mm2 = Med_Nmm / (z_mm * fyd_Nmm2);
        const b_mm = 1000;
        const d_mm = d * 1000;
        const rho_req = (As_req_mm2 / (b_mm * d_mm)) * 100; // %

        // Update UI
        setText("otMoment", Mot);
        setText("resMoment", Mr);
        setText("otSF", SFot);

        setText("slidingH", Hd);
        setText("slidingCap", Rfric);
        setText("slSF", SFsl);

        setText("qMax", qMax);
        setText("qMin", qMin);
        setText("qAllow", soilBearing);

        setText("mEd", Med_per_m);
        setText("rhoReq", rho_req, 3);
        setText("asReq", As_req_mm2 / 100, 2); // cm2/m

        // Status badges
        const otOk = SFot >= 1.5 ? true : SFot >= 1.3 ? "warn" : false;
        const slOk = SFsl >= 1.5 ? true : SFsl >= 1.3 ? "warn" : false;
        const brOk =
          qMax <= soilBearing
            ? true
            : qMax <= 1.1 * soilBearing
            ? "warn"
            : false;
        const rfOk = rho_req <= 2.0 ? true : rho_req <= 3.0 ? "warn" : false;

        setStatus("otStatus", otOk, "Low SF");
        setStatus("slStatus", slOk, "Low SF");
        setStatus("brStatus", brOk, "High q");
        setStatus("rfStatus", rfOk, "Heavy steel");

        if (status) {
          status.textContent =
            "Calculation complete. Review safety factors and bearing pressures.";
        }
      }

      document
        .getElementById("calculateBtn")
        ?.addEventListener("click", function () {
          try {
            calculateFoundation();
          } catch (e) {
            const status = document.getElementById("statusMessage");
            if (status) {
              status.textContent =
                "Error during calculation. Please check your inputs.";
            }
            console.error(e);
          }
        });

      document
        .getElementById("resetBtn")
        ?.addEventListener("click", function () {
          // Reset to default values
          document.getElementById("hubHeight").value = 90;
          document.getElementById("rotorDiameter").value = 120;
          document.getElementById("designWindSpeed").value = 50;
          document.getElementById("turbineMass").value = 120;
          document.getElementById("towerMass").value = 200;
          document.getElementById("towerCgHeight").value = 45;
          document.getElementById("partialSafetyLoad").value = 1.35;

          document.getElementById("soilBearing").value = 250;
          document.getElementById("soilFriction").value = 0.6;
          document.getElementById("footingDiameter").value = 18;
          document.getElementById("footingThickness").value = 2.5;
          document.getElementById("embedmentDepth").value = 3;
          document.getElementById("concreteDensity").value = 24;
          document.getElementById("concreteStrength").value = 35;
          document.getElementById("steelYield").value = 500;

          [
            "otMoment",
            "resMoment",
            "otSF",
            "slidingH",
            "slidingCap",
            "slSF",
            "qMax",
            "qMin",
            "qAllow",
            "mEd",
            "rhoReq",
            "asReq",
          ].forEach((id) => {
            const el = document.getElementById(id);
            if (el) el.textContent = "–";
          });
          ["otStatus", "slStatus", "brStatus", "rfStatus"].forEach((id) =>
            setStatus(id, null)
          );
          const status = document.getElementById("statusMessage");
          if (status) status.textContent = "";
        });

      // Auto-run once on load with defaults
      window.addEventListener("DOMContentLoaded", () => {
        calculateFoundation();
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
