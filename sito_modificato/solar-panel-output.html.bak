    <script>
      // Helper: clamp
      function clamp(val, min, max) {
        return Math.min(max, Math.max(min, val));
      }

      const systemSizeInput = document.getElementById("system-size");
      const systemSizeUnit = document.getElementById("system-size-unit");
      const locationPreset = document.getElementById("location-preset");
      const sunHoursInput = document.getElementById("sun-hours");
      const lossesInput = document.getElementById("losses");
      const lossesValue = document.getElementById("losses-value");
      const tiltInput = document.getElementById("tilt");
      const tiltValue = document.getElementById("tilt-value");
      const azimuthInput = document.getElementById("azimuth");
      const azimuthValue = document.getElementById("azimuth-value");
      const tempCoeffInput = document.getElementById("temp-coeff");
      const avgTempInput = document.getElementById("avg-temp");

      const dailyKwhEl = document.getElementById("daily-kwh");
      const dailyWhEl = document.getElementById("daily-wh");
      const monthlyKwhEl = document.getElementById("monthly-kwh");
      const monthlyKwh30El = document.getElementById("monthly-kwh-30");
      const yearlyKwhEl = document.getElementById("yearly-kwh");
      const specificYieldEl = document.getElementById("specific-yield");
      const effectiveKwEl = document.getElementById("effective-kw");
      const capacityFactorEl = document.getElementById("capacity-factor");
      const lossSummaryEl = document.getElementById("loss-summary");

      const calcBtn = document.getElementById("calculate-btn");
      const resetBtn = document.getElementById("reset-btn");

      function updateRangeLabels() {
        lossesValue.textContent = lossesInput.value + "%";
        tiltValue.textContent = tiltInput.value + "°";

        const az = parseInt(azimuthInput.value, 10);
        let dir = "";
        if (az === 0 || az === 360) dir = "N";
        else if (az === 90) dir = "E";
        else if (az === 180) dir = "S";
        else if (az === 270) dir = "W";
        else if (az > 0 && az < 90) dir = "NE";
        else if (az > 90 && az < 180) dir = "SE";
        else if (az > 180 && az < 270) dir = "SW";
        else if (az > 270 && az < 360) dir = "NW";
        azimuthValue.textContent = az + "° " + dir;
      }

      lossesInput.addEventListener("input", updateRangeLabels);
      tiltInput.addEventListener("input", updateRangeLabels);
      azimuthInput.addEventListener("input", updateRangeLabels);

      locationPreset.addEventListener("change", () => {
        const val = locationPreset.value;
        if (val === "custom") return;
        sunHoursInput.value = val;
      });

      function parseNumber(input, fallback) {
        const v = parseFloat(input.value);
        return isNaN(v) ? fallback : v;
      }

      function calculateSolarOutput() {
        let size = parseNumber(systemSizeInput, 0);
        if (size < 0) size = 0;

        // Convert to kW
        let sizeKw = systemSizeUnit.value === "W" ? size / 1000 : size;

        const sunHours = clamp(parseNumber(sunHoursInput, 0), 0, 24);
        const baseLossPct = clamp(parseNumber(lossesInput, 20), 0, 80) / 100;

        // Orientation factor: simple cosine-based derate around 180°
        const az = clamp(parseNumber(azimuthInput, 180), 0, 360);
        const azDiff = Math.min(Math.abs(az - 180), Math.abs(az - 540)); // wrap-around
        const azNorm = clamp(azDiff / 90, 0, 1); // 0 at 180°, 1 at 90° or 270°
        const orientationLossPct = 0.15 * azNorm; // up to 15% loss at 90° off-south
        const orientationFactor = 1 - orientationLossPct;

        // Temperature derate
        const tempCoeff = parseNumber(tempCoeffInput, -0.35) / 100; // %/°C to fraction/°C
        const deltaT = parseNumber(avgTempInput, 10);
        const tempFactor = 1 + tempCoeff * deltaT;
        const tempLossPct = 1 - tempFactor;

        // Total effective factor
        const systemFactor = 1 - baseLossPct;
        const totalFactor = systemFactor * orientationFactor * tempFactor;

        const dailyKwh = sizeKw * sunHours * totalFactor;
        const monthlyKwh = dailyKwh * 30.44; // average month
        const yearlyKwh = dailyKwh * 365;

        const specificYield = sizeKw > 0 ? yearlyKwh / sizeKw : 0;
        const effectiveKw = sizeKw * totalFactor;

        const capacityFactor =
          sizeKw > 0 ? (yearlyKwh / (sizeKw * 24 * 365)) * 100 : 0;

        // Update UI
        dailyKwhEl.textContent = dailyKwh.toFixed(1);
        dailyWhEl.textContent =
          Math.round(dailyKwh * 1000).toLocaleString() + " Wh";

        monthlyKwhEl.textContent = monthlyKwh.toFixed(0);
        monthlyKwh30El.textContent = (dailyKwh * 30).toFixed(0);

        yearlyKwhEl.textContent = yearlyKwh.toFixed(0);
        specificYieldEl.textContent = specificYield.toFixed(0);

        effectiveKwEl.textContent = effectiveKw.toFixed(2);
        capacityFactorEl.textContent = capacityFactor.toFixed(1);

        const baseLossStr = Math.round(baseLossPct * 100) + "%";
        const orientLossStr = Math.round(orientationLossPct * 100) + "%";
        const tempLossStr = Math.round(tempLossPct * 100) + "%";
        lossSummaryEl.textContent = `System: ${baseLossStr}, Orientation: ${orientLossStr}, Temperature: ${tempLossStr}`;
      }

      calcBtn.addEventListener("click", calculateSolarOutput);

      resetBtn.addEventListener("click", () => {
        systemSizeInput.value = 4;
        systemSizeUnit.value = "kW";
        locationPreset.value = "4.0";
        sunHoursInput.value = 4.0;
        lossesInput.value = 20;
        tiltInput.value = 30;
        azimuthInput.value = 180;
        tempCoeffInput.value = -0.35;
        avgTempInput.value = 10;
        updateRangeLabels();
        calculateSolarOutput();
      });

      // Initial setup
      updateRangeLabels();
      calculateSolarOutput();
    </script>

