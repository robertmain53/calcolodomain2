    <script>
      (function () {
        const FACTOR = 1055.056; // 1 Btu/s = 1055.056 W (BTU_IT)

        const btuPerSecondInput = document.getElementById("btuPerSecondInput");
        const wattsFromBtu = document.getElementById("wattsFromBtu");
        const wattsInput = document.getElementById("wattsInput");
        const btuFromWatts = document.getElementById("btuFromWatts");
        const swapButton = document.getElementById("swapButton");
        const clearButton = document.getElementById("clearButton");
        const tableBody = document.getElementById("conversionTableBody");

        function formatNumber(value, decimals) {
          if (!isFinite(value)) return "";
          return Number(value).toLocaleString(undefined, {
            maximumFractionDigits: decimals,
            minimumFractionDigits: 0,
          });
        }

        function convertBtuToWatts() {
          const val = parseFloat(btuPerSecondInput.value);
          if (isNaN(val)) {
            wattsFromBtu.value = "";
            return;
          }
          const result = val * FACTOR;
          wattsFromBtu.value = formatNumber(result, 6);
        }

        function convertWattsToBtu() {
          const val = parseFloat(wattsInput.value);
          if (isNaN(val)) {
            btuFromWatts.value = "";
            return;
          }
          const result = val / FACTOR;
          btuFromWatts.value = formatNumber(result, 6);
        }

        function swapValues() {
          const btuVal = btuPerSecondInput.value;
          const wVal = wattsInput.value;
          btuPerSecondInput.value = wVal;
          wattsInput.value = btuVal;
          convertBtuToWatts();
          convertWattsToBtu();
        }

        function clearAll() {
          btuPerSecondInput.value = "";
          wattsFromBtu.value = "";
          wattsInput.value = "";
          btuFromWatts.value = "";
          btuPerSecondInput.focus();
        }

        function buildTable() {
          const baseValues = [0.1, 0.25, 0.5, 1, 2, 5, 10];
          const fragment = document.createDocumentFragment();
          baseValues.forEach((v) => {
            const tr = document.createElement("tr");
            tr.className = "odd:bg-white even:bg-gray-50";
            const td1 = document.createElement("td");
            const td2 = document.createElement("td");
            td1.className = "px-3 py-1.5 border-b";
            td2.className = "px-3 py-1.5 border-b";
            td1.textContent = v;
            td2.textContent = formatNumber(v * FACTOR, 3);
            tr.appendChild(td1);
            tr.appendChild(td2);
            fragment.appendChild(tr);
          });
          tableBody.appendChild(fragment);
        }

        if (btuPerSecondInput && wattsFromBtu && wattsInput && btuFromWatts) {
          btuPerSecondInput.addEventListener("input", convertBtuToWatts);
          wattsInput.addEventListener("input", convertWattsToBtu);
        }
        if (swapButton) swapButton.addEventListener("click", swapValues);
        if (clearButton) clearButton.addEventListener("click", clearAll);

        buildTable();
      })();

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
