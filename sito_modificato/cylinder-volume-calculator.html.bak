    <script>
      const modeEl = document.getElementById('mode');
      const unitEl = document.getElementById('unit');
      const dimensionsEl = document.getElementById('dimensions');
      const horizontalExtra = document.getElementById('horizontal-extra');

      const resM3 = document.getElementById('res-m3');
      const resL = document.getElementById('res-l');
      const resCm3 = document.getElementById('res-cm3');
      const resFt3 = document.getElementById('res-ft3');
      const resGal = document.getElementById('res-gal');

      const resFillM3 = document.getElementById('res-fill-m3');
      const resFillL = document.getElementById('res-fill-l');
      const resFillPct = document.getElementById('res-fill-pct');

      function buildDims(mode) {
        dimensionsEl.innerHTML = '';
        const addField = (id, label, def='') => {
          const wrap = document.createElement('div');
          wrap.innerHTML = `
            <label for="${id}" class="block text-sm font-medium text-gray-700 mb-1">${label}</label>
            <input id="${id}" type="number" min="0" step="any" value="${def}" class="w-full border border-gray-300 rounded-md px-3 py-2 focus:ring-blue-500 focus:border-blue-500">
          `;
          dimensionsEl.appendChild(wrap);
        };
        if (mode === 'full') {
          addField('cyl-radius', 'Radius', '0.5');
          addField('cyl-diameter', 'Diameter (optional)', '');
          addField('cyl-height', 'Height / Length', '1.5');
          const note = document.createElement('p');
          note.className = 'text-xs text-gray-400 col-span-full';
          note.textContent = 'Tip: if you measured the cylinder outside, use inner diameter for liquid volume.';
          dimensionsEl.appendChild(note);
          horizontalExtra.classList.add('hidden');
        } else {
          addField('h-diameter', 'Diameter', '1');
          addField('h-length', 'Length', '2');
          addField('h-fill', 'Fill height', '0.3');
          const note = document.createElement('p');
          note.className = 'text-xs text-gray-400 col-span-full';
          note.textContent = 'Fill height must be between 0 and diameter.';
          dimensionsEl.appendChild(note);
          horizontalExtra.classList.remove('hidden');
        }
      }

      buildDims(modeEl.value);
      modeEl.addEventListener('change', () => buildDims(modeEl.value));

      function toMeters(value, unit) {
        const v = parseFloat(value);
        if (isNaN(v) || v < 0) return NaN;
        switch (unit) {
          case 'mm': return v / 1000;
          case 'cm': return v / 100;
          case 'm': return v;
          case 'in': return v * 0.0254;
          default: return v;
        }
      }

      function calculate() {
        const mode = modeEl.value;
        const unit = unitEl.value;

        let volume_m3 = 0;
        let fillVolume_m3 = null;
        let fillPct = null;

        if (mode === 'full') {
          const rRaw = document.getElementById('cyl-radius').value;
          const dRaw = document.getElementById('cyl-diameter').value;
          const hRaw = document.getElementById('cyl-height').value;

          const h_m = toMeters(hRaw, unit);
          if (isNaN(h_m)) return;

          let r_m = toMeters(rRaw, unit);
          if (dRaw.trim() !== '') {
            const d_m = toMeters(dRaw, unit);
            if (!isNaN(d_m)) {
              r_m = d_m / 2;
            }
          }
          if (isNaN(r_m)) return;

          volume_m3 = Math.PI * r_m * r_m * h_m;
        } else {
          const d_m = toMeters(document.getElementById('h-diameter').value, unit);
          const L_m = toMeters(document.getElementById('h-length').value, unit);
          const h_fill_m = toMeters(document.getElementById('h-fill').value, unit);
          if ([d_m, L_m, h_fill_m].some(isNaN)) return;

          const r = d_m / 2;
          const full_vol = Math.PI * r * r * L_m;
          volume_m3 = full_vol;

          const h = Math.max(0, Math.min(h_fill_m, d_m));
          // circular segment method
          let segmentArea = 0;
          if (h === 0) {
            segmentArea = 0;
          } else if (h === d_m) {
            segmentArea = Math.PI * r * r;
          } else {
            const theta = 2 * Math.acos((r - h) / r); // radians
            segmentArea = 0.5 * r * r * (theta - Math.sin(theta));
          }
          fillVolume_m3 = segmentArea * L_m;
          fillPct = (fillVolume_m3 / full_vol) * 100;
        }

        // conversions
        const LITERS_PER_M3 = 1000;
        const CM3_PER_M3 = 1_000_000;
        const FT3_PER_M3 = 35.3146667215;
        const GAL_PER_M3 = 264.172052358;

        const volL = volume_m3 * LITERS_PER_M3;
        const volCm3 = volume_m3 * CM3_PER_M3;
        const volFt3 = volume_m3 * FT3_PER_M3;
        const volGal = volume_m3 * GAL_PER_M3;

        resM3.textContent = volume_m3 > 0 ? volume_m3.toFixed(6) : '0';
        resL.textContent = volL > 0 ? volL.toFixed(2) : '0';
        resCm3.textContent = volCm3 > 0 ? volCm3.toFixed(0) : '0';
        resFt3.textContent = volFt3 > 0 ? volFt3.toFixed(4) : '0';
        resGal.textContent = volGal > 0 ? volGal.toFixed(3) : '0';

        if (mode === 'horizontal') {
          if (fillVolume_m3 !== null) {
            resFillM3.textContent = fillVolume_m3.toFixed(6);
            resFillL.textContent = (fillVolume_m3 * LITERS_PER_M3).toFixed(2);
            resFillPct.textContent = fillPct.toFixed(1) + '%';
          } else {
            resFillM3.textContent = '—';
            resFillL.textContent = '—';
            resFillPct.textContent = '—';
          }
        }
      }

      document.getElementById('calc-btn').addEventListener('click', calculate);
      calculate();
    </script>

