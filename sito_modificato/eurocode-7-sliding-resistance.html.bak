    <script>
      // Toggle passive resistance block
      const includePassive = document.getElementById("includePassive");
      const passiveBlock = document.getElementById("passiveBlock");
      includePassive.addEventListener("change", () => {
        passiveBlock.classList.toggle("hidden", !includePassive.checked);
      });

      function formatNumber(value, decimals = 2) {
        if (!isFinite(value)) return "-";
        return Number(value).toFixed(decimals);
      }

      document.getElementById("calcBtn").addEventListener("click", () => {
        const Ed = parseFloat(document.getElementById("Ed").value);
        const Vd = parseFloat(document.getElementById("Vd").value);
        const deltaDeg = parseFloat(document.getElementById("delta").value);
        const phiDeg = parseFloat(document.getElementById("phiR").value);
        const cR = parseFloat(document.getElementById("cR").value);
        const A = parseFloat(document.getElementById("A").value);
        const gammaR = parseFloat(document.getElementById("gammaR").value);
        const Rpassive = includePassive.checked
          ? parseFloat(document.getElementById("Rpassive").value || "0")
          : 0;

        const resultsCard = document.getElementById("resultsCard");

        if ([Ed, Vd, deltaDeg, phiDeg, A, gammaR].some((v) => isNaN(v))) {
          resultsCard.innerHTML =
            '<p class="text-red-600">Please fill in all required numeric fields.</p>';
          return;
        }
        if (Ed <= 0 || Vd <= 0 || A <= 0 || gammaR <= 0) {
          resultsCard.innerHTML =
            '<p class="text-red-600">E<sub>d</sub>, V<sub>d</sub>, A and γ<sub>R</sub> must be positive.</p>';
          return;
        }

        const deltaRad = (deltaDeg * Math.PI) / 180;
        const phiRad = (phiDeg * Math.PI) / 180;

        const tanDelta = Math.tan(deltaRad);
        const cotPhi = 1 / Math.tan(phiRad);

        const frictionTerm = Vd * tanDelta; // kN
        const cohesionTerm = (cR * A * cotPhi) / 1000; // cR in kPa = kN/m², multiply by A (m²) → kN; cotφ is dimensionless
        const Rk = frictionTerm + cohesionTerm + Rpassive; // "characteristic" or design before γR

        const Rd = Rk / gammaR;
        const FS = Rd / Ed;

        const passes = Rd >= Ed;
        const statusColor = passes
          ? "text-green-700 bg-green-50 border-green-200"
          : "text-red-700 bg-red-50 border-red-200";
        const statusText = passes
          ? "OK – Sliding resistance is adequate."
          : "NOT OK – Sliding resistance is insufficient.";

        resultsCard.innerHTML = `
      <div class="border ${statusColor} rounded-md p-3 mb-3">
        <p class="font-semibold mb-1">${statusText}</p>
        <p class="text-xs text-gray-600">Check: E<sub>d</sub> ≤ R<sub>d</sub></p>
      </div>
      <dl class="space-y-1">
        <div class="flex justify-between">
          <dt>Design horizontal action, E<sub>d</sub>:</dt>
          <dd><strong>${formatNumber(Ed, 2)} kN</strong></dd>
        </div>
        <div class="flex justify-between">
          <dt>Base friction term, V<sub>d</sub>·tanδ:</dt>
          <dd>${formatNumber(frictionTerm, 2)} kN</dd>
        </div>
        <div class="flex justify-between">
          <dt>Cohesion term, c<sub>R</sub>·A·cotφ<sub>R</sub>:</dt>
          <dd>${formatNumber(cohesionTerm, 2)} kN</dd>
        </div>
        <div class="flex justify-between">
          <dt>Additional resistance, R<sub>p,d</sub>:</dt>
          <dd>${formatNumber(Rpassive, 2)} kN</dd>
        </div>
        <div class="flex justify-between border-t pt-1 mt-1">
          <dt>Total before γ<sub>R</sub>, R:</dt>
          <dd>${formatNumber(Rk, 2)} kN</dd>
        </div>
        <div class="flex justify-between">
          <dt>Resistance factor, γ<sub>R</sub>:</dt>
          <dd>${formatNumber(gammaR, 3)}</dd>
        </div>
        <div class="flex justify-between">
          <dt><strong>Design sliding resistance, R<sub>d</sub>:</strong></dt>
          <dd><strong>${formatNumber(Rd, 2)} kN</strong></dd>
        </div>
        <div class="flex justify-between">
          <dt>Apparent factor of safety, FS<sub>sliding</sub> = R<sub>d</sub>/E<sub>d</sub>:</dt>
          <dd><strong>${formatNumber(FS, 2)}</strong></dd>
        </div>
      </dl>
      <p class="mt-2 text-xs text-gray-500">
        This result assumes a planar sliding surface at the base and uniform parameters along the plane.
        Always verify assumptions and partial factors against EN 1997-1 and your National Annex.
      </p>
    `;
      });

