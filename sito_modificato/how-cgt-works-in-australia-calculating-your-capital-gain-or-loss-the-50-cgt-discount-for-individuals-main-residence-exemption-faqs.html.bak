    <script>
      // Utility: format as Australian dollars
      function formatAUD(value) {
        if (!isFinite(value)) return "A$0";
        return (
          "A$" +
          value.toLocaleString("en-AU", {
            maximumFractionDigits: 2,
            minimumFractionDigits: 2,
          })
        );
      }

      function parseNumber(id) {
        const v = parseFloat(document.getElementById(id).value);
        return isNaN(v) ? 0 : v;
      }

      function calcDaysBetween(start, end) {
        if (!start || !end) return null;
        const s = new Date(start);
        const e = new Date(end);
        if (isNaN(s.getTime()) || isNaN(e.getTime())) return null;
        const diffMs = e - s;
        return diffMs <= 0 ? 0 : Math.round(diffMs / (1000 * 60 * 60 * 24));
      }

      function updatePartialResidenceVisibility() {
        const partial = document.getElementById("mainResidencePartial").checked;
        document
          .getElementById("partialResidenceWrapper")
          .classList.toggle("hidden", !partial);
        if (partial) {
          document.getElementById("mainResidenceFull").checked = false;
        }
      }

      document
        .getElementById("mainResidencePartial")
        .addEventListener("change", updatePartialResidenceVisibility);
      document
        .getElementById("mainResidenceFull")
        .addEventListener("change", function () {
          if (this.checked) {
            document.getElementById("mainResidencePartial").checked = false;
            updatePartialResidenceVisibility();
          }
        });

      document.getElementById("calcBtn").addEventListener("click", function () {
        const purchasePrice = parseNumber("purchasePrice");
        const purchaseCosts = parseNumber("purchaseCosts");
        const salePrice = parseNumber("salePrice");
        const saleCosts = parseNumber("saleCosts");
        const capitalImprovements = parseNumber("capitalImprovements");

        const purchaseDate = document.getElementById("purchaseDate").value;
        const saleDate = document.getElementById("saleDate").value;
        const residencyStatus =
          document.getElementById("residencyStatus").value;
        const marginalRate =
          parseFloat(document.getElementById("marginalRate").value) || 0;

        const held12Months = document.getElementById("held12Months").checked;
        const applyDiscount = document.getElementById("applyDiscount").checked;
        const mainResidenceFull =
          document.getElementById("mainResidenceFull").checked;
        const mainResidencePartial = document.getElementById(
          "mainResidencePartial"
        ).checked;
        const mainResidenceDays =
          parseFloat(document.getElementById("mainResidenceDays").value) || 0;

        const proceeds = Math.max(0, salePrice - saleCosts);
        const costBase = Math.max(
          0,
          purchasePrice + purchaseCosts + capitalImprovements
        );

        let grossGain = proceeds - costBase;

        // Holding period
        const totalDays = calcDaysBetween(purchaseDate, saleDate);
        const holdingPeriodEl = document.getElementById("holdingPeriod");
        if (totalDays === null) {
          holdingPeriodEl.textContent =
            "Holding period: – (enter valid purchase and sale dates)";
        } else if (totalDays <= 0) {
          holdingPeriodEl.textContent =
            "Holding period: 0 days (check that sale date is after purchase date)";
        } else {
          holdingPeriodEl.textContent =
            "Holding period: " +
            totalDays +
            " days (" +
            (totalDays / 365).toFixed(2) +
            " years approx.)";
        }

        // Apply main residence exemption
        let exemptAmount = 0;
        let taxableGain = grossGain;

        if (grossGain > 0) {
          if (mainResidenceFull) {
            exemptAmount = grossGain;
            taxableGain = 0;
          } else if (
            mainResidencePartial &&
            totalDays &&
            totalDays > 0 &&
            mainResidenceDays > 0 &&
            mainResidenceDays <= totalDays
          ) {
            const nonMainDays = Math.max(0, totalDays - mainResidenceDays);
            const taxableFraction = nonMainDays / totalDays;
            taxableGain = grossGain * taxableFraction;
            exemptAmount = grossGain - taxableGain;
          }
        }

        // 50% discount eligibility (simplified)
        let discountAmount = 0;
        let discountedGain = taxableGain;

        const eligibleForDiscount =
          applyDiscount &&
          held12Months &&
          taxableGain > 0 &&
          residencyStatus === "resident";

        if (eligibleForDiscount) {
          discountAmount = taxableGain * 0.5;
          discountedGain = taxableGain - discountAmount;
        }

        // Net capital gain cannot be negative
        let netGain = Math.max(0, discountedGain);
        let estimatedTax = 0;

        if (marginalRate > 0 && netGain > 0) {
          estimatedTax = netGain * marginalRate;
        }

        // Update UI
        document.getElementById("costBaseOut").textContent =
          formatAUD(costBase);
        document.getElementById("proceedsOut").textContent =
          formatAUD(proceeds);
        document.getElementById("grossGainOut").textContent =
          formatAUD(grossGain);
        document.getElementById("discountOut").textContent = formatAUD(
          exemptAmount + discountAmount
        );
        document.getElementById("netGainOut").textContent = formatAUD(netGain);
        document.getElementById("cgtOut").textContent = formatAUD(estimatedTax);

        // Notes
        const lossNote = document.getElementById("lossNote");
        const mainResNote = document.getElementById("mainResNote");
        const discountNote = document.getElementById("discountNote");

        lossNote.classList.toggle("hidden", !(grossGain < 0));
        mainResNote.classList.toggle(
          "hidden",
          !(mainResidenceFull || mainResidencePartial)
        );
        discountNote.classList.toggle("hidden", !eligibleForDiscount);
      });

      document
        .getElementById("resetBtn")
        .addEventListener("click", function () {
          const ids = [
            "purchaseDate",
            "saleDate",
            "purchasePrice",
            "purchaseCosts",
            "salePrice",
            "saleCosts",
            "capitalImprovements",
            "mainResidenceDays",
          ];
          ids.forEach((id) => {
            const el = document.getElementById(id);
            if (el) el.value = "";
          });

          document.getElementById("assetType").value = "property";
          document.getElementById("residencyStatus").value = "resident";
          document.getElementById("marginalRate").value = "0.325";
          document.getElementById("held12Months").checked = false;
          document.getElementById("applyDiscount").checked = false;
          document.getElementById("mainResidenceFull").checked = false;
          document.getElementById("mainResidencePartial").checked = false;
          updatePartialResidenceVisibility();

          document.getElementById("costBaseOut").textContent = "A$0";
          document.getElementById("proceedsOut").textContent = "A$0";
          document.getElementById("grossGainOut").textContent = "A$0";
          document.getElementById("discountOut").textContent = "A$0";
          document.getElementById("netGainOut").textContent = "A$0";
          document.getElementById("cgtOut").textContent = "A$0";
          document.getElementById("holdingPeriod").textContent =
            "Holding period: –";

          document.getElementById("lossNote").classList.add("hidden");
          document.getElementById("mainResNote").classList.add("hidden");
          document.getElementById("discountNote").classList.add("hidden");
        });

