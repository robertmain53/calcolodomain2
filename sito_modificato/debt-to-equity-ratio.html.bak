    <script>
      // Debt to Equity Ratio Calculator Logic
      (function () {
        const debtInput = document.getElementById("de-debt");
        const equityInput = document.getElementById("de-equity");
        const typeSelect = document.getElementById("de-type");
        const calcBtn = document.getElementById("de-calc-btn");
        const resetBtn = document.getElementById("de-reset-btn");
        const errorEl = document.getElementById("de-error");

        const ratioEl = document.getElementById("de-ratio");
        const percentEl = document.getElementById("de-percent");
        const categoryEl = document.getElementById("de-category");
        const interpEl = document.getElementById("de-interpretation");

        const scnMoreDebtEl = document.getElementById("de-scn-more-debt");
        const scnLessDebtEl = document.getElementById("de-scn-less-debt");
        const scnMoreEquityEl = document.getElementById("de-scn-more-equity");

        function formatRatio(value) {
          if (!isFinite(value)) return "–";
          if (value === 0) return "0.00x";
          if (Math.abs(value) < 0.01) return value.toExponential(2) + "x";
          return value.toFixed(2) + "x";
        }

        function formatPercent(value) {
          if (!isFinite(value)) return "–";
          return (value * 100).toFixed(1) + "%";
        }

        function classifyRatio(ratio) {
          if (!isFinite(ratio))
            return {
              label: "N/A",
              text: "Equity is zero or negative; the ratio is not meaningful. Focus on solvency and cash flow instead.",
            };
          if (ratio < 0)
            return {
              label: "Negative equity",
              text: "Shareholders’ equity is negative. This usually indicates severe financial stress or accumulated losses.",
            };
          if (ratio < 0.3)
            return {
              label: "Very low leverage",
              text: "The business is mostly equity-financed. Risk from debt is low, but you may be underusing cheap debt financing.",
            };
          if (ratio < 1.0)
            return {
              label: "Moderate leverage",
              text: "Balanced mix of debt and equity for many industries. Still, compare to sector peers and cash flow stability.",
            };
          if (ratio < 2.0)
            return {
              label: "High leverage",
              text: "Debt is significant. This can boost returns but increases sensitivity to interest rates and downturns.",
            };
          return {
            label: "Very high leverage",
            text: "The business is heavily debt-financed. Lenders and investors may see this as risky, especially if cash flows are volatile.",
          };
        }

        function updateScenarios(debt, equity) {
          if (!isFinite(debt) || !isFinite(equity) || equity === 0) {
            scnMoreDebtEl.textContent = "Enter valid debt and equity first.";
            scnLessDebtEl.textContent = "Enter valid debt and equity first.";
            scnMoreEquityEl.textContent = "Enter valid debt and equity first.";
            return;
          }

          const moreDebt = debt * 1.1;
          const lessDebt = debt * 0.9;
          const moreEquity = equity * 1.2;

          const r1 = moreDebt / equity;
          const r2 = lessDebt / equity;
          const r3 = debt / moreEquity;

          scnMoreDebtEl.textContent = `${formatRatio(r1)} (${formatPercent(
            r1
          )} of equity)`;
          scnLessDebtEl.textContent = `${formatRatio(r2)} (${formatPercent(
            r2
          )} of equity)`;
          scnMoreEquityEl.textContent = `${formatRatio(r3)} (${formatPercent(
            r3
          )} of equity)`;
        }

        function calculate() {
          errorEl.classList.add("hidden");
          errorEl.textContent = "";

          const debt = parseFloat(debtInput.value);
          const equity = parseFloat(equityInput.value);

          if (isNaN(debt) || debt < 0) {
            errorEl.textContent = "Please enter a non-negative debt amount.";
            errorEl.classList.remove("hidden");
            return;
          }
          if (isNaN(equity)) {
            errorEl.textContent = "Please enter shareholders’ equity.";
            errorEl.classList.remove("hidden");
            return;
          }
          if (equity === 0) {
            ratioEl.textContent = "∞";
            percentEl.textContent = "∞";
            categoryEl.textContent = "N/A";
            interpEl.textContent =
              "Equity is zero, so the ratio is undefined. This usually indicates extreme financial stress or a very new company.";
            updateScenarios(debt, equity);
            return;
          }

          const ratio = debt / equity;
          const cls = classifyRatio(ratio);

          ratioEl.textContent = formatRatio(ratio);
          percentEl.textContent = formatPercent(ratio);
          categoryEl.textContent = cls.label;
          interpEl.textContent =
            cls.text +
            " Always compare to industry peers and track the trend over time.";

          updateScenarios(debt, equity);
        }

        function reset() {
          debtInput.value = "";
          equityInput.value = "";
          typeSelect.value = "interest";
          errorEl.classList.add("hidden");
          errorEl.textContent = "";

          ratioEl.textContent = "–";
          percentEl.textContent = "–";
          categoryEl.textContent = "–";
          interpEl.textContent =
            "Enter debt and equity to see how leveraged the business is.";

          scnMoreDebtEl.textContent = "–";
          scnLessDebtEl.textContent = "–";
          scnMoreEquityEl.textContent = "–";
        }

        calcBtn.addEventListener("click", calculate);
        resetBtn.addEventListener("click", reset);

        // Allow Enter key to trigger calculation
        [debtInput, equityInput].forEach((el) => {
          el.addEventListener("keydown", (e) => {
            if (e.key === "Enter") {
              e.preventDefault();
              calculate();
            }
          });
        });
      })();
    </script>

