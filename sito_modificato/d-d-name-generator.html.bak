        ],
        placeSuffix: [
          "fall",
          "haven",
          "ford",
          "gate",
          "watch",
          "crest",
          "moor",
          "port",
          "wick",
        ],
      };

      function rand(arr) {
        return arr[Math.floor(Math.random() * arr.length)];
      }

      function pickAdjByTone(tone) {
        switch (tone) {
          case "good":
            return rand(wordBanks.adjGood);
          case "evil":
            return rand(wordBanks.adjEvil);
          case "whimsical":
            return rand(wordBanks.adjWhimsical);
          default:
            return rand([...wordBanks.adjGood, ...wordBanks.adjNeutral]);
        }
      }

      function buildCharacterName(
        race,
        gender,
        includeSurname,
        style,
        tone,
        classFlavor
      ) {
        const raceKey = baseData[race] ? race : "human";
        const raceData = baseData[raceKey];

        let pool;
        if (gender === "female") pool = raceData.female;
        else if (gender === "male") pool = raceData.male;
        else
          pool = [
            ...raceData.male,
            ...raceData.female,
            ...(raceData.neutral || []),
          ];

        let first = rand(pool);

        // Style tweaks
        if (style === "short") {
          first = first.replace(/[aeiou]+[^aeiou]*$/i, ""); // trim ending
          if (first.length > 5) first = first.slice(0, 5);
        } else if (style === "elaborate") {
          const extra = rand(pool)
            .toLowerCase()
            .replace(/^[aeiou]/, "");
          first = first + extra.slice(0, 3);
        }

        // Tone / alignment tweaks
        if (tone === "evil") {
          if (!/[xzkr]$/i.test(first)) first += rand(["x", "k", "r", "z"]);
        } else if (tone === "good") {
          if (!/[aeiou]$/i.test(first)) first += rand(["a", "e", "ia", "iel"]);
        }

        let surname = "";
        if (includeSurname) {
          surname = rand(raceData.surname);
        }

        // Optional class-based title
        let title = "";
        switch (classFlavor) {
          case "fighter":
            title = "the Bold";
            break;
          case "rogue":
            title = "the Shadow";
            break;
          case "wizard":
            title = "the Wise";
            break;
          case "cleric":
            title = "the Faithful";
            break;
          case "bard":
            title = "the Silver Tongue";
            break;
          case "druid":
            title = "of the Wilds";
            break;
          case "warlock":
            title = "the Hexed";
            break;
        }

        let full = first;
        if (surname) full += " " + surname;
        if (title) full += " " + title;

        return full;
      }

      function buildTavernName(tone) {
        const template = rand(tavernTemplates);
        const adj = pickAdjByTone(tone);
        const noun = rand(wordBanks.noun);
        const creature = rand(wordBanks.creature);
        const color = rand(wordBanks.color);
        const owner = buildCharacterName(
          "human",
          "any",
          false,
          "classic",
          "neutral",
          "any"
        ).split(" ")[0];

        return template
          .replace("{adj}", adj)
          .replace("{noun}", noun)
          .replace("{creature}", creature)
          .replace("{color}", color)
          .replace("{owner}", owner);
      }

      function buildGuildName(tone) {
        const template = rand(guildTemplates);
        const adj = pickAdjByTone(tone);
        const noun = rand(wordBanks.noun);
        const color = rand(wordBanks.color);
        const place = rand(wordBanks.landform);
        const collective = rand(wordBanks.collective);
        const founder = buildCharacterName(
          "human",
          "any",
          false,
          "classic",
          "neutral",
          "any"
        ).split(" ")[0];

        return template
          .replace("{adj}", adj)
          .replace("{noun}", noun)
          .replace("{color}", color)
          .replace("{place}", place)
          .replace("{collective}", collective)
          .replace("{founder}", founder);
      }

      function buildPlaceName(tone) {
        const template = rand(placeTemplates);
        const adj = pickAdjByTone(tone);
        const landform = rand(wordBanks.landform);
        const noun = rand(wordBanks.noun);
        const placePrefix = rand(wordBanks.placePrefix);
        const placeSuffix = rand(wordBanks.placeSuffix);
        const color = rand(wordBanks.color);

        return template
          .replace("{adj}", adj)
          .replace("{landform}", landform)
          .replace("{noun}", noun)
          .replace("{placePrefix}", placePrefix)
          .replace("{placeSuffix}", placeSuffix)
          .replace("{color}", color);
      }

      function generateNames() {
        const type = document.getElementById("nameType").value;
        const race = document.getElementById("race").value;
        const gender = document.getElementById("gender").value;
        const classFlavor = document.getElementById("classFlavor").value;
        const alignment = document.getElementById("alignment").value;
        const style = document.getElementById("style").value;
        const includeSurname =
          document.getElementById("includeSurname").checked;
        const countInput = document.getElementById("count");
        let count = parseInt(countInput.value, 10);

        if (isNaN(count) || count < 1) count = 1;
        if (count > 50) count = 50;
        countInput.value = count;

        const names = [];
        for (let i = 0; i < count; i++) {
          let name;
          if (type === "character") {
            const raceChoice =
              race === "any" ? rand(Object.keys(baseData)) : race;
            const genderChoice =
              gender === "any" ? rand(["male", "female", "neutral"]) : gender;
            const toneChoice =
              alignment === "any"
                ? rand(["good", "neutral", "evil", "whimsical"])
                : alignment;
            const classChoice = classFlavor === "any" ? "any" : classFlavor;
            name = buildCharacterName(
              raceChoice,
              genderChoice,
              includeSurname,
              style,
              toneChoice,
              classChoice
            );
          } else if (type === "tavern") {
            const toneChoice = alignment === "any" ? "neutral" : alignment;
            name = buildTavernName(toneChoice);
          } else if (type === "guild") {
            const toneChoice = alignment === "any" ? "neutral" : alignment;
            name = buildGuildName(toneChoice);
          } else {
            const toneChoice = alignment === "any" ? "neutral" : alignment;
            name = buildPlaceName(toneChoice);
          }
          names.push(name);
        }

        const resultsContainer = document.getElementById("resultsContainer");
        resultsContainer.textContent = names
          .map((n, idx) => `${idx + 1}. ${n}`)
          .join("\n");
        document.getElementById("resultsCount").textContent = `${
          names.length
        } name${names.length !== 1 ? "s" : ""} generated`;
      }

      function copyAll() {
        const text = document
          .getElementById("resultsContainer")
          .textContent.trim();
        if (!text) return;
        navigator.clipboard
          .writeText(text)
          .then(() => {
            const status = document.getElementById("copyStatus");
            status.classList.remove("hidden");
            setTimeout(() => status.classList.add("hidden"), 1500);
          })
          .catch(() => {});
      }

      function applyPreset(preset) {
        const nameType = document.getElementById("nameType");
        const race = document.getElementById("race");
        const gender = document.getElementById("gender");
        const classFlavor = document.getElementById("classFlavor");
        const alignment = document.getElementById("alignment");
        const style = document.getElementById("style");
        const includeSurname = document.getElementById("includeSurname");
        const count = document.getElementById("count");

        switch (preset) {
          case "heroHuman":
            nameType.value = "character";
            race.value = "human";
            gender.value = "any";
            classFlavor.value = "fighter";
            alignment.value = "good";
            style.value = "classic";
            includeSurname.checked = true;
            count.value = 10;
            break;
          case "elfWizard":
            nameType.value = "character";
            race.value = "elf";
            gender.value = "any";
            classFlavor.value = "wizard";
            alignment.value = "good";
            style.value = "elaborate";
            includeSurname.checked = true;
            count.value = 10;
            break;
          case "dwarfCleric":
            nameType.value = "character";
            race.value = "dwarf";
            gender.value = "any";
            classFlavor.value = "cleric";
            alignment.value = "neutral";
            style.value = "classic";
            includeSurname.checked = true;
            count.value = 10;
            break;
          case "tieflingWarlock":
            nameType.value = "character";
            race.value = "tiefling";
            gender.value = "any";
            classFlavor.value = "warlock";
            alignment.value = "evil";
            style.value = "elaborate";
            includeSurname.checked = true;
            count.value = 10;
            break;
          case "evilGuild":
            nameType.value = "guild";
            alignment.value = "evil";
            count.value = 10;
            break;
          case "cozyTavern":
            nameType.value = "tavern";
            alignment.value = "whimsical";
            count.value = 10;
            break;
        }

        generateNames();
      }

      document.addEventListener("DOMContentLoaded", () => {
        document
          .getElementById("generateBtn")
          .addEventListener("click", generateNames);
        document
          .getElementById("copyAllBtn")
          .addEventListener("click", copyAll);

        document.querySelectorAll("button[data-preset]").forEach((btn) => {
          btn.addEventListener("click", () =>
            applyPreset(btn.getAttribute("data-preset"))
          );
        });

        // Generate an initial set of names for UX
        generateNames();
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
