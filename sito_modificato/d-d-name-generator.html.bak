      const tavernTemplates = [
        "The {adj} {noun}",
        "The {noun} & {noun}",
        "{owner}'s {noun}",
        "The {adj} {creature}",
        "The {color} {creature}",
      ];

      const guildTemplates = [
        "Order of the {adj} {noun}",
        "The {color} {noun}",
        "{noun} of the {place}",
        "The {adj} {collective}",
        "{founder}'s {collective}",
      ];

      const placeTemplates = [
        "{adj} {landform}",
        "{color} {landform}",
        "{landform} of {noun}",
        "{placePrefix}{placeSuffix}",
      ];

      const wordBanks = {
        adjGood: [
          "Silver",
          "Golden",
          "Radiant",
          "Noble",
          "Valiant",
          "Shining",
          "Blessed",
          "Honored",
