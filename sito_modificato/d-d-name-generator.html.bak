          "Emerald",
          "Ivory",
          "Onyx",
          "Crimson",
          "Azure",
          "Sable",
          "Amber",
        ],
        collective: [
          "Company",
          "Brotherhood",
          "Circle",
          "Cabal",
          "Consortium",
          "Syndicate",
          "Legion",
          "Guild",
          "Order",
        ],
        landform: [
          "Forest",
