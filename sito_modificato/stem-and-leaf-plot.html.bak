    <script>
      // ---------- Utility functions ----------
      function slpParseNumbers(raw) {
        if (!raw) return [];
        // replace commas used as decimal separators and split on whitespace / commas / semicolons
        const cleaned = raw.replace(/;/g, " ").replace(/,/g, "."); // treat comma as decimal point
        const tokens = cleaned.split(/[\s]+/).filter(Boolean);
        const values = [];
        for (const t of tokens) {
          const num = Number(t);
          if (!Number.isFinite(num)) {
            throw new Error(
              'Non-numeric value detected: "' +
                t +
                '". Please remove or correct it.'
            );
          }
          values.push(num);
        }
        return values;
      }

      function slpRoundTo(num, decimals) {
        const f = Math.pow(10, decimals);
        return Math.round(num * f) / f;
      }

      function slpMedian(sorted) {
        const n = sorted.length;
        if (n === 0) return NaN;
        const mid = Math.floor(n / 2);
        if (n % 2 === 1) {
          return sorted[mid];
        } else {
          return (sorted[mid - 1] + sorted[mid]) / 2;
        }
      }

      function slpMode(values) {
        const freq = new Map();
        let maxCount = 0;
        for (const v of values) {
          const key = v;
          const c = (freq.get(key) || 0) + 1;
          freq.set(key, c);
          if (c > maxCount) maxCount = c;
        }
        if (maxCount <= 1) return [];
        const modes = [];
        for (const [k, c] of freq.entries()) {
          if (c === maxCount) modes.push(k);
        }
        modes.sort((a, b) => a - b);
        return modes;
      }

      function slpBuildStemLeaf(values, mode, split, leafOrder) {
        // Returns { rows: [ { stemLabel, leavesString } ], settings: {...} }
        if (values.length === 0) {
          throw new Error("Please enter at least one numeric value.");
        }

        // Copy & sort numeric values increasing
        const nums = values.slice().sort((a, b) => a - b);

        // Build mapping stem -> leaves
        const stemMap = new Map(); // key: stemNumber, value: array of leaves digits (0-9)
        for (const xRaw of nums) {
          let x = xRaw;
          if (mode === "integer") {
            // Round to nearest integer, then stem=floor(x/10), leaf = abs(x) mod 10
            const rounded = Math.round(x);
            const stem =
              rounded >= 0 ? Math.floor(rounded / 10) : Math.ceil(rounded / 10);
            const leaf = Math.abs(rounded) % 10;
            const arr = stemMap.get(stem) || [];
            arr.push(leaf);
            stemMap.set(stem, arr);
          } else {
            // decimal1: use first decimal digit as leaf
            const scaled = Math.round(x * 10); // keep one decimal
            const stemInt =
              scaled >= 0 ? Math.floor(scaled / 10) : Math.ceil(scaled / 10);
            const leaf = Math.abs(scaled) % 10;
            const arr = stemMap.get(stemInt) || [];
            arr.push(leaf);
            stemMap.set(stemInt, arr);
          }
        }

        const stems = Array.from(stemMap.keys()).sort((a, b) => a - b);
        const rows = [];

        for (const stem of stems) {
          const leaves = stemMap.get(stem).slice();
          if (leafOrder === "asc") {
            leaves.sort((a, b) => a - b);
          } else {
            leaves.sort((a, b) => b - a);
          }

          const stemLabel = String(stem);
          if (!split) {
            rows.push({
              stemLabel: stemLabel,
              leavesString: leaves.join(" "),
            });
          } else {
            const low = leaves.filter((d) => d <= 4);
            const high = leaves.filter((d) => d >= 5);
            if (low.length > 0) {
              rows.push({
                stemLabel: stemLabel,
                leavesString: low.join(" "),
              });
            }
            if (high.length > 0) {
              rows.push({
                stemLabel: stemLabel,
                leavesString: high.join(" "),
              });
            }
          }
        }

        return {
          rows,
        };
      }

      // ---------- Main UI logic ----------
      (function () {
        const inputEl = document.getElementById("stem-input");
        const errorEl = document.getElementById("stem-error");
        const resultsSection = document.getElementById("stem-results-section");
        const orderEl = document.getElementById("stem-order");
        const splitEl = document.getElementById("stem-split");

        const statCountEl = document.getElementById("stem-stat-count");
        const statMinEl = document.getElementById("stem-stat-min");
        const statMaxEl = document.getElementById("stem-stat-max");
        const statMeanEl = document.getElementById("stem-stat-mean");
        const statMedianEl = document.getElementById("stem-stat-median");
        const statModeEl = document.getElementById("stem-stat-mode");

        const settingModeEl = document.getElementById("stem-setting-mode");
        const settingSplitEl = document.getElementById("stem-setting-split");
        const settingOrderEl = document.getElementById("stem-setting-order");
        const settingRangeEl = document.getElementById("stem-setting-range");
        const tableBodyEl = document.getElementById("stem-table-body");

        function getSelectedMode() {
          const radios = document.querySelectorAll(".stem-mode-radio");
          for (const r of radios) {
            if (r.checked) return r.value;
          }
          return "integer";
        }

        function clearOutput() {
          resultsSection.classList.add("hidden");
          tableBodyEl.innerHTML = "";
          errorEl.classList.add("hidden");
          errorEl.textContent = "";
        }

        document
          .getElementById("stem-generate-btn")
          .addEventListener("click", function () {
            clearOutput();
            try {
              const raw = inputEl.value.trim();
              const values = slpParseNumbers(raw);
              if (values.length === 0) {
                throw new Error("Please enter at least one numeric value.");
              }
              if (values.length > 300) {
                throw new Error(
                  "For readability, please limit the data set to 300 values or fewer."
                );
              }

              const mode = getSelectedMode();
              const split = splitEl.checked;
              const leafOrder = orderEl.value;

              // Basic stats:
              const sorted = values.slice().sort((a, b) => a - b);
              const n = sorted.length;
              const min = sorted[0];
              const max = sorted[sorted.length - 1];
              const mean = sorted.reduce((acc, x) => acc + x, 0) / n;
              const median = slpMedian(sorted);
              const modes = slpMode(sorted);

              statCountEl.textContent = n;
              statMinEl.textContent = slpRoundTo(min, 3);
              statMaxEl.textContent = slpRoundTo(max, 3);
              statMeanEl.textContent = slpRoundTo(mean, 3);
              statMedianEl.textContent = slpRoundTo(median, 3);
              if (modes.length === 0) {
                statModeEl.textContent = "No mode";
              } else if (modes.length <= 3) {
                statModeEl.textContent = modes
                  .map((m) => slpRoundTo(m, 3))
                  .join(", ");
              } else {
                const firstThree = modes
                  .slice(0, 3)
                  .map((m) => slpRoundTo(m, 3))
                  .join(", ");
                statModeEl.textContent =
                  firstThree + " (+" + (modes.length - 3) + " more)";
              }

              settingModeEl.innerHTML =
                "<strong>Data mode:</strong> " +
                (mode === "integer"
                  ? "Integer (leaf = ones digit)"
                  : "1 decimal place (leaf = first decimal)");
              settingSplitEl.innerHTML =
                "<strong>Split stems:</strong> " +
                (split ? "Yes (0–4 / 5–9)" : "No");
              settingOrderEl.innerHTML =
                "<strong>Leaf order:</strong> " +
                (leafOrder === "asc" ? "Ascending" : "Descending");
              settingRangeEl.innerHTML =
                "<strong>Range:</strong> " +
                slpRoundTo(min, 3) +
                " to " +
                slpRoundTo(max, 3);

              const structure = slpBuildStemLeaf(
                values,
                mode,
                split,
                leafOrder
              );

              // Fill HTML table
              tableBodyEl.innerHTML = "";
              for (const row of structure.rows) {
                const tr = document.createElement("tr");
                const tdStem = document.createElement("td");
                const tdLeaf = document.createElement("td");
                tdStem.className = "stem-cell";
                tdLeaf.className = "leaf-cell";
                tdStem.textContent = row.stemLabel;
                tdLeaf.textContent = row.leavesString;
                tr.appendChild(tdStem);
                tr.appendChild(tdLeaf);
                tableBodyEl.appendChild(tr);
              }

              resultsSection.classList.remove("hidden");
            } catch (err) {
              errorEl.textContent =
                err.message ||
                "Unable to generate the stem and leaf plot. Please check your input.";
              errorEl.classList.remove("hidden");
            }
          });

        document
          .getElementById("stem-clear-btn")
          .addEventListener("click", function () {
            inputEl.value = "";
            clearOutput();
          });

        document
          .getElementById("stem-copy-btn")
          .addEventListener("click", function () {
            if (tableBodyEl.children.length === 0) return;
            let text = "Stem | Leaves\n";
            for (const tr of tableBodyEl.children) {
              const tds = tr.querySelectorAll("td");
              if (tds.length >= 2) {
                text +=
                  tds[0].textContent.trim() +
                  " | " +
                  tds[1].textContent.trim() +
                  "\n";
              }
            }
            if (navigator.clipboard && navigator.clipboard.writeText) {
              navigator.clipboard.writeText(text).catch(function () {});
            }
          });
      })();
    </script>

