    <script>
      // Simple line chart for NPV profile (no external chart library)
      function drawNPVChart(canvas, points) {
        if (!canvas || !canvas.getContext) return;
        const ctx = canvas.getContext("2d");
        const w = canvas.width;
        const h = canvas.height;
        ctx.clearRect(0, 0, w, h);

        if (!points || points.length === 0) return;

        const rates = points.map((p) => p.rate);
        const npvs = points.map((p) => p.npv);
        const minRate = Math.min(...rates);
        const maxRate = Math.max(...rates);
        const minNPV = Math.min(...npvs);
        const maxNPV = Math.max(...npvs);

        const pad = 30;
        const x0 = pad;
        const y0 = h - pad;
        const x1 = w - pad;
        const y1 = pad;

        const xScale = (r) =>
          x0 + ((r - minRate) / (maxRate - minRate || 1)) * (x1 - x0);
        const yScale = (v) =>
          y0 - ((v - minNPV) / (maxNPV - minNPV || 1)) * (y0 - y1);

        // Axes
        ctx.strokeStyle = "#9ca3af";
        ctx.lineWidth = 1;
        ctx.beginPath();
        ctx.moveTo(x0, y1);
        ctx.lineTo(x0, y0);
        ctx.lineTo(x1, y0);
        ctx.stroke();

        // Zero NPV line
        if (minNPV < 0 && maxNPV > 0) {
          const yZero = yScale(0);
          ctx.strokeStyle = "#e5e7eb";
          ctx.setLineDash([4, 4]);
          ctx.beginPath();
          ctx.moveTo(x0, yZero);
          ctx.lineTo(x1, yZero);
          ctx.stroke();
          ctx.setLineDash([]);
        }

        // Line
        ctx.strokeStyle = "#2563eb";
        ctx.lineWidth = 2;
        ctx.beginPath();
        points.forEach((p, i) => {
          const x = xScale(p.rate);
          const y = yScale(p.npv);
          if (i === 0) ctx.moveTo(x, y);
          else ctx.lineTo(x, y);
        });
        ctx.stroke();

        // Labels (min/max rate)
        ctx.fillStyle = "#6b7280";
        ctx.font = "10px Inter, system-ui, sans-serif";
        ctx.textAlign = "center";
        ctx.fillText((minRate * 100).toFixed(0) + "%", x0, y0 + 12);
        ctx.fillText((maxRate * 100).toFixed(0) + "%", x1, y0 + 12);
      }

      document.addEventListener("DOMContentLoaded", () => {
        const currencyEl = document.getElementById("currency-select");
        const discountRateEl = document.getElementById("discount-rate");
        const irrGuessEl = document.getElementById("irr-guess");
        const timingEndBtn = document.getElementById("timing-end");
        const timingBeginBtn = document.getElementById("timing-begin");
        const cashflowBody = document.getElementById("cashflow-body");
        const addRowBtn = document.getElementById("add-row-btn");
        const resetExampleBtn = document.getElementById("reset-example-btn");
        const calculateBtn = document.getElementById("calculate-btn");
        const clearBtn = document.getElementById("clear-btn");
        const resultsContainer = document.getElementById("results-container");
        const irrResultEl = document.getElementById("irr-result");
        const irrIterationsEl = document.getElementById("irr-iterations");
        const npvResultEl = document.getElementById("npv-result");
        const npvSignEl = document.getElementById("npv-sign");
        const paybackResultEl = document.getElementById("payback-result");
        const errorBox = document.getElementById("error-box");
        const warningBox = document.getElementById("warning-box");
        const periodCountBadge = document.getElementById("period-count-badge");
        const npvCanvas = document.getElementById("npv-chart");

        let timing = "end"; // 'end' or 'begin'

        function setTiming(newTiming) {
          timing = newTiming;
          if (timing === "end") {
            timingEndBtn.classList.add(
              "border-blue-600",
              "bg-blue-50",
              "text-blue-700",
              "font-medium"
            );
            timingEndBtn.classList.remove("border-gray-300", "text-gray-700");
            timingBeginBtn.classList.remove(
              "border-blue-600",
              "bg-blue-50",
              "text-blue-700",
              "font-medium"
            );
            timingBeginBtn.classList.add("border-gray-300", "text-gray-700");
          } else {
            timingBeginBtn.classList.add(
              "border-blue-600",
              "bg-blue-50",
              "text-blue-700",
              "font-medium"
            );
            timingBeginBtn.classList.remove("border-gray-300", "text-gray-700");
            timingEndBtn.classList.remove(
              "border-blue-600",
              "bg-blue-50",
              "text-blue-700",
              "font-medium"
            );
            timingEndBtn.classList.add("border-gray-300", "text-gray-700");
          }
        }

        timingEndBtn.addEventListener("click", () => setTiming("end"));
        timingBeginBtn.addEventListener("click", () => setTiming("begin"));

        function updatePeriodBadge() {
          const rows = cashflowBody.querySelectorAll("tr");
          periodCountBadge.textContent =
            rows.length + (rows.length === 1 ? " period" : " periods");
        }

        function createRow(period, amount) {
          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="px-3 py-2 border-b border-gray-100 text-gray-700">
          <input type="number" class="input-small w-20 border border-gray-200 rounded px-2 py-1 text-xs text-gray-700 period-input" value="${period}">
        </td>
        <td class="px-3 py-2 border-b border-gray-100">
          <div class="flex items-center gap-1">
            <span class="currency-symbol text-gray-500 mr-1">${currencyEl.value}</span>
            <input type="number" step="0.01" class="input-small w-full border border-gray-200 rounded px-2 py-1 text-xs cashflow-input" value="${amount}">
          </div>
        </td>
        <td class="px-3 py-2 border-b border-gray-100 text-gray-700">
          <span class="cumulative-cell text-xs">–</span>
        </td>
        <td class="px-3 py-2 border-b border-gray-100 text-right">
          <button type="button" class="text-xs text-red-600 hover:underline remove-row-btn">Remove</button>
        </td>
      `;
          cashflowBody.appendChild(tr);
          updatePeriodBadge();
        }

        function refreshCurrencySymbols() {
          const symbol = currencyEl.value;
          document
            .querySelectorAll(".currency-symbol")
            .forEach((el) => (el.textContent = symbol));
        }

        currencyEl.addEventListener("change", () => {
          refreshCurrencySymbols();
          if (resultsContainer.classList.contains("hidden") === false) {
            // Re-render NPV result with new symbol
            calculate();
          }
        });

        cashflowBody.addEventListener("click", (e) => {
          if (e.target.classList.contains("remove-row-btn")) {
            const rows = cashflowBody.querySelectorAll("tr");
            if (rows.length > 1) {
              e.target.closest("tr").remove();
              updatePeriodBadge();
            }
          }
        });

        addRowBtn.addEventListener("click", () => {
          const rows = cashflowBody.querySelectorAll("tr");
          const lastPeriod = rows.length
            ? parseInt(
                rows[rows.length - 1].querySelector(".period-input").value ||
                  rows.length - 1,
                10
              )
            : 0;
          createRow(lastPeriod + 1, 0);
        });

        clearBtn.addEventListener("click", () => {
          cashflowBody.innerHTML = "";
          createRow(0, -1000);
          createRow(1, 300);
          createRow(2, 400);
          createRow(3, 500);
          resultsContainer.classList.add("hidden");
          errorBox.classList.add("hidden");
          warningBox.classList.add("hidden");
          updateCumulative();
          refreshCurrencySymbols();
        });

        resetExampleBtn.addEventListener("click", () => {
          cashflowBody.innerHTML = "";
          createRow(0, -1000);
          createRow(1, 300);
          createRow(2, 400);
          createRow(3, 500);
          updateCumulative();
          refreshCurrencySymbols();
          resultsContainer.classList.add("hidden");
          errorBox.classList.add("hidden");
          warningBox.classList.add("hidden");
        });

        function getCashflows() {
          const rows = Array.from(cashflowBody.querySelectorAll("tr"));
          const flows = [];
          for (const row of rows) {
            const period = parseFloat(row.querySelector(".period-input").value);
            const amount = parseFloat(
              row.querySelector(".cashflow-input").value
            );
            if (!isFinite(period) || !isFinite(amount)) continue;
            flows.push({ t: period, c: amount });
          }
          flows.sort((a, b) => a.t - b.t);
          return flows;
        }

        function updateCumulative() {
          const flows = getCashflows();
          let cum = 0;
          const map = new Map();
          flows.forEach((f) => {
            cum += f.c;
            map.set(f.t, cum);
          });
          Array.from(cashflowBody.querySelectorAll("tr")).forEach((row) => {
            const t = parseFloat(row.querySelector(".period-input").value);
            const cumCell = row.querySelector(".cumulative-cell");
            const val = map.get(t);
            if (val === undefined) {
              cumCell.textContent = "–";
            } else {
              cumCell.textContent =
                (val >= 0 ? "" : "") +
                (Math.round(val * 100) / 100).toLocaleString();
            }
          });
        }

        cashflowBody.addEventListener("input", (e) => {
          if (
            e.target.classList.contains("cashflow-input") ||
            e.target.classList.contains("period-input")
          ) {
            updateCumulative();
          }
        });

        function npv(rate, flows) {
          let v = 0;
          for (const f of flows) {
            const t = timing === "end" ? f.t : f.t - 1;
            v += f.c / Math.pow(1 + rate, t);
          }
          return v;
        }

        function countSignChanges(flows) {
          const nonZero = flows.map((f) => f.c).filter((c) => c !== 0);
          let changes = 0;
          for (let i = 1; i < nonZero.length; i++) {
            if (nonZero[i - 1] * nonZero[i] < 0) changes++;
          }
          return changes;
        }

        function findIRR(flows, guessRate) {
          const maxIter = 100;
          const tol = 1e-7;
          let low = -0.9999;
          let high = 10.0; // 1000%
          let fLow = npv(low, flows);
          let fHigh = npv(high, flows);

          // Expand bounds if needed
          let expandIter = 0;
          while (fLow * fHigh > 0 && expandIter < 20) {
            low = Math.max(-0.9999, low - 0.5);
            high = high + 0.5;
            fLow = npv(low, flows);
            fHigh = npv(high, flows);
            expandIter++;
          }
          if (fLow * fHigh > 0) {
            return { rate: null, iterations: 0 };
          }

          let a = low,
            b = high,
            fa = fLow,
            fb = fHigh;
          let iter = 0;
          while (iter < maxIter) {
            const mid = (a + b) / 2;
            const fm = npv(mid, flows);
            if (Math.abs(fm) < tol || (b - a) / 2 < tol) {
              return { rate: mid, iterations: iter + 1 };
            }
            if (fa * fm <= 0) {
              b = mid;
              fb = fm;
            } else {
              a = mid;
              fa = fm;
            }
            iter++;
          }
          return { rate: (a + b) / 2, iterations: iter };
        }

        function computePayback(flows) {
          let cum = 0;
          let lastT = flows.length ? flows[0].t : 0;
          for (const f of flows) {
            const prevCum = cum;
            cum += f.c;
            if (prevCum < 0 && cum >= 0) {
              const dt = f.t - lastT || 1;
              const frac = prevCum !== cum ? -prevCum / (cum - prevCum) : 0;
              return lastT + frac * dt;
            }
            lastT = f.t;
          }
          return null;
        }

        function formatPercent(x) {
          if (x === null || !isFinite(x)) return "–";
          return (x * 100).toFixed(2) + "%";
        }

        function formatCurrency(x) {
          const symbol = currencyEl.value || "";
          if (x === null || !isFinite(x)) return symbol + "0";
          const abs = Math.abs(x);
          const formatted = abs.toLocaleString(undefined, {
            maximumFractionDigits: 2,
            minimumFractionDigits: 2,
          });
          return (x < 0 ? "-" : "") + symbol + formatted;
        }

        function calculate() {
          errorBox.classList.add("hidden");
          warningBox.classList.add("hidden");
          const flows = getCashflows();
          if (flows.length < 2) {
            errorBox.textContent =
              "Please enter at least two cash flows (an initial investment and at least one return).";
            errorBox.classList.remove("hidden");
            resultsContainer.classList.add("hidden");
            return;
          }

          const signChanges = countSignChanges(flows);
          if (signChanges === 0) {
            errorBox.textContent =
              "Cash flows never change sign. IRR is not defined (NPV is always positive or always negative).";
            errorBox.classList.remove("hidden");
            resultsContainer.classList.add("hidden");
            return;
          }

          const guess = parseFloat(irrGuessEl.value);
          const { rate, iterations } = findIRR(
            flows,
            isFinite(guess) ? guess / 100 : 0.1
          );
          if (rate === null || !isFinite(rate)) {
            errorBox.textContent =
              "Unable to find an IRR for the given cash flows. Try adjusting the cash flows or timing.";
            errorBox.classList.remove("hidden");
            resultsContainer.classList.add("hidden");
            return;
          }

          const disc = parseFloat(discountRateEl.value);
          const k = isFinite(disc) ? disc / 100 : 0.0;
          const npvVal = npv(k, flows);
          const payback = computePayback(flows);

          irrResultEl.textContent = formatPercent(rate);
          irrIterationsEl.textContent = `Found in ${iterations} iterations`;
          npvResultEl.textContent = formatCurrency(npvVal);
          npvSignEl.textContent =
            npvVal > 0
              ? "NPV > 0: project adds value at this discount rate."
              : npvVal < 0
              ? "NPV < 0: project destroys value at this discount rate."
              : "NPV = 0: discount rate equals IRR.";
          if (payback === null) {
            paybackResultEl.textContent = "No payback";
          } else {
            paybackResultEl.textContent = payback.toFixed(2) + " periods";
          }

          if (signChanges > 1) {
            warningBox.textContent =
              "Warning: your cash flow pattern has multiple sign changes. This can lead to multiple IRRs or ambiguous results. Always check NPV at your required return and consider using MIRR.";
            warningBox.classList.remove("hidden");
          }

          // Build simple NPV profile around IRR
          const points = [];
          const center = rate;
          const span = Math.max(Math.abs(center) * 1.5, 0.05);
          const rMin = Math.max(-0.9, center - span);
          const rMax = center + span;
          const steps = 20;
          for (let i = 0; i <= steps; i++) {
            const r = rMin + (rMax - rMin) * (i / steps);
            points.push({ rate: r, npv: npv(r, flows) });
          }
          drawNPVChart(npvCanvas, points);

          resultsContainer.classList.remove("hidden");
        }

        calculateBtn.addEventListener("click", () => {
          updateCumulative();
          calculate();
        });

        // Initialize with example
        clearBtn.click();
      });
    </script>

