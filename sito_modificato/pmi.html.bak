    <script>
      // Sync down payment amount and percent
      function syncDownPaymentFromAmount() {
        const price =
          parseFloat(document.getElementById("homePrice").value) || 0;
        const dpAmt =
          parseFloat(document.getElementById("downPaymentAmount").value) || 0;
        const percentInput = document.getElementById("downPaymentPercent");
        if (price > 0) {
          percentInput.value = ((dpAmt / price) * 100).toFixed(2);
        } else {
          percentInput.value = "0";
        }
      }

      function syncDownPaymentFromPercent() {
        const price =
          parseFloat(document.getElementById("homePrice").value) || 0;
        const dpPct =
          parseFloat(document.getElementById("downPaymentPercent").value) || 0;
        const amountInput = document.getElementById("downPaymentAmount");
        amountInput.value = ((dpPct / 100) * price).toFixed(0);
      }

      document
        .getElementById("downPaymentAmount")
        .addEventListener("input", syncDownPaymentFromAmount);
      document
        .getElementById("homePrice")
        .addEventListener("input", syncDownPaymentFromAmount);
      document
        .getElementById("downPaymentPercent")
        .addEventListener("input", syncDownPaymentFromPercent);

      function formatCurrency(value) {
        if (!isFinite(value)) return "$0";
        return (
          "$" + value.toLocaleString(undefined, { maximumFractionDigits: 0 })
        );
      }

      function formatCurrency2(value) {
        if (!isFinite(value)) return "$0";
        return (
          "$" +
          value.toLocaleString(undefined, {
            minimumFractionDigits: 0,
            maximumFractionDigits: 0,
          })
        );
      }

      function calculateMonthlyPayment(loanAmount, annualRate, years) {
        const n = years * 12;
        const r = annualRate / 100 / 12;
        if (r === 0) return loanAmount / n;
        return (
          (loanAmount * (r * Math.pow(1 + r, n))) / (Math.pow(1 + r, n) - 1)
        );
      }

      function simulatePmiEnd(
        loanAmount,
        annualRate,
        years,
        extraMonthly,
        lumpSum,
        homePrice,
        targetLtv
      ) {
        const monthlyRate = annualRate / 100 / 12;
        const n = years * 12;
        let balance = loanAmount;
        let month = 0;

        // Apply lump sum at month 1 if provided
        if (lumpSum > 0) {
          balance = Math.max(0, balance - lumpSum);
        }

        const basePayment = calculateMonthlyPayment(
          loanAmount,
          annualRate,
          years
        );

        while (month < n && balance > 0) {
          month++;
          const interest = balance * monthlyRate;
          let principal = basePayment - interest + extraMonthly;
          if (principal < 0) principal = 0;
          balance = balance - principal;
          if (balance < 0) balance = 0;

          const ltv = balance / homePrice;
          if (ltv <= targetLtv) {
            return month;
          }
        }
        return null; // never reaches target within term
      }

      function calculatePmi() {
        const price =
          parseFloat(document.getElementById("homePrice").value) || 0;
        const dpAmt =
          parseFloat(document.getElementById("downPaymentAmount").value) || 0;
        const rate =
          parseFloat(document.getElementById("interestRate").value) || 0;
        const years =
          parseInt(document.getElementById("loanTerm").value, 10) || 30;
        const pmiRate =
          parseFloat(document.getElementById("pmiRate").value) || 0;
        const extraMonthly =
          parseFloat(document.getElementById("extraMonthly").value) || 0;
        const lumpSum =
          parseFloat(document.getElementById("lumpSum").value) || 0;

        const loanAmount = Math.max(0, price - dpAmt);
        const ltv = price > 0 ? loanAmount / price : 0;

        const loanAmountDisplay = document.getElementById("loanAmountDisplay");
        const ltvDisplay = document.getElementById("ltvDisplay");
        const piPaymentDisplay = document.getElementById("piPaymentDisplay");
        const monthlyPmiDisplay = document.getElementById("monthlyPmiDisplay");
        const totalPmiDisplay = document.getElementById("totalPmiDisplay");
        const pmiEndDateDisplay = document.getElementById("pmiEndDateDisplay");
        const pmiEndDateExtraDisplay = document.getElementById(
          "pmiEndDateExtraDisplay"
        );
        const monthsSavedDisplay =
          document.getElementById("monthsSavedDisplay");
        const pmiSavedDisplay = document.getElementById("pmiSavedDisplay");

        if (loanAmount <= 0 || price <= 0 || rate <= 0 || years <= 0) {
          loanAmountDisplay.textContent = "$0";
          ltvDisplay.textContent = "0%";
          piPaymentDisplay.textContent = "$0";
          monthlyPmiDisplay.textContent = "$0";
          totalPmiDisplay.textContent = "$0";
          pmiEndDateDisplay.textContent = "–";
          pmiEndDateExtraDisplay.textContent = "–";
          monthsSavedDisplay.textContent = "–";
          pmiSavedDisplay.textContent = "–";
          return;
        }

        loanAmountDisplay.textContent = formatCurrency2(loanAmount);
        ltvDisplay.textContent = (ltv * 100).toFixed(1) + "%";

        const basePayment = calculateMonthlyPayment(loanAmount, rate, years);
        piPaymentDisplay.textContent = formatCurrency2(basePayment);

        if (ltv <= 0.8 || pmiRate <= 0) {
          monthlyPmiDisplay.textContent = "$0";
          totalPmiDisplay.textContent = "$0";
          pmiEndDateDisplay.textContent = "Not required (LTV ≤ 80%)";
          pmiEndDateExtraDisplay.textContent = "Not applicable";
          monthsSavedDisplay.textContent = "0";
          pmiSavedDisplay.textContent = "$0";
          return;
        }

        const annualPmi = loanAmount * (pmiRate / 100);
        const monthlyPmi = annualPmi / 12;
        monthlyPmiDisplay.textContent = formatCurrency2(monthlyPmi);

        // Base scenario: no extra payments
        const baseMonth78 = simulatePmiEnd(
          loanAmount,
          rate,
          years,
          0,
          0,
          price,
          0.78
        );
        const baseMonth80 = simulatePmiEnd(
          loanAmount,
          rate,
          years,
          0,
          0,
          price,
          0.8
        );

        if (!baseMonth78) {
          pmiEndDateDisplay.textContent = "Never reaches 78% LTV within term";
          totalPmiDisplay.textContent = "–";
        } else {
          pmiEndDateDisplay.textContent =
            "Month " + baseMonth78 + " (auto cancel at 78% LTV)";
          const totalPmi = monthlyPmi * baseMonth78;
          totalPmiDisplay.textContent = formatCurrency2(totalPmi);
        }

        // Extra payments scenario
        if (extraMonthly > 0 || lumpSum > 0) {
          const extraMonth78 = simulatePmiEnd(
            loanAmount,
            rate,
            years,
            extraMonthly,
            lumpSum,
            price,
            0.78
          );
          if (!extraMonth78 || !baseMonth78) {
            pmiEndDateExtraDisplay.textContent = "–";
            monthsSavedDisplay.textContent = "–";
            pmiSavedDisplay.textContent = "–";
          } else {
            pmiEndDateExtraDisplay.textContent = "Month " + extraMonth78;
            const monthsSaved = Math.max(0, baseMonth78 - extraMonth78);
            monthsSavedDisplay.textContent = monthsSaved.toString();
            const pmiSaved = monthlyPmi * monthsSaved;
            pmiSavedDisplay.textContent = formatCurrency2(pmiSaved);
          }
        } else {
          pmiEndDateExtraDisplay.textContent = "No extra payments entered";
          monthsSavedDisplay.textContent = "0";
          pmiSavedDisplay.textContent = "$0";
        }
      }

      document
        .getElementById("calculateBtn")
        .addEventListener("click", calculatePmi);

      // Auto-calc on load
      window.addEventListener("DOMContentLoaded", () => {
        syncDownPaymentFromAmount();
        calculatePmi();
      });

      // Simple FAQ toggle (in addition to global script_faq if present)
      document
        .querySelectorAll("#faq-container .faq-question")
        .forEach((btn) => {
          btn.addEventListener("click", () => {
            const answer = btn.parentElement.querySelector(".faq-answer");
            const isHidden = answer.classList.contains("hidden");
            document
              .querySelectorAll("#faq-container .faq-answer")
              .forEach((a) => a.classList.add("hidden"));
            document
              .querySelectorAll("#faq-container .faq-question span:last-child")
              .forEach((icon) => (icon.textContent = "+"));
            if (isHidden) {
              answer.classList.remove("hidden");
              btn.querySelector("span:last-child").textContent = "−";
            }
