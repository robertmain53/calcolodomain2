    <script>
      // Utility: format currency
      function getCurrencySymbol() {
        const currency = document.getElementById("currency").value;
        if (currency === "USD") return "$";
        if (currency === "EUR") return "€";
        if (currency === "GBP") return "£";
        if (currency === "INR") return "₹";
        if (currency === "JPY") return "¥";
        if (currency === "custom") {
          const custom = document.getElementById("customCurrency").value.trim();
          return custom || "";
        }
        return "";
      }

      function formatCurrency(value) {
        const symbol = getCurrencySymbol();
        const absVal = Math.abs(value);
        const formatted = absVal.toLocaleString(undefined, {
          maximumFractionDigits: 2,
          minimumFractionDigits: 2,
        });
        return (value < 0 ? "-" : "") + symbol + formatted;
      }

      function formatNumber(value, decimals = 4) {
        return Number(value).toLocaleString(undefined, {
          minimumFractionDigits: decimals,
          maximumFractionDigits: decimals,
        });
      }

      // Cash flow rows management
      let rowCounter = 0;

      function createRow(time, amount, type) {
        rowCounter++;
        const tbody = document.getElementById("cashFlowBody");
        const tr = document.createElement("tr");
        tr.dataset.index = rowCounter;

        tr.innerHTML = `
      <td class="px-3 py-2 text-gray-700">${rowCounter}</td>
      <td class="px-3 py-2">
        <input type="number" step="0.0001" value="${time}" class="time-input w-24 border border-gray-300 rounded-md px-2 py-1 text-sm focus:ring-2 focus:ring-blue-500 focus:border-transparent" />
      </td>
      <td class="px-3 py-2">
        <input type="number" step="0.01" value="${amount}" class="amount-input w-32 border border-gray-300 rounded-md px-2 py-1 text-sm focus:ring-2 focus:ring-blue-500 focus:border-transparent" />
      </td>
      <td class="px-3 py-2">
        <span class="inline-flex items-center px-2 py-0.5 rounded-full text-xs font-medium ${
          amount < 0
            ? "bg-red-50 text-red-700"
            : "bg-emerald-50 text-emerald-700"
        } type-badge">
          ${amount < 0 ? "Outflow" : "Inflow"}
        </span>
      </td>
      <td class="px-3 py-2 text-right">
        <button type="button" class="remove-row text-xs text-red-600 hover:underline">Remove</button>
      </td>
    `;

        tbody.appendChild(tr);
      }

      function resetRows() {
        const tbody = document.getElementById("cashFlowBody");
        tbody.innerHTML = "";
        rowCounter = 0;
        // Default example-like structure
        createRow(0, -10000, "outflow");
        createRow(1, 4000, "inflow");
        createRow(2, 4000, "inflow");
        createRow(3, 4000, "inflow");
      }

      function loadExample() {
        document.getElementById("discountRate").value = 8;
        document.getElementById("compounding").value = "annual";
        document.getElementById("customPeriodWrapper").classList.add("hidden");
        const tbody = document.getElementById("cashFlowBody");
        tbody.innerHTML = "";
        rowCounter = 0;
        createRow(0, -10000, "outflow");
        createRow(1, 4000, "inflow");
        createRow(2, 4000, "inflow");
        createRow(3, 4000, "inflow");
      }

      // NPV calculation
      function getPeriodLengthYears() {
        const comp = document.getElementById("compounding").value;
        if (comp === "annual") return 1;
        if (comp === "semiannual") return 0.5;
        if (comp === "quarterly") return 0.25;
        if (comp === "monthly") return 1 / 12;
        if (comp === "custom") {
          const val = parseFloat(document.getElementById("customPeriod").value);
          return isNaN(val) || val <= 0 ? 1 : val;
        }
        return 1;
      }

      function calculateNPV() {
        const errorBox = document.getElementById("errorBox");
        errorBox.classList.add("hidden");
        errorBox.textContent = "";

        const rateInput = parseFloat(
          document.getElementById("discountRate").value
        );
        if (isNaN(rateInput)) {
          errorBox.textContent = "Please enter a valid discount rate.";
          errorBox.classList.remove("hidden");
          return;
        }
        const r = rateInput / 100;

        const periodLength = getPeriodLengthYears();

        const rows = Array.from(document.querySelectorAll("#cashFlowBody tr"));
        if (rows.length === 0) {
          errorBox.textContent = "Please add at least one cash flow.";
          errorBox.classList.remove("hidden");
          return;
        }

        const cashFlows = [];
        let hasError = false;

        rows.forEach((row) => {
          const timeInput = row.querySelector(".time-input");
          const amountInput = row.querySelector(".amount-input");
          timeInput.classList.remove("input-error");
          amountInput.classList.remove("input-error");

          const tYears = parseFloat(timeInput.value);
          const cf = parseFloat(amountInput.value);

          if (isNaN(tYears) || tYears < 0) {
            timeInput.classList.add("input-error");
            hasError = true;
          }
          if (isNaN(cf)) {
            amountInput.classList.add("input-error");
            hasError = true;
          }

          if (!hasError) {
            cashFlows.push({ tYears, cf });
          }
        });

        if (hasError) {
          errorBox.textContent = "Please correct highlighted cash flow fields.";
          errorBox.classList.remove("hidden");
          return;
        }

        // Sort by time
        cashFlows.sort((a, b) => a.tYears - b.tYears);

        let npv = 0;
        let totalCash = 0;
        let cumulativePV = 0;
        const dcfRows = [];
        const periodPV = [];

        cashFlows.forEach((item, idx) => {
          const tPeriods = item.tYears / periodLength;
          const df = Math.pow(1 + r, tPeriods);
          const pv = item.cf / df;
          npv += pv;
          totalCash += item.cf;
          cumulativePV += pv;
          dcfRows.push({
            index: idx + 1,
            tYears: item.tYears,
            cf: item.cf,
            df,
            pv,
            cumulativePV,
          });
          periodPV.push({ tYears: item.tYears, pv });
        });

        // Update results
        document.getElementById("resultsSection").classList.remove("hidden");
        document.getElementById("npvResult").textContent = formatCurrency(npv);
        document.getElementById("totalCashResult").textContent =
          formatCurrency(totalCash);

        // Decision text
        const decisionEl = document.getElementById("decisionText");
        if (npv > 0) {
          decisionEl.textContent =
            "NPV is positive. The project is expected to create value above the chosen discount rate.";
        } else if (Math.abs(npv) < 1e-6) {
          decisionEl.textContent =
            "NPV is approximately zero. The project is expected to earn about your required rate of return.";
        } else {
          decisionEl.textContent =
            "NPV is negative. The project is expected to destroy value relative to the chosen discount rate.";
        }

        // Discounted payback (approximate)
        let cumulative = 0;
        let paybackYears = null;
        for (let i = 0; i < dcfRows.length; i++) {
          const row = dcfRows[i];
          const prevCumulative = cumulative;
          cumulative += row.pv;
          if (prevCumulative < 0 && cumulative >= 0) {
            const needed = -prevCumulative;
            const fraction = row.pv !== 0 ? needed / row.pv : 0;
            const deltaT =
              i === 0 ? row.tYears : row.tYears - dcfRows[i - 1].tYears;
            const approxTime =
              (i === 0 ? 0 : dcfRows[i - 1].tYears) + fraction * deltaT;
            paybackYears = approxTime;
            break;
          }
        }

        const paybackEl = document.getElementById("paybackText");
        if (paybackYears === null) {
          paybackEl.textContent =
            "Discounted payback period not reached (cumulative discounted cash flow stays negative).";
        } else if (paybackYears === 0) {
          paybackEl.textContent =
            "Discounted payback occurs immediately (at time 0).";
        } else {
          paybackEl.textContent = `Approximate discounted payback period: ${formatNumber(
            paybackYears,
            2
          )} years.`;
        }

        // Fill discounted cash flow table
        const dcfBody = document.getElementById("dcfTableBody");
        dcfBody.innerHTML = "";
        dcfRows.forEach((row) => {
          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="px-3 py-1.5 text-gray-700">${row.index}</td>
        <td class="px-3 py-1.5">${formatNumber(row.tYears, 4)}</td>
        <td class="px-3 py-1.5">${formatCurrency(row.cf)}</td>
        <td class="px-3 py-1.5">${formatNumber(1 / row.df, 6)}</td>
        <td class="px-3 py-1.5">${formatCurrency(row.pv)}</td>
        <td class="px-3 py-1.5">${formatCurrency(row.cumulativePV)}</td>
      `;
          dcfBody.appendChild(tr);
        });
      }

      function updateTypeBadges() {
        document.querySelectorAll("#cashFlowBody tr").forEach((row) => {
          const amountInput = row.querySelector(".amount-input");
          const badge = row.querySelector(".type-badge");
          const val = parseFloat(amountInput.value);
          if (!badge) return;
          if (!isNaN(val) && val < 0) {
            badge.textContent = "Outflow";
            badge.className =
              "inline-flex items-center px-2 py-0.5 rounded-full text-xs font-medium bg-red-50 text-red-700 type-badge";
          } else {
            badge.textContent = "Inflow";
            badge.className =
              "inline-flex items-center px-2 py-0.5 rounded-full text-xs font-medium bg-emerald-50 text-emerald-700 type-badge";
          }
        });
      }

      function exportCsv() {
        const rows = Array.from(document.querySelectorAll("#cashFlowBody tr"));
        if (rows.length === 0) return;

        let csv = "Index,Time (years),Cash flow\n";
        rows.forEach((row, idx) => {
          const t = row.querySelector(".time-input").value;
          const cf = row.querySelector(".amount-input").value;
          csv += `${idx + 1},${t},${cf}\n`;
        });

        const blob = new Blob([csv], { type: "text/csv;charset=utf-8;" });
        const url = URL.createObjectURL(blob);
        const a = document.createElement("a");
        a.href = url;
        a.download = "npv-cash-flows.csv";
        document.body.appendChild(a);
        a.click();
        document.body.removeChild(a);
        URL.revokeObjectURL(url);
      }

      document.addEventListener("DOMContentLoaded", () => {
        // Initialize default rows
        resetRows();

        document.getElementById("addRowBtn").addEventListener("click", () => {
          const lastRow = document.querySelector("#cashFlowBody tr:last-child");
          let nextTime = 0;
          if (lastRow) {
            const lastTime = parseFloat(
              lastRow.querySelector(".time-input").value
            );
            const periodLength = getPeriodLengthYears();
            nextTime = isNaN(lastTime) ? 0 : lastTime + periodLength;
          }
          createRow(nextTime, 0, "inflow");
        });

        document
          .getElementById("resetRowsBtn")
          .addEventListener("click", resetRows);
        document
          .getElementById("exampleBtn")
          .addEventListener("click", loadExample);
        document
          .getElementById("calculateBtn")
          .addEventListener("click", calculateNPV);
        document
          .getElementById("exportCsvBtn")
          .addEventListener("click", exportCsv);

        document
          .getElementById("cashFlowBody")
          .addEventListener("click", (e) => {
            if (e.target.classList.contains("remove-row")) {
              const row = e.target.closest("tr");
              row.remove();
            }
          });

        document
          .getElementById("cashFlowBody")
          .addEventListener("input", (e) => {
            if (e.target.classList.contains("amount-input")) {
              updateTypeBadges();
            }
          });

        document
          .getElementById("compounding")
          .addEventListener("change", () => {
            const comp = document.getElementById("compounding").value;
            const wrapper = document.getElementById("customPeriodWrapper");
            if (comp === "custom") {
              wrapper.classList.remove("hidden");
            } else {
              wrapper.classList.add("hidden");
            }
          });

        document.getElementById("currency").addEventListener("change", () => {
          const customInput = document.getElementById("customCurrency");
          if (document.getElementById("currency").value === "custom") {
            customInput.classList.remove("hidden");
          } else {
            customInput.classList.add("hidden");
          }
        });

        document
          .getElementById("customCurrency")
          .addEventListener("input", () => {
            // Recalculate to refresh formatting if results are visible
            if (
              !document
                .getElementById("resultsSection")
                .classList.contains("hidden")
            ) {
              calculateNPV();
            }
          });
      });
    </script>

