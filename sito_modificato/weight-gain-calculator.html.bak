    <script>
      // Utility functions
      function cmFromImperial(ft, inch) {
        return ft * 30.48 + inch * 2.54;
      }
      function kgFromLbs(lb) {
        return lb * 0.45359237;
      }
      function lbsFromKg(kg) {
        return kg / 0.45359237;
      }

      function calculateBMR(sex, weightKg, heightCm, age) {
        if (sex === "male") {
          return 10 * weightKg + 6.25 * heightCm - 5 * age + 5;
        } else {
          return 10 * weightKg + 6.25 * heightCm - 5 * age - 161;
        }
      }

      function formatNumber(num, decimals) {
        return Number(num).toFixed(decimals);
      }

      document.addEventListener("DOMContentLoaded", function () {
        const unitsRadios = document.querySelectorAll('input[name="units"]');
        const weightInput = document.getElementById("weight");
        const goalWeightInput = document.getElementById("goalWeight");
        const weightUnitLabel = document.getElementById("weight-unit-label");
        const goalWeightUnitLabel = document.getElementById(
          "goal-weight-unit-label"
        );
        const heightMetricGroup = document.getElementById(
          "height-metric-group"
        );
        const heightImperialGroup = document.getElementById(
          "height-imperial-group"
        );
        const heightCmInput = document.getElementById("heightCm");
        const heightFtInput = document.getElementById("heightFt");
        const heightInInput = document.getElementById("heightIn");

        const proteinPerKgInput = document.getElementById("proteinPerKg");
        const fatPercentInput = document.getElementById("fatPercent");
        const resetMacrosBtn = document.getElementById("resetMacros");

        const calculateBtn = document.getElementById("calculateBtn");
        const resultsSection = document.getElementById("resultsSection");
        const warningText = document.getElementById("warningText");

        const weeklyGainText = document.getElementById("weeklyGainText");
        const weeklyGainLbsText = document.getElementById("weeklyGainLbsText");
        const maintenanceCalsText = document.getElementById(
          "maintenanceCalsText"
        );
        const targetCalsText = document.getElementById("targetCalsText");
        const surplusText = document.getElementById("surplusText");

        const proteinGramsText = document.getElementById("proteinGramsText");
        const proteinCalsText = document.getElementById("proteinCalsText");
        const fatGramsText = document.getElementById("fatGramsText");
        const fatCalsText = document.getElementById("fatCalsText");
        const carbGramsText = document.getElementById("carbGramsText");
        const carbCalsText = document.getElementById("carbCalsText");

        const projectionTableBody = document.getElementById(
          "projectionTableBody"
        );

        // Handle unit switching with conversion
        unitsRadios.forEach((radio) => {
          radio.addEventListener("change", () => {
            const selected = document.querySelector(
              'input[name="units"]:checked'
            ).value;
            if (selected === "metric") {
              // Convert existing imperial values to metric
              const wLb = parseFloat(weightInput.value);
              const gwLb = parseFloat(goalWeightInput.value);
              if (!isNaN(wLb))
                weightInput.value = formatNumber(kgFromLbs(wLb), 1);
              if (!isNaN(gwLb))
                goalWeightInput.value = formatNumber(kgFromLbs(gwLb), 1);

              // Convert height to cm
              const ft = parseFloat(heightFtInput.value);
              const inch = parseFloat(heightInInput.value);
              if (!isNaN(ft) && !isNaN(inch)) {
                heightCmInput.value = formatNumber(cmFromImperial(ft, inch), 0);
              }

              weightUnitLabel.textContent = "kg";
              goalWeightUnitLabel.textContent = "kg";
              heightMetricGroup.classList.remove("hidden");
              heightImperialGroup.classList.add("hidden");
            } else {
              // Convert metric to imperial
              const wKg = parseFloat(weightInput.value);
              const gwKg = parseFloat(goalWeightInput.value);
              if (!isNaN(wKg))
                weightInput.value = formatNumber(lbsFromKg(wKg), 1);
              if (!isNaN(gwKg))
                goalWeightInput.value = formatNumber(lbsFromKg(gwKg), 1);

              const cm = parseFloat(heightCmInput.value);
              if (!isNaN(cm)) {
                const totalInches = cm / 2.54;
                const ft = Math.floor(totalInches / 12);
                const inch = Math.round(totalInches - ft * 12);
                heightFtInput.value = ft;
                heightInInput.value = inch;
              }

              weightUnitLabel.textContent = "lb";
              goalWeightUnitLabel.textContent = "lb";
              heightMetricGroup.classList.add("hidden");
              heightImperialGroup.classList.remove("hidden");
            }
          });
        });

        // Reset macros to defaults
        resetMacrosBtn.addEventListener("click", () => {
          proteinPerKgInput.value = 1.8;
          fatPercentInput.value = 25;
        });

        calculateBtn.addEventListener("click", () => {
          warningText.classList.add("hidden");
          warningText.textContent = "";

          const units = document.querySelector(
            'input[name="units"]:checked'
          ).value;
          const sex = document.querySelector('input[name="sex"]:checked').value;
          const age = parseFloat(document.getElementById("age").value);
          const activity = parseFloat(
            document.getElementById("activity").value
          );
          const weeks = parseFloat(document.getElementById("weeks").value);

          let weight = parseFloat(weightInput.value);
          let goalWeight = parseFloat(goalWeightInput.value);
          let heightCm;

          if (units === "metric") {
            heightCm = parseFloat(heightCmInput.value);
          } else {
            const ft = parseFloat(heightFtInput.value);
            const inch = parseFloat(heightInInput.value);
            heightCm = cmFromImperial(ft, inch);
            weight = kgFromLbs(weight);
            goalWeight = kgFromLbs(goalWeight);
          }

          if (
            isNaN(age) ||
            isNaN(heightCm) ||
            isNaN(weight) ||
            isNaN(goalWeight) ||
            isNaN(weeks) ||
            age <= 0 ||
            heightCm <= 0 ||
            weight <= 0 ||
            goalWeight <= 0 ||
            weeks <= 0
          ) {
            warningText.textContent =
              "Please fill in all fields with valid positive numbers.";
            warningText.classList.remove("hidden");
            resultsSection.classList.add("hidden");
            return;
          }

          if (goalWeight <= weight) {
            warningText.textContent =
              "Goal weight must be higher than your current weight.";
            warningText.classList.remove("hidden");
            resultsSection.classList.add("hidden");
            return;
          }

          const totalGainKg = goalWeight - weight;
          const weeklyGainKg = totalGainKg / weeks;
          const weeklyGainLbs = lbsFromKg(weeklyGainKg);

          // Safety checks on rate of gain
          if (weeklyGainKg > 1) {
            warningText.textContent =
              "Your requested rate of gain is very aggressive (>1 kg/week). Consider extending your timeframe for a healthier plan.";
            warningText.classList.remove("hidden");
          } else if (weeklyGainKg > 0.5) {
            warningText.textContent =
              "You are targeting a fast rate of gain. Expect a higher proportion of fat gain.";
            warningText.classList.remove("hidden");
          }

          const bmr = calculateBMR(sex, weight, heightCm, age);
          const tdee = bmr * activity;

          // 7700 kcal per kg gained
          const dailySurplus = (weeklyGainKg * 7700) / 7;
          const targetCalories = tdee + dailySurplus;

          // Macros
          const proteinPerKg = parseFloat(proteinPerKgInput.value);
          const fatPercent = parseFloat(fatPercentInput.value);

          const proteinGrams = proteinPerKg * weight;
          const proteinKcal = proteinGrams * 4;

          const fatKcal = targetCalories * (fatPercent / 100);
          const fatGrams = fatKcal / 9;

          let carbKcal = targetCalories - proteinKcal - fatKcal;
          if (carbKcal < 0) carbKcal = 0;
          const carbGrams = carbKcal / 4;

          const proteinPct = (proteinKcal / targetCalories) * 100;
          const fatPct = (fatKcal / targetCalories) * 100;
          const carbPct = (carbKcal / targetCalories) * 100;

          // Populate results
          weeklyGainText.textContent =
            formatNumber(weeklyGainKg, 2) + " kg/week";
          weeklyGainLbsText.textContent =
            formatNumber(weeklyGainLbs, 2) + " lb/week";

          maintenanceCalsText.textContent = formatNumber(tdee, 0) + " kcal/day";
          targetCalsText.textContent =
            formatNumber(targetCalories, 0) + " kcal/day";
          surplusText.textContent =
            "+" + formatNumber(dailySurplus, 0) + " kcal/day above maintenance";

          proteinGramsText.textContent =
            formatNumber(proteinGrams, 0) + " g/day";
          proteinCalsText.textContent =
            formatNumber(proteinKcal, 0) +
            " kcal (" +
            formatNumber(proteinPct, 0) +
            "%)";

          fatGramsText.textContent = formatNumber(fatGrams, 0) + " g/day";
          fatCalsText.textContent =
            formatNumber(fatKcal, 0) +
            " kcal (" +
            formatNumber(fatPct, 0) +
            "%)";

          carbGramsText.textContent = formatNumber(carbGrams, 0) + " g/day";
          carbCalsText.textContent =
            formatNumber(carbKcal, 0) +
            " kcal (" +
            formatNumber(carbPct, 0) +
            "%)";

          // Projection table (up to 12 rows or total weeks, whichever is smaller)
          projectionTableBody.innerHTML = "";
          const maxRows = Math.min(12, Math.ceil(weeks));
          for (let i = 0; i <= maxRows; i++) {
            const weekNum = i;
            const projectedWeightKg = weight + weeklyGainKg * weekNum;
            const changeKg = projectedWeightKg - weight;

            const tr = document.createElement("tr");
            if (weekNum % 2 === 1) tr.classList.add("bg-gray-50");

            const tdWeek = document.createElement("td");
            tdWeek.className = "px-2 py-1 border-b border-gray-200";
            tdWeek.textContent = weekNum === 0 ? "Start" : weekNum;

            const tdWeight = document.createElement("td");
            tdWeight.className = "px-2 py-1 border-b border-gray-200";
            tdWeight.textContent =
              formatNumber(projectedWeightKg, 1) +
              " kg (" +
              formatNumber(lbsFromKg(projectedWeightKg), 1) +
              " lb)";

            const tdChange = document.createElement("td");
            tdChange.className = "px-2 py-1 border-b border-gray-200";
            tdChange.textContent =
              (weekNum === 0 ? "0.0" : formatNumber(changeKg, 1)) + " kg";

            tr.appendChild(tdWeek);
            tr.appendChild(tdWeight);
            tr.appendChild(tdChange);
            projectionTableBody.appendChild(tr);
          }

          resultsSection.classList.remove("hidden");
          resultsSection.scrollIntoView({ behavior: "smooth", block: "start" });
        });
      });
    </script>

