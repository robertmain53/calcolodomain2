    <script>
      // Basic linear algebra utilities for PCA

      function parseData(text, hasHeader, hasRowLabels) {
        const lines = text
          .split(/\r?\n/)
          .map((l) => l.trim())
          .filter((l) => l.length > 0);
        if (lines.length === 0) throw new Error("No data provided.");

        let header = null;
        let startRow = 0;

        if (hasHeader) {
          header = lines[0].split(/[\t,;]+/).map((s) => s.trim());
          startRow = 1;
        }

        const data = [];
        const rowLabels = [];

        for (let i = startRow; i < lines.length; i++) {
          const parts = lines[i].split(/[\t,;]+/).map((s) => s.trim());
          if (hasRowLabels) {
            if (parts.length < 2) continue;
            rowLabels.push(parts[0]);
          }
          const offset = hasRowLabels ? 1 : 0;
          const row = [];
          for (let j = offset; j < parts.length; j++) {
            const v = parseFloat(parts[j]);
            if (!isFinite(v)) {
              // skip entire row if any non-numeric
              row.length = 0;
              break;
            }
            row.push(v);
          }
          if (row.length > 0) data.push(row);
        }

        if (data.length === 0) throw new Error("No valid numeric rows found.");

        const n = data.length;
        const p = data[0].length;
        for (let i = 0; i < n; i++) {
          if (data[i].length !== p)
            throw new Error(
              "All rows must have the same number of numeric columns."
            );
        }

        let varNames;
        if (header) {
          if (hasRowLabels) {
            varNames = header.slice(1);
          } else {
            varNames = header;
          }
          if (varNames.length !== p) {
            // fallback to generic names
            varNames = Array.from({ length: p }, (_, i) => "x" + (i + 1));
          }
        } else {
          varNames = Array.from({ length: p }, (_, i) => "x" + (i + 1));
        }

        const obsLabels =
          rowLabels.length === n
            ? rowLabels
            : Array.from({ length: n }, (_, i) => "Obs " + (i + 1));

        return { data, varNames, obsLabels };
      }

      function mean(arr) {
        return arr.reduce((a, b) => a + b, 0) / arr.length;
      }

      function std(arr, m) {
        const mu = typeof m === "number" ? m : mean(arr);
        const n = arr.length;
        if (n < 2) return 0;
        let s2 = 0;
        for (let i = 0; i < n; i++) {
          const d = arr[i] - mu;
          s2 += d * d;
        }
        return Math.sqrt(s2 / (n - 1));
      }

      function centerAndScale(data, scaling) {
        const n = data.length;
        const p = data[0].length;
        const means = new Array(p).fill(0);
        const sds = new Array(p).fill(1);

        // compute means
        for (let j = 0; j < p; j++) {
          let sum = 0;
          for (let i = 0; i < n; i++) sum += data[i][j];
          means[j] = sum / n;
        }

        // compute sds if needed
        if (scaling === "standardize") {
          for (let j = 0; j < p; j++) {
            let s2 = 0;
            for (let i = 0; i < n; i++) {
              const d = data[i][j] - means[j];
              s2 += d * d;
            }
            sds[j] = Math.sqrt(s2 / (n - 1)) || 1;
          }
        }

        const Xc = Array.from({ length: n }, () => new Array(p).fill(0));
        for (let i = 0; i < n; i++) {
          for (let j = 0; j < p; j++) {
            let v = data[i][j] - means[j];
            if (scaling === "standardize") v / sds[j];
            Xc[i][j] = v;
          }
        }
        return { Xc, means, sds };
      }

      function covarianceMatrix(Xc) {
        const n = Xc.length;
        const p = Xc[0].length;
        const S = Array.from({ length: p }, () => new Array(p).fill(0));
        const factor = 1 / (n - 1);
        for (let i = 0; i < p; i++) {
          for (let j = i; j < p; j++) {
            let sum = 0;
            for (let k = 0; k < n; k++) sum += Xc[k][i] * Xc[k][j];
            const v = sum * factor;
            S[i][j] = v;
            S[j][i] = v;
          }
        }
        return S;
      }

      // Jacobi eigenvalue algorithm for symmetric matrices
      function jacobiEigenDecomposition(A, tol = 1e-10, maxIter = 100) {
        const n = A.length;
        let V = Array.from({ length: n }, (_, i) => {
          const row = new Array(n).fill(0);
          row[i] = 1;
          return row;
        });
        let iter = 0;

        function maxOffDiag(mat) {
          let maxVal = 0;
          let p = 0,
            q = 1;
          for (let i = 0; i < n; i++) {
            for (let j = i + 1; j < n; j++) {
              const v = Math.abs(mat[i][j]);
              if (v > maxVal) {
                maxVal = v;
                p = i;
                q = j;
              }
            }
          }
          return { maxVal, p, q };
        }

        const a = A.map((row) => row.slice());

        while (iter < maxIter) {
          const { maxVal, p, q } = maxOffDiag(a);
          if (maxVal < tol) break;

          const app = a[p][p];
          const aqq = a[q][q];
          const apq = a[p][q];

          const phi = 0.5 * Math.atan2(2 * apq, aqq - app);
          const c = Math.cos(phi);
          const s = Math.sin(phi);

          for (let i = 0; i < n; i++) {
            const aip = a[i][p];
            const aiq = a[i][q];
            a[i][p] = c * aip - s * aiq;
            a[i][q] = s * aip + c * aiq;
          }
          for (let j = 0; j < n; j++) {
            const apj = a[p][j];
            const aqj = a[q][j];
            a[p][j] = c * apj - s * aqj;
            a[q][j] = s * apj + c * aqj;
          }
          a[p][p] = c * c * app - 2 * s * c * apq + s * s * aqq;
          a[q][q] = s * s * app + 2 * s * c * apq + c * c * aqq;
          a[p][q] = 0;
          a[q][p] = 0;

          for (let i = 0; i < n; i++) {
            const vip = V[i][p];
            const viq = V[i][q];
            V[i][p] = c * vip - s * viq;
            V[i][q] = s * vip + c * viq;
          }

          iter++;
        }

        const eigenvalues = new Array(n);
        for (let i = 0; i < n; i++) eigenvalues[i] = a[i][i];

        // V columns are eigenvectors
        return { eigenvalues, eigenvectors: V };
      }

      function sortEigenpairs(evals, evecs) {
        const n = evals.length;
        const pairs = [];
        for (let i = 0; i < n; i++) {
          pairs.push({ val: evals[i], vec: evecs.map((row) => row[i]) });
        }
        pairs.sort((a, b) => b.val - a.val);
        const sortedVals = pairs.map((p) => p.val);
        const sortedVecs = Array.from({ length: n }, () =>
          new Array(n).fill(0)
        );
        for (let j = 0; j < n; j++) {
          for (let i = 0; i < n; i++) {
            sortedVecs[i][j] = pairs[j].vec[i];
          }
        }
        return { eigenvalues: sortedVals, eigenvectors: sortedVecs };
      }

      function multiplyMatrix(X, V) {
        const n = X.length;
        const p = X[0].length;
        const k = V[0].length;
        const Z = Array.from({ length: n }, () => new Array(k).fill(0));
        for (let i = 0; i < n; i++) {
          for (let j = 0; j < k; j++) {
            let sum = 0;
            for (let t = 0; t < p; t++) sum += X[i][t] * V[t][j];
            Z[i][j] = sum;
          }
        }
        return Z;
      }

      function formatNumber(x, digits = 4) {
        if (!isFinite(x)) return "";
        return Number(x).toFixed(digits);
      }

      // DOM wiring
      document.addEventListener("DOMContentLoaded", () => {
        const dataInput = document.getElementById("data-input");
        const hasHeader = document.getElementById("has-header");
        const hasRowLabels = document.getElementById("has-row-labels");
        const runBtn = document.getElementById("run-pca");
        const statusMsg = document.getElementById("status-message");
        const loadExampleBtn = document.getElementById("load-example");
        const clearBtn = document.getElementById("clear-data");

        const summaryWrapper = document.getElementById(
          "summary-section-wrapper"
        );
        const eigenWrapper = document.getElementById("eigen-section-wrapper");
        const loadingsWrapper = document.getElementById(
          "loadings-section-wrapper"
        );
        const scoresWrapper = document.getElementById("scores-section-wrapper");

        const summaryN = document.getElementById("summary-n");
        const summaryP = document.getElementById("summary-p");
        const summaryScaling = document.getElementById("summary-scaling");
        const summaryK = document.getElementById("summary-k");
        const summaryTargetVar = document.getElementById("summary-target-var");

        const eigenBody = document.getElementById("eigen-table-body");
        const loadingsHeader = document.getElementById("loadings-header");
        const loadingsBody = document.getElementById("loadings-body");
        const scoresHeader = document.getElementById("scores-header");
        const scoresBody = document.getElementById("scores-body");

        loadExampleBtn.addEventListener("click", () => {
          dataInput.value = `id,x1,x2,x3
A,2.5,2.4,1.2
B,0.5,0.7,0.3
C,2.2,2.9,1.4
D,1.9,2.2,1.1
E,3.1,3.0,1.6
F,2.3,2.7,1.3
G,2.0,1.6,0.9
H,1.0,1.1,0.5
I,1.5,1.6,0.7
J,1.1,0.9,0.4`;
          hasHeader.checked = true;
          hasRowLabels.checked = true;
        });

        clearBtn.addEventListener("click", () => {
          dataInput.value = "";
          statusMsg.textContent = "";
          [
            summaryWrapper,
            eigenWrapper,
            loadingsWrapper,
            scoresWrapper,
          ].forEach((el) => el.classList.add("hidden"));
        });

        runBtn.addEventListener("click", () => {
          statusMsg.textContent = "Running PCA...";
          statusMsg.classList.remove("text-red-600");
          statusMsg.classList.add("text-gray-600");

          setTimeout(() => {
            try {
              const text = dataInput.value;
              const headerFlag = hasHeader.checked;
              const rowLabelFlag = hasRowLabels.checked;
              const scaling = document.querySelector(
                'input[name="scaling"]:checked'
              ).value;
              const targetVar =
                parseFloat(
                  document.getElementById("variance-threshold").value
                ) || 90;

              const { data, varNames, obsLabels } = parseData(
                text,
                headerFlag,
                rowLabelFlag
              );
              const n = data.length;
              const p = data[0].length;

              const { Xc } = centerAndScale(data, scaling);
              const S = covarianceMatrix(Xc);
              const { eigenvalues, eigenvectors } = jacobiEigenDecomposition(S);
              const sorted = sortEigenpairs(eigenvalues, eigenvectors);
              const evals = sorted.eigenvalues;
              const evecs = sorted.eigenvectors;
              const scores = multiplyMatrix(Xc, evecs);

              const totalVar = evals.reduce((a, b) => a + b, 0);
              const propVar = evals.map((v) => v / totalVar);
              const cumVar = [];
              let acc = 0;
              for (let i = 0; i < propVar.length; i++) {
                acc += propVar[i];
                cumVar.push(acc);
              }

              let k = propVar.length;
              for (let i = 0; i < propVar.length; i++) {
                if (cumVar[i] * 100 >= targetVar) {
                  k = i + 1;
                  break;
                }
              }

              // Summary
              summaryN.textContent = n;
              summaryP.textContent = p;
              summaryScaling.textContent =
                scaling === "standardize"
                  ? "Standardized (correlation PCA)"
                  : "Centered (covariance PCA)";
              summaryK.textContent = k;
              summaryTargetVar.textContent = targetVar.toFixed(0);
              summaryWrapper.classList.remove("hidden");

              // Eigen table
              eigenBody.innerHTML = "";
              for (let i = 0; i < evals.length; i++) {
                const tr = document.createElement("tr");
                tr.innerHTML = `
              <td class="px-2 py-1 border-b">PC${i + 1}</td>
              <td class="px-2 py-1 border-b text-right">${formatNumber(
                evals[i],
                4
              )}</td>
              <td class="px-2 py-1 border-b text-right">${formatNumber(
                propVar[i] * 100,
                2
              )}</td>
              <td class="px-2 py-1 border-b text-right">${formatNumber(
                cumVar[i] * 100,
                2
              )}</td>
            `;
                eigenBody.appendChild(tr);
              }
              eigenWrapper.classList.remove("hidden");

              // Loadings table
              loadingsHeader.innerHTML = "";
              loadingsBody.innerHTML = "";
              const lhRow = document.createElement("tr");
              lhRow.innerHTML =
                `<th class="px-2 py-1 border-b text-left bg-gray-100">Variable</th>` +
                evals
                  .map(
                    (_, i) =>
                      `<th class="px-2 py-1 border-b text-right bg-gray-100">PC${
                        i + 1
                      }</th>`
                  )
                  .join("");
              loadingsHeader.appendChild(lhRow);

              for (let j = 0; j < p; j++) {
                const tr = document.createElement("tr");
                let rowHtml = `<td class="px-2 py-1 border-b text-left">${
                  varNames[j] || "x" + (j + 1)
                }</td>`;
                for (let k2 = 0; k2 < evals.length; k2++) {
                  rowHtml += `<td class="px-2 py-1 border-b text-right">${formatNumber(
                    evecs[j][k2],
                    4
                  )}</td>`;
                }
                tr.innerHTML = rowHtml;
                loadingsBody.appendChild(tr);
              }
              loadingsWrapper.classList.remove("hidden");

              // Scores table (limit to first 10 PCs for readability)
              const maxPCsToShow = Math.min(evals.length, 10);
              scoresHeader.innerHTML = "";
              scoresBody.innerHTML = "";
              const shRow = document.createElement("tr");
              shRow.innerHTML =
                `<th class="px-2 py-1 border-b text-left bg-gray-100">Observation</th>` +
                Array.from(
                  { length: maxPCsToShow },
                  (_, i) =>
                    `<th class="px-2 py-1 border-b text-right bg-gray-100">PC${
                      i + 1
                    }</th>`
                ).join("");
              scoresHeader.appendChild(shRow);

              for (let i = 0; i < n; i++) {
                const tr = document.createElement("tr");
                let rowHtml = `<td class="px-2 py-1 border-b text-left">${
                  obsLabels[i] || "Obs " + (i + 1)
                }</td>`;
                for (let j = 0; j < maxPCsToShow; j++) {
                  rowHtml += `<td class="px-2 py-1 border-b text-right">${formatNumber(
                    scores[i][j],
                    4
                  )}</td>`;
                }
                tr.innerHTML = rowHtml;
                scoresBody.appendChild(tr);
              }
              scoresWrapper.classList.remove("hidden");

              statusMsg.textContent = "PCA completed successfully.";
              statusMsg.classList.remove("text-red-600");
              statusMsg.classList.add("text-green-700");
            } catch (err) {
              console.error(err);
              statusMsg.textContent =
                err.message ||
                "Error while running PCA. Please check your data.";
              statusMsg.classList.remove("text-gray-600", "text-green-700");
              statusMsg.classList.add("text-red-600");
              [
                summaryWrapper,
                eigenWrapper,
                loadingsWrapper,
                scoresWrapper,
              ].forEach((el) => el.classList.add("hidden"));
            }
          }, 10);
        });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
