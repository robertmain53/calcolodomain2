    <script>
      // Basic data tables for bolt tensile areas (mm²) for common diameters
      const boltAreas = {
        12: 84.3,
        14: 115,
        16: 157,
        18: 192,
        20: 245,
        22: 303,
        24: 353,
        27: 459,
        30: 561,
        36: 817,
      };

      function getBoltArea(d) {
        const dia = parseInt(d, 10);
        if (boltAreas[dia]) return boltAreas[dia];
        // approximate As ≈ 0.785*(d-1.2268)^2 (rough ISO thread approximation)
        const core = dia - 1.23;
        return 0.785 * core * core;
      }

      function getFub(grade) {
        if (grade === "4.6") return 400;
        if (grade === "10.9") return 1000;
        return 800; // 8.8 default
      }

      function getFuSteel(grade) {
        if (grade === "S235") return 360;
        if (grade === "S275") return 430;
        return 510; // S355
      }

      function ksForHole(holeType) {
        switch (holeType) {
          case "oversize":
            return 0.85;
          case "shortSlot":
            return 0.85;
          case "longSlot":
            return 0.7;
          default:
            return 1.0;
        }
      }

      function alphaBForGeometry(e1, e2, d) {
        // Very simplified conservative estimate:
        // alpha_b = min(e1/(3d), e2/(3d), 1.0) but not less than 0.5
        const val = Math.min(e1 / (3 * d), e2 / (3 * d), 1.0);
        return Math.max(val, 0.5);
      }

      function formatNum(x, digits = 2) {
        if (!isFinite(x)) return "–";
        return Number(x).toFixed(digits);
      }

      function calculateEurocode3() {
        const connType = document.getElementById("connType").value;
        const shearConfig = document.getElementById("shearConfig").value;
        const d = parseFloat(document.getElementById("boltDia").value);
        const grade = document.getElementById("boltGrade").value;
        const nBolts = parseInt(document.getElementById("nBolts").value, 10);

        const t = parseFloat(document.getElementById("plateThk").value);
        const steelGrade = document.getElementById("steelGrade").value;
        const e1 = parseFloat(document.getElementById("e1").value);
        const e2 = parseFloat(document.getElementById("e2").value);
        const holeType = document.getElementById("holeType").value;

        const VEd = parseFloat(document.getElementById("VEd").value);
        const NEd = parseFloat(document.getElementById("NEd").value);

        const gammaM2 = parseFloat(document.getElementById("gammaM2").value);
        const gammaM3 = parseFloat(document.getElementById("gammaM3").value);
        const gammaMb = parseFloat(document.getElementById("gammaMb").value);

        const statusMsg = document.getElementById("statusMsg");
        statusMsg.textContent = "";

        if (!d || !nBolts || !t || (!VEd && !NEd)) {
          statusMsg.textContent =
            "Please provide bolt size, plate thickness and at least one design action.";
          return;
        }

        const As = getBoltArea(d); // mm²
        const fub = getFub(grade); // MPa
        const fu = getFuSteel(steelGrade); // MPa

        const nShearPlanes = shearConfig === "double" ? 2 : 1;

        // 1) Shear resistance per bolt (kN)
        const alphaV = 0.6;
        const VRdBolt = (alphaV * fub * As * nShearPlanes) / (gammaM2 * 1000); // kN

        // 2) Tension resistance per bolt (kN)
        const NRdBolt = (0.9 * fub * As) / (gammaM2 * 1000); // kN

        // 3) Design actions per bolt (kN)
        const VEdBolt = VEd / nBolts;
        const NEdBolt = NEd / nBolts;

        const etaV = VEdBolt / VRdBolt;
        const etaN = NEdBolt / NRdBolt;
        const etaVN = (VEdBolt / VRdBolt) ** 2 + (NEdBolt / NRdBolt) ** 2;

        // 4) Bearing resistance per bolt (kN) – simplified
        const alphaB = alphaBForGeometry(e1, e2, d);
        const k1 = 2.5; // conservative upper bound factor
        const FbRdBolt = (k1 * alphaB * fu * d * t) / (gammaMb * 1000); // kN
        const etaB = VEdBolt / FbRdBolt;

        // 5) Slip resistance per bolt (kN) if slip-resistant
        let FslRdBolt = 0;
        let etaSlip = 0;
        if (connType === "slip") {
          const mu = parseFloat(document.getElementById("muSlip").value);
          const gammaM3Use =
            parseFloat(document.getElementById("gammaMSlip").value) || gammaM3;
          const ks = ksForHole(holeType);
          const ne = nShearPlanes; // number of friction surfaces
          const FpC = (0.7 * fub * As) / 1000; // kN
          FslRdBolt = (ks * ne * mu * FpC) / gammaM3Use;
          etaSlip = VEdBolt / FslRdBolt;
        }

        // Update UI
        document.getElementById("VRdBolt").textContent = formatNum(VRdBolt);
        document.getElementById("NRdBolt").textContent = formatNum(NRdBolt);
        document.getElementById("VEdBolt").textContent = formatNum(VEdBolt);
        document.getElementById("NEdBolt").textContent = formatNum(NEdBolt);
        document.getElementById("etaV").textContent = formatNum(etaV);
        document.getElementById("etaN").textContent = formatNum(etaN);
        document.getElementById("etaVN").textContent = formatNum(etaVN);

        document.getElementById("FbRdBolt").textContent = formatNum(FbRdBolt);
        document.getElementById("etaB").textContent = formatNum(etaB);

        document.getElementById("FslRdBolt").textContent =
          connType === "slip" ? formatNum(FslRdBolt) : "–";
        document.getElementById("etaSlip").textContent =
          connType === "slip" ? formatNum(etaSlip) : "–";

        // Summary & governing check
        const utilisations = [];
        utilisations.push({ name: "Shear", value: etaV });
        if (NEd > 0) utilisations.push({ name: "Tension", value: etaN });
        utilisations.push({
          name: "Shear + tension interaction",
          value: etaVN,
        });
        utilisations.push({ name: "Bearing", value: etaB });
        if (connType === "slip")
          utilisations.push({ name: "Slip", value: etaSlip });

        let governing = utilisations.reduce(
          (max, cur) => (cur.value > max.value ? cur : max),
          { name: "", value: -Infinity }
        );

        const summary = document.getElementById("summaryText");
        const governingText = document.getElementById("governingCheck");

        const maxEta = governing.value;
        const pass = maxEta <= 1.0;

        summary.innerHTML =
          "Maximum utilisation η<sub>max</sub> = " +
          formatNum(maxEta) +
          " (" +
          governing.name +
          "). " +
          (pass
            ? '<span class="result-ok font-semibold">Connection OK.</span>'
            : '<span class="result-ng font-semibold">Connection NOT OK – increase bolt size/number or adjust geometry.</span>');

        governingText.innerHTML =
          "Governing check: <strong>" +
          governing.name +
          "</strong> with utilisation " +
          "<strong>" +
          formatNum(maxEta) +
          "</strong>. " +
          "Shear η<sub>V</sub> = " +
          formatNum(etaV) +
          ", " +
          "Tension η<sub>N</sub> = " +
          formatNum(etaN) +
          ", " +
          "Bearing η<sub>b</sub> = " +
          formatNum(etaB) +
          (connType === "slip"
            ? ", Slip η<sub>sl</sub> = " + formatNum(etaSlip) + "."
            : ".");

        statusMsg.textContent = "Calculation completed.";
      }

      document
        .getElementById("calcBtn")
        .addEventListener("click", calculateEurocode3);

      document.getElementById("resetBtn").addEventListener("click", () => {
        document.getElementById("boltDia").value = 20;
        document.getElementById("boltGrade").value = "8.8";
        document.getElementById("nBolts").value = 4;
        document.getElementById("plateThk").value = 10;
        document.getElementById("steelGrade").value = "S355";
        document.getElementById("e1").value = 40;
        document.getElementById("e2").value = 40;
        document.getElementById("holeType").value = "normal";
        document.getElementById("VEd").value = 120;
        document.getElementById("NEd").value = 60;
        document.getElementById("connType").value = "bearing";
        document.getElementById("shearConfig").value = "single";
        document.getElementById("gammaM2").value = 1.25;
        document.getElementById("gammaM3").value = 1.25;
        document.getElementById("gammaMb").value = 1.25;
        document.getElementById("muSlip").value = 0.3;
        document.getElementById("gammaMSlip").value = 1.25;
        document.getElementById("slipParams").classList.add("hidden");
        document.getElementById("statusMsg").textContent = "";
        document.getElementById("summaryText").textContent =
          "Enter data and click “Calculate” to see utilisation ratios and governing failure mode.";
        document.getElementById("governingCheck").textContent = "–";
        [
          "VRdBolt",
          "NRdBolt",
          "VEdBolt",
          "NEdBolt",
          "etaV",
          "etaN",
          "etaVN",
          "FbRdBolt",
          "etaB",
          "FslRdBolt",
          "etaSlip",
        ].forEach((id) => (document.getElementById(id).textContent = "–"));
      });

      document.getElementById("connType").addEventListener("change", (e) => {
        const slipParams = document.getElementById("slipParams");
        if (e.target.value === "slip") {
          slipParams.classList.remove("hidden");
        } else {
          slipParams.classList.add("hidden");
        }
      });
    </script>

    <script>
      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
