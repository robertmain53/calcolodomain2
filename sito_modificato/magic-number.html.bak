    <script>
      // Tab switching
      const tabs = {
        sports: document.getElementById("tab-sports"),
        saas: document.getElementById("tab-saas"),
        math: document.getElementById("tab-math"),
      };
      const panels = {
        sports: document.getElementById("panel-sports"),
        saas: document.getElementById("panel-saas"),
        math: document.getElementById("panel-math"),
      };

      function activateTab(name) {
        Object.keys(tabs).forEach((key) => {
          if (key === name) {
            tabs[key].classList.add("tab-active");
            tabs[key].classList.remove("text-gray-600");
            panels[key].classList.remove("hidden");
          } else {
            tabs[key].classList.remove("tab-active");
            tabs[key].classList.add("text-gray-600");
            panels[key].classList.add("hidden");
          }
        });
      }

      tabs.sports.addEventListener("click", () => activateTab("sports"));
      tabs.saas.addEventListener("click", () => activateTab("saas"));
      tabs.math.addEventListener("click", () => activateTab("math"));

      // Sports magic number calculation
      document.getElementById("sports-calc").addEventListener("click", () => {
        const games = Number(document.getElementById("sports-games").value);
        const lw = Number(document.getElementById("sports-leader-wins").value);
        const ll = Number(
          document.getElementById("sports-leader-losses").value
        );
        const cw = Number(document.getElementById("sports-chaser-wins").value);
        const cl = Number(
          document.getElementById("sports-chaser-losses").value
        );

        const resultBox = document.getElementById("sports-result");
        const magicSpan = document.getElementById("sports-magic");
        const statusP = document.getElementById("sports-status");

        if (games <= 0 || lw < 0 || ll < 0 || cw < 0 || cl < 0) {
          resultBox.classList.remove("hidden");
          magicSpan.textContent = "–";
          statusP.textContent =
            "Please enter non‑negative values and a positive number of games.";
          return;
        }

        const leaderPlayed = lw + ll;
        const chaserPlayed = cw + cl;
        if (leaderPlayed > games || chaserPlayed > games) {
          resultBox.classList.remove("hidden");
          magicSpan.textContent = "–";
          statusP.textContent =
            "Wins + losses for a team cannot exceed the total games in the season.";
          return;
        }

        let magic = games + 1 - lw - cl;
        magic = Math.max(magic, 0);

        resultBox.classList.remove("hidden");
        magicSpan.textContent = magic.toString();

        if (magic === 0) {
          statusP.textContent =
            "The leader has already clinched against this chaser (or the race is mathematically over).";
        } else {
          statusP.textContent =
            "Each win by the leader or loss by the chaser reduces this number by 1.";
        }
      });

      // SaaS magic number calculation
      document.getElementById("saas-calc").addEventListener("click", () => {
        const prevArr = Number(document.getElementById("saas-prev-arr").value);
        const currArr = Number(document.getElementById("saas-curr-arr").value);
        const snm = Number(document.getElementById("saas-snm").value);

        const resultBox = document.getElementById("saas-result");
        const magicSpan = document.getElementById("saas-magic");
        const interpP = document.getElementById("saas-interpretation");

        if (prevArr < 0 || currArr < 0 || snm <= 0) {
          resultBox.classList.remove("hidden");
          magicSpan.textContent = "–";
          interpP.textContent =
            "ARR values must be non‑negative and sales & marketing spend must be greater than 0.";
          return;
        }

        const delta = currArr - prevArr;
        const magic = (delta * 4) / snm;
        const rounded = Number.isFinite(magic) ? magic.toFixed(2) : "–";

        resultBox.classList.remove("hidden");
        magicSpan.textContent = rounded;

        let msg = "";
        if (!Number.isFinite(magic)) {
          msg = "Unable to compute magic number with the given inputs.";
        } else if (magic < 0.5) {
          msg =
            "Low efficiency: growth is expensive relative to sales & marketing spend.";
        } else if (magic < 0.75) {
          msg = "Moderate efficiency: improving, but not yet strong.";
        } else if (magic < 1.0) {
          msg =
            "Healthy efficiency: often considered a good SaaS magic number range.";
        } else {
          msg =
            "Excellent efficiency: very strong growth relative to sales & marketing spend. Check sustainability and data quality.";
        }
        interpP.textContent = msg;
      });

      // Math magic numbers
      document.getElementById("math-calc").addEventListener("click", () => {
        const nVal = Number(document.getElementById("math-number").value);
        const nSquare = Number(document.getElementById("math-square-n").value);

        const resultBox = document.getElementById("math-result");
        const digitSumSpan = document.getElementById("math-digit-sum");
        const armstrongSpan = document.getElementById("math-armstrong");
        const harshadSpan = document.getElementById("math-harshad");
        const magicConstSpan = document.getElementById("math-magic-constant");

        if (!Number.isInteger(nVal)) {
          resultBox.classList.remove("hidden");
          digitSumSpan.textContent = "–";
          armstrongSpan.textContent = "Input must be an integer.";
          harshadSpan.textContent = "–";
          magicConstSpan.textContent = "–";
          return;
        }

        const absStr = Math.abs(nVal).toString();
        let digitSum = 0;
        for (const ch of absStr) {
          digitSum += Number(ch);
        }

        // Armstrong check
        const numDigits = absStr.length;
        let armstrongSum = 0;
        for (const ch of absStr) {
          armstrongSum += Math.pow(Number(ch), numDigits);
        }
        const isArmstrong = armstrongSum === Math.abs(nVal);

        // Harshad check (exclude 0)
        const isHarshad = nVal !== 0 && nVal % digitSum === 0;

        // Magic square constant
        let magicConst = "–";
        if (Number.isInteger(nSquare) && nSquare >= 3 && nSquare <= 20) {
          magicConst = ((nSquare * (nSquare * nSquare + 1)) / 2).toString();
        }

        resultBox.classList.remove("hidden");
        digitSumSpan.textContent = digitSum.toString();
        armstrongSpan.textContent = isArmstrong
          ? "Yes – this is an Armstrong (narcissistic) number."
          : "No – this is not an Armstrong number.";
        harshadSpan.textContent = isHarshad
          ? "Yes – this is a Harshad (Niven) number."
          : "No – this is not a Harshad number.";
        magicConstSpan.textContent = magicConst;
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
