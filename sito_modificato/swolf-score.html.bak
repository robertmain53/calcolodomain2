<script>
  // Utility: parse time input (mm:ss or seconds)
  function parseTimeToSeconds(value) {
    if (!value) return NaN;
    value = value.trim();
    if (value.includes(':')) {
      const parts = value.split(':');
      if (parts.length !== 2) return NaN;
      const m = parseFloat(parts[0]);
      const s = parseFloat(parts[1]);
      if (isNaN(m) || isNaN(s)) return NaN;
      return m * 60 + s;
    } else {
      const s = parseFloat(value);
      return isNaN(s) ? NaN : s;
    }
  }

  function formatSecondsToPace(sec) {
    if (!isFinite(sec) || sec <= 0) return '–';
    const m = Math.floor(sec / 60);
    const s = Math.round(sec % 60);
    return m + ':' + (s < 10 ? '0' + s : s);
  }

  document.addEventListener('DOMContentLoaded', function () {
    const poolLengthSelect = document.getElementById('poolLength');
    const customPoolWrapper = document.getElementById('customPoolWrapper');
    const customPoolLength = document.getElementById('customPoolLength');
    const customPoolUnit = document.getElementById('customPoolUnit');
    const swolfRows = document.getElementById('swolfRows');
    const addRowBtn = document.getElementById('addRowBtn');
    const clearAllBtn = document.getElementById('clearAllBtn');
    const errorMsg = document.getElementById('errorMsg');

    const avgSwolfEl = document.getElementById('avgSwolf');
    const avgSwolfPer25El = document.getElementById('avgSwolfPer25');
    const avgPaceEl = document.getElementById('avgPace');
    const avgPacePer100El = document.getElementById('avgPacePer100');
    const totalDistanceEl = document.getElementById('totalDistance');
    const totalTimeEl = document.getElementById('totalTime');

    let rowCount = 0;

    function getPoolInfo() {
      const val = poolLengthSelect.value;
      if (val === 'custom') {
        const len = parseFloat(customPoolLength.value);
        const unit = customPoolUnit.value || 'm';
        return { length: isNaN(len) ? NaN : len, unit, label: len ? len + ' ' + unit : 'custom' };
      }
      if (val.endsWith('y')) {
        const num = parseFloat(val);
        return { length: num, unit: 'y', label: num + ' yd' };
      }
      const num = parseFloat(val);
      return { length: num, unit: 'm', label: num + ' m' };
    }

    function addRow(initial = false) {
      rowCount += 1;
      const tr = document.createElement('tr');
      tr.className = 'border-t';
      tr.dataset.index = rowCount;

      tr.innerHTML = `
        <td class="px-2 py-2 text-xs text-gray-500">${rowCount}</td>
        <td class="px-2 py-2">
          <input type="number" min="1" step="1" value="${initial ? 1 : ''}" class="lengths-input w-20 border border-gray-300 rounded-md px-2 py-1 text-sm" aria-label="Number of lengths">
        </td>
        <td class="px-2 py-2">
          <input type="text" placeholder="e.g. 0:25" class="time-input w-24 border border-gray-300 rounded-md px-2 py-1 text-sm" aria-label="Time">
        </td>
        <td class="px-2 py-2">
          <input type="number" min="1" step="0.5" class="strokes-input w-24 border border-gray-300 rounded-md px-2 py-1 text-sm" aria-label="Strokes per length">
        </td>
        <td class="px-2 py-2 text-sm text-gray-800">
          <div class="swolf-row font-semibold">–</div>
          <div class="text-xs text-gray-500 swolf-row-per25">Per 25: –</div>
        </td>
        <td class="px-2 py-2 text-sm text-gray-800">
          <div class="pace-row">–</div>
          <div class="text-xs text-gray-500 pace-row-100">/100: –</div>
        </td>
        <td class="px-2 py-2">
          <button type="button" class="remove-row text-xs text-red-600 hover:underline">Remove</button>
        </td>
      `;
      swolfRows.appendChild(tr);
    }

    function recalc() {
      errorMsg.textContent = '';
      const pool = getPoolInfo();
      if (isNaN(pool.length) || pool.length <= 0) {
        avgSwolfEl.textContent = '–';
        avgSwolfPer25El.textContent = 'Per 25: –';
        avgPaceEl.textContent = '–';
        avgPacePer100El.textContent = 'Per 100: –';
        totalDistanceEl.textContent = '–';
        totalTimeEl.textContent = 'Total time: –';
        return;
      }

      let totalLengths = 0;
      let totalTimeSec = 0;
      let totalStrokes = 0;
      let totalSwolfSum = 0;

      const rows = swolfRows.querySelectorAll('tr');
      rows.forEach(row => {
        const lenInput = row.querySelector('.lengths-input');
        const timeInput = row.querySelector('.time-input');
        const strokesInput = row.querySelector('.strokes-input');
        const swolfRowEl = row.querySelector('.swolf-row');
        const swolfRowPer25El = row.querySelector('.swolf-row-per25');
        const paceRowEl = row.querySelector('.pace-row');
        const paceRow100El = row.querySelector('.pace-row-100');

        const lengths = parseFloat(lenInput.value);
        const timeSec = parseTimeToSeconds(timeInput.value);
        const strokesPerLen = parseFloat(strokesInput.value);

        if (!isFinite(lengths) || lengths <= 0 || !isFinite(timeSec) || timeSec <= 0 || !isFinite(strokesPerLen) || strokesPerLen <= 0) {
          swolfRowEl.textContent = '–';
          swolfRowPer25El.textContent = 'Per 25: –';
          paceRowEl.textContent = '–';
          paceRow100El.textContent = '/100: –';
          return;
        }

        const rowStrokesTotal = strokesPerLen * lengths;
        const rowSwolfTotal = timeSec + rowStrokesTotal;
        const swolfPerLength = rowSwolfTotal / lengths;

        const rowDistance = pool.length * lengths;
        const pacePerLength = timeSec / lengths;
        const pacePer100 = (timeSec / rowDistance) * 100;

        swolfRowEl.textContent = swolfPerLength.toFixed(1);

        // Normalize per 25 units
        const base = pool.length >= 37 ? 50 : 25;
        const factor = base / pool.length;
        const swolfPerBase = swolfPerLength * factor;
        swolfRowPer25El.textContent = `Per ${base}${pool.unit}: ${swolfPerBase.toFixed(1)}`;

        paceRowEl.textContent = formatSecondsToPace(pacePerLength);
        paceRow100El.textContent = `/100: ${formatSecondsToPace(pacePer100)}`;

        totalLengths += lengths;
        totalTimeSec += timeSec;
        totalStrokes += rowStrokesTotal;
        totalSwolfSum += rowSwolfTotal;
      });

      if (totalLengths <= 0 || totalTimeSec <= 0) {
        avgSwolfEl.textContent = '–';
        avgSwolfPer25El.textContent = 'Per 25: –';
        avgPaceEl.textContent = '–';
        avgPacePer100El.textContent = 'Per 100: –';
        totalDistanceEl.textContent = '–';
        totalTimeEl.textContent = 'Total time: –';
        return;
      }

      const avgSwolfPerLength = totalSwolfSum / totalLengths;
      const base = pool.length >= 37 ? 50 : 25;
      const factor = base / pool.length;
      const avgSwolfPerBase = avgSwolfPerLength * factor;

      const totalDistance = totalLengths * pool.length;
      const avgPacePerLength = totalTimeSec / totalLengths;
      const avgPacePer100 = (totalTimeSec / totalDistance) * 100;

      avgSwolfEl.textContent = avgSwolfPerLength.toFixed(1);
      avgSwolfPer25El.textContent = `Per ${base}${pool.unit}: ${avgSwolfPerBase.toFixed(1)}`;
      avgPaceEl.textContent = formatSecondsToPace(avgPacePerLength);
      avgPacePer100El.textContent = `Per 100${pool.unit}: ${formatSecondsToPace(avgPacePer100)}`;
      totalDistanceEl.textContent = `${totalDistance.toFixed(1)} ${pool.unit}`;
      totalTimeEl.textContent = `Total time: ${formatSecondsToPace(totalTimeSec)}`;
    }

    poolLengthSelect.addEventListener('change', () => {
      if (poolLengthSelect.value === 'custom') {
        customPoolWrapper.classList.remove('hidden');
      } else {
        customPoolWrapper.classList.add('hidden');
      }
      recalc();
    });

    [customPoolLength, customPoolUnit].forEach(el => {
      el.addEventListener('input', recalc);
      el.addEventListener('change', recalc);
    });

    addRowBtn.addEventListener('click', () => {
      addRow();
    });

    clearAllBtn.addEventListener('click', () => {
      swolfRows.innerHTML = '';
      rowCount = 0;
      recalc();
    });

    swolfRows.addEventListener('input', (e) => {
      if (e.target.classList.contains('lengths-input') ||
          e.target.classList.contains('time-input') ||
          e.target.classList.contains('strokes-input')) {
        recalc();
      }
    });

    swolfRows.addEventListener('click', (e) => {
      if (e.target.classList.contains('remove-row')) {
        const row = e.target.closest('tr');
        if (row) row.remove();
        recalc();
      }
    });

    // Initialize with one empty row
    addRow(true);
  });
</script>

