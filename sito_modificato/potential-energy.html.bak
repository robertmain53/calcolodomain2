    <script>
      (function () {
        const g = 9.81; // m/s^2

        const tabs = document.querySelectorAll(".pe-tab");
        const solveForSelect = document.getElementById("solveFor");
        const inputFieldsDiv = document.getElementById("inputFields");
        const calcBtn = document.getElementById("calcBtn");
        const resetBtn = document.getElementById("resetBtn");
        const resultBox = document.getElementById("resultBox");
        const resultValue = document.getElementById("resultValue");
        const resultSteps = document.getElementById("resultSteps");

        let mode = "grav";

        const unitOptions = {
          energy: [
            { value: "J", label: "J" },
            { value: "kJ", label: "kJ" },
            { value: "MJ", label: "MJ" },
          ],
          mass: [
            { value: "kg", label: "kg" },
            { value: "g", label: "g" },
            { value: "lb", label: "lb" },
          ],
          length: [
            { value: "m", label: "m" },
            { value: "cm", label: "cm" },
            { value: "mm", label: "mm" },
            { value: "ft", label: "ft" },
          ],
          k: [{ value: "N/m", label: "N/m" }],
          charge: [
            { value: "C", label: "C" },
            { value: "mC", label: "mC" },
            { value: "uC", label: "µC" },
            { value: "nC", label: "nC" },
          ],
          field: [{ value: "V/m", label: "V/m" }],
        };

        function createInput(id, label, type, unitType) {
          const wrapper = document.createElement("div");
          wrapper.className = "flex flex-col text-sm";

          const labelEl = document.createElement("label");
          labelEl.htmlFor = id;
          labelEl.className = "mb-1 font-medium text-gray-700";
          labelEl.textContent = label;

          const row = document.createElement("div");
          row.className = "flex";

          const input = document.createElement("input");
          input.type = "number";
          input.step = "any";
          input.id = id;
          input.className =
            "flex-1 border rounded-l-md px-3 py-2 focus:outline-none focus:ring-1 focus:ring-blue-500";
          input.placeholder = "Enter value";

          const select = document.createElement("select");
          select.id = id + "Unit";
          select.className =
            "border border-l-0 rounded-r-md px-2 py-2 bg-gray-50 text-gray-700 text-xs";

          (unitOptions[unitType] || []).forEach((u) => {
            const opt = document.createElement("option");
            opt.value = u.value;
            opt.textContent = u.label;
            select.appendChild(opt);
          });

          row.appendChild(input);
          row.appendChild(select);

          wrapper.appendChild(labelEl);
          wrapper.appendChild(row);
          return wrapper;
        }

        function renderInputs() {
          inputFieldsDiv.innerHTML = "";
          const solveFor = solveForSelect.value;

          if (mode === "grav") {
            if (solveFor !== "U") {
              inputFieldsDiv.appendChild(
                createInput("U", "Potential energy U", "number", "energy")
              );
            }
            if (solveFor !== "m") {
              inputFieldsDiv.appendChild(
                createInput("m", "Mass m", "number", "mass")
              );
            }
            if (solveFor !== "h") {
              inputFieldsDiv.appendChild(
                createInput("h", "Height h", "number", "length")
              );
            }
          } else if (mode === "spring") {
            if (solveFor !== "U") {
              inputFieldsDiv.appendChild(
                createInput("U", "Potential energy U", "number", "energy")
              );
            }
            inputFieldsDiv.appendChild(
              createInput("k", "Spring constant k", "number", "k")
            );
            inputFieldsDiv.appendChild(
              createInput("x", "Displacement x", "number", "length")
            );
          } else if (mode === "electric") {
            if (solveFor !== "U") {
              inputFieldsDiv.appendChild(
                createInput("U", "Potential energy U", "number", "energy")
              );
            }
            inputFieldsDiv.appendChild(
              createInput("q", "Charge q", "number", "charge")
            );
            inputFieldsDiv.appendChild(
              createInput("E", "Electric field E", "number", "field")
            );
            inputFieldsDiv.appendChild(
              createInput("d", "Displacement d", "number", "length")
            );
          }
        }

        function toSI(value, unit, type) {
          const v = parseFloat(value);
          if (isNaN(v)) return NaN;
          switch (type) {
            case "energy":
              if (unit === "kJ") return v * 1e3;
              if (unit === "MJ") return v * 1e6;
              return v;
            case "mass":
              if (unit === "g") return v / 1e3;
              if (unit === "lb") return v * 0.45359237;
              return v;
            case "length":
              if (unit === "cm") return v / 100;
              if (unit === "mm") return v / 1000;
              if (unit === "ft") return v * 0.3048;
              return v;
            case "charge":
              if (unit === "mC") return v * 1e-3;
              if (unit === "uC") return v * 1e-6;
              if (unit === "nC") return v * 1e-9;
              return v;
            case "field":
              return v; // V/m
            case "k":
              return v; // N/m
            default:
              return v;
          }
        }

        function fromSI(value, unit, type) {
          const v = value;
          switch (type) {
            case "energy":
              if (unit === "kJ") return v / 1e3;
              if (unit === "MJ") return v / 1e6;
              return v;
            default:
              return v;
          }
        }

        function formatNumber(x) {
          if (!isFinite(x)) return "NaN";
          const ax = Math.abs(x);
          if (ax !== 0 && (ax < 1e-3 || ax >= 1e5)) {
            return x.toExponential(3);
          }
          return x.toFixed(4).replace(/\.?0+$/, "");
        }

        function calculate() {
          const solveFor = solveForSelect.value;
          resultBox.classList.remove("hidden");
          resultSteps.innerHTML = "";
          resultValue.textContent = "";

          try {
            if (mode === "grav") {
              const UInput = document.getElementById("U");
              const mInput = document.getElementById("m");
              const hInput = document.getElementById("h");

              let U_SI, m_SI, h_SI;

              if (solveFor !== "U") {
                const UUnit = document.getElementById("UUnit").value;
                U_SI = toSI(UInput.value, UUnit, "energy");
              }
              if (solveFor !== "m") {
                const mUnit = document.getElementById("mUnit").value;
                m_SI = toSI(mInput.value, mUnit, "mass");
              }
              if (solveFor !== "h") {
                const hUnit = document.getElementById("hUnit").value;
                h_SI = toSI(hInput.value, hUnit, "length");
              }

              let result,
                stepsHtml = "";

              if (solveFor === "U") {
                if (isNaN(m_SI) || isNaN(h_SI))
                  throw new Error("Please enter valid mass and height.");
                result = m_SI * g * h_SI;
                const outUnit = document.getElementById("UUnit").value;
                const outVal = fromSI(result, outUnit, "energy");
                resultValue.textContent = `U = ${formatNumber(
                  outVal
                )} ${outUnit}`;
                stepsHtml = `
            <p>Using \( U = mgh \)</p>
            <p>\( m = ${formatNumber(
              m_SI
            )}\\,\\text{kg},\\ g = 9.81\\,\\text{m/s}^2,\\ h = ${formatNumber(
                  h_SI
                )}\\,\\text{m} \)</p>
            <p>\( U = ${formatNumber(m_SI)} \\times 9.81 \\times ${formatNumber(
                  h_SI
                )} = ${formatNumber(result)}\\,\\text{J} \)</p>
          `;
              } else if (solveFor === "m") {
                if (isNaN(U_SI) || isNaN(h_SI) || h_SI === 0)
                  throw new Error("Please enter valid U and non-zero h.");
                result = U_SI / (g * h_SI);
                const outUnit = document.getElementById("mUnit").value;
                let outVal = result;
                if (outUnit === "g") outVal = result * 1e3;
                if (outUnit === "lb") outVal = result / 0.45359237;
                resultValue.textContent = `m = ${formatNumber(
                  outVal
                )} ${outUnit}`;
                stepsHtml = `
            <p>From \( U = mgh \Rightarrow m = \\dfrac{U}{gh} \)</p>
            <p>\( U = ${formatNumber(
              U_SI
            )}\\,\\text{J},\\ g = 9.81\\,\\text{m/s}^2,\\ h = ${formatNumber(
                  h_SI
                )}\\,\\text{m} \)</p>
            <p>\( m = \\dfrac{${formatNumber(
              U_SI
            )}}{9.81 \\times ${formatNumber(h_SI)}} = ${formatNumber(
                  result
                )}\\,\\text{kg} \)</p>
          `;
              } else if (solveFor === "h") {
                if (isNaN(U_SI) || isNaN(m_SI) || m_SI === 0)
                  throw new Error("Please enter valid U and non-zero m.");
                result = U_SI / (m_SI * g);
                const outUnit = document.getElementById("hUnit").value;
                let outVal = result;
                if (outUnit === "cm") outVal = result * 100;
                if (outUnit === "mm") outVal = result * 1000;
                if (outUnit === "ft") outVal = result / 0.3048;
                resultValue.textContent = `h = ${formatNumber(
                  outVal
                )} ${outUnit}`;
                stepsHtml = `
            <p>From \( U = mgh \Rightarrow h = \\dfrac{U}{mg} \)</p>
            <p>\( U = ${formatNumber(U_SI)}\\,\\text{J},\\ m = ${formatNumber(
                  m_SI
                )}\\,\\text{kg},\\ g = 9.81\\,\\text{m/s}^2 \)</p>
            <p>\( h = \\dfrac{${formatNumber(U_SI)}}{${formatNumber(
                  m_SI
                )} \\times 9.81} = ${formatNumber(result)}\\,\\text{m} \)</p>
          `;
              }

              resultSteps.innerHTML = stepsHtml;
              if (window.MathJax) MathJax.typesetPromise();
            } else if (mode === "spring") {
              const UInput = document.getElementById("U");
              const kInput = document.getElementById("k");
              const xInput = document.getElementById("x");

              let U_SI, k_SI, x_SI;

              if (solveFor !== "U") {
                const UUnit = document.getElementById("UUnit").value;
                U_SI = toSI(UInput.value, UUnit, "energy");
              }
              k_SI = toSI(
                kInput.value,
                document.getElementById("kUnit").value,
                "k"
              );
              x_SI = toSI(
                xInput.value,
                document.getElementById("xUnit").value,
                "length"
              );

              let result,
                stepsHtml = "";

              if (solveFor === "U") {
                if (isNaN(k_SI) || isNaN(x_SI))
                  throw new Error("Please enter valid k and x.");
                result = 0.5 * k_SI * x_SI * x_SI;
                const outUnit = document.getElementById("UUnit").value;
                const outVal = fromSI(result, outUnit, "energy");
                resultValue.textContent = `U = ${formatNumber(
                  outVal
                )} ${outUnit}`;
                stepsHtml = `
            <p>Using \( U = \\dfrac{1}{2}kx^2 \)</p>
            <p>\( k = ${formatNumber(k_SI)}\\,\\text{N/m},\\ x = ${formatNumber(
                  x_SI
                )}\\,\\text{m} \)</p>
            <p>\( U = \\dfrac{1}{2} \\times ${formatNumber(
              k_SI
            )} \\times (${formatNumber(x_SI)})^2 = ${formatNumber(
                  result
                )}\\,\\text{J} \)</p>
          `;
              } else {
                throw new Error(
                  "For spring mode this tool currently solves only for U."
                );
              }

              resultSteps.innerHTML = stepsHtml;
              if (window.MathJax) MathJax.typesetPromise();
            } else if (mode === "electric") {
              const UInput = document.getElementById("U");
              const qInput = document.getElementById("q");
              const EInput = document.getElementById("E");
              const dInput = document.getElementById("d");

              let U_SI, q_SI, E_SI, d_SI;

              if (solveFor !== "U") {
                const UUnit = document.getElementById("UUnit").value;
                U_SI = toSI(UInput.value, UUnit, "energy");
              }
              q_SI = toSI(
                qInput.value,
                document.getElementById("qUnit").value,
                "charge"
              );
              E_SI = toSI(
                EInput.value,
                document.getElementById("EUnit").value,
                "field"
              );
              d_SI = toSI(
                dInput.value,
                document.getElementById("dUnit").value,
                "length"
              );

              let result,
                stepsHtml = "";

              if (solveFor === "U") {
                if (isNaN(q_SI) || isNaN(E_SI) || isNaN(d_SI))
                  throw new Error("Please enter valid q, E and d.");
                result = q_SI * E_SI * d_SI;
                const outUnit = document.getElementById("UUnit").value;
                const outVal = fromSI(result, outUnit, "energy");
                resultValue.textContent = `U = ${formatNumber(
                  outVal
                )} ${outUnit}`;
                stepsHtml = `
            <p>Using \( U = qEd \)</p>
            <p>\( q = ${formatNumber(q_SI)}\\,\\text{C},\\ E = ${formatNumber(
                  E_SI
                )}\\,\\text{V/m},\\ d = ${formatNumber(d_SI)}\\,\\text{m} \)</p>
            <p>\( U = ${formatNumber(q_SI)} \\times ${formatNumber(
                  E_SI
                )} \\times ${formatNumber(d_SI)} = ${formatNumber(
                  result
                )}\\,\\text{J} \)</p>
          `;
              } else {
                throw new Error(
                  "For electric mode this tool currently solves only for U."
                );
              }

              resultSteps.innerHTML = stepsHtml;
              if (window.MathJax) MathJax.typesetPromise();
            }
          } catch (err) {
            resultValue.textContent = "Error";
            resultSteps.textContent = err.message;
          }
        }

        tabs.forEach((btn) => {
          btn.addEventListener("click", () => {
            tabs.forEach((b) => b.classList.remove("tab-active"));
            btn.classList.add("tab-active");
            mode = btn.getAttribute("data-mode");

            if (mode === "grav") {
              solveForSelect.disabled = false;
            } else {
              // For now, only solve for U in spring/electric
              solveForSelect.value = "U";
              solveForSelect.disabled = true;
            }
            renderInputs();
            resultBox.classList.add("hidden");
          });
        });

        solveForSelect.addEventListener("change", () => {
          renderInputs();
          resultBox.classList.add("hidden");
        });

        calcBtn.addEventListener("click", calculate);

        resetBtn.addEventListener("click", () => {
          document
            .querySelectorAll("#inputFields input")
            .forEach((inp) => (inp.value = ""));
          resultBox.classList.add("hidden");
        });

        // Initial render
        renderInputs();
      })();
    </script>
  
