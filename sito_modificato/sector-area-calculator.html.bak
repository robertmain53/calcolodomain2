    <script>
      const tabRdeg = document.getElementById('tab-rdeg');
      const tabRrad = document.getElementById('tab-rrad');
      const tabDiam = document.getElementById('tab-diam');
      const tabArc = document.getElementById('tab-arc');
      const tabPct = document.getElementById('tab-percent');

      const panelRdeg = document.getElementById('panel-rdeg');
      const panelRrad = document.getElementById('panel-rrad');
      const panelDiam = document.getElementById('panel-diam');
      const panelArc = document.getElementById('panel-arc');
      const panelPct = document.getElementById('panel-percent');

      function activateTab(btn, panel) {
        [tabRdeg, tabRrad, tabDiam, tabArc, tabPct].forEach(b => b.classList.remove('tab-active'));
        btn.classList.add('tab-active');
        [panelRdeg, panelRrad, panelDiam, panelArc, panelPct].forEach(p => p.classList.add('hidden'));
        panel.classList.remove('hidden');
      }

      tabRdeg.addEventListener('click', () => activateTab(tabRdeg, panelRdeg));
      tabRrad.addEventListener('click', () => activateTab(tabRrad, panelRrad));
      tabDiam.addEventListener('click', () => activateTab(tabDiam, panelDiam));
      tabArc.addEventListener('click', () => activateTab(tabArc, panelArc));
      tabPct.addEventListener('click', () => activateTab(tabPct, panelPct));

      const btnCalc = document.getElementById('btn-calc');
      const btnClear = document.getElementById('btn-clear');

      const resBox = document.getElementById('results-box');
      const resArea = document.getElementById('res-area');
      const resArc = document.getElementById('res-arc');
      const resPerim = document.getElementById('res-perim');
      const resVol = document.getElementById('res-vol');
      const resDeg = document.getElementById('res-deg');
      const resRad = document.getElementById('res-rad');
      const resCircle = document.getElementById('res-circle');
      const resPct = document.getElementById('res-pct');

      function round4(x) {
        return Math.round((x + Number.EPSILON) * 10000) / 10000;
      }

      btnCalc.addEventListener('click', () => {
        let r, angleDeg, angleRad, arc, area, perimeter, circleArea, pct, volume = '—';

        // radius + angle (deg)
        if (!panelRdeg.classList.contains('hidden')) {
          r = parseFloat(document.getElementById('rdeg-r').value);
          angleDeg = parseFloat(document.getElementById('rdeg-a').value);
          const h = parseFloat(document.getElementById('rdeg-h').value);
          if (isNaN(r) || isNaN(angleDeg) || r <= 0) { alert('Please enter radius and angle in degrees.'); return; }
          angleRad = angleDeg * Math.PI / 180;
          area = (angleDeg / 360) * Math.PI * r * r;
          arc = (angleDeg / 360) * 2 * Math.PI * r;
          perimeter = 2 * r + arc;
          circleArea = Math.PI * r * r;
          pct = (angleDeg / 360) * 100;
          if (!isNaN(h) && h > 0) {
            volume = round4(area * h);
          }

        // radius + angle (rad)
        } else if (!panelRrad.classList.contains('hidden')) {
          r = parseFloat(document.getElementById('rrad-r').value);
          angleRad = parseFloat(document.getElementById('rrad-a').value);
          const h = parseFloat(document.getElementById('rrad-h').value);
          if (isNaN(r) || isNaN(angleRad) || r <= 0 || angleRad <= 0) { alert('Please enter radius and angle in radians.'); return; }
          angleDeg = angleRad * 180 / Math.PI;
          area = 0.5 * r * r * angleRad;
          arc = r * angleRad;
          perimeter = 2 * r + arc;
          circleArea = Math.PI * r * r;
          pct = (angleDeg / 360) * 100;
          if (!isNaN(h) && h > 0) {
            volume = round4(area * h);
          }

        // diameter + angle
        } else if (!panelDiam.classList.contains('hidden')) {
          const d = parseFloat(document.getElementById('diam-d').value);
          angleDeg = parseFloat(document.getElementById('diam-a').value);
          const h = parseFloat(document.getElementById('diam-h').value);
          if (isNaN(d) || isNaN(angleDeg) || d <= 0) { alert('Please enter diameter and angle.'); return; }
          r = d / 2;
          angleRad = angleDeg * Math.PI / 180;
          area = (angleDeg / 360) * Math.PI * r * r;
          arc = (angleDeg / 360) * 2 * Math.PI * r;
          perimeter = 2 * r + arc;
          circleArea = Math.PI * r * r;
          pct = (angleDeg / 360) * 100;
          if (!isNaN(h) && h > 0) {
            volume = round4(area * h);
          }

        // arc + radius
        } else if (!panelArc.classList.contains('hidden')) {
          arc = parseFloat(document.getElementById('arc-l').value);
          r = parseFloat(document.getElementById('arc-r').value);
          const h = parseFloat(document.getElementById('arc-h').value);
          if (isNaN(arc) || isNaN(r) || arc <= 0 || r <= 0) { alert('Please enter arc length and radius.'); return; }
          angleRad = arc / r;
          angleDeg = angleRad * 180 / Math.PI;
          area = 0.5 * r * arc;
          perimeter = 2 * r + arc;
          circleArea = Math.PI * r * r;
          pct = (angleDeg / 360) * 100;
          if (!isNaN(h) && h > 0) {
            volume = round4(area * h);
          }

        // percent of circle
        } else {
          r = parseFloat(document.getElementById('pct-r').value);
          pct = parseFloat(document.getElementById('pct-p').value);
          const h = parseFloat(document.getElementById('pct-h').value);
          if (isNaN(r) || isNaN(pct) || r <= 0) { alert('Please enter radius and percentage.'); return; }
          circleArea = Math.PI * r * r;
          area = (pct / 100) * circleArea;
          angleDeg = (pct / 100) * 360;
          angleRad = angleDeg * Math.PI / 180;
          arc = (angleDeg / 360) * 2 * Math.PI * r;
          perimeter = 2 * r + arc;
          if (!isNaN(h) && h > 0) {
            volume = round4(area * h);
          }
        }

        resBox.classList.remove('hidden');
        resArea.textContent = round4(area);
        resArc.textContent = round4(arc);
        resPerim.textContent = round4(perimeter);
        resVol.textContent = volume;
        resDeg.textContent = round4(angleDeg) + '°';
        resRad.textContent = round4(angleRad);
        resCircle.textContent = round4(circleArea);
        resPct.textContent = round4(pct) + '%';
      });

      btnClear.addEventListener('click', () => {
        document.getElementById('rdeg-r').value = 10;
        document.getElementById('rdeg-a').value = 60;
        document.getElementById('rdeg-h').value = '';
        document.getElementById('rrad-r').value = 5;
        document.getElementById('rrad-a').value = 1.0472;
        document.getElementById('rrad-h').value = '';
        document.getElementById('diam-d').value = 20;
        document.getElementById('diam-a').value = 90;
        document.getElementById('diam-h').value = '';
        document.getElementById('arc-l').value = 12;
        document.getElementById('arc-r').value = 10;
        document.getElementById('arc-h').value = '';
        document.getElementById('pct-r').value = 8;
        document.getElementById('pct-p').value = 25;
        document.getElementById('pct-h').value = '';
        resBox.classList.add('hidden');
      });
    </script>

    <script>
