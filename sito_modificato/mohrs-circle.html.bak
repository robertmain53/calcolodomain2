    <script>
      // Utility: format number with fixed decimals and trim
      function fmt(val, decimals = 3) {
        if (!isFinite(val)) return "–";
        const s = val.toFixed(decimals);
        return s.replace(/\.?0+$/, "");
      }

      function degToRad(d) {
        return (d * Math.PI) / 180;
      }
      function radToDeg(r) {
        return (r * 180) / Math.PI;
      }

      function computeMohr() {
        const sigmaX = parseFloat(document.getElementById("sigmaX").value);
        const sigmaY = parseFloat(document.getElementById("sigmaY").value);
        const tauXY = parseFloat(document.getElementById("tauXY").value);
        const thetaVal = parseFloat(document.getElementById("theta").value);
        const thetaUnit = document.getElementById("thetaUnit").value;

        if (!isFinite(sigmaX) || !isFinite(sigmaY) || !isFinite(tauXY)) {
          alert("Please enter valid numeric values for σx, σy and τxy.");
          return null;
        }

        const C = 0.5 * (sigmaX + sigmaY);
        const diff = 0.5 * (sigmaX - sigmaY);
        const R = Math.sqrt(diff * diff + tauXY * tauXY);

        const sigma1 = C + R;
        const sigma2 = C - R;
        const tauMax = R;

        // principal angle
        let thetaP = 0;
        if (Math.abs(sigmaX - sigmaY) < 1e-12 && Math.abs(tauXY) < 1e-12) {
          thetaP = 0; // isotropic
        } else {
          const twoThetaP = 0.5 * Math.atan2(2 * tauXY, sigmaX - sigmaY) * 2; // simplify
          thetaP = 0.5 * Math.atan2(2 * tauXY, sigmaX - sigmaY);
        }
        const thetaS = thetaP + Math.PI / 4; // 45° from principal

        // stresses on plane theta
        let theta = isFinite(thetaVal) ? thetaVal : 0;
        if (thetaUnit === "deg") theta = degToRad(theta);

        const cos2 = Math.cos(2 * theta);
        const sin2 = Math.sin(2 * theta);

        const sigmaN = C + diff * cos2 + tauXY * sin2;
        const tauN = -diff * sin2 + tauXY * cos2;

        return {
          sigmaX,
          sigmaY,
          tauXY,
          C,
          R,
          sigma1,
          sigma2,
          tauMax,
          theta,
          thetaP,
          thetaS,
          sigmaN,
          tauN,
        };
      }

      function updateResults(res) {
        if (!res) return;
        const unit = document.getElementById("unit").value;

        document.getElementById("sigma1Out").innerHTML =
          "σ₁: " + fmt(res.sigma1) + " " + unit;
        document.getElementById("sigma2Out").innerHTML =
          "σ₂: " + fmt(res.sigma2) + " " + unit;
        document.getElementById("tauMaxOut").innerHTML =
          "τ<sub>max</sub>: " + fmt(res.tauMax) + " " + unit;
        document.getElementById("centerOut").innerHTML =
          "Circle center C: " + fmt(res.C) + " " + unit;
        document.getElementById("radiusOut").innerHTML =
          "Radius R: " + fmt(res.R) + " " + unit;

        document.getElementById("sigmaThetaOut").innerHTML =
          "σ<sub>n</sub>(θ): " + fmt(res.sigmaN) + " " + unit;
        document.getElementById("tauThetaOut").innerHTML =
          "τ<sub>n</sub>(θ): " + fmt(res.tauN) + " " + unit;

        document.getElementById("thetaPOut").innerHTML =
          "θ<sub>p</sub> to σ₁ plane: " + fmt(radToDeg(res.thetaP), 2) + "°";
        document.getElementById("thetaSOut").innerHTML =
          "θ<sub>s</sub> to τ<sub>max</sub> plane: " +
          fmt(radToDeg(res.thetaS), 2) +
          "°";
      }

      function drawMohr(res) {
        const canvas = document.getElementById("mohrCanvas");
        if (!canvas || !canvas.getContext) return;
        const ctx = canvas.getContext("2d");
        const w = canvas.width;
        const h = canvas.height;

        ctx.clearRect(0, 0, w, h);

        // If radius is zero, nothing meaningful to draw
        if (!isFinite(res.R) || res.R <= 0) {
          ctx.fillStyle = "#4b5563";
          ctx.font = "14px Inter, sans-serif";
          ctx.fillText(
            "Enter a non-trivial stress state to see Mohr's Circle.",
            20,
            30
          );
          return;
        }

        // Determine scale: leave margins
        const margin = 50;
        const sigmaMin = Math.min(
          res.sigma1,
          res.sigma2,
          res.sigmaX,
          res.sigmaY,
          res.sigmaN
        );
        const sigmaMax = Math.max(
          res.sigma1,
          res.sigma2,
          res.sigmaX,
          res.sigmaY,
          res.sigmaN
        );
        const tauMin = Math.min(-res.R, res.tauXY, res.tauN);
        const tauMax = Math.max(res.R, res.tauXY, res.tauN);

        const sigmaSpan = sigmaMax - sigmaMin || 1;
        const tauSpan = tauMax - tauMin || 1;

        const xScale = (w - 2 * margin) / sigmaSpan;
        const yScale = (h - 2 * margin) / tauSpan;

        function toCanvasX(sigma) {
          return margin + (sigma - sigmaMin) * xScale;
        }
        function toCanvasY(tau) {
          // y axis inverted in canvas
          return h - margin - (tau - tauMin) * yScale;
        }

        // Draw axes
        ctx.strokeStyle = "#d1d5db";
        ctx.lineWidth = 1;

        // σ axis (horizontal through τ=0)
        const yZero = toCanvasY(0);
        ctx.beginPath();
        ctx.moveTo(margin, yZero);
        ctx.lineTo(w - margin, yZero);
        ctx.stroke();

        // τ axis (vertical through σ=0 if in range, else through center)
        const sigmaAxis = 0 >= sigmaMin && 0 <= sigmaMax ? 0 : res.C;
        const xZero = toCanvasX(sigmaAxis);
        ctx.beginPath();
        ctx.moveTo(xZero, margin);
        ctx.lineTo(xZero, h - margin);
        ctx.stroke();

        // Axis labels
        ctx.fillStyle = "#4b5563";
        ctx.font = "12px Inter, sans-serif";
        ctx.fillText("σ", w - margin + 10, yZero + 4);
        ctx.fillText("τ", xZero - 4, margin - 10);

        // Draw circle
        const centerX = toCanvasX(res.C);
        const centerY = toCanvasY(0);
        const radiusPx = res.R * xScale; // use xScale for circle

        ctx.strokeStyle = "#2563eb";
        ctx.lineWidth = 2;
        ctx.beginPath();
        ctx.arc(centerX, centerY, radiusPx, 0, 2 * Math.PI);
        ctx.stroke();

        // Helper to draw point
        function drawPoint(sigma, tau, color, label) {
          const x = toCanvasX(sigma);
          const y = toCanvasY(tau);
          ctx.fillStyle = color;
          ctx.beginPath();
          ctx.arc(x, y, 4, 0, 2 * Math.PI);
          ctx.fill();
          if (label) {
            ctx.fillStyle = "#111827";
            ctx.font = "11px Inter, sans-serif";
            ctx.fillText(label, x + 6, y - 6);
          }
        }

        // Original stress points
        drawPoint(res.sigmaX, res.tauXY, "#dc2626", "A (σx, τxy)");
        drawPoint(res.sigmaY, -res.tauXY, "#16a34a", "B (σy, -τxy)");

        // Principal stresses (on σ axis)
        drawPoint(res.sigma1, 0, "#7c3aed", "σ1");
        drawPoint(res.sigma2, 0, "#7c3aed", "σ2");

        // Plane θ point
        drawPoint(res.sigmaN, res.tauN, "#f97316", "θ");

        // Center
        drawPoint(res.C, 0, "#0f172a", "C");

        // Legend
        const legendX = margin;
        const legendY = margin - 30;
        ctx.fillStyle = "#111827";
        ctx.font = "12px Inter, sans-serif";
        ctx.fillText(
          "Mohr's Circle: A(σx, τxy), B(σy, -τxy), σ1, σ2, θ-plane",
          legendX,
          legendY
        );
      }

      document.getElementById("computeBtn").addEventListener("click", () => {
        const res = computeMohr();
        if (!res) return;
        updateResults(res);
        drawMohr(res);
      });

      document.getElementById("resetBtn").addEventListener("click", () => {
        document.getElementById("sigmaX").value = 80;
        document.getElementById("sigmaY").value = 20;
        document.getElementById("tauXY").value = 30;
        document.getElementById("theta").value = 0;
        document.getElementById("unit").value = "MPa";
        document.getElementById("thetaUnit").value = "deg";
        const res = computeMohr();
        if (res) {
          updateResults(res);
          drawMohr(res);
        }
      });

      // Initial draw with default values
      window.addEventListener("load", () => {
        const res = computeMohr();
        if (res) {
          updateResults(res);
          drawMohr(res);
        }
      });

      // Mobile menu script (simple toggle)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", () => {
          const menu = document.getElementById("mobile-menu");
          const expanded = menu.classList.contains("hidden") ? "true" : "false";
          menu.classList.toggle("hidden");
          document
            .getElementById("mobile-menu-toggle")
            .setAttribute("aria-expanded", expanded);
        });
    </script>

    <script>
      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
