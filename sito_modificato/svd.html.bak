    <script>
      // Utility functions for matrix operations and SVD (Jacobi eigenvalue method on A^T A)
      function parseMatrix(text) {
        if (!text.trim()) throw new Error("Please enter a matrix.");
        const rows = text
          .trim()
          .replace(/;/g, "\n")
          .split(/\n+/)
          .map((r) => r.trim())
          .filter((r) => r.length > 0);

        const matrix = rows
          .map((r) => {
            const parts = r.split(/[\s,]+/).filter((x) => x.length > 0);
            if (parts.length === 0) return null;
            return parts.map((v) => {
              const num = Number(v);
              if (!isFinite(num))
                throw new Error("All entries must be real numbers.");
              return num;
            });
          })
          .filter((r) => r !== null);

        const m = matrix.length;
        const n = matrix[0].length;
        if (m === 0 || n === 0) throw new Error("Matrix cannot be empty.");
        if (m > 8 || n > 8)
          throw new Error("Matrix size is limited to 8×8 for this calculator.");
        for (let i = 1; i < m; i++) {
          if (matrix[i].length !== n)
            throw new Error("All rows must have the same number of columns.");
        }
        return matrix;
      }

      function zeros(r, c) {
        const A = new Array(r);
        for (let i = 0; i < r; i++) {
          A[i] = new Array(c).fill(0);
        }
        return A;
      }

      function transpose(A) {
        const m = A.length,
          n = A[0].length;
        const AT = zeros(n, m);
        for (let i = 0; i < m; i++) {
          for (let j = 0; j < n; j++) {
            AT[j][i] = A[i][j];
          }
        }
        return AT;
      }

      function matMul(A, B) {
        const m = A.length,
          n = A[0].length,
          p = B[0].length;
        if (B.length !== n)
          throw new Error("Incompatible matrix dimensions for multiplication.");
        const C = zeros(m, p);
        for (let i = 0; i < m; i++) {
          for (let k = 0; k < n; k++) {
            const aik = A[i][k];
            for (let j = 0; j < p; j++) {
              C[i][j] += aik * B[k][j];
            }
          }
        }
        return C;
      }

      function frobeniusNorm(A) {
        let s = 0;
        for (let i = 0; i < A.length; i++) {
          for (let j = 0; j < A[0].length; j++) {
            s += A[i][j] * A[i][j];
          }
        }
        return Math.sqrt(s);
      }

      // Jacobi eigenvalue algorithm for symmetric matrices
      function jacobiEigen(A, maxIter = 100, tol = 1e-12) {
        const n = A.length;
        const V = zeros(n, n);
        for (let i = 0; i < n; i++) V[i][i] = 1;
        const B = A.map((row) => row.slice());

        for (let iter = 0; iter < maxIter; iter++) {
          // find largest off-diagonal element
          let p = 0,
            q = 1;
          let max = Math.abs(B[p][q]);
          for (let i = 0; i < n; i++) {
            for (let j = i + 1; j < n; j++) {
              const val = Math.abs(B[i][j]);
              if (val > max) {
                max = val;
                p = i;
                q = j;
              }
            }
          }
          if (max < tol) break;

          const app = B[p][p],
            aqq = B[q][q],
            apq = B[p][q];
          const phi = 0.5 * Math.atan2(2 * apq, aqq - app);
          const c = Math.cos(phi),
            s = Math.sin(phi);

          // rotate B
          for (let i = 0; i < n; i++) {
            const bip = B[i][p],
              biq = B[i][q];
            B[i][p] = c * bip - s * biq;
            B[i][q] = s * bip + c * biq;
          }
          for (let i = 0; i < n; i++) {
            const bpi = B[p][i],
              bqi = B[q][i];
            B[p][i] = c * bpi - s * bqi;
            B[q][i] = s * bpi + c * bqi;
          }
          B[p][p] = c * c * app - 2 * s * c * apq + s * s * aqq;
          B[q][q] = s * s * app + 2 * s * c * apq + c * c * aqq;
          B[p][q] = B[q][p] = 0;

          // accumulate eigenvectors
          for (let i = 0; i < n; i++) {
            const vip = V[i][p],
              viq = V[i][q];
            V[i][p] = c * vip - s * viq;
            V[i][q] = s * vip + c * viq;
          }
        }

        const eigenvalues = new Array(n);
        for (let i = 0; i < n; i++) eigenvalues[i] = B[i][i];

        return { eigenvalues, eigenvectors: V };
      }

      function svdDecompose(A) {
        const m = A.length,
          n = A[0].length;
        const AT = transpose(A);
        const ATA = matMul(AT, A); // n x n symmetric

        const { eigenvalues, eigenvectors } = jacobiEigen(ATA);
        const eig = eigenvalues.map((lambda, idx) => ({
          lambda,
          vec: eigenvectors.map((row) => row[idx]),
        }));
        // sort by descending eigenvalue
        eig.sort((a, b) => b.lambda - a.lambda);

        const tol = 1e-12;
        const singularValues = [];
        const V = zeros(n, n);
        for (let i = 0; i < n; i++) {
          const lambda = Math.max(eig[i].lambda, 0);
          const sigma = Math.sqrt(lambda);
          singularValues.push(sigma);
          for (let j = 0; j < n; j++) {
            V[j][i] = eig[i].vec[j];
          }
        }

        // Compute U columns: u_i = (1/sigma_i) A v_i
        const U = zeros(m, m);
        const Av = zeros(m, 1);
        for (let i = 0; i < Math.min(m, n); i++) {
          const sigma = singularValues[i];
          if (sigma > tol) {
            // v_i is column i of V
            const v_i = V.map((row) => [row[i]]);
            const Av_i = matMul(A, v_i); // m x 1
            for (let r = 0; r < m; r++) {
              U[r][i] = Av_i[r][0] / sigma;
            }
          } else {
            // if sigma is zero, leave column for later orthonormalization
            for (let r = 0; r < m; r++) U[r][i] = 0;
          }
        }
        // Fill remaining columns of U with an orthonormal basis (Gram-Schmidt)
        function normalizeColumn(M, col) {
          let norm = 0;
          for (let i = 0; i < m; i++) norm += M[i][col] * M[i][col];
          norm = Math.sqrt(norm);
          if (norm < tol) return false;
          for (let i = 0; i < m; i++) M[i][col] / norm;
          return true;
        }
        function subtractProjection(M, col, ontoCol) {
          let dot = 0;
          for (let i = 0; i < m; i++) dot += M[i][col] * U[i][ontoCol];
          for (let i = 0; i < m; i++) M[i][col] -= dot * U[i][ontoCol];
        }

        for (let j = 0; j < m; j++) {
          if (j < Math.min(m, n) && singularValues[j] > tol) {
            normalizeColumn(U, j);
          } else {
            // start with a random vector
            for (let i = 0; i < m; i++) U[i][j] = Math.random() - 0.5;
            // orthogonalize against previous columns
            for (let k = 0; k < j; k++) subtractProjection(U, j, k);
            if (!normalizeColumn(U, j)) {
              // fallback to standard basis if random failed
              for (let i = 0; i < m; i++) U[i][j] = i === j ? 1 : 0;
            }
          }
        }

        // Build full Sigma matrix (m x n)
        const Sigma = zeros(m, n);
        for (let i = 0; i < Math.min(m, n); i++) {
          Sigma[i][i] = singularValues[i];
        }

        return { U, Sigma, V, singularValues };
      }

      function formatNumber(x, decimals) {
        if (!isFinite(x)) return x.toString();
        const factor = Math.pow(10, decimals);
        const y = Math.round(x * factor) / factor;
        return y.toFixed(decimals);
      }

      function renderMatrix(A, decimals) {
        const rows = A.length,
          cols = A[0].length;
        let html = '<table class="matrix-table border-collapse"><tbody>';
        for (let i = 0; i < rows; i++) {
          html += "<tr>";
          for (let j = 0; j < cols; j++) {
            html += "<td>" + formatNumber(A[i][j], decimals) + "</td>";
          }
          html += "</tr>";
        }
        html += "</tbody></table>";
        return html;
      }

      function renderDiagonal(values, decimals) {
        let html = '<table class="matrix-table border-collapse"><tbody><tr>';
        for (let i = 0; i < values.length; i++) {
          html += "<td>" + formatNumber(values[i], decimals) + "</td>";
        }
        html += "</tr></tbody></table>";
        return html;
      }

      document.addEventListener("DOMContentLoaded", function () {
        const inputEl = document.getElementById("matrix-input");
        const exampleBtn = document.getElementById("example-btn");
        const clearBtn = document.getElementById("clear-btn");
        const computeBtn = document.getElementById("compute-btn");
        const errorMsg = document.getElementById("error-msg");
        const resultsSection = document.getElementById("results-section");
        const matrixSummary = document.getElementById("matrix-summary");
        const sigmaValuesEl = document.getElementById("sigma-values");
        const diagnosticsEl = document.getElementById("diagnostics");
        const uMatrixEl = document.getElementById("u-matrix");
        const sigmaMatrixEl = document.getElementById("sigma-matrix");
        const vtMatrixEl = document.getElementById("vt-matrix");
        const reconstructionSection = document.getElementById(
          "reconstruction-section"
        );
        const reconstructedMatrixEl = document.getElementById(
          "reconstructed-matrix"
        );
        const reconstructionErrorEl = document.getElementById(
          "reconstruction-error"
        );
        const reducedSvdEl = document.getElementById("reduced-svd");
        const decimalsEl = document.getElementById("decimals");
        const showReconstructionEl = document.getElementById(
          "show-reconstruction"
        );

        exampleBtn.addEventListener("click", function () {
          inputEl.value = "1 2\n3 4\n5 6";
        });

        clearBtn.addEventListener("click", function () {
          inputEl.value = "";
          errorMsg.classList.add("hidden");
          resultsSection.classList.add("hidden");
        });

        computeBtn.addEventListener("click", function () {
          errorMsg.classList.add("hidden");
          reconstructionSection.classList.add("hidden");
          try {
            const A = parseMatrix(inputEl.value);
            const m = A.length,
              n = A[0].length;
            let decimals = parseInt(decimalsEl.value, 10);
            if (!isFinite(decimals) || decimals < 0) decimals = 4;
            if (decimals > 10) decimals = 10;

            const { U, Sigma, V, singularValues } = svdDecompose(A);

            // Reduced SVD option: determine effective rank
            const maxSigma = Math.max.apply(null, singularValues);
            const tol = Math.max(m, n) * maxSigma * 1e-12;
            let rank = 0;
            for (let i = 0; i < singularValues.length; i++) {
              if (singularValues[i] > tol) rank++;
            }

            const useReduced = reducedSvdEl.checked;
            let r = useReduced ? rank : Math.min(m, n);

            // Build reduced matrices for display
            const Udisp = zeros(m, useReduced ? r : m);
            const Sigmadisp = zeros(useReduced ? r : m, useReduced ? r : n);
            const Vdisp = zeros(useReduced ? n : n, useReduced ? r : n); // full V but we may show only first r columns

            for (let i = 0; i < m; i++) {
              for (let j = 0; j < (useReduced ? r : m); j++) {
                Udisp[i][j] = U[i][j] || 0;
              }
            }
            for (let i = 0; i < (useReduced ? r : Math.min(m, n)); i++) {
              for (let j = 0; j < (useReduced ? r : n); j++) {
                Sigmadisp[i][j] = i === j ? singularValues[i] : 0;
              }
            }
            for (let i = 0; i < n; i++) {
              for (let j = 0; j < (useReduced ? r : n); j++) {
                Vdisp[i][j] = V[i][j] || 0;
              }
            }

            // Summary
            matrixSummary.innerHTML =
              `Input matrix A is <strong>${m} × ${n}</strong>. ` +
              `Computed ${
                useReduced ? "reduced" : "full"
              } SVD with numerical rank <strong>${rank}</strong>.`;

            // Singular values list
            sigmaValuesEl.innerHTML = renderDiagonal(singularValues, decimals);

            // Diagnostics
            diagnosticsEl.innerHTML = "";
            const liRank = document.createElement("li");
            liRank.textContent = "Numerical rank: " + rank;
            diagnosticsEl.appendChild(liRank);

            if (m === n && rank === n) {
              const nonZero = singularValues.filter((s) => s > tol);
              const kappa =
                nonZero.length > 0
                  ? nonZero[0] / nonZero[nonZero.length - 1]
                  : Infinity;
              const liCond = document.createElement("li");
              liCond.textContent =
                "Condition number κ(A): " + formatNumber(kappa, 4);
              diagnosticsEl.appendChild(liCond);
            } else {
              const liCond = document.createElement("li");
              liCond.textContent =
                "Condition number κ(A): defined only for square full‑rank matrices.";
              diagnosticsEl.appendChild(liCond);
            }

            const liNote = document.createElement("li");
            liNote.textContent =
              "Tolerance for zero singular values: " + formatNumber(tol, 4);
            diagnosticsEl.appendChild(liNote);

            // U, Sigma, V^T
            uMatrixEl.innerHTML = renderMatrix(Udisp, decimals);
            sigmaMatrixEl.innerHTML = renderMatrix(Sigmadisp, decimals);
            vtMatrixEl.innerHTML = renderMatrix(transpose(Vdisp), decimals);

            // Reconstruction
            if (showReconstructionEl.checked) {
              const Ahat = matMul(matMul(U, Sigma), transpose(V));
              const err = frobeniusNorm(
                A.map((row, i) => row.map((val, j) => val - Ahat[i][j]))
              );
              reconstructedMatrixEl.innerHTML = renderMatrix(Ahat, decimals);
              reconstructionErrorEl.textContent =
                "‖A − UΣVᵀ‖₍F₎ ≈ " + formatNumber(err, 6);
              reconstructionSection.classList.remove("hidden");
            }

            resultsSection.classList.remove("hidden");
          } catch (e) {
            errorMsg.textContent =
              e.message || "An error occurred while computing the SVD.";
            errorMsg.classList.remove("hidden");
            resultsSection.classList.add("hidden");
          }
        });
      });
    </script>

