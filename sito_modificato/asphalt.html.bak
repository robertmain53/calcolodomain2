    <script>
      (function () {
        const unitSystemEl = document.getElementById("unit-system");
        const lengthEl = document.getElementById("length");
        const widthEl = document.getElementById("width");
        const lengthUnitLabel = document.getElementById("length-unit-label");
        const widthUnitLabel = document.getElementById("width-unit-label");
        const thicknessEl = document.getElementById("thickness");
        const thicknessUnitEl = document.getElementById("thickness-unit");
        const compactionEl = document.getElementById("compaction");
        const wasteEl = document.getElementById("waste");
        const densityEl = document.getElementById("density");
        const densityUnitLabel = document.getElementById("density-unit-label");
        const costPerTonEl = document.getElementById("cost-per-ton");
        const calcBtn = document.getElementById("calc-btn");
        const resultsCard = document.getElementById("results-card");
        const truckCapacityEl = document.getElementById("truck-capacity");
        const truckCapacityUnit = document.getElementById(
          "truck-capacity-unit"
        );

        function updateUnitLabels() {
          if (unitSystemEl.value === "imperial") {
            lengthUnitLabel.textContent = "feet";
            widthUnitLabel.textContent = "feet";
            densityUnitLabel.textContent = "lb/ft³";
            truckCapacityUnit.textContent = "tons";
            if (
              thicknessUnitEl.value === "cm" ||
              thicknessUnitEl.value === "m"
            ) {
              thicknessUnitEl.value = "in";
            }
          } else {
            lengthUnitLabel.textContent = "meters";
            widthUnitLabel.textContent = "meters";
            densityUnitLabel.textContent = "t/m³";
            truckCapacityUnit.textContent = "tonnes";
            if (
              thicknessUnitEl.value === "in" ||
              thicknessUnitEl.value === "ft"
            ) {
              thicknessUnitEl.value = "cm";
            }
          }
        }

        unitSystemEl.addEventListener("change", () => {
          updateUnitLabels();
          if (unitSystemEl.value === "imperial") {
            if (Math.abs((parseFloat(densityEl.value) || 145) - 2.35) < 0.5) {
              densityEl.value = 145;
            }
          } else {
            if (Math.abs((parseFloat(densityEl.value) || 145) - 145) < 10) {
              densityEl.value = 2.35;
            }
          }
        });

        function toBaseThicknessMeters(value, unit) {
          const v = Number(value);
          if (!isFinite(v) || v <= 0) return NaN;
          switch (unit) {
            case "m":
              return v;
            case "cm":
              return v / 100;
            case "ft":
              return v * 0.3048;
            case "in":
              return v * 0.0254;
            default:
              return NaN;
          }
        }

        function formatNumber(num, decimals = 2) {
          return Number(num).toLocaleString(undefined, {
            minimumFractionDigits: decimals,
            maximumFractionDigits: decimals,
          });
        }

        function calculate() {
          const unitSystem = unitSystemEl.value;
          const length = parseFloat(lengthEl.value);
          const width = parseFloat(widthEl.value);
          const thicknessInput = parseFloat(thicknessEl.value);
          const thicknessUnit = thicknessUnitEl.value;
          const compaction = parseFloat(compactionEl.value);
          const waste = parseFloat(wasteEl.value);
          const densityInput = parseFloat(densityEl.value);
          const costPerTon = parseFloat(costPerTonEl.value);
          const truckCapacity = parseFloat(truckCapacityEl.value);

          if (
            !isFinite(length) ||
            length <= 0 ||
            !isFinite(width) ||
            width <= 0 ||
            !isFinite(thicknessInput) ||
            thicknessInput <= 0
          ) {
            resultsCard.innerHTML =
              '<p class="text-red-600 text-sm">Please enter valid positive values for length, width, and thickness.</p>';
            return;
          }

          const t_m = toBaseThicknessMeters(thicknessInput, thicknessUnit);
          if (!isFinite(t_m) || t_m <= 0) {
            resultsCard.innerHTML =
              '<p class="text-red-600 text-sm">Thickness value or unit is invalid.</p>';
            return;
          }

          let area_m2;
          if (unitSystem === "imperial") {
            const length_m = length * 0.3048;
            const width_m = width * 0.3048;
            area_m2 = length_m * width_m;
          } else {
            area_m2 = length * width;
          }

          const volumeCompacted_m3 = area_m2 * t_m;

          const compactionFactor =
            isFinite(compaction) && compaction > 0 ? compaction / 100 : 0.92;
          const wasteFactor =
            isFinite(waste) && waste >= 0 ? 1 + waste / 100 : 1.05;

          const volumeLoose_m3 = volumeCompacted_m3 / compactionFactor;
          const volumeTotal_m3 = volumeLoose_m3 * wasteFactor;

          let density_kg_m3;
          if (unitSystem === "imperial") {
            const rho_lb_ft3 =
              isFinite(densityInput) && densityInput > 0 ? densityInput : 145;
            density_kg_m3 = rho_lb_ft3 * 16.018463;
          } else {
            const rho_t_m3 =
              isFinite(densityInput) && densityInput > 0 ? densityInput : 2.35;
            density_kg_m3 = rho_t_m3 * 1000;
          }

          const weight_kg = volumeTotal_m3 * density_kg_m3;
          const tonnes = weight_kg / 1000;
          const tonsUS = weight_kg / 907.18474;

          let displayTons, displayUnit;
          if (unitSystem === "imperial") {
            displayTons = tonsUS;
            displayUnit = "US tons";
          } else {
            displayTons = tonnes;
            displayUnit = "tonnes";
          }

          let costHtml = "";
          if (isFinite(costPerTon) && costPerTon > 0) {
            const totalCost = displayTons * costPerTon;
            costHtml = `<p><span class="font-semibold">Estimated material cost:</span> $${formatNumber(
              totalCost,
              2
            )} (at $${formatNumber(costPerTon, 2)} per ${displayUnit.slice(
              0,
              -1
            )})</p>`;
          }

          let trucksHtml = "";
          if (isFinite(truckCapacity) && truckCapacity > 0) {
            const trucks = displayTons / truckCapacity;
            trucksHtml = `<p><span class="font-semibold">Estimated truckloads:</span> ${formatNumber(
              trucks,
              2
            )} × ${formatNumber(truckCapacity, 1)} ${displayUnit} trucks</p>`;
          }

          const area_ft2 = area_m2 / 0.092903;
          const volumeCompacted_ft3 = volumeCompacted_m3 / 0.0283168;
          const volumeTotal_ft3 = volumeTotal_m3 / 0.0283168;
          const volumeCompacted_yd3 = volumeCompacted_ft3 / 27;
          const volumeTotal_yd3 = volumeTotal_ft3 / 27;

          resultsCard.innerHTML = `
        <p><span class="font-semibold">Paved area:</span> ${formatNumber(
          unitSystem === "imperial" ? area_ft2 : area_m2,
          2
        )} ${unitSystem === "imperial" ? "ft²" : "m²"}</p>
        <p><span class="font-semibold">Compacted volume:</span> ${formatNumber(
          unitSystem === "imperial" ? volumeCompacted_yd3 : volumeCompacted_m3,
          2
        )} ${unitSystem === "imperial" ? "yd³" : "m³"}</p>
        <p><span class="font-semibold">Total loose volume (incl. compaction & waste):</span> ${formatNumber(
          unitSystem === "imperial" ? volumeTotal_yd3 : volumeTotal_m3,
          2
        )} ${unitSystem === "imperial" ? "yd³" : "m³"}</p>
        <p><span class="font-semibold">Required asphalt mix:</span> ${formatNumber(
          displayTons,
          2
        )} ${displayUnit}</p>
        ${costHtml}
        ${trucksHtml}
        <p class="text-xs text-gray-500 mt-2">Note: Results are estimates. Always confirm quantities with your asphalt supplier or contractor.</p>
      `;
        }

        calcBtn.addEventListener("click", calculate);

        document.querySelectorAll("button[data-preset]").forEach((btn) => {
          btn.addEventListener("click", () => {
            const preset = btn.getAttribute("data-preset");
            unitSystemEl.value = "imperial";
            updateUnitLabels();
            densityEl.value = 145;
            compactionEl.value = 92;
            wasteEl.value = 5;
            thicknessUnitEl.value = "in";

            switch (preset) {
              case "driveway":
                lengthEl.value = 40;
                widthEl.value = 10;
                thicknessEl.value = 2.5;
                break;
              case "double-driveway":
                lengthEl.value = 40;
                widthEl.value = 20;
                thicknessEl.value = 3;
                break;
              case "parking-space":
                lengthEl.value = 18;
                widthEl.value = 9;
                thicknessEl.value = 3;
                break;
              case "road-lane":
                lengthEl.value = 100;
                widthEl.value = 12;
                thicknessEl.value = 4;
                break;
            }
            calculate();
          });
        });

        updateUnitLabels();
      })();
    </script>

