    <script>
      function parseNum(id) {
        const el = document.getElementById(id);
        if (!el) return 0;
        const v = parseFloat(el.value);
        return isNaN(v) ? 0 : v;
      }

      function fmt(num, currencySymbol) {
        if (!isFinite(num)) return "–";
        const abs = Math.abs(num);
        const options = { maximumFractionDigits: 0 };
        if (abs < 1 && abs !== 0) options.maximumFractionDigits = 2;
        const formatted = num.toLocaleString(undefined, options);
        return (currencySymbol || "") + formatted;
      }

      function buildRow(label, amount, currencySymbol, opts = {}) {
        const { bold = false, section = false, indent = 0 } = opts;
        const classes = [];
        if (section) classes.push("bg-gray-50");
        const labelClasses = ["py-1.5", "pl-" + (2 + indent * 4)];
        const amountClasses = [
          "py-1.5",
          "pr-4",
          "text-right",
          bold ? "font-semibold" : "",
        ];
        return `
      <tr class="${classes.join(" ")}">
        <td class="${labelClasses.join(" ")}">${label}</td>
        <td class="${amountClasses.join(" ")}">${
          amount != null ? fmt(amount, currencySymbol) : ""
        }</td>
      </tr>
    `;
      }

      function calculateCashFlow() {
        const currencySymbol =
          document.getElementById("currency-select").value || "";
        const scale =
          parseFloat(document.getElementById("scale-select").value) || 1;

        // Income statement
        const netIncome = parseNum("is-net-income") * scale;
        const depr = parseNum("is-depr") * scale;
        const interestExp = parseNum("is-interest-expense") * scale;
        const taxExp = parseNum("is-tax-expense") * scale;

        // Balance sheet end/beg
        const cashEnd = parseNum("bs-cash-end") * scale;
        const cashBeg = parseNum("bs-cash-beg") * scale;

        const arEnd = parseNum("bs-ar-end") * scale;
        const arBeg = parseNum("bs-ar-beg") * scale;

        const invEnd = parseNum("bs-inv-end") * scale;
        const invBeg = parseNum("bs-inv-beg") * scale;

        const ocaEnd = parseNum("bs-oca-end") * scale;
        const ocaBeg = parseNum("bs-oca-beg") * scale;

        const ppeEnd = parseNum("bs-ppe-end") * scale;
        const ppeBeg = parseNum("bs-ppe-beg") * scale;

        const intEnd = parseNum("bs-intang-end") * scale;
        const intBeg = parseNum("bs-intang-beg") * scale;

        const apEnd = parseNum("bs-ap-end") * scale;
        const apBeg = parseNum("bs-ap-beg") * scale;

        const oclEnd = parseNum("bs-ocl-end") * scale;
        const oclBeg = parseNum("bs-ocl-beg") * scale;

        const taxPayEnd = parseNum("bs-taxpay-end") * scale;
        const taxPayBeg = parseNum("bs-taxpay-beg") * scale;

        const stdEnd = parseNum("bs-std-end") * scale;
        const stdBeg = parseNum("bs-std-beg") * scale;

        const ltdEnd = parseNum("bs-ltd-end") * scale;
        const ltdBeg = parseNum("bs-ltd-beg") * scale;

        const equityEnd = parseNum("bs-equity-end") * scale;
        const equityBeg = parseNum("bs-equity-beg") * scale;

        const reEnd = parseNum("bs-re-end") * scale;
        const reBeg = parseNum("bs-re-beg") * scale;

        // Direct items
        const capexDirect = parseNum("direct-capex") * scale;
        const dividendsDirect = parseNum("direct-dividends") * scale;
        const otherInvDirect = parseNum("direct-other-investing") * scale;
        const otherFinDirect = parseNum("direct-other-financing") * scale;

        const warningBox = document.getElementById("warning-box");
        warningBox.classList.add("hidden");
        warningBox.textContent = "";

        if (!netIncome && !cashEnd && !cashBeg) {
          warningBox.textContent =
            "Enter at least net income and beginning/ending cash balances to build a cash flow statement.";
          warningBox.classList.remove("hidden");
          document.getElementById("results-table-body").innerHTML = "";
          document.getElementById("fcf-display").textContent = "–";
          document.getElementById("recon-display").textContent = "–";
          document.getElementById("interpretation-display").textContent =
            "Enter data and click “Calculate” to see insights.";
          return;
        }

        // Changes
        const dCash = cashEnd - cashBeg;
        const dAR = arEnd - arBeg;
        const dInv = invEnd - invBeg;
        const dOCA = ocaEnd - ocaBeg;
        const dPPE = ppeEnd - ppeBeg;
        const dInt = intEnd - intBeg;
        const dAP = apEnd - apBeg;
        const dOCL = oclEnd - oclBeg;
        const dTaxPay = taxPayEnd - taxPayBeg;
        const dSTD = stdEnd - stdBeg;
        const dLTD = ltdEnd - ltdBeg;
        const dEquity = equityEnd - equityBeg;
        const dRE = reEnd - reBeg;

        // Operating cash flow (indirect)
        let cfo = netIncome;
        const adjustments = [];

        if (depr) {
          cfo += depr;
          adjustments.push({
            label: "Depreciation & amortization (non‑cash)",
            amount: depr,
          });
        }

        if (dAR) {
          const adj = -dAR;
          cfo += adj;
          adjustments.push({
            label: "Change in accounts receivable",
            amount: adj,
          });
        }
        if (dInv) {
          const adj = -dInv;
          cfo += adj;
          adjustments.push({ label: "Change in inventory", amount: adj });
        }
        if (dOCA) {
          const adj = -dOCA;
          cfo += adj;
          adjustments.push({
            label: "Change in other current assets",
            amount: adj,
          });
        }
        if (dAP) {
          const adj = dAP;
          cfo += adj;
          adjustments.push({
            label: "Change in accounts payable",
            amount: adj,
          });
        }
        if (dOCL) {
          const adj = dOCL;
          cfo += adj;
          adjustments.push({
            label: "Change in other current liabilities",
            amount: adj,
          });
        }
        if (dTaxPay) {
          const adj = dTaxPay;
          cfo += adj;
          adjustments.push({
            label: "Change in income taxes payable",
            amount: adj,
          });
        }

        // Investing cash flow
        let cfi = 0;
        let capex = 0;
        if (capexDirect) {
          capex = capexDirect;
          cfi += capexDirect;
        } else {
          // Approximate capex as negative change in PP&E minus depreciation
          const inferredCapex = -(dPPE + depr);
          if (inferredCapex) {
            capex = inferredCapex;
            cfi += inferredCapex;
          }
        }
        if (dInt) {
          const adj = -dInt;
          cfi += adj;
        }
        if (otherInvDirect) {
          cfi += otherInvDirect;
        }

        // Financing cash flow
        let cff = 0;
        const dDebt = dSTD + dLTD;
        if (dDebt) cff += dDebt;

        if (dividendsDirect) {
          cff += dividendsDirect;
        } else if (dRE && netIncome) {
          // Approximate dividends if not provided: ΔRE ≈ Net income − Dividends
          const approxDiv = dRE - netIncome;
          if (approxDiv) cff += approxDiv;
        }

        if (dEquity) cff += dEquity;
        if (otherFinDirect) cff += otherFinDirect;

        const netChangeCash = cfo + cfi + cff;

        // Build results table
        let html = "";

        // Operating
        html += buildRow(
          "Cash flows from operating activities",
          null,
          currencySymbol,
          { section: true, bold: true }
        );
        html += buildRow("Net income", netIncome, currencySymbol, {
          indent: 1,
        });
        adjustments.forEach((adj) => {
          html += buildRow(adj.label, adj.amount, currencySymbol, {
            indent: 1,
          });
        });
        html += buildRow(
          "Net cash provided by operating activities (CFO)",
          cfo,
          currencySymbol,
          { bold: true }
        );

        // Investing
        html += buildRow(
          "Cash flows from investing activities",
          null,
          currencySymbol,
          { section: true, bold: true }
        );
        if (capex) {
          html += buildRow(
            "Capital expenditures (PP&E)",
            capex,
            currencySymbol,
            { indent: 1 }
          );
        }
        if (dInt) {
          html += buildRow(
            "Change in intangibles & other long‑term assets",
            -dInt,
            currencySymbol,
            { indent: 1 }
          );
        }
        if (otherInvDirect) {
          html += buildRow(
            "Other investing cash flows (net)",
            otherInvDirect,
            currencySymbol,
            { indent: 1 }
          );
        }
        html += buildRow(
          "Net cash used in investing activities (CFI)",
          cfi,
          currencySymbol,
          { bold: true }
        );

        // Financing
        html += buildRow(
          "Cash flows from financing activities",
          null,
          currencySymbol,
          { section: true, bold: true }
        );
        if (dDebt) {
          html += buildRow(
            "Net change in debt (short‑term & long‑term)",
            dDebt,
            currencySymbol,
            { indent: 1 }
          );
        }
        if (dividendsDirect) {
          html += buildRow("Dividends paid", dividendsDirect, currencySymbol, {
            indent: 1,
          });
        } else if (dRE && netIncome) {
          const approxDiv = dRE - netIncome;
          if (approxDiv) {
            html += buildRow(
              "Estimated dividends paid (from retained earnings)",
              approxDiv,
              currencySymbol,
              { indent: 1 }
            );
          }
        }
        if (dEquity) {
          html += buildRow(
            "Net change in equity (stock issued / repurchased)",
            dEquity,
            currencySymbol,
            { indent: 1 }
          );
        }
        if (otherFinDirect) {
          html += buildRow(
            "Other financing cash flows (net)",
            otherFinDirect,
            currencySymbol,
            { indent: 1 }
          );
        }
        html += buildRow(
          "Net cash provided by (used in) financing activities (CFF)",
          cff,
          currencySymbol,
          { bold: true }
        );

        // Summary
        html += buildRow(
          "Net increase (decrease) in cash",
          netChangeCash,
          currencySymbol,
          { section: true, bold: true }
        );
        html += buildRow(
          "Cash & cash equivalents at beginning of period",
          cashBeg,
          currencySymbol
        );
        html += buildRow(
          "Cash & cash equivalents at end of period",
          cashEnd,
          currencySymbol,
          { bold: true }
        );

        document.getElementById("results-table-body").innerHTML = html;

        // FCF
        const fcf = cfo - capex;
        document.getElementById("fcf-display").textContent = fmt(
          fcf,
          currencySymbol
        );

        // Reconciliation
        if (cashBeg || cashEnd) {
          const reconText = `Beginning cash ${fmt(
            cashBeg,
            currencySymbol
          )} + net change in cash ${fmt(netChangeCash, currencySymbol)} = ${fmt(
            cashBeg + netChangeCash,
            currencySymbol
          )} (reported ending cash ${fmt(cashEnd, currencySymbol)}).`;
          document.getElementById("recon-display").textContent = reconText;
          if (Math.abs(cashBeg + netChangeCash - cashEnd) > 1e-6) {
            warningBox.textContent =
              "Warning: Net change in cash does not exactly reconcile with beginning and ending cash. Some accounts may be missing or misclassified.";
            warningBox.classList.remove("hidden");
          }
        } else {
          document.getElementById("recon-display").textContent =
            "Enter beginning and ending cash balances to see a reconciliation.";
        }

        // Interpretation
        let interp = [];
        if (cfo > 0) {
          interp.push(
            "Operating cash flow is positive, which usually indicates that the core business is generating cash."
          );
        } else if (cfo < 0) {
          interp.push(
            "Operating cash flow is negative. Review working capital changes and non‑cash items to understand why."
          );
        }
        if (cfi < 0) {
          interp.push(
            "Investing cash flow is negative, often a sign of investment in PP&E or other long‑term assets."
          );
        } else if (cfi > 0) {
          interp.push(
            "Investing cash flow is positive, which may indicate asset sales or reduced investment."
          );
        }
        if (cff < 0) {
          interp.push(
            "Financing cash flow is negative, suggesting debt repayment, share buybacks, or dividends."
          );
        } else if (cff > 0) {
          interp.push(
            "Financing cash flow is positive, suggesting the company raised capital through debt or equity."
          );
        }
        if (interp.length === 0) {
          interp.push(
            "Review the section totals for operating, investing, and financing activities to interpret your cash flows."
          );
        }
        document.getElementById("interpretation-display").textContent =
          interp.join(" ");

        // Label
        const scaleLabel =
          document.getElementById("scale-select").selectedOptions[0]
            .textContent;
        document.getElementById(
          "results-currency-label"
        ).textContent = `Currency: ${
          currencySymbol || "no symbol"
        } | ${scaleLabel}`;
      }

      function resetForm() {
        document
          .querySelectorAll('input[type="number"]')
          .forEach((el) => (el.value = ""));
        document.getElementById("results-table-body").innerHTML = "";
        document.getElementById("fcf-display").textContent = "–";
        document.getElementById("recon-display").textContent = "–";
        document.getElementById("interpretation-display").textContent =
          "Enter data and click “Calculate” to see insights.";
        const warningBox = document.getElementById("warning-box");
        warningBox.classList.add("hidden");
        warningBox.textContent = "";
      }

      function loadExample() {
        resetForm();
        document.getElementById("currency-select").value = "$";
        document.getElementById("scale-select").value = "1000";

        // Simple example in thousands
        document.getElementById("is-net-income").value = 120;
        document.getElementById("is-depr").value = 30;

        document.getElementById("bs-cash-beg").value = 50;
        document.getElementById("bs-cash-end").value = 80;

        document.getElementById("bs-ar-beg").value = 90;
        document.getElementById("bs-ar-end").value = 110;

        document.getElementById("bs-inv-beg").value = 70;
        document.getElementById("bs-inv-end").value = 60;

        document.getElementById("bs-oca-beg").value = 20;
        document.getElementById("bs-oca-end").value = 25;

        document.getElementById("bs-ppe-beg").value = 300;
        document.getElementById("bs-ppe-end").value = 340;

        document.getElementById("bs-intang-beg").value = 40;
        document.getElementById("bs-intang-end").value = 38;

        document.getElementById("bs-ap-beg").value = 80;
        document.getElementById("bs-ap-end").value = 95;

        document.getElementById("bs-ocl-beg").value = 30;
        document.getElementById("bs-ocl-end").value = 28;

        document.getElementById("bs-taxpay-beg").value = 10;
        document.getElementById("bs-taxpay-end").value = 12;

        document.getElementById("bs-std-beg").value = 40;
        document.getElementById("bs-std-end").value = 35;

        document.getElementById("bs-ltd-beg").value = 200;
        document.getElementById("bs-ltd-end").value = 210;

        document.getElementById("bs-equity-beg").value = 150;
        document.getElementById("bs-equity-end").value = 160;

        document.getElementById("bs-re-beg").value = 140;
        document.getElementById("bs-re-end").value = 230;

        document.getElementById("direct-capex").value = -70;
        document.getElementById("direct-dividends").value = -30;

        calculateCashFlow();
      }

      document
        .getElementById("calculate-btn")
        .addEventListener("click", calculateCashFlow);
      document.getElementById("reset-btn").addEventListener("click", resetForm);
      document
        .getElementById("load-example")
        .addEventListener("click", loadExample);

