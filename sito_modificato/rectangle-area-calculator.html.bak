    <script>
      const tabLW = document.getElementById('tab-lw');
      const tabPerim = document.getElementById('tab-perim');
      const tabDiag = document.getElementById('tab-diag');

      const panelLW = document.getElementById('panel-lw');
      const panelPerim = document.getElementById('panel-perim');
      const panelDiag = document.getElementById('panel-diag');

      function activateTab(btn, panel) {
        [tabLW, tabPerim, tabDiag].forEach(b => b.classList.remove('tab-active'));
        btn.classList.add('tab-active');
        [panelLW, panelPerim, panelDiag].forEach(p => p.classList.add('hidden'));
        panel.classList.remove('hidden');
      }

      tabLW.addEventListener('click', () => activateTab(tabLW, panelLW));
      tabPerim.addEventListener('click', () => activateTab(tabPerim, panelPerim));
      tabDiag.addEventListener('click', () => activateTab(tabDiag, panelDiag));

      const btnCalc = document.getElementById('btn-calc');
      const btnClear = document.getElementById('btn-clear');

      const resBox = document.getElementById('results-box');
      const resArea = document.getElementById('res-area');
      const resPerimeter = document.getElementById('res-perimeter');
      const resDiagonal = document.getElementById('res-diagonal');
      const resRatio = document.getElementById('res-ratio');
      const resLength = document.getElementById('res-length');
      const resWidth = document.getElementById('res-width');
      const resCost = document.getElementById('res-cost');
      const resNote = document.getElementById('res-note');

      function round4(x) {
        return Math.round((x + Number.EPSILON) * 10000) / 10000;
      }
      function unitSq(u) {
        return u + '²';
      }

      btnCalc.addEventListener('click', () => {
        let L, W, unit, cost, area, perim, diag, ratio, note = '';

        if (!panelLW.classList.contains('hidden')) {
          L = parseFloat(document.getElementById('lw-length').value);
          W = parseFloat(document.getElementById('lw-width').value);
          unit = document.getElementById('lw-unit').value;
          cost = parseFloat(document.getElementById('lw-cost').value);
          if (isNaN(L) || isNaN(W) || L <= 0 || W <= 0) {
            alert('Please enter both length and width.');
            return;
          }
        } else if (!panelPerim.classList.contains('hidden')) {
          const P = parseFloat(document.getElementById('p-perim').value);
          const side = parseFloat(document.getElementById('p-side').value);
          unit = document.getElementById('p-unit').value;
          cost = parseFloat(document.getElementById('p-cost').value);
          if (isNaN(P) || isNaN(side) || P <= 0 || side <= 0) {
            alert('Please enter perimeter and one side.');
            return;
          }
          W = (P / 2) - side;
          L = side;
          if (W <= 0) {
            alert('Perimeter too small for the given side.');
            return;
          }
        } else {
          const d = parseFloat(document.getElementById('d-diag').value);
          const side = parseFloat(document.getElementById('d-side').value);
          unit = document.getElementById('d-unit').value;
          cost = parseFloat(document.getElementById('d-cost').value);
          if (isNaN(d) || isNaN(side) || d <= 0 || side <= 0) {
            alert('Please enter diagonal and one side.');
            return;
          }
          if (side >= d) {
            alert('Known side must be shorter than the diagonal.');
            return;
          }
          L = side;
          W = Math.sqrt(d*d - L*L);
        }

        area = L * W;
        perim = 2 * (L + W);
        diag = Math.sqrt(L*L + W*W);
        ratio = L >= W ? (L / W) : (W / L);

        if (Math.abs(L - W) < 1e-6) {
          note = 'This rectangle is actually a square (all sides equal).';
        }

        resBox.classList.remove('hidden');
        resArea.textContent = round4(area) + ' ' + unitSq(unit);
        resPerimeter.textContent = round4(perim) + ' ' + unit;
        resDiagonal.textContent = round4(diag) + ' ' + unit;
        resRatio.textContent = '1 : ' + round4(ratio >= 1 ? ratio : 1/ratio);
        resLength.textContent = round4(L) + ' ' + unit;
        resWidth.textContent = round4(W) + ' ' + unit;

        if (!isNaN(cost) && cost > 0) {
          const total = area * cost;
          resCost.textContent = round4(total);
        } else {
          resCost.textContent = '—';
        }
        resNote.textContent = note;
      });

      btnClear.addEventListener('click', () => {
        document.getElementById('lw-length').value = 10;
        document.getElementById('lw-width').value = 5;
        document.getElementById('lw-cost').value = '';
        document.getElementById('p-perim').value = 32;
        document.getElementById('p-side').value = 10;
        document.getElementById('p-cost').value = '';
        document.getElementById('d-diag').value = 12;
        document.getElementById('d-side').value = 6;
        document.getElementById('d-cost').value = '';
        resBox.classList.add('hidden');
      });
    </script>

