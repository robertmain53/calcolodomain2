    <script>
      (function () {
        const currentSharesEl = document.getElementById("currentShares");
        const currentPriceEl = document.getElementById("currentPrice");
        const targetPriceEl = document.getElementById("targetPrice");
        const newSharesEl = document.getElementById("newShares");
        const newPriceEl = document.getElementById("newPrice");
        const feesPerTradeEl = document.getElementById("feesPerTrade");

        const addLotBtn = document.getElementById("addLotBtn");
        const resetLotsBtn = document.getElementById("resetLotsBtn");
        const lotsContainer = document.getElementById("lotsContainer");
        const lotsTableBody = document.getElementById("lotsTableBody");
        const lotsCountLabel = document.getElementById("lotsCountLabel");

        const calculateBtn = document.getElementById("calculateBtn");
        const errorMsg = document.getElementById("errorMsg");

        const resSection = document.getElementById("resultsSection");
        const resOldShares = document.getElementById("resOldShares");
        const resOldAvg = document.getElementById("resOldAvg");
        const resOldCost = document.getElementById("resOldCost");
        const resOldValueTarget = document.getElementById("resOldValueTarget");
        const resOldPnL = document.getElementById("resOldPnL");

        const resNewShares = document.getElementById("resNewShares");
        const resNewAvg = document.getElementById("resNewAvg");
        const resNewCost = document.getElementById("resNewCost");
        const resNewValueTarget = document.getElementById("resNewValueTarget");
        const resNewPnL = document.getElementById("resNewPnL");
        const resBreakevenChange =
          document.getElementById("resBreakevenChange");

        const targetAverageEl = document.getElementById("targetAverage");
        const helperNewPriceEl = document.getElementById("helperNewPrice");
        const helperCalcBtn = document.getElementById("helperCalcBtn");
        const helperResult = document.getElementById("helperResult");

        let lots = [];

        function formatMoney(value) {
          if (!isFinite(value)) return "–";
          return "$" + value.toFixed(2);
        }

        function formatNumber(value) {
          if (!isFinite(value)) return "–";
          return value.toLocaleString(undefined, { maximumFractionDigits: 4 });
        }

        function updateLotsUI() {
          lotsTableBody.innerHTML = "";
          if (lots.length === 0) {
            lotsContainer.classList.add("hidden");
            lotsCountLabel.textContent = "0 additional lots";
            return;
          }
          lotsContainer.classList.remove("hidden");
          lots.forEach((lot, index) => {
            const tr = document.createElement("tr");
            tr.innerHTML = `
          <td class="px-3 py-2">${index + 1}</td>
          <td class="px-3 py-2">${formatNumber(lot.shares)}</td>
          <td class="px-3 py-2">${formatMoney(lot.price)}</td>
          <td class="px-3 py-2">${formatMoney(lot.shares * lot.price)}</td>
          <td class="px-3 py-2">
            <button type="button" data-index="${index}" class="text-xs text-red-600 hover:underline remove-lot-btn">Remove</button>
          </td>
        `;
            lotsTableBody.appendChild(tr);
          });
          lotsCountLabel.textContent =
            lots.length +
            (lots.length === 1 ? " additional lot" : " additional lots");
        }

        addLotBtn.addEventListener("click", () => {
          errorMsg.classList.add("hidden");
          [newSharesEl, newPriceEl].forEach((el) =>
            el.classList.remove("input-error")
          );

          const shares = parseFloat(newSharesEl.value);
          const price = parseFloat(newPriceEl.value);

          if (!shares || shares <= 0) {
            errorMsg.textContent = "Enter a positive number of new shares.";
            errorMsg.classList.remove("hidden");
            newSharesEl.classList.add("input-error");
            return;
          }
          if (!price || price <= 0) {
            errorMsg.textContent = "Enter a valid new buy price.";
            errorMsg.classList.remove("hidden");
            newPriceEl.classList.add("input-error");
            return;
          }

          lots.push({ shares, price });
          updateLotsUI();

          newSharesEl.value = "";
          newPriceEl.value = "";
        });

        lotsTableBody.addEventListener("click", (e) => {
          if (e.target.classList.contains("remove-lot-btn")) {
            const idx = parseInt(e.target.getAttribute("data-index"), 10);
            if (!isNaN(idx)) {
              lots.splice(idx, 1);
              updateLotsUI();
            }
          }
        });

        resetLotsBtn.addEventListener("click", () => {
          lots = [];
          updateLotsUI();
          resSection.classList.add("hidden");
          errorMsg.classList.add("hidden");
        });

        calculateBtn.addEventListener("click", () => {
          errorMsg.classList.add("hidden");
          [currentSharesEl, currentPriceEl].forEach((el) =>
            el.classList.remove("input-error")
          );

          const Q0 = parseFloat(currentSharesEl.value);
          const P0 = parseFloat(currentPriceEl.value);
          const targetPrice = parseFloat(targetPriceEl.value);
          const fee = parseFloat(feesPerTradeEl.value) || 0;

          if (!Q0 || Q0 <= 0) {
            errorMsg.textContent = "Enter your current number of shares.";
            errorMsg.classList.remove("hidden");
            currentSharesEl.classList.add("input-error");
            return;
          }
          if (!P0 || P0 <= 0) {
            errorMsg.textContent = "Enter your current average price.";
            errorMsg.classList.remove("hidden");
            currentPriceEl.classList.add("input-error");
            return;
          }

          let totalNewShares = 0;
          let totalNewCost = 0;
          lots.forEach((lot) => {
            totalNewShares += lot.shares;
            totalNewCost += lot.shares * lot.price;
          });

          const oldCostNoFee = Q0 * P0;
          const oldFees = fee > 0 ? fee : 0;
          const oldTotalCost = oldCostNoFee + oldFees;

          const newFees = fee > 0 ? fee * lots.length : 0;
          const newTotalCost = oldCostNoFee + totalNewCost + oldFees + newFees;
          const newTotalShares = Q0 + totalNewShares;

          const oldAvg = oldTotalCost / Q0;
          const newAvg =
            newTotalShares > 0 ? newTotalCost / newTotalShares : NaN;

          resOldShares.textContent = formatNumber(Q0);
          resOldAvg.textContent = formatMoney(oldAvg);
          resOldCost.textContent = formatMoney(oldTotalCost);

          resNewShares.textContent = formatNumber(newTotalShares);
          resNewAvg.textContent = formatMoney(newAvg);
          resNewCost.textContent = formatMoney(newTotalCost);

          if (targetPrice && targetPrice > 0) {
            const oldValue = Q0 * targetPrice;
            const oldPnL = oldValue - oldTotalCost;
            const newValue = newTotalShares * targetPrice;
            const newPnL = newValue - newTotalCost;

            resOldValueTarget.textContent = formatMoney(oldValue);
            resOldPnL.textContent =
              (oldPnL >= 0 ? "+" : "") + formatMoney(oldPnL).replace("$", "$");

            resNewValueTarget.textContent = formatMoney(newValue);
            resNewPnL.textContent =
              (newPnL >= 0 ? "+" : "") + formatMoney(newPnL).replace("$", "$");
          } else {
            resOldValueTarget.textContent = "–";
            resOldPnL.textContent = "–";
            resNewValueTarget.textContent = "–";
            resNewPnL.textContent = "–";
          }

          const diff = newAvg - oldAvg;
          if (isFinite(diff) && Math.abs(diff) > 1e-6) {
            const direction = diff < 0 ? "lower" : "higher";
            resBreakevenChange.textContent = `${direction} by ${formatMoney(
              Math.abs(diff)
            )} per share`;
          } else {
            resBreakevenChange.textContent = "no change";
          }

          resSection.classList.remove("hidden");
        });

        helperCalcBtn.addEventListener("click", () => {
          helperResult.textContent = "";
          [
            targetAverageEl,
            helperNewPriceEl,
            currentSharesEl,
            currentPriceEl,
          ].forEach((el) => el.classList.remove("input-error"));

          const Q0 = parseFloat(currentSharesEl.value);
          const P0 = parseFloat(currentPriceEl.value);
          const Ptarget = parseFloat(targetAverageEl.value);
          const Pnew = parseFloat(helperNewPriceEl.value);

          if (!Q0 || Q0 <= 0 || !P0 || P0 <= 0) {
            helperResult.textContent =
              "Enter your current shares and average price first.";
            helperResult.classList.remove("text-emerald-700");
            helperResult.classList.add("text-red-600");
            currentSharesEl.classList.add("input-error");
            currentPriceEl.classList.add("input-error");
            return;
          }
          if (!Ptarget || Ptarget <= 0) {
            helperResult.textContent = "Enter a valid desired average price.";
            helperResult.classList.remove("text-emerald-700");
            helperResult.classList.add("text-red-600");
            targetAverageEl.classList.add("input-error");
            return;
          }
          if (!Pnew || Pnew <= 0) {
            helperResult.textContent = "Enter a valid expected buy price.";
            helperResult.classList.remove("text-emerald-700");
            helperResult.classList.add("text-red-600");
            helperNewPriceEl.classList.add("input-error");
            return;
          }

          if (Math.abs(Pnew - Ptarget) < 1e-9) {
            helperResult.textContent =
              "If your buy price equals your desired average, you would need an infinite number of shares. Choose a different target or buy price.";
            helperResult.classList.remove("text-emerald-700");
            helperResult.classList.add("text-red-600");
            return;
          }

          const numerator = Q0 * (P0 - Ptarget);
          const denominator = Ptarget - Pnew;
          const Q1 = numerator / denominator;

          if (!isFinite(Q1) || Q1 <= 0) {
            helperResult.textContent =
              "With these inputs, you cannot reach that average price by buying at the specified level.";
            helperResult.classList.remove("text-emerald-700");
            helperResult.classList.add("text-red-600");
            return;
          }

          const totalShares = Q0 + Q1;
          const totalCost = Q0 * P0 + Q1 * Pnew;
          const impliedAvg = totalCost / totalShares;

          helperResult.classList.remove("text-red-600");
          helperResult.classList.add("text-emerald-700");
          helperResult.textContent = `You would need to buy approximately ${Q1.toFixed(
            2
          )} shares at $${Pnew.toFixed(
            2
          )} to reach an average price of about $${impliedAvg.toFixed(
            2
          )} (total shares ≈ ${totalShares.toFixed(2)}).`;
        });
      })();
    </script>

