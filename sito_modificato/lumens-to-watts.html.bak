      return parseFloat(val);
    }

    function updateLumensToWatts() {
      if (isUpdating) return;
      isUpdating = true;
      const lm = parseFloat(lumensInput.value || 0);
      const lmPerW = getCurrentLmPerW();
      const watts = lmPerW > 0 ? lm / lmPerW : 0;
      wattsOutput.value = +watts.toFixed(2);
      effLabel.textContent = lmPerW + ' lm/W';
      // also update watts->lumens using same efficacy
      const w = parseFloat(wattsInput.value || 0);
      lumensOutput.value = +(w * lmPerW).toFixed(0);
      isUpdating = false;
    }

    function updateWattsToLumens() {
      if (isUpdating) return;
      isUpdating = true;
      const w = parseFloat(wattsInput.value || 0);
      const lmPerW = getCurrentLmPerW();
      const lm = w * lmPerW;
      lumensOutput.value = +lm.toFixed(0);
      isUpdating = false;
    }

    efficacySelect.addEventListener('change', () => {
      if (efficacySelect.value === 'custom') {
        customEfficacy.classList.remove('hidden');
      } else {
        customEfficacy.classList.add('hidden');
      }
      updateLumensToWatts();
    });
    customEfficacy.addEventListener('input', updateLumensToWatts);
    lumensInput.addEventListener('input', updateLumensToWatts);
    wattsInput.addEventListener('input', updateWattsToLumens);

    // fill quick table for LED 100 lm/W
    document.getElementById('tbl-40-led').textContent = (450 / 100).toFixed(1) + ' W';
    document.getElementById('tbl-60-led').textContent = (800 / 100).toFixed(1) + ' W';
    document.getElementById('tbl-75-led').textContent = (1100 / 100).toFixed(1) + ' W';
    document.getElementById('tbl-100-led').textContent = (1600 / 100).toFixed(1) + ' W';

    // init
    updateLumensToWatts();
  </script>

