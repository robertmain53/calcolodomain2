    <script>
      // Utility: parse "HH:MM" to decimal hours
      function parseTimeToHours(str) {
        if (!str) return 0;
        const parts = str.split(":");
        if (parts.length !== 2) return 0;
        const h = parseInt(parts[0], 10);
        const m = parseInt(parts[1], 10);
        if (isNaN(h) || isNaN(m)) return 0;
        return h + m / 60;
      }

      function formatMoney(value) {
        if (!isFinite(value)) return "$0.00";
        return "$" + value.toFixed(2);
      }

      function formatHours(value) {
        if (!isFinite(value)) return "0.00";
        return value.toFixed(2);
      }

      // Tabs
      const tabSimple = document.getElementById("tab-simple");
      const tabTimesheet = document.getElementById("tab-timesheet");
      const panelSimple = document.getElementById("panel-simple");
      const panelTimesheet = document.getElementById("panel-timesheet");

      tabSimple.addEventListener("click", () => {
        tabSimple.classList.add("border-blue-600", "text-blue-600");
        tabSimple.classList.remove("border-transparent", "text-gray-600");
        tabTimesheet.classList.remove("border-blue-600", "text-blue-600");
        tabTimesheet.classList.add("border-transparent", "text-gray-600");
        panelSimple.classList.remove("hidden");
        panelTimesheet.classList.add("hidden");
      });

      tabTimesheet.addEventListener("click", () => {
        tabTimesheet.classList.add("border-blue-600", "text-blue-600");
        tabTimesheet.classList.remove("border-transparent", "text-gray-600");
        tabSimple.classList.remove("border-blue-600", "text-blue-600");
        tabSimple.classList.add("border-transparent", "text-gray-600");
        panelTimesheet.classList.remove("hidden");
        panelSimple.classList.add("hidden");
      });

      // Pay type toggle
      const payType = document.getElementById("payType");
      const hourlyRateGroup = document.getElementById("hourlyRateGroup");
      const salaryGroup = document.getElementById("salaryGroup");

      payType.addEventListener("change", () => {
        if (payType.value === "hourly") {
          hourlyRateGroup.classList.remove("hidden");
          salaryGroup.classList.add("hidden");
        } else {
          hourlyRateGroup.classList.add("hidden");
          salaryGroup.classList.remove("hidden");
        }
      });

      // Timesheet rows
      const timesheetBody = document.getElementById("timesheetBody");
      const days = ["Mon", "Tue", "Wed", "Thu", "Fri", "Sat", "Sun"];

      function buildTimesheet() {
        timesheetBody.innerHTML = "";
        days.forEach((day, idx) => {
          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="px-3 py-2 text-gray-700">${day}</td>
        <td class="px-3 py-2">
          <input type="time" class="table-input ts-start" value="${
            idx < 5 ? "09:00" : ""
          }">
        </td>
        <td class="px-3 py-2">
          <input type="time" class="table-input ts-end" value="${
            idx < 5 ? "17:00" : ""
          }">
        </td>
        <td class="px-3 py-2">
          <input type="number" min="0" step="5" class="table-input ts-break" value="${
            idx < 5 ? "60" : "0"
          }">
        </td>
        <td class="px-3 py-2 text-right text-gray-700 ts-hours">8.00</td>
        <td class="px-3 py-2 text-right text-gray-700 ts-ot">0.00</td>
      `;
          timesheetBody.appendChild(tr);
        });
      }

      buildTimesheet();

      function recalcTimesheetDaily() {
        const dailyEnabled = document.getElementById("dailyEnabled").checked;
        const dailyThreshold =
          parseFloat(document.getElementById("dailyThreshold").value) || 0;
        const rows = timesheetBody.querySelectorAll("tr");
        rows.forEach((row) => {
          const start = row.querySelector(".ts-start").value;
          const end = row.querySelector(".ts-end").value;
          const brk = parseFloat(row.querySelector(".ts-break").value) || 0;
          let hours = 0;
          if (start && end) {
            const hStart = parseTimeToHours(start);
            const hEnd = parseTimeToHours(end);
            if (hEnd > hStart) {
              hours = hEnd - hStart - brk / 60;
              if (hours < 0) hours = 0;
            }
          }
          const otDaily =
            dailyEnabled && hours > dailyThreshold ? hours - dailyThreshold : 0;
          row.querySelector(".ts-hours").textContent = formatHours(hours);
          row.querySelector(".ts-ot").textContent = formatHours(otDaily);
        });
      }

      timesheetBody.addEventListener("input", (e) => {
        if (
          e.target.classList.contains("ts-start") ||
          e.target.classList.contains("ts-end") ||
          e.target.classList.contains("ts-break")
        ) {
          recalcTimesheetDaily();
        }
      });

      document
        .getElementById("dailyEnabled")
        .addEventListener("change", recalcTimesheetDaily);
      document
        .getElementById("dailyThreshold")
        .addEventListener("input", recalcTimesheetDaily);

      // Calculation
      const calcBtn = document.getElementById("calcBtn");
      const resetBtn = document.getElementById("resetBtn");
      const calcError = document.getElementById("calcError");

      function getHourlyRate() {
        if (payType.value === "hourly") {
          const rate = parseFloat(document.getElementById("hourlyRate").value);
          return isFinite(rate) && rate > 0 ? rate : 0;
        } else {
          const salary = parseFloat(
            document.getElementById("salaryAmount").value
          );
          const hours = parseFloat(
            document.getElementById("salaryHours").value
          );
          if (
            !isFinite(salary) ||
            salary <= 0 ||
            !isFinite(hours) ||
            hours <= 0
          )
            return 0;
          return salary / hours;
        }
      }

      function calculateSimple() {
        const rate = getHourlyRate();
        const regularHours =
          parseFloat(document.getElementById("simpleRegularHours").value) || 0;
        const ot15 =
          parseFloat(document.getElementById("simpleOt15").value) || 0;
        const ot20 =
          parseFloat(document.getElementById("simpleOt20").value) || 0;

        const regularPay = regularHours * rate;
        const ot15Pay = ot15 * rate * 1.5;
        const ot20Pay = ot20 * rate * 2;

        return {
          regularHours,
          ot15Hours: ot15,
          ot20Hours: ot20,
          regularPay,
          ot15Pay,
          ot20Pay,
        };
      }

      function calculateTimesheet() {
        const rate = getHourlyRate();
        const weeklyEnabled = document.getElementById("weeklyEnabled").checked;
        const weeklyThreshold =
          parseFloat(document.getElementById("weeklyThreshold").value) || 0;
        const weeklyMultiplier =
          parseFloat(document.getElementById("weeklyMultiplier").value) || 1.5;
        const dailyEnabled = document.getElementById("dailyEnabled").checked;
        const dailyThreshold =
          parseFloat(document.getElementById("dailyThreshold").value) || 0;
        const dailyMultiplier =
          parseFloat(document.getElementById("dailyMultiplier").value) || 1.5;

        const rows = timesheetBody.querySelectorAll("tr");
        let totalHours = 0;
        let dailyOtHours = 0;
        let baseHours = 0;

        rows.forEach((row) => {
          const h = parseFloat(row.querySelector(".ts-hours").textContent) || 0;
          const ot = dailyEnabled
            ? parseFloat(row.querySelector(".ts-ot").textContent) || 0
            : 0;
          totalHours += h;
          dailyOtHours += ot;
          baseHours += h - ot;
        });

        let weeklyOtHours = 0;
        if (weeklyEnabled && totalHours > weeklyThreshold) {
          const weeklyExcess = totalHours - weeklyThreshold;
          const availableForWeeklyOt = Math.max(baseHours - dailyOtHours, 0);
          weeklyOtHours = Math.min(weeklyExcess, availableForWeeklyOt);
        }

        const regularHours = totalHours - dailyOtHours - weeklyOtHours;
        const otDailyPay = dailyOtHours * rate * dailyMultiplier;
        const otWeeklyPay = weeklyOtHours * rate * weeklyMultiplier;
        const regularPay = regularHours * rate;

        // For display, map daily OT to 1.5x bucket and weekly OT to 1.5x as well (or 2x if multiplier=2)
        let ot15Hours = 0,
          ot20Hours = 0,
          ot15Pay = 0,
          ot20Pay = 0;

        function bucket(hours, multiplier, pay) {
          if (multiplier >= 1.75) {
            ot20Hours += hours;
            ot20Pay += pay;
          } else {
            ot15Hours += hours;
            ot15Pay += pay;
          }
        }

        bucket(dailyOtHours, dailyMultiplier, otDailyPay);
        bucket(weeklyOtHours, weeklyMultiplier, otWeeklyPay);

        return {
          regularHours,
          ot15Hours,
          ot20Hours,
          regularPay,
          ot15Pay,
          ot20Pay,
        };
      }

      function updateResults(res) {
        const totalHours = res.regularHours + res.ot15Hours + res.ot20Hours;
        const totalOtPay = res.ot15Pay + res.ot20Pay;
        const totalPay = res.regularPay + totalOtPay;

        document.getElementById("resRegularHours").textContent = formatHours(
          res.regularHours
        );
        document.getElementById("resOt15Hours").textContent = formatHours(
          res.ot15Hours
        );
        document.getElementById("resOt20Hours").textContent = formatHours(
          res.ot20Hours
        );
        document.getElementById("resTotalHours").textContent =
          formatHours(totalHours);

        document.getElementById("resRegularPay").textContent = formatMoney(
          res.regularPay
        );
        document.getElementById("resOt15Pay").textContent = formatMoney(
          res.ot15Pay
        );
        document.getElementById("resOt20Pay").textContent = formatMoney(
          res.ot20Pay
        );
        document.getElementById("resTotalOtPay").textContent =
          formatMoney(totalOtPay);
        document.getElementById("resTotalPay").textContent =
          formatMoney(totalPay);
      }

      calcBtn.addEventListener("click", () => {
        calcError.textContent = "";
        const rate = getHourlyRate();
        if (!rate || rate <= 0) {
          calcError.textContent = "Please enter a valid hourly rate or salary.";
          return;
        }

        let res;
        if (!panelSimple.classList.contains("hidden")) {
          res = calculateSimple();
        } else {
          recalcTimesheetDaily();
          res = calculateTimesheet();
        }
        updateResults(res);
      });

      resetBtn.addEventListener("click", () => {
        document.getElementById("hourlyRate").value = 20;
        document.getElementById("salaryAmount").value = 800;
        document.getElementById("salaryHours").value = 40;
        document.getElementById("weeklyEnabled").checked = true;
        document.getElementById("weeklyThreshold").value = 40;
        document.getElementById("weeklyMultiplier").value = "1.5";
        document.getElementById("dailyEnabled").checked = false;
        document.getElementById("dailyThreshold").value = 8;
        document.getElementById("dailyMultiplier").value = "1.5";
        document.getElementById("simpleRegularHours").value = 40;
        document.getElementById("simpleOt15").value = 5;
        document.getElementById("simpleOt20").value = 0;
        buildTimesheet();
        recalcTimesheetDaily();
        calcError.textContent = "";
        updateResults({
          regularHours: 40,
          ot15Hours: 5,
          ot20Hours: 0,
          regularPay: 800,
          ot15Pay: 150,
          ot20Pay: 0,
        });
