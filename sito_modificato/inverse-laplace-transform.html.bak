    <script>
      // Simple parser helpers for common patterns
      function parseExpression(expr) {
        // Normalize
        return expr.replace(/\s+/g, "");
      }

      function isNumber(str) {
        return /^-?\d+(\.\d+)?$/.test(str);
      }

      function matchSimplePairs(Fs) {
        // 1/s^n -> t^{n-1}/(n-1)!
        let mPow = /^([0-9.+-]+)?\/s\^([0-9]+)$/.exec(Fs);
        if (mPow) {
          const coeff = mPow[1] ? parseFloat(mPow[1]) : 1;
          const n = parseInt(mPow[2], 10);
          if (n >= 1 && Number.isFinite(coeff)) {
            const power = n - 1;
            // f(t) = coeff * t^{n-1} / (n-1)!
            let fact = 1;
            for (let k = 1; k <= power; k++) fact *= k;
            const c = coeff / fact;
            const tPart = power === 0 ? "1" : power === 1 ? "t" : `t^${power}`;
            return {
              f: `${c !== 1 ? c.toString() + " * " : ""}${tPart}`,
              steps: [
                `Recognize the form F(s) = a / s^n.`,
                `Here a = ${coeff}, n = ${n}.`,
                `We use L{t^{n-1}} = (n-1)! / s^n.`,
                `So f(t) = a * t^{n-1} / (n-1)! = ${c} * ${tPart}.`,
              ],
            };
          }
        }

        // 1/(s-a) -> e^{at}
        let mExp = /^([0-9.+-]+)?\/\(s([+-][0-9.]+)\)$/.exec(Fs);
        if (mExp) {
          const coeff = mExp[1] ? parseFloat(mExp[1]) : 1;
          const a = -parseFloat(mExp[2]); // because s-a => (s - a) = (s + (-a))
          const sign = a >= 0 ? "+" : "";
          return {
            f: `${
              coeff !== 1 ? coeff.toString() + " * " : ""
            }e^{${sign}${a} t}`,
            steps: [
              `Recognize the form F(s) = a / (s - c).`,
              `Here a = ${coeff}, c = ${a}.`,
              `We use L{e^{ct}} = 1 / (s - c).`,
              `So f(t) = a * e^{ct} = ${coeff} * e^{${sign}${a} t}.`,
            ],
          };
        }

        // 1/(s^2 + b^2) or b/(s^2 + b^2) -> sin(bt)
        let mSin = /^([0-9.+-]+)?\/\(s\^2\+([0-9.]+)\)$/.exec(Fs);
        if (mSin) {
          const coeff = mSin[1] ? parseFloat(mSin[1]) : 1;
          const b2 = parseFloat(mSin[2]);
          const b = Math.sqrt(b2);
          const baseCoeff = coeff / b;
          return {
            f: `${
              baseCoeff !== 1 ? baseCoeff.toFixed(6) + " * " : ""
            }sin(${b.toFixed(6)} t)`,
            steps: [
              `Rewrite denominator as s^2 + b^2 with b^2 = ${b2}.`,
              `So b = sqrt(${b2}) = ${b.toFixed(6)}.`,
              `We know L{sin(bt)} = b / (s^2 + b^2).`,
              `Given F(s) = ${coeff}/(s^2 + b^2) = ( ${coeff}/${b.toFixed(
                6
              )} ) * [ ${b.toFixed(6)} / (s^2 + b^2) ].`,
              `Thus f(t) = (${coeff}/${b.toFixed(6)}) * sin(${b.toFixed(
                6
              )} t).`,
            ],
          };
        }

        // s/(s^2 + b^2) -> cos(bt)
        let mCos = /^([0-9.+-]+)?s\/\(s\^2\+([0-9.]+)\)$/.exec(Fs);
        if (mCos) {
          const coeff = mCos[1] ? parseFloat(mCos[1]) : 1;
          const b2 = parseFloat(mCos[2]);
          const b = Math.sqrt(b2);
          return {
            f: `${coeff !== 1 ? coeff.toString() + " * " : ""}cos(${b.toFixed(
              6
            )} t)`,
            steps: [
              `Recognize the form F(s) = a * s / (s^2 + b^2).`,
              `Here a = ${coeff}, b^2 = ${b2} so b = ${b.toFixed(6)}.`,
              `We use L{cos(bt)} = s / (s^2 + b^2).`,
              `So f(t) = a * cos(bt) = ${coeff} * cos(${b.toFixed(6)} t).`,
            ],
          };
        }

        return null;
      }

      function partialFractionSimple(Fs) {
        // Very limited: (as + b)/((s + p)(s + q)) with numeric a,b,p,q
        const re =
          /^\(([-0-9.+]+)s([+-][0-9.]+)\)\/\(\(s([+-][0-9.]+)\)\(s([+-][0-9.]+)\)\)$/;
        const m = re.exec(Fs);
        if (!m) return null;
        const a = parseFloat(m[1]);
        const b = parseFloat(m[2]);
        const p = parseFloat(m[3]);
        const q = parseFloat(m[4]);

        // (as + b)/((s+p)(s+q)) = A/(s+p) + B/(s+q)
        // as + b = A(s+q) + B(s+p) = (A+B)s + (Aq + Bp)
        // => A + B = a, Aq + Bp = b
        const A = (b - a * p) / (q - p);
        const B = a - A;

        const term1 = `${A}/(s${p >= 0 ? "+" + p : p})`;
        const term2 = `${B}/(s${q >= 0 ? "+" + q : q})`;

        const steps = [
          `We have F(s) = ( ${a} s ${
            b >= 0 ? "+ " + b : "- " + Math.abs(b)
          } ) / [ (s ${p >= 0 ? "+ " + p : "- " + Math.abs(p)}) (s ${
            q >= 0 ? "+ " + q : "- " + Math.abs(q)
          }) ].`,
          `Assume a decomposition: F(s) = A/(s + ${p}) + B/(s + ${q}).`,
          `Then (as + b) = A(s + ${q}) + B(s + ${p}) = (A + B)s + (A·${q} + B·${p}).`,
          `Match coefficients: A + B = ${a}, A·${q} + B·${p} = ${b}.`,
          `Solving gives A = ${A}, B = ${B}.`,
          `So F(s) = ${A}/(s + ${p}) + ${B}/(s + ${q}).`,
        ];

        const f1 = `${A} * e^{-${p} t}`;
        const f2 = `${B} * e^{-${q} t}`;
        const f = `${A} e^{-${p} t} + ${B} e^{-${q} t}`;

        steps.push(
          `Use L^{-1}{1/(s + a)} = e^{-a t}.`,
          `Thus L^{-1}{${A}/(s + ${p})} = ${f1}, and L^{-1}{${B}/(s + ${q})} = ${f2}.`,
          `So f(t) = ${f}.`
        );

        return { f, steps, decomposition: `${term1} + ${term2}` };
      }

      function computeInverseLaplace() {
        const inputEl = document.getElementById("input-expression");
        const statusEl = document.getElementById("calc-status");
        const resultContainer = document.getElementById("result-container");
        const resultOutput = document.getElementById("result-output");
        const resultNote = document.getElementById("result-note");
        const stepsOutput = document.getElementById("steps-output");
        const format = document.getElementById("output-format").value;

        let expr = inputEl.value.trim();
        if (!expr) {
          statusEl.textContent = "Please enter an expression for F(s).";
          resultContainer.classList.add("hidden");
          return;
        }

        statusEl.textContent = "Parsing and matching patterns...";
        resultContainer.classList.add("hidden");
        stepsOutput.innerHTML = "";
        resultOutput.textContent = "";
        resultNote.textContent = "";

        const Fs = parseExpression(expr);

        // Try simple direct matches
        let match = matchSimplePairs(Fs);
        let steps = [];

        if (match) {
          resultOutput.textContent = format === "plain" ? match.f : match.f;
          steps = match.steps;
          resultNote.textContent = "Matched a standard Laplace transform pair.";
        } else {
          // Try simple partial fraction pattern
          const pf = partialFractionSimple(Fs);
          if (pf) {
            resultOutput.textContent = format === "plain" ? pf.f : pf.f;
            steps = pf.steps;
            resultNote.textContent =
              "Used partial fraction decomposition and standard pairs.";
          } else {
            resultOutput.textContent =
              "No closed-form match found with the built-in pattern set.";
            resultNote.textContent =
              "Try simplifying the expression, or use a CAS tool for more complex inverses.";
            steps = [
              "The expression did not match any of the implemented standard forms.",
              "Currently supported patterns include:",
              "- a / s^n (n integer ≥ 1)",
              "- a / (s - c)",
              "- a / (s^2 + b^2)",
              "- a s / (s^2 + b^2)",
              "- (a s + b) / ((s + p)(s + q)) with numeric a,b,p,q.",
            ];
          }
        }

        // Render steps
        stepsOutput.innerHTML = "";
        steps.forEach((line) => {
          const p = document.createElement("p");
          p.textContent = line;
          stepsOutput.appendChild(p);
        });

        resultContainer.classList.remove("hidden");
        statusEl.textContent = "Done.";
      }

      document.addEventListener("DOMContentLoaded", () => {
        const computeBtn = document.getElementById("compute-btn");
        const clearBtn = document.getElementById("clear-btn");
        const inputEl = document.getElementById("input-expression");
        const statusEl = document.getElementById("calc-status");

        computeBtn.addEventListener("click", computeInverseLaplace);

        clearBtn.addEventListener("click", () => {
          inputEl.value = "";
          document.getElementById("result-container").classList.add("hidden");
          statusEl.textContent = "";
        });

        document.querySelectorAll(".example-btn").forEach((btn) => {
          btn.addEventListener("click", () => {
            inputEl.value = btn.getAttribute("data-example");
            statusEl.textContent =
              'Example loaded. Click "Calculate f(t)" to see the result.';
          });
        });

        inputEl.addEventListener("keydown", (e) => {
          if ((e.ctrlKey || e.metaKey) && e.key === "Enter") {
            computeInverseLaplace();
          }
        });
      });

      // Mobile menu script (simple)
      document.addEventListener("DOMContentLoaded", () => {
        const toggle = document.getElementById("mobile-menu-toggle");
        const menu = document.getElementById("mobile-menu");
        if (toggle && menu) {
          toggle.addEventListener("click", () => {
            const isHidden = menu.classList.contains("hidden");
            menu.classList.toggle("hidden", !isHidden);
            toggle.setAttribute("aria-expanded", isHidden ? "true" : "false");
          });
        }
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
