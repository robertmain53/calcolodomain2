    <script>
      // Child Support Calculator Logic
      (function () {
        const $ = (id) => document.getElementById(id);

        const payerIncomeEl = $("payer-income");
        const receiverIncomeEl = $("receiver-income");
        const payerDedEl = $("payer-deductions");
        const receiverDedEl = $("receiver-deductions");
        const numChildrenEl = $("num-children");
        const overnightsEl = $("payer-overnights");
        const healthEl = $("health-insurance");
        const childcareEl = $("childcare");
        const extraEl = $("extra-expenses");
        const otherSupportEl = $("other-support");

        const calcBtn = $("calculate-btn");
        const resetBtn = $("reset-btn");
        const errorEl = $("calc-error");
        const resultsSection = $("results-section");
        const resultAmountEl = $("result-amount");
        const resultSummaryEl = $("result-summary");
        const resultBreakdownEl = $("result-breakdown");

        function parseVal(el) {
          const v = parseFloat(el.value.replace(",", "."));
          return isNaN(v) ? 0 : Math.max(0, v);
        }

        function getGuidelinePercent(children) {
          switch (children) {
            case 1:
              return 0.17;
            case 2:
              return 0.25;
            case 3:
              return 0.29;
            case 4:
              return 0.31;
            case 5:
              return 0.33;
            default:
              return 0.35;
          }
        }

        function getParentingAdj(overnights) {
          if (overnights >= 183) return 0.3;
          if (overnights >= 130) return 0.2;
          if (overnights >= 80) return 0.1;
          return 0.0;
        }

        function formatMoney(v) {
          return "$" + v.toFixed(2).replace(/\B(?=(\d{3})+(?!\d))/g, ",");
        }

        function calculate() {
          const payerGross = parseVal(payerIncomeEl);
          const receiverGross = parseVal(receiverIncomeEl);
          const payerDed = parseVal(payerDedEl);
          const receiverDed = parseVal(receiverDedEl);
          const children = parseInt(numChildrenEl.value, 10) || 0;
          const overnights = parseInt(overnightsEl.value, 10) || 0;
          const health = parseVal(healthEl);
          const childcare = parseVal(childcareEl);
          const extra = parseVal(extraEl);
          const otherSupport = parseVal(otherSupportEl);

          if (payerGross <= 0 || children <= 0) {
            errorEl.classList.remove("hidden");
            resultsSection.classList.add("hidden");
            return;
          }
          errorEl.classList.add("hidden");

          const effTaxRate = 0.25;

          const payerAdjGross = Math.max(0, payerGross - payerDed);
          const receiverAdjGross = Math.max(0, receiverGross - receiverDed);

          const payerNet = payerAdjGross * (1 - effTaxRate);
          const receiverNet = receiverAdjGross * (1 - effTaxRate);
          const combinedNet = payerNet + receiverNet;

          if (combinedNet <= 0) {
            errorEl.textContent =
              "Combined net income is zero. Please adjust the income values.";
            errorEl.classList.remove("hidden");
            resultsSection.classList.add("hidden");
            return;
          } else {
            errorEl.textContent =
              "Please enter at least the paying parent’s income and number of children.";
          }

          const guidelinePct = getGuidelinePercent(children);
          const baseObligation = combinedNet * guidelinePct;

          const payerShareRatio = payerNet / combinedNet;
          const payerShare = baseObligation * payerShareRatio;

          const parentingAdj = getParentingAdj(overnights);
          const payerAfterParenting = payerShare * (1 - parentingAdj);

          const addOns = health + childcare + extra;
          const payerAddOnShare = addOns * payerShareRatio;

          let finalSupport =
            payerAfterParenting + payerAddOnShare - otherSupport;
          finalSupport = Math.max(0, finalSupport);

          // Populate UI
          resultAmountEl.textContent = formatMoney(finalSupport);

          const payerPct = (payerShareRatio * 100).toFixed(1);
          const receiverPct = (100 - payerShareRatio * 100).toFixed(1);
          const parentingPct = Math.min(
            100,
            Math.max(0, (overnights / 365) * 100)
          ).toFixed(1);

          resultSummaryEl.innerHTML = `
        <li>Payer net income (approx.): <strong>${formatMoney(
          payerNet
        )}</strong> / month</li>
        <li>Receiver net income (approx.): <strong>${formatMoney(
          receiverNet
        )}</strong> / month</li>
        <li>Combined net income: <strong>${formatMoney(
          combinedNet
        )}</strong> / month</li>
        <li>Income share: payer ${payerPct}% · receiver ${receiverPct}%</li>
        <li>Parenting time: payer ${overnights} overnights/year (~${parentingPct}% of year)</li>
        <li>Guideline percentage for ${children} child(ren): <strong>${(
            guidelinePct * 100
          ).toFixed(1)}%</strong></li>
      `;

          resultBreakdownEl.innerHTML = `
        <p><strong>1. Net income (approximate)</strong></p>
        <ul class="list-disc ml-5">
          <li>Payer adjusted gross: ${formatMoney(
            payerAdjGross
          )} → net ≈ ${formatMoney(payerNet)}</li>
          <li>Receiver adjusted gross: ${formatMoney(
            receiverAdjGross
          )} → net ≈ ${formatMoney(receiverNet)}</li>
          <li>Combined net income: ${formatMoney(combinedNet)}</li>
        </ul>

        <p class="mt-2"><strong>2. Combined base child support obligation</strong></p>
        <ul class="list-disc ml-5">
          <li>Guideline rate for ${children} child(ren): ${(
            guidelinePct * 100
          ).toFixed(1)}%</li>
          <li>Base obligation: ${formatMoney(baseObligation)}</li>
        </ul>

        <p class="mt-2"><strong>3. Allocation between parents (income shares)</strong></p>
        <ul class="list-disc ml-5">
          <li>Payer share ratio: ${payerPct}% → ${formatMoney(payerShare)}</li>
          <li>Receiver share ratio: ${receiverPct}% → ${formatMoney(
            baseObligation - payerShare
          )}</li>
        </ul>

        <p class="mt-2"><strong>4. Parenting time adjustment</strong></p>
        <ul class="list-disc ml-5">
          <li>Payer overnights: ${overnights} (~${parentingPct}% of year)</li>
          <li>Parenting time adjustment: ${(parentingAdj * 100).toFixed(
            0
          )}% reduction</li>
          <li>Payer after parenting adjustment: ${formatMoney(
            payerAfterParenting
          )}</li>
        </ul>

        <p class="mt-2"><strong>5. Add-ons and credits</strong></p>
        <ul class="list-disc ml-5">
          <li>Total add-ons (health + childcare + extraordinary): ${formatMoney(
            addOns
          )}</li>
          <li>Payer share of add-ons: ${formatMoney(payerAddOnShare)}</li>
          <li>Other support credit (payer): −${formatMoney(otherSupport)}</li>
        </ul>

        <p class="mt-2"><strong>6. Final estimated support</strong></p>
        <ul class="list-disc ml-5">
          <li>Final estimated monthly child support: <strong>${formatMoney(
            finalSupport
          )}</strong></li>
        </ul>
      `;

          resultsSection.classList.remove("hidden");
          resultsSection.scrollIntoView({ behavior: "smooth", block: "start" });
        }

        calcBtn.addEventListener("click", calculate);

        resetBtn.addEventListener("click", () => {
          document.getElementById("child-support-form").reset();
          errorEl.classList.add("hidden");
          resultsSection.classList.add("hidden");
        });
      })();
    </script>

