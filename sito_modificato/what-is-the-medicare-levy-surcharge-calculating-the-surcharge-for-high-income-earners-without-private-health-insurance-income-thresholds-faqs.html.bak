    <script>
      // Simple MLS configuration – example tiers (you can update values per income year)
      const MLS_CONFIG = {
        // Example thresholds; should be updated when ATO changes them
        single: [
          { tier: "Tier 0 (no MLS)", min: 0, max: 90000, rate: 0.0 },
          { tier: "Tier 1", min: 90001, max: 105000, rate: 0.01 },
          { tier: "Tier 2", min: 105001, max: 140000, rate: 0.0125 },
          { tier: "Tier 3", min: 140001, max: Infinity, rate: 0.015 },
        ],
        familyBase: 180000, // base family threshold
        perChildIncrease: 1500, // example increment per child after first
        familyTiers: [
          { tier: "Tier 0 (no MLS)", rate: 0.0 },
          { tier: "Tier 1", rate: 0.01 },
          { tier: "Tier 2", rate: 0.0125 },
          { tier: "Tier 3", rate: 0.015 },
        ],
      };

      function getFamilyThreshold(numChildren) {
        const extraChildren = Math.max(0, numChildren - 1);
        return (
          MLS_CONFIG.familyBase + extraChildren * MLS_CONFIG.perChildIncrease
        );
      }

      function determineTier(status, income, numChildren) {
        if (status === "single") {
          for (const row of MLS_CONFIG.single) {
            if (income >= row.min && income <= row.max) {
              return { tier: row.tier, rate: row.rate };
            }
          }
          return { tier: "Tier 0 (no MLS)", rate: 0 };
        } else {
          const base = getFamilyThreshold(numChildren);
          if (income <= base) return { tier: "Tier 0 (no MLS)", rate: 0 };
          const tier1Upper = base + 15000;
          const tier2Upper = base + 50000;
          if (income <= tier1Upper) return { tier: "Tier 1", rate: 0.01 };
          if (income <= tier2Upper) return { tier: "Tier 2", rate: 0.0125 };
          return { tier: "Tier 3", rate: 0.015 };
        }
      }

      function formatCurrency(value) {
        return new Intl.NumberFormat("en-AU", {
          style: "currency",
          currency: "AUD",
          maximumFractionDigits: 0,
        }).format(value);
      }

      function populateThresholdTable() {
        const tbody = document.getElementById("mls-thresholds-body");
        if (!tbody) return;
        tbody.innerHTML = "";

        const familyBase = MLS_CONFIG.familyBase;
        const childInc = MLS_CONFIG.perChildIncrease;

        MLS_CONFIG.single.forEach((row, index) => {
          const tr = document.createElement("tr");
          tr.className = index % 2 === 0 ? "bg-white" : "bg-gray-50";

          const singleRange =
            row.max === Infinity
              ? formatCurrency(row.min) + " and above"
              : `${formatCurrency(row.min)} – ${formatCurrency(row.max)}`;

          let familyRange;
          if (row.rate === 0) {
            familyRange = `Up to ${formatCurrency(
              familyBase
            )} (base, +${formatCurrency(childInc)} per child after first)`;
          } else if (row.rate === 0.01) {
            familyRange = `${formatCurrency(familyBase + 1)} – ${formatCurrency(
              familyBase + 15000
            )} (approx., +${formatCurrency(childInc)} per child)`;
          } else if (row.rate === 0.0125) {
            familyRange = `${formatCurrency(
              familyBase + 15001
            )} – ${formatCurrency(
              familyBase + 50000
            )} (approx., +${formatCurrency(childInc)} per child)`;
          } else {
            familyRange =
              formatCurrency(familyBase + 50001) +
              " and above (approx., + per child)";
          }

          tr.innerHTML = `
        <td class="px-3 py-2 border-b border-gray-200">${row.tier}</td>
        <td class="px-3 py-2 border-b border-gray-200">${singleRange}</td>
        <td class="px-3 py-2 border-b border-gray-200">${familyRange}</td>
        <td class="px-3 py-2 border-b border-gray-200">${(
          row.rate * 100
        ).toFixed(2)}%</td>
      `;
          tbody.appendChild(tr);
        });
      }

      document.addEventListener("DOMContentLoaded", () => {
        populateThresholdTable();

        const familyRadios = document.querySelectorAll(
          'input[name="mls-family-status"]'
        );
        const dependantsWrapper = document.getElementById("dependants-wrapper");
        const coverRadios = document.querySelectorAll(
          'input[name="mls-cover"]'
        );
        const partialCoverWrapper = document.getElementById(
          "partial-cover-wrapper"
        );

        familyRadios.forEach((r) => {
          r.addEventListener("change", () => {
            const val = document.querySelector(
              'input[name="mls-family-status"]:checked'
            ).value;
            dependantsWrapper.classList.toggle("hidden", val !== "family");
          });
        });

        coverRadios.forEach((r) => {
          r.addEventListener("change", () => {
            const val = document.querySelector(
              'input[name="mls-cover"]:checked'
            ).value;
            partialCoverWrapper.classList.toggle("hidden", val !== "partial");
          });
        });

        document
          .getElementById("mls-calc-btn")
          .addEventListener("click", () => {
            const income = parseFloat(
              document.getElementById("mls-income").value || "0"
            );
            const status = document.querySelector(
              'input[name="mls-family-status"]:checked'
            ).value;
            const numChildren =
              status === "family"
                ? parseInt(
                    document.getElementById("mls-dependants").value || "0",
                    10
                  )
                : 0;
            const coverStatus = document.querySelector(
              'input[name="mls-cover"]:checked'
            ).value;
            let daysCovered = 365;

            if (coverStatus === "none") {
              daysCovered = 0;
            } else if (coverStatus === "partial") {
              daysCovered = Math.min(
                365,
                Math.max(
                  0,
                  parseInt(
                    document.getElementById("mls-days-covered").value || "0",
                    10
                  )
                )
              );
            }

            const daysNoCover = 365 - daysCovered;
            const prorata = daysNoCover / 365;

            const { tier, rate } = determineTier(status, income, numChildren);

            const mlsAmount = income * rate * prorata;

            document.getElementById("mls-tier").textContent = tier;
            document.getElementById("mls-rate").textContent =
              (rate * 100).toFixed(2) + "%";
            document.getElementById("mls-days-no-cover").textContent =
              daysNoCover;
            document.getElementById("mls-prorata").textContent =
              prorata.toFixed(3);
            document.getElementById("mls-amount").textContent =
              formatCurrency(mlsAmount);

            const summary = document.getElementById("mls-summary");
            if (rate === 0 || prorata === 0) {
              summary.textContent =
                "Based on the details entered, no Medicare Levy Surcharge is estimated for this year.";
            } else {
              summary.textContent = `Based on an MLS income of ${formatCurrency(
                income
              )}, your estimated Medicare Levy Surcharge is ${formatCurrency(
                mlsAmount
              )} at a rate of ${(rate * 100).toFixed(
                2
              )}%, pro‑rated for days without cover.`;
            }
          });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
