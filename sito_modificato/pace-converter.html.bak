<script>
  // Utility functions
  function toSeconds(min, sec) {
    if (isNaN(min) && isNaN(sec)) return null;
    min = isNaN(min) ? 0 : min;
    sec = isNaN(sec) ? 0 : sec;
    return min * 60 + sec;
  }

  function secondsToMinString(totalSeconds) {
    if (!isFinite(totalSeconds) || totalSeconds <= 0) return '–';
    const minutes = Math.floor(totalSeconds / 60);
    const seconds = Math.round(totalSeconds % 60);
    return minutes + ':' + String(seconds).padStart(2, '0');
  }

  function formatNumber(value, decimals) {
    if (!isFinite(value)) return '–';
    return value.toFixed(decimals);
  }

  // Tab handling
  document.querySelectorAll('.tab-btn').forEach(btn => {
    btn.addEventListener('click', () => {
      document.querySelectorAll('.tab-btn').forEach(b => {
        b.classList.remove('tab-active');
        b.setAttribute('aria-selected', 'false');
      });
      document.querySelectorAll('.converter-panel').forEach(p => p.classList.add('hidden'));
      btn.classList.add('tab-active');
      btn.setAttribute('aria-selected', 'true');
      const target = document.getElementById(btn.dataset.target);
      if (target) target.classList.remove('hidden');
    });
  });

  // Panel 1: Known pace
  const paceMinutesInput = document.getElementById('pace-minutes');
  const paceSecondsInput = document.getElementById('pace-seconds');
  const paceUnitSelect = document.getElementById('pace-unit');

  function updateFromPace() {
    const min = parseFloat(paceMinutesInput.value);
    const sec = parseFloat(paceSecondsInput.value);
    const totalSec = toSeconds(min, sec);
    const unit = paceUnitSelect.value;

    const outKm = document.getElementById('out-pace-min-per-km');
    const outMile = document.getElementById('out-pace-min-per-mile');
    const outKmh = document.getElementById('out-speed-kmh');
    const outMph = document.getElementById('out-speed-mph');

    if (totalSec === null || totalSec <= 0) {
      outKm.textContent = outMile.textContent = outKmh.textContent = outMph.textContent = '–';
      updateRaceTimes(null);
      return;
    }

    let paceSecPerKm, paceSecPerMile;

    if (unit === 'min_per_km') {
      paceSecPerKm = totalSec;
      paceSecPerMile = totalSec * 1.60934;
    } else {
      paceSecPerMile = totalSec;
      paceSecPerKm = totalSec / 1.60934;
    }

    outKm.textContent = secondsToMinString(paceSecPerKm);
    outMile.textContent = secondsToMinString(paceSecPerMile);

    const paceMinPerKm = paceSecPerKm / 60;
    const paceMinPerMile = paceSecPerMile / 60;

    const speedKmh = 60 / paceMinPerKm;
    const speedMph = 60 / paceMinPerMile;

    outKmh.textContent = formatNumber(speedKmh, 2);
    outMph.textContent = formatNumber(speedMph, 2);

    updateRaceTimes(paceSecPerKm);
  }

  [paceMinutesInput, paceSecondsInput, paceUnitSelect].forEach(el => {
    el.addEventListener('input', updateFromPace);
    el.addEventListener('change', updateFromPace);
  });

  // Race time estimates
  const raceDistances = [
    { label: '1 km', km: 1, miles: 0.621371 },
    { label: '5 km', km: 5, miles: 3.10686 },
    { label: '10 km', km: 10, miles: 6.21371 },
    { label: 'Half marathon', km: 21.0975, miles: 13.1094 },
    { label: 'Marathon', km: 42.195, miles: 26.2188 }
  ];

  function updateRaceTimes(paceSecPerKm) {
    const tbody = document.getElementById('race-time-body');
    tbody.innerHTML = '';
    raceDistances.forEach(d => {
      const tr = document.createElement('tr');
      tr.className = 'odd:bg-white even:bg-gray-50';
      const tds = [];
      tds.push(`<td class="px-3 py-2 border-b">${d.label}</td>`);
      tds.push(`<td class="px-3 py-2 border-b">${d.km.toFixed(2)}</td>`);
      tds.push(`<td class="px-3 py-2 border-b">${d.miles.toFixed(2)}</td>`);
      let timeStr = '–';
      if (paceSecPerKm && paceSecPerKm > 0) {
        const totalSec = paceSecPerKm * d.km;
        const h = Math.floor(totalSec / 3600);
        const m = Math.floor((totalSec % 3600) / 60);
        const s = Math.round(totalSec % 60);
        if (h > 0) {
          timeStr = h + ':' + String(m).padStart(2, '0') + ':' + String(s).padStart(2, '0');
        } else {
          timeStr = m + ':' + String(s).padStart(2, '0');
        }
      }
      tds.push(`<td class="px-3 py-2 border-b font-medium">${timeStr}</td>`);
      tr.innerHTML = tds.join('');
      tbody.appendChild(tr);
    });
  }

  // Panel 2: Known speed
  const speedValueInput = document.getElementById('speed-value');
  const speedUnitSelect = document.getElementById('speed-unit');

  function updateFromSpeed() {
    const val = parseFloat(speedValueInput.value);
    const unit = speedUnitSelect.value;

    const outPaceKm = document.getElementById('speed-out-pace-min-per-km');
    const outPaceMile = document.getElementById('speed-out-pace-min-per-mile');
    const outKmh = document.getElementById('speed-out-kmh');
    const outMph = document.getElementById('speed-out-mph');

    if (!val || val <= 0) {
      outPaceKm.textContent = outPaceMile.textContent = outKmh.textContent = outMph.textContent = '–';
      return;
    }

    let kmh, mph;
    if (unit === 'kmh') {
      kmh = val;
      mph = val / 1.60934;
    } else {
      mph = val;
      kmh = val * 1.60934;
    }

    const paceMinPerKm = 60 / kmh;
    const paceMinPerMile = 60 / mph;

    outPaceKm.textContent = secondsToMinString(paceMinPerKm * 60);
    outPaceMile.textContent = secondsToMinString(paceMinPerMile * 60);
    outKmh.textContent = formatNumber(kmh, 2);
    outMph.textContent = formatNumber(mph, 2);
  }

  [speedValueInput, speedUnitSelect].forEach(el => {
    el.addEventListener('input', updateFromSpeed);
    el.addEventListener('change', updateFromSpeed);
  });

  // Panel 3: Distance & time → pace
  const dtDistanceInput = document.getElementById('dt-distance');
  const dtDistanceUnit = document.getElementById('dt-distance-unit');
  const dtHoursInput = document.getElementById('dt-hours');
  const dtMinutesInput = document.getElementById('dt-minutes');
  const dtSecondsInput = document.getElementById('dt-seconds');

  function updateFromDistanceTime() {
    const dist = parseFloat(dtDistanceInput.value);
    const unit = dtDistanceUnit.value;
    const h = parseFloat(dtHoursInput.value) || 0;
    const m = parseFloat(dtMinutesInput.value) || 0;
    const s = parseFloat(dtSecondsInput.value) || 0;
    const totalSec = h * 3600 + m * 60 + s;

    const outPaceKm = document.getElementById('dt-out-pace-min-per-km');
    const outPaceMile = document.getElementById('dt-out-pace-min-per-mile');
    const outKmh = document.getElementById('dt-out-kmh');
    const outMph = document.getElementById('dt-out-mph');

    if (!dist || dist <= 0 || totalSec <= 0) {
      outPaceKm.textContent = outPaceMile.textContent = outKmh.textContent = outMph.textContent = '–';
      return;
    }

    const distKm = unit === 'km' ? dist : dist * 1.60934;
    const distMiles = unit === 'mile' ? dist : dist / 1.60934;

    const paceSecPerKm = totalSec / distKm;
    const paceSecPerMile = totalSec / distMiles;

    outPaceKm.textContent = secondsToMinString(paceSecPerKm);
    outPaceMile.textContent = secondsToMinString(paceSecPerMile);

    const hours = totalSec / 3600;
    const speedKmh = distKm / hours;
    const speedMph = distMiles / hours;

    outKmh.textContent = formatNumber(speedKmh, 2);
    outMph.textContent = formatNumber(speedMph, 2);
  }

  [dtDistanceInput, dtDistanceUnit, dtHoursInput, dtMinutesInput, dtSecondsInput].forEach(el => {
    el.addEventListener('input', updateFromDistanceTime);
    el.addEventListener('change', updateFromDistanceTime);
  });

  // Quick pace table
  function buildPaceTable() {
    const tbody = document.getElementById('pace-table-body');
    const pacesMinPerKm = [3, 3.5, 4, 4.5, 5, 5.5, 6, 6.5, 7, 8, 9, 10]; // minutes per km
    tbody.innerHTML = '';
    pacesMinPerKm.forEach(p => {
      const paceSecPerKm = p * 60;
      const paceSecPerMile = paceSecPerKm * 1.60934;
      const paceMinPerMile = paceSecPerMile / 60;
      const speedKmh = 60 / p;
      const speedMph = 60 / paceMinPerMile;

      const tr = document.createElement('tr');
      tr.className = 'odd:bg-white even:bg-gray-50';
      tr.innerHTML = `
        <td class="px-3 py-1.5 border-b">${secondsToMinString(paceSecPerKm)}</td>
        <td class="px-3 py-1.5 border-b">${secondsToMinString(paceSecPerMile)}</td>
        <td class="px-3 py-1.5 border-b">${formatNumber(speedKmh, 2)}</td>
        <td class="px-3 py-1.5 border-b">${formatNumber(speedMph, 2)}</td>
      `;
      tbody.appendChild(tr);
    });
  }

  // Initialize defaults
  buildPaceTable();
  paceMinutesInput.value = '5';
  paceSecondsInput.value = '0';
  updateFromPace();
</script>

