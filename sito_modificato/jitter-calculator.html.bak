    <script>
      // Tab handling
      const tabs = [
        { btn: "tab-phase-noise", panel: "panel-phase-noise" },
        { btn: "tab-rms-pp", panel: "panel-rms-pp" },
        { btn: "tab-snr", panel: "panel-snr" },
        { btn: "tab-period", panel: "panel-period" },
      ];
      function activateTab(id) {
        tabs.forEach((t) => {
          const btn = document.getElementById(t.btn);
          const panel = document.getElementById(t.panel);
          if (t.btn === id) {
            btn.classList.add("tab-active");
            btn.classList.remove("border-transparent");
            btn.classList.add("border-blue-600");
            panel.classList.remove("hidden");
          } else {
            btn.classList.remove("tab-active");
            btn.classList.remove("border-blue-600");
            btn.classList.add("border-transparent");
            panel.classList.add("hidden");
          }
        });
      }
      tabs.forEach((t) => {
        document
          .getElementById(t.btn)
          .addEventListener("click", () => activateTab(t.btn));
      });
      activateTab("tab-phase-noise");

      // Helpers
      function unitToFactor(unit) {
        switch (unit) {
          case "s":
            return 1;
          case "ms":
            return 1e-3;
          case "us":
            return 1e-6;
          case "ns":
            return 1e-9;
          case "ps":
            return 1e-12;
          case "fs":
            return 1e-15;
          case "Hz":
            return 1;
          case "kHz":
            return 1e3;
          case "MHz":
            return 1e6;
          case "GHz":
            return 1e9;
          case "Gbps":
            return 1e9;
          default:
            return 1;
        }
      }
      function parseNumber(v) {
        if (v === null || v === undefined) return NaN;
        const s = String(v).trim();
        if (!s) return NaN;
        const n = Number(s);
        return isNaN(n) ? NaN : n;
      }

      // Phase noise example
      document
        .getElementById("pn-fill-example")
        .addEventListener("click", () => {
          const example = [
            "10      -80",
            "100     -100",
            "1e3     -120",
            "10e3    -140",
            "100e3   -150",
            "1e6     -155",
          ].join("\\n");
          document.getElementById("pn-table").value = example;
        });

      // Phase noise -> jitter
      document.getElementById("pn-calc").addEventListener("click", () => {
        const f0 =
          parseNumber(document.getElementById("pn-carrier").value) *
          unitToFactor(document.getElementById("pn-carrier-unit").value);
        const fmin =
          parseNumber(document.getElementById("pn-fmin").value) *
          unitToFactor(document.getElementById("pn-offset-unit").value);
        const fmax =
          parseNumber(document.getElementById("pn-fmax").value) *
          unitToFactor(document.getElementById("pn-offset-unit").value);
        const scale = document.getElementById("pn-integration-scale").value;
        const text = document.getElementById("pn-table").value || "";
        if (!f0 || !text.trim()) {
          alert("Please enter carrier frequency and phase‑noise data.");
          return;
        }

        const lines = text.split(/\\r?\\n/);
        let points = [];
        for (const line of lines) {
          const s = line.trim();
          if (!s) continue;
          const parts = s.split(/[,\\s]+/).filter(Boolean);
          if (parts.length < 2) continue;
          const f = parseNumber(parts[0]);
          const L = parseNumber(parts[1]);
          if (isNaN(f) || isNaN(L) || f <= 0) continue;
          points.push({ f, L });
        }
        if (points.length < 2) {
          alert("Please provide at least two valid phase‑noise points.");
          return;
        }
        points.sort((a, b) => a.f - b.f);

        // Filter by integration band
        const bandPoints = points.filter(
          (p) => (!fmin || p.f >= fmin) && (!fmax || p.f <= fmax)
        );
        if (bandPoints.length < 2) {
          alert(
            "No phase‑noise points fall inside the selected integration band."
          );
          return;
        }

        // Numeric integration of 2 * L_lin(f) df
        let sigmaPhi2 = 0;
        for (let i = 0; i < bandPoints.length - 1; i++) {
          const p1 = bandPoints[i];
          const p2 = bandPoints[i + 1];
          const f1 = p1.f;
          const f2 = p2.f;
          if (f2 <= f1) continue;

          const L1 = Math.pow(10, p1.L / 10);
          const L2 = Math.pow(10, p2.L / 10);

          let integralSegment = 0;
          if (scale === "log") {
            // integrate in log-frequency space using trapezoidal rule
            const x1 = Math.log10(f1);
            const x2 = Math.log10(f2);
            const dx = x2 - x1;
            const m = (L2 - L1) / dx;
            // approximate integral of L(f) df over [f1,f2] via sampling in log space
            const steps = 20;
            let sum = 0;
            for (let k = 0; k <= steps; k++) {
              const x = x1 + (dx * k) / steps;
              const Lx = L1 + m * (x - x1);
              const fx = Math.pow(10, x);
              const dfdx = fx * Math.log(10);
              const weight = k === 0 || k === steps ? 0.5 : 1;
              sum += weight * Lx * dfdx;
            }
            integralSegment = (dx / steps) * sum;
          } else {
            // linear frequency trapezoidal rule
            integralSegment = 0.5 * (L1 + L2) * (f2 - f1);
          }
          sigmaPhi2 += 2 * integralSegment;
        }

        if (sigmaPhi2 <= 0 || !isFinite(sigmaPhi2)) {
          alert(
            "Integration failed. Please check your phase‑noise data and integration band."
          );
          return;
        }

        const sigmaPhi = Math.sqrt(sigmaPhi2); // radians
        const sigmaT = sigmaPhi / (2 * Math.PI * f0); // seconds

        const fmt = (v) => (isFinite(v) ? v.toExponential(3) : "–");
        document.getElementById("pn-phase-rad").textContent = fmt(sigmaPhi);
        document.getElementById("pn-phase-deg").textContent = fmt(
          (sigmaPhi * 180) / Math.PI
        );
        document.getElementById("pn-noise-power").textContent = fmt(sigmaPhi2);

        document.getElementById("pn-jitter-s").textContent = fmt(sigmaT);
        document.getElementById("pn-jitter-ps").textContent = fmt(
          sigmaT * 1e12
        );
        document.getElementById("pn-jitter-fs").textContent = fmt(
          sigmaT * 1e15
        );
      });

      // RMS <-> Peak-to-peak
      function convertTimeToSeconds(value, unitSelId) {
        return (
          parseNumber(value) *
          unitToFactor(document.getElementById(unitSelId).value)
        );
      }
      function setTimeFromSeconds(seconds, valueId, unitSelId) {
        const unit = document.getElementById(unitSelId).value;
        const factor = unitToFactor(unit);
        document.getElementById(valueId).value = (seconds / factor).toPrecision(
          6
        );
      }

      document.getElementById("btn-rms-to-pp").addEventListener("click", () => {
        const jrms_s = convertTimeToSeconds(
          document.getElementById("rms-value").value,
          "rms-unit"
        );
        const mult = parseNumber(document.getElementById("sigma-mult").value);
        if (!jrms_s || !mult) {
          alert("Enter RMS jitter and multiplier.");
          return;
        }
        const jpp_s = jrms_s * mult;
        setTimeFromSeconds(jpp_s, "pp-value", "pp-unit");
      });

      document.getElementById("btn-pp-to-rms").addEventListener("click", () => {
        const jpp_s = convertTimeToSeconds(
          document.getElementById("pp-value").value,
          "pp-unit"
        );
        const mult = parseNumber(document.getElementById("sigma-mult").value);
        if (!jpp_s || !mult) {
          alert("Enter peak‑to‑peak jitter and multiplier.");
          return;
        }
        const jrms_s = jpp_s / mult;
        setTimeFromSeconds(jrms_s, "rms-value", "rms-unit");
      });

      // SNR <-> Jitter
      document
        .getElementById("btn-snr-to-jitter")
        .addEventListener("click", () => {
          const fin =
            parseNumber(document.getElementById("snr-fin").value) *
            unitToFactor(document.getElementById("snr-fin-unit").value);
          const snr = parseNumber(document.getElementById("snr-value").value);
          if (!fin || !snr) {
            alert("Enter input frequency and SNR.");
            return;
          }
          const sigma_t = 1 / (2 * Math.PI * fin * Math.pow(10, snr / 20));
          setTimeFromSeconds(sigma_t, "snr-jitter", "snr-jitter-unit");
        });

      document
        .getElementById("btn-jitter-to-snr")
        .addEventListener("click", () => {
          const fin =
            parseNumber(document.getElementById("snr-fin").value) *
            unitToFactor(document.getElementById("snr-fin-unit").value);
          const sigma_t = convertTimeToSeconds(
            document.getElementById("snr-jitter").value,
            "snr-jitter-unit"
          );
          if (!fin || !sigma_t) {
            alert("Enter input frequency and jitter.");
            return;
          }
          const snr = -20 * Math.log10(2 * Math.PI * fin * sigma_t);
          document.getElementById("snr-value").value = snr.toFixed(2);
        });

      // Period & UI jitter
      function updateUiPeriod() {
        const rate = parseNumber(document.getElementById("ui-rate").value);
        const unit = document.getElementById("ui-rate-unit").value;
        const Rb = rate * unitToFactor(unit);
        if (!Rb) {
          document.getElementById("ui-period-s").textContent = "–";
          document.getElementById("ui-period-ps").textContent = "–";
          return;
        }
        const T = 1 / Rb;
        document.getElementById("ui-period-s").textContent = T.toExponential(3);
        document.getElementById("ui-period-ps").textContent = (
          T * 1e12
        ).toFixed(3);
      }
      document
        .getElementById("ui-rate")
        .addEventListener("input", updateUiPeriod);
      document
        .getElementById("ui-rate-unit")
        .addEventListener("change", updateUiPeriod);
      updateUiPeriod();

      document
        .getElementById("btn-time-to-ui")
        .addEventListener("click", () => {
          const rate = parseNumber(document.getElementById("ui-rate").value);
          const unit = document.getElementById("ui-rate-unit").value;
          const Rb = rate * unitToFactor(unit);
          const jt = convertTimeToSeconds(
            document.getElementById("ui-jitter-time").value,
            "ui-jitter-time-unit"
          );
          if (!Rb || !jt) {
            alert("Enter bit rate and jitter time.");
            return;
          }
          const T = 1 / Rb;
          document.getElementById("ui-jitter-ui").value = (jt / T).toPrecision(
            6
          );
        });

      document
        .getElementById("btn-ui-to-time")
        .addEventListener("click", () => {
          const rate = parseNumber(document.getElementById("ui-rate").value);
          const unit = document.getElementById("ui-rate-unit").value;
          const Rb = rate * unitToFactor(unit);
          const jui = parseNumber(
            document.getElementById("ui-jitter-ui").value
          );
          if (!Rb || !jui) {
            alert("Enter bit rate and jitter in UI.");
            return;
          }
          const T = 1 / Rb;
          const jt = jui * T;
          setTimeFromSeconds(jt, "ui-jitter-time", "ui-jitter-time-unit");
        });

      // Mobile menu accessibility
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", () => {
          const menu = document.getElementById("mobile-menu");
          const expanded = menu.classList.toggle("hidden");
          document
            .getElementById("mobile-menu-toggle")
            .setAttribute("aria-expanded", (!expanded).toString());
        });

      // MathJax config
      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
