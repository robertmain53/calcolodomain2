    <script>
      // Lumber calculator logic
      (function () {
        const tableBody = document.getElementById("lumberTableBody");
        const addRowBtn = document.getElementById("addRowBtn");
        const clearRowsBtn = document.getElementById("clearRowsBtn");
        const exampleBtn = document.getElementById("exampleBtn");

        const pricingBasisEl = document.getElementById("pricingBasis");
        const unitPriceEl = document.getElementById("unitPrice");
        const unitPriceHintEl = document.getElementById("unitPriceHint");
        const wasteFactorEl = document.getElementById("wasteFactor");
        const defaultThicknessEl = document.getElementById("defaultThickness");
        const defaultWidthEl = document.getElementById("defaultWidth");

        const totalBoardFeetEl = document.getElementById("totalBoardFeet");
        const totalLinearFeetEl = document.getElementById("totalLinearFeet");
        const totalCubicMetersEl = document.getElementById("totalCubicMeters");
        const wasteAppliedEl = document.getElementById("wasteApplied");
        const requiredBoardFeetEl =
          document.getElementById("requiredBoardFeet");
        const requiredLinearFeetEl =
          document.getElementById("requiredLinearFeet");
        const requiredCubicMetersEl = document.getElementById(
          "requiredCubicMeters"
        );
        const estimatedCostEl = document.getElementById("estimatedCost");

        function createRow(initial = {}) {
          const tr = document.createElement("tr");
          tr.className = "border-t border-gray-100";

          tr.innerHTML = `
        <td class="px-2 py-1">
          <input type="number" min="0" step="1" value="${
            initial.qty ?? 1
          }" class="table-input input-number text-right" aria-label="Quantity">
        </td>
        <td class="px-2 py-1">
          <select class="table-input border border-gray-200 rounded-md text-xs md:text-sm" aria-label="Unit system">
            <option value="imperial"${
              initial.unit === "metric" ? "" : " selected"
            }>in/ft</option>
            <option value="metric"${
              initial.unit === "metric" ? " selected" : ""
            }>mm/m</option>
          </select>
        </td>
        <td class="px-2 py-1">
          <input type="number" step="0.01" min="0" value="${
            initial.thickness ?? defaultThicknessEl.value
          }" class="table-input input-number text-right" aria-label="Thickness">
        </td>
        <td class="px-2 py-1">
          <input type="number" step="0.01" min="0" value="${
            initial.width ?? defaultWidthEl.value
          }" class="table-input input-number text-right" aria-label="Width">
        </td>
        <td class="px-2 py-1">
          <input type="number" step="0.01" min="0" value="${
            initial.length ?? 8
          }" class="table-input input-number text-right" aria-label="Length">
        </td>
        <td class="px-2 py-1 text-right text-gray-700">
          <span class="row-bf">0.00</span>
        </td>
        <td class="px-2 py-1 text-right text-gray-700">
          <span class="row-lf">0.00</span>
        </td>
        <td class="px-2 py-1 text-right text-gray-700">
          <span class="row-m3">0.0000</span>
        </td>
        <td class="px-2 py-1 text-center">
          <button type="button" class="text-red-500 hover:text-red-700 text-xs md:text-sm" aria-label="Remove row">&times;</button>
        </td>
      `;

          // Attach listeners
          tr.querySelectorAll("input, select").forEach((el) => {
            el.addEventListener("input", recalc);
            el.addEventListener("change", recalc);
          });
          tr.querySelector("button").addEventListener("click", () => {
            tr.remove();
            recalc();
          });

          tableBody.appendChild(tr);
          recalc();
        }

        function parseNumber(val) {
          const n = parseFloat(val);
          return isNaN(n) ? 0 : n;
        }

        function recalc() {
          let totalBF = 0;
          let totalLF = 0;
          let totalM3 = 0;

          Array.from(tableBody.rows).forEach((row) => {
            const inputs = row.querySelectorAll("input, select");
            const qty = parseNumber(inputs[0].value);
            const unit = inputs[1].value;
            const thickness = parseNumber(inputs[2].value);
            const width = parseNumber(inputs[3].value);
            const length = parseNumber(inputs[4].value);

            let bf = 0,
              lf = 0,
              m3 = 0;

            if (qty > 0 && thickness > 0 && width > 0 && length > 0) {
              if (unit === "imperial") {
                // thickness, width in inches; length in feet
                bf = (thickness * width * length * qty) / 12;
                lf = length * qty;
                const t_m = thickness * 0.0254;
                const w_m = width * 0.0254;
                const l_m = length * 0.3048;
                m3 = t_m * w_m * l_m * qty;
              } else {
                // metric: thickness, width in mm; length in meters
                const t_m = thickness / 1000;
                const w_m = width / 1000;
                const l_m = length;
                m3 = t_m * w_m * l_m * qty;
                bf = m3 * 423.776; // 1 m³ ≈ 423.776 board feet
                const l_ft = length * 3.28084;
                lf = l_ft * qty;
              }
            }

            row.querySelector(".row-bf").textContent = bf.toFixed(2);
            row.querySelector(".row-lf").textContent = lf.toFixed(2);
            row.querySelector(".row-m3").textContent = m3.toFixed(4);

            totalBF += bf;
            totalLF += lf;
            totalM3 += m3;
          });

          const wastePct = Math.max(
            0,
            Math.min(50, parseNumber(wasteFactorEl.value))
          );
          const factor = 1 + wastePct / 100;

          const reqBF = totalBF * factor;
          const reqLF = totalLF * factor;
          const reqM3 = totalM3 * factor;

          totalBoardFeetEl.textContent = totalBF.toFixed(2) + " bf";
          totalLinearFeetEl.textContent = totalLF.toFixed(2) + " ft";
          totalCubicMetersEl.textContent = totalM3.toFixed(4) + " m³";

          wasteAppliedEl.textContent = wastePct.toFixed(0) + "%";
          requiredBoardFeetEl.textContent = reqBF.toFixed(2) + " bf";
          requiredLinearFeetEl.textContent = reqLF.toFixed(2) + " ft";
          requiredCubicMetersEl.textContent = reqM3.toFixed(4) + " m³";

          // Cost
          const basis = pricingBasisEl.value;
          const unitPrice = parseNumber(unitPriceEl.value);
          let cost = 0;

          if (basis === "bf") {
            cost = reqBF * unitPrice;
          } else if (basis === "lf") {
            cost = reqLF * unitPrice;
          } else if (basis === "m3") {
            cost = reqM3 * unitPrice;
          }

          estimatedCostEl.textContent = "$" + cost.toFixed(2);

          // Update hint
          let hint =
            "Set a pricing basis to see the unit (board ft, linear ft, or m³).";
          if (basis === "bf") hint = "Unit price per board foot (bf).";
          if (basis === "lf") hint = "Unit price per linear foot (ft).";
          if (basis === "m3") hint = "Unit price per cubic meter (m³).";
          unitPriceHintEl.textContent = hint;
        }

        addRowBtn.addEventListener("click", () => createRow());
        clearRowsBtn.addEventListener("click", () => {
          tableBody.innerHTML = "";
          recalc();
        });

        exampleBtn.addEventListener("click", () => {
          tableBody.innerHTML = "";
          // Simple example framing list
          const exampleRows = [
            {
              qty: 20,
              unit: "imperial",
              thickness: 1.5,
              width: 3.5,
              length: 8,
            }, // 2x4x8
            {
              qty: 10,
              unit: "imperial",
              thickness: 1.5,
              width: 5.5,
              length: 10,
            }, // 2x6x10
            {
              qty: 8,
              unit: "imperial",
              thickness: 1.5,
              width: 7.25,
              length: 12,
            }, // 2x8x12
          ];
          exampleRows.forEach((r) => createRow(r));
        });

        pricingBasisEl.addEventListener("change", recalc);
        unitPriceEl.addEventListener("input", recalc);
        wasteFactorEl.addEventListener("input", recalc);
        defaultThicknessEl.addEventListener("change", () => {});
        defaultWidthEl.addEventListener("change", () => {});

        // Initialize with one row
        createRow();
      })();
    </script>

