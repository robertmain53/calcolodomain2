              { upTo: 365600, rate: 0.35 },
              { upTo: Infinity, rate: 0.37 },
            ],
            hoh: [
              { upTo: 16550, rate: 0.1 },
              { upTo: 63100, rate: 0.12 },
              { upTo: 100500, rate: 0.22 },
              { upTo: 191950, rate: 0.24 },
              { upTo: 243700, rate: 0.32 },
              { upTo: 609350, rate: 0.35 },
              { upTo: Infinity, rate: 0.37 },
            ],
          },
          ssWageBase: 168600,
        },
      };

      function formatMoney(n) {
        if (!isFinite(n)) return "$0";
        return n.toLocaleString("en-US", {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 0,
        });
      }

      function calcFederalTax(taxable, year, status) {
        if (taxable <= 0) return 0;
        const brackets = FED_PARAMS[year].brackets[status];
        let remaining = taxable;
        let lastLimit = 0;
        let tax = 0;
        for (const b of brackets) {
          const span = Math.min(remaining, b.upTo - lastLimit);
          if (span <= 0) break;
          tax += span * b.rate;
          remaining -= span;
          lastLimit = b.upTo;
          if (remaining <= 0) break;
        }
        return tax;
      }

      function calcFica(wages, year) {
        if (wages <= 0) return 0;
        const base = FED_PARAMS[year].ssWageBase;
        const ss = 0.062 * Math.min(wages, base);
        const medicare = 0.0145 * wages;
        return ss + medicare;
      }

      function recalc() {
        const year = parseInt(document.getElementById("taxYear").value, 10);
        const status = document.getElementById("filingStatus").value;
        const wages = Math.max(
          0,
          Number(document.getElementById("wageIncome").value) || 0
        );
        const preTax = Math.max(
          0,
          Number(document.getElementById("preTaxContrib").value) || 0
        );
        const deductionType = document.getElementById("deductionType").value;
        const itemized = Math.max(
          0,
          Number(document.getElementById("itemizedAmount").value) || 0
        );
        const propertyTax = Math.max(
          0,
          Number(document.getElementById("propertyTax").value) || 0
        );
        const salesTax = Math.max(
          0,
          Number(document.getElementById("salesTax").value) || 0
        );
        const ficaOption =
          document.querySelector('input[name="ficaOption"]:checked')?.value ||
          "yes";

        const stdDed = FED_PARAMS[year].standardDeduction[status];
        let deduction = 0;
        if (deductionType === "standard") deduction = stdDed;
        else if (deductionType === "itemized") deduction = itemized;
        else deduction = 0;

        const adjIncome = Math.max(0, wages - preTax);
        const taxable = Math.max(0, adjIncome - deduction);

        const fedTax = calcFederalTax(taxable, year, status);
        const fica = ficaOption === "yes" ? calcFica(wages, year) : 0;
        const totalTax = fedTax + fica;
        const takeHome = wages - totalTax;
        const effRate = wages > 0 ? (totalTax / wages) * 100 : 0;

        const otherTax = propertyTax + salesTax;
        const grandTotal = totalTax + otherTax;

        document.getElementById("federalTaxable").textContent =
          formatMoney(taxable);
        document.getElementById("federalTax").textContent = formatMoney(fedTax);
        document.getElementById("ficaTax").textContent = formatMoney(fica);
        document.getElementById("totalTax").textContent = formatMoney(totalTax);
        document.getElementById("effectiveRate").textContent =
          effRate.toFixed(1) + "%";
        document.getElementById("takeHome").textContent = formatMoney(takeHome);
        document.getElementById("propertyTaxOut").textContent =
          formatMoney(propertyTax);
        document.getElementById("salesTaxOut").textContent =
          formatMoney(salesTax);
        document.getElementById("grandTotalTax").textContent =
          formatMoney(grandTotal);

        // Update bar chart
        const base = wages > 0 ? wages : 1;
        const pctFed = Math.max(0, Math.min(100, (fedTax / base) * 100));
        const pctFica = Math.max(0, Math.min(100, (fica / base) * 100));
        const pctOther = Math.max(0, Math.min(100, (otherTax / base) * 100));
        let pctNet = 100 - (pctFed + pctFica + pctOther);
        if (pctNet < 0) pctNet = 0;

        document.getElementById("barFederal").style.width = pctFed + "%";
        document.getElementById("barFica").style.width = pctFica + "%";
        document.getElementById("barOther").style.width = pctOther + "%";
        document.getElementById("barNet").style.width = pctNet + "%";
      }

      document.addEventListener("DOMContentLoaded", () => {
        const deductionTypeEl = document.getElementById("deductionType");
        const itemizedWrapper = document.getElementById("itemizedWrapper");
        deductionTypeEl.addEventListener("change", () => {
          if (deductionTypeEl.value === "itemized") {
            itemizedWrapper.classList.remove("hidden");
          } else {
            itemizedWrapper.classList.add("hidden");
          }
          recalc();
        });

        document.getElementById("calcBtn").addEventListener("click", (e) => {
          e.preventDefault();
          recalc();
        });

        // Recalculate on key changes for responsiveness
        [
          "taxYear",
          "filingStatus",
          "wageIncome",
          "preTaxContrib",
          "itemizedAmount",
          "propertyTax",
          "salesTax",
        ].forEach((id) => {
          const el = document.getElementById(id);
          if (el) {
            el.addEventListener("input", () => {
              // debounce lightly
              clearTimeout(el._t);
              el._t = setTimeout(recalc, 150);
            });
          }
        });
        document.querySelectorAll('input[name="ficaOption"]').forEach((r) => {
          r.addEventListener("change", recalc);
        });

        recalc();
      });
    </script>

