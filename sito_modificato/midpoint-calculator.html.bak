            <script>
              document.addEventListener('DOMContentLoaded', function() {
                const inputs = {
                    x1: document.getElementById('x1'),
                    y1: document.getElementById('y1'),
                    z1: document.getElementById('z1'),
                    x2: document.getElementById('x2'),
                    y2: document.getElementById('y2'),
                    z2: document.getElementById('z2')
                };
                const calculateBtn = document.getElementById('calculate-btn');
                const resultsContainer = document.getElementById('results-container');
                const errorMessage = document.getElementById('error-message');
                const resultMidpoint = document.getElementById('result-midpoint');
                const resultDistance = document.getElementById('result-distance');
                const stepByStep = document.getElementById('step-by-step');

                // --- Helper Functions ---
                function parseInput(val) {
                    const num = parseFloat(val);
                    return isNaN(num) ? null : num;
                }
                
                function formatNumber(num) {
                    if (Math.abs(num) < 1e-10) return '0';
                    return parseFloat(num.toFixed(6)).toString();
                }

                function showError(message) {
                    errorMessage.textContent = `Error: ${message}`;
                    resultsContainer.classList.add('hidden');
                }

                // --- Core Calculation Logic ---
                function calculateMidpointAndDistance(p1, p2) {
                    const Mx = (p1.x + p2.x) / 2;
                    const My = (p1.y + p2.y) / 2;
                    const Mz = (p1.z !== null && p2.z !== null) ? (p1.z + p2.z) / 2 : null;
                    
                    const dx = p2.x - p1.x;
                    const dy = p2.y - p1.y;
                    let dz_sq = 0;
                    if (p1.z !== null && p2.z !== null) {
                        const dz = p2.z - p1.z;
                        dz_sq = dz * dz;
                    }
                    const distance = Math.sqrt(dx * dx + dy * dy + dz_sq);

                    return { Mx, My, Mz, distance };
                }

                calculateBtn.addEventListener('click', () => {
                    const p1 = {
                        x: parseInput(inputs.x1.value),
                        y: parseInput(inputs.y1.value),
                        z: parseInput(inputs.z1.value)
                    };
                    const p2 = {
                        x: parseInput(inputs.x2.value),
                        y: parseInput(inputs.y2.value),
                        z: parseInput(inputs.z2.value)
                    };
                    
                    try {
                        if (p1.x === null || p1.y === null || p2.x === null || p2.y === null) {
                            throw new Error('Please enter valid numerical values for all X and Y coordinates.');
                        }
                        
                        errorMessage.textContent = '';
                        
                        const is3D = p1.z !== null || p2.z !== null;
                        
                        // Ensure 3D consistency
                        if (is3D && (p1.z === null || p2.z === null)) {
                            p1.z = p1.z === null ? 0 : p1.z;
                            p2.z = p2.z === null ? 0 : p2.z;
                        }

                        const { Mx, My, Mz, distance } = calculateMidpointAndDistance(p1, p2);

                        // 1. Midpoint Step-by-Step
                        let steps = `
                            <p>1. <strong>Midpoint Formula:</strong> The formula is the average of the coordinates: ${is3D ? `$\\left(\\frac{x_1 + x_2}{2}, \\frac{y_1 + y_2}{2}, \\frac{z_1 + z_2}{2}\\right)$` : `$\\left(\\frac{x_1 + x_2}{2}, \\frac{y_1 + y_2}{2}\\right)$`}</p>
                            
                            <h4 class="font-semibold text-lg mt-4">2. Calculate X-Midpoint ($M_x$):</h4>
                            <p class="formula-box text-center">$M_x = \\frac{${formatNumber(p1.x)} + ${formatNumber(p2.x)}}{2} = \\frac{${formatNumber(p1.x + p2.x)}}{2} = ${formatNumber(Mx)}$</p>

                            <h4 class="font-semibold text-lg mt-4">3. Calculate Y-Midpoint ($M_y$):</h4>
                            <p class="formula-box text-center">$M_y = \\frac{${formatNumber(p1.y)} + ${formatNumber(p2.y)}}{2} = \\frac{${formatNumber(p1.y + p2.y)}}{2} = ${formatNumber(My)}$</p>
                        `;
                        
                        if (is3D) {
                             steps += `
                                <h4 class="font-semibold text-lg mt-4">4. Calculate Z-Midpoint ($M_z$):</h4>
                                <p class="formula-box text-center">$M_z = \\frac{${formatNumber(p1.z)} + ${formatNumber(p2.z)}}{2} = \\frac{${formatNumber(p1.z + p2.z)}}{2} = ${formatNumber(Mz)}$</p>
                            `;
                        }

                        // 2. Distance Step-by-Step (Simplified)
                        const dx_sq = (p2.x - p1.x) * (p2.x - p1.x);
                        const dy_sq = (p2.y - p1.y) * (p2.y - p1.y);
                        let sum_sq = dx_sq + dy_sq;
                        let distanceFormula = `D = \\sqrt{(${formatNumber(p2.x)} - ${formatNumber(p1.x)})^2 + (${formatNumber(p2.y)} - ${formatNumber(p1.y)})^2}`;
                        
                        if (is3D) {
                            const dz_sq = (p2.z - p1.z) * (p2.z - p1.z);
                            sum_sq += dz_sq;
                            distanceFormula = `D = \\sqrt{(${formatNumber(p2.x)} - ${formatNumber(p1.x)})^2 + (${formatNumber(p2.y)} - ${formatNumber(p1.y)})^2 + (${formatNumber(p2.z)} - ${formatNumber(p1.z)})^2}`;
                        }
                        
                         steps += `
                            <h4 class="font-semibold text-lg mt-4">5. Calculate Segment Length (Distance):</h4>
                            <p>Formula: ${distanceFormula}$</p>
                            <p class="formula-box text-center">$D = \\sqrt{${formatNumber(dx_sq)} + ${formatNumber(dy_sq)} ${is3D ? `+ ${formatNumber(dz_sq)}` : ''}} = \\sqrt{${formatNumber(sum_sq)}} \approx ${formatNumber(distance)}$</p>
                        `;


                        // 3. Update DOM
                        let midpointCoords = `(${formatNumber(Mx)}, ${formatNumber(My)}`;
                        if (is3D) midpointCoords += `, ${formatNumber(Mz)}`;
                        midpointCoords += ')';
                        
                        resultMidpoint.textContent = midpointCoords;
                        resultDistance.textContent = formatNumber(distance);
                        stepByStep.innerHTML = steps;
                        resultsContainer.classList.remove('hidden');

                        // Re-render MathJax
                        if (window.MathJax) {
                            window.MathJax.typesetPromise([resultMidpoint, stepByStep]);
                        }

                    } catch (e) {
                        showError(e.message);
                    }
                });
                
                // Initial calculation (for example values)
                calculateBtn.click(); 
              });
            </script>

