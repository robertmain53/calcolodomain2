    <script>
      // State
      let mode = "speed"; // 'speed' or 'pace'
      let unitSystem = "metric"; // 'metric' or 'imperial'

      const speedInputGroup = document.getElementById("speed-input-group");
      const paceInputGroup = document.getElementById("pace-input-group");
      const speedInput = document.getElementById("speed-input");
      const paceMinInput = document.getElementById("pace-min");
      const paceSecInput = document.getElementById("pace-sec");
      const inclineInput = document.getElementById("incline-input");
      const distanceInput = document.getElementById("distance-input");
      const distanceUnit = document.getElementById("distance-unit");
      const errorMsg = document.getElementById("error-msg");

      const speedUnitLabel = document.getElementById("speed-unit-label");
      const paceUnitLabel = document.getElementById("pace-unit-label");

      const resultSpeed = document.getElementById("result-speed");
      const resultPace = document.getElementById("result-pace");
      const resultPaceAlt = document.getElementById("result-pace-alt");
      const resultEquivalentPace = document.getElementById(
        "result-equivalent-pace"
      );
      const resultEffortFactor = document.getElementById(
        "result-effort-factor"
      );
      const resultTime = document.getElementById("result-time");

      function formatPace(minPerUnit) {
        if (!isFinite(minPerUnit) || minPerUnit <= 0) return "–";
        const totalSeconds = Math.round(minPerUnit * 60);
        const m = Math.floor(totalSeconds / 60);
        const s = totalSeconds % 60;
        return (
          m +
          ":" +
          String(s).padStart(2, "0") +
          " /" +
          (unitSystem === "metric" ? "km" : "mi")
        );
      }

      function formatPaceCustom(minPerUnit, unitLabel) {
        if (!isFinite(minPerUnit) || minPerUnit <= 0) return "–";
        const totalSeconds = Math.round(minPerUnit * 60);
        const m = Math.floor(totalSeconds / 60);
        const s = totalSeconds % 60;
        return m + ":" + String(s).padStart(2, "0") + " /" + unitLabel;
      }

      function formatTimeMinutes(totalMinutes) {
        if (!isFinite(totalMinutes) || totalMinutes <= 0) return "–";
        const totalSeconds = Math.round(totalMinutes * 60);
        const h = Math.floor(totalSeconds / 3600);
        const m = Math.floor((totalSeconds % 3600) / 60);
        const s = totalSeconds % 60;
        if (h > 0) {
          return h + "h " + m + "m " + s + "s";
        }
        if (m > 0) {
          return m + "m " + s + "s";
        }
        return s + "s";
      }

      function mphToKmh(mph) {
        return mph * 1.609344;
      }
      function kmhToMph(kmh) {
        return kmh / 1.609344;
      }

      function recalc() {
        errorMsg.classList.add("hidden");
        errorMsg.textContent = "";

        let speedKmh = null; // internal canonical speed
        let paceMinPerKm = null;

        try {
          if (mode === "speed") {
            const speedVal = parseFloat(speedInput.value);
            if (!isFinite(speedVal) || speedVal <= 0) {
              clearResults();
              return;
            }
            if (unitSystem === "metric") {
              speedKmh = speedVal;
            } else {
              speedKmh = mphToKmh(speedVal);
            }
            paceMinPerKm = 60 / speedKmh;
          } else {
            const min = parseFloat(paceMinInput.value);
            const sec = parseFloat(paceSecInput.value);
            const totalMin =
              (isFinite(min) ? min : 0) + (isFinite(sec) ? sec : 0) / 60;
            if (!isFinite(totalMin) || totalMin <= 0) {
              clearResults();
              return;
            }
            if (unitSystem === "metric") {
              paceMinPerKm = totalMin;
              speedKmh = 60 / paceMinPerKm;
            } else {
              const paceMinPerMile = totalMin;
              const speedMph = 60 / paceMinPerMile;
              speedKmh = mphToKmh(speedMph);
              paceMinPerKm = 60 / speedKmh;
            }
          }

          // Base speed/pace in both systems
          const speedMph = kmhToMph(speedKmh);
          const paceMinPerMile = 60 / speedMph;

          // Effort factor from incline (simple model)
          const incline = parseFloat(inclineInput.value);
          let effortFactor = 1;
          if (isFinite(incline) && incline > 0) {
            effortFactor = 1 + 0.03 * incline;
          }
          if (effortFactor < 1) effortFactor = 1;

          // Equivalent outdoor pace in current primary unit
          const basePace =
            unitSystem === "metric" ? paceMinPerKm : paceMinPerMile;
          const equivPace = basePace * effortFactor;

          // Distance/time
          let totalTimeMin = null;
          const distVal = parseFloat(distanceInput.value);
          if (isFinite(distVal) && distVal > 0) {
            const distUnit = distanceUnit.value;
            if (distUnit === "km") {
              const paceForKm = paceMinPerKm;
              totalTimeMin = paceForKm * distVal;
            } else {
              const paceForMile = paceMinPerMile;
              totalTimeMin = paceForMile * distVal;
            }
          }

          // Update results display
          if (unitSystem === "metric") {
            resultSpeed.textContent =
              speedKmh.toFixed(2) + " km/h (" + speedMph.toFixed(2) + " mph)";
            resultPace.textContent = formatPaceCustom(paceMinPerKm, "km");
            resultPaceAlt.textContent = formatPaceCustom(
              paceMinPerMile,
              "mile"
            );
            resultEquivalentPace.textContent = formatPaceCustom(
              equivPace,
              "km"
            );
          } else {
            resultSpeed.textContent =
              speedMph.toFixed(2) + " mph (" + speedKmh.toFixed(2) + " km/h)";
            resultPace.textContent = formatPaceCustom(paceMinPerMile, "mile");
            resultPaceAlt.textContent = formatPaceCustom(paceMinPerKm, "km");
            resultEquivalentPace.textContent = formatPaceCustom(
              equivPace,
              unitSystem === "metric" ? "km" : "mile"
            );
          }

          resultEffortFactor.textContent = effortFactor.toFixed(2) + "×";
          resultTime.textContent = totalTimeMin
            ? formatTimeMinutes(totalTimeMin)
            : "–";
        } catch (e) {
          clearResults();
          errorMsg.textContent =
            "Something went wrong. Please check your inputs.";
          errorMsg.classList.remove("hidden");
        }
      }

      function clearResults() {
        resultSpeed.textContent = "–";
        resultPace.textContent = "–";
        resultPaceAlt.textContent = "–";
        resultEquivalentPace.textContent = "–";
        resultEffortFactor.textContent = "–";
        resultTime.textContent = "–";
      }

      // Mode toggle
      document.getElementById("mode-speed").addEventListener("click", () => {
        mode = "speed";
        document
          .getElementById("mode-speed")
          .classList.add("bg-blue-600", "text-white");
        document
          .getElementById("mode-speed")
          .classList.remove("bg-white", "text-gray-700");
        document
          .getElementById("mode-pace")
          .classList.add("bg-white", "text-gray-700");
        document
          .getElementById("mode-pace")
          .classList.remove("bg-blue-600", "text-white");
        speedInputGroup.classList.remove("hidden");
        paceInputGroup.classList.add("hidden");
        recalc();
      });

      document.getElementById("mode-pace").addEventListener("click", () => {
        mode = "pace";
        document
          .getElementById("mode-pace")
          .classList.add("bg-blue-600", "text-white");
        document
          .getElementById("mode-pace")
          .classList.remove("bg-white", "text-gray-700");
        document
          .getElementById("mode-speed")
          .classList.add("bg-white", "text-gray-700");
        document
          .getElementById("mode-speed")
          .classList.remove("bg-blue-600", "text-white");
        speedInputGroup.classList.add("hidden");
        paceInputGroup.classList.remove("hidden");
        recalc();
      });

      // Unit toggle
      document.getElementById("unit-metric").addEventListener("click", () => {
        unitSystem = "metric";
        document
          .getElementById("unit-metric")
          .classList.add("bg-blue-600", "text-white");
        document
          .getElementById("unit-metric")
          .classList.remove("bg-white", "text-gray-700");
        document
          .getElementById("unit-imperial")
          .classList.add("bg-white", "text-gray-700");
        document
          .getElementById("unit-imperial")
          .classList.remove("bg-blue-600", "text-white");
        speedUnitLabel.textContent = "(km/h)";
        paceUnitLabel.textContent = "(min/km)";
        recalc();
      });

      document.getElementById("unit-imperial").addEventListener("click", () => {
        unitSystem = "imperial";
        document
          .getElementById("unit-imperial")
          .classList.add("bg-blue-600", "text-white");
        document
          .getElementById("unit-imperial")
          .classList.remove("bg-white", "text-gray-700");
        document
          .getElementById("unit-metric")
          .classList.add("bg-white", "text-gray-700");
        document
          .getElementById("unit-metric")
          .classList.remove("bg-blue-600", "text-white");
        speedUnitLabel.textContent = "(mph)";
        paceUnitLabel.textContent = "(min/mile)";
        recalc();
      });

      // Inputs listeners
      [
        speedInput,
        paceMinInput,
        paceSecInput,
        inclineInput,
        distanceInput,
        distanceUnit,
      ].forEach((el) => {
        el.addEventListener("input", recalc);
        el.addEventListener("change", recalc);
      });

      document.getElementById("reset-btn").addEventListener("click", () => {
        speedInput.value = "";
        paceMinInput.value = "";
        paceSecInput.value = "";
        inclineInput.value = "";
        distanceInput.value = "";
        distanceUnit.value = "km";
        clearResults();
        errorMsg.classList.add("hidden");
        errorMsg.textContent = "";
      });

      // Quick table generation
      function buildQuickTable() {
        const tbody = document.getElementById("quick-table-body");
        const speedsMph = [
          3, 3.5, 4, 4.5, 5, 5.5, 6, 6.5, 7, 7.5, 8, 8.5, 9, 10, 11, 12,
        ];
        tbody.innerHTML = "";
        speedsMph.forEach((mph) => {
          const kmh = mphToKmh(mph);
          const paceMinPerMile = 60 / mph;
          const paceMinPerKm = 60 / kmh;

          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="px-3 py-1.5 text-gray-800">${mph.toFixed(1)}</td>
        <td class="px-3 py-1.5 text-gray-800">${kmh.toFixed(1)}</td>
        <td class="px-3 py-1.5 text-gray-800">${formatPaceCustom(
          paceMinPerMile,
          "mile"
        )}</td>
        <td class="px-3 py-1.5 text-gray-800">${formatPaceCustom(
          paceMinPerKm,
          "km"
        )}</td>
      `;
          tbody.appendChild(tr);
        });
      }

      buildQuickTable();
      clearResults();
    </script>

