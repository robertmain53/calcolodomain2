    <script>
      function clampComponent(x) {
        if (isNaN(x) || !isFinite(x)) return 0;
        if (x < 0) return 0;
        if (x > 2.375) return 2.375;
        return x;
      }

      function classifyRating(r) {
        if (!isFinite(r)) return "—";
        if (r < 70) return "Poor / struggling";
        if (r < 85) return "Below average";
        if (r < 95) return "Solid starter";
        if (r < 105) return "Very good";
        if (r < 115) return "Pro Bowl / elite season";
        if (r <= 158.3) return "MVP-level efficiency";
        return "—";
      }

      function formatPct(value) {
        if (!isFinite(value)) return "—";
        return value.toFixed(1) + "%";
      }

      function calculatePasserRating() {
        const att = parseFloat(document.getElementById("att").value);
        const comp = parseFloat(document.getElementById("comp").value);
        const yards = parseFloat(document.getElementById("yards").value);
        const td = parseFloat(document.getElementById("td").value);
        const ints = parseFloat(document.getElementById("int").value);
        const errorEl = document.getElementById("error-msg");

        if (isNaN(att) || att <= 0) {
          errorEl.textContent = "Pass attempts (ATT) must be greater than 0.";
          errorEl.classList.remove("hidden");
          return null;
        }
        if (comp < 0 || yards < 0 || td < 0 || ints < 0) {
          errorEl.textContent = "Stats cannot be negative.";
          errorEl.classList.remove("hidden");
          return null;
        }
        if (comp > att) {
          errorEl.textContent =
            "Completions (COMP) cannot exceed attempts (ATT).";
          errorEl.classList.remove("hidden");
          return null;
        }
        if (td > att) {
          errorEl.textContent = "Touchdowns (TD) cannot exceed attempts (ATT).";
          errorEl.classList.remove("hidden");
          return null;
        }
        if (ints > att) {
          errorEl.textContent =
            "Interceptions (INT) cannot exceed attempts (ATT).";
          errorEl.classList.remove("hidden");
          return null;
        }

        errorEl.classList.add("hidden");

        const aRaw = (comp / att - 0.3) * 5;
        const bRaw = (yards / att - 3) * 0.25;
        const cRaw = (td / att) * 20;
        const dRaw = 2.375 - (ints / att) * 25;

        const a = clampComponent(aRaw);
        const b = clampComponent(bRaw);
        const c = clampComponent(cRaw);
        const d = clampComponent(dRaw);

        const rating = ((a + b + c + d) / 6) * 100;

        const compPct = (comp / att) * 100;
        const ypa = yards / att;
        const tdPct = (td / att) * 100;
        const intPct = (ints / att) * 100;

        return {
          rating,
          a,
          b,
          c,
          d,
          compPct,
          ypa,
          tdPct,
          intPct,
        };
      }

      function updateUI() {
        const result = calculatePasserRating();
        if (!result) return;

        document.getElementById("rating-display").textContent =
          result.rating.toFixed(1);
        document.getElementById("rating-label").textContent = classifyRating(
          result.rating
        );

        document.getElementById("comp-pct-display").textContent = formatPct(
          result.compPct
        );
        document.getElementById("ypa-display").textContent = isFinite(
          result.ypa
        )
          ? result.ypa.toFixed(1)
          : "—";
        document.getElementById("td-pct-display").textContent = formatPct(
          result.tdPct
        );
        document.getElementById("int-pct-display").textContent = formatPct(
          result.intPct
        );

        document.getElementById("comp-a").textContent =
          result.a.toFixed(3) +
          " (raw " +
          ((result.compPct / 100 - 0.3) * 5).toFixed(3) +
          ")";
        document.getElementById("comp-b").textContent =
          result.b.toFixed(3) +
          " (raw " +
          ((result.ypa - 3) * 0.25).toFixed(3) +
          ")";
        document.getElementById("comp-c").textContent =
          result.c.toFixed(3) +
          " (raw " +
          ((result.tdPct / 100) * 20).toFixed(3) +
          ")";
        document.getElementById("comp-d").textContent =
          result.d.toFixed(3) +
          " (raw " +
          (2.375 - (result.intPct / 100) * 25).toFixed(3) +
          ")";
      }

      document
        .getElementById("calc-btn")
        .addEventListener("click", function (e) {
          e.preventDefault();
          updateUI();
        });

      document
        .getElementById("reset-btn")
        .addEventListener("click", function () {
          document.getElementById("att").value = 35;
          document.getElementById("comp").value = 25;
          document.getElementById("yards").value = 275;
          document.getElementById("td").value = 2;
          document.getElementById("int").value = 1;
          document.getElementById("error-msg").classList.add("hidden");
          updateUI();
        });

      document
        .getElementById("toggle-components")
        .addEventListener("click", function () {
          const panel = document.getElementById("components-panel");
          const isHidden = panel.classList.contains("hidden");
          panel.classList.toggle("hidden");
          this.textContent = isHidden
            ? "Hide formula components (a, b, c, d)"
            : "Show formula components (a, b, c, d)";
        });

      ["att", "comp", "yards", "td", "int"].forEach((id) => {
        document.getElementById(id).addEventListener("change", updateUI);
        document.getElementById(id).addEventListener("keyup", function (e) {
          if (e.key === "Enter") updateUI();
        });
      });

      // Initial calculation on load
      document.addEventListener("DOMContentLoaded", updateUI);
    </script>

