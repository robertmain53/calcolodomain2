    <script>
      // Helix row template
      function createHelixRow(index, units) {
        const row = document.createElement("div");
        row.className =
          "border rounded p-3 flex flex-col md:flex-row md:items-end gap-3";
        row.dataset.index = index;

        row.innerHTML = `
      <div class="flex-1">
        <label class="block text-xs font-medium mb-1">Helix ${
          index + 1
        } diameter</label>
        <div class="flex">
          <input type="number" step="0.001" class="helix-diameter flex-1 border rounded-l px-3 py-2" value="${
            units === "si" ? 0.3 : 1
          }">
          <span class="helix-diameter-unit inline-flex items-center px-3 border border-l-0 rounded-r bg-gray-50 text-xs text-gray-600">${
            units === "si" ? "m" : "ft"
          }</span>
        </div>
      </div>
      <div class="flex-1">
        <label class="block text-xs font-medium mb-1">Depth to helix center</label>
        <div class="flex">
          <input type="number" step="0.01" class="helix-depth flex-1 border rounded-l px-3 py-2" value="${
            units === "si" ? 4 + index : 12 + 2 * index
          }">
          <span class="helix-depth-unit inline-flex items-center px-3 border border-l-0 rounded-r bg-gray-50 text-xs text-gray-600">${
            units === "si" ? "m" : "ft"
          }</span>
        </div>
      </div>
      <button type="button" class="remove-helix text-xs text-red-600 mt-1 md:mt-0">Remove</button>
    `;
        return row;
      }

      (function () {
        const unitsBtnSI = document.getElementById("units-si");
        const unitsBtnUS = document.getElementById("units-us");
        const modeSoil = document.getElementById("mode-soil");
        const modeTorque = document.getElementById("mode-torque");
        const modeBoth = document.getElementById("mode-both");

        const shaftDiameter = document.getElementById("shaft-diameter");
        const shaftDiameterUnit = document.getElementById(
          "shaft-diameter-unit"
        );
        const embeddedLength = document.getElementById("embedded-length");
        const embeddedLengthUnit = document.getElementById(
          "embedded-length-unit"
        );
        const su = document.getElementById("su");
        const suUnit = document.getElementById("su-unit");
        const gammaEff = document.getElementById("gamma-eff");
        const gammaUnit = document.getElementById("gamma-unit");
        const torque = document.getElementById("torque");
        const torqueUnit = document.getElementById("torque-unit");
        const lateralK = document.getElementById("lateral-k");
        const lateralKUnit = document.getElementById("lateral-k-unit");

        const soilParamsSection = document.getElementById(
          "soil-params-section"
        );
        const torqueSection = document.getElementById("torque-section");

        const helixContainer = document.getElementById("helix-container");
        const addHelixBtn = document.getElementById("add-helix");

        const calcBtn = document.getElementById("calc-btn");
        const resetBtn = document.getElementById("reset-btn");
        const errorEl = document.getElementById("calc-error");

        const resultsSection = document.getElementById("results-section");
        const qUltCompEl = document.getElementById("q-ult-comp");
        const qAllCompEl = document.getElementById("q-all-comp");
        const qUltTensEl = document.getElementById("q-ult-tens");
        const qAllTensEl = document.getElementById("q-all-tens");
        const hUltEl = document.getElementById("h-ult");
        const qUltTorqueEl = document.getElementById("q-ult-torque");
        const summaryText = document.getElementById("summary-text");

        const soilType = document.getElementById("soil-type");
        const clayInputs = document.getElementById("clay-inputs");
        const sandInputs = document.getElementById("sand-inputs");
        const phi = document.getElementById("phi");
        const alphaShaft = document.getElementById("alpha-shaft");
        const fs = document.getElementById("fs");
        const kt = document.getElementById("kt");

        let currentUnits = "si"; // 'si' or 'us'
        let currentMode = "soil"; // 'soil', 'torque', 'both'

        function setUnits(units) {
          currentUnits = units;
          if (units === "si") {
            unitsBtnSI.classList.add("bg-blue-600", "text-white");
            unitsBtnUS.classList.remove("bg-blue-600", "text-white");
            shaftDiameterUnit.textContent = "m";
            embeddedLengthUnit.textContent = "m";
            suUnit.textContent = "kPa";
            gammaUnit.textContent = "kN/m³";
            torqueUnit.textContent = "kN·m";
            lateralKUnit.textContent = "kN/m³";

            // convert approximate values if switching from US
            // (lightweight: only adjust defaults if user hasn't changed much)
          } else {
            unitsBtnUS.classList.add("bg-blue-600", "text-white");
            unitsBtnSI.classList.remove("bg-blue-600", "text-white");
            shaftDiameterUnit.textContent = "in";
            embeddedLengthUnit.textContent = "ft";
            suUnit.textContent = "psf";
            gammaUnit.textContent = "pcf";
            torqueUnit.textContent = "kip·ft";
            lateralKUnit.textContent = "k/ft³";
          }

          // Update helix row unit labels
          document.querySelectorAll(".helix-diameter-unit").forEach((el) => {
            el.textContent = units === "si" ? "m" : "ft";
          });
          document.querySelectorAll(".helix-depth-unit").forEach((el) => {
            el.textContent = units === "si" ? "m" : "ft";
          });
        }

        function setMode(mode) {
          currentMode = mode;
          [modeSoil, modeTorque, modeBoth].forEach((btn) => {
            btn.classList.remove("bg-blue-600", "text-white");
            btn.classList.add("text-gray-700");
          });
          if (mode === "soil") {
            modeSoil.classList.add("bg-blue-600", "text-white");
            soilParamsSection.classList.remove("hidden");
            torqueSection.classList.add("hidden");
          } else if (mode === "torque") {
            modeTorque.classList.add("bg-blue-600", "text-white");
            soilParamsSection.classList.add("hidden");
            torqueSection.classList.remove("hidden");
          } else {
            modeBoth.classList.add("bg-blue-600", "text-white");
            soilParamsSection.classList.remove("hidden");
            torqueSection.classList.remove("hidden");
          }
        }

        function initHelices() {
          helixContainer.innerHTML = "";
          helixContainer.appendChild(createHelixRow(0, currentUnits));
          helixContainer.appendChild(createHelixRow(1, currentUnits));
        }

        function parseNumber(el) {
          const v = parseFloat(el.value);
          return isNaN(v) ? null : v;
        }

        function handleSoilTypeChange() {
          if (soilType.value === "clay") {
            clayInputs.classList.remove("hidden");
            sandInputs.classList.add("hidden");
          } else {
            sandInputs.classList.remove("hidden");
            clayInputs.classList.add("hidden");
          }
        }

        function calculateCapacity() {
          errorEl.textContent = "";
          const D_s = parseNumber(shaftDiameter);
          const L_e = parseNumber(embeddedLength);
          const F_s = parseNumber(fs);

          if (!D_s || !L_e || !F_s || F_s <= 0) {
            errorEl.textContent =
              "Please enter valid shaft diameter, embedded length, and safety factor.";
            return;
          }

          const helices = [];
          document.querySelectorAll("#helix-container > div").forEach((row) => {
            const d = parseFloat(row.querySelector(".helix-diameter").value);
            const z = parseFloat(row.querySelector(".helix-depth").value);
            if (d > 0 && z > 0) {
              helices.push({ D: d, z: z });
            }
          });

          if (helices.length === 0) {
            errorEl.textContent = "Please define at least one helix.";
            return;
          }

          let Qb = 0; // bearing
          let Qs = 0; // shaft
          let QultTorque = null;

          // Unit conversions to consistent internal SI (kN, m, kPa)
          let D_s_m = D_s;
          let L_e_m = L_e;
          let su_kPa = soilType.value === "clay" ? parseNumber(su) : null;
          let gamma_kN_m3 = parseNumber(gammaEff);
          let phi_deg = soilType.value === "sand" ? parseNumber(phi) : null;
          let alpha = parseNumber(alphaShaft);

          if (currentUnits === "us") {
            // inches to m, ft to m, psf to kPa, pcf to kN/m3
            D_s_m = D_s * 0.0254;
            L_e_m = L_e * 0.3048;
            if (su_kPa !== null) su_kPa = su_kPa * 0.0478803; // psf to kPa
            gamma_kN_m3 = gamma_kN_m3 * 0.157087; // pcf to kN/m3
            helices.forEach((h) => {
              h.D = h.D * 0.3048;
              h.z = h.z * 0.3048;
            });
          }

          // Soil-based capacity
          if (currentMode === "soil" || currentMode === "both") {
            if (soilType.value === "clay") {
              if (!su_kPa || su_kPa <= 0) {
                errorEl.textContent =
                  "Enter a valid undrained shear strength s\u2095.";
                return;
              }
              const Nc = 9;
              helices.forEach((h) => {
                const Ah = (Math.PI * Math.pow(h.D, 2)) / 4;
                const qb = Nc * su_kPa; // kPa
                Qb += (qb * Ah) / 1000; // kN
              });
              const tau = alpha * su_kPa; // kPa
              const As = Math.PI * D_s_m * L_e_m;
              Qs = (tau * As) / 1000; // kN
            } else {
              if (!phi_deg || !gamma_kN_m3) {
                errorEl.textContent = "Enter valid φ and γ' for sand.";
                return;
              }
              const phi_rad = (phi_deg * Math.PI) / 180;
              const Nq =
                Math.exp(Math.PI * Math.tan(phi_rad)) *
                Math.pow(Math.tan(Math.PI / 4 + phi_rad / 2), 2);
              helices.forEach((h) => {
                const Ah = (Math.PI * Math.pow(h.D, 2)) / 4;
                const qb = 0.5 * gamma_kN_m3 * h.D * Nq; // kPa approx
                Qb += (qb * Ah) / 1000; // kN
              });
              const sigma_v_avg = gamma_kN_m3 * (L_e_m / 2); // kPa
              const tau = alpha * sigma_v_avg * Math.tan(phi_rad);
              const As = Math.PI * D_s_m * L_e_m;
              Qs = (tau * As) / 1000; // kN
            }
          }

          // Torque-based capacity
          if (currentMode === "torque" || currentMode === "both") {
            const T = parseNumber(torque);
            const Kt = parseNumber(kt);
            if (!T || !Kt || T <= 0 || Kt <= 0) {
              errorEl.textContent = "Enter valid installation torque and Kt.";
              return;
            }
            let T_kNm = T;
            if (currentUnits === "us") {
              // kip·ft to kN·m
              T_kNm = T * 4.44822 * 0.3048;
            }
            QultTorque = Kt * T_kNm; // kN
          }

          // Combine capacities
          let Qult_soil =
            currentMode === "soil" || currentMode === "both" ? Qb + Qs : null;
          let Qult_comp = 0;
          let Qult_tens = 0;

          if (currentMode === "soil") {
            Qult_comp = Qult_soil;
            Qult_tens = Qult_soil; // same expression; in practice may differ slightly
          } else if (currentMode === "torque") {
            Qult_comp = QultTorque;
            Qult_tens = QultTorque;
          } else {
            // both: show soil-based for comp/uplift, torque separately
            Qult_comp = Qult_soil;
            Qult_tens = Qult_soil;
          }

          // Lateral capacity (simplified)
          let k_h = parseNumber(lateralK);
          let Hult = null;
          if (k_h && k_h > 0) {
            let k_h_kN_m3 = k_h;
            if (currentUnits === "us") {
              // k/ft³ to kN/m³ (approx)
              k_h_kN_m3 = (k_h * 14.5939) / Math.pow(0.3048, 3);
            }
            const delta_lim = Math.min(0.025, L_e_m / 240); // m
            Hult = k_h_kN_m3 * L_e_m * delta_lim; // kN
          }

          // Apply safety factor
          const Qall_comp = Qult_comp / F_s;
          const Qall_tens = Qult_tens / F_s;
          const Hall = Hult !== null ? Hult / F_s : null;

          // Convert back to display units
          function fmt(val, decimals) {
            if (val === null || isNaN(val)) return "–";
            return val.toFixed(decimals);
          }

          let compUnit = "kN";
          let tensUnit = "kN";
          let latUnit = "kN";
          let torqueCapUnit = "kN";

          if (currentUnits === "us") {
            compUnit = "kips";
            tensUnit = "kips";
            latUnit = "kips";
            torqueCapUnit = "kips";
            Qult_comp *= 0.224809;
            Qall_comp *= 0.224809;
            Qult_tens *= 0.224809;
            Qall_tens *= 0.224809;
            if (Hult !== null) Hult *= 0.224809;
            if (QultTorque !== null) QultTorque *= 0.224809;
          }

          qUltCompEl.textContent = fmt(Qult_comp, 2) + " " + compUnit;
          qAllCompEl.textContent = fmt(Qall_comp, 2) + " " + compUnit;
          qUltTensEl.textContent = fmt(Qult_tens, 2) + " " + tensUnit;
          qAllTensEl.textContent = fmt(Qall_tens, 2) + " " + tensUnit;
          hUltEl.textContent =
            Hult !== null ? fmt(Hult, 2) + " " + latUnit : "–";
          qUltTorqueEl.textContent =
            QultTorque !== null
              ? fmt(QultTorque, 2) + " " + torqueCapUnit
              : "–";

          let summary = [];
          summary.push(
            "Compression ultimate capacity: " +
              fmt(Qult_comp, 2) +
              " " +
              compUnit +
              ", allowable: " +
              fmt(Qall_comp, 2) +
              " " +
              compUnit +
              "."
          );
          summary.push(
            "Uplift ultimate capacity: " +
              fmt(Qult_tens, 2) +
              " " +
              tensUnit +
              ", allowable: " +
              fmt(Qall_tens, 2) +
              " " +
              tensUnit +
              "."
          );
          if (Hult !== null) {
            summary.push(
              "Estimated lateral ultimate capacity at ground line: " +
                fmt(Hult, 2) +
                " " +
                latUnit +
                " (before safety factor)."
            );
          }
          if (
            QultTorque !== null &&
            (currentMode === "torque" || currentMode === "both")
          ) {
            summary.push(
              "Torque-based ultimate capacity: " +
                fmt(QultTorque, 2) +
                " " +
                torqueCapUnit +
                "."
            );
          }
          summaryText.textContent = summary.join(" ");

          resultsSection.classList.remove("hidden");
        }

        function resetForm() {
          shaftDiameter.value = currentUnits === "si" ? 0.089 : 3.5;
          embeddedLength.value = currentUnits === "si" ? 6 : 20;
          su.value = 50;
          phi.value = 32;
          gammaEff.value = currentUnits === "si" ? 18 : 115;
          alphaShaft.value = 0.5;
          torque.value = currentUnits === "si" ? 10 : 7.4;
          kt.value = 10;
          fs.value = 2.5;
          lateralK.value = currentUnits === "si" ? 20000 : 50000;
          soilType.value = "clay";
          handleSoilTypeChange();
          initHelices();
          resultsSection.classList.add("hidden");
          errorEl.textContent = "";
        }

        // Event listeners
        unitsBtnSI.addEventListener("click", () => {
          setUnits("si");
          resetForm();
        });
        unitsBtnUS.addEventListener("click", () => {
          setUnits("us");
          resetForm();
        });

        modeSoil.addEventListener("click", () => setMode("soil"));
        modeTorque.addEventListener("click", () => setMode("torque"));
        modeBoth.addEventListener("click", () => setMode("both"));

        soilType.addEventListener("change", handleSoilTypeChange);

        addHelixBtn.addEventListener("click", () => {
          const index = helixContainer.children.length;
          helixContainer.appendChild(createHelixRow(index, currentUnits));
        });

        helixContainer.addEventListener("click", (e) => {
          if (e.target.classList.contains("remove-helix")) {
            const row = e.target.closest("div[data-index]");
            if (row && helixContainer.children.length > 1) {
              helixContainer.removeChild(row);
            }
          }
        });

        calcBtn.addEventListener("click", calculateCapacity);
        resetBtn.addEventListener("click", resetForm);

        // FAQ toggle (simple, in addition to global script)
        document
          .getElementById("faq-container")
          .addEventListener("click", (e) => {
            const btn = e.target.closest(".faq-question");
            if (!btn) return;
            const answer = btn.parentElement.querySelector(".faq-answer");
            const sign = btn.querySelector("span:last-child");
            const isHidden = answer.classList.contains("hidden");
            document
              .querySelectorAll("#faq-container .faq-answer")
              .forEach((a) => a.classList.add("hidden"));
            document
              .querySelectorAll("#faq-container .faq-question span:last-child")
              .forEach((s) => (s.textContent = "+"));
            if (isHidden) {
              answer.classList.remove("hidden");
              sign.textContent = "−";
            }
          });

        // Initialize
        setUnits("si");
        setMode("soil");
        handleSoilTypeChange();
        initHelices();
      })();
    </script>

