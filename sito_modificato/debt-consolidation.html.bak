      );
      const resTimeCurrent = document.getElementById("res-time-current");
      const resTimeConsol = document.getElementById("res-time-consol");
      const resTimeDiff = document.getElementById("res-time-diff");
      const resBalanceTotal = document.getElementById("res-balance-total");
      const resWeightedApr = document.getElementById("res-weighted-apr");
      const resTotalPaidCurrent = document.getElementById(
        "res-total-paid-current"
      );
      const resConsolAmount = document.getElementById("res-consol-amount");
      const resConsolApr = document.getElementById("res-consol-apr");
      const resConsolFees = document.getElementById("res-consol-fees");
      const resTotalPaidConsol = document.getElementById(
        "res-total-paid-consol"
      );

      function createDebtRow(index, data = {}) {
        const tr = document.createElement("tr");
        tr.className = index % 2 === 0 ? "bg-white" : "bg-gray-50";
        tr.innerHTML = `
      <td class="px-3 py-2">
        <input type="text" class="w-full border border-gray-300 rounded-md px-2 py-1 text-sm focus:ring-1 focus:ring-blue-500 focus:outline-none" placeholder="e.g. Credit card ${
          index + 1
        }" value="${data.name || ""}">
      </td>
      <td class="px-3 py-2 text-right">
        <input type="number" min="0" step="1" class="input-number w-full text-right border border-gray-300 rounded-md px-2 py-1 text-sm focus:ring-1 focus:ring-blue-500 focus:outline-none" value="${
          data.balance || ""
        }">
      </td>
      <td class="px-3 py-2 text-right">
        <input type="number" min="0" step="0.01" class="input-number w-full text-right border border-gray-300 rounded-md px-2 py-1 text-sm focus:ring-1 focus:ring-blue-500 focus:outline-none" value="${
          data.apr || ""
        }">
      </td>
      <td class="px-3 py-2 text-right">
        <input type="number" min="1" step="1" class="input-number w-full text-right border border-gray-300 rounded-md px-2 py-1 text-sm focus:ring-1 focus:ring-blue-500 focus:outline-none" value="${
          data.term || ""
        }">
      </td>
      <td class="px-3 py-2 text-center">
        <button type="button" class="remove-debt-row text-red-600 hover:text-red-800 text-sm">✕</button>
      </td>
    `;
        return tr;
      }

      function refreshRowStriping() {
        Array.from(debtsBody.children).forEach((tr, idx) => {
          tr.className = idx % 2 === 0 ? "bg-white" : "bg-gray-50";
        });
      }

      function addDefaultDebts() {
        debtsBody.innerHTML = "";
        const defaults = [
          { name: "Credit card 1", balance: 4500, apr: 19.99, term: 48 },
          { name: "Credit card 2", balance: 2500, apr: 22.99, term: 36 },
          { name: "Personal loan", balance: 6000, apr: 12.5, term: 60 },
        ];
        defaults.forEach((d, i) => debtsBody.appendChild(createDebtRow(i, d)));
        refreshRowStriping();
      }

      addDebtRowBtn.addEventListener("click", () => {
        const idx = debtsBody.children.length;
        debtsBody.appendChild(createDebtRow(idx, {}));
        refreshRowStriping();
      });

      debtsBody.addEventListener("click", (e) => {
        if (e.target.classList.contains("remove-debt-row")) {
          const row = e.target.closest("tr");
          if (row) {
            debtsBody.removeChild(row);
            refreshRowStriping();
          }
        }
      });

      resetDebtsBtn.addEventListener("click", () => {
        addDefaultDebts();
      });

      currentStrategySelect.addEventListener("change", () => {
        if (currentStrategySelect.value === "fixed") {
          fixedPaymentWrapper.classList.remove("hidden");
        } else {
          fixedPaymentWrapper.classList.add("hidden");
        }
      });

      clearAllBtn.addEventListener("click", () => {
        debtsBody.innerHTML = "";
        consolRateInput.value = "";
        consolTermInput.value = "";
        consolFeesInput.value = "";
        fixedMonthlyPaymentInput.value = "";
        currentStrategySelect.value = "minimum";
        fixedPaymentWrapper.classList.add("hidden");
        errorMessage.textContent = "";
        resultsSection.classList.add("hidden");
      });

      calculateBtn.addEventListener("click", () => {
        errorMessage.textContent = "";

        const debts = [];
        Array.from(debtsBody.children).forEach((tr) => {
          const inputs = tr.querySelectorAll("input");
          const name = inputs[0].value.trim();
          const balance = toNumber(inputs[1].value);
          const apr = toNumber(inputs[2].value);
          const term = toNumber(inputs[3].value);
          if (balance > 0 && apr >= 0 && term > 0) {
            debts.push({ name, balance, apr, term });
          }
        });

        if (debts.length === 0) {
          errorMessage.textContent =
            "Please enter at least one valid debt (balance, APR, and term).";
          resultsSection.classList.add("hidden");
          return;
        }

        const consolRate = toNumber(consolRateInput.value);
        const consolTerm = toNumber(consolTermInput.value);
        const consolFees = toNumber(consolFeesInput.value);

        if (consolRate < 0 || consolTerm <= 0) {
          errorMessage.textContent =
            "Please enter a valid consolidation loan APR and term.";
          resultsSection.classList.add("hidden");
          return;
        }

        const totalBalance = debts.reduce((sum, d) => sum + d.balance, 0);
        const weightedApr =
          debts.reduce((sum, d) => sum + d.balance * d.apr, 0) / totalBalance;

        // Current scenario
        let currentMonthly = 0;
        let currentTotalInterest = 0;
        let currentTotalPaid = 0;
        let currentMonths = 0;

        const strategy = currentStrategySelect.value;

        if (strategy === "minimum") {
          debts.forEach((d) => {
            const minPayment = Math.max(d.balance * 0.02, 25);
            const months = calcMonthsToPayoff(d.balance, d.apr, minPayment);
            const totalPaid = minPayment * months;
            const interest = totalPaid - d.balance;
            currentMonthly += minPayment;
            currentTotalInterest += interest;
            currentTotalPaid += totalPaid;
            currentMonths = Math.max(currentMonths, months);
          });
        } else {
          const budget = toNumber(fixedMonthlyPaymentInput.value);
          if (budget <= 0) {
            errorMessage.textContent =
              "Please enter your total monthly payment budget.";
            resultsSection.classList.add("hidden");
            return;
          }
          const minSum = debts.reduce(
            (sum, d) => sum + Math.max(d.balance * 0.02, 25),
            0
          );
          if (budget < minSum * 0.5) {
            errorMessage.textContent =
              "Your monthly budget is very low relative to your debts. Increase it to see realistic results.";
          }
          // Allocate budget proportionally by balance
          let maxMonths = 0;
          let totalPaid = 0;
          debts.forEach((d) => {
            const share = d.balance / totalBalance;
            const payment = budget * share;
            const months = calcMonthsToPayoff(d.balance, d.apr, payment);
            const paid = payment * months;
            totalPaid += paid;
            maxMonths = Math.max(maxMonths, months);
          });
          currentMonthly = budget;
          currentTotalPaid = totalPaid;
          currentTotalInterest = totalPaid - totalBalance;
          currentMonths = maxMonths;
        }

        // Consolidation scenario
        const consolAmount = totalBalance;
        const consolMonthly = calcMonthlyPayment(
          consolAmount,
          consolRate,
          consolTerm
        );
        const consolTotalPaid = consolMonthly * consolTerm + consolFees;
        const consolInterest = consolTotalPaid - consolAmount;

        // Results
        resMonthlyCurrent.textContent = formatMoney(currentMonthly);
        resMonthlyConsol.textContent = formatMoney(consolMonthly);
        const monthlyDiff = consolMonthly - currentMonthly;
        if (Math.abs(monthlyDiff) < 1e-2) {
          resMonthlyDiff.textContent = "About the same monthly payment.";
          resMonthlyDiff.className = "text-sm text-gray-600";
        } else if (monthlyDiff < 0) {
          resMonthlyDiff.textContent =
            formatMoney(-monthlyDiff) + " lower per month with consolidation.";
          resMonthlyDiff.className = "text-sm text-green-700";
        } else {
          resMonthlyDiff.textContent =
            formatMoney(monthlyDiff) + " higher per month with consolidation.";
          resMonthlyDiff.className = "text-sm text-yellow-700";
        }

        resInterestCurrent.textContent = formatMoney(currentTotalInterest);
        resInterestConsol.textContent = formatMoney(consolInterest);
        const interestDiff = currentTotalInterest - consolInterest;
        if (Math.abs(interestDiff) < 1e-2) {
          resInterestSavings.textContent =
            "No significant difference in total interest.";
          resInterestSavings.className = "text-sm text-gray-600";
        } else if (interestDiff > 0) {
          resInterestSavings.textContent =
            "You save about " + formatMoney(interestDiff) + " in interest.";
          resInterestSavings.className = "text-sm text-green-700";
        } else {
          resInterestSavings.textContent =
            "Consolidation costs about " +
            formatMoney(-interestDiff) +
            " more in interest and fees.";
          resInterestSavings.className = "text-sm text-red-700";
        }

        resTimeCurrent.textContent = formatMonthsToYearsMonths(currentMonths);
        resTimeConsol.textContent = formatMonthsToYearsMonths(consolTerm);
        const timeDiff = currentMonths - consolTerm;
        if (!isFinite(timeDiff) || currentMonths === 0) {
          resTimeDiff.textContent = "";
        } else if (Math.abs(timeDiff) < 1) {
          resTimeDiff.textContent = "Payoff time is about the same.";
          resTimeDiff.className = "text-sm text-gray-600";
        } else if (timeDiff > 0) {
          resTimeDiff.textContent =
            "Consolidation pays off about " +
            formatMonthsToYearsMonths(timeDiff) +
            " sooner.";
          resTimeDiff.className = "text-sm text-green-700";
        } else {
          resTimeDiff.textContent =
            "Consolidation extends payoff by about " +
            formatMonthsToYearsMonths(-timeDiff) +
            ".";
          resTimeDiff.className = "text-sm text-yellow-700";
        }

        resBalanceTotal.textContent = formatMoney(totalBalance);
        resWeightedApr.textContent = formatPercent(weightedApr);
        resTotalPaidCurrent.textContent = formatMoney(currentTotalPaid);
        resConsolAmount.textContent = formatMoney(consolAmount);
        resConsolApr.textContent = formatPercent(consolRate);
        resConsolFees.textContent = formatMoney(consolFees);
        resTotalPaidConsol.textContent = formatMoney(consolTotalPaid);

        resultsSection.classList.remove("hidden");
      });

      // Initialize defaults
      addDefaultDebts();
      consolRateInput.value = 9.99;
      consolTermInput.value = 60;
      consolFeesInput.value = 0;
    </script>

