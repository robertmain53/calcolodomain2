    <script>
      // Utility functions
      function toNumber(val) {
        const n = parseFloat(val);
        return isNaN(n) ? 0 : n;
      }
      function formatMoney(val) {
        if (!isFinite(val)) return "$0";
        return (
          "$" +
          val.toLocaleString(undefined, {
            maximumFractionDigits: 2,
            minimumFractionDigits: 2,
          })
        );
      }
      function formatPercent(val) {
        if (!isFinite(val)) return "0%";
        return (
          val.toLocaleString(undefined, { maximumFractionDigits: 2 }) + "%"
        );
      }
      function formatMonthsToYearsMonths(months) {
        if (!isFinite(months) || months <= 0) return "–";
        const m = Math.round(months);
        const years = Math.floor(m / 12);
        const rem = m % 12;
        const parts = [];
        if (years > 0) parts.push(years + " year" + (years > 1 ? "s" : ""));
        if (rem > 0) parts.push(rem + " month" + (rem > 1 ? "s" : ""));
        return parts.join(" ");
      }

      // Amortization payment
      function calcMonthlyPayment(P, annualRate, nMonths) {
        if (P <= 0 || nMonths <= 0) return 0;
        const r = annualRate / 100 / 12;
        if (r === 0) return P / nMonths;
        const pow = Math.pow(r + 1, nMonths);
        return (P * r * pow) / (pow - 1);
      }

      // Approximate payoff months given fixed payment
      function calcMonthsToPayoff(
        P,
        annualRate,
        monthlyPayment,
        maxMonths = 600
      ) {
        if (P <= 0 || monthlyPayment <= 0) return 0;
        const r = annualRate / 100 / 12;
        if (r === 0) return Math.min(maxMonths, P / monthlyPayment);
        let balance = P;
        let months = 0;
        while (balance > 0 && months < maxMonths) {
          const interest = balance * r;
          const principal = monthlyPayment - interest;
          if (principal <= 0) {
            return Infinity; // payment too low to ever pay off
          }
          balance -= principal;
          months++;
        }
        return months;
      }

      // DOM elements
      const debtsBody = document.getElementById("debts-body");
      const addDebtRowBtn = document.getElementById("add-debt-row");
      const resetDebtsBtn = document.getElementById("reset-debts");
      const currentStrategySelect = document.getElementById("current-strategy");
      const fixedPaymentWrapper = document.getElementById(
        "fixed-payment-wrapper"
      );
      const fixedMonthlyPaymentInput = document.getElementById(
        "fixed-monthly-payment"
      );
      const consolRateInput = document.getElementById("consol-rate");
      const consolTermInput = document.getElementById("consol-term");
      const consolFeesInput = document.getElementById("consol-fees");
      const calculateBtn = document.getElementById("calculate-btn");
      const clearAllBtn = document.getElementById("clear-all-btn");
      const errorMessage = document.getElementById("error-message");
      const resultsSection = document.getElementById("results-section");

      // Results fields
      const resMonthlyCurrent = document.getElementById("res-monthly-current");
      const resMonthlyConsol = document.getElementById("res-monthly-consol");
      const resMonthlyDiff = document.getElementById("res-monthly-diff");
      const resInterestCurrent = document.getElementById(
        "res-interest-current"
      );
      const resInterestConsol = document.getElementById("res-interest-consol");
      const resInterestSavings = document.getElementById(
