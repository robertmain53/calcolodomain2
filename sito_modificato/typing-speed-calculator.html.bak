    <script>
      // Typing test logic
      (function () {
        const defaultTexts = [
          "Typing is a fundamental digital skill. The more you practice, the more natural it becomes to express ideas through the keyboard.",
          "Accuracy is more important than raw speed. When you focus on clean typing, your words per minute will increase over time.",
          "Short daily typing sessions are more effective than rare, long practice marathons. Consistency builds muscle memory.",
          "Professional typists keep their eyes on the screen, not the keyboard. This reduces errors and improves overall rhythm.",
          "Mixing different kinds of content, such as emails, code, and articles, helps you build a flexible and reliable typing style.",
        ];

        const targetEl = document.getElementById("target-text");
        const inputEl = document.getElementById("typing-input");
        const durationEl = document.getElementById("test-duration");
        const modeButtons = document.querySelectorAll(".mode-btn");
        const customWrapper = document.getElementById("custom-text-wrapper");
        const customTextEl = document.getElementById("custom-text");
        const btnNewText = document.getElementById("btn-new-text");
        const btnStart = document.getElementById("btn-start");
        const btnReset = document.getElementById("btn-reset");

        const statTime = document.getElementById("stat-time");
        const statWpm = document.getElementById("stat-wpm");
        const statAccuracy = document.getElementById("stat-accuracy");
        const statErrors = document.getElementById("stat-errors");
        const statKeystrokesTotal = document.getElementById(
          "stat-keystrokes-total"
        );
        const statKeystrokesCorrect = document.getElementById(
          "stat-keystrokes-correct"
        );
        const statKeystrokesIncorrect = document.getElementById(
          "stat-keystrokes-incorrect"
        );
        const statCharsTyped = document.getElementById("stat-chars-typed");
        const statCpm = document.getElementById("stat-cpm");
        const statWords = document.getElementById("stat-words");
        const statBackspaces = document.getElementById("stat-backspaces");
        const statCompletion = document.getElementById("stat-completion");

        const resultSummary = document.getElementById("result-summary");
        const summaryWpm = document.getElementById("summary-wpm");
        const summaryAccuracy = document.getElementById("summary-accuracy");

        let mode = "random";
        let targetText = "";
        let timer = null;
        let startTime = null;
        let elapsed = 0;
        let isRunning = false;
        let isFinished = false;

        let keystrokesTotal = 0;
        let keystrokesCorrect = 0;
        let keystrokesIncorrect = 0;
        let backspaces = 0;

        function pickRandomText() {
          return defaultTexts[Math.floor(Math.random() * defaultTexts.length)];
        }

        function setTargetText(text) {
          targetText = text || "";
          targetEl.innerHTML = "";
          const frag = document.createDocumentFragment();
          for (let i = 0; i < targetText.length; i++) {
            const span = document.createElement("span");
            span.textContent = targetText[i];
            span.classList.add("pending");
            if (i === 0) span.classList.add("current");
            frag.appendChild(span);
          }
          targetEl.appendChild(frag);
        }

        function resetStats() {
          elapsed = 0;
          keystrokesTotal = 0;
          keystrokesCorrect = 0;
          keystrokesIncorrect = 0;
          backspaces = 0;
          updateStats();
          resultSummary.classList.add("hidden");
        }

        function resetTest() {
          clearInterval(timer);
          timer = null;
          isRunning = false;
          isFinished = false;
          startTime = null;
          inputEl.value = "";
          resetStats();
          highlightProgress("");
        }

        function getDurationSeconds() {
          return parseInt(durationEl.value, 10) || 60;
        }

        function startTimer() {
          if (isRunning) return;
          isRunning = true;
          startTime = performance.now();
          const duration = getDurationSeconds();

          timer = setInterval(() => {
            const now = performance.now();
            elapsed = (now - startTime) / 1000;
            if (elapsed >= duration) {
              elapsed = duration;
              finishTest();
            }
            updateStats();
          }, 100);
        }

        function finishTest() {
          if (isFinished) return;
          isFinished = true;
          isRunning = false;
          clearInterval(timer);
          timer = null;
          inputEl.blur();
          const { wpm, accuracy } = computeMetrics();
          summaryWpm.textContent = `${wpm.toFixed(1)} WPM`;
          summaryAccuracy.textContent = `${accuracy.toFixed(1)}% accuracy`;
          resultSummary.classList.remove("hidden");
        }

        function computeMetrics() {
          const minutes = elapsed > 0 ? elapsed / 60 : 1 / 60;
          const charsTyped = inputEl.value.length;
          const words = charsTyped / 5;
          const wpm = words / minutes;
          const cpm = charsTyped / minutes;
          const accuracy =
            keystrokesTotal > 0
              ? (keystrokesCorrect / keystrokesTotal) * 100
              : 100;
          const completion =
            targetText.length > 0
              ? Math.min(100, (charsTyped / targetText.length) * 100)
              : 0;
          return { wpm, cpm, accuracy, charsTyped, words, completion };
        }

        function updateStats() {
          const { wpm, cpm, accuracy, charsTyped, words, completion } =
            computeMetrics();
          statTime.textContent = `${elapsed.toFixed(1)}s`;
          statWpm.textContent = isFinite(wpm) ? wpm.toFixed(0) : "0";
          statAccuracy.textContent = `${accuracy.toFixed(0)}%`;
          statErrors.textContent = keystrokesIncorrect.toString();
          statKeystrokesTotal.textContent = keystrokesTotal.toString();
          statKeystrokesCorrect.textContent = keystrokesCorrect.toString();
          statKeystrokesIncorrect.textContent = keystrokesIncorrect.toString();
          statCharsTyped.textContent = charsTyped.toString();
          statCpm.textContent = isFinite(cpm) ? cpm.toFixed(0) : "0";
          statWords.textContent = words.toFixed(1);
          statBackspaces.textContent = backspaces.toString();
          statCompletion.textContent = `${completion.toFixed(0)}%`;
        }

        function highlightProgress(currentInput) {
          const spans = targetEl.querySelectorAll("span");
          for (let i = 0; i < spans.length; i++) {
            const span = spans[i];
            const expectedChar = targetText[i] || "";
            const typedChar = currentInput[i];

            span.classList.remove("correct", "incorrect", "current", "pending");

            if (typedChar == null) {
              span.classList.add("pending");
            } else if (typedChar === expectedChar) {
              span.classList.add("correct");
            } else {
              span.classList.add("incorrect");
            }
          }
          const nextIndex = Math.min(currentInput.length, spans.length - 1);
          if (spans[nextIndex]) {
            spans[nextIndex].classList.add("current");
          }
        }

        // Event handlers
        inputEl.addEventListener("input", (e) => {
          if (isFinished) {
            e.target.value = e.target.value.slice(0, targetText.length);
            return;
          }
          if (!isRunning && e.target.value.length > 0) {
            startTimer();
          }

          const value = e.target.value;
          const lastChar = value[value.length - 1];

          // Detect backspace via length decrease (handled in keydown too)
          highlightProgress(value);

          // Update keystroke stats
          // We'll rely on keydown for backspace counting; here we just recompute correctness.
          keystrokesTotal = value.length + backspaces;
          keystrokesCorrect = 0;
          keystrokesIncorrect = 0;
          for (let i = 0; i < value.length; i++) {
            if (value[i] === targetText[i]) {
              keystrokesCorrect++;
            } else {
              keystrokesIncorrect++;
            }
          }

          updateStats();

          // Auto-finish if user completes text before time
          if (value.length >= targetText.length) {
            finishTest();
          }
        });

        inputEl.addEventListener("keydown", (e) => {
          if (e.key === "Backspace") {
            backspaces++;
            updateStats();
          }
        });

        btnStart.addEventListener("click", () => {
          if (!isRunning && !isFinished) {
            inputEl.focus();
            if (inputEl.value.length === 0) {
              // Timer will start on first input
            } else {
              startTimer();
            }
          } else if (isFinished) {
            resetTest();
            inputEl.focus();
          }
        });

        btnReset.addEventListener("click", () => {
          resetTest();
          inputEl.focus();
        });

        btnNewText.addEventListener("click", () => {
          if (mode === "random") {
            setTargetText(pickRandomText());
          } else {
            const txt = (customTextEl.value || "").trim();
            if (txt.length > 0) {
              setTargetText(txt);
            }
          }
          resetTest();
          inputEl.focus();
        });

        modeButtons.forEach((btn) => {
          btn.addEventListener("click", () => {
            modeButtons.forEach((b) => {
              b.classList.remove(
                "border-blue-600",
                "text-blue-600",
                "bg-blue-50"
              );
              b.classList.add("border-gray-300", "text-gray-700");
            });
            btn.classList.add("border-blue-600", "text-blue-600", "bg-blue-50");
            btn.classList.remove("border-gray-300", "text-gray-700");

            mode = btn.getAttribute("data-mode");
            if (mode === "custom") {
              customWrapper.classList.remove("hidden");
              const txt = (customTextEl.value || "").trim();
              setTargetText(
                txt ||
                  'Paste or type your own text above, then click "Start test".'
              );
            } else {
              customWrapper.classList.add("hidden");
              setTargetText(pickRandomText());
            }
            resetTest();
          });
        });

        // Initialize
        setTargetText(pickRandomText());
        resetStats();
      })();
    </script>

    <script>
      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
