    <script>
      // Utility helpers
      function toNumber(value) {
        const n = parseFloat(value);
        return isNaN(n) ? 0 : n;
      }

      function formatCurrency(value, currencySymbol = "$") {
        if (!isFinite(value)) return currencySymbol + "0.00";
        return currencySymbol + value.toFixed(2);
      }

      function formatKwh(value) {
        if (!isFinite(value)) return "0.00 kWh";
        return value.toFixed(2) + " kWh";
      }

      function formatKg(value) {
        if (!isFinite(value)) return "0.00 kg CO₂";
        return value.toFixed(2) + " kg CO₂";
      }

      // Device row management
      let deviceIdCounter = 0;

      const deviceRowsTbody = document.getElementById("device-rows");
      const addDeviceBtn = document.getElementById("add-device-btn");
      const loadPresetsBtn = document.getElementById("load-presets-btn");
      const resetBtn = document.getElementById("reset-btn");
      const exportBtn = document.getElementById("export-btn");

      const electricityRateInput = document.getElementById("electricity-rate");
      const electricityRateUnitSelect = document.getElementById(
        "electricity-rate-unit"
      );
      const co2FactorInput = document.getElementById("co2-factor");

      const totalKwhEl = document.getElementById("total-kwh");
      const totalKwhYearEl = document.getElementById("total-kwh-year");
      const totalCostEl = document.getElementById("total-cost");
      const totalCostYearEl = document.getElementById("total-cost-year");
      const totalCo2El = document.getElementById("total-co2");
      const totalCo2YearEl = document.getElementById("total-co2-year");

      function createDeviceRow(preset = {}) {
        const id = ++deviceIdCounter;
        const tr = document.createElement("tr");
        tr.dataset.id = id;

        tr.innerHTML = `
      <td class="px-3 py-2 align-top">
        <input type="text" class="w-full border border-gray-300 rounded-md px-2 py-1 text-xs md:text-sm focus:ring-1 focus:ring-blue-500 focus:outline-none" placeholder="e.g., Fridge" value="${
          preset.name || ""
        }">
        <div class="flex flex-wrap gap-1 mt-1">
          ${
            preset.category
              ? `<span class="badge bg-gray-100 text-gray-600 border border-gray-200">${preset.category}</span>`
              : ""
          }
        </div>
      </td>
      <td class="px-3 py-2 align-top">
        <div class="flex items-center gap-1 mb-1">
          <input type="number" min="0" step="0.1" class="w-24 border border-gray-300 rounded-md px-2 py-1 text-xs md:text-sm focus:ring-1 focus:ring-blue-500 focus:outline-none device-power" value="${
            preset.power || ""
          }">
          <select class="border border-gray-300 rounded-md px-1.5 py-1 text-xs md:text-sm input-unit device-power-unit">
            <option value="W"${
              preset.powerUnit === "kW" ? "" : " selected"
            }>W</option>
            <option value="kW"${
              preset.powerUnit === "kW" ? " selected" : ""
            }>kW</option>
          </select>
        </div>
        <div class="flex items-center gap-1 text-[11px] text-gray-500">
          <label class="flex items-center gap-1">
            <input type="checkbox" class="device-always-on">
            <span>Always on (24 h/day)</span>
          </label>
        </div>
      </td>
      <td class="px-3 py-2 align-top">
        <div class="flex items-center gap-1 mb-1">
          <input type="number" min="0" step="0.1" class="w-20 border border-gray-300 rounded-md px-2 py-1 text-xs md:text-sm focus:ring-1 focus:ring-blue-500 focus:outline-none device-hours" value="${
            preset.hoursPerDay || 1
          }">
          <span class="text-xs text-gray-600">h/day</span>
        </div>
        <div class="flex items-center gap-1">
          <input type="number" min="0" step="1" class="w-20 border border-gray-300 rounded-md px-2 py-1 text-xs md:text-sm focus:ring-1 focus:ring-blue-500 focus:outline-none device-days" value="${
            preset.daysPerMonth || 30
          }">
          <span class="text-xs text-gray-600">days/month</span>
        </div>
      </td>
      <td class="px-3 py-2 align-top text-xs md:text-sm">
        <div class="device-kwh-day text-gray-800" title="Daily energy use">0.00 kWh</div>
        <div class="device-kwh-month text-gray-500 mt-1" title="Monthly energy use">0.00 kWh / month</div>
      </td>
      <td class="px-3 py-2 align-top text-xs md:text-sm">
        <div class="device-cost-month text-gray-800" title="Monthly cost">$0.00 / month</div>
        <div class="device-cost-year text-gray-500 mt-1" title="Yearly cost">$0.00 / year</div>
      </td>
      <td class="px-3 py-2 align-top text-xs md:text-sm">
        <div class="device-co2-month text-gray-800" title="Monthly CO₂">0.00 kg CO₂ / month</div>
        <div class="device-co2-year text-gray-500 mt-1" title="Yearly CO₂">0.00 kg CO₂ / year</div>
      </td>
      <td class="px-3 py-2 align-top text-right">
        <button type="button" class="text-red-500 hover:text-red-700 text-xs md:text-sm device-remove-btn">Remove</button>
      </td>
    `;

        deviceRowsTbody.appendChild(tr);
        attachRowListeners(tr);
        updateCalculations();
      }

      function attachRowListeners(tr) {
        const inputs = tr.querySelectorAll("input, select");
        inputs.forEach((input) => {
          input.addEventListener("input", () => {
            if (input.classList.contains("device-always-on")) {
              const hoursInput = tr.querySelector(".device-hours");
              if (input.checked) {
                hoursInput.value = 24;
              }
            }
            updateCalculations();
          });
          input.addEventListener("change", updateCalculations);
        });

        const removeBtn = tr.querySelector(".device-remove-btn");
        removeBtn.addEventListener("click", () => {
          tr.remove();
          updateCalculations();
        });
      }

      function getElectricityRatePerKwh() {
        const rate = toNumber(electricityRateInput.value);
        const unit = electricityRateUnitSelect.value;
        if (unit === "per_kwh") return rate;
        if (unit === "per_mwh") return rate / 1000;
        return rate;
      }

      function updateCalculations() {
        const ratePerKwh = getElectricityRatePerKwh();
        const co2Factor = toNumber(co2FactorInput.value);

        let totalKwhMonth = 0;
        let totalKwhYear = 0;
        let totalCostMonth = 0;
        let totalCostYear = 0;
        let totalCo2Month = 0;
        let totalCo2Year = 0;

        const rows = deviceRowsTbody.querySelectorAll("tr");
        rows.forEach((tr) => {
          const powerInput = tr.querySelector(".device-power");
          const powerUnitSelect = tr.querySelector(".device-power-unit");
          const hoursInput = tr.querySelector(".device-hours");
          const daysInput = tr.querySelector(".device-days");
          const alwaysOnCheckbox = tr.querySelector(".device-always-on");

          let power = toNumber(powerInput.value);
          const powerUnit = powerUnitSelect.value;
          if (powerUnit === "kW") {
            power = power * 1000;
          }

          let hoursPerDay = toNumber(hoursInput.value);
          if (alwaysOnCheckbox.checked) {
            hoursPerDay = 24;
            hoursInput.value = 24;
          }

          const daysPerMonth = toNumber(daysInput.value) || 30;

          const kwhPerDay = (power * hoursPerDay) / 1000;
          const kwhPerMonth = kwhPerDay * daysPerMonth;
          const kwhPerYear = kwhPerDay * 365;

          const costPerMonth = kwhPerMonth * ratePerKwh;
          const costPerYear = kwhPerYear * ratePerKwh;

          const co2PerMonth = kwhPerMonth * co2Factor;
          const co2PerYear = kwhPerYear * co2Factor;

          totalKwhMonth += kwhPerMonth;
          totalKwhYear += kwhPerYear;
          totalCostMonth += costPerMonth;
          totalCostYear += costPerYear;
          totalCo2Month += co2PerMonth;
          totalCo2Year += co2PerYear;

          const kwhDayEl = tr.querySelector(".device-kwh-day");
          const kwhMonthEl = tr.querySelector(".device-kwh-month");
          const costMonthEl = tr.querySelector(".device-cost-month");
          const costYearEl = tr.querySelector(".device-cost-year");
          const co2MonthEl = tr.querySelector(".device-co2-month");
          const co2YearEl = tr.querySelector(".device-co2-year");

          if (kwhDayEl) kwhDayEl.textContent = formatKwh(kwhPerDay);
          if (kwhMonthEl)
            kwhMonthEl.textContent = formatKwh(kwhPerMonth) + " / month";
          if (costMonthEl)
            costMonthEl.textContent = formatCurrency(costPerMonth) + " / month";
          if (costYearEl)
            costYearEl.textContent = formatCurrency(costPerYear) + " / year";
          if (co2MonthEl)
            co2MonthEl.textContent = formatKg(co2PerMonth) + " / month";
          if (co2YearEl)
            co2YearEl.textContent = formatKg(co2PerYear) + " / year";
        });

        totalKwhEl.textContent = formatKwh(totalKwhMonth) + " / month";
        totalKwhYearEl.textContent = formatKwh(totalKwhYear) + " / year";
        totalCostEl.textContent = formatCurrency(totalCostMonth) + " / month";
        totalCostYearEl.textContent = formatCurrency(totalCostYear) + " / year";
        totalCo2El.textContent = formatKg(totalCo2Month) + " / month";
        totalCo2YearEl.textContent = formatKg(totalCo2Year) + " / year";
      }

      function loadPresets() {
        const presets = [
          {
            name: "Refrigerator (modern)",
            category: "Kitchen",
            power: 150,
            powerUnit: "W",
            hoursPerDay: 24,
            daysPerMonth: 30,
          },
          {
            name: "LED TV",
            category: "Entertainment",
            power: 80,
            powerUnit: "W",
            hoursPerDay: 4,
            daysPerMonth: 30,
          },
          {
            name: "Laptop",
            category: "Office",
            power: 60,
            powerUnit: "W",
            hoursPerDay: 8,
            daysPerMonth: 22,
          },
          {
            name: "Electric oven",
            category: "Kitchen",
            power: 2500,
            powerUnit: "W",
            hoursPerDay: 1,
            daysPerMonth: 20,
          },
          {
            name: "Air conditioner (room)",
            category: "Cooling",
            power: 1200,
            powerUnit: "W",
            hoursPerDay: 8,
            daysPerMonth: 30,
          },
          {
            name: "Wi‑Fi router",
            category: "Network",
            power: 10,
            powerUnit: "W",
            hoursPerDay: 24,
            daysPerMonth: 30,
          },
        ];

        deviceRowsTbody.innerHTML = "";
        presets.forEach((p) => createDeviceRow(p));
      }

      function resetCalculator() {
        deviceRowsTbody.innerHTML = "";
        electricityRateInput.value = 0.15;
        electricityRateUnitSelect.value = "per_kwh";
        co2FactorInput.value = 0.4;
        createDeviceRow();
        updateCalculations();
      }

      function exportToCsv() {
        const rows = deviceRowsTbody.querySelectorAll("tr");
        if (!rows.length) return;

        const headers = [
          "Device",
          "Power (W)",
          "Hours per day",
          "Days per month",
          "kWh per day",
          "kWh per month",
          "Cost per month",
          "Cost per year",
          "CO2 per month (kg)",
          "CO2 per year (kg)",
        ];

        const ratePerKwh = getElectricityRatePerKwh();
        const co2Factor = toNumber(co2FactorInput.value);

        const data = [headers];

        rows.forEach((tr) => {
          const name = tr.querySelector('input[type="text"]')?.value || "";
          let power = toNumber(tr.querySelector(".device-power")?.value);
          const powerUnit =
            tr.querySelector(".device-power-unit")?.value || "W";
          if (powerUnit === "kW") power *= 1000;
          const hoursPerDay = toNumber(
            tr.querySelector(".device-hours")?.value
          );
          const daysPerMonth =
            toNumber(tr.querySelector(".device-days")?.value) || 30;

          const kwhPerDay = (power * hoursPerDay) / 1000;
          const kwhPerMonth = kwhPerDay * daysPerMonth;
          const kwhPerYear = kwhPerDay * 365;
          const costPerMonth = kwhPerMonth * ratePerKwh;
          const costPerYear = kwhPerYear * ratePerKwh;
          const co2PerMonth = kwhPerMonth * co2Factor;
          const co2PerYear = kwhPerYear * co2Factor;

          data.push([
            name,
            power.toFixed(2),
            hoursPerDay.toFixed(2),
            daysPerMonth.toFixed(0),
            kwhPerDay.toFixed(3),
            kwhPerMonth.toFixed(3),
            costPerMonth.toFixed(2),
            costPerYear.toFixed(2),
            co2PerMonth.toFixed(3),
            co2PerYear.toFixed(3),
          ]);
        });

        const csvContent = data
          .map((row) =>
            row
              .map((field) => {
                const f = String(field).replace(/"/g, '""');
                return `"${f}"`;
              })
              .join(",")
          )
          .join("\n");

        const blob = new Blob([csvContent], {
          type: "text/csv;charset=utf-8;",
        });
        const url = URL.createObjectURL(blob);
        const a = document.createElement("a");
        a.href = url;
        a.download = "power-consumption-estimate.csv";
        document.body.appendChild(a);
        a.click();
        document.body.removeChild(a);
        URL.revokeObjectURL(url);
      }

      // Event bindings
      addDeviceBtn.addEventListener("click", () => createDeviceRow());
      loadPresetsBtn.addEventListener("click", loadPresets);
      resetBtn.addEventListener("click", resetCalculator);
      exportBtn.addEventListener("click", exportToCsv);

      electricityRateInput.addEventListener("input", updateCalculations);
      electricityRateUnitSelect.addEventListener("change", updateCalculations);
      co2FactorInput.addEventListener("input", updateCalculations);

      // Initialize with one empty row
      createDeviceRow();
    </script>

