    <script>
      // Batch reactor design JS
      function toSeconds(value, unit) {
        if (!isFinite(value)) return NaN;
        if (unit === "s") return value;
        if (unit === "min") return value * 60;
        if (unit === "h") return value * 3600;
        return value;
      }

      function fromSeconds(value, unit) {
        if (!isFinite(value)) return NaN;
        if (unit === "s") return value;
        if (unit === "min") return value / 60;
        if (unit === "h") return value / 3600;
        return value;
      }

      function computeTimeForX(n, k, CA0, X) {
        if (X <= 0) return 0;
        if (X >= 1) return Infinity;
        if (n === 0) {
          return (X * CA0) / k;
        } else if (n === 1) {
          return -Math.log(1 - X) / k;
        } else if (n === 2) {
          return X / (k * CA0 * (1 - X));
        }
        return NaN;
      }

      function computeXForTime(n, k, CA0, t) {
        if (t <= 0) return 0;
        if (n === 0) {
          const CA = CA0 - k * t;
          if (CA <= 0) return 1;
          return 1 - CA / CA0;
        } else if (n === 1) {
          const X = 1 - Math.exp(-k * t);
          return X > 1 ? 1 : X;
        } else if (n === 2) {
          const denom = 1 + k * CA0 * t;
          if (denom <= 0) return 1;
          const X = 1 - 1 / denom;
          return X > 1 ? 1 : X;
        }
        return NaN;
      }

      function formatNumber(val, digits = 4) {
        if (!isFinite(val)) return "—";
        if (
          Math.abs(val) >= 1e4 ||
          (Math.abs(val) > 0 && Math.abs(val) < 1e-3)
        ) {
          return val.toExponential(3);
        }
        return Number(val.toFixed(digits)).toString();
      }

      function handleCalculate() {
        const n = parseInt(document.getElementById("reaction-order").value, 10);
        const k = parseFloat(document.getElementById("k").value);
        const CA0 = parseFloat(document.getElementById("ca0").value);
        const Xtarget = parseFloat(document.getElementById("target-x").value);
        const tInput = parseFloat(document.getElementById("time").value);
        const tUnit = document.getElementById("time-units").value;
        const mode = document.getElementById("mode").value;

        const fa0Val = parseFloat(document.getElementById("fa0").value);
        const fa0Unit = document.getElementById("fa0-units").value;
        const Vval = parseFloat(document.getElementById("batch-volume").value);
        const Vunit = document.getElementById("volume-units").value;

        const resultsSummary = document.getElementById("results-summary");
        const keyOutputs = document.getElementById("key-outputs");
        const tableBody = document.getElementById("conversion-table-body");

        keyOutputs.innerHTML = "";
        tableBody.innerHTML = "";

        if (!isFinite(k) || k <= 0 || !isFinite(CA0) || CA0 <= 0) {
          resultsSummary.innerHTML =
            '<p class="text-red-600 text-sm">Please enter positive values for rate constant k and initial concentration C<sub>A0</sub>.</p>';
          return;
        }

        let tDesign_s, XDesign;

        if (mode === "time-for-X") {
          if (!isFinite(Xtarget) || Xtarget <= 0 || Xtarget >= 1) {
            resultsSummary.innerHTML =
              '<p class="text-red-600 text-sm">Target conversion X must be between 0 and 1 (e.g., 0.9).</p>';
            return;
          }
          tDesign_s = computeTimeForX(n, k, CA0, Xtarget);
          XDesign = Xtarget;
        } else {
          if (!isFinite(tInput) || tInput <= 0) {
            resultsSummary.innerHTML =
              '<p class="text-red-600 text-sm">Batch time t must be a positive number.</p>';
            return;
          }
          const t_s = toSeconds(tInput, tUnit);
          tDesign_s = t_s;
          XDesign = computeXForTime(n, k, CA0, t_s);
        }

        if (!isFinite(tDesign_s) || tDesign_s < 0) {
          resultsSummary.innerHTML =
            '<p class="text-red-600 text-sm">The specified conditions lead to an invalid design (check k, C<sub>A0</sub>, and X).</p>';
          return;
        }

        const tDesign_display = fromSeconds(tDesign_s, tUnit);
        const CA_design = CA0 * (1 - XDesign);

        // Volume estimate if F_A0 provided
        let V_est = NaN;
        let V_est_unit = "L";
        if (isFinite(fa0Val) && fa0Val > 0 && XDesign > 0) {
          let FA0_s = fa0Val;
          if (fa0Unit === "mol/h") FA0_s = fa0Val / 3600;
          if (fa0Unit === "kmol/h") FA0_s = (fa0Val * 1000) / 3600;
          const V_L = (FA0_s * tDesign_s) / (CA0 * XDesign);
          V_est = V_L;
          V_est_unit = "L";
        }

        // Moles per batch if V provided
        let N_A0_batch = NaN;
        if (isFinite(Vval) && Vval > 0) {
          let V_L = Vval;
          if (Vunit === "m3") V_L = Vval * 1000;
          N_A0_batch = CA0 * V_L;
        }

        // Summary
        const modeText =
          mode === "time-for-X"
            ? `Required batch time to reach X = ${formatNumber(
                XDesign,
                3
              )} is <strong>${formatNumber(
                tDesign_display,
                3
              )} ${tUnit}</strong>.`
            : `Conversion at t = ${formatNumber(
                tInput,
                3
              )} ${tUnit} is <strong>X = ${formatNumber(
                XDesign,
                3
              )}</strong> (${formatNumber(XDesign * 100, 2)}%).`;

        resultsSummary.innerHTML = `
      <p class="text-sm mb-1">${modeText}</p>
      <p class="text-xs text-gray-600">Assumptions: isothermal, constant volume, perfectly mixed batch reactor with a single irreversible reaction A → products.</p>
    `;

        // Key outputs list
        const items = [];
        items.push(
          `<div class="flex justify-between"><dt>Reaction order n</dt><dd>${n}</dd></div>`
        );
        items.push(
          `<div class="flex justify-between"><dt>Rate constant k</dt><dd>${formatNumber(
            k,
            4
          )}</dd></div>`
        );
        items.push(
          `<div class="flex justify-between"><dt>Initial concentration C<sub>A0</sub></dt><dd>${formatNumber(
            CA0,
            4
          )}</dd></div>`
        );
        items.push(
          `<div class="flex justify-between"><dt>Design time t</dt><dd>${formatNumber(
            tDesign_display,
            4
          )} ${tUnit}</dd></div>`
        );
        items.push(
          `<div class="flex justify-between"><dt>Design conversion X</dt><dd>${formatNumber(
            XDesign,
            4
          )} (${formatNumber(XDesign * 100, 2)}%)</dd></div>`
        );
        items.push(
          `<div class="flex justify-between"><dt>Outlet concentration C<sub>A</sub></dt><dd>${formatNumber(
            CA_design,
            4
          )}</dd></div>`
        );

        if (isFinite(V_est) && V_est > 0) {
          items.push(
            `<div class="flex justify-between"><dt>Estimated reactor volume V</dt><dd>${formatNumber(
              V_est,
              3
            )} ${V_est_unit}</dd></div>`
          );
        }
        if (isFinite(N_A0_batch) && N_A0_batch > 0) {
          items.push(
            `<div class="flex justify-between"><dt>Moles of A per batch N<sub>A0</sub></dt><dd>${formatNumber(
              N_A0_batch,
              3
            )} mol</dd></div>`
          );
        }

        keyOutputs.innerHTML = items.map((x) => `<div>${x}</div>`).join("");

        // Conversion vs time table (0 to tDesign or up to some multiple)
        const rows = [];
        const nPoints = 8;
        let tMax_s = tDesign_s;
        if (!isFinite(tMax_s) || tMax_s <= 0) {
          tMax_s = toSeconds(tInput, tUnit);
        }
        if (!isFinite(tMax_s) || tMax_s <= 0) tMax_s = 1;

        for (let i = 0; i <= nPoints; i++) {
          const frac = i / nPoints;
          const t_s = frac * tMax_s;
          const t_disp = fromSeconds(t_s, tUnit);
          const X = computeXForTime(n, k, CA0, t_s);
          const CA = CA0 * (1 - X);
          rows.push(`
        <tr class="${i % 2 === 0 ? "bg-white" : "bg-gray-50"}">
          <td class="px-2 py-1">${formatNumber(t_disp, 3)} ${tUnit}</td>
          <td class="px-2 py-1">${formatNumber(X, 4)}</td>
          <td class="px-2 py-1">${formatNumber(CA, 4)}</td>
        </tr>
      `);
        }
        tableBody.innerHTML = rows.join("");
      }

      document
        .getElementById("calculate-btn")
        .addEventListener("click", handleCalculate);
      document.getElementById("reset-btn").addEventListener("click", () => {
        document.getElementById("batch-form").reset();
        document.getElementById("reaction-order").value = "1";
        document.getElementById("k").value = "0.2";
        document.getElementById("k-units").value = "1/s";
        document.getElementById("ca0").value = "1.0";
        document.getElementById("ca-units").value = "mol/L";
        document.getElementById("target-x").value = "0.9";
        document.getElementById("time").value = "10";
        document.getElementById("time-units").value = "s";
        document.getElementById("mode").value = "time-for-X";
        document.getElementById("fa0").value = "";
        document.getElementById("batch-volume").value = "";
        document.getElementById("results-summary").innerHTML =
          '<p class="text-gray-500 text-sm">Enter your data and click “Calculate” to see batch reactor design results.</p>';
        document.getElementById("key-outputs").innerHTML = "";
        document.getElementById("conversion-table-body").innerHTML = "";
      });

      // Adjust k-units dropdown when reaction order changes (for user guidance only)
      document
        .getElementById("reaction-order")
        .addEventListener("change", (e) => {
          const n = parseInt(e.target.value, 10);
          const kUnits = document.getElementById("k-units");
          if (n === 0) kUnits.value = "molL-1s-1";
          if (n === 1) kUnits.value = "1/s";
          if (n === 2) kUnits.value = "Lmol-1s-1";
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
