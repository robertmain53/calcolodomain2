    <script>
      // --- Utility functions ---
      function clamp(value, min, max) {
        return Math.min(max, Math.max(min, value));
      }

      function hexToRgb(hex) {
        hex = hex.replace("#", "").trim();
        if (hex.length === 3) {
          hex = hex
            .split("")
            .map((c) => c + c)
            .join("");
        }
        if (hex.length !== 6) return null;
        const num = parseInt(hex, 16);
        if (Number.isNaN(num)) return null;
        return {
          r: (num >> 16) & 255,
          g: (num >> 8) & 255,
          b: num & 255,
        };
      }

      function rgbToHex(r, g, b) {
        const toHex = (c) => {
          const v = clamp(Math.round(c), 0, 255);
          return v.toString(16).padStart(2, "0").toUpperCase();
        };
        return toHex(r) + toHex(g) + toHex(b);
      }

      function approxDeltaE(rgb1, rgb2) {
        // Simple Euclidean distance in RGB space, scaled
        const dr = rgb1.r - rgb2.r;
        const dg = rgb1.g - rgb2.g;
        const db = rgb1.b - rgb2.b;
        const dist = Math.sqrt(dr * dr + dg * dg + db * db);
        return dist.toFixed(1);
      }

      function convertVolume(value, fromUnit, toUnit) {
        const factorsToMl = {
          ml: 1,
          l: 1000,
          "fl-oz": 29.5735,
          gal: 3785.41,
        };
        if (!factorsToMl[fromUnit] || !factorsToMl[toUnit]) return value;
        const ml = value * factorsToMl[fromUnit];
        return ml / factorsToMl[toUnit];
      }

      // --- Ratio & Volume Mode ---
      const ratioRowsContainer = document.getElementById("ratio-rows");
      const ratioColorCountEl = document.getElementById("ratio-color-count");
      const ratioTotalVolumeEl = document.getElementById("ratio-total-volume");
      const ratioVolumeUnitEl = document.getElementById("ratio-volume-unit");
      const ratioWasteEl = document.getElementById("ratio-waste");
      const ratioTotalPartsEl = document.getElementById("ratio-total-parts");
      const ratioTotalWithWasteEl = document.getElementById(
        "ratio-total-with-waste"
      );
      const ratioTotalWithWasteUnitEl = document.getElementById(
        "ratio-total-with-waste-unit"
      );
      const ratioCopyRecipeBtn = document.getElementById("ratio-copy-recipe");

      const defaultRatioColors = [
        { name: "Color 1", hex: "#FFFFFF", parts: 2 },
        { name: "Color 2", hex: "#0000FF", parts: 1 },
        { name: "Color 3", hex: "#000000", parts: 0.5 },
        { name: "Color 4", hex: "#FF0000", parts: 0 },
        { name: "Color 5", hex: "#00FF00", parts: 0 },
        { name: "Color 6", hex: "#FFFF00", parts: 0 },
      ];

      function renderRatioRows() {
        const count = parseInt(ratioColorCountEl.value, 10);
        ratioRowsContainer.innerHTML = "";
        for (let i = 0; i < count; i++) {
          const c = defaultRatioColors[i] || {
            name: "Color " + (i + 1),
            hex: "#FFFFFF",
            parts: 0,
          };
          const row = document.createElement("tr");
          row.innerHTML = `
        <td class="px-3 py-2">
          <input type="color" value="${c.hex}" class="ratio-color-picker w-10 h-8 border border-gray-200 rounded cursor-pointer" data-index="${i}">
        </td>
        <td class="px-3 py-2">
          <input type="text" value="${c.name}" class="ratio-name w-full border-gray-200 rounded px-2 py-1 text-xs focus:ring-blue-500 focus:border-blue-500" data-index="${i}">
        </td>
        <td class="px-3 py-2">
          <input type="number" min="0" step="0.01" value="${c.parts}" class="ratio-parts w-24 border-gray-200 rounded px-2 py-1 text-xs focus:ring-blue-500 focus:border-blue-500" data-index="${i}">
        </td>
        <td class="px-3 py-2 text-xs text-gray-700">
          <div>Volume: <span class="ratio-volume font-semibold">0</span> <span class="ratio-volume-unit"></span></div>
          <div>Percent: <span class="ratio-percent font-semibold">0%</span></div>
        </td>
      `;
          ratioRowsContainer.appendChild(row);
        }
        updateRatioCalculations();
      }

      function updateRatioCalculations() {
        const rows = ratioRowsContainer.querySelectorAll("tr");
        let totalParts = 0;
        const partsArr = [];
        rows.forEach((row) => {
          const partsInput = row.querySelector(".ratio-parts");
          const parts = parseFloat(partsInput.value) || 0;
          partsArr.push(parts);
          totalParts += parts;
        });
        ratioTotalPartsEl.textContent = totalParts.toFixed(2);

        const totalVolume = parseFloat(ratioTotalVolumeEl.value) || 0;
        const waste = clamp(parseFloat(ratioWasteEl.value) || 0, 0, 100);
        ratioWasteEl.value = waste;
        const totalWithWaste = totalVolume * (1 + waste / 100);
        ratioTotalWithWasteEl.textContent = totalWithWaste.toFixed(1);
        ratioTotalWithWasteUnitEl.textContent = ratioVolumeUnitEl.value;

        rows.forEach((row, idx) => {
          const parts = partsArr[idx];
          const volumeSpan = row.querySelector(".ratio-volume");
          const volumeUnitSpan = row.querySelector(".ratio-volume-unit");
          const percentSpan = row.querySelector(".ratio-percent");

          if (totalParts <= 0 || parts <= 0) {
            volumeSpan.textContent = "0";
            volumeUnitSpan.textContent = ratioVolumeUnitEl.value;
            percentSpan.textContent = "0%";
            return;
          }
          const fraction = parts / totalParts;
          const vol = fraction * totalWithWaste;
          volumeSpan.textContent = vol.toFixed(1);
          volumeUnitSpan.textContent = ratioVolumeUnitEl.value;
          percentSpan.textContent = (fraction * 100).toFixed(1) + "%";
        });
      }

      ratioColorCountEl.addEventListener("change", renderRatioRows);
      ratioTotalVolumeEl.addEventListener("input", updateRatioCalculations);
      ratioVolumeUnitEl.addEventListener("change", updateRatioCalculations);
      ratioWasteEl.addEventListener("input", updateRatioCalculations);
      ratioRowsContainer.addEventListener("input", (e) => {
        if (
          e.target.classList.contains("ratio-parts") ||
          e.target.classList.contains("ratio-name") ||
          e.target.classList.contains("ratio-color-picker")
        ) {
          updateRatioCalculations();
        }
      });

      ratioCopyRecipeBtn.addEventListener("click", () => {
        const rows = ratioRowsContainer.querySelectorAll("tr");
        const lines = [];
        const totalParts = parseFloat(ratioTotalPartsEl.textContent) || 0;
        const totalVolume = parseFloat(ratioTotalWithWasteEl.textContent) || 0;
        const unit =
          ratioTotalWithWasteUnitEl.textContent || ratioVolumeUnitEl.value;

        lines.push(
          `Paint mixing recipe (${totalVolume.toFixed(
            1
          )} ${unit} total including waste):`
        );
        rows.forEach((row) => {
          const name = row.querySelector(".ratio-name").value || "Color";
          const color = row.querySelector(".ratio-color-picker").value;
          const parts =
            parseFloat(row.querySelector(".ratio-parts").value) || 0;
          const volume = row.querySelector(".ratio-volume").textContent;
          const percent = row.querySelector(".ratio-percent").textContent;
          if (parts > 0) {
            lines.push(
              `- ${name} (${color}): ${parts} parts → ${volume} ${unit} (${percent})`
            );
          }
        });
        lines.push(`Total parts: ${totalParts.toFixed(2)}`);
        const text = lines.join("\n");
        navigator.clipboard
          .writeText(text)
          .then(() => {
            ratioCopyRecipeBtn.textContent = "Copied!";
            setTimeout(
              () => (ratioCopyRecipeBtn.textContent = "Copy recipe"),
              1500
            );
          })
          .catch(() => {
            alert("Recipe copied:\n\n" + text);
          });
      });

      // --- Color Match Mode ---
      const targetHexInput = document.getElementById("target-hex");
      const targetRInput = document.getElementById("target-r");
      const targetGInput = document.getElementById("target-g");
      const targetBInput = document.getElementById("target-b");
      const targetPreview = document.getElementById("target-preview");
      const targetHexDisplay = document.getElementById("target-hex-display");
      const targetRgbDisplay = document.getElementById("target-rgb-display");
      const mixPreview = document.getElementById("mix-preview");
      const mixHexDisplay = document.getElementById("mix-hex-display");
      const mixRgbDisplay = document.getElementById("mix-rgb-display");
      const mixDeltaEDisplay = document.getElementById("mix-deltae-display");
      const colorMixRows = document.getElementById("color-mix-rows");
      const colorSuggestBtn = document.getElementById("color-suggest-mix");
      const colorCopyRecipeBtn = document.getElementById("color-copy-recipe");

      const basePaints = [
        { name: "Titanium White", hex: "#FFFFFF" },
        { name: "Primary Yellow", hex: "#FFD400" },
        { name: "Primary Magenta", hex: "#FF007F" },
        { name: "Primary Cyan", hex: "#00A0FF" },
        { name: "Burnt Umber", hex: "#5A3A1A" },
        { name: "Ivory Black", hex: "#111111" },
      ];

      function renderColorMixRows() {
        colorMixRows.innerHTML = "";
        basePaints.forEach((p, idx) => {
          const defaultParts = idx === 0 ? 4 : idx === 3 ? 1 : 0; // start with white + a bit of cyan
          const row = document.createElement("tr");
          row.innerHTML = `
        <td class="px-3 py-2">
          <div class="w-8 h-6 rounded border border-gray-200" style="background:${p.hex};"></div>
        </td>
        <td class="px-3 py-2 text-xs text-gray-800">${p.name}</td>
        <td class="px-3 py-2 text-xs font-mono text-gray-700">${p.hex}</td>
        <td class="px-3 py-2">
          <input type="number" min="0" step="0.1" value="${defaultParts}" class="color-parts w-20 border-gray-200 rounded px-2 py-1 text-xs focus:ring-blue-500 focus:border-blue-500" data-index="${idx}">
        </td>
        <td class="px-3 py-2 text-xs text-gray-700">
          <span class="color-percent">0%</span>
        </td>
      `;
          colorMixRows.appendChild(row);
        });
        updateColorMix();
      }

      function getTargetRgb() {
        const r = clamp(parseInt(targetRInput.value, 10) || 0, 0, 255);
        const g = clamp(parseInt(targetGInput.value, 10) || 0, 0, 255);
        const b = clamp(parseInt(targetBInput.value, 10) || 0, 0, 255);
        return { r, g, b };
      }

      function updateTargetFromHex() {
        const rgb = hexToRgb(targetHexInput.value);
        if (!rgb) return;
        targetRInput.value = rgb.r;
        targetGInput.value = rgb.g;
        targetBInput.value = rgb.b;
        updateTargetPreview();
      }

      function updateTargetFromRgb() {
        const rgb = getTargetRgb();
        const hex = rgbToHex(rgb.r, rgb.g, rgb.b);
        targetHexInput.value = hex;
        updateTargetPreview();
      }

      function updateTargetPreview() {
        const rgb = getTargetRgb();
        const hex = rgbToHex(rgb.r, rgb.g, rgb.b);
        targetPreview.style.background = "#" + hex;
        targetHexDisplay.textContent = "#" + hex;
        targetRgbDisplay.textContent = `${rgb.r}, ${rgb.g}, ${rgb.b}`;
        updateColorMix();
      }

      function updateColorMix() {
        const rows = colorMixRows.querySelectorAll("tr");
        let totalParts = 0;
        const partsArr = [];
        rows.forEach((row) => {
          const partsInput = row.querySelector(".color-parts");
          const parts = parseFloat(partsInput.value) || 0;
          partsArr.push(parts);
          totalParts += parts;
        });

        let mixR = 0,
          mixG = 0,
          mixB = 0;
        rows.forEach((row, idx) => {
          const parts = partsArr[idx];
          const percentSpan = row.querySelector(".color-percent");
          if (totalParts > 0 && parts > 0) {
            const fraction = parts / totalParts;
            percentSpan.textContent = (fraction * 100).toFixed(1) + "%";
            const rgb = hexToRgb(basePaints[idx].hex);
            mixR += rgb.r * fraction;
            mixG += rgb.g * fraction;
            mixB += rgb.b * fraction;
          } else {
            percentSpan.textContent = "0%";
          }
        });

        if (totalParts <= 0) {
          mixPreview.style.background = "#E5E7EB";
          mixHexDisplay.textContent = "–";
          mixRgbDisplay.textContent = "–";
          mixDeltaEDisplay.textContent = "–";
          return;
        }

        const mixRgb = { r: mixR, g: mixG, b: mixB };
        const mixHex = rgbToHex(mixRgb.r, mixRgb.g, mixRgb.b);
        mixPreview.style.background = "#" + mixHex;
        mixHexDisplay.textContent = "#" + mixHex;
        mixRgbDisplay.textContent = `${Math.round(mixRgb.r)}, ${Math.round(
          mixRgb.g
        )}, ${Math.round(mixRgb.b)}`;

        const targetRgb = getTargetRgb();
        mixDeltaEDisplay.textContent = approxDeltaE(targetRgb, mixRgb);
      }

      targetHexInput.addEventListener("input", () => {
        if (targetHexInput.value.length >= 3) {
          updateTargetFromHex();
        }
      });
      [targetRInput, targetGInput, targetBInput].forEach((el) => {
        el.addEventListener("input", updateTargetFromRgb);
      });

      colorMixRows.addEventListener("input", (e) => {
        if (e.target.classList.contains("color-parts")) {
          updateColorMix();
        }
      });

      colorSuggestBtn.addEventListener("click", () => {
        // Simple heuristic: derive CMY from target RGB and map to base paints
        const t = getTargetRgb();
        const c = 1 - t.r / 255;
        const m = 1 - t.g / 255;
        const y = 1 - t.b / 255;

        const rows = colorMixRows.querySelectorAll("tr");
        const whiteParts = 4;
        const scale = 4; // scale CMY contributions
        rows.forEach((row, idx) => {
          const input = row.querySelector(".color-parts");
          let val = 0;
          if (basePaints[idx].name === "Titanium White") val = whiteParts;
          else if (basePaints[idx].name === "Primary Cyan")
            val = Math.max(0, c * scale);
          else if (basePaints[idx].name === "Primary Magenta")
            val = Math.max(0, m * scale);
          else if (basePaints[idx].name === "Primary Yellow")
            val = Math.max(0, y * scale);
          else val = 0;
          input.value = val.toFixed(2);
        });
        updateColorMix();
      });

      colorCopyRecipeBtn.addEventListener("click", () => {
        const rows = colorMixRows.querySelectorAll("tr");
        const lines = [];
        const targetRgb = getTargetRgb();
        const targetHex = rgbToHex(targetRgb.r, targetRgb.g, targetRgb.b);
        lines.push(
          `Color match recipe for target #${targetHex} (RGB ${targetRgb.r}, ${targetRgb.g}, ${targetRgb.b}):`
        );
        rows.forEach((row, idx) => {
          const parts =
            parseFloat(row.querySelector(".color-parts").value) || 0;
          const percent = row.querySelector(".color-percent").textContent;
          if (parts > 0) {
            lines.push(
              `- ${basePaints[idx].name} (${basePaints[idx].hex}): ${parts} parts (${percent})`
            );
          }
        });
        const mixHex = mixHexDisplay.textContent;
        const mixRgb = mixRgbDisplay.textContent;
        const deltaE = mixDeltaEDisplay.textContent;
        lines.push(
          `Approximate mixed color: ${mixHex} (RGB ${mixRgb}), ΔE ≈ ${deltaE}`
        );
        const text = lines.join("\n");
        navigator.clipboard
          .writeText(text)
          .then(() => {
            colorCopyRecipeBtn.textContent = "Copied!";
            setTimeout(
              () => (colorCopyRecipeBtn.textContent = "Copy color mix recipe"),
              1500
            );
          })
          .catch(() => {
            alert("Recipe copied:\n\n" + text);
          });
      });

      // --- Coverage Mode ---
      const covWidthEl = document.getElementById("cov-width");
      const covHeightEl = document.getElementById("cov-height");
      const covUnitEl = document.getElementById("cov-unit");
      const covCoverageEl = document.getElementById("cov-coverage");
      const covCoverageUnitEl = document.getElementById("cov-coverage-unit");
      const covCoatsEl = document.getElementById("cov-coats");
      const covAreaEl = document.getElementById("cov-area");
      const covAreaUnitEl = document.getElementById("cov-area-unit");
      const covCalcBtn = document.getElementById("cov-calc");
      const covVolumeMainEl = document.getElementById("cov-volume-main");
      const covVolumeMainUnitEl = document.getElementById(
        "cov-volume-main-unit"
      );

      function updateCoverage() {
        const w = parseFloat(covWidthEl.value) || 0;
        const h = parseFloat(covHeightEl.value) || 0;
        const coats = Math.max(1, parseInt(covCoatsEl.value, 10) || 1);
        covCoatsEl.value = coats;
        const unit = covUnitEl.value;

        let area = w * h;
        if (unit === "ft") {
          covAreaUnitEl.textContent = "ft²";
        } else {
          covAreaUnitEl.textContent = "m²";
        }
        covAreaEl.textContent = area.toFixed(2);

        const coverage = parseFloat(covCoverageEl.value) || 1;
        const covUnit = covCoverageUnitEl.value;

        // Convert area and coverage to a common system (m² & L)
        let areaM2 = area;
        if (unit === "ft") {
          areaM2 = area * 0.092903;
        }
        let covM2PerL = coverage;
        if (covUnit === "ft2-gal") {
          const areaPerL = (coverage * 0.092903) / 3.78541;
          covM2PerL = areaPerL;
        }

        const totalAreaAllCoats = areaM2 * coats;
        let liters = totalAreaAllCoats / covM2PerL;
        liters *= 1.1; // 10% waste

        if (liters >= 3.78541) {
          const gallons = liters / 3.78541;
          covVolumeMainEl.textContent = gallons.toFixed(2);
          covVolumeMainUnitEl.textContent = "gal";
        } else {
          covVolumeMainEl.textContent = liters.toFixed(2);
          covVolumeMainUnitEl.textContent = "L";
        }
      }

      covCalcBtn.addEventListener("click", updateCoverage);
      [
        covWidthEl,
        covHeightEl,
        covUnitEl,
        covCoverageEl,
        covCoverageUnitEl,
        covCoatsEl,
      ].forEach((el) => {
        el.addEventListener("input", () => {
          // live preview of area
          const w = parseFloat(covWidthEl.value) || 0;
          const h = parseFloat(covHeightEl.value) || 0;
          let area = w * h;
          if (covUnitEl.value === "ft") {
            covAreaUnitEl.textContent = "ft²";
          } else {
            covAreaUnitEl.textContent = "m²";
          }
          covAreaEl.textContent = area.toFixed(2);
        });
      });

      // --- Preset Swatches ---
      const presetSwatchesContainer =
        document.getElementById("preset-swatches");
      const presets = [
        {
          name: "Warm Light Gray",
          hex: "#D9D3CC",
          recipe: [
            { name: "White", hex: "#FFFFFF", parts: 8 },
            { name: "Black", hex: "#000000", parts: 0.5 },
            { name: "Yellow Ochre", hex: "#C99700", parts: 0.5 },
          ],
        },
        {
          name: "Soft Sky Blue",
          hex: "#AFCBEF",
          recipe: [
            { name: "White", hex: "#FFFFFF", parts: 6 },
            { name: "Primary Cyan", hex: "#00A0FF", parts: 1 },
            { name: "Primary Magenta", hex: "#FF007F", parts: 0.2 },
          ],
        },
        {
          name: "Muted Sage Green",
          hex: "#A6B89C",
          recipe: [
            { name: "White", hex: "#FFFFFF", parts: 5 },
            { name: "Primary Yellow", hex: "#FFD400", parts: 1 },
            { name: "Primary Cyan", hex: "#00A0FF", parts: 0.8 },
            { name: "Burnt Umber", hex: "#5A3A1A", parts: 0.3 },
          ],
        },
        {
          name: "Dusty Rose",
          hex: "#D9A0A8",
          recipe: [
            { name: "White", hex: "#FFFFFF", parts: 6 },
            { name: "Primary Magenta", hex: "#FF007F", parts: 1 },
            { name: "Burnt Umber", hex: "#5A3A1A", parts: 0.3 },
          ],
        },
        {
          name: "Deep Navy",
          hex: "#1E2A3A",
          recipe: [
            { name: "Primary Cyan", hex: "#00A0FF", parts: 3 },
            { name: "Primary Magenta", hex: "#FF007F", parts: 1 },
            { name: "Black", hex: "#111111", parts: 1 },
          ],
        },
        {
          name: "Creamy Off‑White",
          hex: "#F5E9D6",
          recipe: [
            { name: "White", hex: "#FFFFFF", parts: 10 },
            { name: "Yellow Ochre", hex: "#C99700", parts: 0.3 },
            { name: "Burnt Umber", hex: "#5A3A1A", parts: 0.1 },
          ],
        },
      ];

      function renderPresets() {
        presets.forEach((p, idx) => {
          const tile = document.createElement("button");
          tile.type = "button";
          tile.className = "swatch-tile text-left";
          tile.dataset.index = idx;
          tile.innerHTML = `
        <div class="swatch-color" style="background:${p.hex};"></div>
        <div class="swatch-label">
          <div class="font-medium text-xs text-gray-800 truncate">${p.name}</div>
          <div class="flex items-center justify-between mt-0.5">
            <span class="font-mono text-[10px] text-gray-500">${p.hex}</span>
            <span class="ratio-pill">${p.recipe.length} colors</span>
          </div>
        </div>
      `;
          presetSwatchesContainer.appendChild(tile);
        });

        presetSwatchesContainer.addEventListener("click", (e) => {
          const tile = e.target.closest(".swatch-tile");
          if (!tile) return;
          const idx = parseInt(tile.dataset.index, 10);
          const preset = presets[idx];
          applyPresetToRatioMode(preset);
        });
      }

      function applyPresetToRatioMode(preset) {
        const count = preset.recipe.length;
        ratioColorCountEl.value = count;
        renderRatioRows();
        const rows = ratioRowsContainer.querySelectorAll("tr");
        rows.forEach((row, idx) => {
          const r = preset.recipe[idx];
          const nameInput = row.querySelector(".ratio-name");
          const colorInput = row.querySelector(".ratio-color-picker");
          const partsInput = row.querySelector(".ratio-parts");
          nameInput.value = r.name;
          colorInput.value = r.hex;
          partsInput.value = r.parts;
        });
        updateRatioCalculations();
        // Switch to ratio mode tab if not already
        setActiveMode("ratio");
        window.scrollTo({
          top: document.querySelector("#paint-mixing-app").offsetTop - 80,
          behavior: "smooth",
        });
      }

      // --- Mode Tabs ---
      const modeTabs = document.querySelectorAll(".mode-tab");
      const modePanels = {
        ratio: document.getElementById("mode-ratio"),
        color: document.getElementById("mode-color"),
        coverage: document.getElementById("mode-coverage"),
      };

      function setActiveMode(mode) {
        modeTabs.forEach((tab) => {
          const isActive = tab.dataset.mode === mode;
          tab.classList.toggle("border-blue-600", isActive);
          tab.classList.toggle("text-blue-600", isActive);
          tab.classList.toggle("border-transparent", !isActive);
          tab.classList.toggle("text-gray-600", !isActive);
        });
        Object.keys(modePanels).forEach((key) => {
          modePanels[key].classList.toggle("hidden", key !== mode);
        });
      }

      modeTabs.forEach((tab) => {
        tab.addEventListener("click", () => {
          setActiveMode(tab.dataset.mode);
        });
      });

      // --- Init ---
      document.addEventListener("DOMContentLoaded", () => {
        renderRatioRows();
        renderColorMixRows();
        renderPresets();
        updateTargetFromHex();
        updateCoverage();
      });

      // Ensure init runs even if DOMContentLoaded already fired
      if (
        document.readyState === "interactive" ||
        document.readyState === "complete"
      ) {
        renderRatioRows();
        renderColorMixRows();
        renderPresets();
        updateTargetFromHex();
        updateCoverage();
      }

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
