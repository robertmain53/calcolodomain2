    <script>
      // Mode switching
      const modeButtons = {
        hts: document.getElementById("mode-hts"),
        k: document.getElementById("mode-k"),
        dg0q: document.getElementById("mode-dg0q"),
      };
      const panels = {
        hts: document.getElementById("panel-hts"),
        k: document.getElementById("panel-k"),
        dg0q: document.getElementById("panel-dg0q"),
      };
      let currentMode = "hts";

      function setMode(mode) {
        currentMode = mode;
        // Toggle button styles
        Object.keys(modeButtons).forEach((key) => {
          if (key === mode) {
            modeButtons[key].classList.add("toggle-btn-active");
            modeButtons[key].classList.remove("text-gray-700");
          } else {
            modeButtons[key].classList.remove("toggle-btn-active");
            modeButtons[key].classList.add("text-gray-700");
          }
        });
        // Toggle panels
        Object.keys(panels).forEach((key) => {
          panels[key].classList.toggle("hidden", key !== mode);
        });
        // Clear results and errors when switching
        document.getElementById("results").classList.add("hidden");
        document.getElementById("errorMsg").textContent = "";
      }

      modeButtons.hts.addEventListener("click", () => setMode("hts"));
      modeButtons.k.addEventListener("click", () => setMode("k"));
      modeButtons.dg0q.addEventListener("click", () => setMode("dg0q"));

      // Calculation logic
      const R = 8.314; // J/(mol·K)

      function toKelvin(value, unit) {
        if (unit === "K") return value;
        return value + 273.15;
      }

      function formatNumber(val, decimals = 2) {
        if (!isFinite(val)) return "—";
        if (Math.abs(val) >= 1e4 || (Math.abs(val) <= 1e-3 && val !== 0)) {
          return val.toExponential(decimals);
        }
        return val.toFixed(decimals);
      }

      function evaluateSpontaneity(deltaGkJ) {
        const eps = 1e-6;
        if (deltaGkJ < -eps) {
          return {
            label: "Spontaneous (ΔG < 0)",
            comment:
              "The forward reaction is thermodynamically favorable at this temperature.",
          };
        } else if (deltaGkJ > eps) {
          return {
            label: "Non‑spontaneous (ΔG > 0)",
            comment:
              "The forward reaction is not thermodynamically favorable; the reverse direction is favored.",
          };
        } else {
          return {
            label: "At equilibrium (ΔG ≈ 0)",
            comment:
              "The system is at equilibrium with no net driving force in either direction.",
          };
        }
      }

      document.getElementById("calculateBtn").addEventListener("click", () => {
        const errorEl = document.getElementById("errorMsg");
        const resultsEl = document.getElementById("results");
        const resultDeltaG = document.getElementById("resultDeltaG");
        const resultDeltaGAlt = document.getElementById("resultDeltaGAlt");
        const resultSpontaneity = document.getElementById("resultSpontaneity");
        const resultComment = document.getElementById("resultComment");
        const resultExtra = document.getElementById("resultExtra");

        errorEl.textContent = "";
        resultsEl.classList.add("hidden");
        resultExtra.textContent = "";

        try {
          let deltaG_kJ = null;
          let extraHtml = "";

          if (currentMode === "hts") {
            const dH = parseFloat(document.getElementById("deltaH").value);
            const dHUnit = document.getElementById("deltaHUnit").value;
            const dS = parseFloat(document.getElementById("deltaS").value);
            const dSUnit = document.getElementById("deltaSUnit").value;
            const T = parseFloat(document.getElementById("temperature").value);
            const TUnit = document.getElementById("temperatureUnit").value;

            if (isNaN(dH) || isNaN(dS) || isNaN(T)) {
              throw new Error("Please enter values for ΔH, ΔS, and T.");
            }

            let T_K = toKelvin(T, TUnit);
            if (T_K <= 0)
              throw new Error("Temperature in kelvin must be greater than 0.");

            // Convert ΔH to kJ/mol
            let dH_kJmol = dH;
            if (dHUnit === "Jmol") dH_kJmol = dH / 1000;

            // Convert ΔS to kJ/(mol·K)
            let dS_kJmolK = dS;
            if (dSUnit === "JmolK") dS_kJmolK = dS / 1000;

            deltaG_kJ = dH_kJmol - T_K * dS_kJmolK;

            extraHtml = `<p>Using ΔG = ΔH − TΔS with T = ${formatNumber(
              T_K,
              2
            )} K, ΔH = ${formatNumber(
              dH_kJmol,
              3
            )} kJ/mol, and ΔS = ${formatNumber(dS_kJmolK, 4)} kJ/(mol·K).</p>`;
          } else if (currentMode === "k") {
            const K = parseFloat(document.getElementById("equilibriumK").value);
            const T = parseFloat(document.getElementById("temperatureK").value);
            const TUnit = document.getElementById("temperatureKUnit").value;

            if (isNaN(K) || isNaN(T)) {
              throw new Error("Please enter values for K and T.");
            }
            if (K <= 0)
              throw new Error("Equilibrium constant K must be greater than 0.");

            let T_K = toKelvin(T, TUnit);
            if (T_K <= 0)
              throw new Error("Temperature in kelvin must be greater than 0.");

            const deltaG_Jmol = -R * T_K * Math.log(K);
            deltaG_kJ = deltaG_Jmol / 1000;

            extraHtml = `<p>Using ΔG° = −RT ln K with R = 8.314 J/(mol·K), T = ${formatNumber(
              T_K,
              2
            )} K, and K = ${formatNumber(K, 3)}.</p>`;
          } else if (currentMode === "dg0q") {
            const dG0 = parseFloat(document.getElementById("deltaG0").value);
            const dG0Unit = document.getElementById("deltaG0Unit").value;
            const Q = parseFloat(document.getElementById("reactionQ").value);
            const T = parseFloat(document.getElementById("temperatureQ").value);
            const TUnit = document.getElementById("temperatureQUnit").value;

            if (isNaN(dG0) || isNaN(Q) || isNaN(T)) {
              throw new Error("Please enter values for ΔG°, Q, and T.");
            }
            if (Q <= 0)
              throw new Error("Reaction quotient Q must be greater than 0.");

            let T_K = toKelvin(T, TUnit);
            if (T_K <= 0)
              throw new Error("Temperature in kelvin must be greater than 0.");

            // Convert ΔG° to J/mol
            let dG0_Jmol = dG0;
            if (dG0Unit === "kJmol") dG0_Jmol = dG0 * 1000;

            const deltaG_Jmol = dG0_Jmol + R * T_K * Math.log(Q);
            deltaG_kJ = deltaG_Jmol / 1000;

            extraHtml = `<p>Using ΔG = ΔG° + RT ln Q with R = 8.314 J/(mol·K), T = ${formatNumber(
              T_K,
              2
            )} K, ΔG° = ${formatNumber(
              dG0_Jmol / 1000,
              3
            )} kJ/mol, and Q = ${formatNumber(Q, 3)}.</p>`;
          }

          if (deltaG_kJ === null || !isFinite(deltaG_kJ)) {
            throw new Error("Unable to compute ΔG. Please check your inputs.");
          }

          const spontaneity = evaluateSpontaneity(deltaG_kJ);

          resultDeltaG.textContent = `ΔG = ${formatNumber(
            deltaG_kJ,
            3
          )} kJ/mol`;
          resultDeltaGAlt.textContent = `(${formatNumber(
            deltaG_kJ * 1000,
            1
          )} J/mol)`;
          resultSpontaneity.textContent = spontaneity.label;
          resultComment.textContent = spontaneity.comment;
          resultExtra.innerHTML = extraHtml;

          resultsEl.classList.remove("hidden");
        } catch (err) {
          errorEl.textContent = err.message;
        }
      });

      document.getElementById("resetBtn").addEventListener("click", () => {
        document.getElementById("gibbs-form").reset();
        document.getElementById("results").classList.add("hidden");
        document.getElementById("errorMsg").textContent = "";
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
