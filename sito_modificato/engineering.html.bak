    <script>
      // Simple client-side filter for engineering cards
      (function () {
        const disciplineSelect = document.getElementById("eng-discipline");
        const keywordInput = document.getElementById("eng-keyword");
        const cards = Array.from(
          document.querySelectorAll("#eng-calculator-grid article")
        );
        const noResults = document.getElementById("eng-no-results");

        function normalize(str) {
          return (str || "").toLowerCase();
        }

        function applyFilter() {
          const disc = disciplineSelect.value;
          const kw = normalize(keywordInput.value);
          let visibleCount = 0;

          cards.forEach((card) => {
            const cardDisc = card.getAttribute("data-discipline");
            const cardKw = normalize(card.getAttribute("data-keywords"));
            const matchesDisc = disc === "all" || disc === cardDisc;
            const matchesKw = !kw || cardKw.includes(kw);
            const show = matchesDisc && matchesKw;
            card.style.display = show ? "" : "none";
            if (show) visibleCount++;
          });

          if (noResults) {
            noResults.classList.toggle("hidden", visibleCount > 0);
          }
        }

        if (disciplineSelect && keywordInput && cards.length) {
          disciplineSelect.addEventListener("change", applyFilter);
          keywordInput.addEventListener("input", applyFilter);
        }
      })();

      window.MathJax = {
