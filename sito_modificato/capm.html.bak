    <script>
      // CAPM logic
      function parsePercent(id) {
        const el = document.getElementById(id);
        if (!el) return NaN;
        const v = parseFloat(el.value);
        return isNaN(v) ? NaN : v;
      }

      function parseNumber(id) {
        const el = document.getElementById(id);
        if (!el) return NaN;
        const v = parseFloat(el.value);
        return isNaN(v) ? NaN : v;
      }

      function formatPct(v) {
        if (!isFinite(v)) return "–";
        return v.toFixed(2) + "%";
      }

      function calcCapm() {
        const rf = parsePercent("riskFree");
        const rm = parsePercent("marketReturn");
        const beta = parseNumber("beta");

        const resultEl = document.getElementById("capmResult");
        const mrpEl = document.getElementById("mrpResult");
        const summaryEl = document.getElementById("capmSummary");

        if (!isFinite(rf) || !isFinite(rm) || !isFinite(beta)) {
          resultEl.textContent = "–";
          mrpEl.textContent = "–";
          summaryEl.textContent =
            "Please enter valid numeric values for all inputs.";
          return null;
        }

        const mrp = rm - rf;
        const required = rf + beta * mrp;

        resultEl.textContent = formatPct(required);
        mrpEl.textContent = formatPct(mrp);

        summaryEl.textContent =
          "With a risk-free rate of " +
          formatPct(rf) +
          ", expected market return of " +
          formatPct(rm) +
          ", and beta of " +
          beta.toFixed(2) +
          ", the CAPM required return is " +
          formatPct(required) +
          ".";

        updateValuationHint(required);
        return { rf, rm, beta, mrp, required };
      }

      function updateValuationHint(required) {
        const exp = parsePercent("expectedReturn");
        const hintEl = document.getElementById("valuationHint");
        if (!isFinite(required) || !isFinite(exp)) {
          hintEl.classList.add("hidden");
          hintEl.textContent = "";
          return;
        }
        const diff = exp - required;
        const sign = diff > 0 ? "above" : diff < 0 ? "below" : "equal to";
        const absDiff = Math.abs(diff);

        let msg =
          "Your expected return of " +
          formatPct(exp) +
          " is " +
          sign +
          " the CAPM required return by " +
          formatPct(absDiff) +
          ". ";

        if (diff > 0.5) {
          msg +=
            "If your expectations are accurate, the asset may be undervalued relative to CAPM.";
        } else if (diff < -0.5) {
          msg +=
            "If your expectations are accurate, the asset may be overvalued relative to CAPM.";
        } else {
          msg +=
            "The asset appears roughly fairly valued under CAPM assumptions.";
        }

        hintEl.textContent = msg;
        hintEl.className =
          "mt-1 text-xs " + (diff >= 0 ? "text-green-700" : "text-red-700");
      }

      document.addEventListener("DOMContentLoaded", function () {
        const betaPreset = document.getElementById("betaPreset");
        const calcBtn = document.getElementById("calcBtn");
        const resetBtn = document.getElementById("resetBtn");
        const addScenarioBtn = document.getElementById("addScenarioBtn");
        const expectedReturnInput = document.getElementById("expectedReturn");

        if (betaPreset) {
          betaPreset.addEventListener("change", function () {
            if (this.value !== "") {
              const betaInput = document.getElementById("beta");
              betaInput.value = this.value;
              calcCapm();
            }
          });
        }

        if (calcBtn) {
          calcBtn.addEventListener("click", function () {
            calcCapm();
          });
        }

        if (resetBtn) {
          resetBtn.addEventListener("click", function () {
            document.getElementById("riskFree").value = 4;
            document.getElementById("marketReturn").value = 9;
            document.getElementById("beta").value = 1.2;
            document.getElementById("betaPreset").value = "";
            document.getElementById("expectedReturn").value = "";
            document.getElementById("capmResult").textContent = "–";
            document.getElementById("mrpResult").textContent = "–";
            document.getElementById("capmSummary").textContent = "";
            const hintEl = document.getElementById("valuationHint");
            hintEl.classList.add("hidden");
            hintEl.textContent = "";
          });
        }

        if (expectedReturnInput) {
          expectedReturnInput.addEventListener("input", function () {
            const last = calcCapm();
            if (last) updateValuationHint(last.required);
          });
        }

        let scenarioCount = 0;
        const scenarioTableBody = document.getElementById("scenarioTableBody");

        function renderEmptyRowIfNeeded() {
          if (!scenarioTableBody) return;
          if (scenarioTableBody.children.length === 0) {
            const tr = document.createElement("tr");
            tr.innerHTML =
              '<td colspan="7" class="px-3 py-3 text-center text-gray-400 text-xs">No scenarios yet. Calculate and click “Add scenario to comparison”.</td>';
            scenarioTableBody.appendChild(tr);
          }
        }

        if (addScenarioBtn && scenarioTableBody) {
          addScenarioBtn.addEventListener("click", function () {
            const res = calcCapm();
            if (!res) return;

            // Remove placeholder row if present
            if (
              scenarioTableBody.children.length === 1 &&
              scenarioTableBody.children[0].querySelector("td") &&
              scenarioTableBody.children[0].querySelector("td").colSpan === 7
            ) {
              scenarioTableBody.innerHTML = "";
            }

            scenarioCount += 1;
            const tr = document.createElement("tr");
            tr.className = scenarioCount % 2 === 0 ? "bg-gray-50" : "";
            const label = "Scenario " + scenarioCount;

            tr.innerHTML =
              '<td class="px-3 py-2 border-b text-gray-600">' +
              scenarioCount +
              "</td>" +
              '<td class="px-3 py-2 border-b text-gray-800">' +
              label +
              "</td>" +
              '<td class="px-3 py-2 border-b text-gray-700">' +
              res.rf.toFixed(2) +
              "</td>" +
              '<td class="px-3 py-2 border-b text-gray-700">' +
              res.rm.toFixed(2) +
              "</td>" +
              '<td class="px-3 py-2 border-b text-gray-700">' +
              res.beta.toFixed(2) +
              "</td>" +
              '<td class="px-3 py-2 border-b font-semibold">' +
              res.required.toFixed(2) +
              "</td>" +
              '<td class="px-3 py-2 border-b">' +
              '<button type="button" class="text-xs text-blue-600 hover:underline mr-2 load-scenario">Load</button>' +
              '<button type="button" class="text-xs text-red-600 hover:underline delete-scenario">Delete</button>' +
              "</td>";

            // Store data attributes for reloading
            tr.dataset.rf = res.rf;
            tr.dataset.rm = res.rm;
            tr.dataset.beta = res.beta;
            tr.dataset.required = res.required;

            scenarioTableBody.appendChild(tr);
          });

          scenarioTableBody.addEventListener("click", function (e) {
            const target = e.target;
            if (!(target instanceof HTMLElement)) return;
            const row = target.closest("tr");
            if (!row) return;

            if (target.classList.contains("delete-scenario")) {
              row.remove();
              if (scenarioTableBody.children.length === 0) {
                renderEmptyRowIfNeeded();
              }
            } else if (target.classList.contains("load-scenario")) {
              const rf = parseFloat(row.dataset.rf);
              const rm = parseFloat(row.dataset.rm);
              const beta = parseFloat(row.dataset.beta);
              if (isFinite(rf))
                document.getElementById("riskFree").value = rf.toFixed(2);
              if (isFinite(rm))
                document.getElementById("marketReturn").value = rm.toFixed(2);
              if (isFinite(beta))
                document.getElementById("beta").value = beta.toFixed(2);
              calcCapm();
            }
          });

          renderEmptyRowIfNeeded();
        }

        // Initial calculation
        calcCapm();
      });
    </script>

