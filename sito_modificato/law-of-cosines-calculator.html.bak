            <script>
              document.addEventListener('DOMContentLoaded', function() {
                const inputs = {
                    a: document.getElementById('side-a'),
                    b: document.getElementById('side-b'),
                    c: document.getElementById('side-c'),
                    alpha: document.getElementById('angle-a'),
                    beta: document.getElementById('angle-b'),
                    gamma: document.getElementById('angle-c')
                };
                const caseSSSBtn = document.getElementById('case-sss-btn');
                const caseSASBtn = document.getElementById('case-sas-btn');
                const calculateBtn = document.getElementById('calculate-btn');
                const resultsContainer = document.getElementById('results-container');
                const errorMessage = document.getElementById('error-message');
                const resultSides = document.getElementById('result-sides');
                const resultAngles = document.getElementById('result-angles');
                const stepByStep = document.getElementById('step-by-step');

                const R_TO_D = 180 / Math.PI;
                const D_TO_R = Math.PI / 180;
                let currentCase = 'SSS'; // Default case

                // --- Helper Functions ---
                function parseInput(val) {
                    const num = parseFloat(val);
                    return (num > 0 && !isNaN(num)) ? num : null;
                }
                
                function formatNumber(num) {
                    if (Math.abs(num) < 1e-10) return '0';
                    return parseFloat(num.toFixed(6)).toString();
                }

                function showError(message) {
                    errorMessage.textContent = `Error: ${message}`;
                    resultsContainer.classList.add('hidden');
                }
                
                function validateTriangle(a, b, c) {
                    return a + b > c && a + c > b && b + c > a;
                }
                
                // --- UI Switching Logic ---
                function updateInputFields() {
                    // Clear all inputs first, except for example values for SSS
                    Object.values(inputs).forEach(input => input.value = '');
                    inputs.a.value = '8';
                    inputs.b.value = '12';
                    inputs.c.value = '15';

                    if (currentCase === 'SSS') {
                        inputs.a.disabled = false; inputs.a.classList.remove('bg-gray-100');
                        inputs.b.disabled = false; inputs.b.classList.remove('bg-gray-100');
                        inputs.c.disabled = false; inputs.c.classList.remove('bg-gray-100');
                        inputs.alpha.disabled = true; inputs.alpha.classList.add('bg-gray-100'); inputs.alpha.value = '';
                        inputs.beta.disabled = true; inputs.beta.classList.add('bg-gray-100'); inputs.beta.value = '';
                        inputs.gamma.disabled = true; inputs.gamma.classList.add('bg-gray-100'); inputs.gamma.value = '';
                        caseSSSBtn.classList.replace('bg-gray-200', 'bg-blue-100');
                        caseSSSBtn.classList.add('border-blue-300');
                        caseSASBtn.classList.replace('bg-blue-100', 'bg-gray-200');
                        caseSASBtn.classList.remove('border-blue-300');
                    } else if (currentCase === 'SAS') {
                         // Set example values for SAS (a, b, gamma)
                        inputs.a.value = 10;
                        inputs.b.value = 12;
                        inputs.gamma.value = 35;
                        inputs.a.disabled = false; inputs.a.classList.remove('bg-gray-100');
                        inputs.b.disabled = false; inputs.b.classList.remove('bg-gray-100');
                        inputs.c.disabled = true; inputs.c.classList.add('bg-gray-100'); inputs.c.value = '';
                        inputs.alpha.disabled = true; inputs.alpha.classList.add('bg-gray-100'); inputs.alpha.value = '';
                        inputs.beta.disabled = true; inputs.beta.classList.add('bg-gray-100'); inputs.beta.value = '';
                        inputs.gamma.disabled = false; inputs.gamma.classList.remove('bg-gray-100');
                        caseSASBtn.classList.replace('bg-gray-200', 'bg-blue-100');
                        caseSASBtn.classList.add('border-blue-300');
                        caseSSSBtn.classList.replace('bg-blue-100', 'bg-gray-200');
                        caseSSSBtn.classList.remove('border-blue-300');
                        
                       
                    }
                    resultsContainer.classList.add('hidden');
                    errorMessage.textContent = '';
                }

                caseSSSBtn.addEventListener('click', () => { currentCase = 'SSS'; updateInputFields(); });
                caseSASBtn.addEventListener('click', () => { currentCase = 'SAS'; updateInputFields(); });
                
                // --- Core Solver Logic (Solves Triangle using Law of Cosines first, then Law of Sines) ---
                calculateBtn.addEventListener('click', () => {
                    let a = parseInput(inputs.a.value);
                    let b = parseInput(inputs.b.value);
                    let c = parseInput(inputs.c.value);
                    let alpha = parseInput(inputs.alpha.value);
                    let beta = parseInput(inputs.beta.value);
                    let gamma = parseInput(inputs.gamma.value);

                    let steps = '';
                    let startingCase = '';

                    try {
                        let missingCount = [a, b, c, alpha, beta, gamma].filter(n => n === null).length;

                        // 1. Validation and Setup
                        if (currentCase === 'SSS') {
                            if ([a, b, c].filter(n => n !== null).length !== 3) throw new Error('For SSS, enter all three side lengths (a, b, c).');
                            if (!validateTriangle(a, b, c)) throw new Error('Invalid Triangle: The sum of any two sides must be greater than the third side.');
                            startingCase = 'SSS: Sides a, b, c are known.';
                        } else if (currentCase === 'SAS') {
                            if (!isNaN(alpha) || !isNaN(beta)) throw new Error('For SAS, only the included angle (gamma) should be entered with the two adjacent sides (a, b).');
                            if (isNaN(a) || isNaN(b) || isNaN(gamma)) throw new Error('For SAS, enter two sides (a, b) and the included angle (gamma).');
                            startingCase = 'SAS: Sides a, b and included angle $\\gamma$ are known.';
                        } else {
                            throw new Error('Please select a valid case (SSS or SAS) and ensure 3 values are entered for that case.');
                        }
                        
                        // Check angles < 180
                        if (alpha > 180 || beta > 180 || gamma > 180) {
                             throw new Error('Angles cannot be greater than 180 degrees.');
                        }

                        // 2. Law of Cosines (Find the first missing value)
                        
                        if (currentCase === 'SSS') {
                            // Find angle alpha first: A = cos⁻¹((b² + c² - a²) / 2bc)
                            const cosA = (b * b + c * c - a * a) / (2 * b * c);
                            if (Math.abs(cosA) > 1 + 1e-9) throw new Error('Triangle inequality check failed to prevent a non-real angle.');
                            alpha = Math.acos(cosA) * R_TO_D;
                            
                            steps += `<p>1. <strong>Law of Cosines (Find $\\alpha$):</strong> $\\alpha = \\cos^{-1} \\left( \\frac{b^2 + c^2 - a^2}{2bc} \\right)$</p>`;
                            steps += `<p class="formula-box text-center">$\\alpha = \\cos^{-1} \\left( \\frac{${formatNumber(b)}^2 + ${formatNumber(c)}^2 - ${formatNumber(a)}^2}{2 \\cdot ${formatNumber(b)} \\cdot ${formatNumber(c)}} \\right) \\approx ${formatNumber(alpha)}^\\circ$</p>`;
                        
                        } else if (currentCase === 'SAS') {
                            // Find side c first: c² = a² + b² - 2ab cos(C)
                            const c_sq = a * a + b * b - 2 * a * b * Math.cos(gamma * D_TO_R);
                            if (c_sq <= 1e-9) throw new Error('Calculation resulted in a non-real or zero side length.');
                            c = Math.sqrt(c_sq);
                            
                            steps += `<p>1. <strong>Law of Cosines (Find side $c$):</strong> $c^2 = a^2 + b^2 - 2ab \\cos(\\gamma)$</p>`;
                            steps += `<p class="formula-box text-center">$c^2 = ${formatNumber(a)}^2 + ${formatNumber(b)}^2 - 2 \\cdot ${formatNumber(a)} \\cdot ${formatNumber(b)} \\cos(${formatNumber(gamma)}^\\circ) \\approx ${formatNumber(c_sq)}$</p>`;
                            steps += `<p>$c = \\sqrt{${formatNumber(c_sq)}} \\approx ${formatNumber(c)}$</p>`;
                        }
                        
                        // 3. Law of Sines (Find second angle)
                        // Find a second angle, say beta, using Law of Sines: sin(B)/b = sin(C)/c
                        
                        if (isNaN(beta)) {
                            // Use Law of Sines with the largest known side/angle pair (which is c/gamma from SAS, or a/alpha from SSS)
                            let angleKnown, sideKnown;
                            if (currentCase === 'SSS') {
                                angleKnown = alpha; sideKnown = a;
                            } else {
                                angleKnown = gamma; sideKnown = c;
                            }
                            
                            const sinB = (b / sideKnown) * Math.sin(angleKnown * D_TO_R);
                            if (Math.abs(sinB) > 1 + 1e-9) throw new Error('Invalid Law of Sines calculation result.');
                            beta = Math.asin(sinB) * R_TO_D;
                            
                            steps += `<p>2. <strong>Law of Sines (Find $\\beta$):</strong> $\\sin(\\beta) = \\frac{b \\sin(\\text{known angle})}{\\text{known side}}$</p>`;
                            steps += `<p class="formula-box text-center">$\\beta = \\sin^{-1} \\left( \\frac{${formatNumber(b)} \\sin(${formatNumber(angleKnown)}^\\circ)}{${formatNumber(sideKnown)}} \\right) \\approx ${formatNumber(beta)}^\\circ$</p>`;
                        }

                        // 4. Find the last angle (Angle Sum Rule)
                        gamma = 180 - alpha - beta;
                        steps += `<p>3. <strong>Angle Sum Rule (Find last angle):</strong> $\\text{Angle} = 180^\\circ - \\alpha - \\beta \\approx ${formatNumber(gamma)}^\\circ$</p>`;
                        
                        // Re-assign angles based on which one was found last
                        if (currentCase === 'SSS') {
                            // gamma was found last
                        } else if (currentCase === 'SAS') {
                             // beta was found second, alpha was found last
                             alpha = 180 - beta - gamma; // recalculate alpha to be the final one based on the order
                             steps.splice(3, 0, `<p>3. <strong>Angle Sum Rule (Find $\\alpha$):</strong> $\\alpha = 180^\\circ - \\beta - \\gamma \\approx ${formatNumber(alpha)}^\\circ$</p>`);
                        }
                        
                        // Ensure all angles are correctly assigned for final output
                        let A_final = (currentCase === 'SAS') ? alpha : alpha;
                        let B_final = (currentCase === 'SAS') ? beta : beta;
                        let C_final = (currentCase === 'SAS') ? gamma : gamma;

                        if (currentCase === 'SSS') {
                            const temp_alpha = alpha;
                            alpha = temp_alpha; // Alpha is the angle found by LoC in SSS
                            const temp_gamma = 180 - temp_alpha - beta;
                            // Find gamma first using LoC: gamma = cos⁻¹((a² + b² - c²) / 2ab)
                            const cosC = (a * a + b * b - c * c) / (2 * a * b);
                            gamma = Math.acos(cosC) * R_TO_D;
                            // Find alpha last: 180 - beta - gamma
                            alpha = 180 - beta - gamma;
                        }
                        
                        // Rerunning to ensure correct assignment logic for final display:
                        // SSS: a, b, c -> gamma (LoC) -> beta (LoS) -> alpha (Sum)
                        if (currentCase === 'SSS') {
                            const cosC = (a * a + b * b - c * c) / (2 * a * b);
                            const C_val = Math.acos(cosC) * R_TO_D;
                            const sinB = (b / c) * Math.sin(C_val * D_TO_R);
                            const B_val = Math.asin(sinB) * R_TO_D;
                            const A_val = 180 - C_val - B_val;
                            A_final = A_val; B_final = B_val; C_final = C_val;
                            
                        } else if (currentCase === 'SAS') {
                            // SAS: a, b, gamma -> c (LoC) -> alpha (LoS) -> beta (Sum)
                            const c_sq = a * a + b * b - 2 * a * b * Math.cos(gamma * D_TO_R);
                            const C_val = Math.sqrt(c_sq);
                            const sinA = (a / C_val) * Math.sin(gamma * D_TO_R);
                            const A_val = Math.asin(sinA) * R_TO_D;
                            const B_val = 180 - A_val - gamma;
                            a = a; b = b; c = C_val;
                            A_final = A_val; B_final = B_val; C_final = gamma;
                        }
                        

                        // Final Validation Check
                        if (A_final <= 0 || B_final <= 0 || C_final <= 0 || formatNumber(A_final + B_final + C_final) !== '180') {
                             throw new Error('Calculated angles are invalid. Please check side lengths.');
                        }
                        
                        // 5. Final Output
                        resultSides.innerHTML = `a = ${formatNumber(a)}<br>b = ${formatNumber(b)}<br>c = ${formatNumber(c)}`;
                        resultAngles.innerHTML = `$\\alpha = ${formatNumber(A_final)}^\\circ$<br>$\\beta = ${formatNumber(B_final)}^\\circ$<br>$\\gamma = ${formatNumber(C_final)}^\\circ$`;
                        stepByStep.innerHTML = steps;
                        resultsContainer.classList.remove('hidden');

                        // Re-render MathJax
                        if (window.MathJax) {
                            window.MathJax.typesetPromise([resultAngles, stepByStep]);
                        }

                    } catch (e) {
                        showError(e.message);
                    }
                });

                // Initial setup and click
                updateInputFields();
                calculateBtn.click();
            </script>

