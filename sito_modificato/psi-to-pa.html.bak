    <script>
      // PSI <-> Pa conversion logic
      (function() {
        const FACTOR = 6894.757293168;
        const psiInput = document.getElementById('psiInput');
        const paInput = document.getElementById('paInput');
        const swapBtn = document.getElementById('swapBtn');
        const tableBody = document.getElementById('psiPaTableBody');

        function roundVal(val, decimals = 2) {
          if (!isFinite(val)) return '';
          const factor = Math.pow(10, decimals);
          return Math.round(val * factor) / factor;
        }

        function psiToPa(psi) {
          return psi * FACTOR;
        }

        function paToPsi(pa) {
          return pa / FACTOR;
        }

        psiInput.addEventListener('input', function() {
          const psiVal = parseFloat(psiInput.value);
          if (isNaN(psiVal)) {
            paInput.value = '';
            return;
          }
          const paVal = psiToPa(psiVal);
          paInput.value = roundVal(paVal, 4);
        });

        paInput.addEventListener('input', function() {
          const paVal = parseFloat(paInput.value);
          if (isNaN(paVal)) {
            psiInput.value = '';
            return;
          }
          const psiVal = paToPsi(paVal);
          psiInput.value = roundVal(psiVal, 4);
        });

        if (swapBtn) {
          swapBtn.addEventListener('click', function() {
            const psiVal = psiInput.value;
            const paVal = paInput.value;
            psiInput.value = paVal ? roundVal(paToPsi(parseFloat(paVal)), 4) : '';
            paInput.value = psiVal ? roundVal(psiToPa(parseFloat(psiVal)), 4) : '';
          });
        }

        // populate table
        const samplePsi = [0, 5, 10, 15, 30, 50, 75, 100, 150, 200, 300];
        if (tableBody) {
          samplePsi.forEach(function(psi) {
            const pa = psiToPa(psi);
            const kpa = pa / 1000;
            const tr = document.createElement('tr');
            tr.innerHTML = `
              <td class="text-right">${psi}</td>
              <td class="text-right">${roundVal(pa, 2)}</td>
              <td class="text-right">${roundVal(kpa, 3)}</td>
            `;
            tableBody.appendChild(tr);
          });
        }
      })();
    </script>

