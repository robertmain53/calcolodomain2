    <script>
      // Utility: time parsing and formatting
      function parseTimeToMinutes(t) {
        const [h, m] = t.split(":").map(Number);
        return (h * 60 + m) % (24 * 60);
      }
      function minutesToTime(mins) {
        mins = ((mins % (24 * 60)) + 24 * 60) % (24 * 60);
        const h = Math.floor(mins / 60);
        const m = mins % 60;
        return `${String(h).padStart(2, "0")}:${String(m).padStart(2, "0")}`;
      }

      function getMethodConfig(method, customFast, customEat) {
        switch (method) {
          case "16-8":
            return { fast: 16, eat: 8, type: "tre" };
          case "18-6":
            return { fast: 18, eat: 6, type: "tre" };
          case "20-4":
            return { fast: 20, eat: 4, type: "tre" };
          case "14-10":
            return { fast: 14, eat: 10, type: "tre" };
          case "12-12":
            return { fast: 12, eat: 12, type: "tre" };
          case "omad":
            return { fast: 23, eat: 1, type: "tre" };
          case "5-2":
            return { fast: null, eat: null, type: "5-2" };
          case "custom":
            return { fast: customFast, eat: customEat, type: "tre" };
          default:
            return { fast: 16, eat: 8, type: "tre" };
        }
      }

      function updateMethodNote(method) {
        const note = document.getElementById("method-note");
        const customWrapper = document.getElementById("custom-window-wrapper");
        const fiveTwoWrapper = document.getElementById("five-two-wrapper");
        const fiveTwoSummary = document.getElementById("five-two-summary");
        const fiveTwoColHeader = document.getElementById("five-two-col-header");

        customWrapper.classList.add("hidden");
        fiveTwoWrapper.classList.add("hidden");
        fiveTwoSummary.classList.add("hidden");
        fiveTwoColHeader.classList.add("hidden");

        switch (method) {
          case "16-8":
            note.textContent =
              "16:8 is a popular, sustainable daily schedule for many adults.";
            break;
          case "18-6":
            note.textContent =
              "18:6 is more intensive; consider building up gradually from 14:10 or 16:8.";
            break;
          case "20-4":
            note.textContent =
              "20:4 (Warrior diet) is quite restrictive and may not be appropriate for everyone.";
            break;
          case "14-10":
            note.textContent =
              "14:10 is a gentle time-restricted eating pattern suitable for many beginners.";
            break;
          case "12-12":
            note.textContent =
              "12:12 approximates a traditional overnight fast and can be a first step toward IF.";
            break;
          case "omad":
            note.textContent =
              "OMAD (one meal a day) is very restrictive. Medical guidance is strongly recommended.";
            break;
          case "5-2":
            note.textContent =
              "5:2 uses two low-calorie days per week rather than a strict daily fasting window.";
            fiveTwoWrapper.classList.remove("hidden");
            fiveTwoSummary.classList.remove("hidden");
            fiveTwoColHeader.classList.remove("hidden");
            break;
          case "custom":
            note.textContent =
              "Define your own fasting and eating window. Ensure it is safe and sustainable for you.";
            customWrapper.classList.remove("hidden");
            break;
        }
      }

      function buildTimelineSegments(eatStartMin, eatHours, wakeMin, sleepMin) {
        const totalMinutes = 24 * 60;
        const eatEndMin = eatStartMin + eatHours * 60;
        const segments = [];

        function isEating(min) {
          const inWindow =
            eatEndMin > eatStartMin
              ? min >= eatStartMin && min < eatEndMin
              : min >= eatStartMin ||
                min < (eatEndMin + totalMinutes) % totalMinutes;
          return inWindow;
        }

        function isAwake(min) {
          if (wakeMin === sleepMin) return true; // treat as always awake if equal
          return sleepMin > wakeMin
            ? min >= wakeMin && min < sleepMin
            : min >= wakeMin || min < sleepMin;
        }

        let currentType = null;
        let currentStart = 0;

        for (let m = 0; m < totalMinutes; m += 15) {
          let type;
          if (isEating(m)) {
            type = "eat";
          } else if (isAwake(m)) {
            type = "fast-awake";
          } else {
            type = "fast-sleep";
          }

          if (currentType === null) {
            currentType = type;
            currentStart = m;
          } else if (type !== currentType) {
            segments.push({ type: currentType, start: currentStart, end: m });
            currentType = type;
            currentStart = m;
          }
        }
        segments.push({
          type: currentType,
          start: currentStart,
          end: totalMinutes,
        });
        return segments;
      }

      function renderTimeline(eatStartMin, eatHours, wakeMin, sleepMin) {
        const container = document.getElementById("timeline-bar");
        container.innerHTML = "";
        const totalMinutes = 24 * 60;
        const segments = buildTimelineSegments(
          eatStartMin,
          eatHours,
          wakeMin,
          sleepMin
        );

        segments.forEach((seg) => {
          const widthPercent = ((seg.end - seg.start) / totalMinutes) * 100;
          const div = document.createElement("div");
          div.style.width = widthPercent + "%";
          let bg, label;
          if (seg.type === "eat") {
            bg = "bg-blue-500";
            label = "Eat";
          } else if (seg.type === "fast-awake") {
            bg = "bg-slate-300";
            label = "Fast";
          } else {
            bg = "bg-slate-500";
            label = "Fast (sleep)";
          }
          div.className = `flex items-center justify-center text-white ${bg}`;
          div.textContent = label;
          container.appendChild(div);
        });
      }

      function generateWeeklyTable(config, eatStartMin, wakeMin, sleepMin) {
        const tbody = document.getElementById("weekly-body");
        tbody.innerHTML = "";
        const days = [
          "Monday",
          "Tuesday",
          "Wednesday",
          "Thursday",
          "Friday",
          "Saturday",
          "Sunday",
        ];

        const method = document.getElementById("if-method").value;
        const low1 = document.getElementById("lowday1").value;
        const low2 = document.getElementById("lowday2").value;
        const lowCal =
          parseFloat(document.getElementById("lowday-cal").value) || 500;
        const maintCal =
          parseFloat(document.getElementById("maint-cal").value) || 2000;

        let weeklyDeficit = 0;

        days.forEach((day) => {
          const tr = document.createElement("tr");
          const dayTd = document.createElement("td");
          dayTd.textContent = day;
          tr.appendChild(dayTd);

          const patternTd = document.createElement("td");
          const eatTd = document.createElement("td");
          const fastTd = document.createElement("td");
          const kcalTd = document.createElement("td");

          if (method === "5-2") {
            const isLow = day === low1 || day === low2;
            patternTd.textContent = isLow ? "Low-calorie day" : "Normal eating";
            eatTd.textContent = isLow ? "Flexible (spread meals)" : "Flexible";
            fastTd.textContent = "No fixed fast (aim for overnight 12 h)";
            kcalTd.textContent = isLow
              ? `${lowCal.toFixed(0)} kcal`
              : `${maintCal.toFixed(0)} kcal`;
            if (isLow) {
              weeklyDeficit += maintCal - lowCal;
            }
          } else {
            const eatStart = eatStartMin;
            const eatEnd = eatStart + config.eat * 60;
            const fastStart = eatEnd;
            const fastEnd = eatStart + 24 * 60;

            patternTd.textContent = `${config.fast}h fast / ${config.eat}h eat`;
            eatTd.textContent = `${minutesToTime(eatStart)} – ${minutesToTime(
              eatEnd
            )}`;
            fastTd.textContent = `${minutesToTime(fastStart)} – ${minutesToTime(
              fastEnd
            )}`;
            kcalTd.textContent = "—";
          }

          tr.appendChild(patternTd);
          tr.appendChild(eatTd);
          tr.appendChild(fastTd);
          const fiveTwoColHeader = document.getElementById(
            "five-two-col-header"
          );
          if (!fiveTwoColHeader.classList.contains("hidden")) {
            tr.appendChild(kcalTd);
          }
          tbody.appendChild(tr);
        });

        if (method === "5-2") {
          const deficitP = document.getElementById("five-two-deficit");
          deficitP.textContent = `Approximate weekly deficit: about ${weeklyDeficit.toFixed(
            0
          )} kcal (assuming ${maintCal.toFixed(0)} kcal maintenance).`;
        }
      }

      function updateSummaries(config, eatStartMin) {
        const fastingEl = document.getElementById("summary-fasting");
        const eatingEl = document.getElementById("summary-eating");
        const weeklyEl = document.getElementById("summary-weekly");
        const method = document.getElementById("if-method").value;

        if (method === "5-2") {
          fastingEl.textContent = "Varies (focus on 2 low-calorie days)";
          eatingEl.textContent = "Flexible (aim for 12 h overnight fast)";
          weeklyEl.textContent = "5 normal days + 2 low-calorie days";
          return;
        }

        const eatHours = config.eat;
        const fastHours = config.fast;
        const eatStart = eatStartMin;
        const eatEnd = eatStart + eatHours * 60;
        const fastStart = eatEnd;
        const fastEnd = eatStart + 24 * 60;

        fastingEl.textContent = `${fastHours} h (${minutesToTime(
          fastStart
        )} – ${minutesToTime(fastEnd)})`;
        eatingEl.textContent = `${eatHours} h (${minutesToTime(
          eatStart
        )} – ${minutesToTime(eatEnd)})`;
        weeklyEl.textContent = "7 days / week time-restricted eating";
      }

      document.getElementById("if-method").addEventListener("change", (e) => {
        updateMethodNote(e.target.value);
      });

      document
        .getElementById("custom-fast-hours")
        .addEventListener("input", () => {
          const fast =
            parseInt(document.getElementById("custom-fast-hours").value) || 16;
          const eatInput = document.getElementById("custom-eat-hours");
          eatInput.value = Math.max(1, 24 - fast);
        });

      document
        .getElementById("custom-eat-hours")
        .addEventListener("input", () => {
          const eat =
            parseInt(document.getElementById("custom-eat-hours").value) || 8;
          const fastInput = document.getElementById("custom-fast-hours");
          fastInput.value = Math.max(1, 24 - eat);
        });

      document.getElementById("generate-plan").addEventListener("click", () => {
        const method = document.getElementById("if-method").value;
        const wakeTime = document.getElementById("wake-time").value || "07:00";
        const sleepTime =
          document.getElementById("sleep-time").value || "23:00";
        const eatStartTime =
          document.getElementById("eat-start-time").value || "12:00";

        const wakeMin = parseTimeToMinutes(wakeTime);
        const sleepMin = parseTimeToMinutes(sleepTime);
        const eatStartMin = parseTimeToMinutes(eatStartTime);

        const customFast =
          parseInt(document.getElementById("custom-fast-hours").value) || 16;
        const customEat =
          parseInt(document.getElementById("custom-eat-hours").value) || 8;

        const config = getMethodConfig(method, customFast, customEat);

        updateSummaries(config, eatStartMin);
        if (config.type === "tre") {
          renderTimeline(eatStartMin, config.eat, wakeMin, sleepMin);
        } else {
          // For 5:2, show a generic timeline: 12h eat, 12h fast
          renderTimeline(parseTimeToMinutes("08:00"), 12, wakeMin, sleepMin);
        }
        generateWeeklyTable(config, eatStartMin, wakeMin, sleepMin);
      });

      // Initialize defaults on load
      document.addEventListener("DOMContentLoaded", () => {
        updateMethodNote(document.getElementById("if-method").value);
        document.getElementById("generate-plan").click();
      });

      // Mobile menu script (simple)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const expanded = this.getAttribute("aria-expanded") === "true";
          this.setAttribute("aria-expanded", String(!expanded));
          menu.classList.toggle("hidden");
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
