    <script>
      // Core constants
      const EARTH_RADIUS_KM = 6371;

      function degToKm(deg) {
        return ((deg * Math.PI) / 180) * EARTH_RADIUS_KM;
      }

      function formatNumber(val, decimals = 1) {
        if (!isFinite(val)) return "–";
        return val.toFixed(decimals);
      }

      function getVelocities(model) {
        // Returns {vp, vs, vsurf}
        if (model === "crust") {
          const vp = 6.0;
          const vs = 3.5;
          return { vp, vs, vsurf: 0.92 * vs };
        }
        if (model === "mantle") {
          const vp = 8.0;
          const vs = 4.5;
          return { vp, vs, vsurf: 0.95 * vs };
        }
        // custom
        const vp = parseFloat(document.getElementById("vpCustom").value);
        const vs = parseFloat(document.getElementById("vsCustom").value);
        const vpClamped = isFinite(vp) && vp > 0 ? vp : 6.0;
        const vsClamped = isFinite(vs) && vs > 0 ? vs : 3.5;
        return { vp: vpClamped, vs: vsClamped, vsurf: 0.9 * vsClamped };
      }

      function computeTravelTimes() {
        const distVal = parseFloat(
          document.getElementById("distanceValue").value
        );
        const distUnit = document.getElementById("distanceUnit").value;
        const depthVal =
          parseFloat(document.getElementById("depthValue").value) || 0;
        const model = document.getElementById("velocityModel").value;

        if (!isFinite(distVal) || distVal < 0) {
          alert("Please enter a non‑negative epicentral distance.");
          return;
        }
        if (distUnit === "deg" && distVal > 180) {
          alert("Epicentral distance in degrees must be between 0 and 180.");
          return;
        }
        if (depthVal < 0 || depthVal > 700) {
          alert("Depth should be between 0 and 700 km.");
          return;
        }

        let DsurfKm = distUnit === "km" ? distVal : degToKm(distVal);
        const { vp, vs, vsurf } = getVelocities(model);

        // Straight-line path approximation
        const L = Math.sqrt(DsurfKm * DsurfKm + depthVal * depthVal);

        const tP = L / vp;
        const tS = L / vs;
        const tSurf = L / vsurf;
        const dSP = tS - tP;

        document.getElementById("pArrival").textContent = formatNumber(tP, 1);
        document.getElementById("sArrival").textContent = formatNumber(tS, 1);
        document.getElementById("surfArrival").textContent = formatNumber(
          tSurf,
          1
        );

        document.getElementById("pSpeed").textContent = formatNumber(vp, 2);
        document.getElementById("sSpeed").textContent = formatNumber(vs, 2);
        document.getElementById("surfSpeed").textContent = formatNumber(
          vsurf,
          2
        );

        document.getElementById("spDiff").textContent = formatNumber(dSP, 1);
      }

      document.addEventListener("DOMContentLoaded", function () {
        const modelSelect = document.getElementById("velocityModel");
        const customContainer = document.getElementById(
          "customVelocityContainer"
        );
        const computeBtn = document.getElementById("computeBtn");

        modelSelect.addEventListener("change", () => {
          if (modelSelect.value === "custom") {
            customContainer.classList.remove("hidden");
          } else {
            customContainer.classList.add("hidden");
          }
        });

        computeBtn.addEventListener("click", computeTravelTimes);

        // Preset buttons
        document.querySelectorAll("[data-preset]").forEach((btn) => {
          btn.addEventListener("click", () => {
            const preset = btn.getAttribute("data-preset");
            const distInput = document.getElementById("distanceValue");
            const distUnit = document.getElementById("distanceUnit");
            const depthInput = document.getElementById("depthValue");

            if (preset === "local") {
              distUnit.value = "km";
              distInput.value = 50;
              depthInput.value = 5;
              modelSelect.value = "crust";
              customContainer.classList.add("hidden");
            } else if (preset === "regional") {
              distUnit.value = "km";
              distInput.value = 500;
              depthInput.value = 15;
              modelSelect.value = "crust";
              customContainer.classList.add("hidden");
            } else if (preset === "teleseismic") {
              distUnit.value = "deg";
              distInput.value = 60;
              depthInput.value = 100;
              modelSelect.value = "mantle";
              customContainer.classList.add("hidden");
            }
            computeTravelTimes();
          });
        });

        // Initial computation
        computeTravelTimes();
      });
    </script>

