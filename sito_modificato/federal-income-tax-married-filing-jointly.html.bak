    <script>
      // Simple tax bracket data for MFJ (approximate, for illustration; update as needed)
      const TAX_DATA = {
        2024: {
          standardDeduction: 29200,
          brackets: [
            { upTo: 23200, rate: 0.1 },
            { upTo: 94300, rate: 0.12 },
            { upTo: 201050, rate: 0.22 },
            { upTo: 383900, rate: 0.24 },
            { upTo: 487450, rate: 0.32 },
            { upTo: 731200, rate: 0.35 },
            { upTo: Infinity, rate: 0.37 },
          ],
        },
        2023: {
          standardDeduction: 27700,
          brackets: [
            { upTo: 22000, rate: 0.1 },
            { upTo: 89450, rate: 0.12 },
            { upTo: 190750, rate: 0.22 },
            { upTo: 364200, rate: 0.24 },
            { upTo: 462500, rate: 0.32 },
            { upTo: 693750, rate: 0.35 },
            { upTo: Infinity, rate: 0.37 },
          ],
        },
      };

      function formatMoney(value) {
        if (!isFinite(value)) return "$0";
        return value.toLocaleString("en-US", {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 0,
        });
      }

      function formatPercent(value) {
        if (!isFinite(value)) return "0%";
        return (value * 100).toFixed(1).replace(/\.0$/, "") + "%";
      }

      function getSelectedYear() {
        const year = document.getElementById("tax-year").value;
        return TAX_DATA[year] ? parseInt(year, 10) : 2024;
      }

      function calculateTaxForYear(taxable, year) {
        const data = TAX_DATA[year];
        const brackets = data.brackets;
        let remaining = Math.max(0, taxable);
        let lastLimit = 0;
        let totalTax = 0;
        const slices = [];

        for (const bracket of brackets) {
          if (remaining <= 0) break;
          const sliceIncome = Math.min(remaining, bracket.upTo - lastLimit);
          if (sliceIncome > 0) {
            const sliceTax = sliceIncome * bracket.rate;
            slices.push({
              from: lastLimit,
              to: bracket.upTo,
              rate: bracket.rate,
              income: sliceIncome,
              tax: sliceTax,
            });
            totalTax += sliceTax;
            remaining -= sliceIncome;
            lastLimit = bracket.upTo;
          }
        }

        const marginalRate = slices.length ? slices[slices.length - 1].rate : 0;
        return { totalTax, slices, marginalRate };
      }

      function updateStandardDeductionDisplay() {
        const year = getSelectedYear();
        const data = TAX_DATA[year];
        document
          .querySelectorAll(".tax-year-label")
          .forEach((el) => (el.textContent = year.toString()));
        document.getElementById("standard-deduction-amount").textContent =
          formatMoney(data.standardDeduction);
      }

      function updateDeductionUI() {
        const type =
          document.querySelector('input[name="deduction-type"]:checked')
            ?.value || "standard";
        const itemizedWrapper = document.getElementById("itemized-wrapper");
        const stdDisplay = document.getElementById(
          "standard-deduction-display"
        );
        if (type === "itemized") {
          itemizedWrapper.classList.remove("hidden");
          stdDisplay.classList.add("opacity-60");
        } else {
          itemizedWrapper.classList.add("hidden");
          stdDisplay.classList.remove("opacity-60");
        }
      }

      function renderBracketBreakdown(slices) {
        const container = document.getElementById("bracket-breakdown");
        container.innerHTML = "";
        if (!slices.length) {
          container.textContent =
            "No taxable income; no federal income tax owed.";
          return;
        }
        slices.forEach((slice) => {
          const div = document.createElement("div");
          div.innerHTML = `
        <div class="flex justify-between">
          <span>${formatPercent(slice.rate)} on ${formatMoney(
            slice.income
          )}</span>
          <span class="font-medium">${formatMoney(slice.tax)}</span>
        </div>
      `;
          container.appendChild(div);
        });
      }

      function runCalculation() {
        const year = getSelectedYear();
        const data = TAX_DATA[year];

        const gross = Math.max(
          0,
          Number(document.getElementById("combined-income").value) || 0
        );
        const pretaxRet = Math.max(
          0,
          Number(document.getElementById("pretax-retirement").value) || 0
        );
        const pretaxHsa = Math.max(
          0,
          Number(document.getElementById("pretax-hsa").value) || 0
        );
        const otherAdj = Math.max(
          0,
          Number(document.getElementById("other-adjustments").value) || 0
        );
        const credits = Math.max(
          0,
          Number(document.getElementById("tax-credits").value) || 0
        );

        const agi = Math.max(0, gross - pretaxRet - pretaxHsa - otherAdj);

        const deductionType =
          document.querySelector('input[name="deduction-type"]:checked')
            ?.value || "standard";
        let deduction = data.standardDeduction;
        if (deductionType === "itemized") {
          deduction = Math.max(
            0,
            Number(document.getElementById("itemized-deduction").value) || 0
          );
        }

        const taxableIncome = Math.max(0, agi - deduction);
        const {
          totalTax: preCreditTax,
          slices,
          marginalRate,
        } = calculateTaxForYear(taxableIncome, year);
        const totalTax = Math.max(0, preCreditTax - credits);
        const afterTaxIncome = gross - totalTax;
        const effectiveRate = gross > 0 ? totalTax / gross : 0;

        // Update outputs
        document.getElementById("agi-output").textContent = formatMoney(agi);
        document.getElementById("taxable-income-output").textContent =
          formatMoney(taxableIncome);
        document.getElementById("total-tax-output").textContent =
          formatMoney(totalTax);
        document.getElementById("after-tax-output").textContent =
          formatMoney(afterTaxIncome);
        document.getElementById("marginal-rate-output").textContent =
          formatPercent(marginalRate);
        document.getElementById("effective-rate-output").textContent =
          formatPercent(effectiveRate);

        renderBracketBreakdown(slices);
      }

      document.addEventListener("DOMContentLoaded", () => {
        updateStandardDeductionDisplay();
        updateDeductionUI();
        runCalculation();

        document.getElementById("tax-year").addEventListener("change", () => {
          updateStandardDeductionDisplay();
          runCalculation();
        });

        document
          .querySelectorAll('input[name="deduction-type"]')
          .forEach((radio) => {
            radio.addEventListener("change", () => {
              updateDeductionUI();
              runCalculation();
            });
          });

        document
          .getElementById("calculate-btn")
          .addEventListener("click", (e) => {
            e.preventDefault();
            runCalculation();
          });

        // Recalculate on input blur for responsiveness
        [
          "combined-income",
          "pretax-retirement",
          "pretax-hsa",
          "other-adjustments",
          "itemized-deduction",
          "tax-credits",
        ].forEach((id) => {
          const el = document.getElementById(id);
          if (el) {
            el.addEventListener("change", runCalculation);
            el.addEventListener("blur", runCalculation);
          }
        });
      });

      // MathJax config (kept from template)
      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
