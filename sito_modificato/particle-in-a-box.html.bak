    <script>
      // Physical constants
      const h = 6.62607015e-34; // Planck constant (J s)
      const hbar = h / (2 * Math.PI);
      const eCharge = 1.602176634e-19; // Coulomb
      const mElectron = 9.1093837e-31; // kg
      const mProton = 1.67262192369e-27; // kg

      const dimSelect = document.getElementById("dimension-select");
      const lengths1D = document.getElementById("lengths-1d");
      const lengths2D = document.getElementById("lengths-2d");
      const lengths3D = document.getElementById("lengths-3d");
      const qn1D = document.getElementById("qn-1d");
      const qn2D = document.getElementById("qn-2d");
      const qn3D = document.getElementById("qn-3d");
      const massInput = document.getElementById("mass-input");
      const massUnit = document.getElementById("mass-unit");
      const massPresetBtns = document.querySelectorAll(".mass-preset-btn");
      const calcBtn = document.getElementById("calculate-btn");
      const errorEl = document.getElementById("calc-error");
      const energyJEl = document.getElementById("energy-j");
      const energyEvEl = document.getElementById("energy-ev");
      const waveEl = document.getElementById("wavefunction-display");
      const probEl = document.getElementById("probability-display");
      const plotSvg = document.getElementById("wave-plot");

      function setMassPreset(preset) {
        massPresetBtns.forEach((btn) => {
          if (btn.dataset.massPreset === preset) {
            btn.classList.add("bg-blue-50", "text-blue-700", "border-blue-400");
          } else {
            btn.classList.remove(
              "bg-blue-50",
              "text-blue-700",
              "border-blue-400"
            );
          }
        });
        if (preset === "electron") {
          massUnit.value = "me";
          massInput.value = 1;
        } else if (preset === "proton") {
          massUnit.value = "kg";
          massInput.value = mProton.toExponential(4);
        } else {
          massUnit.value = "kg";
        }
      }

      massPresetBtns.forEach((btn) => {
        btn.addEventListener("click", () =>
          setMassPreset(btn.dataset.massPreset)
        );
      });

      setMassPreset("electron");

      function onDimChange() {
        const d = parseInt(dimSelect.value, 10);
        lengths1D.classList.toggle("hidden", d !== 1);
        lengths2D.classList.toggle("hidden", d !== 2);
        lengths3D.classList.toggle("hidden", d !== 3);
        qn1D.classList.toggle("hidden", d !== 1);
        qn2D.classList.toggle("hidden", d !== 2);
        qn3D.classList.toggle("hidden", d !== 3);
      }

      dimSelect.addEventListener("change", onDimChange);

      function getMassSI() {
        let m = parseFloat(massInput.value);
        if (!isFinite(m) || m <= 0) return null;
        if (massUnit.value === "me") {
          return m * mElectron;
        }
        return m;
      }

      function lengthToMeters(value, unit) {
        const L = parseFloat(value);
        if (!isFinite(L) || L <= 0) return null;
        return unit === "nm" ? L * 1e-9 : L;
      }

      function formatSci(x, digits = 4) {
        if (!isFinite(x)) return "–";
        if (x === 0) return "0";
        const absx = Math.abs(x);
        if (absx >= 1e-2 && absx < 1e3) {
          return x.toPrecision(digits);
        }
        return x.toExponential(digits);
      }

      function clearError() {
        errorEl.textContent = "";
        errorEl.classList.add("hidden");
      }

      function showError(msg) {
        errorEl.textContent = msg;
        errorEl.classList.remove("hidden");
      }

      function computeEnergyAndWave() {
        clearError();
        const dim = parseInt(dimSelect.value, 10);
        const m = getMassSI();
        if (!m) {
          showError("Please enter a valid positive mass.");
          return;
        }

        let Lx, Ly, Lz;
        let nx = 1,
          ny = 1,
          nz = 1;

        if (dim === 1) {
          Lx = lengthToMeters(
            document.getElementById("Lx-input").value,
            document.getElementById("Lx-unit").value
          );
          if (!Lx)
            return showError("Please enter a valid positive box length L.");
          nx = parseInt(document.getElementById("n1-input").value, 10);
          if (!Number.isInteger(nx) || nx < 1)
            return showError("Quantum number n must be a positive integer.");
        } else if (dim === 2) {
          Lx = lengthToMeters(
            document.getElementById("Lx2-input").value,
            document.getElementById("Lx2-unit").value
          );
          Ly = lengthToMeters(
            document.getElementById("Ly2-input").value,
            document.getElementById("Ly2-unit").value
          );
          if (!Lx || !Ly)
            return showError("Please enter valid positive lengths Lx and Ly.");
          nx = parseInt(document.getElementById("nx2-input").value, 10);
          ny = parseInt(document.getElementById("ny2-input").value, 10);
          if (![nx, ny].every((n) => Number.isInteger(n) && n >= 1)) {
            return showError(
              "Quantum numbers n_x and n_y must be positive integers."
            );
          }
        } else if (dim === 3) {
          Lx = lengthToMeters(
            document.getElementById("Lx3-input").value,
            document.getElementById("Lx3-unit").value
          );
          Ly = lengthToMeters(
            document.getElementById("Ly3-input").value,
            document.getElementById("Ly3-unit").value
          );
          Lz = lengthToMeters(
            document.getElementById("Lz3-input").value,
            document.getElementById("Lz3-unit").value
          );
          if (!Lx || !Ly || !Lz)
            return showError(
              "Please enter valid positive lengths Lx, Ly, and Lz."
            );
          nx = parseInt(document.getElementById("nx3-input").value, 10);
          ny = parseInt(document.getElementById("ny3-input").value, 10);
          nz = parseInt(document.getElementById("nz3-input").value, 10);
          if (![nx, ny, nz].every((n) => Number.isInteger(n) && n >= 1)) {
            return showError(
              "Quantum numbers n_x, n_y, n_z must be positive integers."
            );
          }
        }

        let sumTerm;
        if (dim === 1) {
          sumTerm = (nx * nx) / (Lx * Lx);
        } else if (dim === 2) {
          sumTerm = (nx * nx) / (Lx * Lx) + (ny * ny) / (Ly * Ly);
        } else {
          sumTerm =
            (nx * nx) / (Lx * Lx) +
            (ny * ny) / (Ly * Ly) +
            (nz * nz) / (Lz * Lz);
        }

        const E = ((h * h) / (8 * m)) * sumTerm; // Joules
        const Eev = E / eCharge;

        energyJEl.textContent = `E = ${formatSci(E, 4)} J`;
        energyEvEl.textContent = `E = ${formatSci(Eev, 4)} eV`;

        // Wavefunction display (symbolic)
        if (dim === 1) {
          waveEl.textContent = `ψₙ(x) = √(2/L) · sin(n·π·x / L), with n = ${nx}`;
          probEl.textContent = `|ψₙ(x)|² = (2/L) · sin²(n·π·x / L)`;
        } else if (dim === 2) {
          waveEl.textContent = `ψₙₓ,ₙᵧ(x,y) = √(2/Lx)·sin(nₓ·π·x/Lx) · √(2/Ly)·sin(nᵧ·π·y/Ly), with nₓ = ${nx}, nᵧ = ${ny}`;
          probEl.textContent = `|ψ|² = (2/Lx)·sin²(nₓ·π·x/Lx) · (2/Ly)·sin²(nᵧ·π·y/Ly)`;
        } else {
          waveEl.textContent = `ψₙₓ,ₙᵧ,ₙ_z(x,y,z) = √(2/Lx)·sin(nₓ·π·x/Lx) · √(2/Ly)·sin(nᵧ·π·y/Ly) · √(2/Lz)·sin(n_z·π·z/Lz), with nₓ = ${nx}, nᵧ = ${ny}, n_z = ${nz}`;
          probEl.textContent = `|ψ|² = (2/Lx)·sin²(nₓ·π·x/Lx) · (2/Ly)·sin²(nᵧ·π·y/Ly) · (2/Lz)·sin²(n_z·π·z/Lz)`;
        }

        drawWavePlot(nx, Lx);
      }

      function drawWavePlot(n, Lx) {
        // Simple SVG plot of ψ(x) and |ψ(x)|² on 0..L
        while (plotSvg.firstChild) plotSvg.removeChild(plotSvg.firstChild);

        const width = 400;
        const height = 260;
        const margin = { left: 40, right: 10, top: 10, bottom: 30 };
        const plotW = width - margin.left - margin.right;
        const plotH = height - margin.top - margin.bottom;

        function createLine(x1, y1, x2, y2, cls, color) {
          const l = document.createElementNS(
            "http://www.w3.org/2000/svg",
            "line"
          );
          l.setAttribute("x1", x1);
          l.setAttribute("y1", y1);
          l.setAttribute("x2", x2);
          l.setAttribute("y2", y2);
          l.setAttribute("class", cls || "");
          if (color) l.setAttribute("stroke", color);
          return l;
        }

        // Axes
        const x0 = margin.left;
        const y0 = margin.top + plotH / 2;
        const xAxis = createLine(
          margin.left,
          y0,
          margin.left + plotW,
          y0,
          "axis"
        );
        const yAxis = createLine(
          margin.left,
          margin.top,
          margin.left,
          margin.top + plotH,
          "axis"
        );
        plotSvg.appendChild(xAxis);
        plotSvg.appendChild(yAxis);

        // Labels (x and ψ)
        const xLabel = document.createElementNS(
          "http://www.w3.org/2000/svg",
          "text"
        );
        xLabel.setAttribute("x", margin.left + plotW / 2);
        xLabel.setAttribute("y", height - 5);
        xLabel.setAttribute("text-anchor", "middle");
        xLabel.setAttribute("font-size", "10");
        xLabel.textContent = "x";
        plotSvg.appendChild(xLabel);

        const psiLabel = document.createElementNS(
          "http://www.w3.org/2000/svg",
          "text"
        );
        psiLabel.setAttribute("x", 5);
        psiLabel.setAttribute("y", margin.top + 10);
        psiLabel.setAttribute("font-size", "10");
        psiLabel.textContent = "ψ(x), |ψ(x)|²";
        plotSvg.appendChild(psiLabel);

        if (!Lx || !isFinite(Lx)) return;

        const A = Math.sqrt(2 / Lx);
        const maxPsi = A; // max of sin is 1
        const maxProb = A * A; // 2/L

        const psiScale = (plotH * 0.4) / maxPsi;
        const probScale = (plotH * 0.4) / maxProb;

        const numPoints = 200;
        const psiPath = [];
        const probPath = [];

        for (let i = 0; i <= numPoints; i++) {
          const x = (i / numPoints) * Lx;
          const psi = A * Math.sin((n * Math.PI * x) / Lx);
          const prob = psi * psi;

          const sx = margin.left + (x / Lx) * plotW;
          const syPsi = y0 - psi * psiScale;
          const syProb = y0 - prob * probScale;

          psiPath.push({ x: sx, y: syPsi });
          probPath.push({ x: sx, y: syProb });
        }

        function pathFromPoints(points) {
          return points
            .map((p, idx) =>
              idx === 0 ? `M ${p.x} ${p.y}` : `L ${p.x} ${p.y}`
            )
            .join(" ");
        }

        const psiCurve = document.createElementNS(
          "http://www.w3.org/2000/svg",
          "path"
        );
        psiCurve.setAttribute("d", pathFromPoints(psiPath));
        psiCurve.setAttribute("class", "wave-line");
        psiCurve.setAttribute("stroke", "#2563eb"); // blue
        plotSvg.appendChild(psiCurve);

        const probCurve = document.createElementNS(
          "http://www.w3.org/2000/svg",
          "path"
        );
        probCurve.setAttribute("d", pathFromPoints(probPath));
        probCurve.setAttribute("class", "wave-line");
        probCurve.setAttribute("stroke", "#f97316"); // orange
        plotSvg.appendChild(probCurve);

        // Legend
        const legendY = margin.top + 10;
        const legendX = margin.left + 10;

        const psiLegLine = createLine(
          legendX,
          legendY,
          legendX + 20,
          legendY,
          "",
          "#2563eb"
        );
        psiLegLine.setAttribute("stroke-width", "2");
        plotSvg.appendChild(psiLegLine);
        const psiLegText = document.createElementNS(
          "http://www.w3.org/2000/svg",
          "text"
        );
        psiLegText.setAttribute("x", legendX + 25);
        psiLegText.setAttribute("y", legendY + 3);
        psiLegText.setAttribute("font-size", "10");
        psiLegText.textContent = "ψ(x)";
        plotSvg.appendChild(psiLegText);

        const probLegLine = createLine(
          legendX,
