    <script>
      // Mode switching
      const modeTabs = document.querySelectorAll(".mode-tab");
      const panels = document.querySelectorAll(".mode-panel");
      modeTabs.forEach((tab) => {
        tab.addEventListener("click", () => {
          const mode = tab.dataset.mode;
          modeTabs.forEach((t) => {
            t.classList.remove("bg-white", "shadow-sm", "text-blue-600");
            t.classList.add("text-gray-700");
          });
          tab.classList.add("bg-white", "shadow-sm", "text-blue-600");
          panels.forEach((p) => p.classList.add("hidden"));
          document.getElementById("panel-" + mode).classList.remove("hidden");
        });
      });

      // RAM: keep MT/s and MHz linked
      const ramTypeEl = document.getElementById("ram-type");
      const ramMtEl = document.getElementById("ram-mt");
      const ramMhzEl = document.getElementById("ram-mhz");
      function syncFromMt() {
        const type = ramTypeEl.value;
        const mt = parseFloat(ramMtEl.value);
        if (!isFinite(mt) || mt <= 0) return;
        const mhz = type === "ddr" ? mt / 2 : mt;
        ramMhzEl.value = mhz.toFixed(0);
      }
      function syncFromMhz() {
        const type = ramTypeEl.value;
        const mhz = parseFloat(ramMhzEl.value);
        if (!isFinite(mhz) || mhz <= 0) return;
        const mt = type === "ddr" ? mhz * 2 : mhz;
        ramMtEl.value = mt.toFixed(0);
      }
      ramMtEl.addEventListener("input", syncFromMt);
      ramMhzEl.addEventListener("input", syncFromMhz);
      ramTypeEl.addEventListener("change", () => {
        // Re-sync using MT/s as source when type changes
        syncFromMt();
      });

      // RAM calculation
      document.getElementById("ram-calc-btn").addEventListener("click", () => {
        const cl = parseFloat(document.getElementById("ram-cl").value);
        const mt = parseFloat(ramMtEl.value);
        if (!isFinite(cl) || cl <= 0 || !isFinite(mt) || mt <= 0) {
          alert("Please enter valid CL and data rate values.");
          return;
        }
        const cycleNs = 2000 / mt; // DDR formula, also works for SDR if mt==MHz
        const latNs = cl * cycleNs;
        document.getElementById("ram-cycle-ns").textContent =
          cycleNs.toFixed(3);
        document.getElementById("ram-lat-ns").textContent = latNs.toFixed(3);
        document.getElementById("ram-result").classList.remove("hidden");
      });
      document.getElementById("ram-reset-btn").addEventListener("click", () => {
        ramTypeEl.value = "ddr";
        document.getElementById("ram-cl").value = 16;
        ramMtEl.value = 3200;
        ramMhzEl.value = 1600;
        document.getElementById("ram-result").classList.add("hidden");
      });

      // Network medium handling
      const netMediumEl = document.getElementById("net-medium");
      const netCustomWrapper = document.getElementById(
        "net-custom-speed-wrapper"
      );
      const netSpeedEl = document.getElementById("net-speed");
      netMediumEl.addEventListener("change", () => {
        if (netMediumEl.value === "custom") {
          netCustomWrapper.classList.remove("hidden");
        } else {
          netCustomWrapper.classList.add("hidden");
        }
      });

      // Network calculation
      document.getElementById("net-calc-btn").addEventListener("click", () => {
        const dKm = parseFloat(document.getElementById("net-distance").value);
        const extra =
          parseFloat(document.getElementById("net-extra").value) || 0;
        if (!isFinite(dKm) || dKm < 0) {
          alert("Please enter a valid distance.");
          return;
        }
        let speedKmPerMs;
        switch (netMediumEl.value) {
          case "vacuum":
            speedKmPerMs = 299.792;
            break;
          case "fiber":
            speedKmPerMs = 200;
            break; // approx
          case "custom":
            speedKmPerMs = parseFloat(netSpeedEl.value);
            if (!isFinite(speedKmPerMs) || speedKmPerMs <= 0) {
              alert("Please enter a valid custom speed.");
              return;
            }
            break;
          default:
            speedKmPerMs = 200;
        }
        const oneWayMs = dKm / speedKmPerMs + extra / 2;
        const propRttMs = 2 * (dKm / speedKmPerMs);
        const rttMs = 2 * oneWayMs;
        document.getElementById("net-oneway-ms").textContent =
          oneWayMs.toFixed(3);
        document.getElementById("net-rtt-ms").textContent = rttMs.toFixed(3);
        document.getElementById("net-prop-rtt-ms").textContent =
          propRttMs.toFixed(3);
        document.getElementById("net-result").classList.remove("hidden");
      });
      document.getElementById("net-reset-btn").addEventListener("click", () => {
        document.getElementById("net-distance").value = 1000;
        document.getElementById("net-extra").value = 5;
        netMediumEl.value = "fiber";
        netCustomWrapper.classList.add("hidden");
        document.getElementById("net-result").classList.add("hidden");
      });

      // Audio calculation
      document
        .getElementById("audio-calc-btn")
        .addEventListener("click", () => {
          const buf = parseFloat(document.getElementById("audio-buffer").value);
          const rate = parseFloat(document.getElementById("audio-rate").value);
          const nbuf = parseFloat(
            document.getElementById("audio-buffers").value
          );
          if (
            !isFinite(buf) ||
            buf <= 0 ||
            !isFinite(rate) ||
            rate <= 0 ||
            !isFinite(nbuf) ||
            nbuf <= 0
          ) {
            alert("Please enter valid buffer, rate and buffer count.");
            return;
          }
          const oneMs = (buf / rate) * 1000;
          const totalMs = oneMs * nbuf;
          const bps = rate / buf;
          document.getElementById("audio-one-ms").textContent =
            oneMs.toFixed(3);
          document.getElementById("audio-total-ms").textContent =
            totalMs.toFixed(3);
          document.getElementById("audio-bps").textContent = bps.toFixed(2);
          document.getElementById("audio-result").classList.remove("hidden");
        });
      document
        .getElementById("audio-reset-btn")
        .addEventListener("click", () => {
          document.getElementById("audio-buffer").value = 256;
          document.getElementById("audio-rate").value = 48000;
          document.getElementById("audio-buffers").value = 2;
          document.getElementById("audio-result").classList.add("hidden");
        });

      // Fiber mode & medium handling
      const fiberModeEl = document.getElementById("fiber-mode");
      const fiberMediumEl = document.getElementById("fiber-medium");
      const fiberDistanceWrapper = document.getElementById(
        "fiber-distance-wrapper"
      );
      const fiberLatencyWrapper = document.getElementById(
        "fiber-latency-wrapper"
      );
      const fiberCustomWrapper = document.getElementById(
        "fiber-custom-speed-wrapper"
      );
      const fiberSpeedEl = document.getElementById("fiber-speed");

      function updateFiberMode() {
        if (fiberModeEl.value === "latency") {
          fiberDistanceWrapper.classList.remove("hidden");
          fiberLatencyWrapper.classList.add("hidden");
        } else {
          fiberDistanceWrapper.classList.add("hidden");
          fiberLatencyWrapper.classList.remove("hidden");
        }
      }
      fiberModeEl.addEventListener("change", updateFiberMode);
      updateFiberMode();

      fiberMediumEl.addEventListener("change", () => {
        if (fiberMediumEl.value === "custom") {
          fiberCustomWrapper.classList.remove("hidden");
        } else {
          fiberCustomWrapper.classList.add("hidden");
        }
      });

      function getFiberSpeedKmPerS() {
        switch (fiberMediumEl.value) {
          case "vacuum":
            return 299792;
          case "fiber":
            return 204000;
          case "copper":
            return 200000;
          case "custom":
            const v = parseFloat(fiberSpeedEl.value);
            if (!isFinite(v) || v <= 0) return null;
            return v;
          default:
            return 204000;
        }
      }

      // Fiber calculation
      document
        .getElementById("fiber-calc-btn")
        .addEventListener("click", () => {
          const speedKmPerS = getFiberSpeedKmPerS();
          if (!speedKmPerS) {
            alert("Please enter a valid custom speed.");
            return;
          }
          const distInput = parseFloat(
            document.getElementById("fiber-distance").value
          );
          const distUnit = document.getElementById("fiber-distance-unit").value;
          const latInput = parseFloat(
            document.getElementById("fiber-latency").value
          );
          const latUnit = document.getElementById("fiber-latency-unit").value;

          let distanceKm, latencyMs;

          if (fiberModeEl.value === "latency") {
            if (!isFinite(distInput) || distInput < 0) {
              alert("Please enter a valid distance.");
              return;
            }
            // Convert distance to km
            let dMeters;
            switch (distUnit) {
              case "m":
                dMeters = distInput;
                break;
              case "km":
                dMeters = distInput * 1000;
                break;
              case "ft":
                dMeters = distInput * 0.3048;
                break;
              case "mi":
                dMeters = distInput * 1609.344;
                break;
              default:
                dMeters = distInput;
            }
            distanceKm = dMeters / 1000;
            const tSeconds = distanceKm / speedKmPerS;
            latencyMs = tSeconds * 1000;
          } else {
            if (!isFinite(latInput) || latInput < 0) {
              alert("Please enter a valid latency.");
              return;
            }
            // Convert latency to seconds
            let tSeconds;
            switch (latUnit) {
              case "ns":
                tSeconds = latInput * 1e-9;
                break;
              case "us":
                tSeconds = latInput * 1e-6;
                break;
              case "ms":
                tSeconds = latInput * 1e-3;
                break;
              default:
                tSeconds = latInput * 1e-3;
            }
            distanceKm = speedKmPerS * tSeconds;
            latencyMs = tSeconds * 1000;
          }

          // Prepare human-readable outputs
          let distStr;
          if (distanceKm < 1) {
            const m = distanceKm * 1000;
            distStr = m.toFixed(3) + " m";
          } else if (distanceKm < 100) {
            distStr = distanceKm.toFixed(3) + " km";
          } else {
            distStr = distanceKm.toFixed(1) + " km";
          }

          let latStr;
          if (latencyMs < 1) {
            const us = latencyMs * 1000;
            if (us < 1) {
              const ns = us * 1000;
              latStr = ns.toFixed(2) + " ns";
            } else {
              latStr = us.toFixed(2) + " µs";
            }
          } else {
            latStr = latencyMs.toFixed(3) + " ms";
          }

          document.getElementById("fiber-out-distance").textContent = distStr;
          document.getElementById("fiber-out-latency").textContent = latStr;
          document.getElementById("fiber-out-speed").textContent =
            speedKmPerS.toLocaleString() + " km/s";
          document.getElementById("fiber-result").classList.remove("hidden");
        });

      document
        .getElementById("fiber-reset-btn")
        .addEventListener("click", () => {
          fiberModeEl.value = "latency";
          fiberMediumEl.value = "fiber";
          document.getElementById("fiber-distance").value = 10;
          document.getElementById("fiber-distance-unit").value = "km";
          document.getElementById("fiber-latency").value = 1;
          document.getElementById("fiber-latency-unit").value = "ms";
          fiberCustomWrapper.classList.add("hidden");
          updateFiberMode();
          document.getElementById("fiber-result").classList.add("hidden");
        });

      // MathJax config
      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
