    <script>
      // Icosahedron volume calculator logic
      (function () {
        const edgeInput = document.getElementById("edgeLength");
        const edgeUnit = document.getElementById("edgeUnit");
        const showExact = document.getElementById("showExact");
        const volumeApproxEl = document.getElementById("volumeApprox");
        const volumeExactEl = document.getElementById("volumeExact");
        const surfaceAreaEl = document.getElementById("surfaceArea");
        const edgeNormEl = document.getElementById("edgeNormalized");
        const errorEl = document.getElementById("inputError");
        const calcBtn = document.getElementById("calcBtn");
        const resetBtn = document.getElementById("resetBtn");

        function getUnitSymbols(unit) {
          switch (unit) {
            case "mm":
              return { len: "mm", area: "mm²", vol: "mm³" };
            case "cm":
              return { len: "cm", area: "cm²", vol: "cm³" };
            case "m":
              return { len: "m", area: "m²", vol: "m³" };
            case "in":
              return { len: "in", area: "in²", vol: "in³" };
            case "ft":
              return { len: "ft", area: "ft²", vol: "ft³" };
            default:
              return { len: unit, area: unit + "²", vol: unit + "³" };
          }
        }

        function formatNumber(x, decimals) {
          if (!isFinite(x)) return "—";
          return x.toLocaleString(undefined, {
            maximumFractionDigits: decimals,
            minimumFractionDigits: 0,
          });
        }

        function calculate() {
          const a = parseFloat(edgeInput.value);
          const unit = edgeUnit.value;
          const units = getUnitSymbols(unit);

          errorEl.classList.add("hidden");
          errorEl.textContent = "";

          if (isNaN(a) || a <= 0) {
            errorEl.textContent = "Please enter a positive edge length.";
            errorEl.classList.remove("hidden");
            volumeApproxEl.textContent = "—";
            volumeExactEl.textContent = "—";
            surfaceAreaEl.textContent = "—";
            edgeNormEl.textContent = "—";
            return;
          }

          const sqrt5 = Math.sqrt(5);
          const volumeFactor = (5 * (3 + sqrt5)) / 12; // 5(3+√5)/12
          const volume = volumeFactor * Math.pow(a, 3);
          const area = 5 * Math.sqrt(3) * Math.pow(a, 2);

          volumeApproxEl.textContent = `${formatNumber(volume, 6)} ${
            units.vol
          }`;
          surfaceAreaEl.textContent = `${formatNumber(area, 6)} ${units.area}`;
          edgeNormEl.textContent = `${formatNumber(a, 6)} ${units.len}`;

          if (showExact.checked) {
            // Exact: (5(3+√5)/12) a^3
            const a3 = Math.pow(a, 3);
            const exactStr = `V = (5(3 + √5)/12) · ${formatNumber(a3, 6)} ${
              units.len
            }³`;
            volumeExactEl.textContent = exactStr;
          } else {
            volumeExactEl.textContent =
              "Exact form hidden (enable the checkbox to show it).";
          }
        }

        function reset() {
          edgeInput.value = "";
          edgeUnit.value = "cm";
          showExact.checked = true;
          volumeApproxEl.textContent = "—";
          volumeExactEl.textContent = "—";
          surfaceAreaEl.textContent = "—";
          edgeNormEl.textContent = "—";
          errorEl.textContent = "";
          errorEl.classList.add("hidden");
        }

        calcBtn.addEventListener("click", calculate);
        resetBtn.addEventListener("click", reset);
        edgeInput.addEventListener("keydown", function (e) {
          if (e.key === "Enter") {
            e.preventDefault();
            calculate();
          }
        });
      })();

      // Simple FAQ toggle (in addition to global script_faq if present)
      document.addEventListener("DOMContentLoaded", function () {
        const faqButtons = document.querySelectorAll(
          "#faq-container .faq-question"
        );
        faqButtons.forEach((btn) => {
          btn.addEventListener("click", () => {
            const answer = btn.nextElementSibling;
            const icon = btn.querySelector("span:last-child");
            const isHidden = answer.classList.contains("hidden");
            answer.classList.toggle("hidden");
            icon.textContent = isHidden ? "−" : "+";
          });
        });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
