    <script>
      (function () {
        const valueInput = document.getElementById("fe-value");
        const unitSelect = document.getElementById("fe-unit");
        const summaryEl = document.getElementById("fe-summary");

        const out = {
          mpg_us: document.getElementById("out-mpg-us"),
          mpg_uk: document.getElementById("out-mpg-uk"),
          km_per_l: document.getElementById("out-km-l"),
          l_per_100km: document.getElementById("out-l-100km"),
          l_per_km: document.getElementById("out-l-km"),
          gal_us_per_100mi: document.getElementById("out-gal-us-100mi"),
          gal_uk_per_100mi: document.getElementById("out-gal-uk-100mi"),
        };

        const C = {
          L_PER_US_GAL: 3.785411784,
          L_PER_UK_GAL: 4.54609,
          KM_PER_MILE: 1.609344,
        };

        function toNumber(str) {
          if (str === null || str === undefined) return NaN;
          const m = String(str).match(/-?\d+(\.\d+)?/);
          return m ? parseFloat(m[0]) : NaN;
        }

        function format(v) {
          if (!isFinite(v)) return "";
          const abs = Math.abs(v);
          if (abs === 0) return "0";
          if (abs >= 1000) return v.toFixed(1);
          if (abs >= 100) return v.toFixed(2);
          if (abs >= 10) return v.toFixed(3);
          if (abs >= 1) return v.toFixed(3);
          return v.toFixed(4);
        }

        function clearOutputs() {
          Object.values(out).forEach((el) => (el.value = ""));
          summaryEl.textContent = "Enter a value to see a quick summary here.";
        }

        function convertFromBase(base) {
          // base is km per liter
          const kmPerL = base;

          const mpgUS = kmPerL * (1 / C.KM_PER_MILE) * C.L_PER_US_GAL;
          const mpgUK = kmPerL * (1 / C.KM_PER_MILE) * C.L_PER_UK_GAL;
          const lPer100km = kmPerL === 0 ? Infinity : 100 / kmPerL;
          const lPerKm = kmPerL === 0 ? Infinity : 1 / kmPerL;
          const galUSPer100mi = (lPer100km / C.L_PER_US_GAL) * C.KM_PER_MILE; // 100 km ≈ 62.137 mi
          const galUKPer100mi = (lPer100km / C.L_PER_UK_GAL) * C.KM_PER_MILE;

          out.mpg_us.value = format(mpgUS);
          out.mpg_uk.value = format(mpgUK);
          out.km_per_l.value = format(kmPerL);
          out.l_per_100km.value = format(lPer100km);
          out.l_per_km.value = format(lPerKm);
          out.gal_us_per_100mi.value = format(galUSPer100mi);
          out.gal_uk_per_100mi.value = format(galUKPer100mi);

          if (isFinite(lPer100km) && isFinite(mpgUS)) {
            summaryEl.textContent = `${format(mpgUS)} US MPG ≈ ${format(
              lPer100km
            )} L/100km (${format(kmPerL)} km/L).`;
          } else {
            summaryEl.textContent = "Enter a positive value to see a summary.";
          }
        }

        function toBaseFrom(value, unit) {
          const v = value;
          switch (unit) {
            case "mpg_us": {
              // km/L = MPG * (mile->km) / (gal->L)
              return (v * C.KM_PER_MILE) / C.L_PER_US_GAL;
            }
            case "mpg_uk": {
              return (v * C.KM_PER_MILE) / C.L_PER_UK_GAL;
            }
            case "km_per_l": {
              return v;
            }
            case "l_per_100km": {
              if (v === 0) return Infinity;
              return 100 / v;
            }
            case "l_per_km": {
              if (v === 0) return Infinity;
              return 1 / v;
            }
            case "gal_us_per_100mi": {
              // convert gal/100mi -> L/100km -> km/L
              const lPer100mi = v * C.L_PER_US_GAL;
              const lPer100km = lPer100mi / C.KM_PER_MILE;
              return 100 / lPer100km;
            }
            case "gal_uk_per_100mi": {
              const lPer100mi = v * C.L_PER_UK_GAL;
              const lPer100km = lPer100mi / C.KM_PER_MILE;
              return 100 / lPer100km;
            }
            default:
              return NaN;
          }
        }

        function recalc() {
          const raw = valueInput.value;
          const num = toNumber(raw);
          if (!isFinite(num) || num < 0) {
            clearOutputs();
            return;
          }
          const unit = unitSelect.value;
          const base = toBaseFrom(num, unit);
          if (!isFinite(base) || base <= 0) {
            clearOutputs();
            return;
          }
          convertFromBase(base);
        }

        valueInput.addEventListener("input", recalc);
        unitSelect.addEventListener("change", recalc);

        document
          .getElementById("fe-clear-btn")
          .addEventListener("click", () => {
            valueInput.value = "";
            clearOutputs();
            valueInput.focus();
          });

        document.getElementById("fe-swap-btn").addEventListener("click", () => {
          // If current unit is mpg_us, convert to l/100km and show that as input; if l/100km, swap back.
          const currentUnit = unitSelect.value;
          const num = toNumber(valueInput.value);
          if (!isFinite(num) || num <= 0) {
            unitSelect.value =
              currentUnit === "mpg_us" ? "l_per_100km" : "mpg_us";
            recalc();
            return;
          }
          if (currentUnit === "mpg_us") {
            const lPer100km = 235.214583 / num;
            unitSelect.value = "l_per_100km";
            valueInput.value = lPer100km.toFixed(3);
          } else if (currentUnit === "l_per_100km") {
            const mpg = 235.214583 / num;
            unitSelect.value = "mpg_us";
            valueInput.value = mpg.toFixed(3);
          } else {
            // default: switch to mpg_us
            unitSelect.value = "mpg_us";
          }
          recalc();
        });

        // Initialize with a common example
        unitSelect.value = "mpg_us";
        valueInput.value = "30";
        recalc();
      })();
    </script>

