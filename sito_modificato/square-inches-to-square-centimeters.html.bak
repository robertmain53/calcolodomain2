    <script>
      (function () {
        const FACTOR_IN2_TO_CM2 = 6.4516;
        const FACTOR_CM2_TO_IN2 = 1 / FACTOR_IN2_TO_CM2;

        let mode = "in2-to-cm2"; // or 'cm2-to-in2'

        const inputEl = document.getElementById("input-value");
        const outputEl = document.getElementById("output-value");
        const modeIn2Btn = document.getElementById("mode-in2-to-cm2");
        const modeCm2Btn = document.getElementById("mode-cm2-to-in2");
        const inputUnitLabel = document.getElementById("input-unit-label");
        const outputUnitLabel = document.getElementById("output-unit-label");
        const precisionSelect = document.getElementById("precision-select");
        const swapBtn = document.getElementById("swap-btn");
        const clearBtn = document.getElementById("clear-btn");
        const tableBody = document.getElementById("conversion-table-body");
        const presetButtons = document.querySelectorAll("button[data-preset]");

        function formatNumber(value) {
          if (!isFinite(value)) return "";
          const decimals = parseInt(precisionSelect.value, 10);
          return value.toLocaleString(undefined, {
            minimumFractionDigits: decimals,
            maximumFractionDigits: decimals,
          });
        }

        function convert() {
          const raw = inputEl.value.trim();
          if (raw === "" || isNaN(Number(raw))) {
            outputEl.value = "";
            return;
          }
          const val = Number(raw);
          let result;
          if (mode === "in2-to-cm2") {
            result = val * FACTOR_IN2_TO_CM2;
          } else {
            result = val * FACTOR_CM2_TO_IN2;
          }
          outputEl.value = formatNumber(result);
        }

        function setMode(newMode) {
          mode = newMode;
          if (mode === "in2-to-cm2") {
            modeIn2Btn.classList.add("toggle-btn-active");
            modeIn2Btn.classList.remove("toggle-btn-inactive");
            modeCm2Btn.classList.add("toggle-btn-inactive");
            modeCm2Btn.classList.remove("toggle-btn-active");
            inputUnitLabel.textContent = "in²";
            outputUnitLabel.textContent = "cm²";
          } else {
            modeCm2Btn.classList.add("toggle-btn-active");
            modeCm2Btn.classList.remove("toggle-btn-inactive");
            modeIn2Btn.classList.add("toggle-btn-inactive");
            modeIn2Btn.classList.remove("toggle-btn-active");
            inputUnitLabel.textContent = "cm²";
            outputUnitLabel.textContent = "in²";
          }
          convert();
        }

        function swapUnits() {
          if (
            outputEl.value.trim() === "" ||
            isNaN(Number(outputEl.value.replace(/,/g, "")))
          ) {
            setMode(mode === "in2-to-cm2" ? "cm2-to-in2" : "in2-to-cm2");
            return;
          }
          const currentOutput = Number(outputEl.value.replace(/,/g, ""));
          inputEl.value = currentOutput;
          setMode(mode === "in2-to-cm2" ? "cm2-to-in2" : "in2-to-cm2");
          convert();
        }

        function clearFields() {
          inputEl.value = "";
          outputEl.value = "";
          inputEl.focus();
        }

        function buildTable() {
          const values = [
            1, 2, 4, 5, 10, 20, 25, 50, 75, 100, 150, 200, 250, 500, 750, 1000,
          ];
          const rows = values
            .map((v) => {
              const cm2 = v * FACTOR_IN2_TO_CM2;
              return `<tr class="odd:bg-white even:bg-gray-50">
          <td class="px-3 py-2 border-b text-gray-800">${v.toLocaleString()}</td>
          <td class="px-3 py-2 border-b text-gray-800">${cm2.toLocaleString(
            undefined,
            { maximumFractionDigits: 4 }
          )}</td>
        </tr>`;
            })
            .join("");
          tableBody.innerHTML = rows;
        }

        // Event listeners
        inputEl.addEventListener("input", convert);
        precisionSelect.addEventListener("change", convert);
        modeIn2Btn.addEventListener("click", () => setMode("in2-to-cm2"));
        modeCm2Btn.addEventListener("click", () => setMode("cm2-to-in2"));
        swapBtn.addEventListener("click", swapUnits);
        clearBtn.addEventListener("click", clearFields);

        presetButtons.forEach((btn) => {
          btn.addEventListener("click", () => {
            const val = btn.getAttribute("data-preset");
            if (mode !== "in2-to-cm2") {
              setMode("in2-to-cm2");
            }
            inputEl.value = val;
            convert();
            inputEl.focus();
          });
        });

        buildTable();
        convert();
      })();
    </script>

