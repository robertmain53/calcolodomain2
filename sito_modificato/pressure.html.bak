      }

      // --- Tabs handling ---
      document.querySelectorAll(".pressure-tab").forEach((btn) => {
        btn.addEventListener("click", () => {
          const mode = btn.getAttribute("data-mode");
          document
            .querySelectorAll(".pressure-tab")
            .forEach((b) => b.classList.remove("tab-active"));
          btn.classList.add("tab-active");
          document.querySelectorAll(".pressure-panel").forEach((p) => hide(p));
          const panel = document.getElementById("panel-" + mode);
          if (panel) show(panel);
        });
      });

      // --- Conversions to SI (base: Pa, N, m², kg/m³, m, etc.) ---
      const pressureToPa = {
        Pa: (v) => v,
        kPa: (v) => v * 1e3,
        MPa: (v) => v * 1e6,
        bar: (v) => v * 1e5,
        atm: (v) => v * 101325,
        psi: (v) => v * 6894.757293168,
        mmHg: (v) => v * 133.3223684211,
        torr: (v) => v * 133.3223684211,
      };
      const paToPressure = {
        Pa: (v) => v,
        kPa: (v) => v / 1e3,
        MPa: (v) => v / 1e6,
        bar: (v) => v / 1e5,
        atm: (v) => v / 101325,
        psi: (v) => v / 6894.757293168,
        mmHg: (v) => v / 133.3223684211,
        torr: (v) => v / 133.3223684211,
      };
      const forceToN = {
        N: (v) => v,
        kN: (v) => v * 1e3,
        lbf: (v) => v * 4.4482216153,
      };
      const areaToM2 = {
        m2: (v) => v,
        cm2: (v) => v * 1e-4,
        mm2: (v) => v * 1e-6,
        ft2: (v) => v * 0.09290304,
        in2: (v) => v * 0.00064516,
      };
      const densityToKgM3 = {
        kgm3: (v) => v,
        gcm3: (v) => v * 1000,
        lbft3: (v) => v * 16.01846337396,
      };
      const depthToM = {
        m: (v) => v,
        cm: (v) => v / 100,
        ft: (v) => v * 0.3048,
      };
      const gravityToMs2 = {
        ms2: (v) => v,
        fts2: (v) => v * 0.3048,
      };
      const volumeToM3 = {
        m3: (v) => v,
        L: (v) => v / 1000,
      };

      // --- Basic Pressure: F/A ---
      document
        .getElementById("btn-basic-calc")
        .addEventListener("click", () => {
          const F = parseFloat(document.getElementById("force-value").value);
          const A = parseFloat(document.getElementById("area-value").value);
          const P = parseFloat(
            document.getElementById("pressure-basic-value").value
          );
          const Fset = !isNaN(F);
          const Aset = !isNaN(A);
          const Pset = !isNaN(P);

          const filled = [Fset, Aset, Pset].filter(Boolean).length;
          if (filled < 2) {
            alert("Please enter at least two of: force, area, pressure.");
            return;
          }
          if (filled === 3) {
            alert("Leave one field blank to solve for it.");
            return;
          }

          const Funit = document.getElementById("force-unit").value;
          const Aunit = document.getElementById("area-unit").value;
          const Punit = document.getElementById("pressure-basic-unit").value;

          let F_N = Fset ? forceToN[Funit](F) : null;
          let A_m2 = Aset ? areaToM2[Aunit](A) : null;
          let P_Pa = Pset ? pressureToPa[Punit](P) : null;

          if (!Fset) {
            // compute F = P * A
            F_N = P_Pa * A_m2;
            const F_out =
              F_N / (Funit === "kN" ? 1e3 : Funit === "lbf" ? 4.4482216153 : 1);
            document.getElementById("force-value").value = F_out;
          } else if (!Aset) {
            // compute A = F / P
            A_m2 = F_N / P_Pa;
            let factor = 1;
            if (Aunit === "cm2") factor = 1e4;
            else if (Aunit === "mm2") factor = 1e6;
            else if (Aunit === "ft2") factor = 1 / 0.09290304;
            else if (Aunit === "in2") factor = 1 / 0.00064516;
            document.getElementById("area-value").value = A_m2 * factor;
          } else if (!Pset) {
            // compute P = F / A
            P_Pa = F_N / A_m2;
            document.getElementById("pressure-basic-value").value =
              paToPressure[Punit](P_Pa);
          }

          const resultBox = document.getElementById("basic-result");
          const resultText = document.getElementById("basic-result-text");
          const P_display = paToPressure[Punit](F_N / A_m2);
          resultText.textContent = `Pressure = ${P_display.toPrecision(
            6
          )} ${Punit}`;
          show(resultBox);
        });

      document
        .getElementById("btn-basic-clear")
        .addEventListener("click", () => {
          ["force-value", "area-value", "pressure-basic-value"].forEach(
            (id) => (document.getElementById(id).value = "")
          );
          hide(document.getElementById("basic-result"));
        });

      // --- Fluid Pressure: ρ g h ---
      document
        .getElementById("btn-fluid-calc")
        .addEventListener("click", () => {
          const rho = parseFloat(
            document.getElementById("density-value").value
          );
          const h = parseFloat(document.getElementById("depth-value").value);
          const g = parseFloat(document.getElementById("gravity-value").value);

          if (isNaN(rho) || isNaN(h) || isNaN(g)) {
            alert("Please enter density, depth, and gravity.");
            return;
          }

          const rhoSI =
            densityToKgM3[document.getElementById("density-unit").value](rho);
          const hSI = depthToM[document.getElementById("depth-unit").value](h);
          const gSI =
            gravityToMs2[document.getElementById("gravity-unit").value](g);

          let P_gauge = rhoSI * gSI * hSI; // Pa

          const ref = document.querySelector(
            'input[name="fluid-ref"]:checked'
          ).value;
          let P_abs = P_gauge;
          if (ref === "absolute") {
            let PatmInput = parseFloat(
              document.getElementById("atm-pressure-value").value
            );
            const atmUnit = document.getElementById("atm-pressure-unit").value;
            if (isNaN(PatmInput)) {
              PatmInput = 101325; // standard atm in Pa
              document.getElementById("atm-pressure-value").value = 101325;
              document.getElementById("atm-pressure-unit").value = "Pa";
            }
            const PatmPa = pressureToPa[atmUnit](PatmInput);
            P_abs = PatmPa + P_gauge;
          }

          const resultBox = document.getElementById("fluid-result");
          const resultText = document.getElementById("fluid-result-text");

          const gauge_kPa = paToPressure.kPa(P_gauge);
          const abs_kPa = paToPressure.kPa(P_abs);

          if (ref === "absolute") {
            resultText.innerHTML = `Gauge pressure: <strong>${gauge_kPa.toFixed(
              3
            )} kPa</strong><br>Absolute pressure: <strong>${abs_kPa.toFixed(
              3
            )} kPa</strong>`;
          } else {
            resultText.innerHTML = `Gauge pressure: <strong>${gauge_kPa.toFixed(
              3
            )} kPa</strong>`;
          }
          show(resultBox);
        });

      document
        .getElementById("btn-fluid-clear")
        .addEventListener("click", () => {
          [
            "density-value",
            "depth-value",
            "gravity-value",
            "atm-pressure-value",
          ].forEach((id) => (document.getElementById(id).value = ""));
          document.getElementById("gravity-value").value = 9.80665;
          document.querySelector(
            'input[name="fluid-ref"][value="gauge"]'
          ).checked = true;
          hide(document.getElementById("fluid-result"));
        });

      // --- Ideal Gas: PV = nRT ---
      const Rgas = 8.314462618; // J/mol·K

      document.getElementById("btn-gas-calc").addEventListener("click", () => {
        const P = parseFloat(
          document.getElementById("gas-pressure-value").value
        );
        const V = parseFloat(document.getElementById("gas-volume-value").value);
        const n = parseFloat(document.getElementById("gas-moles-value").value);
        const T = parseFloat(document.getElementById("gas-temp-value").value);

        const Pset = !isNaN(P);
        const Vset = !isNaN(V);
        const nset = !isNaN(n);
        const Tset = !isNaN(T);

        const filled = [Pset, Vset, nset, Tset].filter(Boolean).length;
        if (filled < 3) {
          alert(
            "Please enter at least three of: pressure, volume, moles, temperature."
          );
          return;
        }
        if (filled === 4) {
          alert("Leave one field blank to solve for it.");
          return;
        }

        const Punit = document.getElementById("gas-pressure-unit").value;
        const Vunit = document.getElementById("gas-volume-unit").value;
        const Tunit = document.getElementById("gas-temp-unit").value;

        let P_Pa = Pset ? pressureToPa[Punit](P) : null;
        let V_m3 = Vset ? volumeToM3[Vunit](V) : null;
        let n_mol = nset ? n : null;
        let T_K = Tset ? (Tunit === "C" ? T + 273.15 : T) : null;

        if (!Pset) {
          P_Pa = (n_mol * Rgas * T_K) / V_m3;
          document.getElementById("gas-pressure-value").value =
            paToPressure[Punit](P_Pa);
        } else if (!Vset) {
          V_m3 = (n_mol * Rgas * T_K) / P_Pa;
          const V_out = Vunit === "L" ? V_m3 * 1000 : V_m3;
          document.getElementById("gas-volume-value").value = V_out;
        } else if (!nset) {
          n_mol = (P_Pa * V_m3) / (Rgas * T_K);
          document.getElementById("gas-moles-value").value = n_mol;
        } else if (!Tset) {
          T_K = (P_Pa * V_m3) / (n_mol * Rgas);
          const T_out = Tunit === "C" ? T_K - 273.15 : T_K;
          document.getElementById("gas-temp-value").value = T_out;
        }

        const resultBox = document.getElementById("gas-result");
        const resultText = document.getElementById("gas-result-text");
        const P_display = paToPressure[Punit](P_Pa);
        resultText.textContent = `Pressure = ${P_display.toPrecision(
          6
        )} ${Punit}`;
        show(resultBox);
      });

      document.getElementById("btn-gas-clear").addEventListener("click", () => {
        [
          "gas-pressure-value",
          "gas-volume-value",
          "gas-moles-value",
          "gas-temp-value",
        ].forEach((id) => (document.getElementById(id).value = ""));
        hide(document.getElementById("gas-result"));
      });

      // --- Unit Converter ---
      document.getElementById("btn-conv-calc").addEventListener("click", () => {
        const val = parseFloat(
          document.getElementById("conv-input-value").value
        );
        if (isNaN(val)) {
          alert("Please enter a value to convert.");
          return;
        }
        const fromUnit = document.getElementById("conv-input-unit").value;
        const PaVal = pressureToPa[fromUnit](val);

        const list = document.getElementById("conv-result-list");
        list.innerHTML = "";

        Object.keys(paToPressure).forEach((unit) => {
          const converted = paToPressure[unit](PaVal);
          const row = document.createElement("div");
          row.className = "flex justify-between border-b last:border-b-0 pb-1";
          row.innerHTML = `<span>${unit}</span><span class="font-mono">${converted.toPrecision(
            6
          )}</span>`;
          list.appendChild(row);
        });

        show(document.getElementById("conv-result"));
      });

      document
        .getElementById("btn-conv-clear")
        .addEventListener("click", () => {
          document.getElementById("conv-input-value").value = "";
          hide(document.getElementById("conv-result"));
        });

      document
        .querySelectorAll("#faq-container .faq-question")
        .forEach((btn) => {
          btn.addEventListener("click", () => {
            const ans = btn.parentElement.querySelector(".faq-answer");
            const icon = btn.querySelector("span:last-child");
            const isHidden = ans.classList.contains("hidden");
            document
              .querySelectorAll("#faq-container .faq-answer")
              .forEach((a) => a.classList.add("hidden"));
            document
              .querySelectorAll("#faq-container .faq-question span:last-child")
              .forEach((i) => (i.textContent = "+"));
            if (isHidden) {
              ans.classList.remove("hidden");
              icon.textContent = "−";
            }
          });
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
