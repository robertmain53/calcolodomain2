    <script>
      // Utility: parse date safely (YYYY-MM-DD)
      function parseDate(value) {
        if (!value) return null;
        const d = new Date(value);
        return isNaN(d.getTime()) ? null : d;
      }

      function daysDiff(actual, promised) {
        const msPerDay = 24 * 60 * 60 * 1000;
        return Math.round((actual - promised) / msPerDay);
      }

      function createRow(data = {}) {
        const tr = document.createElement("tr");
        tr.className = "border-t border-gray-100";

        tr.innerHTML = `
      <td class="px-2 py-1.5">
        <input type="text" class="table-input w-full border border-gray-200 rounded px-2 py-1 text-xs" value="${
          data.orderId || ""
        }" placeholder="1001">
      </td>
      <td class="px-2 py-1.5">
        <input type="date" class="table-input w-full border border-gray-200 rounded px-2 py-1 text-xs" value="${
          data.promised || ""
        }">
      </td>
      <td class="px-2 py-1.5">
        <input type="date" class="table-input w-full border border-gray-200 rounded px-2 py-1 text-xs" value="${
          data.actual || ""
        }">
      </td>
      <td class="px-2 py-1.5">
        <input type="number" min="0" class="table-input w-20 border border-gray-200 rounded px-2 py-1 text-xs" value="${
          data.qty || ""
        }">
      </td>
      <td class="px-2 py-1.5">
        <input type="text" class="table-input w-full border border-gray-200 rounded px-2 py-1 text-xs" value="${
          data.status || "Delivered"
        }">
      </td>
      <td class="px-2 py-1.5">
        <input type="text" class="table-input w-full border border-gray-200 rounded px-2 py-1 text-xs" value="${
          data.group || ""
        }" placeholder="Customer A">
      </td>
      <td class="px-2 py-1.5">
        <span class="result-pill text-xs text-gray-700"></span>
      </td>
      <td class="px-2 py-1.5">
        <span class="delta-days text-xs text-gray-700"></span>
      </td>
      <td class="px-2 py-1.5 text-right">
        <button type="button" class="delete-row text-xs text-red-600 hover:underline">Delete</button>
      </td>
    `;

        return tr;
      }

      function getSettings() {
        const countEarly = document.getElementById("count-early").checked;
        const excludeCancelled =
          document.getElementById("exclude-cancelled").checked;
        const useWindow = document.getElementById("use-window").checked;
        const earlyWindow =
          parseFloat(document.getElementById("early-window").value) || 0;
        const lateWindow =
          parseFloat(document.getElementById("late-window").value) || 0;

        return {
          countEarly,
          excludeCancelled,
          useWindow,
          earlyWindow,
          lateWindow,
        };
      }

      function classifyOrder(promisedDate, actualDate, settings) {
        if (!promisedDate || !actualDate)
          return {
            label: "",
            delta: null,
            isOnTime: false,
            isEarly: false,
            isLate: false,
          };

        const dPromised = parseDate(promisedDate);
        const dActual = parseDate(actualDate);
        if (!dPromised || !dActual)
          return {
            label: "",
            delta: null,
            isOnTime: false,
            isEarly: false,
            isLate: false,
          };

        const delta = daysDiff(dActual, dPromised); // positive = late, negative = early

        let isOnTime = false;
        let isEarly = false;
        let isLate = false;

        if (settings.useWindow) {
          // On time if within [-earlyWindow, +lateWindow]
          if (delta >= -settings.earlyWindow && delta <= settings.lateWindow) {
            isOnTime = true;
          } else if (delta < -settings.earlyWindow) {
            isEarly = true;
          } else if (delta > settings.lateWindow) {
            isLate = true;
          }
        } else {
          if (delta === 0) {
            isOnTime = true;
          } else if (delta < 0) {
            isEarly = true;
            if (settings.countEarly) isOnTime = true;
          } else if (delta > 0) {
            isLate = true;
          }
        }

        let label = "";
        if (isOnTime && !isLate) label = "On time";
        if (isLate) label = "Late";
        if (isEarly && !isOnTime) label = "Early";

        return { label, delta, isOnTime, isEarly, isLate };
      }

      function recalc() {
        const tbody = document.getElementById("orders-body");
        const rows = Array.from(tbody.querySelectorAll("tr"));
        const settings = getSettings();

        let total = 0;
        let onTime = 0;
        let earlyCount = 0;
        let lateCount = 0;
        let lateDaysSum = 0;
        let earlyDaysSum = 0;

        const groupStats = {};

        rows.forEach((row) => {
          const inputs = row.querySelectorAll(".table-input");
          const orderId = inputs[0].value.trim();
          const promised = inputs[1].value;
          const actual = inputs[2].value;
          const qty = inputs[3].value ? parseFloat(inputs[3].value) : null;
          const status = (inputs[4].value || "").trim().toLowerCase();
          const group = (inputs[5].value || "").trim() || "All";

          const resultSpan = row.querySelector(".result-pill");
          const deltaSpan = row.querySelector(".delta-days");

          // Exclude rows with no dates
          if (!promised || !actual) {
            resultSpan.textContent = "";
            deltaSpan.textContent = "";
            return;
          }

          // Exclude cancelled/returned if chosen
          if (
            settings.excludeCancelled &&
            (status.includes("cancel") || status.includes("return"))
          ) {
            resultSpan.textContent = "Excluded";
            resultSpan.className =
              "result-pill badge bg-gray-100 text-gray-600";
            deltaSpan.textContent = "";
            return;
          }

          const { label, delta, isOnTime, isEarly, isLate } = classifyOrder(
            promised,
            actual,
            settings
          );

          if (delta === null) {
            resultSpan.textContent = "";
            deltaSpan.textContent = "";
            return;
          }

          total += 1;

          // Update per-order UI
          resultSpan.textContent = label;
          resultSpan.className = "result-pill badge";
          if (isOnTime && !isLate) {
            resultSpan.classList.add("bg-green-100", "text-green-700");
          } else if (isLate) {
            resultSpan.classList.add("bg-red-100", "text-red-700");
          } else if (isEarly) {
            resultSpan.classList.add("bg-blue-100", "text-blue-700");
          } else {
            resultSpan.classList.add("bg-gray-100", "text-gray-600");
          }

          deltaSpan.textContent =
            delta === 0 ? "0" : delta > 0 ? "+" + delta : delta;

          // Aggregate stats
          if (isOnTime && !isLate) onTime += 1;
          if (isEarly) {
            earlyCount += 1;
            earlyDaysSum += Math.abs(delta);
          }
          if (isLate) {
            lateCount += 1;
            lateDaysSum += delta;
          }

          // Group stats
          if (!groupStats[group]) {
            groupStats[group] = {
              total: 0,
              onTime: 0,
              late: 0,
              lateDaysSum: 0,
            };
          }
          groupStats[group].total += 1;
          if (isOnTime && !isLate) groupStats[group].onTime += 1;
          if (isLate) {
            groupStats[group].late += 1;
            groupStats[group].lateDaysSum += delta;
          }
        });

        const otdRate = total > 0 ? (onTime / total) * 100 : 0;
        document.getElementById("otd-rate").textContent =
          otdRate.toFixed(1) + "%";
        document.getElementById(
          "otd-summary"
        ).textContent = `${onTime} of ${total} qualifying orders`;

        document.getElementById("count-on-time").textContent = onTime;
        document.getElementById("count-early").textContent = earlyCount;
        document.getElementById("count-late").textContent = lateCount;

        const avgLate = lateCount > 0 ? lateDaysSum / lateCount : 0;
        const avgEarly = earlyCount > 0 ? earlyDaysSum / earlyCount : 0;
        document.getElementById("avg-late").textContent = avgLate.toFixed(1);
        document.getElementById("avg-early").textContent = avgEarly.toFixed(1);

        // Store group stats for later use
        window.__otdGroupStats = groupStats;
      }

      function populateGroupTable() {
        const groupStats = window.__otdGroupStats || {};
        const tbody = document.getElementById("group-body");
        tbody.innerHTML = "";

        const groups = Object.keys(groupStats).filter(
          (g) => g && groupStats[g].total > 0
        );
        if (groups.length === 0) {
          document.getElementById("group-results").classList.add("hidden");
          return;
        }

        groups.forEach((group) => {
          const stats = groupStats[group];
          const rate = stats.total > 0 ? (stats.onTime / stats.total) * 100 : 0;
          const avgLate = stats.late > 0 ? stats.lateDaysSum / stats.late : 0;

          const tr = document.createElement("tr");
          tr.className = "border-t border-gray-100";
          tr.innerHTML = `
        <td class="px-2 py-1.5">${group}</td>
        <td class="px-2 py-1.5">${stats.total}</td>
        <td class="px-2 py-1.5">${rate.toFixed(1)}%</td>
        <td class="px-2 py-1.5">${stats.onTime}</td>
        <td class="px-2 py-1.5">${stats.late}</td>
        <td class="px-2 py-1.5">${avgLate.toFixed(1)}</td>
      `;
          tbody.appendChild(tr);
        });

        document.getElementById("group-results").classList.remove("hidden");
      }

      function exportCSV() {
        const rows = [];
        const header = [
          "Order ID",
          "Promised Date",
          "Actual Date",
          "Quantity",
          "Status",
          "Group",
          "Result",
          "Delta Days",
        ];
        rows.push(header.join(","));

        const tbody = document.getElementById("orders-body");
        Array.from(tbody.querySelectorAll("tr")).forEach((row) => {
          const inputs = row.querySelectorAll(".table-input");
          const orderId = inputs[0].value.trim();
          const promised = inputs[1].value;
          const actual = inputs[2].value;
          const qty = inputs[3].value;
          const status = inputs[4].value;
          const group = inputs[5].value;
          const result = row.querySelector(".result-pill").textContent || "";
          const delta = row.querySelector(".delta-days").textContent || "";

          if (!orderId && !promised && !actual) return;

          const line = [
            orderId,
            promised,
            actual,
            qty,
            status,
            group,
            result,
            delta,
          ]
            .map((v) => `"${(v || "").replace(/"/g, '""')}"`)
            .join(",");
          rows.push(line);
        });

        const blob = new Blob([rows.join("\n")], {
          type: "text/csv;charset=utf-8;",
        });
        const url = URL.createObjectURL(blob);
        const a = document.createElement("a");
        a.href = url;
        a.download = "on-time-delivery-report.csv";
        document.body.appendChild(a);
        a.click();
        document.body.removeChild(a);
        URL.revokeObjectURL(url);
      }

      function parseBulkInput() {
        const text = document.getElementById("bulk-input").value.trim();
        if (!text) return;

        const lines = text.split(/\r?\n/).filter((l) => l.trim() !== "");
        if (lines.length === 0) return;

        const tbody = document.getElementById("orders-body");

        // If first line looks like a header, skip it
        const maybeHeader = lines[0].toLowerCase();
        const hasHeader = ["order", "promised", "actual", "date"].some((k) =>
          maybeHeader.includes(k)
        );
        const dataLines = hasHeader ? lines.slice(1) : lines;

        dataLines.forEach((line) => {
          const parts = line.split(/,|\t/).map((p) => p.trim());
          if (parts.length < 3) return;

          const [orderId, promised, actual, qty, status, group] = parts;
          const row = createRow({
            orderId,
            promised,
            actual,
            qty,
            status,
            group,
          });
          tbody.appendChild(row);
        });

        recalc();
      }

      document.addEventListener("DOMContentLoaded", () => {
        const tbody = document.getElementById("orders-body");

        // Seed with a few example rows
        const today = new Date();
        const fmt = (d) => d.toISOString().slice(0, 10);
        const d1 = new Date(today.getTime() - 2 * 24 * 60 * 60 * 1000);
        const d2 = new Date(today.getTime() - 1 * 24 * 60 * 60 * 1000);
        const d3 = new Date(today.getTime() + 1 * 24 * 60 * 60 * 1000);

        [
          {
            orderId: "1001",
            promised: fmt(d2),
            actual: fmt(d2),
            qty: 10,
            status: "Delivered",
            group: "Carrier A",
          },
          {
            orderId: "1002",
            promised: fmt(d2),
            actual: fmt(d3),
            qty: 5,
            status: "Delivered",
            group: "Carrier B",
          },
          {
            orderId: "1003",
            promised: fmt(d1),
            actual: fmt(d2),
            qty: 8,
            status: "Delivered",
            group: "Carrier A",
          },
        ].forEach((sample) => tbody.appendChild(createRow(sample)));

        recalc();

        document.getElementById("add-row").addEventListener("click", () => {
          tbody.appendChild(createRow());
          recalc();
        });

        document.getElementById("clear-rows").addEventListener("click", () => {
          tbody.innerHTML = "";
          recalc();
        });

        tbody.addEventListener("input", (e) => {
          if (e.target.classList.contains("table-input")) {
            recalc();
          }
        });

        tbody.addEventListener("click", (e) => {
          if (e.target.classList.contains("delete-row")) {
            e.target.closest("tr").remove();
            recalc();
          }
        });

        document
          .getElementById("count-early")
          .addEventListener("change", recalc);
        document
          .getElementById("exclude-cancelled")
          .addEventListener("change", recalc);
        document
          .getElementById("use-window")
          .addEventListener("change", (e) => {
            const win = document.getElementById("window-settings");
            if (e.target.checked) {
              win.classList.remove("hidden");
            } else {
              win.classList.add("hidden");
            }
            recalc();
          });
        document
          .getElementById("early-window")
          .addEventListener("input", recalc);
        document
          .getElementById("late-window")
          .addEventListener("input", recalc);

        document
          .getElementById("calc-by-group")
          .addEventListener("click", () => {
            recalc();
            populateGroupTable();
          });

        document
          .getElementById("export-csv")
          .addEventListener("click", exportCSV);
        document
          .getElementById("parse-bulk")
          .addEventListener("click", parseBulkInput);
      });

      // Mobile menu script (from template)
      document.addEventListener("DOMContentLoaded", function () {
        const toggle = document.getElementById("mobile-menu-toggle");
        const menu = document.getElementById("mobile-menu");
        if (toggle && menu) {
          toggle.addEventListener("click", () => {
            const isHidden = menu.classList.contains("hidden");
            menu.classList.toggle("hidden", !isHidden);
            toggle.setAttribute("aria-expanded", String(isHidden));
          });
        }
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
