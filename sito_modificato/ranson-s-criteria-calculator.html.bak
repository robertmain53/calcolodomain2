    <script>
      function getEtiology() {
        const radios = document.querySelectorAll('input[name="etiology"]');
        for (const r of radios) {
          if (r.checked) return r.value;
        }
        return "non-gallstone";
      }

      function updateEtiologyText() {
        const etiology = getEtiology();
        const ageText = document.getElementById("age-threshold-text");
        const etiologyLabel = document.getElementById("etiology-label");
        if (etiology === "gallstone") {
          ageText.textContent = "Gallstone: ≥ 70 years";
          etiologyLabel.textContent = "Gallstone criteria";
        } else {
          ageText.textContent = "Non-gallstone: > 55 years";
          etiologyLabel.textContent = "Non-gallstone criteria";
        }
      }

      function parseVal(id) {
        const el = document.getElementById(id);
        if (!el) return null;
        const v = parseFloat(el.value);
        return isNaN(v) ? null : v;
      }

      function calculateRanson() {
        const etiology = getEtiology();

        let admissionScore = 0;
        let score48h = 0;
        const admissionMet = [];
        const met48h = [];

        const age = parseVal("age");
        const wbc = parseVal("wbc");
        const glucose = parseVal("glucose");
        const ldh = parseVal("ldh");
        const ast = parseVal("ast");

        const hctFall = parseVal("hct-fall");
        const bunRise = parseVal("bun-rise");
        const calcium = parseVal("calcium");
        const po2 = parseVal("po2");
        const baseDeficit = parseVal("base-deficit");
        const fluid = parseVal("fluid");

        // Admission thresholds
        if (age !== null) {
          if (
            (etiology === "non-gallstone" && age > 55) ||
            (etiology === "gallstone" && age >= 70)
          ) {
            admissionScore++;
            admissionMet.push("Age criterion met");
          }
        }

        if (wbc !== null) {
          const wbcThresh = etiology === "gallstone" ? 18 : 16;
          if (wbc > wbcThresh) {
            admissionScore++;
            admissionMet.push(`WBC > ${wbcThresh} ×10⁹/L`);
          }
        }

        if (glucose !== null) {
          const gThresh = etiology === "gallstone" ? 250 : 200;
          if (glucose > gThresh) {
            admissionScore++;
            admissionMet.push(`Glucose > ${gThresh} mg/dL`);
          }
        }

        if (etiology === "non-gallstone" && ldh !== null && ldh > 350) {
          admissionScore++;
          admissionMet.push("LDH > 350 IU/L");
        }

        if (ast !== null && ast > 250) {
          admissionScore++;
          admissionMet.push("AST > 250 IU/L");
        }

        // 48-hour thresholds
        if (hctFall !== null && hctFall > 10) {
          score48h++;
          met48h.push("Hematocrit fall > 10 percentage points");
        }

        if (bunRise !== null && bunRise > 5) {
          score48h++;
          met48h.push("BUN increase > 5 mg/dL");
        }

        if (calcium !== null && calcium < 8.0) {
          score48h++;
          met48h.push("Calcium < 8.0 mg/dL");
        }

        if (po2 !== null && po2 < 60) {
          score48h++;
          met48h.push("Arterial PO₂ < 60 mmHg");
        }

        if (baseDeficit !== null && baseDeficit > 4) {
          score48h++;
          met48h.push("Base deficit > 4 mEq/L");
        }

        if (fluid !== null && fluid > 6) {
          score48h++;
          met48h.push("Fluid sequestration > 6 L");
        }

        const totalScore = admissionScore + score48h;

        // Update badges
        document.getElementById(
          "admission-met-badge"
        ).textContent = `${admissionScore} / 5 met`;
        document.getElementById(
          "48h-met-badge"
        ).textContent = `${score48h} / 6 met`;

        // Update main results
        document.getElementById("total-score").textContent =
          totalScore.toString();
        document.getElementById(
          "admission-score"
        ).textContent = `${admissionScore} / 5`;
        document.getElementById("48h-score").textContent = `${score48h} / 6`;

        // Mortality risk interpretation (classic)
        let riskText = "";
        let interp = "";

        if (totalScore <= 2) {
          riskText = "< 5% mortality (historical data)";
          interp =
            "Low risk: Ranson score 0–2 is generally associated with mild acute pancreatitis and low mortality. Continue close monitoring and supportive care, but the risk of major complications is relatively low.";
        } else if (totalScore <= 4) {
          riskText = "≈ 15–20% mortality (historical data)";
          interp =
            "Moderate risk: Ranson score 3–4 suggests more severe disease with a significant risk of complications. Consider higher level of monitoring, early involvement of critical care, and aggressive supportive management.";
        } else if (totalScore <= 6) {
          riskText = "≈ 40% mortality (historical data)";
          interp =
            "High risk: Ranson score 5–6 indicates severe acute pancreatitis with high mortality risk. ICU-level care, aggressive resuscitation, and multidisciplinary management are typically warranted.";
        } else {
          riskText = "≈ 100% mortality (historical data)";
          interp =
            "Very high risk: Ranson score ≥7 was associated with extremely high mortality in original cohorts. Modern outcomes may be better, but this still represents critical illness requiring maximal supportive care.";
        }

        document.getElementById("mortality-risk").innerHTML = riskText;
        document.getElementById("interpretation").textContent = interp;

        // Lists of criteria met
        const admList = document.getElementById("admission-met-list");
        const h48List = document.getElementById("48h-met-list");
        admList.innerHTML = "";
        h48List.innerHTML = "";

        if (admissionMet.length === 0) {
          const li = document.createElement("li");
          li.textContent = "No admission criteria met (or insufficient data).";
          admList.appendChild(li);
        } else {
          admissionMet.forEach((txt) => {
            const li = document.createElement("li");
            li.textContent = txt;
            admList.appendChild(li);
          });
        }

        if (met48h.length === 0) {
          const li = document.createElement("li");
          li.textContent = "No 48-hour criteria met (or insufficient data).";
          h48List.appendChild(li);
        } else {
          met48h.forEach((txt) => {
            const li = document.createElement("li");
            li.textContent = txt;
            h48List.appendChild(li);
          });
        }

        document.getElementById("results").classList.remove("hidden");
      }

      function resetForm() {
        document
          .querySelectorAll('#calculator input[type="number"]')
          .forEach((el) => (el.value = ""));
        document.querySelector(
          'input[name="etiology"][value="non-gallstone"]'
        ).checked = true;
        updateEtiologyText();
        document.getElementById("admission-met-badge").textContent =
          "0 / 5 met";
        document.getElementById("48h-met-badge").textContent = "0 / 6 met";
        document.getElementById("results").classList.add("hidden");
      }

      document.addEventListener("DOMContentLoaded", function () {
        document.querySelectorAll('input[name="etiology"]').forEach((r) => {
          r.addEventListener("change", updateEtiologyText);
        });
        updateEtiologyText();

        document
          .getElementById("calculate-btn")
          .addEventListener("click", function () {
            calculateRanson();
            const results = document.getElementById("results");
            results.scrollIntoView({ behavior: "smooth", block: "start" });
          });

        document
          .getElementById("reset-btn")
          .addEventListener("click", resetForm);
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
