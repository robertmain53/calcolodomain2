    <script>
      // Utility functions
      function roundTo(value, decimals) {
        if (!isFinite(value)) return value;
        const factor = Math.pow(10, decimals);
        return Math.round(value * factor) / factor;
      }

      function formatMass(value, unitSystem) {
        if (!isFinite(value)) return "–";
        if (unitSystem === "metric") {
          if (value >= 1) return roundTo(value, 2) + " kg";
          return roundTo(value * 1000, 1) + " g";
        } else {
          if (value >= 1) return roundTo(value, 2) + " lb";
          return roundTo(value * 16, 1) + " oz";
        }
      }

      function formatVolume(value, unitSystem) {
        if (!isFinite(value)) return "–";
        if (unitSystem === "metric") {
          if (value >= 1) return roundTo(value, 2) + " L";
          return roundTo(value * 1000, 1) + " mL";
        } else {
          // value in gallons
          if (value >= 1) return roundTo(value, 2) + " gal";
          return roundTo(value * 128, 1) + " fl oz";
        }
      }

      function updateUnits() {
        const unitSystem = document.getElementById("unitSystem").value;
        const batchUnit = document.getElementById("batchVolumeUnit");
        const cementUnit = document.getElementById("cementContentUnit");
        const perCementUnit = document.getElementById("dosePerCementUnit");
        const perCementUnit2 = document.getElementById("dosePerCementUnit2");
        const perVolumeUnit = document.getElementById("dosePerVolumeUnit");
        const perVolumeUnit2 = document.getElementById("dosePerVolumeUnit2");

        if (unitSystem === "metric") {
          batchUnit.textContent = "m³";
          cementUnit.textContent = "kg/m³";
          perCementUnit.textContent = "mL / 100 kg";
          perCementUnit2.textContent = "mL / 100 kg";
          perVolumeUnit.textContent = "L / m³";
          perVolumeUnit2.textContent = "L / m³";
          if (
            parseFloat(
              document.getElementById("cementContent").value || "0"
            ) === 564
          ) {
            document.getElementById("cementContent").value = 350;
          }
        } else {
          batchUnit.textContent = "yd³";
          cementUnit.textContent = "lb/yd³";
          perCementUnit.textContent = "fl oz / 100 lb";
          perCementUnit2.textContent = "fl oz / 100 lb";
          perVolumeUnit.textContent = "fl oz / yd³";
          perVolumeUnit2.textContent = "fl oz / yd³";
          if (
            parseFloat(
              document.getElementById("cementContent").value || "0"
            ) === 350
          ) {
            document.getElementById("cementContent").value = 564;
          }
        }
      }

      function updateAdmixtureHint() {
        const type = document.getElementById("admixtureType").value;
        const hint = document.getElementById("admixtureHint");
        const doseMode = document.querySelector(
          'input[name="doseMode"]:checked'
        ).value;

        let text = "";
        switch (type) {
          case "waterReducer":
            text =
              "Typical normal water reducer dosage: 0.3–0.8% bwoc or 200–600 mL per 100 kg cement.";
            break;
          case "midRange":
            text = "Typical mid-range water reducer dosage: 0.5–1.5% bwoc.";
            break;
          case "superplasticizer":
            text =
              "Typical superplasticizer dosage: 0.8–2.0% bwoc (check product datasheet).";
            break;
          case "airEntrainer":
            text =
              "Typical air entrainer dosage: 0.01–0.1% bwoc or 50–500 mL per 100 kg cement.";
            break;
          case "accelerator":
            text =
              "Typical accelerator dosage: 0.5–2.0% bwoc (varies with temperature and product).";
            break;
          case "retarder":
            text =
              "Typical retarder dosage: 0.5–2.0% bwoc (follow manufacturer guidance).";
            break;
          case "shrinkageReducer":
            text =
              "Typical shrinkage-reducing admixture dosage: 0.5–2.0% bwoc or 5–15 L/m³.";
            break;
          case "corrosionInhibitor":
            text =
              "Typical corrosion inhibitor dosage is often given per m³ of concrete (e.g., 10–30 L/m³).";
            break;
          case "fiber":
            text =
              "Typical fiber dosage: 0.5–1.5 kg/m³ (0.3–0.9 lb/yd³) depending on fiber type.";
            break;
          default:
            text =
              "Enter the dosage range exactly as specified on the admixture datasheet.";
        }
        hint.textContent = text;

        // Optionally pre-fill dosage fields for convenience (non-binding)
        const percentMin = document.getElementById("dosePercentMin");
        const percentMax = document.getElementById("dosePercentMax");
        const perCMin = document.getElementById("dosePerCementMin");
        const perCMax = document.getElementById("dosePerCementMax");
        const perVMin = document.getElementById("dosePerVolumeMin");
        const perVMax = document.getElementById("dosePerVolumeMax");

        if (doseMode === "percent") {
          if (type === "waterReducer") {
            percentMin.value = 0.3;
            percentMax.value = 0.8;
          } else if (type === "midRange") {
            percentMin.value = 0.5;
            percentMax.value = 1.5;
          } else if (type === "superplasticizer") {
            percentMin.value = 0.8;
            percentMax.value = 2.0;
          } else if (type === "airEntrainer") {
            percentMin.value = 0.01;
            percentMax.value = 0.1;
          } else if (type === "accelerator" || type === "retarder") {
            percentMin.value = 0.5;
            percentMax.value = 2.0;
          } else if (type === "shrinkageReducer") {
            percentMin.value = 0.5;
            percentMax.value = 2.0;
          }
        } else if (doseMode === "perCement") {
          const unitSystem = document.getElementById("unitSystem").value;
          if (unitSystem === "metric") {
            if (type === "waterReducer") {
              perCMin.value = 200;
              perCMax.value = 600;
            } else if (type === "airEntrainer") {
              perCMin.value = 50;
              perCMax.value = 500;
            }
          } else {
            if (type === "waterReducer") {
              perCMin.value = 3;
              perCMax.value = 8;
            } else if (type === "airEntrainer") {
              perCMin.value = 1;
              perCMax.value = 5;
            }
          }
        } else if (doseMode === "perVolume") {
          const unitSystem = document.getElementById("unitSystem").value;
          if (unitSystem === "metric") {
            if (type === "shrinkageReducer") {
              perVMin.value = 5;
              perVMax.value = 15;
            }
          } else {
            if (type === "shrinkageReducer") {
              perVMin.value = 60;
              perVMax.value = 200;
            }
          }
        }
      }

      function updateDoseModeVisibility() {
        const mode = document.querySelector(
          'input[name="doseMode"]:checked'
        ).value;
        document
          .getElementById("percentRow")
          .classList.toggle("hidden", mode !== "percent");
        document
          .getElementById("perCementRow")
          .classList.toggle("hidden", mode !== "perCement");
        document
          .getElementById("perVolumeRow")
          .classList.toggle("hidden", mode !== "perVolume");
        updateAdmixtureHint();
      }

      function showError(msg) {
        const box = document.getElementById("errorBox");
        box.textContent = msg;
        box.classList.remove("hidden");
        document.getElementById("resultsSection").classList.add("hidden");
      }

      function clearError() {
        const box = document.getElementById("errorBox");
        box.textContent = "";
        box.classList.add("hidden");
      }

      function calculate() {
        clearError();
        const unitSystem = document.getElementById("unitSystem").value;
        const V = parseFloat(document.getElementById("batchVolume").value);
        const C = parseFloat(document.getElementById("cementContent").value);
        const SG = parseFloat(document.getElementById("specificGravity").value);
        const mode = document.querySelector(
          'input[name="doseMode"]:checked'
        ).value;

        if (!isFinite(V) || V <= 0)
          return showError(
            "Please enter a valid batch volume greater than zero."
          );
        if (!isFinite(C) || C <= 0)
          return showError(
            "Please enter a valid cement content per unit volume."
          );

        let perUnitMinMass = NaN;
        let perUnitMaxMass = NaN;
        let perUnitMinVol = NaN;
        let perUnitMaxVol = NaN;

        if (mode === "percent") {
          const pMin = parseFloat(
            document.getElementById("dosePercentMin").value
          );
          const pMax = parseFloat(
            document.getElementById("dosePercentMax").value
          );
          if (
            !isFinite(pMin) ||
            !isFinite(pMax) ||
            pMin < 0 ||
            pMax <= 0 ||
            pMax < pMin
          ) {
            return showError(
              "Please enter a valid percentage dosage range (min ≤ max, both ≥ 0)."
            );
          }
          perUnitMinMass = C * (pMin / 100.0);
          perUnitMaxMass = C * (pMax / 100.0);
        } else if (mode === "perCement") {
          const dMin = parseFloat(
            document.getElementById("dosePerCementMin").value
          );
          const dMax = parseFloat(
            document.getElementById("dosePerCementMax").value
          );
          if (
            !isFinite(dMin) ||
            !isFinite(dMax) ||
            dMin < 0 ||
            dMax <= 0 ||
            dMax < dMin
          ) {
            return showError(
              "Please enter a valid dosage range per 100 kg / 100 lb of cement (min ≤ max, both ≥ 0)."
            );
          }
          const factor = C / 100.0;
          if (unitSystem === "metric") {
            // dMin, dMax in mL per 100 kg -> L per m³
            perUnitMinVol = (dMin * factor) / 1000.0;
            perUnitMaxVol = (dMax * factor) / 1000.0;
          } else {
            // dMin, dMax in fl oz per 100 lb -> gallons per yd³
            const flOzPerYd3Min = dMin * factor;
            const flOzPerYd3Max = dMax * factor;
            perUnitMinVol = flOzPerYd3Min / 128.0;
            perUnitMaxVol = flOzPerYd3Max / 128.0;
          }
          if (isFinite(SG) && SG > 0) {
            if (unitSystem === "metric") {
              const rho = SG * 1.0; // kg/L
              perUnitMinMass = perUnitMinVol * rho;
              perUnitMaxMass = perUnitMaxVol * rho;
            } else {
              const rho = SG * 8.34; // lb/gal
              perUnitMinMass = perUnitMinVol * rho;
              perUnitMaxMass = perUnitMaxVol * rho;
            }
          }
        } else if (mode === "perVolume") {
          const vMin = parseFloat(
            document.getElementById("dosePerVolumeMin").value
          );
          const vMax = parseFloat(
            document.getElementById("dosePerVolumeMax").value
          );
          if (
            !isFinite(vMin) ||
            !isFinite(vMax) ||
            vMin < 0 ||
            vMax <= 0 ||
            vMax < vMin
          ) {
            return showError(
              "Please enter a valid dosage range per m³ / yd³ of concrete (min ≤ max, both ≥ 0)."
            );
          }
          if (unitSystem === "metric") {
            // L per m³
            perUnitMinVol = vMin;
            perUnitMaxVol = vMax;
          } else {
            // fl oz per yd³ -> gallons per yd³
            perUnitMinVol = vMin / 128.0;
            perUnitMaxVol = vMax / 128.0;
          }
          if (isFinite(SG) && SG > 0) {
            if (unitSystem === "metric") {
              const rho = SG * 1.0; // kg/L
              perUnitMinMass = perUnitMinVol * rho;
              perUnitMaxMass = perUnitMaxVol * rho;
            } else {
              const rho = SG * 8.34; // lb/gal
              perUnitMinMass = perUnitMinVol * rho;
              perUnitMaxMass = perUnitMaxVol * rho;
            }
          }
        }

        // If we only have mass (percent mode), compute volume if SG is known
        if (mode === "percent" && isFinite(SG) && SG > 0) {
          if (unitSystem === "metric") {
            const rho = SG * 1.0; // kg/L
            perUnitMinVol = perUnitMinMass / rho;
            perUnitMaxVol = perUnitMaxMass / rho;
          } else {
            const rho = SG * 8.34; // lb/gal
            perUnitMinVol = perUnitMinMass / rho;
            perUnitMaxVol = perUnitMaxMass / rho;
          }
        }

        const batchMinMass = isFinite(perUnitMinMass)
          ? perUnitMinMass * V
          : NaN;
        const batchMaxMass = isFinite(perUnitMaxMass)
          ? perUnitMaxMass * V
          : NaN;
        const batchMinVol = isFinite(perUnitMinVol) ? perUnitMinVol * V : NaN;
        const batchMaxVol = isFinite(perUnitMaxVol) ? perUnitMaxVol * V : NaN;

        document.getElementById("perUnitMinMass").textContent = formatMass(
          perUnitMinMass,
          unitSystem
        );
        document.getElementById("perUnitMaxMass").textContent = formatMass(
          perUnitMaxMass,
          unitSystem
        );
        document.getElementById("batchMinMass").textContent = formatMass(
          batchMinMass,
          unitSystem
        );
        document.getElementById("batchMaxMass").textContent = formatMass(
          batchMaxMass,
          unitSystem
        );

        document.getElementById("perUnitMinVol").textContent = isFinite(
          perUnitMinVol
        )
          ? formatVolume(perUnitMinVol, unitSystem)
          : "– (SG required)";
        document.getElementById("perUnitMaxVol").textContent = isFinite(
          perUnitMaxVol
        )
          ? formatVolume(perUnitMaxVol, unitSystem)
          : "– (SG required)";
        document.getElementById("batchMinVol").textContent = isFinite(
          batchMinVol
        )
          ? formatVolume(batchMinVol, unitSystem)
          : "– (SG required)";
        document.getElementById("batchMaxVol").textContent = isFinite(
          batchMaxVol
        )
          ? formatVolume(batchMaxVol, unitSystem)
          : "– (SG required)";

        document.getElementById("resultsSection").classList.remove("hidden");
      }

      document.addEventListener("DOMContentLoaded", function () {
        updateUnits();
        updateDoseModeVisibility();
        updateAdmixtureHint();

        document
          .getElementById("unitSystem")
          .addEventListener("change", function () {
            updateUnits();
            updateAdmixtureHint();
          });

        document
          .getElementById("admixtureType")
          .addEventListener("change", updateAdmixtureHint);

        document.querySelectorAll(".dose-mode").forEach(function (el) {
          el.addEventListener("change", updateDoseModeVisibility);
        });

        document
          .getElementById("calculateBtn")
          .addEventListener("click", function (e) {
            e.preventDefault();
            calculate();
          });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
