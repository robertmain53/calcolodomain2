    <script>
      // Mode toggle
      const modeTimeBtn = document.getElementById("mode-time");
      const modeCountBtn = document.getElementById("mode-count");
      const timeSection = document.getElementById("time-section");
      const countSection = document.getElementById("count-section");

      modeTimeBtn.addEventListener("click", () => {
        modeTimeBtn.classList.add("bg-white", "shadow-sm", "text-blue-600");
        modeCountBtn.classList.remove("bg-white", "shadow-sm", "text-blue-600");
        modeCountBtn.classList.add("text-gray-600");
        timeSection.classList.remove("hidden");
        countSection.classList.add("hidden");
      });

      modeCountBtn.addEventListener("click", () => {
        modeCountBtn.classList.add("bg-white", "shadow-sm", "text-blue-600");
        modeTimeBtn.classList.remove("bg-white", "shadow-sm", "text-blue-600");
        modeTimeBtn.classList.add("text-gray-600");
        countSection.classList.remove("hidden");
        timeSection.classList.add("hidden");
      });

      function toMinutes(value, unit) {
        if (!isFinite(value) || value < 0) return 0;
        return unit === "hr" ? value * 60 : value;
      }

      function cycleTimeToMinutes(value, unit) {
        if (!isFinite(value) || value <= 0) return 0;
        return unit === "sec" ? value / 60 : value;
      }

      function rateToPerMinute(value, unit) {
        if (!isFinite(value) || value <= 0) return 0;
        return unit === "uph" ? value / 60 : value;
      }

      function formatPercent(x) {
        if (!isFinite(x)) return "0%";
        return (x * 100).toFixed(1).replace(/\.0$/, "") + "%";
      }

      function formatMinutes(mins) {
        if (!isFinite(mins)) return "-";
        if (mins === 0) return "0 min";
        if (mins < 60) return mins.toFixed(1).replace(/\.0$/, "") + " min";
        const h = Math.floor(mins / 60);
        const m = mins % 60;
        if (m < 0.5) return h + " h";
        return h + " h " + m.toFixed(0) + " min";
      }

      function getOeeRating(oee) {
        const pct = oee * 100;
        if (pct >= 85) return "World-class OEE";
        if (pct >= 75) return "Strong performance with room for improvement";
        if (pct >= 60) return "Average – significant improvement potential";
        if (pct > 0) return "Low – large improvement opportunity";
        return "";
      }

      document.getElementById("calculateBtn").addEventListener("click", () => {
        const usingTime = !timeSection.classList.contains("hidden");

        let plannedMin,
          runMin,
          downtimeMin,
          idealCycleMin,
          totalUnits,
          goodUnits;

        if (usingTime) {
          const planned = parseFloat(
            document.getElementById("plannedTime").value
          );
          const plannedUnit = document.getElementById("plannedTimeUnit").value;
          const downtime = parseFloat(
            document.getElementById("downtime").value
          );
          const downtimeUnit = document.getElementById("downtimeUnit").value;
          const idealCycle = parseFloat(
            document.getElementById("idealCycleTime").value
          );
          const idealCycleUnit =
            document.getElementById("idealCycleTimeUnit").value;
          totalUnits = parseFloat(
            document.getElementById("totalUnitsTime").value
          );
          goodUnits = parseFloat(
            document.getElementById("goodUnitsTime").value
          );

          plannedMin = toMinutes(planned, plannedUnit);
          downtimeMin = toMinutes(downtime, downtimeUnit);
          runMin = Math.max(plannedMin - downtimeMin, 0);
          idealCycleMin = cycleTimeToMinutes(idealCycle, idealCycleUnit);
        } else {
          goodUnits = parseFloat(
            document.getElementById("goodUnitsCount").value
          );
          totalUnits = parseFloat(
            document.getElementById("totalUnitsCount").value
          );
          const run = parseFloat(document.getElementById("runTimeCount").value);
          const runUnit = document.getElementById("runTimeCountUnit").value;
          const planned = parseFloat(
            document.getElementById("plannedTimeCount").value
          );
          const plannedUnit = document.getElementById(
            "plannedTimeCountUnit"
          ).value;
          const idealRate = parseFloat(
            document.getElementById("idealRateCount").value
          );
          const idealRateUnit =
            document.getElementById("idealRateCountUnit").value;

          runMin = toMinutes(run, runUnit);
          plannedMin = toMinutes(planned, plannedUnit);
          downtimeMin = Math.max(plannedMin - runMin, 0);
          const idealPerMin = rateToPerMinute(idealRate, idealRateUnit);
          idealCycleMin = idealPerMin > 0 ? 1 / idealPerMin : 0;
        }

        // Basic validation
        if (
          !plannedMin ||
          plannedMin <= 0 ||
          !runMin ||
          runMin <= 0 ||
          !idealCycleMin ||
          idealCycleMin <= 0 ||
          !totalUnits ||
          totalUnits <= 0 ||
          !goodUnits ||
          goodUnits < 0 ||
          goodUnits > totalUnits
        ) {
          alert(
            "Please enter valid, positive values for time, cycle time, and units. Good units must be between 0 and total units."
          );
          return;
        }

        const availability = Math.min(runMin / plannedMin, 1);
        const perfNumerator = idealCycleMin * totalUnits;
        const performance = Math.min(perfNumerator / runMin, 1);
        const quality = Math.min(goodUnits / totalUnits, 1);

        const oee = availability * performance * quality;

        // Update results
        document.getElementById("oeeValue").textContent = formatPercent(oee);
        document.getElementById("availabilityValue").textContent =
          formatPercent(availability);
        document.getElementById("performanceValue").textContent =
          formatPercent(performance);
        document.getElementById("qualityValue").textContent =
          formatPercent(quality);
        document.getElementById("oeeRating").textContent = getOeeRating(oee);

        document.getElementById("summaryPlanned").textContent =
          formatMinutes(plannedMin);
        document.getElementById("summaryRun").textContent =
          formatMinutes(runMin);
        document.getElementById("summaryDowntime").textContent =
          formatMinutes(downtimeMin);
        document.getElementById("summaryCycle").textContent =
          idealCycleMin > 0
            ? (idealCycleMin * 60).toFixed(2).replace(/\.00$/, "") + " sec/unit"
            : "-";
        document.getElementById("summaryTotalUnits").textContent = isFinite(
          totalUnits
        )
          ? totalUnits.toFixed(0)
          : "-";
        document.getElementById("summaryGoodUnits").textContent = isFinite(
          goodUnits
        )
          ? goodUnits.toFixed(0)
          : "-";

        document.getElementById("lossAvailability").textContent =
          1 - availability > 0
            ? formatPercent(1 - availability) + " lost vs. 100% uptime"
            : "0% loss";
        document.getElementById("lossPerformance").textContent =
          1 - performance > 0
            ? formatPercent(1 - performance) + " lost vs. ideal speed"
            : "0% loss";
        document.getElementById("lossQuality").textContent =
          1 - quality > 0
            ? formatPercent(1 - quality) + " lost due to defects"
            : "0% loss";

        document.getElementById("resultsSection").classList.remove("hidden");
      });

      document.getElementById("resetBtn").addEventListener("click", () => {
        document
          .querySelectorAll('input[type="number"]')
          .forEach((el) => (el.value = ""));
        document.getElementById("resultsSection").classList.add("hidden");
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
