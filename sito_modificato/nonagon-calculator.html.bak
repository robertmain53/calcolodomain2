    <script>
      // Utility: format numbers nicely
      function formatNumber(val, decimals = 6) {
        if (!isFinite(val)) return "–";
        if (Math.abs(val) >= 1) {
          return val.toFixed(3).replace(/\.?0+$/, "");
        }
        return val.toFixed(decimals).replace(/\.?0+$/, "");
      }

      function updateUnitLabels() {
        const unit = document.getElementById("unit-select").value;
        const linear = unit;
        const area = unit + "\u00B2";

        document.getElementById("side-unit-label").textContent = linear;
        document.getElementById("perimeter-unit-label").textContent = linear;
        document.getElementById("r-unit-label").textContent = linear;
        document.getElementById("a-unit-label").textContent = linear;
        document.getElementById("area-unit-label").textContent = area;
      }

      function clearResults() {
        document.getElementById("res-side").textContent = "–";
        document.getElementById("res-perimeter").textContent = "–";
        document.getElementById("res-area").textContent = "–";
        document.getElementById("res-r").textContent = "–";
        document.getElementById("res-a").textContent = "–";
        document.getElementById("res-central-angle").textContent = "40°";
        document.getElementById("res-int-angle").textContent = "140°";
        document.getElementById("res-ext-angle").textContent = "40°";
      }

      function calculateNonagon() {
        const errEl = document.getElementById("error-msg");
        errEl.textContent = "";

        const sVal = parseFloat(document.getElementById("side-input").value);
        const pVal = parseFloat(
          document.getElementById("perimeter-input").value
        );
        const aVal = parseFloat(document.getElementById("area-input").value);
        const rVal = parseFloat(document.getElementById("r-input").value);
        const inrVal = parseFloat(document.getElementById("a-input").value);
        const thetaVal = parseFloat(
          document.getElementById("central-angle-input").value
        );

        const hasS = !isNaN(sVal) && sVal > 0;
        const hasP = !isNaN(pVal) && pVal > 0;
        const hasA = !isNaN(aVal) && aVal > 0;
        const hasR = !isNaN(rVal) && rVal > 0;
        const hasInr = !isNaN(inrVal) && inrVal > 0;
        const hasTheta = !isNaN(thetaVal) && thetaVal > 0;

        const count =
          (hasS ? 1 : 0) +
          (hasP ? 1 : 0) +
          (hasA ? 1 : 0) +
          (hasR ? 1 : 0) +
          (hasInr ? 1 : 0) +
          (hasTheta ? 1 : 0);

        if (count === 0) {
          errEl.textContent =
            "Enter at least one value to calculate the nonagon.";
          clearResults();
          return;
        }

        // Determine side length s from first available input in priority order
        const n = 9;
        const piOverN = Math.PI / n;
        let s;

        if (hasS) {
          s = sVal;
        } else if (hasP) {
          s = pVal / n;
        } else if (hasA) {
          // A = n s^2 / (4 tan(pi/n)) => s = sqrt(4A tan(pi/n) / n)
          s = Math.sqrt((4 * aVal * Math.tan(piOverN)) / n);
        } else if (hasR) {
          // s = 2 R sin(pi/n)
          s = 2 * rVal * Math.sin(piOverN);
        } else if (hasInr) {
          // s = 2 a tan(pi/n)
          s = 2 * inrVal * Math.tan(piOverN);
        } else if (hasTheta) {
          // For a regular nonagon, theta should be 360/9 = 40°
          const expected = 360 / n;
          if (Math.abs(thetaVal - expected) > 1e-6) {
            errEl.textContent =
              "For a regular nonagon, the central angle must be 40°. Using 40° for calculations.";
          }
          // Central angle alone doesn't set scale, so we cannot determine side length from it.
          // Inform user and stop.
          if (!hasS && !hasP && !hasA && !hasR && !hasInr) {
            clearResults();
            document.getElementById("res-central-angle").textContent = "40°";
            document.getElementById("res-int-angle").textContent = "140°";
            document.getElementById("res-ext-angle").textContent = "40°";
            return;
          }
        }

        if (!isFinite(s) || s <= 0) {
          errEl.textContent =
            "Could not compute a valid side length from the given input. Please check your values.";
          clearResults();
          return;
        }

        const unit = document.getElementById("unit-select").value;
        const linearUnit = unit;
        const areaUnit = unit + "\u00B2";

        const P = n * s;
        const R = s / (2 * Math.sin(piOverN));
        const inr = s / (2 * Math.tan(piOverN));
        const A = (n * s * s) / (4 * Math.tan(piOverN));

        document.getElementById("res-side").textContent = `${formatNumber(
          s
        )} ${linearUnit}`;
        document.getElementById("res-perimeter").textContent = `${formatNumber(
          P
        )} ${linearUnit}`;
        document.getElementById("res-area").textContent = `${formatNumber(
          A
        )} ${areaUnit}`;
        document.getElementById("res-r").textContent = `${formatNumber(
          R
        )} ${linearUnit}`;
        document.getElementById("res-a").textContent = `${formatNumber(
          inr
        )} ${linearUnit}`;
        document.getElementById(
          "res-central-angle"
        ).textContent = `${formatNumber(360 / n)}°`;
        document.getElementById("res-int-angle").textContent = `${formatNumber(
          ((n - 2) * 180) / n
        )}°`;
        document.getElementById("res-ext-angle").textContent = `${formatNumber(
          360 / n
        )}°`;
      }

      document.addEventListener("DOMContentLoaded", function () {
        updateUnitLabels();
        clearResults();

        document
          .getElementById("unit-select")
          .addEventListener("change", updateUnitLabels);
        document
          .getElementById("calc-btn")
          .addEventListener("click", calculateNonagon);
        document
          .getElementById("clear-btn")
          .addEventListener("click", function () {
            document.getElementById("nonagon-form").reset();
            document.getElementById("error-msg").textContent = "";
            updateUnitLabels();
            clearResults();
          });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
