    <script>
      // Utility conversions
      function toFeet(value, unit) {
        if (unit === "ft") return value;
        if (unit === "m") return value * 3.28084;
        if (unit === "in") return value / 12;
        if (unit === "cm") return value / 30.48;
        return value;
      }
      function toFeetFromLengthUnit(value, unit) {
        return toFeet(value, unit);
      }
      function toCubicFeetFromCyl(diameter, diameterUnit, depth, depthUnit) {
        const dFt = toFeet(diameter, diameterUnit);
        const hFt = toFeet(depth, depthUnit);
        const radius = dFt / 2;
        return Math.PI * radius * radius * hFt;
      }

      // Layout & build mode toggles
      const layoutButtons = document.querySelectorAll(".layout-mode-btn");
      const buildButtons = document.querySelectorAll(".build-mode-btn");
      const singleWrapper = document.getElementById("single-length-wrapper");
      const segmentsWrapper = document.getElementById("segments-wrapper");
      const panelInputs = document.getElementById("panel-inputs");
      const boardInputs = document.getElementById("board-inputs");

      let layoutMode = "single";
      let buildMode = "panels";

      layoutButtons.forEach((btn) => {
        btn.addEventListener("click", () => {
          layoutMode = btn.dataset.layoutMode;
          layoutButtons.forEach((b) => {
            b.classList.remove(
              "border-blue-600",
              "text-blue-700",
              "bg-blue-50"
            );
            b.classList.add("border-gray-300", "text-gray-700", "bg-white");
          });
          btn.classList.add("border-blue-600", "text-blue-700", "bg-blue-50");
          btn.classList.remove("border-gray-300", "text-gray-700", "bg-white");

          if (layoutMode === "single") {
            singleWrapper.classList.remove("hidden");
            segmentsWrapper.classList.add("hidden");
          } else {
            singleWrapper.classList.add("hidden");
            segmentsWrapper.classList.remove("hidden");
            if (!document.querySelector(".segment-row")) {
              addSegmentRow();
            }
          }
        });
      });

      buildButtons.forEach((btn) => {
        btn.addEventListener("click", () => {
          buildMode = btn.dataset.buildMode;
          buildButtons.forEach((b) => {
            b.classList.remove(
              "border-blue-600",
              "text-blue-700",
              "bg-blue-50"
            );
            b.classList.add("border-gray-300", "text-gray-700", "bg-white");
          });
          btn.classList.add("border-blue-600", "text-blue-700", "bg-blue-50");
          btn.classList.remove("border-gray-300", "text-gray-700", "bg-white");

          if (buildMode === "panels") {
            panelInputs.classList.remove("hidden");
            boardInputs.classList.add("hidden");
          } else {
            panelInputs.classList.add("hidden");
            boardInputs.classList.remove("hidden");
          }
        });
      });

      // Segments handling
      const segmentsContainer = document.getElementById("segments-container");
      const addSegmentBtn = document.getElementById("add-segment");
      const resetSegmentsBtn = document.getElementById("reset-segments");

      function addSegmentRow(length = "") {
        const unit = document.getElementById("segments-length-unit").value;
        const row = document.createElement("div");
        row.className = "segment-row flex items-center gap-2";
        row.innerHTML = `
      <span class="text-xs text-gray-500 w-10">Seg</span>
      <input type="number" min="0" step="0.01" value="${length}" class="flex-1 border border-gray-300 rounded-md px-3 py-1.5" placeholder="Length">
      <span class="text-sm text-gray-700">${unit}</span>
      <button type="button" class="remove-segment text-xs text-red-600 hover:underline">Remove</button>
    `;
        segmentsContainer.appendChild(row);
        row.querySelector(".remove-segment").addEventListener("click", () => {
          row.remove();
        });
      }

      addSegmentBtn.addEventListener("click", () => addSegmentRow());
      resetSegmentsBtn.addEventListener("click", () => {
        segmentsContainer.innerHTML = "";
      });

      document
        .getElementById("segments-length-unit")
        .addEventListener("change", (e) => {
          const unit = e.target.value;
          segmentsContainer
            .querySelectorAll(".segment-row span.text-sm")
            .forEach((span) => {
              span.textContent = unit;
            });
        });

      // Gates handling
      const gatesContainer = document.getElementById("gates-container");
      const addGateBtn = document.getElementById("add-gate");

      function addGateRow(width = "") {
        const row = document.createElement("div");
        row.className = "gate-row flex items-center gap-2";
        row.innerHTML = `
      <span class="text-xs text-gray-500 w-10">Gate</span>
      <input type="number" min="0" step="0.01" value="${width}" class="flex-1 border border-gray-300 rounded-md px-3 py-1.5" placeholder="Width">
      <select class="border border-gray-300 rounded-md px-2 py-1.5 text-sm">
        <option value="ft">ft</option>
        <option value="m">m</option>
      </select>
      <button type="button" class="remove-gate text-xs text-red-600 hover:underline">Remove</button>
    `;
        gatesContainer.appendChild(row);
        row
          .querySelector(".remove-gate")
          .addEventListener("click", () => row.remove());
      }

      addGateBtn.addEventListener("click", () => addGateRow());

      // Calculation
      const calcBtn = document.getElementById("calculate-btn");
      const resetBtn = document.getElementById("reset-btn");
      const errorEl = document.getElementById("calc-error");
      const resultsSection = document.getElementById("results-section");
      const resultsSummary = document.getElementById("results-summary");
      const resultsTableBody = document.getElementById("results-table-body");
      const resultsNotes = document.getElementById("results-notes");

      calcBtn.addEventListener("click", () => {
        errorEl.textContent = "";
        resultsSection.classList.add("hidden");
        resultsTableBody.innerHTML = "";
        resultsNotes.textContent = "";

        let totalLengthFt = 0;

        if (layoutMode === "single") {
          const lenVal = parseFloat(
            document.getElementById("total-length").value
          );
          const lenUnit = document.getElementById("length-unit").value;
          if (isNaN(lenVal) || lenVal <= 0) {
            errorEl.textContent = "Enter a valid total fence length.";
            return;
          }
          totalLengthFt = toFeetFromLengthUnit(lenVal, lenUnit);
        } else {
          const unit = document.getElementById("segments-length-unit").value;
          const rows = document.querySelectorAll(".segment-row");
          if (!rows.length) {
            errorEl.textContent = "Add at least one fence segment.";
            return;
          }
          let sum = 0;
          for (const row of rows) {
            const input = row.querySelector("input");
            const v = parseFloat(input.value);
            if (isNaN(v) || v <= 0) {
              errorEl.textContent = "Each segment must have a positive length.";
              return;
            }
            sum += toFeetFromLengthUnit(v, unit);
          }
          totalLengthFt = sum;
        }

        if (totalLengthFt <= 0) {
          errorEl.textContent = "Total fence length must be greater than zero.";
          return;
        }

        // Gates
        const gateRows = document.querySelectorAll(".gate-row");
        let gateCount = gateRows.length;

        // Panels or bays
        let baysOrPanels = 0;
        let baseItems = {};

        if (buildMode === "panels") {
          const wVal = parseFloat(document.getElementById("panel-width").value);
          const wUnit = document.getElementById("panel-width-unit").value;
          if (isNaN(wVal) || wVal <= 0) {
            errorEl.textContent = "Enter a valid panel width.";
            return;
          }
          const panelWidthFt = toFeetFromLengthUnit(wVal, wUnit);
          baysOrPanels = Math.ceil(totalLengthFt / panelWidthFt);
          if (baysOrPanels < 1) baysOrPanels = 1;
          baseItems["Fence panels"] = baysOrPanels;
        } else {
          const spacingVal = parseFloat(
            document.getElementById("post-spacing").value
          );
          const spacingUnit =
            document.getElementById("post-spacing-unit").value;
          if (isNaN(spacingVal) || spacingVal <= 0) {
            errorEl.textContent = "Enter a valid post spacing.";
            return;
          }
          const spacingFt = toFeetFromLengthUnit(spacingVal, spacingUnit);
          baysOrPanels = Math.ceil(totalLengthFt / spacingFt);
          if (baysOrPanels < 1) baysOrPanels = 1;
          baseItems["Bays (post-to-post spans)"] = baysOrPanels;

          const railsPerBay = parseInt(
            document.getElementById("rails-per-bay").value,
            10
          );
          if (!isNaN(railsPerBay) && railsPerBay > 0) {
            baseItems["Rails"] = baysOrPanels * railsPerBay;
          }

          const boardWidthVal = parseFloat(
            document.getElementById("board-width").value
          );
          if (!isNaN(boardWidthVal) && boardWidthVal > 0) {
            const boardUnit = document.getElementById("board-width-unit").value;
            const boardWidthFt = toFeet(boardWidthVal, boardUnit);
            const boards = Math.ceil(totalLengthFt / boardWidthFt);
            baseItems["Boards / pickets (solid fence)"] = boards;
          }
        }

        // Posts
        let linePosts = baysOrPanels + 1;
        let gatePosts = gateCount * 2;
        let totalPosts = linePosts + gatePosts;
        baseItems["Line posts"] = linePosts;
        if (gatePosts > 0) baseItems["Gate posts"] = gatePosts;
        baseItems["Total posts"] = totalPosts;

        // Concrete
        const postHeightVal = parseFloat(
          document.getElementById("post-height").value
        );
        const postHeightUnit =
          document.getElementById("post-height-unit").value;
        const burialVal = parseFloat(
          document.getElementById("post-burial").value
        );
        const burialUnit = document.getElementById("post-burial-unit").value;
        const footingDiaVal = parseFloat(
          document.getElementById("footing-diameter").value
        );
        const footingDiaUnit = document.getElementById(
          "footing-diameter-unit"
        ).value;
        const bagSizeFt3 = parseFloat(
          document.getElementById("bag-size").value
        );

        if (
          !isNaN(burialVal) &&
          burialVal > 0 &&
          !isNaN(footingDiaVal) &&
          footingDiaVal > 0
        ) {
          const volPerPost = toCubicFeetFromCyl(
            footingDiaVal,
            footingDiaUnit,
            burialVal,
            burialUnit
          );
          const totalVol = volPerPost * totalPosts;
          const bags = Math.ceil(totalVol / bagSizeFt3);
          baseItems["Concrete volume (ft³)"] = parseFloat(totalVol.toFixed(2));
          baseItems["Concrete bags"] = bags;
        }

        // Waste
        const wastePercent =
          parseFloat(document.getElementById("waste-percent").value) || 0;

        // Build results table
        for (const [label, qty] of Object.entries(baseItems)) {
          const baseQty = qty;
          let withWaste = baseQty;
          if (
            typeof baseQty === "number" &&
            !isNaN(baseQty) &&
            wastePercent > 0 &&
            !label.includes("volume")
          ) {
            withWaste = Math.ceil(baseQty * (1 + wastePercent / 100));
          }
          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="border-b border-gray-100">${label}</td>
        <td class="border-b border-gray-100 text-right">${
          typeof baseQty === "number"
            ? baseQty.toLocaleString(undefined, { maximumFractionDigits: 2 })
            : baseQty
        }</td>
        <td class="border-b border-gray-100 text-right">${
          wastePercent > 0 && !label.includes("volume")
            ? withWaste.toLocaleString()
            : "—"
        }</td>
      `;
          resultsTableBody.appendChild(tr);
        }

        const totalLengthDisplay = totalLengthFt.toLocaleString(undefined, {
          maximumFractionDigits: 1,
        });
        resultsSummary.textContent = `Based on a total fence length of about ${totalLengthDisplay} ft, here is an estimated material list for your fence.`;

        const notes = [];
        if (gateCount > 0) {
          notes.push(
            `You entered ${gateCount} gate${
              gateCount > 1 ? "s" : ""
            }; each gate uses two gate posts.`
          );
        }
        if (
          !isNaN(postHeightVal) &&
          postHeightVal > 0 &&
          !isNaN(burialVal) &&
          burialVal > 0
        ) {
          const above = toFeet(postHeightVal, postHeightUnit);
          const below = toFeet(burialVal, burialUnit);
          const totalPostLen = above + below;
          notes.push(
            `Total post length per post is about ${totalPostLen.toFixed(
              1
            )} ft (above ground + embed depth).`
          );
        }
        if (wastePercent > 0) {
          notes.push(
            `Waste allowance of ${wastePercent}% has been applied to counts in the “With waste” column.`
          );
        }
        resultsNotes.textContent = notes.join(" ");

        resultsSection.classList.remove("hidden");
      });

      resetBtn.addEventListener("click", () => {
        document.getElementById("total-length").value = "";
        document.getElementById("length-unit").value = "ft";
        document.getElementById("segments-length-unit").value = "ft";
        segmentsContainer.innerHTML = "";
        gatesContainer.innerHTML = "";
        document.getElementById("panel-width").value = "";
        document.getElementById("panel-height").value = "";
        document.getElementById("post-spacing").value = "";
        document.getElementById("rails-per-bay").value = "2";
        document.getElementById("board-width").value = "";
        document.getElementById("post-height").value = "";
        document.getElementById("post-burial").value = "";
        document.getElementById("footing-diameter").value = "";
        document.getElementById("bag-size").value = "0.6";
        document.getElementById("waste-percent").value = "10";
        errorEl.textContent = "";
        resultsSection.classList.add("hidden");
        resultsTableBody.innerHTML = "";
        resultsNotes.textContent = "";
      });

      // Mobile menu script (simple)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", () => {
          const menu = document.getElementById("mobile-menu");
          const expanded = menu.classList.contains("hidden") ? "true" : "false";
          menu.classList.toggle("hidden");
          document
            .getElementById("mobile-menu-toggle")
            .setAttribute("aria-expanded", expanded);
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
