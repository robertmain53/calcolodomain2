    <script>
      // Utility: unit conversions
      function lbToKg(lb) {
        return lb * 0.45359237;
      }

      function roundTo(value, decimals) {
        const factor = Math.pow(10, decimals);
        return Math.round(value * factor) / factor;
      }

      // Default TBW fractions based on patient type and sex
      function getDefaultTbwFraction(patientType, sex) {
        if (patientType === "pediatric") {
          return 0.65; // generic pediatric default
        }
        // adult
        return sex === "male" ? 0.6 : 0.5;
      }

      function updateTbwHint() {
        const patientType = document.getElementById("patientType").value;
        const sex = document.getElementById("sex").value;
        const tbwFraction = parseFloat(
          document.getElementById("tbwFraction").value
        );
        const hintEl = document.getElementById("tbwHint");

        let base = "";
        if (patientType === "pediatric") {
          base = "Typical pediatric TBW ≈ 0.6–0.7 × weight.";
        } else if (sex === "male") {
          base = "Typical adult male TBW ≈ 0.6 × weight.";
        } else {
          base = "Typical adult female TBW ≈ 0.5 × weight.";
        }
        hintEl.textContent =
          base +
          " Current setting: " +
          (isNaN(tbwFraction) ? "–" : tbwFraction.toFixed(2));
      }

      function setDefaultTbwFraction() {
        const patientType = document.getElementById("patientType").value;
        const sex = document.getElementById("sex").value;
        const def = getDefaultTbwFraction(patientType, sex);
        document.getElementById("tbwFraction").value = def.toFixed(2);
        updateTbwHint();
      }

      document.addEventListener("DOMContentLoaded", function () {
        const patientTypeEl = document.getElementById("patientType");
        const sexEl = document.getElementById("sex");
        const resetTbwBtn = document.getElementById("resetTbwFraction");
        const calculateBtn = document.getElementById("calculateBtn");
        const clearBtn = document.getElementById("clearBtn");

        // Initialize TBW fraction
        setDefaultTbwFraction();

        patientTypeEl.addEventListener("change", setDefaultTbwFraction);
        sexEl.addEventListener("change", setDefaultTbwFraction);
        document
          .getElementById("tbwFraction")
          .addEventListener("input", updateTbwHint);
        resetTbwBtn.addEventListener("click", setDefaultTbwFraction);

        clearBtn.addEventListener("click", function () {
          document.getElementById("weight").value = "";
          document.getElementById("weightUnit").value = "kg";
          document.getElementById("serumNa").value = "";
          document.getElementById("targetNa").value = 140;
          document.getElementById("chronicity").value = "unknown";
          document.getElementById("maxCorrection").value = 10;
          setDefaultTbwFraction();
          document.getElementById("resultsSection").classList.add("hidden");
        });

        calculateBtn.addEventListener("click", function () {
          const weightVal = parseFloat(document.getElementById("weight").value);
          const weightUnit = document.getElementById("weightUnit").value;
          const serumNa = parseFloat(document.getElementById("serumNa").value);
          const targetNa = parseFloat(
            document.getElementById("targetNa").value
          );
          const tbwFraction = parseFloat(
            document.getElementById("tbwFraction").value
          );
          const maxCorrection = parseFloat(
            document.getElementById("maxCorrection").value
          );
          const chronicity = document.getElementById("chronicity").value;

          if (!weightVal || weightVal <= 0) {
            alert("Please enter a valid weight.");
            return;
          }
          if (!serumNa || serumNa <= 0) {
            alert("Please enter a valid current serum sodium.");
            return;
          }
          if (!targetNa || targetNa <= 0) {
            alert("Please enter a valid target serum sodium.");
            return;
          }
          if (serumNa <= targetNa) {
            alert(
              "Free water deficit formulas are intended for hypernatremia (serum Na higher than target). Please check your values."
            );
            return;
          }
          if (!tbwFraction || tbwFraction <= 0 || tbwFraction > 0.9) {
            alert(
              "Please enter a reasonable TBW fraction (e.g., 0.6 for adult male, 0.5 for adult female)."
            );
            return;
          }

          let weightKg = weightVal;
          if (weightUnit === "lb") {
            weightKg = lbToKg(weightVal);
          }

          const tbw = weightKg * tbwFraction; // in liters
          const fwdAdrogue = tbw * (serumNa / targetNa - 1);
          const fwdSimple = tbw * (serumNa / 140 - 1);

          // Clamp to non-negative
          const fwdAdrogueClamped = Math.max(0, fwdAdrogue);
          const fwdSimpleClamped = Math.max(0, fwdSimple);

          // Suggested plan based on maxCorrection
          const deltaNa = serumNa - targetNa;
          const maxDelta = Math.max(0, Math.min(deltaNa, maxCorrection));
          let planText = "Unable to suggest plan (check inputs).";

          if (fwdAdrogueClamped > 0 && maxDelta > 0) {
            const fractionOfTotal = maxDelta / deltaNa;
            const volume24h = fwdAdrogueClamped * fractionOfTotal; // L over 24h
            const rateMlHr = (volume24h * 1000) / 24;
            const chronicityNote =
              chronicity === "acute"
                ? "Acute hypernatremia may sometimes be corrected faster, but this should be done in a monitored setting."
                : "For chronic or unknown-duration hypernatremia, many guidelines suggest ≤10–12 mEq/L per 24 h.";

            planText =
              "To lower Na by approximately " +
              roundTo(maxDelta, 1) +
              " mEq/L in 24 h, an initial estimate is about " +
              roundTo(volume24h, 2) +
              " L of free water over 24 h (~" +
              roundTo(rateMlHr, 0) +
              " mL/h), plus replacement of ongoing losses. " +
              chronicityNote;
          }

          // Update UI
          document.getElementById("tbwResult").textContent =
            roundTo(tbw, 2) + " L";
          document.getElementById("tbwResultDetail").textContent =
            "Weight " +
            roundTo(weightKg, 1) +
            " kg × TBW fraction " +
            tbwFraction.toFixed(2);

          document.getElementById("fwdResult").textContent =
            roundTo(fwdAdrogueClamped, 2) + " L";
          document.getElementById("fwdSimpleResult").textContent =
            roundTo(fwdSimpleClamped, 2) + " L";
          document.getElementById("planResult").textContent = planText;

          document.getElementById("resultsSection").classList.remove("hidden");
        });
      });
    </script>

