    <script>
      // Oxygenation Index calculator logic
      function classifyOI(oi) {
        if (!isFinite(oi) || oi <= 0)
          return {
            category: "Invalid",
            interpretation: "Please check your inputs.",
          };

        if (oi < 5) {
          return {
            category: "Normal / near normal",
            interpretation:
              "Oxygenation is adequate on current ventilator settings.",
          };
        } else if (oi < 15) {
          return {
            category: "Mild–moderate impairment",
            interpretation: "Monitor closely and optimize ventilator strategy.",
          };
        } else if (oi < 25) {
          return {
            category: "Moderate–severe respiratory failure",
            interpretation:
              "High ventilatory support; consider rescue strategies (proning, recruitment, HFOV, etc.).",
          };
        } else if (oi < 40) {
          return {
            category: "Severe respiratory failure",
            interpretation:
              "Typical range for ECMO referral in many neonatal/pediatric protocols if persistent.",
          };
        } else {
          return {
            category: "Very severe / refractory",
            interpretation:
              "Strong ECMO consideration in appropriate patients if no contraindications.",
          };
        }
      }

      function calculateOI() {
        const fio2Input = document.getElementById("fio2");
        const fio2Unit = document.getElementById("fio2-unit").value;
        const mapInput = document.getElementById("map");
        const pao2Input = document.getElementById("pao2");
        const errorEl = document.getElementById("oi-error");

        let fio2 = parseFloat(fio2Input.value);
        let map = parseFloat(mapInput.value);
        let pao2 = parseFloat(pao2Input.value);

        errorEl.classList.add("hidden");
        errorEl.textContent = "";

        if (isNaN(fio2) || isNaN(map) || isNaN(pao2)) {
          errorEl.textContent =
            "Please enter numeric values for FiO2, MAP, and PaO2.";
          errorEl.classList.remove("hidden");
          return;
        }

        // Convert FiO2 if entered as percent
        if (fio2Unit === "percent") {
          fio2 = fio2 / 100;
        }

        if (fio2 <= 0 || fio2 > 1.0) {
          errorEl.textContent =
            "FiO2 should be between 0.21 and 1.0 (or 21–100% if using percent).";
          errorEl.classList.remove("hidden");
          return;
        }

        if (map < 0) {
          errorEl.textContent = "MAP cannot be negative.";
          errorEl.classList.remove("hidden");
          return;
        }

        if (pao2 <= 0) {
          errorEl.textContent = "PaO2 must be greater than 0 mmHg.";
          errorEl.classList.remove("hidden");
          return;
        }

        const oi = (fio2 * map * 100) / pao2;
        const oiRounded = Math.round(oi * 10) / 10;

        const { category, interpretation } = classifyOI(oi);

        document.getElementById("oi-value").textContent = oiRounded.toFixed(1);
        document.getElementById("oi-category").textContent = category;
        document.getElementById("oi-interpretation").textContent =
          interpretation;
      }

      document
        .getElementById("calculate-btn")
        .addEventListener("click", calculateOI);

      document.getElementById("reset-btn").addEventListener("click", () => {
        document.getElementById("fio2").value = 0.6;
        document.getElementById("fio2-unit").value = "fraction";
        document.getElementById("map").value = 15;
        document.getElementById("pao2").value = 60;
        document.getElementById("oi-error").classList.add("hidden");
        document.getElementById("oi-error").textContent = "";
        document.getElementById("oi-value").textContent = "–";
        document.getElementById("oi-category").textContent = "–";
        document.getElementById("oi-interpretation").textContent =
          "Enter values to see interpretation.";
      });

      // Auto-calc on input change for better UX
      ["fio2", "map", "pao2", "fio2-unit"].forEach((id) => {
        document.getElementById(id).addEventListener("change", () => {
          // Only auto-calc if fields are non-empty
          if (
            document.getElementById("fio2").value !== "" &&
            document.getElementById("map").value !== "" &&
            document.getElementById("pao2").value !== ""
          ) {
            calculateOI();
          }
        });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
