    <script>
      // Bearing life calculator logic
      (function () {
        const btnBall = document.getElementById("btn-ball");
        const btnRoller = document.getElementById("btn-roller");
        const inputC = document.getElementById("input-C");
        const inputP = document.getElementById("input-P");
        const inputN = document.getElementById("input-n");
        const inputR = document.getElementById("input-R");
        const inputAISO = document.getElementById("input-aISO");
        const inputLifeUnit = document.getElementById("input-life-unit");
        const btnCalc = document.getElementById("btn-calc");
        const btnReset = document.getElementById("btn-reset");
        const errorBox = document.getElementById("error-box");
        const resultsCard = document.getElementById("results-card");
        const resType = document.getElementById("res-type");
        const resL10 = document.getElementById("res-L10");
        const resL10h = document.getElementById("res-L10h");
        const resA1 = document.getElementById("res-a1");
        const resLna = document.getElementById("res-Lna");
        const resLnah = document.getElementById("res-Lnah");
        const resNote = document.getElementById("res-note");

        let bearingType = "ball"; // 'ball' or 'roller'

        function setBearingType(type) {
          bearingType = type;
          if (type === "ball") {
            btnBall.classList.add("bg-blue-600", "text-white");
            btnBall.classList.remove("bg-white", "text-gray-700");
            btnRoller.classList.remove("bg-blue-600", "text-white");
            btnRoller.classList.add("bg-white", "text-gray-700");
          } else {
            btnRoller.classList.add("bg-blue-600", "text-white");
            btnRoller.classList.remove("bg-white", "text-gray-700");
            btnBall.classList.remove("bg-blue-600", "text-white");
            btnBall.classList.add("bg-white", "text-gray-700");
          }
        }

        btnBall.addEventListener("click", () => setBearingType("ball"));
        btnRoller.addEventListener("click", () => setBearingType("roller"));

        function showError(msg) {
          errorBox.textContent = msg;
          errorBox.classList.remove("hidden");
          resultsCard.classList.add("hidden");
        }

        function clearError() {
          errorBox.classList.add("hidden");
          errorBox.textContent = "";
        }

        function interpolateA1(R) {
          // Reliability factor table for ball bearings (ISO/ABMA typical)
          const table = [
            { R: 90, a1: 1.0 },
            { R: 95, a1: 0.62 },
            { R: 96, a1: 0.53 },
            { R: 97, a1: 0.44 },
            { R: 98, a1: 0.33 },
            { R: 99, a1: 0.21 },
          ];
          if (R <= 90) return 1.0;
          if (R >= 99) return 0.21;
          for (let i = 0; i < table.length - 1; i++) {
            const a = table[i];
            const b = table[i + 1];
            if (R >= a.R && R <= b.R) {
              const t = (R - a.R) / (b.R - a.R);
              return a.a1 + t * (b.a1 - a.a1);
            }
          }
          return 1.0;
        }

        function formatMillions(value) {
          if (!isFinite(value)) return "–";
          if (value < 0.01) return value.toExponential(2) + " million rev";
          return (
            value.toLocaleString(undefined, { maximumFractionDigits: 3 }) +
            " million rev"
          );
        }

        function formatHours(value) {
          if (!isFinite(value)) return "–";
          if (value < 1) {
            const minutes = value * 60;
            return (
              minutes.toLocaleString(undefined, { maximumFractionDigits: 1 }) +
              " min"
            );
          }
          if (value > 1e5) {
            const years = value / (24 * 365);
            return (
              years.toLocaleString(undefined, { maximumFractionDigits: 2 }) +
              " years"
            );
          }
          return (
            value.toLocaleString(undefined, { maximumFractionDigits: 1 }) + " h"
          );
        }

        btnCalc.addEventListener("click", () => {
          clearError();

          const C = parseFloat(inputC.value);
          const P = parseFloat(inputP.value);
          const n = parseFloat(inputN.value);
          const R = parseFloat(inputR.value);
          const aISO = parseFloat(inputAISO.value || "1");

          if (!C || C <= 0 || !P || P <= 0) {
            showError(
              "Please enter positive values for dynamic load rating C and equivalent load P (both > 0)."
            );
            return;
          }
          if (!n || n <= 0) {
            showError("Please enter a positive rotational speed n in rpm.");
            return;
          }
          if (!R || R < 50 || R > 99.9) {
            showError(
              "Please enter a reliability between 50% and 99.9%. Typical design values are 90–99%."
            );
            return;
          }
          if (!aISO || aISO <= 0) {
            showError(
              "Application factor aISO must be a positive number (default 1.0)."
            );
            return;
          }

          const p = bearingType === "ball" ? 3.0 : 10.0 / 3.0;
          const L10 = Math.pow(C / P, p); // million rev
          const L10h = (L10 * 1e6) / (60 * n);

          let a1;
          if (R <= 90) {
            a1 = 1.0;
          } else {
            a1 = interpolateA1(R);
          }

          const Lna = a1 * aISO * L10;
          const Lnah = (Lna * 1e6) / (60 * n);

          const typeLabel =
            bearingType === "ball"
              ? "Ball bearing (p = 3)"
              : "Roller bearing (p = 10/3 ≈ 3.33)";

          resType.textContent = typeLabel;
          resL10.textContent = formatMillions(L10);
          resL10h.textContent =
            "≈ " +
            formatHours(L10h) +
            " (basic rating life L10 at 90% reliability)";
          resA1.textContent = a1.toFixed(3) + ` (for R = ${R.toFixed(1)}%)`;
          resLna.textContent = formatMillions(Lna);
          resLnah.textContent =
            "≈ " +
            formatHours(Lnah) +
            ` (modified life Lna with aISO = ${aISO.toFixed(2)})`;

          if (inputLifeUnit.value === "revs") {
            resNote.textContent =
              "Note: Life values above are expressed primarily in millions of revolutions. Use speed n to convert to hours if needed.";
          } else {
            resNote.textContent =
              "Note: Life values above are expressed in both millions of revolutions and approximate operating hours at the specified speed.";
          }

          resultsCard.classList.remove("hidden");
        });

        btnReset.addEventListener("click", () => {
          inputC.value = "";
          inputP.value = "";
          inputN.value = "";
          inputR.value = "90";
          inputAISO.value = "1.0";
          inputLifeUnit.value = "hours";
          setBearingType("ball");
          clearError();
          resultsCard.classList.add("hidden");
        });

        // Initialize default type
        setBearingType("ball");
      })();
    </script>

