    <script>
      function formatCurrency(value) {
        if (isNaN(value)) return "$0";
        const sign = value < 0 ? "-" : "";
        const abs = Math.abs(value);
        return (
          sign +
          abs.toLocaleString("en-US", {
            style: "currency",
            currency: "USD",
            maximumFractionDigits: 0,
          })
        );
      }

      function formatCurrencyExact(value) {
        if (isNaN(value)) return "$0.00";
        return value.toLocaleString("en-US", {
          style: "currency",
          currency: "USD",
          minimumFractionDigits: 2,
          maximumFractionDigits: 2,
        });
      }

      function calcIowaTax2025(taxable) {
        const rate = 0.038; // flat 3.8%
        return Math.max(0, taxable) * rate;
      }

      // Approximate 2024 brackets; thresholds scaled by filing status
      function get2024Brackets(status) {
        // base single thresholds
        let t1 = 6000,
          t2 = 30000;
        let scale = 1;
        if (status === "married_joint") scale = 2;
        if (status === "head") scale = 1.5;
        t1 *= scale;
        t2 *= scale;
        return [
          { upTo: t1, rate: 0.044 },
          { upTo: t2, rate: 0.0482 },
          { upTo: Infinity, rate: 0.057 },
        ];
      }

      function calcIowaTax2024(taxable, status) {
        let tax = 0;
        let remaining = Math.max(0, taxable);
        const brackets = get2024Brackets(status);
        let prevLimit = 0;
        for (const b of brackets) {
          if (remaining <= 0) break;
          const span = Math.min(remaining, b.upTo - prevLimit);
          if (span > 0) {
            tax += span * b.rate;
            remaining -= span;
          }
          prevLimit = b.upTo;
        }
        return tax;
      }

      function getStandardDeduction(status) {
        if (status === "married_joint") return 24000;
        if (status === "head") return 18000;
        return 12000;
      }

      function recalc() {
        const year = document.getElementById("tax-year").value;
        const status = document.getElementById("filing-status").value;
        const grossSlider = document.getElementById("gross-income");
        const grossInput = document.getElementById("gross-income-input");
        let gross = parseFloat(grossInput.value);
        if (isNaN(gross) || gross < 0) gross = 0;

        const pretax = Math.max(
          0,
          parseFloat(document.getElementById("pretax-deductions").value) || 0
        );
        const itemized = Math.max(
          0,
          parseFloat(document.getElementById("itemized-deductions").value) || 0
        );
        const credits = Math.max(
          0,
          parseFloat(document.getElementById("tax-credits").value) || 0
        );

        // sync slider and display
        const sliderVal = Math.max(0, Math.min(300000, gross));
        grossSlider.value = sliderVal;
        document.getElementById("gross-income-display").textContent =
          formatCurrency(gross);

        const adjIncome = Math.max(0, gross - pretax);
        const deduction =
          itemized > 0 ? itemized : getStandardDeduction(status);
        const taxable = Math.max(0, adjIncome - deduction);

        let grossTax = 0;
        if (year === "2025") {
          grossTax = calcIowaTax2025(taxable);
        } else {
          grossTax = calcIowaTax2024(taxable, status);
        }

        const netTax = Math.max(0, grossTax - credits);
        const afterTax = gross - netTax;
        const effRate = gross > 0 ? (netTax / gross) * 100 : 0;

        document.getElementById("result-taxable-income").textContent =
          formatCurrency(taxable);
        document.getElementById("result-state-tax").textContent =
          formatCurrencyExact(netTax);
        document.getElementById("result-after-tax").textContent =
          formatCurrencyExact(afterTax);
        document.getElementById("result-effective-rate").textContent =
          effRate.toFixed(1) + "%";

        const tbody = document.getElementById("breakdown-body");
        tbody.innerHTML = `
      <tr class="border-t">
        <td class="px-2 py-1">Gross income</td>
        <td class="px-2 py-1 text-right">${formatCurrencyExact(gross)}</td>
      </tr>
      <tr class="border-t">
        <td class="px-2 py-1">Pre‑tax deductions</td>
        <td class="px-2 py-1 text-right">−${formatCurrencyExact(pretax)}</td>
      </tr>
      <tr class="border-t">
        <td class="px-2 py-1">${
          itemized > 0
            ? "Itemized deductions"
            : "Standard deduction (estimated)"
        }</td>
        <td class="px-2 py-1 text-right">−${formatCurrencyExact(deduction)}</td>
      </tr>
      <tr class="border-t bg-gray-50">
        <td class="px-2 py-1 font-medium">Iowa taxable income</td>
        <td class="px-2 py-1 text-right font-medium">${formatCurrencyExact(
          taxable
        )}</td>
      </tr>
      <tr class="border-t">
        <td class="px-2 py-1">Gross IA tax</td>
        <td class="px-2 py-1 text-right">${formatCurrencyExact(grossTax)}</td>
      </tr>
      <tr class="border-t">
        <td class="px-2 py-1">Credits</td>
        <td class="px-2 py-1 text-right">−${formatCurrencyExact(
          Math.min(credits, grossTax)
        )}</td>
      </tr>
      <tr class="border-t bg-gray-50">
        <td class="px-2 py-1 font-medium">Net IA tax</td>
        <td class="px-2 py-1 text-right font-medium">${formatCurrencyExact(
          netTax
        )}</td>
      </tr>
    `;
      }

      document.addEventListener("DOMContentLoaded", function () {
        const grossSlider = document.getElementById("gross-income");
        const grossInput = document.getElementById("gross-income-input");

        grossSlider.addEventListener("input", function () {
          grossInput.value = this.value;
          recalc();
        });

        grossInput.addEventListener("input", recalc);
        document.getElementById("tax-year").addEventListener("change", recalc);
        document
          .getElementById("filing-status")
          .addEventListener("change", recalc);
        document
          .getElementById("pretax-deductions")
          .addEventListener("input", recalc);
        document
          .getElementById("itemized-deductions")
          .addEventListener("input", recalc);
        document
          .getElementById("tax-credits")
          .addEventListener("input", recalc);
        document
          .getElementById("calculate-btn")
          .addEventListener("click", recalc);

        recalc();
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
