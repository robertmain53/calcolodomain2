    <script>
      // Mobile menu toggle
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          this.setAttribute("aria-expanded", isHidden ? "true" : "false");
        });

      function computeDistance() {
        const level = document.getElementById("level").value;
        const yardLine = parseFloat(document.getElementById("yardLine").value);
        const side = document.getElementById("side").value;
        const maxRange = parseFloat(document.getElementById("maxRange").value);
        const confidence = parseFloat(
          document.getElementById("confidence").value
        );
        const hash =
          document.querySelector('input[name="hash"]:checked')?.value ||
          "middle";

        if (isNaN(yardLine) || yardLine < 1 || yardLine > 50) {
          alert("Please enter a yard line between 1 and 50.");
          return;
        }

        // Distance from line of scrimmage to goal line
        let yardsToGoal;
        if (side === "own") {
          yardsToGoal = 60 - yardLine; // e.g., own 40 -> 20 yards to goal line
        } else {
          yardsToGoal = yardLine; // e.g., opp 25 -> 25 yards to goal line
        }

        const fgDistance = yardsToGoal + 17; // 10 end zone + 7 holder depth

        // Spot text
        let spotSide, spotYard;
        if (side === "own") {
          const absoluteFromOwnGoal = 100 - yardsToGoal; // LOS absolute
          const spotAbs = absoluteFromOwnGoal + 7;
          if (spotAbs <= 50) {
            spotSide = "Your";
            spotYard = spotAbs;
          } else {
            spotSide = "Opponent";
            spotYard = 100 - spotAbs;
          }
        } else {
          const absoluteFromOwnGoal = 50 + (50 - yardsToGoal);
          const spotAbs = absoluteFromOwnGoal + 7;
          if (spotAbs <= 50) {
            spotSide = "Your";
            spotYard = spotAbs;
          } else {
            spotSide = "Opponent";
            spotYard = 100 - spotAbs;
          }
        }

        const fgDistanceEl = document.getElementById("fgDistance");
        const formulaTextEl = document.getElementById("formulaText");
        const spotTextEl = document.getElementById("spotText");
        const rangeMessageEl = document.getElementById("rangeMessage");
        const probMessageEl = document.getElementById("probMessage");
        const difficultyBarEl = document.getElementById("difficultyBar");
        const difficultyLabelEl = document.getElementById("difficultyLabel");
        const resultsEl = document.getElementById("results");

        fgDistanceEl.textContent = fgDistance.toFixed(0);
        formulaTextEl.textContent = yardsToGoal.toFixed(0) + " + 17";

        spotTextEl.textContent = `${spotSide} ${spotYard.toFixed(
          0
        )}-yard line (${hash} hash)`;

        // Range check
        let msg, colorClass;
        if (fgDistance <= maxRange - 5) {
          msg = `This is well within your kicker's stated range (${maxRange.toFixed(
            0
          )} yards).`;
          colorClass = "text-green-700";
        } else if (fgDistance <= maxRange + 2) {
          msg = `This is near the edge of your kicker's range (${maxRange.toFixed(
            0
          )} yards).`;
          colorClass = "text-amber-700";
        } else {
          msg = `This is beyond your kicker's stated range (${maxRange.toFixed(
            0
          )} yards).`;
          colorClass = "text-red-700";
        }
        rangeMessageEl.textContent = msg;
        rangeMessageEl.className = "text-sm font-medium " + colorClass;

        // Simple probability model: linear drop from 100% at 20 yards to "confidence" at maxRange, then down to 0 at maxRange+15
        let estProb;
        if (fgDistance <= 20) {
          estProb = 0.98;
        } else if (fgDistance <= maxRange) {
          const span = maxRange - 20;
          const t = span > 0 ? (fgDistance - 20) / span : 1;
          estProb = 0.98 + (confidence - 0.98) * t;
        } else if (fgDistance <= maxRange + 15) {
          const t = (fgDistance - maxRange) / 15;
          estProb = confidence * (1 - t);
        } else {
          estProb = 0.02;
        }
        estProb = Math.max(0.01, Math.min(0.99, estProb));
        probMessageEl.textContent = `Rough estimated make probability: ${(
          estProb * 100
        ).toFixed(
          0
        )}% (for a kicker with the range and confidence you entered).`;

        // Difficulty bar: 0–80 yards mapped to 0–100%
        const difficulty = Math.max(0, Math.min(1, fgDistance / 80));
        const width = 20 + difficulty * 80; // keep at least 20% visible
        difficultyBarEl.style.width = width + "%";
        difficultyBarEl.className =
          "h-2 rounded-full " +
          (fgDistance <= maxRange ? "bg-green-500" : "bg-red-500");

        let diffLabel;
        if (fgDistance <= 30) diffLabel = "Short chip shot for most kickers.";
        else if (fgDistance <= 40)
          diffLabel =
            "Standard attempt for good high school and college kickers.";
        else if (fgDistance <= 50)
          diffLabel =
            "Long attempt; typical range for strong college and NFL kickers.";
        else
          diffLabel =
            "Very long attempt; usually only tried by elite legs or in special situations.";
        difficultyLabelEl.textContent = diffLabel;

        resultsEl.classList.remove("hidden");
      }

      document
        .getElementById("calcBtn")
        .addEventListener("click", computeDistance);
      // Auto-calc on load with defaults
      window.addEventListener("DOMContentLoaded", computeDistance);
    </script>

