<script>
  // YMCA Step Test calculator logic

  // Simple normative VO2max tables (approximate, for demonstration)
  const norms = {
    male: [
      { range: [20, 29], veryPoor: 0, poor: 33, fair: 37, good: 42, excellent: 48 },
      { range: [30, 39], veryPoor: 0, poor: 31, fair: 35, good: 40, excellent: 46 },
      { range: [40, 49], veryPoor: 0, poor: 28, fair: 32, good: 37, excellent: 43 },
      { range: [50, 59], veryPoor: 0, poor: 26, fair: 30, good: 35, excellent: 41 },
      { range: [60, 69], veryPoor: 0, poor: 24, fair: 28, good: 33, excellent: 39 },
      { range: [70, 99], veryPoor: 0, poor: 22, fair: 26, good: 31, excellent: 37 }
    ],
    female: [
      { range: [20, 29], veryPoor: 0, poor: 27, fair: 31, good: 36, excellent: 42 },
      { range: [30, 39], veryPoor: 0, poor: 25, fair: 29, good: 34, excellent: 40 },
      { range: [40, 49], veryPoor: 0, poor: 23, fair: 27, good: 32, excellent: 38 },
      { range: [50, 59], veryPoor: 0, poor: 21, fair: 25, good: 30, excellent: 36 },
      { range: [60, 69], veryPoor: 0, poor: 19, fair: 23, good: 28, excellent: 34 },
      { range: [70, 99], veryPoor: 0, poor: 17, fair: 21, good: 26, excellent: 32 }
    ]
  };

  function getAgeGroup(age, sex) {
    const table = norms[sex];
    for (const row of table) {
      if (age >= row.range[0] && age <= row.range[1]) {
        return row;
      }
    }
    // Fallback to last group
    return table[table.length - 1];
  }

  function classifyFitness(vo2, age, sex) {
    const group = getAgeGroup(age, sex);
    const label = `${group.range[0]}–${group.range[1]} yrs`;
    let category;

    if (vo2 < group.poor) {
      category = "Very Poor";
    } else if (vo2 < group.fair) {
      category = "Poor";
    } else if (vo2 < group.good) {
      category = "Fair";
    } else if (vo2 < group.excellent) {
      category = "Good";
    } else {
      category = "Excellent";
    }

    return { category, label };
  }

  document.getElementById('calculateBtn').addEventListener('click', function () {
    const sex = document.getElementById('sex').value;
    const age = parseInt(document.getElementById('age').value, 10);
    const hr = parseInt(document.getElementById('hr').value, 10);
    const protocol = document.getElementById('protocol').value;
    const errorMsg = document.getElementById('errorMsg');
    const vo2El = document.getElementById('vo2Value');
    const catEl = document.getElementById('fitnessCategory');
    const ageGroupEl = document.getElementById('ageGroupLabel');
    const catNoteEl = document.getElementById('fitnessCategoryNote');

    errorMsg.classList.add('hidden');
    errorMsg.textContent = '';

    if (isNaN(age) || age < 10 || age > 90) {
      errorMsg.textContent = 'Please enter a valid age between 10 and 90 years.';
      errorMsg.classList.remove('hidden');
      return;
    }

    if (isNaN(hr) || hr < 60 || hr > 220) {
      errorMsg.textContent = 'Please enter a realistic 1‑minute recovery heart rate between 60 and 220 bpm.';
      errorMsg.classList.remove('hidden');
      return;
    }

    // VO2max estimation
    let vo2 = 65.81 - 0.1847 * hr;
    if (sex === 'female') {
      vo2 -= 1.5;
    }

    // Clamp to reasonable range
    if (vo2 < 10) vo2 = 10;
    if (vo2 > 80) vo2 = 80;

    vo2El.textContent = vo2.toFixed(1) + ' mL·kg⁻¹·min⁻¹';

    if (protocol === 'ymca') {
      const { category, label } = classifyFitness(vo2, age, sex);
      catEl.textContent = category;
      ageGroupEl.textContent = label;
      catNoteEl.textContent = 'Based on approximate VO₂max norms for your age and sex.';
    } else {
      catEl.textContent = 'Not available for custom protocol';
      ageGroupEl.textContent = 'N/A';
      catNoteEl.textContent = 'Fitness categories require the standard YMCA protocol (12 in step, 96 bpm, 3 minutes).';
    }
  });
</script>

