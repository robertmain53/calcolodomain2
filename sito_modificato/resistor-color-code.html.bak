    <script>
      // Data definitions
      const COLOR_DATA = {
        black: {
          name: "Black",
          digit: 0,
          multiplier: 1,
          tolerance: null,
          tempco: null,
          hex: "#111827",
        },
        brown: {
          name: "Brown",
          digit: 1,
          multiplier: 10,
          tolerance: 1,
          tempco: 100,
          hex: "#78350f",
        },
        red: {
          name: "Red",
          digit: 2,
          multiplier: 100,
          tolerance: 2,
          tempco: 50,
          hex: "#b91c1c",
        },
        orange: {
          name: "Orange",
          digit: 3,
          multiplier: 1000,
          tolerance: null,
          tempco: 15,
          hex: "#ea580c",
        },
        yellow: {
          name: "Yellow",
          digit: 4,
          multiplier: 10000,
          tolerance: null,
          tempco: 25,
          hex: "#eab308",
        },
        green: {
          name: "Green",
          digit: 5,
          multiplier: 100000,
          tolerance: 0.5,
          tempco: null,
          hex: "#16a34a",
        },
        blue: {
          name: "Blue",
          digit: 6,
          multiplier: 1000000,
          tolerance: 0.25,
          tempco: 10,
          hex: "#2563eb",
        },
        violet: {
          name: "Violet",
          digit: 7,
          multiplier: 10000000,
          tolerance: 0.1,
          tempco: 5,
          hex: "#7c3aed",
        },
        gray: {
          name: "Gray",
          digit: 8,
          multiplier: 100000000,
          tolerance: 0.05,
          tempco: null,
          hex: "#6b7280",
        },
        white: {
          name: "White",
          digit: 9,
          multiplier: 1000000000,
          tolerance: null,
          tempco: null,
          hex: "#e5e7eb",
        },
        gold: {
          name: "Gold",
          digit: null,
          multiplier: 0.1,
          tolerance: 5,
          tempco: null,
          hex: "#d4af37",
        },
        silver: {
          name: "Silver",
          digit: null,
          multiplier: 0.01,
          tolerance: 10,
          tempco: null,
          hex: "#9ca3af",
        },
        none: {
          name: "None",
          digit: null,
          multiplier: null,
          tolerance: 20,
          tempco: null,
          hex: "transparent",
        },
      };

      const BAND_ROLE_CONFIG = {
        3: ["digit1", "digit2", "multiplier"],
        4: ["digit1", "digit2", "multiplier", "tolerance"],
        5: ["digit1", "digit2", "digit3", "multiplier", "tolerance"],
        6: ["digit1", "digit2", "digit3", "multiplier", "tolerance", "tempco"],
      };

      const DIGIT_COLORS = [
        "black",
        "brown",
        "red",
        "orange",
        "yellow",
        "green",
        "blue",
        "violet",
        "gray",
        "white",
      ];
      const MULTIPLIER_COLORS = [
        "black",
        "brown",
        "red",
        "orange",
        "yellow",
        "green",
        "blue",
        "violet",
        "gray",
        "white",
        "gold",
        "silver",
      ];
      const TOLERANCE_COLORS = [
        "brown",
        "red",
        "green",
        "blue",
        "violet",
        "gray",
        "gold",
        "silver",
        "none",
      ];
      const TEMPCO_COLORS = [
        "brown",
        "red",
        "orange",
        "yellow",
        "blue",
        "violet",
      ];

      let currentMode = "colors-to-value";
      let bandCount = 4;
      let bandColors = []; // array of color keys per band

      const el = (id) => document.getElementById(id);

      function formatResistance(ohms) {
        if (!isFinite(ohms)) return "—";
        if (ohms === 0) return "0 Ω";
        const abs = Math.abs(ohms);
        let value, unit;
        if (abs >= 1e6) {
          value = ohms / 1e6;
          unit = "MΩ";
        } else if (abs >= 1e3) {
          value = ohms / 1e3;
          unit = "kΩ";
        } else {
          value = ohms;
          unit = "Ω";
        }
        const digits = abs >= 100 ? 0 : abs >= 10 ? 1 : 2;
        return value.toFixed(digits) + " " + unit;
      }

      function buildBandSelectors() {
        const container = el("band-selectors");
        container.innerHTML = "";
        const roles = BAND_ROLE_CONFIG[bandCount];

        bandColors = [];

        roles.forEach((role, index) => {
          const bandIndex = index;
          let label;
          let options;

          switch (role) {
            case "digit1":
              label = "Band 1 (1st digit)";
              options = DIGIT_COLORS;
              break;
            case "digit2":
              label = "Band 2 (2nd digit)";
              options = DIGIT_COLORS;
              break;
            case "digit3":
              label = "Band 3 (3rd digit)";
              options = DIGIT_COLORS;
              break;
            case "multiplier":
              label = `Band ${bandIndex + 1} (multiplier)`;
              options = MULTIPLIER_COLORS;
              break;
            case "tolerance":
              label = `Band ${bandIndex + 1} (tolerance)`;
              options = TOLERANCE_COLORS;
              break;
            case "tempco":
              label = `Band ${bandIndex + 1} (temp. coefficient)`;
              options = TEMPCO_COLORS;
              break;
          }

          // Default colors
          let defaultColor = options[0];
          if (role === "multiplier") defaultColor = "brown"; // ×10
          if (role === "tolerance") defaultColor = "gold"; // ±5%
          if (role === "tempco") defaultColor = "brown"; // 100 ppm/°C

          bandColors.push(defaultColor);

          const wrapper = document.createElement("div");
          wrapper.innerHTML = `
        <label class="block text-sm font-medium text-gray-700 mb-1" for="band-select-${bandIndex}">${label}</label>
        <select id="band-select-${bandIndex}" data-band-index="${bandIndex}" class="w-full border-gray-300 rounded-md shadow-sm focus:ring-blue-500 focus:border-blue-500 text-sm"></select>
      `;
          container.appendChild(wrapper);

          const select = el(`band-select-${bandIndex}`);
          options.forEach((colorKey) => {
            const c = COLOR_DATA[colorKey];
            const opt = document.createElement("option");
            opt.value = colorKey;
            opt.textContent =
              c.name +
              (role === "tolerance" && c.tolerance != null
                ? ` (±${c.tolerance}%)`
                : "");
            select.appendChild(opt);
          });
          select.value = defaultColor;
          select.addEventListener("change", () => {
            bandColors[bandIndex] = select.value;
            updateResistorPreview();
            updateFromColors();
          });
        });

        updateResistorPreview();
        updateFromColors();
      }

      function updateResistorPreview() {
        const bandContainer = el("band-container");
        bandContainer.innerHTML = "";
        const roles = BAND_ROLE_CONFIG[bandCount];

        roles.forEach((role, index) => {
          const colorKey = bandColors[index];
          const color = COLOR_DATA[colorKey];
          const bandDiv = document.createElement("div");
          bandDiv.className = "flex flex-col items-center gap-1";
          bandDiv.innerHTML = `
        <div class="band" data-band-index="${index}" style="height: 32px; background:${
            color.hex
          }; border:1px solid rgba(0,0,0,0.25)"></div>
        <div class="band-label text-gray-500">${index + 1}</div>
      `;
          bandContainer.appendChild(bandDiv);
        });

        // Click to focus corresponding select
        bandContainer.querySelectorAll(".band").forEach((bandEl) => {
          bandEl.addEventListener("click", () => {
            const idx = parseInt(bandEl.getAttribute("data-band-index"), 10);
            const select = el(`band-select-${idx}`);
            if (select) {
              select.focus();
              select.classList.add("ring-2", "ring-blue-500", "ring-offset-1");
              setTimeout(() => {
                select.classList.remove(
                  "ring-2",
                  "ring-blue-500",
                  "ring-offset-1"
                );
              }, 600);
            }
          });
        });
      }

      function decodeBandsToValue() {
        const roles = BAND_ROLE_CONFIG[bandCount];
        let digits = "";
        let multiplier = null;
        let tolerance = null;
        let tempco = null;

        roles.forEach((role, index) => {
          const colorKey = bandColors[index];
          const c = COLOR_DATA[colorKey];
          if (!c) return;
          if (role === "digit1" || role === "digit2" || role === "digit3") {
            digits += c.digit != null ? String(c.digit) : "";
          } else if (role === "multiplier") {
            multiplier = c.multiplier;
          } else if (role === "tolerance") {
            tolerance = c.tolerance;
          } else if (role === "tempco") {
            tempco = c.tempco;
          }
        });

        if (!digits || multiplier == null)
          return { ohms: NaN, tolerance: tolerance, tempco: tempco };

        const base = parseInt(digits, 10);
        const ohms = base * multiplier;
        return { ohms, tolerance, tempco };
      }

      function updateFromColors() {
        if (currentMode !== "colors-to-value") return;
        const { ohms, tolerance, tempco } = decodeBandsToValue();
        const resistanceText = isNaN(ohms) ? "—" : formatResistance(ohms);
        el("result-resistance").textContent = resistanceText;
        el("result-tolerance").textContent =
          tolerance != null
            ? `±${tolerance}%`
            : bandCount === 3
            ? "±20% (no band)"
            : "—";
        el("result-tempco").textContent =
          tempco != null ? `${tempco} ppm/°C` : bandCount === 6 ? "—" : "N/A";

        // Range
        if (!isNaN(ohms) && tolerance != null) {
          const delta = ohms * (tolerance / 100);
          const min = ohms - delta;
          const max = ohms + delta;
          el("result-range").textContent = `${formatResistance(
            min
          )} to ${formatResistance(max)}`;
          el("result-range-row").classList.remove("hidden");
        } else {
          el("result-range-row").classList.add("hidden");
        }

        highlightColorRow();
      }

      function encodeValueToBands() {
        const valueInput = parseFloat(el("input-resistance").value);
        const unitFactor = parseFloat(el("input-unit").value);
        if (!isFinite(valueInput) || valueInput <= 0 || !isFinite(unitFactor)) {
          el("encode-error").classList.add("hidden");
          return;
        }
        const ohms = valueInput * unitFactor;

        // Determine digits and multiplier
        const roles = BAND_ROLE_CONFIG[bandCount];
        const digitCount = roles.filter((r) => r.startsWith("digit")).length;

        // Normalize to form: N * 10^exp, where N has digitCount significant digits
        const exp = Math.floor(Math.log10(ohms));
        const scale = digitCount - 1;
        const normalized = ohms / Math.pow(10, exp - scale);
        let rounded = Math.round(normalized);

        // Adjust if rounding produced extra digit
        if (rounded >= Math.pow(10, digitCount)) {
          rounded = Math.round(rounded / 10);
        }

        const newExp = exp - scale;
        const multiplier = Math.pow(10, newExp);

        // Check if multiplier is representable
        let multiplierColor = MULTIPLIER_COLORS.find(
          (c) => COLOR_DATA[c].multiplier === multiplier
        );
        let approximate = false;
        if (!multiplierColor) {
          // Find closest multiplier
          let bestColor = MULTIPLIER_COLORS[0];
          let bestDiff = Infinity;
          MULTIPLIER_COLORS.forEach((c) => {
            const diff = Math.abs(
              Math.log10(COLOR_DATA[c].multiplier) - Math.log10(multiplier)
            );
            if (diff < bestDiff) {
              bestDiff = diff;
              bestColor = c;
            }
          });
          multiplierColor = bestColor;
          approximate = true;
        }

        const digitsStr = rounded
          .toString()
          .padStart(digitCount, "0")
          .slice(0, digitCount);
        const newBandColors = [];

        roles.forEach((role) => {
          if (role.startsWith("digit")) {
            const idx = parseInt(role.slice(-1), 10) - 1;
            const digit = parseInt(digitsStr[idx], 10);
            const colorKey =
              DIGIT_COLORS.find((c) => COLOR_DATA[c].digit === digit) ||
              "black";
            newBandColors.push(colorKey);
          } else if (role === "multiplier") {
            newBandColors.push(multiplierColor);
          } else if (role === "tolerance") {
            newBandColors.push(el("input-tolerance").value || "gold");
          } else if (role === "tempco") {
            const tempcoColor = el("input-tempco").value || "brown";
            newBandColors.push(tempcoColor);
          }
        });

        bandColors = newBandColors;
        // Sync dropdowns if in colors mode
        BAND_ROLE_CONFIG[bandCount].forEach((_, i) => {
          const select = el(`band-select-${i}`);
          if (select) select.value = bandColors[i];
        });

        updateResistorPreview();
        currentMode === "colors-to-value"
          ? updateFromColors()
          : updateFromColors();
        el("encode-error").classList.toggle("hidden", !approximate);
      }

      function buildColorCodeTable() {
        const tbody = el("color-code-table-body");
        tbody.innerHTML = "";
        const order = [
          "black",
          "brown",
          "red",
          "orange",
          "yellow",
          "green",
          "blue",
          "violet",
          "gray",
          "white",
          "gold",
          "silver",
          "none",
        ];

        order.forEach((colorKey) => {
          const c = COLOR_DATA[colorKey];
          if (!c) return;
          const tr = document.createElement("tr");
          tr.setAttribute("data-color-row", colorKey);
          tr.className = "hover:bg-blue-50";
          tr.innerHTML = `
        <td class="px-3 py-2 border-b">
          <div class="flex items-center gap-2">
            <span class="color-swatch" style="background:${c.hex}"></span>
            <span>${c.name}</span>
          </div>
        </td>
        <td class="px-3 py-2 border-b">${c.digit != null ? c.digit : "—"}</td>
        <td class="px-3 py-2 border-b">${
          c.multiplier != null
            ? c.multiplier >= 1
              ? "×" + c.multiplier.toLocaleString()
              : "×" + c.multiplier
            : "—"
        }</td>
        <td class="px-3 py-2 border-b">${
          c.tolerance != null
            ? "±" + c.tolerance + "%"
            : colorKey === "none"
            ? "±20%"
            : "—"
        }</td>
        <td class="px-3 py-2 border-b">${
          c.tempco != null ? c.tempco + " ppm/°C" : "—"
        }</td>
      `;
          tbody.appendChild(tr);
        });
      }

      function highlightColorRow() {
        // Highlight rows used in current bands
        document.querySelectorAll("[data-color-row]").forEach((row) => {
          row.classList.remove("bg-yellow-50");
        });
        bandColors.forEach((colorKey) => {
          const row = document.querySelector(`[data-color-row="${colorKey}"]`);
          if (row) row.classList.add("bg-yellow-50");
        });
      }

      function switchMode(mode) {
        currentMode = mode;
        const btnColors = el("mode-colors-to-value");
        const btnValue = el("mode-value-to-colors");
        const panelColors = el("panel-colors-to-value");
        const panelValue = el("panel-value-to-colors");

        if (mode === "colors-to-value") {
          btnColors.classList.add("border-blue-600", "text-blue-600");
          btnColors.classList.remove("border-transparent", "text-gray-600");
          btnValue.classList.remove("border-blue-600", "text-blue-600");
          btnValue.classList.add("border-transparent", "text-gray-600");
          panelColors.classList.remove("hidden");
          panelValue.classList.add("hidden");
          updateFromColors();
        } else {
          btnValue.classList.add("border-blue-600", "text-blue-600");
          btnValue.classList.remove("border-transparent", "text-gray-600");
          btnColors.classList.remove("border-blue-600", "text-blue-600");
          btnColors.classList.add("border-transparent", "text-gray-600");
          panelValue.classList.remove("hidden");
          panelColors.classList.add("hidden");
          // Show current decoded value in summary
          updateFromColors();
        }
      }

      document.addEventListener("DOMContentLoaded", () => {
        // Initialize
        buildBandSelectors();
        buildColorCodeTable();
        updateFromColors();

        el("band-count").addEventListener("change", () => {
          bandCount = parseInt(el("band-count").value, 10);
          buildBandSelectors();
          updateFromColors();
        });

        el("mode-colors-to-value").addEventListener("click", () =>
          switchMode("colors-to-value")
        );
        el("mode-value-to-colors").addEventListener("click", () =>
          switchMode("value-to-colors")
        );
        el("btn-generate-bands").addEventListener("click", encodeValueToBands);
      });
    </script>

