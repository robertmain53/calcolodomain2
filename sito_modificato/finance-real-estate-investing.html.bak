    <script>
      // Helper: format currency and percent
      function fmtCurrency(v) {
        if (!isFinite(v)) return "$0";
        return "$" + v.toLocaleString(undefined, { maximumFractionDigits: 0 });
      }
      function fmtPercent(v) {
        if (!isFinite(v)) return "0%";
        return v.toLocaleString(undefined, { maximumFractionDigits: 1 }) + "%";
      }

      function analyzeDeal() {
        const purchasePrice =
          parseFloat(document.getElementById("purchasePrice").value) || 0;
        const downPaymentPct =
          (parseFloat(document.getElementById("downPaymentPct").value) || 0) /
          100;
        const interestRate =
          (parseFloat(document.getElementById("interestRate").value) || 0) /
          100;
        const loanTermYears =
          parseFloat(document.getElementById("loanTerm").value) || 0;
        const closingCosts =
          parseFloat(document.getElementById("closingCosts").value) || 0;

        const grossRent =
          parseFloat(document.getElementById("grossRent").value) || 0;
        const vacancyPct =
          (parseFloat(document.getElementById("vacancyPct").value) || 0) / 100;
        const taxes = parseFloat(document.getElementById("taxes").value) || 0;
        const insurance =
          parseFloat(document.getElementById("insurance").value) || 0;
        const maintenancePct =
          (parseFloat(document.getElementById("maintenancePct").value) || 0) /
          100;
        const managementPct =
          (parseFloat(document.getElementById("managementPct").value) || 0) /
          100;
        const otherExpenses =
          parseFloat(document.getElementById("otherExpenses").value) || 0;

        const valueGrowth =
          (parseFloat(document.getElementById("valueGrowth").value) || 0) / 100;
        const rentGrowth =
          (parseFloat(document.getElementById("rentGrowth").value) || 0) / 100;
        const holdingPeriod =
          parseFloat(document.getElementById("holdingPeriod").value) || 0;
        const sellingCostsPct =
          (parseFloat(document.getElementById("sellingCostsPct").value) || 0) /
          100;

        // Financing
        const downPayment = purchasePrice * downPaymentPct;
        const loanAmount = Math.max(purchasePrice - downPayment, 0);
        const totalCash = downPayment + closingCosts;

        const n = loanTermYears * 12;
        const r = interestRate / 12;
        let monthlyPmt = 0;
        if (loanAmount > 0 && r > 0 && n > 0) {
          monthlyPmt =
            (loanAmount * (r * Math.pow(1 + r, n))) / (Math.pow(1 + r, n) - 1);
        } else if (loanAmount > 0 && r === 0 && n > 0) {
          monthlyPmt = loanAmount / n;
        }

        const annualDebtService = monthlyPmt * 12;

        // Income & expenses (Year 1)
        const egi = grossRent * (1 - vacancyPct);
        const maintenance = grossRent * maintenancePct;
        const management = grossRent * managementPct;
        const operatingExpenses =
          taxes + insurance + maintenance + management + otherExpenses;
        const noi = egi - operatingExpenses;

        const annualCashFlow = noi - annualDebtService;

        // Metrics
        const capRate = purchasePrice > 0 ? (noi / purchasePrice) * 100 : 0;
        const cocReturn =
          totalCash > 0 ? (annualCashFlow / totalCash) * 100 : 0;
        const dscr = annualDebtService > 0 ? noi / annualDebtService : 0;

        // Long-term projection (simple)
        const salePrice =
          purchasePrice * Math.pow(1 + valueGrowth, holdingPeriod || 0);
        // Remaining balance after holdingPeriod years
        let remainingBalance = loanAmount;
        if (loanAmount > 0 && n > 0 && holdingPeriod > 0) {
          const paidMonths = Math.min(holdingPeriod * 12, n);
          if (r > 0) {
            remainingBalance =
              loanAmount * Math.pow(1 + r, paidMonths) -
              (monthlyPmt * (Math.pow(1 + r, paidMonths) - 1)) / r;
          } else {
            remainingBalance = loanAmount - monthlyPmt * paidMonths;
          }
        }

        const sellingCosts = salePrice * sellingCostsPct;
        const netSaleProceeds = salePrice - remainingBalance - sellingCosts;

        // Approximate total cash flow over hold (assume rent & expenses grow with rentGrowth)
        let totalCashFlow = 0;
        let currentRent = grossRent;
        let currentTaxes = taxes;
        let currentInsurance = insurance;
        let currentOther = otherExpenses;

        for (let year = 1; year <= holdingPeriod; year++) {
          const yearEgi = currentRent * (1 - vacancyPct);
          const yearMaint = currentRent * maintenancePct;
          const yearMgmt = currentRent * managementPct;
          const yearOpex =
            yearMaint +
            yearMgmt +
            currentTaxes +
            currentInsurance +
            currentOther;
          const yearNoi = yearEgi - yearOpex;
          const yearCF = yearNoi - annualDebtService;
          totalCashFlow += yearCF;

          currentRent *= 1 + rentGrowth;
          currentTaxes *= 1 + rentGrowth * 0.5; // assume slower growth
          currentInsurance *= 1 + rentGrowth * 0.5;
          currentOther *= 1 + rentGrowth * 0.5;
        }

        const totalProfit = totalCashFlow + netSaleProceeds - totalCash;
        const annualizedROI =
          holdingPeriod > 0 && totalCash > 0
            ? ((totalProfit / totalCash) * 100) / holdingPeriod
            : 0;

        // Update UI
        document.getElementById("outTotalCash").textContent =
          fmtCurrency(totalCash);
        document.getElementById("outLoanAmount").textContent =
          fmtCurrency(loanAmount);
        document.getElementById("outMonthlyPmt").textContent =
          fmtCurrency(monthlyPmt);
        document.getElementById("outNOI").textContent = fmtCurrency(noi);

        document.getElementById("outCapRate").textContent = fmtPercent(capRate);
        document.getElementById("outCoC").textContent = fmtPercent(cocReturn);
        document.getElementById("outCashFlow").textContent =
          fmtCurrency(annualCashFlow);
        document.getElementById("outDSCR").textContent = dscr.toFixed(2);

        document.getElementById("outSalePrice").textContent =
          fmtCurrency(salePrice);
        document.getElementById("outRemainingBalance").textContent =
          fmtCurrency(remainingBalance);
        document.getElementById("outTotalProfit").textContent =
          fmtCurrency(totalProfit);
        document.getElementById("outAnnualizedROI").textContent =
          fmtPercent(annualizedROI);

        // Badges & notes
        const capBadge = document.getElementById("capBadge");
        const cocBadge = document.getElementById("cocBadge");
        const dscrNote = document.getElementById("dscrNote");

        capBadge.textContent =
          capRate >= 7 ? "Strong" : capRate >= 5 ? "Typical" : "Thin";
        cocBadge.textContent =
          cocReturn >= 12 ? "Strong" : cocReturn >= 8 ? "Typical" : "Thin";

        if (!isFinite(dscr) || dscr <= 0) {
          dscrNote.textContent = "No debt service or NOI is negative.";
        } else if (dscr < 1.0) {
          dscrNote.textContent = "Warning: NOI does not cover debt service.";
        } else if (dscr < 1.2) {
          dscrNote.textContent =
            "Thin coverage; many lenders prefer ≥ 1.20–1.25.";
        } else {
          dscrNote.textContent =
            "Coverage generally acceptable for many lenders.";
        }
      }

      document
        .getElementById("analyzeBtn")
