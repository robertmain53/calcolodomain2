    <script>
      // Walter Model Dividend Calculator Logic
      function formatCurrency(value) {
        if (!isFinite(value)) return "–";
        return "$" + value.toFixed(2);
      }

      function formatPercent(value) {
        if (!isFinite(value)) return "–";
        return value.toFixed(2) + "%";
      }

      function calculateWalterPrice(E, D, r, k) {
        // E, D in currency; r, k as decimals
        const retained = E - D;
        const pvReinvested = (r * retained) / k;
        return (D + pvReinvested) / k;
      }

      function updateCalculator() {
        const epsInput = document.getElementById("eps");
        const divInput = document.getElementById("dividend");
        const rInput = document.getElementById("r");
        const kInput = document.getElementById("k");
        const errorBox = document.getElementById("errorBox");

        const E = parseFloat(epsInput.value);
        const D = parseFloat(divInput.value);
        const rPct = parseFloat(rInput.value);
        const kPct = parseFloat(kInput.value);

        errorBox.classList.add("hidden");
        errorBox.textContent = "";

        if (isNaN(E) || isNaN(D) || isNaN(rPct) || isNaN(kPct)) {
          errorBox.textContent =
            "Please enter valid numeric values for all fields.";
          errorBox.classList.remove("hidden");
          return;
        }

        if (E <= 0) {
          errorBox.textContent =
            "Earnings per share (E) must be greater than 0 for the Walter Model to apply.";
          errorBox.classList.remove("hidden");
          return;
        }

        if (D < 0) {
          errorBox.textContent = "Dividend per share (D) cannot be negative.";
          errorBox.classList.remove("hidden");
          return;
        }

        if (D > E) {
          errorBox.textContent =
            "Dividend per share (D) should not exceed earnings per share (E) under the model assumptions.";
          errorBox.classList.remove("hidden");
          return;
        }

        if (kPct <= 0) {
          errorBox.textContent = "Cost of equity (k) must be greater than 0%.";
          errorBox.classList.remove("hidden");
          return;
        }

        const r = rPct / 100;
        const k = kPct / 100;

        // Main results
        const b = (E - D) / E;
        const g = r * b;
        const P = calculateWalterPrice(E, D, r, k);

        document.getElementById("retentionRatio").textContent = b.toFixed(3);
        document.getElementById("growthRate").textContent = formatPercent(
          g * 100
        );
        document.getElementById("pricePerShare").textContent =
          formatCurrency(P);

        // Firm classification
        let firmType = "";
        const epsilon = 1e-4;
        if (r - k > epsilon) {
          firmType = "Growth firm (r > k)";
        } else if (k - r > epsilon) {
          firmType = "Declining / low-return firm (r < k)";
        } else {
          firmType = "Normal firm (r ≈ k)";
        }
        document.getElementById("firmType").textContent = firmType;

        // Policy comparison
        const D0 = 0;
        const D50 = 0.5 * E;
        const D100 = E;

        const P0 = calculateWalterPrice(E, D0, r, k);
        const P50 = calculateWalterPrice(E, D50, r, k);
        const P100 = calculateWalterPrice(E, D100, r, k);

        document.getElementById("payout0").textContent = formatCurrency(P0);
        document.getElementById("payout50").textContent = formatCurrency(P50);
        document.getElementById("payout100").textContent = formatCurrency(P100);

        // Recommendation hint
        let hint = "";
        if (r - k > epsilon) {
          hint =
            "With r > k, Walter Model suggests a low dividend payout (high retention) maximizes share value.";
        } else if (k - r > epsilon) {
          hint =
            "With r < k, Walter Model suggests a high dividend payout (low retention) maximizes share value.";
        } else {
          hint =
            "With r ≈ k, Walter Model implies dividend policy is approximately irrelevant to firm value.";
        }
        document.getElementById("policyHint").textContent = hint;
      }

      document
        .getElementById("calculateBtn")
        .addEventListener("click", updateCalculator);

      document
        .getElementById("resetBtn")
        .addEventListener("click", function () {
          document.getElementById("eps").value = 10;
          document.getElementById("dividend").value = 4;
          document.getElementById("r").value = 18;
          document.getElementById("k").value = 12;
          document.getElementById("errorBox").classList.add("hidden");
          document.getElementById("errorBox").textContent = "";
          updateCalculator();
        });

      // Quick scenario buttons
      document
        .getElementById("btn-growth")
        .addEventListener("click", function () {
          const eps = parseFloat(document.getElementById("eps").value) || 10;
          document.getElementById("eps").value = eps.toFixed(2);
          document.getElementById("dividend").value = (0.2 * eps).toFixed(2); // 20% payout
          document.getElementById("r").value = 20; // 20%
          document.getElementById("k").value = 10; // 10%
          updateCalculator();
        });

      document
        .getElementById("btn-normal")
        .addEventListener("click", function () {
          const eps = parseFloat(document.getElementById("eps").value) || 10;
          document.getElementById("eps").value = eps.toFixed(2);
          document.getElementById("dividend").value = (0.5 * eps).toFixed(2); // 50% payout
          document.getElementById("r").value = 12; // 12%
          document.getElementById("k").value = 12; // 12%
          updateCalculator();
        });

      document
        .getElementById("btn-declining")
        .addEventListener("click", function () {
          const eps = parseFloat(document.getElementById("eps").value) || 10;
          document.getElementById("eps").value = eps.toFixed(2);
          document.getElementById("dividend").value = (0.8 * eps).toFixed(2); // 80% payout
          document.getElementById("r").value = 6; // 6%
          document.getElementById("k").value = 12; // 12%
          updateCalculator();
        });

      // Auto-calc on load
      document.addEventListener("DOMContentLoaded", updateCalculator);
    </script>

