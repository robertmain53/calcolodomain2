    <script>
      // Mode switching
      const modeRectBtn = document.getElementById("mode-rect");
      const modeTotalBtn = document.getElementById("mode-total");
      const modeCircleBtn = document.getElementById("mode-circle");
      const rectSection = document.getElementById("rect-section");
      const totalSection = document.getElementById("total-section");
      const circleSection = document.getElementById("circle-section");

      function setMode(mode) {
        [modeRectBtn, modeTotalBtn, modeCircleBtn].forEach((btn) => {
          btn.classList.remove("bg-blue-600", "text-white");
          btn.classList.add("bg-white", "text-gray-700");
        });
        rectSection.classList.add("hidden");
        totalSection.classList.add("hidden");
        circleSection.classList.add("hidden");

        if (mode === "rect") {
          modeRectBtn.classList.add("bg-blue-600", "text-white");
          rectSection.classList.remove("hidden");
        } else if (mode === "total") {
          modeTotalBtn.classList.add("bg-blue-600", "text-white");
          totalSection.classList.remove("hidden");
        } else if (mode === "circle") {
          modeCircleBtn.classList.add("bg-blue-600", "text-white");
          circleSection.classList.remove("hidden");
        }
      }

      modeRectBtn.addEventListener("click", () => setMode("rect"));
      modeTotalBtn.addEventListener("click", () => setMode("total"));
      modeCircleBtn.addEventListener("click", () => setMode("circle"));

      // Preset depth buttons
      document.querySelectorAll("[data-preset-depth]").forEach((btn) => {
        btn.addEventListener("click", () => {
          const depthInput = document.getElementById("depth");
          const depthUnit = document.getElementById("depth-unit");
          depthUnit.value = "in";
          depthInput.value = btn.getAttribute("data-preset-depth");
        });
      });

      // Calculation logic
      function calculateMulch() {
        const errorMsg = document.getElementById("error-msg");
        const resultsEmpty = document.getElementById("results-empty");
        const results = document.getElementById("results");
        const bagsSection = document.getElementById("bags-section");

        errorMsg.classList.add("hidden");

        let areaSqFt = 0;
        // Determine active mode
        if (!rectSection.classList.contains("hidden")) {
          const length = parseFloat(document.getElementById("length").value);
          const width = parseFloat(document.getElementById("width").value);
          const lengthUnit = document.getElementById("length-unit").value;
          const widthUnit = document.getElementById("width-unit").value;

          if (!(length > 0) || !(width > 0)) {
            errorMsg.textContent = "Please enter positive length and width.";
            errorMsg.classList.remove("hidden");
            results.classList.add("hidden");
            resultsEmpty.classList.remove("hidden");
            return;
          }

          let lengthFt = lengthUnit === "m" ? length * 3.28084 : length;
          let widthFt = widthUnit === "m" ? width * 3.28084 : width;
          areaSqFt = lengthFt * widthFt;
        } else if (!totalSection.classList.contains("hidden")) {
          const area = parseFloat(document.getElementById("area").value);
          const areaUnit = document.getElementById("area-unit").value;

          if (!(area > 0)) {
            errorMsg.textContent = "Please enter a positive area.";
            errorMsg.classList.remove("hidden");
            results.classList.add("hidden");
            resultsEmpty.classList.remove("hidden");
            return;
          }

          areaSqFt = areaUnit === "sqm" ? area * 10.7639 : area;
        } else if (!circleSection.classList.contains("hidden")) {
          const radius = parseFloat(document.getElementById("radius").value);
          const radiusUnit = document.getElementById("radius-unit").value;

          if (!(radius > 0)) {
            errorMsg.textContent = "Please enter a positive radius.";
            errorMsg.classList.remove("hidden");
            results.classList.add("hidden");
            resultsEmpty.classList.remove("hidden");
            return;
          }

          let radiusFt = radiusUnit === "m" ? radius * 3.28084 : radius;
          areaSqFt = Math.PI * radiusFt * radiusFt;
        }

        const depth = parseFloat(document.getElementById("depth").value);
        const depthUnit = document.getElementById("depth-unit").value;

        if (!(depth > 0)) {
          errorMsg.textContent = "Please enter a positive mulch depth.";
          errorMsg.classList.remove("hidden");
          results.classList.add("hidden");
          resultsEmpty.classList.remove("hidden");
          return;
        }

        // Convert depth to feet
        let depthFt;
        if (depthUnit === "in") {
          depthFt = depth / 12;
        } else {
          // cm
          depthFt = depth / 2.54 / 12;
        }

        const volumeCuFt = areaSqFt * depthFt;
        const volumeCuYd = volumeCuFt / 27;
        const volumeCuM = volumeCuFt * 0.0283168;
        const volumeLiters = volumeCuFt * 28.3168;

        // Update results
        document.getElementById("res-cuft").textContent = volumeCuFt.toFixed(1);
        document.getElementById("res-cuyd").textContent = volumeCuYd.toFixed(2);
        document.getElementById("res-cum").textContent = volumeCuM.toFixed(3);
        document.getElementById("res-liters").textContent =
          volumeLiters.toFixed(0);

        // Bags calculation
        const bagSize = parseFloat(document.getElementById("bag-size").value);
        const bagUnit = document.getElementById("bag-unit").value;

        if (bagSize > 0) {
          let bagVolumeCuFt;
          if (bagUnit === "cuft") {
            bagVolumeCuFt = bagSize;
          } else {
            // liters to cubic feet
            bagVolumeCuFt = bagSize / 28.3168;
          }
          const bags = volumeCuFt / bagVolumeCuFt;
          document.getElementById("res-bags").textContent = Math.ceil(bags);
          bagsSection.classList.remove("hidden");
        } else {
          bagsSection.classList.add("hidden");
        }

        resultsEmpty.classList.add("hidden");
        results.classList.remove("hidden");
      }

      document
        .getElementById("calculate-btn")
        .addEventListener("click", calculateMulch);

      document.getElementById("reset-btn").addEventListener("click", () => {
        ["length", "width", "area", "radius", "depth", "bag-size"].forEach(
          (id) => {
            const el = document.getElementById(id);
            if (el) el.value = "";
          }
        );
        document.getElementById("error-msg").classList.add("hidden");
        document.getElementById("results").classList.add("hidden");
        document.getElementById("results-empty").classList.remove("hidden");
        document.getElementById("bags-section").classList.add("hidden");
        setMode("rect");
      });

      // Default mode
      setMode("rect");

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
