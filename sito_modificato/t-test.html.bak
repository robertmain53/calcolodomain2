  <script>
    // mobile menu toggle
    const mobileToggle = document.getElementById('mobile-menu-toggle');
    const mobileMenu = document.getElementById('mobile-menu');
    if (mobileToggle && mobileMenu) {
      mobileToggle.addEventListener('click', () => {
        const isHidden = mobileMenu.classList.contains('hidden');
        mobileMenu.classList.toggle('hidden', !isHidden);
        mobileToggle.setAttribute('aria-expanded', String(isHidden));
      });
    }

    // tabs
    const tabButtons = document.querySelectorAll('.tab-btn');
    const tabs = {
      'one-sample': document.getElementById('tab-one-sample'),
      'two-sample': document.getElementById('tab-two-sample'),
      'paired': document.getElementById('tab-paired')
    };
    tabButtons.forEach(btn => {
      btn.addEventListener('click', () => {
        tabButtons.forEach(b => b.classList.remove('active'));
        btn.classList.add('active');
        const target = btn.dataset.tab;
        Object.keys(tabs).forEach(k => {
          tabs[k].classList.toggle('hidden', k !== target);
        });
      });
    });

    // numeric helpers
    function roundTo(val, dec) {
      if (isNaN(val)) return '–';
      return Number(val).toFixed(dec);
    }

    // Beta / t CDF (Numerical Recipes-style)
    function betacf(a, b, x) {
      const MAXIT = 200;
      const EPS = 1e-12;
      const FPMIN = 1e-30;
      let qab = a + b;
      let qap = a + 1.0;
      let qam = a - 1.0;
      let c = 1.0;
      let d = 1.0 - qab * x / qap;
      if (Math.abs(d) < FPMIN) d = FPMIN;
      d = 1.0 / d;
      let h = d;
      for (let m = 1; m <= MAXIT; m++) {
        let m2 = 2 * m;
        // even
        let aa = m * (b - m) * x / ((qam + m2) * (a + m2));
        d = 1.0 + aa * d;
        if (Math.abs(d) < FPMIN) d = FPMIN;
        c = 1.0 + aa / c;
        if (Math.abs(c) < FPMIN) c = FPMIN;
        d = 1.0 / d;
        h *= d * c;
        // odd
        aa = -(a + m) * (qab + m) * x / ((a + m2) * (qap + m2));
        d = 1.0 + aa * d;
        if (Math.abs(d) < FPMIN) d = FPMIN;
        c = 1.0 + aa / c;
        if (Math.abs(c) < FPMIN) c = FPMIN;
        d = 1.0 / d;
        let del = d * c;
        h *= del;
        if (Math.abs(del - 1.0) < EPS) break;
      }
      return h;
    }

    function betai(x, a, b) {
      // regularized incomplete beta I_x(a,b)
      if (x <= 0) return 0;
      if (x >= 1) return 1;
      const bt = Math.exp(
        lgamma(a + b) - lgamma(a) - lgamma(b) +
        a * Math.log(x) + b * Math.log(1.0 - x)
      );
      if (x < (a + 1.0) / (a + b + 2.0)) {
        return bt * betacf(a, b, x) / a;
      } else {
        return 1.0 - bt * betacf(b, a, 1.0 - x) / b;
      }
    }

    // log-gamma approximation (Lanczos)
    function lgamma(z) {
      const cof = [
        76.18009172947146,
        -86.50532032941677,
        24.01409824083091,
        -1.231739572450155,
        0.001208650973866179,
        -0.000005395239384953
      ];
      let x = z;
      let y = z;
      let tmp = x + 5.5;
      tmp -= (x + 0.5) * Math.log(tmp);
      let ser = 1.000000000190015;
      for (let j = 0; j < 6; j++) {
        y += 1;
        ser += cof[j] / y;
      }
      return -tmp + Math.log(2.5066282746310005 * ser / x);
    }

    function tCDF(t, df) {
      // two-sided student's t CDF from 0 to t, based on relation with incomplete beta
      const x = df / (df + t * t);
      const a = df / 2;
      const b = 0.5;
      const ib = betai(x, a, b);
      let p;
      if (t >= 0) {
        p = 1 - 0.5 * ib;
      } else {
        p = 0.5 * ib;
      }
      return p;
    }

    function pValueFromT(t, df, tail) {
      const cdf = tCDF(t, df);
      if (tail === 'two') {
        const p = 2 * (t >= 0 ? (1 - cdf) : cdf);
        return Math.min(1, Math.max(0, p));
      } else if (tail === 'greater') {
        return 1 - cdf;
      } else {
        return cdf;
      }
    }

    // main calculation
    document.getElementById('calc-btn').addEventListener('click', () => {
      const activeTab = document.querySelector('.tab-btn.active').dataset.tab;
      const tail = document.getElementById('tail').value;
      const alpha = parseFloat(document.getElementById('alpha').value) || 0.05;

      let t, df;

      if (activeTab === 'one-sample') {
        const mean = parseFloat(document.getElementById('os-mean').value);
        const sd = parseFloat(document.getElementById('os-sd').value);
        const n = parseInt(document.getElementById('os-n').value, 10);
        const mu0 = parseFloat(document.getElementById('os-mu').value);
        if (isNaN(mean) || isNaN(sd) || isNaN(n) || isNaN(mu0) || n <= 1 || sd <= 0) {
          alert('Please enter valid one-sample data.');
          return;
        }
        t = (mean - mu0) / (sd / Math.sqrt(n));
        df = n - 1;
      } else if (activeTab === 'two-sample') {
        const m1 = parseFloat(document.getElementById('ts1-mean').value);
        const m2 = parseFloat(document.getElementById('ts2-mean').value);
        const s1 = parseFloat(document.getElementById('ts1-sd').value);
        const s2 = parseFloat(document.getElementById('ts2-sd').value);
        const n1 = parseInt(document.getElementById('ts1-n').value, 10);
        const n2 = parseInt(document.getElementById('ts2-n').value, 10);
        const equalVar = document.getElementById('ts-equal-var').checked;
        if ([m1, m2, s1, s2, n1, n2].some(v => isNaN(v)) || n1 <= 1 || n2 <= 1 || s1 <= 0 || s2 <= 0) {
          alert('Please enter valid two-sample data.');
          return;
        }
        if (equalVar) {
          const sp2 = (((n1 - 1) * s1 * s1) + ((n2 - 1) * s2 * s2)) / (n1 + n2 - 2);
          const sp = Math.sqrt(sp2);
          t = (m1 - m2) / (sp * Math.sqrt(1 / n1 + 1 / n2));
          df = n1 + n2 - 2;
        } else {
          const se2 = (s1 * s1) / n1 + (s2 * s2) / n2;
          t = (m1 - m2) / Math.sqrt(se2);
          const num = se2 * se2;
          const den = ((s1 * s1) / n1) ** 2 / (n1 - 1) + ((s2 * s2) / n2) ** 2 / (n2 - 1);
          df = num / den;
        }
      } else {
        // paired
        const dMean = parseFloat(document.getElementById('pd-mean').value);
        const dSd = parseFloat(document.getElementById('pd-sd').value);
        const n = parseInt(document.getElementById('pd-n').value, 10);
        if (isNaN(dMean) || isNaN(dSd) || isNaN(n) || n <= 1 || dSd <= 0) {
          alert('Please enter valid paired data.');
          return;
        }
        t = dMean / (dSd / Math.sqrt(n));
        df = n - 1;
      }

      const p = pValueFromT(t, df, tail);

      document.getElementById('res-t').textContent = roundTo(t, 4);
      document.getElementById('res-df').textContent = roundTo(df, 4);
      document.getElementById('res-p').textContent = roundTo(p, 6);

      const interpretation = p < alpha
        ? `Since p = ${roundTo(p,4)} < α = ${alpha}, reject H₀. The difference is statistically significant.`
        : `Since p = ${roundTo(p,4)} ≥ α = ${alpha}, fail to reject H₀. Not enough evidence of a difference.`;
      document.getElementById('res-interpret').textContent = interpretation;
    });

    // preset initial run
    document.getElementById('calc-btn').click();
  </script>

