    <script>
      // Core conversion constants
      const FACTOR_US = 119.8264; // 1 lb/gal (US) to kg/m3
      const FACTOR_UK = 99.7764; // 1 lb/gal (UK) to kg/m3

      function formatNumber(value) {
        if (!isFinite(value)) return "";
        const abs = Math.abs(value);
        if (abs === 0) return "0";
        if (abs >= 0.001 && abs < 1e6) {
          return Number(value.toFixed(6)).toString();
        }
        return value.toExponential(6);
      }

      function showResults(html) {
        const card = document.getElementById("resultsCard");
        const content = document.getElementById("resultsContent");
        content.innerHTML = html;
        card.classList.remove("hidden");
      }

      document.addEventListener("DOMContentLoaded", () => {
        const densityLbGalInput = document.getElementById("densityLbGal");
        const gallonTypeSelect = document.getElementById("gallonType");
        const densityKgM3Input = document.getElementById("densityKgM3");

        document
          .getElementById("convertFromLbGalBtn")
          .addEventListener("click", () => {
            const val = parseFloat(densityLbGalInput.value);
            if (isNaN(val)) {
              showResults("<p>Please enter a valid lb/gal value.</p>");
              return;
            }
            const type = gallonTypeSelect.value === "uk" ? "UK" : "US";
            const factor = type === "US" ? FACTOR_US : FACTOR_UK;
            const kgm3 = val * factor;
            densityKgM3Input.value = formatNumber(kgm3);

            showResults(`
        <p><strong>${formatNumber(
          val
        )} lb/gal (${type})</strong> = <strong>${formatNumber(
              kgm3
            )} kg/m³</strong></p>
        <p class="text-xs mt-1 text-blue-900">
          Using: kg/m³ = lb/gal (${type}) × ${
              type === "US" ? FACTOR_US : FACTOR_UK
            }
        </p>
      `);
          });

        document
          .getElementById("convertToUsBtn")
          .addEventListener("click", () => {
            const val = parseFloat(densityKgM3Input.value);
            if (isNaN(val)) {
              showResults("<p>Please enter a valid kg/m³ value.</p>");
              return;
            }
            const lbgal = val / FACTOR_US;
            densityLbGalInput.value = formatNumber(lbgal);
            gallonTypeSelect.value = "us";

            showResults(`
        <p><strong>${formatNumber(val)} kg/m³</strong> = <strong>${formatNumber(
              lbgal
            )} lb/gal (US)</strong></p>
        <p class="text-xs mt-1 text-blue-900">
          Using: lb/gal (US) = kg/m³ ÷ ${FACTOR_US}
        </p>
      `);
          });

        document
          .getElementById("convertToUkBtn")
          .addEventListener("click", () => {
            const val = parseFloat(densityKgM3Input.value);
            if (isNaN(val)) {
              showResults("<p>Please enter a valid kg/m³ value.</p>");
              return;
            }
            const lbgal = val / FACTOR_UK;
            densityLbGalInput.value = formatNumber(lbgal);
            gallonTypeSelect.value = "uk";

            showResults(`
        <p><strong>${formatNumber(val)} kg/m³</strong> = <strong>${formatNumber(
              lbgal
            )} lb/gal (UK)</strong></p>
        <p class="text-xs mt-1 text-blue-900">
          Using: lb/gal (UK) = kg/m³ ÷ ${FACTOR_UK}
        </p>
      `);
          });
      });

      // Mobile menu script (simple toggle)
      document.addEventListener("DOMContentLoaded", () => {
        const toggle = document.getElementById("mobile-menu-toggle");
        const menu = document.getElementById("mobile-menu");
        if (toggle && menu) {
          toggle.addEventListener("click", () => {
            const isHidden = menu.classList.contains("hidden");
            menu.classList.toggle("hidden", !isHidden);
            toggle.setAttribute("aria-expanded", isHidden ? "true" : "false");
          });
        }
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
