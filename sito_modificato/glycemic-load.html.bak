<script>
    function calculateGlycemicLoad() {
        const carbAmount = parseFloat(document.getElementById('carbohydrateAmount').value);
        const gi = parseFloat(document.getElementById('glycemicIndex').value);
        const resultsContainer = document.getElementById('results');
        const glycemicLoadValue = document.getElementById('glycemicLoadValue');

        let valid = true;

        if (isNaN(carbAmount) || carbAmount <= 0) {
            document.getElementById('carbohydrateAmountError').textContent = 'Please enter a valid carbohydrate amount.';
            valid = false;
        } else {
            document.getElementById('carbohydrateAmountError').textContent = '';
        }

        if (isNaN(gi) || gi <= 0 || gi > 100) {
            document.getElementById('glycemicIndexError').textContent = 'Please enter a valid glycemic index between 1 and 100.';
            valid = false;
        } else {
            document.getElementById('glycemicIndexError').textContent = '';
        }

        if (valid) {
            const glycemicLoad = (gi * carbAmount) / 100;
            glycemicLoadValue.textContent = glycemicLoad.toFixed(2);
            resultsContainer.style.display = 'block';
        } else {
            resultsContainer.style.display = 'none';
        }
    }
</script>



