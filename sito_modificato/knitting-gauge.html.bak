    <script>
      // Tab switching
      document.querySelectorAll(".tab-btn").forEach((btn) => {
        btn.addEventListener("click", () => {
          const target = btn.getAttribute("data-tab");
          document.querySelectorAll(".tab-btn").forEach((b) => {
            b.classList.remove("border-blue-600", "text-blue-600");
            b.classList.add("border-transparent", "text-gray-500");
          });
          btn.classList.add("border-blue-600", "text-blue-600");
          btn.classList.remove("border-transparent", "text-gray-500");

          document.querySelectorAll(".tab-panel").forEach((panel) => {
            panel.classList.add("hidden");
          });
          document.getElementById(target).classList.remove("hidden");
        });
      });

      function roundTo(value, decimals) {
        const factor = Math.pow(10, decimals);
        return Math.round(value * factor) / factor;
      }

      // TAB 1: Gauge from swatch
      document
        .getElementById("btn-calc-gauge")
        .addEventListener("click", () => {
          const w = parseFloat(document.getElementById("swatch-width").value);
          const h = parseFloat(document.getElementById("swatch-height").value);
          const sts = parseFloat(
            document.getElementById("swatch-stitches").value
          );
          const rows = parseFloat(document.getElementById("swatch-rows").value);
          const wUnit = document.getElementById("swatch-width-unit").value;
          const hUnit = document.getElementById("swatch-height-unit").value;

          if (!w || !h || !sts || !rows || w <= 0 || h <= 0) {
            alert(
              "Please enter positive values for width, height, stitches and rows."
            );
            return;
          }

          // Convert to cm and inches
          const widthCm = wUnit === "cm" ? w : w * 2.54;
          const heightCm = hUnit === "cm" ? h : h * 2.54;
          const widthIn = wUnit === "in" ? w : w / 2.54;
          const heightIn = hUnit === "in" ? h : h / 2.54;

          const stsPerCm = sts / widthCm;
          const rowsPerCm = rows / heightCm;
          const stsPerIn = sts / widthIn;
          const rowsPerIn = rows / heightIn;

          const stsPer10cm = stsPerCm * 10;
          const rowsPer10cm = rowsPerCm * 10;
          const stsPer4in = stsPerIn * 4;
          const rowsPer4in = rowsPerIn * 4;

          document.getElementById("gauge-sts-10cm").textContent = roundTo(
            stsPer10cm,
            1
          );
          document.getElementById("gauge-rows-10cm").textContent = roundTo(
            rowsPer10cm,
            1
          );
          document.getElementById("gauge-sts-4in").textContent = roundTo(
            stsPer4in,
            1
          );
          document.getElementById("gauge-rows-4in").textContent = roundTo(
            rowsPer4in,
            1
          );

          document.getElementById("gauge-sts-cm").textContent = roundTo(
            stsPerCm,
            2
          );
          document.getElementById("gauge-rows-cm").textContent = roundTo(
            rowsPerCm,
            2
          );
          document.getElementById("gauge-sts-in").textContent = roundTo(
            stsPerIn,
            2
          );
          document.getElementById("gauge-rows-in").textContent = roundTo(
            rowsPerIn,
            2
          );

          document.getElementById("gauge-results").classList.remove("hidden");
        });

      // TAB 2: Adjust stitch & row counts
      document
        .getElementById("btn-adjust-gauge")
        .addEventListener("click", () => {
          const GpSts = parseFloat(
            document.getElementById("pattern-sts-10").value
          );
          const GpRows = parseFloat(
            document.getElementById("pattern-rows-10").value
          );
          const GySts = parseFloat(
            document.getElementById("your-sts-10").value
          );
          const GyRows = parseFloat(
            document.getElementById("your-rows-10").value
          );
          const NpSts = parseFloat(
            document.getElementById("pattern-sts-count").value
          );
          const NpRows = parseFloat(
            document.getElementById("pattern-rows-count").value
          );

          if (
            !GpSts ||
            !GySts ||
            !NpSts ||
            GpSts <= 0 ||
            GySts <= 0 ||
            NpSts <= 0
          ) {
            alert(
              "Please enter positive values for pattern gauge (stitches), your gauge (stitches), and pattern stitch count."
            );
            return;
          }

          const NySts = NpSts * (GySts / GpSts);

          document.getElementById("adjust-pattern-sts").textContent =
            Math.round(NpSts);
          document.getElementById("adjust-your-sts").textContent =
            Math.round(NySts);

          if (
            GpRows &&
            GyRows &&
            NpRows &&
            GpRows > 0 &&
            GyRows > 0 &&
            NpRows > 0
          ) {
            const NyRows = NpRows * (GyRows / GpRows);
            document.getElementById("adjust-pattern-rows").textContent =
              Math.round(NpRows);
            document.getElementById("adjust-your-rows").textContent =
              Math.round(NyRows);
          } else {
            document.getElementById("adjust-pattern-rows").textContent = "–";
            document.getElementById("adjust-your-rows").textContent = "–";
          }

          document.getElementById("adjust-results").classList.remove("hidden");
        });

      // TAB 3: Yarn / size substitution
      document.getElementById("btn-yarn-sub").addEventListener("click", () => {
        const Gp10 = parseFloat(
          document.getElementById("sub-pattern-sts-10").value
        );
        const Gp4 = parseFloat(
          document.getElementById("sub-pattern-sts-4").value
        );
        const Gy10 = parseFloat(
          document.getElementById("sub-your-sts-10").value
        );
        const Gy4 = parseFloat(document.getElementById("sub-your-sts-4").value);
        const targetWidth = parseFloat(
          document.getElementById("sub-target-width").value
        );
        const targetUnit = document.getElementById(
          "sub-target-width-unit"
        ).value;
        const sizeSts = parseFloat(
          document.getElementById("sub-size-sts").value
        );

        if (!sizeSts || sizeSts <= 0) {
          alert(
            "Please enter the stitch count for the pattern size you plan to follow."
          );
          return;
        }

        // Determine pattern gauge in sts per cm
        let GpPerCm = null;
        if (Gp10 && Gp10 > 0) {
          GpPerCm = Gp10 / 10;
        } else if (Gp4 && Gp4 > 0) {
          const GpPerIn = Gp4 / 4;
          GpPerCm = GpPerIn / 2.54;
        }

        // Determine your gauge in sts per cm
        let GyPerCm = null;
        if (Gy10 && Gy10 > 0) {
          GyPerCm = Gy10 / 10;
        } else if (Gy4 && Gy4 > 0) {
          const GyPerIn = Gy4 / 4;
          GyPerCm = GyPerIn / 2.54;
        }

        if (!GyPerCm || GyPerCm <= 0) {
          alert("Please enter your gauge (stitches per 10 cm or per 4 in).");
          return;
        }

        // Width you will get with your gauge and chosen size
        const yourWidthCm = sizeSts / GyPerCm;
        const yourWidthIn = yourWidthCm / 2.54;

        // Stitches needed for target width (if provided)
        let stsNeeded = null;
        if (targetWidth && targetWidth > 0) {
          const targetWidthCm =
            targetUnit === "cm" ? targetWidth : targetWidth * 2.54;
          stsNeeded = targetWidthCm * GyPerCm;
          document.getElementById("sub-target-width-label").textContent =
            targetWidth + " " + targetUnit;
        } else {
          document.getElementById("sub-target-width-label").textContent =
            roundTo(yourWidthCm, 1) +
            " cm (" +
            roundTo(yourWidthIn, 1) +
            " in)";
        }

        document.getElementById("sub-your-width").textContent =
          roundTo(yourWidthCm, 1) + " cm (" + roundTo(yourWidthIn, 1) + " in)";

        document.getElementById("sub-sts-needed").textContent =
          stsNeeded && stsNeeded > 0 ? Math.round(stsNeeded) : "–";

        document.getElementById("sub-results").classList.remove("hidden");
      });
    </script>

