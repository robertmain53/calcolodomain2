    <script>
      // --- Missouri tax model (approximate, for educational use) ---
      // Define brackets as [{limit, rate}] where limit is upper bound of bracket.
      // NOTE: These are illustrative; always verify against current MO DOR tables.
      const MO_BRACKETS = [
        { limit: 1000, rate: 0.0 },
        { limit: 2000, rate: 0.02 },
        { limit: 3000, rate: 0.025 },
        { limit: 4000, rate: 0.03 },
        { limit: 5000, rate: 0.035 },
        { limit: 6000, rate: 0.04 },
        { limit: 7000, rate: 0.045 },
        { limit: Infinity, rate: 0.0495 }, // top rate example
      ];

      // Approximate 2024 federal standard deductions (used as proxy for MO standard deduction)
      const FED_STANDARD_DEDUCTION = {
        single: 14600,
        married_joint: 29200,
        married_separate: 14600,
        hoh: 21900,
      };

      function formatMoney(value) {
        if (!isFinite(value)) return "$0";
        return value.toLocaleString("en-US", {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 0,
        });
      }

      function formatPercent(value) {
        if (!isFinite(value)) return "0%";
        return (value * 100).toFixed(1).replace(/\.0$/, "") + "%";
      }

      function getAnnualIncome() {
        const period =
          document.querySelector(".mo-income-period.bg-blue-50")?.dataset
            .incomePeriod || "annual";
        const incomeInput =
          parseFloat(document.getElementById("mo-income-input").value) || 0;
        if (period === "annual") return incomeInput;
        if (period === "monthly") return incomeInput * 12;
        if (period === "hourly") {
          const hours =
            parseFloat(document.getElementById("mo-hours-per-week").value) || 0;
          return incomeInput * hours * 52;
        }
        return incomeInput;
      }

      function computeMoTax(taxableIncome) {
        let remaining = Math.max(0, taxableIncome);
        let lastLimit = 0;
        let totalTax = 0;
        let marginalRate = 0;
        const breakdown = [];

        for (const bracket of MO_BRACKETS) {
          if (remaining <= 0) break;
          const upper = bracket.limit;
          const span = Math.min(remaining, upper - lastLimit);
          if (span > 0) {
            const taxHere = span * bracket.rate;
            breakdown.push({
              from: lastLimit,
              to: upper,
              rate: bracket.rate,
              amount: taxHere,
            });
            totalTax += taxHere;
            marginalRate = bracket.rate;
            remaining -= span;
            lastLimit = upper;
          }
        }

        return { totalTax, marginalRate, breakdown };
      }

      function updateStandardDeductionUI() {
        const status = document.getElementById("mo-filing-status").value;
        const useStd = document.getElementById(
          "mo-use-standard-deduction"
        ).checked;
        const stdInput = document.getElementById(
          "mo-federal-standard-deduction"
        );
        const knob = document.getElementById("mo-standard-toggle-knob");

        if (useStd) {
          stdInput.value =
            FED_STANDARD_DEDUCTION[status] || FED_STANDARD_DEDUCTION.single;
          stdInput.readOnly = true;
          stdInput.classList.add("bg-gray-100");
          knob.classList.add("translate-x-5");
          knob.parentElement.classList.remove("bg-gray-300");
          knob.parentElement.classList.add("bg-blue-500");
        } else {
          stdInput.readOnly = false;
          stdInput.classList.remove("bg-gray-100");
          knob.classList.remove("translate-x-5");
          knob.parentElement.classList.add("bg-gray-300");
          knob.parentElement.classList.remove("bg-blue-500");
        }
      }

      function updateIncomeLabels() {
        const periodBtn = document.querySelector(
          ".mo-income-period.bg-blue-50"
        );
        const period = periodBtn ? periodBtn.dataset.incomePeriod : "annual";
        const label = document.querySelector('label[for="mo-income-input"]');
        const helper = document.getElementById("mo-income-helper");
        const hoursGroup = document.getElementById("mo-hours-per-week-group");

        if (period === "annual") {
          label.textContent = "Annual gross income ($)";
          helper.textContent =
            "Enter your total annual gross income before taxes.";
          hoursGroup.classList.add("hidden");
        } else if (period === "monthly") {
          label.textContent = "Monthly gross income ($)";
          helper.textContent =
            "We will multiply by 12 to estimate your annual income.";
          hoursGroup.classList.add("hidden");
        } else {
          label.textContent = "Hourly wage ($/hour)";
          helper.textContent =
            "We will multiply by your hours per week and 52 weeks to estimate annual income.";
          hoursGroup.classList.remove("hidden");
        }
      }

      function renderBracketBreakdown(breakdown) {
        const container = document.getElementById("mo-bracket-breakdown");
        if (!breakdown.length) {
          container.textContent =
            "No Missouri income tax owed at this income level.";
          return;
        }
        container.innerHTML = "";
        breakdown.forEach((b) => {
          const div = document.createElement("div");
          const from = b.from;
          const to = isFinite(b.to) ? b.to : "and above";
          div.textContent = `$${from.toLocaleString()} – ${
            isFinite(b.to) ? "$" + to.toLocaleString() : to
          }: ${formatPercent(b.rate)} = ${formatMoney(b.amount)}`;
          container.appendChild(div);
        });
      }

      function calculateMoTax() {
        const grossAnnual = getAnnualIncome();
        const stdDeduction =
          parseFloat(
            document.getElementById("mo-federal-standard-deduction").value
          ) || 0;
        const otherDeduction =
          parseFloat(
            document.getElementById("mo-additional-deductions").value
          ) || 0;
        const credits =
          parseFloat(document.getElementById("mo-credits").value) || 0;

        const taxable = Math.max(
          0,
          grossAnnual - stdDeduction - otherDeduction
        );
        const { totalTax, marginalRate, breakdown } = computeMoTax(taxable);
        const taxAfterCredits = Math.max(0, totalTax - credits);
        const effectiveRate =
          grossAnnual > 0 ? taxAfterCredits / grossAnnual : 0;
        const afterTaxIncome = grossAnnual - taxAfterCredits;

        document.getElementById("mo-result-gross").textContent =
          formatMoney(grossAnnual);
        document.getElementById("mo-result-taxable").textContent =
          formatMoney(taxable);
        document.getElementById("mo-result-tax").textContent =
          formatMoney(taxAfterCredits);
        document.getElementById("mo-result-effective-rate").textContent =
          formatPercent(effectiveRate);
        document.getElementById("mo-result-marginal-rate").textContent =
          formatPercent(taxable > 0 ? marginalRate : 0);
        document.getElementById("mo-result-after-tax").textContent =
          formatMoney(afterTaxIncome);

        renderBracketBreakdown(breakdown);
      }

      document.addEventListener("DOMContentLoaded", () => {
        // Initialize standard deduction
        updateStandardDeductionUI();
        updateIncomeLabels();
        calculateMoTax();

        document
          .getElementById("mo-filing-status")
          .addEventListener("change", () => {
            updateStandardDeductionUI();
            calculateMoTax();
          });

        document
          .getElementById("mo-use-standard-deduction")
          .addEventListener("change", () => {
            updateStandardDeductionUI();
            calculateMoTax();
          });

        document.querySelectorAll(".mo-income-period").forEach((btn) => {
          btn.addEventListener("click", () => {
            document.querySelectorAll(".mo-income-period").forEach((b) => {
              b.classList.remove(
                "bg-blue-50",
                "border-blue-600",
                "text-blue-600",
                "font-medium"
              );
              b.classList.add("border-gray-300", "text-gray-700");
            });
            btn.classList.add(
              "bg-blue-50",
              "border-blue-600",
              "text-blue-600",
              "font-medium"
            );
            btn.classList.remove("border-gray-300", "text-gray-700");
            updateIncomeLabels();
            calculateMoTax();
          });
        });

        [
          "mo-income-input",
          "mo-hours-per-week",
          "mo-federal-standard-deduction",
          "mo-additional-deductions",
          "mo-credits",
        ].forEach((id) => {
          const el = document.getElementById(id);
          if (el) {
            el.addEventListener("input", () => {
              calculateMoTax();
            });
          }
        });

        document
          .getElementById("mo-calc-btn")
          .addEventListener("click", calculateMoTax);
      });
    </script>

