    <script>
      // Utility: clamp
      const clamp = (v, min, max) => Math.min(max, Math.max(min, v));

      // Color conversion helpers
      function hexToRgb(hex) {
        hex = hex.replace("#", "").trim();
        if (hex.length === 3) {
          hex = hex
            .split("")
            .map((c) => c + c)
            .join("");
        }
        if (hex.length !== 6 && hex.length !== 8) return null;
        const bigint = parseInt(hex.slice(0, 6), 16);
        if (Number.isNaN(bigint)) return null;
        const r = (bigint >> 16) & 255;
        const g = (bigint >> 8) & 255;
        const b = bigint & 255;
        return { r, g, b };
      }

      function rgbToHex(r, g, b) {
        const toHex = (c) => {
          const h = clamp(Math.round(c), 0, 255).toString(16).padStart(2, "0");
          return h.toUpperCase();
        };
        return "#" + toHex(r) + toHex(g) + toHex(b);
      }

      function rgbToHsl(r, g, b) {
        r / 255;
        g / 255;
        b / 255;
        const max = Math.max(r, g, b),
          min = Math.min(r, g, b);
        let h,
          s,
          l = (max + min) / 2;
        if (max === min) {
          h = s = 0;
        } else {
          const d = max - min;
          s = d / (1 - Math.abs(2 * l - 1));
          switch (max) {
            case r:
              h = 60 * (((g - b) / d) % 6);
              break;
            case g:
              h = 60 * ((b - r) / d + 2);
              break;
            case b:
              h = 60 * ((r - g) / d + 4);
              break;
          }
        }
        if (h < 0) h += 360;
        return { h, s: s * 100, l: l * 100 };
      }

      function hslToRgb(h, s, l) {
        s / 100;
        l / 100;
        const c = (1 - Math.abs(2 * l - 1)) * s;
        const x = c * (1 - Math.abs(((h / 60) % 2) - 1));
        const m = l - c / 2;
        let r1, g1, b1;
        if (h < 60) {
          r1 = c;
          g1 = x;
          b1 = 0;
        } else if (h < 120) {
          r1 = x;
          g1 = c;
          b1 = 0;
        } else if (h < 180) {
          r1 = 0;
          g1 = c;
          b1 = x;
        } else if (h < 240) {
          r1 = 0;
          g1 = x;
          b1 = c;
        } else if (h < 300) {
          r1 = x;
          g1 = 0;
          b1 = c;
        } else {
          r1 = c;
          g1 = 0;
          b1 = x;
        }
        return {
          r: Math.round((r1 + m) * 255),
          g: Math.round((g1 + m) * 255),
          b: Math.round((b1 + m) * 255),
        };
      }

      function rgbToHsv(r, g, b) {
        r / 255;
        g / 255;
        b / 255;
        const max = Math.max(r, g, b),
          min = Math.min(r, g, b);
        const d = max - min;
        let h;
        if (d === 0) h = 0;
        else if (max === r) h = 60 * (((g - b) / d) % 6);
        else if (max === g) h = 60 * ((b - r) / d + 2);
        else h = 60 * ((r - g) / d + 4);
        if (h < 0) h += 360;
        const s = max === 0 ? 0 : d / max;
        const v = max;
        return { h, s: s * 100, v: v * 100 };
      }

      function hsvToRgb(h, s, v) {
        s / 100;
        v / 100;
        const c = v * s;
        const x = c * (1 - Math.abs(((h / 60) % 2) - 1));
        const m = v - c;
        let r1, g1, b1;
        if (h < 60) {
          r1 = c;
          g1 = x;
          b1 = 0;
        } else if (h < 120) {
          r1 = x;
          g1 = c;
          b1 = 0;
        } else if (h < 180) {
          r1 = 0;
          g1 = c;
          b1 = x;
        } else if (h < 240) {
          r1 = 0;
          g1 = x;
          b1 = c;
        } else if (h < 300) {
          r1 = x;
          g1 = 0;
          b1 = c;
        } else {
          r1 = c;
          g1 = 0;
          b1 = x;
        }
        return {
          r: Math.round((r1 + m) * 255),
          g: Math.round((g1 + m) * 255),
          b: Math.round((b1 + m) * 255),
        };
      }

      function rgbToCmyk(r, g, b) {
        const r1 = r / 255,
          g1 = g / 255,
          b1 = b / 255;
        const k = 1 - Math.max(r1, g1, b1);
        if (k === 0) return { c: 0, m: 0, y: 0, k: 0 };
        const c = (1 - r1 - k) / (1 - k);
        const m = (1 - g1 - k) / (1 - k);
        const y = (1 - b1 - k) / (1 - k);
        return { c: c * 100, m: m * 100, y: y * 100, k: k * 100 };
      }

      // Approximate sRGB -> CIELAB via XYZ (D65)
      function rgbToLab(r, g, b) {
        // normalize
        let r1 = r / 255,
          g1 = g / 255,
          b1 = b / 255;
        // gamma correction
        [r1, g1, b1] = [r1, g1, b1].map((v) =>
          v > 0.04045 ? Math.pow((v + 0.055) / 1.055, 2.4) : v / 12.92
        );
        // sRGB to XYZ (D65)
        const x = (r1 * 0.4124 + g1 * 0.3576 + b1 * 0.1805) / 0.95047;
        const y = (r1 * 0.2126 + g1 * 0.7152 + b1 * 0.0722) / 1.0;
        const z = (r1 * 0.0193 + g1 * 0.1192 + b1 * 0.9505) / 1.08883;
        const f = (t) => (t > 0.008856 ? Math.cbrt(t) : 7.787 * t + 16 / 116);
        const fx = f(x),
          fy = f(y),
          fz = f(z);
        const L = 116 * fy - 16;
        const a = 500 * (fx - fy);
        const b2 = 200 * (fy - fz);
        return { L, a, b: b2 };
      }

      // Relative luminance and contrast (WCAG)
      function relativeLuminance(r, g, b) {
        const srgb = [r, g, b].map((v) => v / 255);
        const rgb = srgb.map((v) =>
          v <= 0.03928 ? v / 12.92 : Math.pow((v + 0.055) / 1.055, 2.4)
        );
        return 0.2126 * rgb[0] + 0.7152 * rgb[1] + 0.0722 * rgb[2];
      }

      function contrastRatio(rgb1, rgb2) {
        const L1 = relativeLuminance(rgb1.r, rgb1.g, rgb1.b);
        const L2 = relativeLuminance(rgb2.r, rgb2.g, rgb2.b);
        const lighter = Math.max(L1, L2);
        const darker = Math.min(L1, L2);
        return (lighter + 0.05) / (darker + 0.05);
      }

      // Global state
      let currentColor = { r: 37, g: 99, b: 235 };

      function updateAllFromRgb(rgb, options = {}) {
        currentColor = { ...rgb };
        const { r, g, b } = currentColor;
        const hex = rgbToHex(r, g, b);

        // Update base color input and preview
        if (!options.skipPicker) {
          const colorInput = document.getElementById("color-input");
          if (colorInput) colorInput.value = hex;
        }
        const preview = document.getElementById("color-preview");
        const previewText = document.getElementById("color-preview-text");
        const luminanceLabel = document.getElementById("color-luminance-label");
        const contrastBadge = document.getElementById("color-contrast-text");
        if (preview) preview.style.backgroundColor = hex;
        if (previewText) previewText.textContent = hex.toUpperCase();

        const lum = relativeLuminance(r, g, b);
        if (luminanceLabel)
          luminanceLabel.textContent = `Luminance: ${lum.toFixed(3)}`;
        if (contrastBadge) {
          const crWhite = contrastRatio(
            { r, g, b },
            { r: 255, g: 255, b: 255 }
          );
          const crBlack = contrastRatio({ r, g, b }, { r: 0, g: 0, b: 0 });
          const best = Math.max(crWhite, crBlack);
          let rating = "Fail";
          if (best >= 7) rating = "AAA";
          else if (best >= 4.5) rating = "AA";
          else if (best >= 3) rating = "AA Large";
          contrastBadge.textContent = rating;
        }

        // HEX section
        const hexInput = document.getElementById("hex-input");
        if (hexInput && document.activeElement !== hexInput)
          hexInput.value = hex;

        const hexShortEl = document.getElementById("hex-short");
        const hexAlphaEl = document.getElementById("hex-alpha");
        if (hexShortEl) {
          const rr = hex[1] + hex[2];
          const gg = hex[3] + hex[4];
          const bb = hex[5] + hex[6];
          if (rr[0] === rr[1] && gg[0] === gg[1] && bb[0] === bb[1]) {
            hexShortEl.textContent = "#" + rr[0] + gg[0] + bb[0];
          } else {
            hexShortEl.textContent = "—";
          }
        }
        if (hexAlphaEl) hexAlphaEl.textContent = hex + "FF";

        // RGB section
        const rgbR = document.getElementById("rgb-r");
        const rgbG = document.getElementById("rgb-g");
        const rgbB = document.getElementById("rgb-b");
        if (rgbR && document.activeElement !== rgbR) rgbR.value = r;
        if (rgbG && document.activeElement !== rgbG) rgbG.value = g;
        if (rgbB && document.activeElement !== rgbB) rgbB.value = b;
        const rgbString = document.getElementById("rgb-string");
        if (rgbString) rgbString.value = `rgb(${r}, ${g}, ${b})`;

        // HSL
        const hsl = rgbToHsl(r, g, b);
        const hslH = document.getElementById("hsl-h");
        const hslS = document.getElementById("hsl-s");
        const hslL = document.getElementById("hsl-l");
        if (hslH && document.activeElement !== hslH)
          hslH.value = Math.round(hsl.h);
        if (hslS && document.activeElement !== hslS)
          hslS.value = Math.round(hsl.s);
        if (hslL && document.activeElement !== hslL)
          hslL.value = Math.round(hsl.l);
        const hslString = document.getElementById("hsl-string");
        if (hslString)
          hslString.value = `hsl(${Math.round(hsl.h)}, ${Math.round(
            hsl.s
          )}%, ${Math.round(hsl.l)}%)`;

        // HSV
        const hsv = rgbToHsv(r, g, b);
        const hsvH = document.getElementById("hsv-h");
        const hsvS = document.getElementById("hsv-s");
        const hsvV = document.getElementById("hsv-v");
        if (hsvH && document.activeElement !== hsvH)
          hsvH.value = Math.round(hsv.h);
        if (hsvS && document.activeElement !== hsvS)
          hsvS.value = Math.round(hsv.s);
        if (hsvV && document.activeElement !== hsvV)
          hsvV.value = Math.round(hsv.v);
        const hsvString = document.getElementById("hsv-string");
        if (hsvString)
          hsvString.value = `hsv(${Math.round(hsv.h)}, ${Math.round(
            hsv.s
          )}%, ${Math.round(hsv.v)}%)`;

        // CMYK
        const cmyk = rgbToCmyk(r, g, b);
        const cmykC = document.getElementById("cmyk-c");
        const cmykM = document.getElementById("cmyk-m");
        const cmykY = document.getElementById("cmyk-y");
        const cmykK = document.getElementById("cmyk-k");
        if (cmykC && document.activeElement !== cmykC)
          cmykC.value = Math.round(cmyk.c);
        if (cmykM && document.activeElement !== cmykM)
          cmykM.value = Math.round(cmyk.m);
        if (cmykY && document.activeElement !== cmykY)
          cmykY.value = Math.round(cmyk.y);
        if (cmykK && document.activeElement !== cmykK)
          cmykK.value = Math.round(cmyk.k);
        const cmykString = document.getElementById("cmyk-string");
        if (cmykString) {
          cmykString.value = `cmyk(${Math.round(cmyk.c)}%, ${Math.round(
            cmyk.m
          )}%, ${Math.round(cmyk.y)}%, ${Math.round(cmyk.k)}%)`;
        }

        // LAB
        const lab = rgbToLab(r, g, b);
        const labL = document.getElementById("lab-l");
        const labA = document.getElementById("lab-a");
        const labB = document.getElementById("lab-b");
        if (labL && document.activeElement !== labL)
          labL.value = lab.L.toFixed(1);
        if (labA && document.activeElement !== labA)
          labA.value = lab.a.toFixed(1);
        if (labB && document.activeElement !== labB)
          labB.value = lab.b.toFixed(1);
        const labString = document.getElementById("lab-string");
        if (labString)
          labString.value = `lab(${lab.L.toFixed(1)}, ${lab.a.toFixed(
            1
          )}, ${lab.b.toFixed(1)})`;

        // Palettes
        updatePalettes();

        // CSS snippets
        updateCssSnippets();

        // If requested, sync contrast background or text
        if (options.syncContrastBg) {
          const bgHexInput = document.getElementById("bg-color-hex");
          const bgPicker = document.getElementById("bg-color-input");
          if (bgHexInput) bgHexInput.value = hex;
          if (bgPicker) bgPicker.value = hex;
        }
        updateContrastPreview();
      }

      function createSwatch(hex) {
        const div = document.createElement("button");
        div.type = "button";
        div.className =
          "swatch h-10 w-10 rounded-md border border-gray-200 flex-shrink-0 relative group";
        div.style.backgroundColor = hex;
        div.title = hex;
        div.setAttribute("aria-label", hex);
        const label = document.createElement("span");
        label.className =
          "absolute inset-x-0 bottom-0 text-[10px] text-center text-white/90 bg-black/40 px-0.5 py-0.5 mono-input opacity-0 group-hover:opacity-100 transition-opacity";
        label.textContent = hex.replace("#", "").toUpperCase();
        div.appendChild(label);
        div.addEventListener("click", () => {
          const rgb = hexToRgb(hex);
          if (rgb) updateAllFromRgb(rgb);
          navigator.clipboard &&
            navigator.clipboard.writeText(hex).catch(() => {});
        });
        return div;
      }

      function updatePalettes() {
        const { r, g, b } = currentColor;
        const baseHsl = rgbToHsl(r, g, b);
        const baseHex = rgbToHex(r, g, b);

        const tintsRow = document.getElementById("tints-row");
        const shadesRow = document.getElementById("shades-row");
        const compRow = document.getElementById("complementary-row");
        const analogRow = document.getElementById("analogous-row");
        const triadicRow = document.getElementById("triadic-row");
        if (!tintsRow || !shadesRow || !compRow || !analogRow || !triadicRow)
          return;

        tintsRow.innerHTML = "";
        shadesRow.innerHTML = "";
        compRow.innerHTML = "";
        analogRow.innerHTML = "";
        triadicRow.innerHTML = "";

        // Tints: increase lightness
        for (let i = 1; i <= 6; i++) {
          const l = clamp(baseHsl.l + i * 8, 0, 100);
          const rgbTint = hslToRgb(baseHsl.h, baseHsl.s, l);
          const hex = rgbToHex(rgbTint.r, rgbTint.g, rgbTint.b);
          tintsRow.appendChild(createSwatch(hex));
        }

        // Shades: decrease lightness
        for (let i = 1; i <= 6; i++) {
          const l = clamp(baseHsl.l - i * 8, 0, 100);
          const rgbShade = hslToRgb(baseHsl.h, baseHsl.s, l);
          const hex = rgbToHex(rgbShade.r, rgbShade.g, rgbShade.b);
          shadesRow.appendChild(createSwatch(hex));
        }

        // Complementary
        const compH = (baseHsl.h + 180) % 360;
        const compRgb = hslToRgb(compH, baseHsl.s, baseHsl.l);
        compRow.appendChild(createSwatch(baseHex));
        compRow.appendChild(
          createSwatch(rgbToHex(compRgb.r, compRgb.g, compRgb.b))
        );

        // Analogous
        const ana1H = (baseHsl.h - 30 + 360) % 360;
        const ana2H = (baseHsl.h + 30) % 360;
        const ana1Rgb = hslToRgb(ana1H, baseHsl.s, baseHsl.l);
        const ana2Rgb = hslToRgb(ana2H, baseHsl.s, baseHsl.l);
        analogRow.appendChild(
          createSwatch(rgbToHex(ana1Rgb.r, ana1Rgb.g, ana1Rgb.b))
        );
        analogRow.appendChild(createSwatch(baseHex));
        analogRow.appendChild(
          createSwatch(rgbToHex(ana2Rgb.r, ana2Rgb.g, ana2Rgb.b))
        );

        // Triadic
        const tri1H = (baseHsl.h + 120) % 360;
        const tri2H = (baseHsl.h + 240) % 360;
        const tri1Rgb = hslToRgb(tri1H, baseHsl.s, baseHsl.l);
        const tri2Rgb = hslToRgb(tri2H, baseHsl.s, baseHsl.l);
        triadicRow.appendChild(createSwatch(baseHex));
        triadicRow.appendChild(
          createSwatch(rgbToHex(tri1Rgb.r, tri1Rgb.g, tri1Rgb.b))
        );
        triadicRow.appendChild(
          createSwatch(rgbToHex(tri2Rgb.r, tri2Rgb.g, tri2Rgb.b))
        );
      }

      function updateCssSnippets() {
        const { r, g, b } = currentColor;
        const hex = rgbToHex(r, g, b);
        const hsl = rgbToHsl(r, g, b);

        const cssSolid = document.getElementById("css-solid");
        const cssGradient = document.getElementById("css-gradient");
        if (!cssSolid || !cssGradient) return;

        const textColor =
          contrastRatio({ r, g, b }, { r: 255, g: 255, b: 255 }) >= 4.5
            ? "#FFFFFF"
            : "#111827";

        cssSolid.value = `/* Solid background */
.element {
  background-color: ${hex};
  color: ${textColor};
}

/* Using RGB */
.element-alt {
  background-color: rgb(${r}, ${g}, ${b});
}`;

        // gradient from lighter tint to base
        const lighter = hslToRgb(hsl.h, hsl.s, clamp(hsl.l + 15, 0, 100));
        const lighterHex = rgbToHex(lighter.r, lighter.g, lighter.b);

        cssGradient.value = `/* Simple linear gradient */
.element {
  background: linear-gradient(135deg, ${lighterHex}, ${hex});
  color: ${textColor};
}`;
      }

      function parseHexInput(value) {
        let v = value.trim();
        if (!v) return null;
        if (v[0] !== "#") v = "#" + v;
        const rgb = hexToRgb(v);
        return rgb;
      }

      // Contrast checker
      function updateContrastPreview() {
        const textHexInput = document.getElementById("text-color-hex");
        const bgHexInput = document.getElementById("bg-color-hex");
        if (!textHexInput || !bgHexInput) return;
        const textRgb = parseHexInput(textHexInput.value) || {
          r: 17,
          g: 24,
          b: 39,
        };
        const bgRgb = parseHexInput(bgHexInput.value) || {
          r: 255,
          g: 255,
          b: 255,
        };

        const ratio = contrastRatio(textRgb, bgRgb);
        const ratioEl = document.getElementById("contrast-ratio");
        const ratingEl = document.getElementById("contrast-rating");
        const aaNormal = document.getElementById("contrast-aa-normal");
        const aaaNormal = document.getElementById("contrast-aaa-normal");
        const aaLarge = document.getElementById("contrast-aa-large");
        const aaaLarge = document.getElementById("contrast-aaa-large");
        const previewBox = document.getElementById("contrast-preview-box");
        const previewText = document.getElementById("contrast-preview-text");

        if (previewBox)
          previewBox.style.backgroundColor = rgbToHex(
            bgRgb.r,
            bgRgb.g,
            bgRgb.b
          );
        if (previewText)
          previewText.style.color = rgbToHex(textRgb.r, textRgb.g, textRgb.b);

        if (ratioEl) ratioEl.textContent = ratio.toFixed(2) + ":1";

        let rating = "Fail";
        if (ratio >= 7) rating = "AAA";
        else if (ratio >= 4.5) rating = "AA";
        else if (ratio >= 3) rating = "AA Large";
        if (ratingEl) ratingEl.textContent = "WCAG rating: " + rating;

        if (aaNormal)
          aaNormal.textContent =
            "AA normal text: " + (ratio >= 4.5 ? "Pass" : "Fail");
        if (aaaNormal)
          aaaNormal.textContent =
            "AAA normal text: " + (ratio >= 7 ? "Pass" : "Fail");
        if (aaLarge)
          aaLarge.textContent =
            "AA large text: " + (ratio >= 3 ? "Pass" : "Fail");
        if (aaaLarge)
          aaaLarge.textContent =
            "AAA large text: " + (ratio >= 4.5 ? "Pass" : "Fail");
      }

      document.addEventListener("DOMContentLoaded", () => {
        // Initialize from default color
        updateAllFromRgb(currentColor, { syncContrastBg: true });

        // Tabs
        document.querySelectorAll("[data-format-tab]").forEach((btn) => {
          btn.addEventListener("click", () => {
            const target = btn.getAttribute("data-format-tab");
            document.querySelectorAll("[data-format-tab]").forEach((b) => {
              if (b === btn) {
                b.classList.remove("tab-inactive");
                b.classList.add("tab-active");
              } else {
                b.classList.remove("tab-active");
                b.classList.add("tab-inactive");
              }
            });
            document
              .querySelectorAll("[data-format-panel]")
              .forEach((panel) => {
                panel.classList.toggle(
                  "hidden",
                  panel.getAttribute("data-format-panel") !== target
                );
              });
          });
        });

        // Base color input
        const colorInput = document.getElementById("color-input");
        if (colorInput) {
          colorInput.addEventListener("input", () => {
            const rgb = hexToRgb(colorInput.value);
            if (rgb) updateAllFromRgb(rgb);
          });
        }

        // HEX input
        const hexInput = document.getElementById("hex-input");
        if (hexInput) {
          hexInput.addEventListener("change", () => {
            const rgb = parseHexInput(hexInput.value);
            if (rgb) updateAllFromRgb(rgb);
          });
          hexInput.addEventListener("blur", () => {
            const rgb = parseHexInput(hexInput.value);
            if (rgb) {
              hexInput.value = rgbToHex(rgb.r, rgb.g, rgb.b);
            }
          });
        }

        // RGB inputs
        ["rgb-r", "rgb-g", "rgb-b"].forEach((id) => {
          const el = document.getElementById(id);
          if (!el) return;
          el.addEventListener("change", () => {
            const r = parseInt(document.getElementById("rgb-r").value, 10);
            const g = parseInt(document.getElementById("rgb-g").value, 10);
            const b = parseInt(document.getElementById("rgb-b").value, 10);
            if ([r, g, b].some((v) => Number.isNaN(v))) return;
            updateAllFromRgb({
              r: clamp(r, 0, 255),
              g: clamp(g, 0, 255),
              b: clamp(b, 0, 255),
            });
          });
        });

        // HSL inputs
        ["hsl-h", "hsl-s", "hsl-l"].forEach((id) => {
          const el = document.getElementById(id);
          if (!el) return;
          el.addEventListener("change", () => {
            const h = parseFloat(document.getElementById("hsl-h").value);
            const s = parseFloat(document.getElementById("hsl-s").value);
            const l = parseFloat(document.getElementById("hsl-l").value);
            if ([h, s, l].some((v) => Number.isNaN(v))) return;
            const rgb = hslToRgb(
              clamp(h, 0, 360),
              clamp(s, 0, 100),
              clamp(l, 0, 100)
            );
            updateAllFromRgb(rgb);
          });
        });

        // HSV inputs
        ["hsv-h", "hsv-s", "hsv-v"].forEach((id) => {
          const el = document.getElementById(id);
          if (!el) return;
          el.addEventListener("change", () => {
            const h = parseFloat(document.getElementById("hsv-h").value);
            const s = parseFloat(document.getElementById("hsv-s").value);
            const v = parseFloat(document.getElementById("hsv-v").value);
            if ([h, s, v].some((vv) => Number.isNaN(vv))) return;
            const rgb = hsvToRgb(
              clamp(h, 0, 360),
              clamp(s, 0, 100),
              clamp(v, 0, 100)
            );
            updateAllFromRgb(rgb);
          });
        });

        // CMYK inputs
        ["cmyk-c", "cmyk-m", "cmyk-y", "cmyk-k"].forEach((id) => {
          const el = document.getElementById(id);
          if (!el) return;
          el.addEventListener("change", () => {
            const c = parseFloat(document.getElementById("cmyk-c").value) / 100;
            const m = parseFloat(document.getElementById("cmyk-m").value) / 100;
            const y = parseFloat(document.getElementById("cmyk-y").value) / 100;
            const k = parseFloat(document.getElementById("cmyk-k").value) / 100;
            if ([c, m, y, k].some((v) => Number.isNaN(v))) return;
            const r = 255 * (1 - clamp(c, 0, 1)) * (1 - clamp(k, 0, 1));
            const g = 255 * (1 - clamp(m, 0, 1)) * (1 - clamp(k, 0, 1));
            const b = 255 * (1 - clamp(y, 0, 1)) * (1 - clamp(k, 0, 1));
            updateAllFromRgb({ r, g, b });
          });
        });

        // LAB inputs (approximate inverse not implemented; treat as read-only for now)
        ["lab-l", "lab-a", "lab-b"].forEach((id) => {
          const el = document.getElementById(id);
          if (!el) return;
          el.addEventListener("input", () => {
            // Keep them editable but not driving color updates to avoid confusion.
          });
        });

        // Copy buttons
        document.querySelectorAll(".copy-btn").forEach((btn) => {
          btn.addEventListener("click", () => {
            const selector = btn.getAttribute("data-copy");
            const target = document.querySelector(selector);
            if (!target) return;
            const value = target.value || target.textContent;
            if (!value) return;
            if (navigator.clipboard && navigator.clipboard.writeText) {
              navigator.clipboard
                .writeText(value)
                .then(() => {
                  btn.textContent = "Copied";
                  setTimeout(() => {
                    btn.textContent = btn.textContent.includes("CSS")
                      ? "Copy CSS"
                      : "Copy";
                  }, 1200);
                })
                .catch(() => {});
            } else {
              const range = document.createRange();
              range.selectNodeContents(target);
              const sel = window.getSelection();
              sel.removeAllRanges();
              sel.addRange(range);
              try {
                document.execCommand("copy");
              } catch (e) {}
              sel.removeAllRanges();
            }
          });
        });

        // Image upload & sampling
        const imageUpload = document.getElementById("image-upload");
        const canvas = document.getElementById("image-canvas");
        const imageHelp = document.getElementById("image-help");
        if (imageUpload && canvas) {
          const ctx = canvas.getContext("2d");
          imageUpload.addEventListener("change", (e) => {
            const file = e.target.files && e.target.files[0];
            if (!file) return;
            const img = new Image();
            img.onload = () => {
              const maxWidth = canvas.parentElement.clientWidth || 400;
              const scale = Math.min(1, maxWidth / img.width);
              const w = img.width * scale;
              const h = img.height * scale;
              canvas.width = w;
              canvas.height = h;
              ctx.clearRect(0, 0, w, h);
              ctx.drawImage(img, 0, 0, w, h);
              canvas.classList.remove("hidden");
              if (imageHelp)
                imageHelp.textContent = "Click on the image to sample a color.";
            };
            img.src = URL.createObjectURL(file);
          });

          canvas.addEventListener("click", (e) => {
            const rect = canvas.getBoundingClientRect();
            const x = Math.floor(
              (e.clientX - rect.left) * (canvas.width / rect.width)
            );
            const y = Math.floor(
              (e.clientY - rect.top) * (canvas.height / rect.height)
            );
            const data = canvas.getContext("2d").getImageData(x, y, 1, 1).data;
            const rgb = { r: data[0], g: data[1], b: data[2] };
            updateAllFromRgb(rgb, { syncContrastBg: true });
          });
        }

        // Contrast color inputs
        const textColorPicker = document.getElementById("text-color-input");
        const textColorHex = document.getElementById("text-color-hex");
        const bgColorPicker = document.getElementById("bg-color-input");
        const bgColorHex = document.getElementById("bg-color-hex");

        if (textColorPicker && textColorHex) {
          textColorPicker.addEventListener("input", () => {
            textColorHex.value = textColorPicker.value;
            updateContrastPreview();
          });
          textColorHex.addEventListener("change", () => {
            const rgb = parseHexInput(textColorHex.value);
            if (rgb) textColorPicker.value = rgbToHex(rgb.r, rgb.g, rgb.b);
            updateContrastPreview();
          });
        }

        if (bgColorPicker && bgColorHex) {
          bgColorPicker.addEventListener("input", () => {
            bgColorHex.value = bgColorPicker.value;
            updateContrastPreview();
          });
          bgColorHex.addEventListener("change", () => {
            const rgb = parseHexInput(bgColorHex.value);
            if (rgb) bgColorPicker.value = rgbToHex(rgb.r, rgb.g, rgb.b);
            updateContrastPreview();
          });
        }

        updateContrastPreview();
      });
    </script>

