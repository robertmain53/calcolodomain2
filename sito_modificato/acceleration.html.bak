    <script>
      // Basic helpers
      const G = 9.80665; // m/s^2

      function toMetersPerSecond(value, unit) {
        if (isNaN(value)) return NaN;
        switch (unit) {
          case "m_s":
            return value;
          case "km_h":
            return (value * 1000) / 3600;
          case "ft_s":
            return value * 0.3048;
          case "mph":
            return (value * 1609.344) / 3600;
          default:
            return value;
        }
      }

      function fromMetersPerSecond(value, unit) {
        if (isNaN(value)) return NaN;
        switch (unit) {
          case "m_s":
            return value;
          case "km_h":
            return value * 3.6;
          case "ft_s":
            return value / 0.3048;
          case "mph":
            return (value * 3600) / 1609.344;
          default:
            return value;
        }
      }

      function toSeconds(value, unit) {
        if (isNaN(value)) return NaN;
        switch (unit) {
          case "s":
            return value;
          case "ms":
            return value / 1000;
          case "min":
            return value * 60;
          case "h":
            return value * 3600;
          default:
            return value;
        }
      }

      function toMeters(value, unit) {
        if (isNaN(value)) return NaN;
        switch (unit) {
          case "m":
            return value;
          case "km":
            return value * 1000;
          case "ft":
            return value * 0.3048;
          case "mi":
            return value * 1609.344;
          default:
            return value;
        }
      }

      function toAngularRadPerSec2(value, unit) {
        if (isNaN(value)) return NaN;
        switch (unit) {
          case "rad_s2":
            return value;
          case "deg_s2":
            return (value * Math.PI) / 180;
          case "rpm_s":
            return (value * 2 * Math.PI) / 60;
          default:
            return value;
        }
      }

      function formatNumber(x, digits = 4) {
        if (!isFinite(x)) return "—";
        const abs = Math.abs(x);
        if (abs !== 0 && (abs < 1e-3 || abs >= 1e5)) {
          return x.toExponential(3);
        }
        return parseFloat(x.toFixed(digits)).toString();
      }

      // Mode switching
      const modeTabs = document.querySelectorAll(".mode-tab");
      const modeSections = {
        average: document.getElementById("calc-average"),
        constant: document.getElementById("calc-constant"),
        centripetal: document.getElementById("calc-centripetal"),
        tangential: document.getElementById("calc-tangential"),
      };
      let currentMode = "average";

      modeTabs.forEach((tab) => {
        tab.addEventListener("click", () => {
          const mode = tab.getAttribute("data-mode");
          currentMode = mode;
          modeTabs.forEach((t) => t.classList.remove("tab-active"));
          tab.classList.add("tab-active");
          Object.keys(modeSections).forEach((key) => {
            modeSections[key].classList.toggle("hidden", key !== mode);
          });
          document.getElementById("error-msg").textContent = "";
          document.getElementById("result-card").classList.add("hidden");
          drawVTChart(null);
        });
      });

      // Main calculate handler
      document.getElementById("btn-calc").addEventListener("click", () => {
        document.getElementById("error-msg").textContent = "";
        let a_ms2 = NaN;
        let extra = {};

        try {
          if (currentMode === "average") {
            a_ms2 = calcAverage(extra);
          } else if (currentMode === "constant") {
            a_ms2 = calcConstant(extra);
          } else if (currentMode === "centripetal") {
            a_ms2 = calcCentripetal(extra);
          } else if (currentMode === "tangential") {
            a_ms2 = calcTangential(extra);
          }
        } catch (e) {
          document.getElementById("error-msg").textContent =
            e.message || "Invalid input.";
          document.getElementById("result-card").classList.add("hidden");
          drawVTChart(null);
          return;
        }

        if (!isFinite(a_ms2)) {
          document.getElementById("error-msg").textContent =
            "Please provide valid values for the selected mode.";
          document.getElementById("result-card").classList.add("hidden");
          drawVTChart(null);
          return;
        }

        const unitSel = document.getElementById("accel-unit").value;
        let mainVal, mainUnit;
        switch (unitSel) {
          case "m_s2":
            mainVal = a_ms2;
            mainUnit = "m/s²";
            break;
          case "ft_s2":
            mainVal = a_ms2 / 0.3048;
            mainUnit = "ft/s²";
            break;
          case "km_h_s":
            mainVal = a_ms2 * 3.6;
            mainUnit = "km/h·s";
            break;
          case "g":
            mainVal = a_ms2 / G;
            mainUnit = "g";
            break;
          default:
            mainVal = a_ms2;
            mainUnit = "m/s²";
        }

        const resultMain = document.getElementById("result-main");
        const resultAlt = document.getElementById("result-alt");
        const resultG = document.getElementById("result-g");
        const card = document.getElementById("result-card");

        resultMain.textContent = `${formatNumber(mainVal)} ${mainUnit}`;
        resultAlt.textContent = `= ${formatNumber(
          a_ms2
        )} m/s²  ≈ ${formatNumber(a_ms2 / 0.3048)} ft/s²`;
        resultG.textContent = `This corresponds to about ${formatNumber(
          a_ms2 / G,
          3
        )} g (multiples of standard gravity).`;
        card.classList.remove("hidden");

        if (
          currentMode === "constant" &&
          extra.v0 !== undefined &&
          extra.v !== undefined &&
          extra.t !== undefined
        ) {
          drawVTChart({ v0: extra.v0, v: extra.v, t: extra.t });
        } else {
          drawVTChart(null);
        }
      });

      document.getElementById("btn-reset").addEventListener("click", () => {
        document
          .querySelectorAll('input[type="number"]')
          .forEach((inp) => (inp.value = ""));
        document.getElementById("error-msg").textContent = "";
        document.getElementById("result-card").classList.add("hidden");
        drawVTChart(null);
      });

      // Mode-specific calculations
      function calcAverage(extra) {
        const v1 = parseFloat(document.getElementById("avg-v1").value);
        const v2 = parseFloat(document.getElementById("avg-v2").value);
        const dt = parseFloat(document.getElementById("avg-dt").value);
        const v1u = document.getElementById("avg-v-unit").value;
        const v2u = document.getElementById("avg-v2-unit").value;
        const dtu = document.getElementById("avg-dt-unit").value;

        if (isNaN(v1) || isNaN(v2) || isNaN(dt)) {
          throw new Error(
            "Enter initial velocity, final velocity, and time interval."
          );
        }
        const v1_ms = toMetersPerSecond(v1, v1u);
        const v2_ms = toMetersPerSecond(v2, v2u);
        const dt_s = toSeconds(dt, dtu);
        if (dt_s === 0) throw new Error("Time interval must be non-zero.");

        return (v2_ms - v1_ms) / dt_s;
      }

      function calcConstant(extra) {
        const v0 =
          document.getElementById("const-v0").value === ""
            ? null
            : parseFloat(document.getElementById("const-v0").value);
        const v =
          document.getElementById("const-v").value === ""
            ? null
            : parseFloat(document.getElementById("const-v").value);
        const t =
          document.getElementById("const-t").value === ""
            ? null
            : parseFloat(document.getElementById("const-t").value);
        const s =
          document.getElementById("const-s").value === ""
            ? null
            : parseFloat(document.getElementById("const-s").value);

        const v0u = document.getElementById("const-v0-unit").value;
        const vu = document.getElementById("const-v-unit").value;
        const tu = document.getElementById("const-t-unit").value;
        const su = document.getElementById("const-s-unit").value;

        const known = [v0, v, t, s].filter((x) => x !== null).length;
        if (known < 3) {
          throw new Error(
            "Provide at least three of v₀, v, t, and s (leave exactly one blank)."
          );
        }

        let v0_ms = v0 !== null ? toMetersPerSecond(v0, v0u) : null;
        let v_ms = v !== null ? toMetersPerSecond(v, vu) : null;
        let t_s = t !== null ? toSeconds(t, tu) : null;
        let s_m = s !== null ? toMeters(s, su) : null;

        let a;

        if (v0_ms !== null && v_ms !== null && t_s !== null) {
          if (t_s === 0) throw new Error("Time must be non-zero.");
          a = (v_ms - v0_ms) / t_s;
          if (s_m === null) {
            s_m = ((v0_ms + v_ms) / 2) * t_s;
            document.getElementById("const-s").value = formatNumber(
              fromMeters(s_m, su)
            );
          }
        } else if (v0_ms !== null && v_ms !== null && s_m !== null) {
          const dv2 = v_ms * v_ms - v0_ms * v0_ms;
          if (s_m === 0) throw new Error("Displacement must be non-zero.");
          a = dv2 / (2 * s_m);
          if (t_s === null) {
            t_s = (v_ms - v0_ms) / a;
            document.getElementById("const-t").value = formatNumber(
              fromSeconds(t_s, tu)
            );
          }
        } else if (v0_ms !== null && t_s !== null && s_m !== null) {
          if (t_s === 0) throw new Error("Time must be non-zero.");
          a = (2 * (s_m - v0_ms * t_s)) / (t_s * t_s);
          if (v_ms === null) {
            v_ms = v0_ms + a * t_s;
            document.getElementById("const-v").value = formatNumber(
              fromMetersPerSecond(v_ms, vu)
            );
          }
        } else if (v_ms !== null && t_s !== null && s_m !== null) {
          if (t_s === 0) throw new Error("Time must be non-zero.");
          a = (2 * (v_ms * t_s - s_m)) / (t_s * t_s);
          if (v0_ms === null) {
            v0_ms = v_ms - a * t_s;
            document.getElementById("const-v0").value = formatNumber(
              fromMetersPerSecond(v0_ms, v0u)
            );
          }
        } else {
          throw new Error("Unsupported combination of known values.");
        }

        extra.v0 = v0_ms;
        extra.v = v_ms;
        extra.t = t_s;
        return a;
      }

      function fromMeters(value, unit) {
        switch (unit) {
          case "m":
            return value;
          case "km":
            return value / 1000;
          case "ft":
            return value / 0.3048;
          case "mi":
            return value / 1609.344;
          default:
            return value;
        }
      }

      function fromSeconds(value, unit) {
        switch (unit) {
          case "s":
            return value;
          case "ms":
            return value * 1000;
          case "min":
            return value / 60;
          case "h":
            return value / 3600;
          default:
            return value;
        }
      }

      function calcCentripetal(extra) {
        const v = parseFloat(document.getElementById("cent-v").value);
        const r = parseFloat(document.getElementById("cent-r").value);
        const vu = document.getElementById("cent-v-unit").value;
        const ru = document.getElementById("cent-r-unit").value;

        if (isNaN(v) || isNaN(r)) {
          throw new Error("Enter both speed and radius.");
        }
        const v_ms = toMetersPerSecond(v, vu);
        const r_m = toMeters(r, ru);
        if (r_m === 0) throw new Error("Radius must be non-zero.");
        return (v_ms * v_ms) / r_m;
      }

      function calcTangential(extra) {
        const alpha = parseFloat(document.getElementById("tan-alpha").value);
        const r = parseFloat(document.getElementById("tan-r").value);
        const au = document.getElementById("tan-alpha-unit").value;
        const ru = document.getElementById("tan-r-unit").value;

        if (isNaN(alpha) || isNaN(r)) {
          throw new Error("Enter both angular acceleration and radius.");
        }
        const alpha_rad = toAngularRadPerSec2(alpha, au);
        const r_m = toMeters(r, ru);
        return alpha_rad * r_m;
      }

      // Simple v-t chart (no external chart library)
      const vtCanvas = document.getElementById("vt-chart");
      const vtCtx = vtCanvas.getContext ? vtCanvas.getContext("2d") : null;

      function drawVTChart(data) {
        if (!vtCtx) return;
        const ctx = vtCtx;
        const w = (vtCanvas.width = vtCanvas.clientWidth);
        const h = (vtCanvas.height = 200);

        ctx.clearRect(0, 0, w, h);
        ctx.fillStyle = "#ffffff";
        ctx.fillRect(0, 0, w, h);

        ctx.strokeStyle = "#e5e7eb";
        ctx.lineWidth = 1;
        for (let i = 1; i < 5; i++) {
          const y = (h / 5) * i;
          ctx.beginPath();
          ctx.moveTo(40, y);
          ctx.lineTo(w - 10, y);
          ctx.stroke();
        }

        ctx.strokeStyle = "#9ca3af";
        ctx.beginPath();
        ctx.moveTo(40, 10);
        ctx.lineTo(40, h - 30);
        ctx.lineTo(w - 10, h - 30);
        ctx.stroke();

        ctx.fillStyle = "#374151";
        ctx.font = "10px Inter, sans-serif";
        ctx.fillText("t", w - 20, h - 20);
        ctx.fillText("v", 25, 15);

        if (!data) return;

        const { v0, v, t } = data;
        if (!isFinite(v0) || !isFinite(v) || !isFinite(t) || t <= 0) return;

        const marginLeft = 40;
        const marginRight = 10;
        const marginTop = 10;
        const marginBottom = 30;

        const x0 = marginLeft;
        const x1 = w - marginRight;
        const y0 = h - marginBottom;
        const y1 = marginTop;

        const vMin = Math.min(v0, v, 0);
        const vMax = Math.max(v0, v, 0);
        const vRange = vMax - vMin || 1;

        function mapX(time) {
          return x0 + (time / t) * (x1 - x0);
        }
        function mapY(vel) {
          return y0 - ((vel - vMin) / vRange) * (y0 - y1);
        }

        ctx.strokeStyle = "#2563eb";
        ctx.lineWidth = 2;
        ctx.beginPath();
        ctx.moveTo(mapX(0), mapY(v0));
        ctx.lineTo(mapX(t), mapY(v));
        ctx.stroke();

        ctx.fillStyle = "#2563eb";
        ctx.beginPath();
        ctx.arc(mapX(0), mapY(v0), 3, 0, 2 * Math.PI);
        ctx.arc(mapX(t), mapY(v), 3, 0, 2 * Math.PI);
        ctx.fill();

        ctx.fillStyle = "#4b5563";
        ctx.fillText("0", x0 - 10, y0 + 12);
        ctx.fillText(formatNumber(t, 2), x1 - 10, y0 + 12);
        ctx.fillText(formatNumber(v0, 2), x0 - 30, mapY(v0) + 3);
        ctx.fillText(formatNumber(v, 2), x0 - 30, mapY(v) + 3);
      }

      drawVTChart(null);

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
