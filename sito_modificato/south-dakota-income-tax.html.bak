    <script>
      // --- Federal tax parameters (approximate 2024-2025 values) ---
      const FEDERAL_BRACKETS = {
        single: [
          { upTo: 11600, rate: 0.1 },
          { upTo: 47150, rate: 0.12 },
          { upTo: 100525, rate: 0.22 },
          { upTo: 191950, rate: 0.24 },
          { upTo: 243725, rate: 0.32 },
          { upTo: 609350, rate: 0.35 },
          { upTo: Infinity, rate: 0.37 },
        ],
        married_joint: [
          { upTo: 23200, rate: 0.1 },
          { upTo: 94300, rate: 0.12 },
          { upTo: 201050, rate: 0.22 },
          { upTo: 383900, rate: 0.24 },
          { upTo: 487450, rate: 0.32 },
          { upTo: 731200, rate: 0.35 },
          { upTo: Infinity, rate: 0.37 },
        ],
        married_separate: [
          { upTo: 11600, rate: 0.1 },
          { upTo: 47150, rate: 0.12 },
          { upTo: 100525, rate: 0.22 },
          { upTo: 191950, rate: 0.24 },
          { upTo: 243725, rate: 0.32 },
          { upTo: 365600, rate: 0.35 },
          { upTo: Infinity, rate: 0.37 },
        ],
        head_household: [
          { upTo: 16550, rate: 0.1 },
          { upTo: 63100, rate: 0.12 },
          { upTo: 100500, rate: 0.22 },
          { upTo: 191950, rate: 0.24 },
          { upTo: 243700, rate: 0.32 },
          { upTo: 609350, rate: 0.35 },
          { upTo: Infinity, rate: 0.37 },
        ],
      };

      const STANDARD_DEDUCTION = {
        single: 14600,
        married_joint: 29200,
        married_separate: 14600,
        head_household: 21900,
      };

      const SS_RATE = 0.062;
      const SS_WAGE_BASE = 168600; // approximate
      const MEDICARE_RATE = 0.0145;

      function formatMoney(value) {
        if (!isFinite(value)) return "$0";
        return "$" + value.toFixed(0).replace(/\B(?=(\d{3})+(?!\d))/g, ",");
      }

      function formatPercent(value) {
        if (!isFinite(value)) return "0%";
        return (value * 100).toFixed(1) + "%";
      }

      function computeFederalTax(taxableIncome, filingStatus) {
        const brackets =
          FEDERAL_BRACKETS[filingStatus] || FEDERAL_BRACKETS.single;
        let remaining = Math.max(0, taxableIncome);
        let tax = 0;
        let lastRate = 0;
        let prevLimit = 0;

        for (const bracket of brackets) {
          if (remaining <= 0) break;
          const span = Math.min(remaining, bracket.upTo - prevLimit);
          tax += span * bracket.rate;
          remaining -= span;
          lastRate = bracket.rate;
          prevLimit = bracket.upTo;
        }

        return { tax, marginalRate: lastRate };
      }

      function estimateChildTaxCredit(
        adjustedIncome,
        filingStatus,
        numChildren,
        numOtherDeps
      ) {
        const baseChild = 2000;
        const baseOther = 500;
        const phaseoutStart =
          filingStatus === "married_joint" ? 400000 : 200000;
        let credit = numChildren * baseChild + numOtherDeps * baseOther;

        if (adjustedIncome > phaseoutStart) {
          const excess = adjustedIncome - phaseoutStart;
          const reduction = Math.floor(excess / 1000) * 50;
          credit = Math.max(0, credit - reduction);
        }
        return credit;
      }

      function calculateSouthDakotaTax() {
        const filingStatus = document.getElementById("filingStatus").value;
        const income =
          parseFloat(document.getElementById("annualIncome").value) || 0;
        const pretax =
          parseFloat(document.getElementById("pretaxDeductions").value) || 0;
        const deductionType =
          document.querySelector('input[name="deductionType"]:checked')
            ?.value || "standard";
        const numChildren =
          parseInt(document.getElementById("numChildren").value) || 0;
        const numOtherDeps =
          parseInt(document.getElementById("numOtherDependents").value) || 0;
        const payFreq =
          parseInt(document.getElementById("payFrequency").value) || 12;

        const adjustedIncome = Math.max(0, income - pretax);
        const stdDed =
          deductionType === "standard"
            ? STANDARD_DEDUCTION[filingStatus] || STANDARD_DEDUCTION.single
            : 0;
        const taxableIncome = Math.max(0, adjustedIncome - stdDed);

        const fedResult = computeFederalTax(taxableIncome, filingStatus);
        let federalTax = fedResult.tax;

        const ctc = estimateChildTaxCredit(
          adjustedIncome,
          filingStatus,
          numChildren,
          numOtherDeps
        );
        federalTax = Math.max(0, federalTax - ctc);

        const ssTax = Math.min(adjustedIncome, SS_WAGE_BASE) * SS_RATE;
        const medicareTax = adjustedIncome * MEDICARE_RATE;
        const ficaTax = ssTax + medicareTax;

        const stateTax = 0;
        const totalTax = federalTax + ficaTax + stateTax;
        const netPay = income - totalTax;
        const effectiveRate = income > 0 ? totalTax / income : 0;

        // Update main outputs
        document.getElementById("federalTaxOut").textContent =
          formatMoney(federalTax);
        document.getElementById("ficaTaxOut").textContent =
          formatMoney(ficaTax);
        document.getElementById("totalTaxOut").textContent =
          formatMoney(totalTax);
        document.getElementById("netPayOut").textContent = formatMoney(netPay);
        document.getElementById("effectiveRateOut").textContent =
          formatPercent(effectiveRate);
        document.getElementById("marginalBracketOut").textContent =
          (fedResult.marginalRate * 100).toFixed(0) + "%";

        // Per-paycheck
        const grossPerPay = income / payFreq;
        const federalPerPay = federalTax / payFreq;
        const ficaPerPay = ficaTax / payFreq;
        const netPerPay = netPay / payFreq;

        document.getElementById("grossPerPayOut").textContent =
          formatMoney(grossPerPay);
        document.getElementById("federalPerPayOut").textContent =
          formatMoney(federalPerPay);
        document.getElementById("ficaPerPayOut").textContent =
          formatMoney(ficaPerPay);
        document.getElementById("netPerPayOut").textContent =
          formatMoney(netPerPay);

        // Breakdown table
        const tbody = document.getElementById("breakdownTableBody");
        tbody.innerHTML = "";

        const rows = [
          { label: "Gross income", value: income },
          { label: "Pre-tax deductions", value: -pretax },
          { label: "Adjusted income", value: adjustedIncome },
          { label: "Standard deduction", value: -stdDed },
          { label: "Taxable income (federal)", value: taxableIncome },
          {
            label: "Federal income tax (before credits)",
            value: fedResult.tax,
          },
          { label: "Estimated Child & Dependent Tax Credits", value: -ctc },
          { label: "Federal income tax (after credits)", value: federalTax },
          { label: "Social Security tax", value: ssTax },
          { label: "Medicare tax", value: medicareTax },
          { label: "South Dakota state income tax", value: stateTax },
          { label: "Total tax (federal + FICA + state)", value: totalTax },
          { label: "Net (take-home) pay", value: netPay },
        ];

        rows.forEach((row) => {
          const tr = document.createElement("tr");
          const tdLabel = document.createElement("td");
          const tdValue = document.createElement("td");
          tdLabel.className = "px-3 py-2 text-gray-700";
          tdValue.className = "px-3 py-2 text-right font-medium";

          tdLabel.textContent = row.label;
          tdValue.textContent = formatMoney(row.value);
          tbody.appendChild(tr);
          tr.appendChild(tdLabel);
          tr.appendChild(tdValue);
        });
      }

      document
        .getElementById("calculateBtn")
        .addEventListener("click", calculateSouthDakotaTax);

      [
        "filingStatus",
        "annualIncome",
        "pretaxDeductions",
        "numChildren",
        "numOtherDependents",
        "payFrequency",
      ].forEach((id) => {
        const el = document.getElementById(id);
        if (el) el.addEventListener("change", calculateSouthDakotaTax);
      });
      document.querySelectorAll('input[name="deductionType"]').forEach((r) => {
        r.addEventListener("change", calculateSouthDakotaTax);
      });

      document.addEventListener("DOMContentLoaded", calculateSouthDakotaTax);
    </script>

