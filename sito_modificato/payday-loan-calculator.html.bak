    <script>
      function formatMoney(value) {
        if (!isFinite(value)) return "$0.00";
        return "$" + value.toFixed(2);
      }

      function formatPercent(value) {
        if (!isFinite(value)) return "0%";
        return value.toFixed(1) + "%";
      }

      function updateCalculator() {
        const amount = Math.max(
          0,
          Number(document.getElementById("loanAmount").value) || 0
        );
        const days = Math.max(
          1,
          Number(document.getElementById("loanDays").value) || 1
        );
        const rollovers = Math.max(
          0,
          Number(document.getElementById("rollovers").value) || 0
        );

        const feeMode =
          document.querySelector('input[name="feeMode"]:checked')?.value ||
          "per100";
        let feeTerm = 0;

        if (feeMode === "per100") {
          const feePer100 = Math.max(
            0,
            Number(document.getElementById("feePer100").value) || 0
          );
          feeTerm = amount * (feePer100 / 100);
        } else {
          const termRate = Math.max(
            0,
            Number(document.getElementById("termRate").value) || 0
          );
          feeTerm = amount * (termRate / 100);
        }

        const totalFees = feeTerm * (1 + rollovers);
        const totalRepay = amount + totalFees;
        const costPercent = amount > 0 ? (totalFees / amount) * 100 : 0;

        const rTerm = amount > 0 ? feeTerm / amount : 0;
        const apr = days > 0 ? rTerm * (365 / days) * 100 : 0;
        const dailyRate = days > 0 ? (rTerm / days) * 100 : 0;

        document.getElementById("summaryPrincipal").textContent =
          formatMoney(amount);
        document.getElementById("summaryFeeFirst").textContent =
          formatMoney(feeTerm);
        document.getElementById("summaryFeeRollovers").textContent =
          formatMoney(totalFees - feeTerm);
        document.getElementById("summaryTotalFees").textContent =
          formatMoney(totalFees);
        document.getElementById("summaryTotalRepay").textContent =
          formatMoney(totalRepay);

        document.getElementById("summaryCostPercent").textContent =
          formatPercent(costPercent) + " of amount borrowed";
        document.getElementById("summaryAPR").textContent =
          formatPercent(apr) + " APR";
        document.getElementById("summaryDailyRate").textContent =
          formatPercent(dailyRate) + " per day";

        const aprWarning = document.getElementById("aprWarning");
        if (apr >= 36) {
          aprWarning.hidden = false;
        } else {
          aprWarning.hidden = true;
        }

        const rolloverMsg = document.getElementById("rolloverMessage");
        if (rollovers === 0) {
          rolloverMsg.textContent = `With 0 rollovers, you pay ${formatMoney(
            totalFees
          )} in fees and repay ${formatMoney(totalRepay)} after ${days} days.`;
        } else {
          rolloverMsg.textContent = `With ${rollovers} rollover${
            rollovers > 1 ? "s" : ""
          }, you pay ${formatMoney(
            totalFees
          )} in total fees and repay ${formatMoney(
            totalRepay
          )} after approximately ${(1 + rollovers) * days} days.`;
        }

        const rolloverWarning = document.getElementById("rolloverWarning");
        rolloverWarning.hidden = rollovers === 0;
      }

      document.addEventListener("DOMContentLoaded", function () {
        const rolloversInput = document.getElementById("rollovers");
        const rolloversValue = document.getElementById("rolloversValue");
        rolloversInput.addEventListener("input", () => {
          rolloversValue.textContent = rolloversInput.value;
        });

        document.querySelectorAll('input[name="feeMode"]').forEach((radio) => {
          radio.addEventListener("change", () => {
            const mode =
              document.querySelector('input[name="feeMode"]:checked')?.value ||
              "per100";
            const per100Wrapper = document.getElementById("per100Wrapper");
            const rateWrapper = document.getElementById("rateWrapper");
            if (mode === "per100") {
              per100Wrapper.classList.remove("hidden");
              rateWrapper.classList.add("hidden");
            } else {
              per100Wrapper.classList.add("hidden");
              rateWrapper.classList.remove("hidden");
            }
            updateCalculator();
          });
        });

        [
          "loanAmount",
          "feePer100",
          "termRate",
          "loanDays",
          "rollovers",
        ].forEach((id) => {
          const el = document.getElementById(id);
          if (el) {
            el.addEventListener("input", () => {
              if (id === "rollovers") {
                document.getElementById("rolloversValue").textContent =
                  el.value;
              }
            });
          }
        });

        document
          .getElementById("calculateBtn")
          .addEventListener("click", function (e) {
            e.preventDefault();
            updateCalculator();
          });

        updateCalculator();
      });
    </script>

