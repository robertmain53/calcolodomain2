    <script>
      // Utility: unit conversions
      function toPascals(Evalue, unit) {
        if (isNaN(Evalue)) return NaN;
        switch (unit) {
          case "GPa":
            return Evalue * 1e9;
          case "MPa":
            return Evalue * 1e6;
          case "Pa":
            return Evalue;
          default:
            return Evalue;
        }
      }

      function fromPascalsToMPa(stressPa) {
        return stressPa / 1e6;
      }

      function toAlphaPerC(alphaValue, alphaUnit) {
        if (isNaN(alphaValue)) return NaN;
        if (alphaUnit === "micro") {
          return alphaValue * 1e-6;
        }
        return alphaValue;
      }

      function convertDeltaT(deltaT, Tunit) {
        // ΔT is the same in K and °C; for °F, ΔT_F = 1.8 * ΔT_C
        if (Tunit === "F") {
          return deltaT / 1.8; // convert to equivalent °C/K
        }
        return deltaT;
      }

      function convertDeltaTBack(deltaTC, Tunit) {
        if (Tunit === "F") {
          return deltaTC * 1.8;
        }
        return deltaTC;
      }

      function formatNumber(val, decimals) {
        if (!isFinite(val)) return "–";
        if (Math.abs(val) >= 1e4 || (Math.abs(val) < 1e-3 && val !== 0)) {
          return val.toExponential(decimals);
        }
        return val.toFixed(decimals);
      }

      // Mode switching
      const modeButtons = document.querySelectorAll(".mode-btn");
      const modeDescription = document.getElementById("mode-description");
      const TfContainer = document.getElementById("Tf-container");
      const deltaTContainer = document.getElementById("deltaT-container");
      const strainInputContainer = document.getElementById(
        "strain-input-container"
      );
      const stressInputContainer = document.getElementById(
        "stress-input-container"
      );
      const calcBtn = document.getElementById("calc-btn");

      let currentMode = "stress"; // 'stress', 'strain', 'deltaT'

      function updateModeUI() {
        modeButtons.forEach((btn) => {
          if (btn.dataset.mode === currentMode) {
            btn.classList.remove(
              "border-gray-300",
              "bg-white",
              "text-gray-700"
            );
            btn.classList.add("border-blue-500", "bg-blue-50", "text-blue-700");
          } else {
            btn.classList.add("border-gray-300", "bg-white", "text-gray-700");
            btn.classList.remove(
              "border-blue-500",
              "bg-blue-50",
              "text-blue-700"
            );
          }
        });

        if (currentMode === "stress") {
          modeDescription.textContent =
            "Mode: compute induced thermal stress σ from material properties and temperature change.";
          TfContainer.classList.remove("hidden");
          deltaTContainer.classList.add("hidden");
          strainInputContainer.classList.add("hidden");
          stressInputContainer.classList.add("hidden");
        } else if (currentMode === "strain") {
          modeDescription.textContent =
            "Mode: compute thermal strain ε from α and ΔT (no E needed).";
          TfContainer.classList.remove("hidden");
          deltaTContainer.classList.add("hidden");
          strainInputContainer.classList.add("hidden");
          stressInputContainer.classList.add("hidden");
        } else if (currentMode === "deltaT") {
          modeDescription.textContent =
            "Mode: compute the temperature change ΔT required to reach a given thermal stress σ.";
          TfContainer.classList.add("hidden");
          deltaTContainer.classList.remove("hidden");
          strainInputContainer.classList.add("hidden");
          stressInputContainer.classList.remove("hidden");
        }
      }

      modeButtons.forEach((btn) => {
        btn.addEventListener("click", () => {
          currentMode = btn.dataset.mode;
          updateModeUI();
        });
      });

      // Biaxial / Poisson toggle
      const biaxialCheckbox = document.getElementById("biaxial-checkbox");
      const poissonContainer = document.getElementById("poisson-container");
      biaxialCheckbox.addEventListener("change", () => {
        if (biaxialCheckbox.checked) {
          poissonContainer.classList.remove("hidden");
        } else {
          poissonContainer.classList.add("hidden");
        }
      });

      // Material presets
      const materialSelect = document.getElementById("material-select");
      const EInput = document.getElementById("E-value");
      const EUnit = document.getElementById("E-unit");
      const alphaInput = document.getElementById("alpha-value");
      const alphaUnit = document.getElementById("alpha-unit");
      const nuInput = document.getElementById("nu-value");

      materialSelect.addEventListener("change", () => {
        const val = materialSelect.value;
        if (val === "steel") {
          EInput.value = 200;
          EUnit.value = "GPa";
          alphaInput.value = 12;
          alphaUnit.value = "micro";
          nuInput.value = 0.3;
        } else if (val === "aluminum") {
          EInput.value = 70;
          EUnit.value = "GPa";
          alphaInput.value = 23;
          alphaUnit.value = "micro";
          nuInput.value = 0.33;
        } else if (val === "concrete") {
          EInput.value = 30;
          EUnit.value = "GPa";
          alphaInput.value = 10;
          alphaUnit.value = "micro";
          nuInput.value = 0.2;
        } else if (val === "glass") {
          EInput.value = 70;
          EUnit.value = "GPa";
          alphaInput.value = 9;
          alphaUnit.value = "micro";
          nuInput.value = 0.22;
        } else if (val === "copper") {
          EInput.value = 110;
          EUnit.value = "GPa";
          alphaInput.value = 17;
          alphaUnit.value = "micro";
          nuInput.value = 0.34;
        }
      });

      // Calculation
      const TiInput = document.getElementById("Ti-value");
      const TfInput = document.getElementById("Tf-value");
      const TUnit = document.getElementById("T-unit");
      const deltaTInput = document.getElementById("deltaT-value");
      const strainInput = document.getElementById("strain-value");
      const stressInput = document.getElementById("stress-value");
      const stressUnit = document.getElementById("stress-unit");

      const resultStress = document.getElementById("result-stress");
      const resultStressNote = document.getElementById("result-stress-note");
      const resultStrain = document.getElementById("result-strain");
      const resultDeltaT = document.getElementById("result-deltaT");
      const resultExpansion = document.getElementById("result-expansion");
      const resultWarning = document.getElementById("result-warning");

      calcBtn.addEventListener("click", () => {
        resultWarning.classList.add("hidden");
        resultWarning.textContent = "";

        const EPa = toPascals(parseFloat(EInput.value), EUnit.value);
        const alphaPerC = toAlphaPerC(
          parseFloat(alphaInput.value),
          alphaUnit.value
        );
        const nu = parseFloat(nuInput.value);

        let deltaT_C;
        if (currentMode === "deltaT") {
          const deltaTUser = parseFloat(deltaTInput.value);
          deltaT_C = convertDeltaT(deltaTUser, TUnit.value);
        } else {
          const Ti = parseFloat(TiInput.value);
          const Tf = parseFloat(TfInput.value);
          if (!isFinite(Ti) || !isFinite(Tf)) {
            alert("Please enter both initial and final temperatures.");
            return;
          }
          const deltaTUser = Tf - Ti;
          deltaT_C = convertDeltaT(deltaTUser, TUnit.value);
        }

        let sigmaPa = NaN;
        let eps = NaN;
        let deltaTRequired_C = NaN;

        if (currentMode === "stress" || currentMode === "strain") {
          if (!isFinite(alphaPerC)) {
            alert("Please enter a valid thermal expansion coefficient α.");
            return;
          }
          eps = alphaPerC * deltaT_C;

          if (currentMode === "stress") {
            if (!isFinite(EPa)) {
              alert("Please enter a valid Young’s modulus E.");
              return;
            }
            if (biaxialCheckbox.checked) {
              if (!isFinite(nu) || nu >= 0.5 || nu <= -1) {
                alert(
                  "Please enter a realistic Poisson’s ratio ν (typically 0–0.5)."
                );
                return;
              }
              sigmaPa = (EPa * alphaPerC * deltaT_C) / (1 - nu);
            } else {
              sigmaPa = EPa * alphaPerC * deltaT_C;
            }
          }
        } else if (currentMode === "deltaT") {
          if (!isFinite(alphaPerC) || !isFinite(EPa)) {
            alert("Please enter valid E and α values.");
            return;
          }
          const sigmaUser = parseFloat(stressInput.value);
          if (!isFinite(sigmaUser)) {
            alert("Please enter a valid target stress.");
            return;
          }
          let sigmaTargetPa;
          switch (stressUnit.value) {
            case "GPa":
              sigmaTargetPa = sigmaUser * 1e9;
              break;
            case "MPa":
              sigmaTargetPa = sigmaUser * 1e6;
              break;
            default:
              sigmaTargetPa = sigmaUser;
          }
          if (biaxialCheckbox.checked) {
            if (!isFinite(nu) || nu >= 0.5 || nu <= -1) {
              alert(
                "Please enter a realistic Poisson’s ratio ν (typically 0–0.5)."
              );
              return;
            }
            deltaTRequired_C = (sigmaTargetPa * (1 - nu)) / (EPa * alphaPerC);
          } else {
            deltaTRequired_C = sigmaTargetPa / (EPa * alphaPerC);
          }
          deltaT_C = deltaTRequired_C;
          eps = alphaPerC * deltaT_C;
          sigmaPa = sigmaTargetPa;
        }

        // Prepare outputs
        const deltaTOut = convertDeltaTBack(deltaT_C, TUnit.value);
        const sigmaMPa = fromPascalsToMPa(sigmaPa);
        const freeExpansionPerMeter = eps; // ΔL for L=1 m

        if (currentMode === "stress") {
          resultStress.textContent = isFinite(sigmaMPa)
            ? formatNumber(sigmaMPa, 3) + " MPa"
            : "–";
        } else if (currentMode === "deltaT") {
          resultStress.textContent = isFinite(sigmaMPa)
            ? formatNumber(sigmaMPa, 3) + " MPa (target)"
            : "–";
        } else {
          resultStress.textContent = "–";
        }

        if (isFinite(eps)) {
          resultStrain.textContent = formatNumber(eps, 6);
        } else {
          resultStrain.textContent = "–";
        }

        if (
          currentMode === "deltaT" ||
          currentMode === "stress" ||
          currentMode === "strain"
        ) {
          resultDeltaT.textContent = isFinite(deltaTOut)
            ? formatNumber(deltaTOut, 3) + " " + TUnit.value
            : "–";
        } else {
          resultDeltaT.textContent = "–";
        }

        if (isFinite(freeExpansionPerMeter)) {
          resultExpansion.textContent =
            formatNumber(freeExpansionPerMeter * 1000, 3) + " mm per 10 m";
        } else {
          resultExpansion.textContent = "–";
        }

        // Stress note and warning
        resultStressNote.textContent = "";
        if (isFinite(sigmaMPa)) {
          if (sigmaMPa > 0) {
            resultStressNote.textContent =
              "Positive sign indicates tensile stress (cooling in a restrained member).";
          } else if (sigmaMPa < 0) {
            resultStressNote.textContent =
              "Negative sign indicates compressive stress (heating in a restrained member).";
          }
          const absSigma = Math.abs(sigmaMPa);
          if (absSigma > 300) {
            resultWarning.textContent =
              "Warning: Computed thermal stress exceeds ~300 MPa. Verify material properties, constraints, and that a simple 1D model is appropriate. Real structures may yield, creep, or crack before reaching this level.";
            resultWarning.classList.remove("hidden");
          }
        }
      });

      // Initialize UI
      updateModeUI();

      // Mobile menu script (simple)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          this.setAttribute("aria-expanded", isHidden ? "true" : "false");
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
