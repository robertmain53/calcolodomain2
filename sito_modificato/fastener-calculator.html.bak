    <script>
      // Tab switching
      const tabQuantity = document.getElementById("tab-quantity");
      const tabCapacity = document.getElementById("tab-capacity");
      const panelQuantity = document.getElementById("panel-quantity");
      const panelCapacity = document.getElementById("panel-capacity");

      function activateTab(tab) {
        if (tab === "quantity") {
          tabQuantity.classList.add("tab-active");
          tabCapacity.classList.remove("tab-active");
          panelQuantity.classList.remove("hidden");
          panelCapacity.classList.add("hidden");
        } else {
          tabCapacity.classList.add("tab-active");
          tabQuantity.classList.remove("tab-active");
          panelCapacity.classList.remove("hidden");
          panelQuantity.classList.add("hidden");
        }
      }

      tabQuantity.addEventListener("click", () => activateTab("quantity"));
      tabCapacity.addEventListener("click", () => activateTab("capacity"));

      // Units switching for quantity
      const unitRadios = document.querySelectorAll('input[name="units"]');
      const labelBoardLengthUnit = document.getElementById(
        "labelBoardLengthUnit"
      );
      const labelBoardWidthUnit = document.getElementById(
        "labelBoardWidthUnit"
      );
      const labelSupportSpacingUnit = document.getElementById(
        "labelSupportSpacingUnit"
      );
      const labelSpacingAlongUnit = document.getElementById(
        "labelSpacingAlongUnit"
      );
      const labelEndDistanceUnit = document.getElementById(
        "labelEndDistanceUnit"
      );
      const labelEdgeDistanceUnit = document.getElementById(
        "labelEdgeDistanceUnit"
      );

      unitRadios.forEach((r) => {
        r.addEventListener("change", () => {
          const isMetric = r.value === "metric";
          labelBoardLengthUnit.textContent = isMetric ? "(m)" : "(ft)";
          labelBoardWidthUnit.textContent = isMetric ? "(mm)" : "(in)";
          labelSupportSpacingUnit.textContent = isMetric ? "(mm)" : "(in)";
          labelSpacingAlongUnit.textContent = isMetric ? "(mm)" : "(in)";
          labelEndDistanceUnit.textContent = isMetric ? "(mm)" : "(in)";
          labelEdgeDistanceUnit.textContent = isMetric ? "(mm)" : "(in)";
        });
      });

      // Quantity calculation
      const btnCalcQuantity = document.getElementById("btnCalcQuantity");
      const quantityResults = document.getElementById("quantityResults");
      const resPerSupport = document.getElementById("resPerSupport");
      const resPerBoard = document.getElementById("resPerBoard");
      const resTotal = document.getElementById("resTotal");
      const resSpacingAlong = document.getElementById("resSpacingAlong");
      const resEndDistance = document.getElementById("resEndDistance");
      const resEdgeDistance = document.getElementById("resEdgeDistance");
      const resRows = document.getElementById("resRows");
      const resSupports = document.getElementById("resSupports");
      const layoutDiagram = document.getElementById("layoutDiagram");
      const spacingWarnings = document.getElementById("spacingWarnings");

      function parsePositive(id) {
        const el = document.getElementById(id);
        const v = parseFloat(el.value);
        return isFinite(v) && v > 0 ? v : NaN;
      }

      btnCalcQuantity.addEventListener("click", () => {
        const units = document.querySelector(
          'input[name="units"]:checked'
        ).value;
        let L = parsePositive("boardLength");
        let W = parsePositive("boardWidth");
        let supportSpacing = parsePositive("supportSpacing");
        const rows = parseInt(document.getElementById("rows").value, 10);
        let spacingAlong = parsePositive("spacingAlong");
        let endDistance = parsePositive("endDistance");
        let edgeDistance = parsePositive("edgeDistance");
        const supportsCount = parseInt(
          document.getElementById("supportsCount").value,
          10
        );

        if (
          !isFinite(L) ||
          !isFinite(W) ||
          !isFinite(supportSpacing) ||
          !isFinite(spacingAlong) ||
          !isFinite(endDistance) ||
          !isFinite(edgeDistance) ||
          !Number.isInteger(rows) ||
          rows <= 0 ||
          !Number.isInteger(supportsCount) ||
          supportsCount <= 0
        ) {
          alert("Please enter valid positive values for all fields.");
          return;
        }

        // Convert metric to imperial internally for consistent logic (optional)
        if (units === "metric") {
          // m to ft, mm to in
          L = L * 3.28084;
          W = W * 0.0393701;
          supportSpacing = supportSpacing * 0.0393701;
          spacingAlong = spacingAlong * 0.0393701;
          endDistance = endDistance * 0.0393701;
          edgeDistance = edgeDistance * 0.0393701;
        }

        const effectiveLength = Math.max(L - 2 * endDistance, spacingAlong);
        const nAlong = Math.max(
          1,
          Math.ceil(effectiveLength / spacingAlong) + 1
        );
        const perSupport = nAlong * rows;
        const perBoard = perSupport * supportsCount;
        const total = perBoard; // one board; user can scale manually

        resPerSupport.textContent = perSupport.toString();
        resPerBoard.textContent = perBoard.toString();
        resTotal.textContent = total.toString();

        const fmt = (val, unitImp, unitMet) => {
          if (units === "metric") {
            // convert back to metric for display
            if (unitImp === "in") return (val / 0.0393701).toFixed(0) + " mm";
            if (unitImp === "ft") return (val / 3.28084).toFixed(2) + " m";
          }
          return unitImp === "in"
            ? val.toFixed(1) + " in"
            : val.toFixed(2) + " ft";
        };

        resSpacingAlong.textContent = fmt(spacingAlong, "in");
        resEndDistance.textContent = fmt(endDistance, "in");
        resEdgeDistance.textContent = fmt(edgeDistance, "in");
        resRows.textContent = rows.toString();
        resSupports.textContent = supportsCount.toString();

        // Simple ASCII layout: supports as vertical lines, fasteners as dots
        const maxCols = Math.min(60, nAlong);
        const step = Math.max(1, Math.floor(nAlong / maxCols));
        let diagram = "";
        for (let r = 0; r < rows; r++) {
          for (let s = 0; s < supportsCount; s++) {
            diagram += "|";
            for (let i = 0; i < nAlong; i += step) {
              diagram += "•";
            }
            diagram += "|\n";
          }
          if (r < rows - 1) diagram += "\n";
        }
        layoutDiagram.textContent = diagram;

        // Spacing warnings (rule-of-thumb)
        spacingWarnings.innerHTML = "";
        const warnings = [];
        const dApprox = 0.164; // assume #8 screw equivalent for spacing checks
        const minEnd = 7 * dApprox;
        const minEdge = 4 * dApprox;
        const minAlong = 7 * dApprox;

        if (endDistance < minEnd) {
          warnings.push(
            `End distance is quite small (less than about 7× fastener diameter). Consider increasing to reduce splitting risk.`
          );
        }
        if (edgeDistance < minEdge) {
          warnings.push(
            `Edge distance is quite small (less than about 4× fastener diameter). Consider moving fasteners further from the edge.`
          );
        }
        if (spacingAlong < minAlong) {
          warnings.push(
            `Spacing along the board is tight (less than about 7× fastener diameter). Check code and manufacturer recommendations.`
          );
        }

        if (warnings.length) {
          warnings.forEach((w) => {
            const p = document.createElement("p");
            p.className =
              "text-amber-700 bg-amber-50 border border-amber-200 rounded px-2 py-1";
            p.textContent = w;
            spacingWarnings.appendChild(p);
          });
        }

        quantityResults.classList.remove("hidden");
      });

      // Units switching for capacity
      const unitCapRadios = document.querySelectorAll('input[name="unitsCap"]');
      const labelDiaUnit = document.getElementById("labelDiaUnit");
      const labelEmbedUnit = document.getElementById("labelEmbedUnit");

      unitCapRadios.forEach((r) => {
        r.addEventListener("change", () => {
          const isMetric = r.value === "metric";
          labelDiaUnit.textContent = isMetric ? "(mm)" : "(in)";
          labelEmbedUnit.textContent = isMetric ? "(mm)" : "(in)";
        });
      });

      // Capacity calculation
      const btnCalcCapacity = document.getElementById("btnCalcCapacity");
      const capacityResults = document.getElementById("capacityResults");
      const resPullPer = document.getElementById("resPullPer");
      const resShearPer = document.getElementById("resShearPer");
      const resPullTotal = document.getElementById("resPullTotal");
      const resShearTotal = document.getElementById("resShearTotal");

      btnCalcCapacity.addEventListener("click", () => {
        const units = document.querySelector(
          'input[name="unitsCap"]:checked'
        ).value;
        let d = parsePositive("fastenerDiameter");
        let le = parsePositive("embedDepth");
        const n = parseInt(
          document.getElementById("fastenerCountCap").value,
          10
        );
        const density = document.getElementById("woodDensity").value;

        if (!isFinite(d) || !isFinite(le) || !Number.isInteger(n) || n <= 0) {
          alert(
            "Please enter valid positive values for diameter, embedment and count."
          );
          return;
        }

        // Convert metric to imperial internally
        if (units === "metric") {
          d = d * 0.0393701; // mm to in
          le = le * 0.0393701; // mm to in
        }

        let k;
        switch (density) {
          case "soft":
            k = 250;
            break;
          case "dense":
            k = 320;
            break;
          case "hard":
            k = 380;
            break;
          default:
            k = 250;
        }

        const pullPer = k * d * le; // lb (approx)
        const shearPer = k * d * d; // lb (approx)
        const pullTotal = pullPer * n;
        const shearTotal = shearPer * n;

        if (units === "metric") {
          const tokN = (v) => (v * 0.00444822).toFixed(2) + " kN";
          resPullPer.textContent = tokN(pullPer);
          resShearPer.textContent = tokN(shearPer);
          resPullTotal.textContent = tokN(pullTotal);
          resShearTotal.textContent = tokN(shearTotal);
        } else {
          const toLb = (v) => v.toFixed(0) + " lb";
          resPullPer.textContent = toLb(pullPer);
          resShearPer.textContent = toLb(shearPer);
          resPullTotal.textContent = toLb(pullTotal);
          resShearTotal.textContent = toLb(shearTotal);
        }

        capacityResults.classList.remove("hidden");
      });

      // Mobile menu toggle
      const mobileToggle = document.getElementById("mobile-menu-toggle");
      const mobileMenu = document.getElementById("mobile-menu");
      if (mobileToggle && mobileMenu) {
        mobileToggle.addEventListener("click", () => {
          mobileMenu.classList.toggle("hidden");
          const expanded =
            mobileToggle.getAttribute("aria-expanded") === "true";
          mobileToggle.setAttribute("aria-expanded", String(!expanded));
        });
      }
    </script>

