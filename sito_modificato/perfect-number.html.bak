    <!-- Calculator-specific JS -->
    <script>
      // Mobile menu toggle
      const mobileToggle = document.getElementById('mobile-menu-toggle');
      const mobileMenu = document.getElementById('mobile-menu');
      if (mobileToggle && mobileMenu) {
        mobileToggle.addEventListener('click', () => {
          const expanded = mobileToggle.getAttribute('aria-expanded') === 'true';
          mobileToggle.setAttribute('aria-expanded', String(!expanded));
          mobileMenu.classList.toggle('hidden');
        });
      }

      const perfectModeSingle = document.getElementById('perfect-mode-single');
      const perfectModeRange = document.getElementById('perfect-mode-range');
      const perfectSinglePanel = document.getElementById('perfect-single-panel');
      const perfectRangePanel = document.getElementById('perfect-range-panel');
      const perfectResult = document.getElementById('perfect-result');
      const perfectNInput = document.getElementById('perfect-n');
      const perfectMaxInput = document.getElementById('perfect-max');
      const perfectCalcBtn = document.getElementById('perfect-calc-btn');
      const perfectClearBtn = document.getElementById('perfect-clear-btn');

      function formatNumber(x) {
        if (!isFinite(x)) return 'NaN';
        return x.toString();
      }

      function resetResult() {
        if (perfectResult) {
          perfectResult.innerHTML = '<p class="text-gray-500">Results, divisor lists, and classifications will appear here after you click <span class="font-semibold">Calculate</span>.</p>';
        }
      }

      function showError(msg) {
        if (!perfectResult) return;
        perfectResult.innerHTML = '<p class="text-red-600 text-sm">' + msg + '</p>';
      }

      function classifyNumber(n) {
        // n positive integer
        if (n === 1) {
          return {
            divisors: [],
            sum: 0,
            type: 'deficient',
            label: 'Deficient number'
          };
        }

        const divisorsSet = new Set();
        divisorsSet.add(1);
        const limit = Math.floor(Math.sqrt(n));

        for (let d = 2; d <= limit; d++) {
          if (n % d === 0) {
            divisorsSet.add(d);
            const other = n / d;
            if (other !== n) {
              divisorsSet.add(other);
            }
          }
        }

        // Remove n itself if somehow included
        divisorsSet.delete(n);

        const divisors = Array.from(divisorsSet);
        divisors.sort((a, b) => a - b);

        const sum = divisors.reduce((acc, v) => acc + v, 0);

        let type = 'deficient';
        let label = 'Deficient number';
        if (sum === n) {
          type = 'perfect';
          label = 'Perfect number';
        } else if (sum > n) {
          type = 'abundant';
          label = 'Abundant number';
        }

        return { divisors, sum, type, label };
      }

      function renderClassificationTag(type) {
        let cls = 'result-tag ';
        let text = '';
        if (type === 'perfect') {
          cls += 'result-tag-perfect';
          text = 'Perfect';
        } else if (type === 'abundant') {
          cls += 'result-tag-abundant';
          text = 'Abundant';
        } else {
          cls += 'result-tag-deficient';
          text = 'Deficient';
        }
        return '<span class="' + cls + '"><span class="w-1.5 h-1.5 rounded-full bg-current inline-block"></span><span>' + text + '</span></span>';
      }

      function calculateSingle() {
        const raw = perfectNInput.value.trim();
        if (!raw) {
          showError('Please enter a positive integer n to check.');
          return;
        }
        const n = Number(raw);
        if (!Number.isFinite(n) || !Number.isInteger(n) || n <= 0) {
          showError('n must be a positive integer.');
          return;
        }
        if (n > 1e10) {
          showError('For performance and browser stability, please use n ≤ 10,000,000 for detailed divisor analysis.');
          return;
        }

        const result = classifyNumber(n);
        const divisors = result.divisors;
        const sum = result.sum;
        const type = result.type;
        const label = result.label;

        let html = '<div class="space-y-3">';
        html += '<p><span class="font-semibold">Number:</span> <span class="font-mono">' + formatNumber(n) + '</span> ' + renderClassificationTag(type) + '</p>';
        html += '<p><span class="font-semibold">Sum of proper divisors:</span> <span class="font-mono">' + formatNumber(sum) + '</span></p>';

        if (divisors.length > 0) {
          html += '<p><span class="font-semibold">Proper divisors d (d &lt; n):</span></p>';
          html += '<p class="font-mono text-xs bg-white border border-gray-200 rounded p-2 max-h-32 overflow-y-auto">' +
            divisors.join(', ') + '</p>';
        } else {
          html += '<p class="text-xs text-gray-600">This number has no proper divisors other than itself (or the divisor set is empty for n = 1).</p>';
        }

        html += '<p class="text-xs text-gray-600">';
        if (type === 'perfect') {
          html += 'Since the sum of proper divisors equals n, ' + n + ' is a perfect number.';
        } else if (type === 'abundant') {
          html += 'Since the sum of proper divisors is greater than n, ' + n + ' is an abundant number.';
        } else {
          html += 'Since the sum of proper divisors is less than n, ' + n + ' is a deficient number.';
        }
        html += '</p>';

        html += '<p class="mt-2 text-xs text-gray-500">Divisors are computed via an O(√n) search that adds symmetric factors d and n/d for each divisor found.</p>';

        html += '</div>';

        perfectResult.innerHTML = html;
      }

      function calculateRange() {
        const raw = perfectMaxInput.value.trim();
        if (!raw) {
          showError('Please enter an upper bound N for the range scan.');
          return;
        }
        const N = Number(raw);
        if (!Number.isFinite(N) || !Number.isInteger(N) || N < 2) {
          showError('N must be an integer ≥ 2.');
          return;
        }
        if (N > 1_000_000) {
          showError('For this educational tool, N is limited to 1,000,000. Please choose a smaller bound.');
          return;
        }

        const found = [];
        for (let n = 2; n <= N; n++) {
          const info = classifyNumber(n);
          if (info.type === 'perfect') {
            found.push({ n, divisors: info.divisors, sum: info.sum });
          }
        }

        let html = '<div class="space-y-3">';
        html += '<p><span class="font-semibold">Range scanned:</span> <span class="font-mono">2 ≤ n ≤ ' + formatNumber(N) + '</span></p>';
        html += '<p><span class="font-semibold">Perfect numbers found:</span> <span class="font-mono">' + found.length + '</span></p>';

        if (found.length === 0) {
          html += '<p class="text-sm text-gray-700">No perfect numbers were found in this range. Try increasing N within the allowed limit or consult known lists of perfect numbers.</p>';
        } else {
          html += '<p class="text-sm text-gray-700">Perfect numbers in this range:</p>';
          html += '<p class="font-mono text-xs bg-white border border-gray-200 rounded p-2 max-h-24 overflow-y-auto">' +
            found.map(item => item.n).join(', ') + '</p>';

          html += '<div class="mt-3">';
          html += '<p class="font-semibold text-sm mb-1">Details for each perfect number</p>';
          html += '<div class="table-scroll"><table class="data-table">';
          html += '<thead class="bg-gray-100"><tr><th>n</th><th>Proper divisors</th><th>Divisor sum</th></tr></thead><tbody>';
          found.forEach(item => {
            html += '<tr>';
            html += '<td class="font-mono">' + item.n + '</td>';
            html += '<td class="font-mono">' + item.divisors.join(', ') + '</td>';
            html += '<td class="font-mono">' + item.sum + '</td>';
            html += '</tr>';
          });
          html += '</tbody></table></div>';
          html += '</div>';
        }

        html += '<p class="mt-2 text-xs text-gray-500">Range scanning is deterministic and exhaustive for the chosen interval. It does not rely on precomputed tables or heuristics: each integer is checked directly from its divisors.</p>';

        html += '</div>';
        perfectResult.innerHTML = html;
      }

      function calculatePerfect() {
        const mode = perfectModeRange && perfectModeRange.checked ? 'range' : 'single';
        if (mode === 'single') {
          calculateSingle();
        } else {
          calculateRange();
        }
      }

      // Mode switching
      if (perfectModeSingle && perfectModeRange && perfectSinglePanel && perfectRangePanel) {
        perfectModeSingle.addEventListener('change', () => {
          if (perfectModeSingle.checked) {
            perfectSinglePanel.classList.remove('hidden');
            perfectRangePanel.classList.add('hidden');
            resetResult();
          }
        });
        perfectModeRange.addEventListener('change', () => {
          if (perfectModeRange.checked) {
            perfectSinglePanel.classList.add('hidden');
            perfectRangePanel.classList.remove('hidden');
            resetResult();
          }
        });
      }

      // Example buttons for single mode
      document.querySelectorAll('[data-example]').forEach(btn => {
        btn.addEventListener('click', () => {
          const val = btn.getAttribute('data-example');
          if (perfectNInput) {
            perfectNInput.value = val;
            resetResult();
          }
        });
      });

      // Calculate / clear
      if (perfectCalcBtn) {
        perfectCalcBtn.addEventListener('click', calculatePerfect);
      }

      if (perfectClearBtn) {
        perfectClearBtn.addEventListener('click', () => {
          if (perfectNInput) perfectNInput.value = '';
          if (perfectMaxInput) perfectMaxInput.value = '';
          if (perfectModeSingle) perfectModeSingle.checked = true;
          if (perfectModeRange) perfectModeRange.checked = false;
          if (perfectSinglePanel && perfectRangePanel) {
            perfectSinglePanel.classList.remove('hidden');
            perfectRangePanel.classList.add('hidden');
          }
          resetResult();
        });
      }

      // FAQ toggle
      document.querySelectorAll('#faq-container .faq-question').forEach(btn => {
        btn.addEventListener('click', () => {
          const answer = btn.parentElement.querySelector('.faq-answer');
          if (!answer) return;
          const isHidden = answer.classList.contains('hidden');
          answer.classList.toggle('hidden', !isHidden);
          const icon = btn.querySelector('span:last-child');
          if (icon) icon.textContent = isHidden ? '−' : '+';
        });
      });
    </script>

