    <script>
      function toNumber(id) {
        const v = parseFloat(document.getElementById(id).value);
        return isNaN(v) ? 0 : v;
      }

      function formatNumber(val, decimals = 2) {
        return Number(val).toFixed(decimals);
      }

      function calculateCompositeBeam() {
        const L = toNumber("span"); // m
        const bf = toNumber("beamWidth") / 1000; // m
        const ha = toNumber("beamDepth") / 1000; // m
        const tw = toNumber("webThickness") / 1000; // m
        const tf = toNumber("flangeThickness") / 1000; // m
        const hc = toNumber("slabThickness") / 1000; // m
        const beff = toNumber("slabWidth") / 1000; // m
        const fck = toNumber("fck") * 1e6; // Pa
        const fyk = toNumber("fyk") * 1e6; // Pa
        const gammaM0 = toNumber("gammaM0") || 1.0;
        const gammaC = toNumber("gammaC") || 1.5;

        const MEd = toNumber("MdEd") * 1e3; // kNm -> Nm
        const VEd = toNumber("VEd"); // kN (not used in simplified calc yet)

        const Pstud = toNumber("studResistance"); // kN
        const NstudHalf = toNumber("studsPerSpan");

        const results = [];

        // Basic geometry checks
        if (
          L <= 0 ||
          ha <= 0 ||
          beff <= 0 ||
          hc <= 0 ||
          bf <= 0 ||
          tw <= 0 ||
          tf <= 0
        ) {
          document.getElementById("resultsContainer").innerHTML =
            '<p class="text-red-600 text-sm">Please enter positive values for all geometry parameters.</p>';
          return;
        }

        // Areas
        const Ac = beff * hc; // m2
        const Aa = 2 * bf * tf + (ha - 2 * tf) * tw; // m2

        // Design forces
        const CcRd = ((0.85 * fck) / gammaC) * Ac; // N
        const TaRd = (fyk / gammaM0) * Aa; // N
        const FcRd = Math.min(CcRd, TaRd); // governing axial force for full interaction

        // Lever arm approximation
        const yC = ha + hc / 2; // centroid of concrete from bottom of steel
        const yS = ha / 2; // centroid of steel from bottom
        let z = yC - yS; // m
        const hTot = ha + hc;
        if (z > hTot) z = hTot;

        const MplRd = FcRd * z; // Nm

        const muM = MplRd > 0 ? MEd / MplRd : 0;

        // Shear connection
        const sumPRd = NstudHalf * Pstud * 1e3; // kN -> N
        const eta = FcRd > 0 ? sumPRd / FcRd : 0;

        // Build results HTML
        let html = "";

        html += '<div class="border border-gray-200 rounded-lg p-4">';
        html +=
          '<h3 class="font-semibold mb-2">Ultimate Limit State – Bending</h3>';
        html +=
          '<p class="mb-1 text-sm">Plastic bending resistance of composite section:</p>';
        html += '<ul class="text-sm list-disc ml-5 mb-2">';
        html += `<li>A<sub>c</sub> = ${formatNumber(Ac * 1e4, 2)} cm²</li>`;
        html += `<li>A<sub>a</sub> ≈ ${formatNumber(Aa * 1e4, 2)} cm²</li>`;
        html += `<li>C<sub>c,Rd</sub> = ${formatNumber(CcRd / 1e3, 1)} kN</li>`;
        html += `<li>T<sub>a,Rd</sub> = ${formatNumber(TaRd / 1e3, 1)} kN</li>`;
        html += `<li>Lever arm z ≈ ${formatNumber(z * 1000, 0)} mm</li>`;
        html += `</ul>`;
        html += `<p class="text-sm mb-1">M<sub>pl,Rd</sub> ≈ <strong>${formatNumber(
          MplRd / 1e6,
          1
        )} kNm</strong></p>`;
        html += `<p class="text-sm mb-2">M<sub>Ed</sub> = <strong>${formatNumber(
          MEd / 1e6,
          1
        )} kNm</strong></p>`;

        const okBend = muM <= 1.0 && muM > 0;
        html += `<p class="text-sm mb-1">Bending utilisation μ<sub>M</sub> = <strong>${formatNumber(
          muM,
          2
        )}</strong> `;
        html += okBend
          ? '<span class="result-badge ok ml-2">OK</span>'
          : '<span class="result-badge ng ml-2">NG</span>';
        html += "</p>";
        if (!okBend) {
          html +=
            '<p class="text-xs text-red-600">Increase section size, slab width/thickness, or reduce design moment.</p>';
        }
        html += "</div>";

        html += '<div class="border border-gray-200 rounded-lg p-4 mt-4">';
        html += '<h3 class="font-semibold mb-2">Shear Connection</h3>';
        html += '<ul class="text-sm list-disc ml-5 mb-2">';
        html += `<li>Total stud resistance per half-span ΣP<sub>Rd</sub> = ${formatNumber(
          sumPRd / 1e3,
          1
        )} kN</li>`;
        html += `<li>Required axial force for full interaction F<sub>c,Rd</sub> = ${formatNumber(
          FcRd / 1e3,
          1
        )} kN</li>`;
        html += "</ul>";
        html += `<p class="text-sm mb-1">Degree of shear connection η = <strong>${formatNumber(
          eta,
          2
        )}</strong></p>`;

        const okShear = eta >= 1.0;
        html += `<p class="text-sm mb-1">Interaction check (η ≥ 1.0 for full interaction): `;
        html += okShear
          ? '<span class="result-badge ok ml-1">OK</span>'
          : '<span class="result-badge ng ml-1">NG</span>';
        html += "</p>";

        if (!okShear) {
          html +=
            '<p class="text-xs text-red-600">Increase the number or capacity of shear studs, or accept partial interaction with a more detailed Eurocode 4 check.</p>';
        }
        html += "</div>";

        html +=
          '<p class="mt-4 text-xs text-gray-500">Note: This is a simplified Eurocode 4 check for simply supported beams in sagging. Always verify full code compliance, including shear, deflection, stability and detailing.</p>';

        document.getElementById("resultsContainer").innerHTML = html;
      }

      document
        .getElementById("calculateBtn")
        .addEventListener("click", calculateCompositeBeam);

      document.getElementById("resetBtn").addEventListener("click", () => {
        document.getElementById("span").value = 8;
        document.getElementById("beamWidth").value = 300;
        document.getElementById("beamDepth").value = 400;
        document.getElementById("webThickness").value = 8;
        document.getElementById("flangeThickness").value = 16;
        document.getElementById("slabThickness").value = 130;
        document.getElementById("slabWidth").value = 3000;
        document.getElementById("fck").value = 30;
        document.getElementById("fyk").value = 355;
        document.getElementById("gammaM0").value = 1.0;
        document.getElementById("gammaC").value = 1.5;
        document.getElementById("MdEd").value = 600;
        document.getElementById("VEd").value = 350;
        document.getElementById("studResistance").value = 90;
        document.getElementById("studsPerSpan").value = 20;
        document.getElementById("resultsContainer").innerHTML =
          '<p class="text-gray-500">Enter your data and click “Run Eurocode 4 Checks” to see utilisation ratios and shear connection degree.</p>';
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
