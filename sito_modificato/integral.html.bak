    <script>
      // Mobile menu toggle
      document.getElementById('mobile-menu-toggle')?.addEventListener('click', function () {
        const menu = document.getElementById('mobile-menu');
        if (!menu) return;
        const isHidden = menu.classList.contains('hidden');
        menu.classList.toggle('hidden', !isHidden);
        this.setAttribute('aria-expanded', String(isHidden));
      });

      document.addEventListener('DOMContentLoaded', function () {
        const funcInput = document.getElementById('func-input');
        const lowerInput = document.getElementById('lower-input');
        const upperInput = document.getElementById('upper-input');
        const subintervalsInput = document.getElementById('subintervals-input');
        const decimalsSelect = document.getElementById('decimals');
        const samplesDisplayInput = document.getElementById('samples-display-input');

        const btnCalc = document.getElementById('btn-calc-integral');
        const btnClear = document.getElementById('btn-clear-integral');
        const errorEl = document.getElementById('integral-error');

        const resIntegral = document.getElementById('res-integral');
        const resAbsArea = document.getElementById('res-abs-area');
        const resAverage = document.getElementById('res-average');
        const resError = document.getElementById('res-error');
        const resEvals = document.getElementById('res-evals');
        const resConvergence = document.getElementById('res-convergence');
        const resWarnings = document.getElementById('res-warnings');
        const samplesBody = document.getElementById('table-samples-body');

        const exampleButtons = document.querySelectorAll('[data-example]');

        function setResult(container, text) {
          if (!container) return;
          const span = container.querySelector('.result-value');
          if (span) span.textContent = text;
        }

        function clearAll() {
          [resIntegral, resAbsArea, resAverage, resError, resEvals, resConvergence, resWarnings].forEach(function (el) {
            if (!el) return;
            const spans = el.querySelectorAll('.result-value');
            spans.forEach(span => span.textContent = '—');
          });
          if (samplesBody) samplesBody.innerHTML = '';
          if (errorEl) errorEl.textContent = '';
        }

        function getDecimals() {
          const d = Number(decimalsSelect.value);
          if (!Number.isInteger(d) || d < 0 || d > 16) return 6;
          return d;
        }

        function fmt(v) {
          if (!isFinite(v)) {
            if (v === Infinity) return '+∞';
            if (v === -Infinity) return '−∞';
            return 'NaN';
          }
          const d = getDecimals();
          return Number(v.toFixed(d)).toString();
        }

        function buildEvaluator(exprRaw) {
          if (!exprRaw || !exprRaw.trim()) {
            throw new Error('Function is empty.');
          }
          let expr = exprRaw.trim();

          // Replace caret with exponentiation operator
          expr = expr.replace(/\^/g, '**');

          // Replace common math functions with Math.* equivalents
          const funcs = [
            'sin','cos','tan','asin','acos','atan',
            'sinh','cosh','tanh',
            'log','ln','sqrt','abs','exp',
            'floor','ceil','round','pow','max','min'
          ];
          funcs.forEach(fn => {
            const re = new RegExp('\\b' + fn + '\\b', 'g');
            if (fn === 'ln') {
              expr = expr.replace(re, 'Math.log');
            } else if (fn === 'log') {
              expr = expr.replace(re, 'Math.log');
            } else {
              expr = expr.replace(re, 'Math.' + fn);
            }
          });

          // Constants
          expr = expr.replace(/\bpi\b/gi, 'Math.PI');
          expr = expr.replace(/π/g, 'Math.PI');
          expr = expr.replace(/\be\b/g, 'Math.E');

          // Build evaluator
          const fn = new Function('x', '"use strict"; return (' + expr + ');');
          return function (x) {
            const val = fn(x);
            return val;
          };
        }

        function integrateSimpson(f, a, b, n, evalCounterObj) {
          if (n % 2 === 1) n += 1;
          if (n < 2) n = 2;
          const h = (b - a) / n;
          let sum = 0;
          // f(a) and f(b)
          let fa, fb;
          try { fa = f(a); evalCounterObj.count++; } catch (e) { fa = NaN; }
          try { fb = f(b); evalCounterObj.count++; } catch (e) { fb = NaN; }
          if (!isFinite(fa) || !isFinite(fb)) return { value: NaN, n };

          sum = fa + fb;
          let fourSum = 0;
          let twoSum = 0;
          for (let i = 1; i < n; i++) {
            const x = a + i * h;
            let fx;
            try { fx = f(x); evalCounterObj.count++; } catch (e) { fx = NaN; }
            if (!isFinite(fx)) {
              return { value: NaN, n };
            }
            if (i % 2 === 1) {
              fourSum += fx;
            } else {
              twoSum += fx;
            }
          }
          const val = (h / 3) * (sum + 4 * fourSum + 2 * twoSum);
          return { value: val, n };
        }

        function populateSamples(f, a, b, m) {
          if (!samplesBody) return;
          samplesBody.innerHTML = '';
          if (!isFinite(a) || !isFinite(b) || m < 2) return;

          const n = Math.max(2, Math.min(1000, m));
          const step = (b - a) / (n - 1);
          const d = getDecimals();

          for (let i = 0; i < n; i++) {
            const x = a + i * step;
            let fx = NaN;
            try { fx = f(x); } catch (e) { fx = NaN; }

            const tr = document.createElement('tr');
            tr.className = 'border-b';
            const tdX = document.createElement('td');
            tdX.className = 'px-3 py-1.5 text-gray-700';
            tdX.textContent = Number(x.toFixed(d)).toString();
            const tdY = document.createElement('td');
            tdY.className = 'px-3 py-1.5 text-gray-700';

            if (!isFinite(fx)) {
              tdY.textContent = 'NaN / undefined';
              tdY.classList.add('text-red-600');
            } else {
              tdY.textContent = Number(fx.toFixed(d)).toString();
            }

            tr.appendChild(tdX);
            tr.appendChild(tdY);
            samplesBody.appendChild(tr);
          }
        }

        function computeIntegral() {
          clearAll();

          const expr = funcInput.value;
          if (!expr || !expr.trim()) {
            errorEl.textContent = 'Please enter a function f(x).';
            return;
          }

          const a = Number(lowerInput.value);
          const b = Number(upperInput.value);
          if (!isFinite(a) || !isFinite(b)) {
            errorEl.textContent = 'Lower and upper limits must be valid numbers.';
            return;
          }
          if (a === b) {
            errorEl.textContent = 'Integration limits a and b must be different.';
            return;
          }

          let n = Number(subintervalsInput.value);
          if (!Number.isInteger(n) || n < 2 || n > 20000) {
            errorEl.textContent = 'Subintervals must be an integer between 2 and 20000.';
            return;
          }

          let m = Number(samplesDisplayInput.value);
          if (!Number.isInteger(m) || m < 5 || m > 200) {
            m = 21;
          }

          let f;
          try {
            f = buildEvaluator(expr);
          } catch (e) {
            errorEl.textContent = 'Could not parse function: ' + e.message;
            return;
          }

          // Ensure n is even
          if (n % 2 === 1) n += 1;
          if (n < 2) n = 2;

          const warnings = [];
          const evalCounter = { count: 0 };

          // Simpson with n and 2n
          const I1 = integrateSimpson(f, a, b, n, evalCounter);
          if (!isFinite(I1.value)) {
            setResult(resIntegral, 'NaN');
            setResult(resAbsArea, 'NaN');
            setResult(resAverage, 'NaN');
            setResult(resError, '—');
            setResult(resConvergence, 'Function returned non-finite values inside the interval.');
            setResult(resWarnings, 'Check the domain of your function. Division by zero, negative arguments for sqrt, or log of non-positive numbers may occur.');
            setResult(resEvals, evalCounter.count.toString());
            populateSamples(f, a, b, m);
            return;
          }

          const I2 = integrateSimpson(f, a, b, 2 * n, evalCounter);
          if (!isFinite(I2.value)) {
            warnings.push('Refined Simpson evaluation (2n subintervals) produced non-finite values; error estimate may be unreliable.');
          }

          const I = isFinite(I2.value) ? I2.value : I1.value;
          const err = isFinite(I2.value) ? Math.abs(I2.value - I1.value) : NaN;

          // Absolute area
          const evalCounterAbs = { count: 0 };
          function fabs(x) {
            let v;
            try { v = f(x); evalCounterAbs.count++; } catch (e) { v = NaN; }
            if (!isFinite(v)) return NaN;
            return Math.abs(v);
          }
          const A1 = integrateSimpson(fabs, a, b, n, evalCounterAbs);
          const absArea = A1.value;

          const totalEvals = evalCounter.count + evalCounterAbs.count;
          setResult(resIntegral, fmt(I));
          setResult(resAbsArea, isFinite(absArea) ? fmt(absArea) : 'NaN');

          if (b !== a) {
            const avg = I / (b - a);
            setResult(resAverage, isFinite(avg) ? fmt(avg) : 'NaN');
          } else {
            setResult(resAverage, 'Not defined (a = b)');
          }

          if (isFinite(err)) {
            setResult(resError, fmt(err));
          } else {
            setResult(resError, '—');
            warnings.push('Error estimate not available (refined integral failed).');
          }

          setResult(resEvals, totalEvals.toString());

          let convMsg = 'Simpson’s rule converged normally with the chosen subintervals.';
          if (isFinite(err)) {
            if (err > 1e-2) {
              convMsg = 'Error estimate is relatively large; consider increasing the number of subintervals.';
            } else if (err < 1e-8) {
              convMsg = 'Integral appears very stable; numerical error is likely negligible for most practical purposes.';
            }
          } else {
            convMsg = 'Convergence could not be assessed due to numerical issues in the refined evaluation.';
          }
          setResult(resConvergence, convMsg);

          if (warnings.length === 0) {
            setResult(resWarnings, 'No major numerical issues detected. Always confirm important results analytically when stakes are high.');
          } else {
            setResult(resWarnings, warnings.join(' '));
          }

          populateSamples(f, a, b, m);
        }

        if (btnCalc) btnCalc.addEventListener('click', computeIntegral);

        if (btnClear) {
          btnClear.addEventListener('click', function () {
            if (funcInput) funcInput.value = '';
            if (lowerInput) lowerInput.value = '0';
            if (upperInput) upperInput.value = '1';
            if (subintervalsInput) subintervalsInput.value = '1000';
            if (decimalsSelect) decimalsSelect.value = '6';
            if (samplesDisplayInput) samplesDisplayInput.value = '21';
            clearAll();
          });
        }

        // Example buttons
        exampleButtons.forEach(function (btn) {
          btn.addEventListener('click', function () {
            const type = btn.getAttribute('data-example');
            if (!type || !funcInput || !lowerInput || !upperInput) return;
            if (type === 'sin') {
              funcInput.value = 'sin(x)';
              lowerInput.value = '0';
              upperInput.value = String(Math.PI);
            } else if (type === 'poly') {
              funcInput.value = 'x^2';
              lowerInput.value = '0';
              upperInput.value = '1';
            } else if (type === 'gauss') {
              funcInput.value = 'exp(-x^2)';
              lowerInput.value = '-2';
              upperInput.value = '2';
            } else if (type === 'abs') {
              funcInput.value = 'abs(x)';
              lowerInput.value = '-1';
              upperInput.value = '1';
            }
            clearAll();
          });
        });
      });
    </script>

