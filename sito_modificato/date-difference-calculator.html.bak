    <script>
      // Utility: parse date safely (yyyy-mm-dd) to Date at local midnight
      function parseDateInput(value) {
        if (!value) return null;
        const [y, m, d] = value.split("-").map(Number);
        if (!y || !m || !d) return null;
        return new Date(y, m - 1, d);
      }

      function formatDateHuman(date) {
        if (!date) return "";
        return date.toLocaleDateString(undefined, {
          year: "numeric",
          month: "short",
          day: "numeric",
        });
      }

      function diffInDays(start, end, includeEnd) {
        const msPerDay = 24 * 60 * 60 * 1000;
        const utcStart = Date.UTC(
          start.getFullYear(),
          start.getMonth(),
          start.getDate()
        );
        const utcEnd = Date.UTC(
          end.getFullYear(),
          end.getMonth(),
          end.getDate()
        );
        let days = Math.round((utcEnd - utcStart) / msPerDay);
        if (includeEnd && days >= 0) days += 1;
        return days;
      }

      function calendarDiff(start, end) {
        // Ensure start <= end
        let s = new Date(
          start.getFullYear(),
          start.getMonth(),
          start.getDate()
        );
        let e = new Date(end.getFullYear(), end.getMonth(), end.getDate());
        if (e < s) [s, e] = [e, s];

        let years = e.getFullYear() - s.getFullYear();
        let months = e.getMonth() - s.getMonth();
        let days = e.getDate() - s.getDate();

        if (days < 0) {
          months -= 1;
          const prevMonth = new Date(e.getFullYear(), e.getMonth(), 0);
          days += prevMonth.getDate();
        }
        if (months < 0) {
          years -= 1;
          months += 12;
        }
        return { years, months, days };
      }

      function countWorkingDays(start, end, includeEnd, allowedWeekdays) {
        let s = new Date(
          start.getFullYear(),
          start.getMonth(),
          start.getDate()
        );
        let e = new Date(end.getFullYear(), end.getMonth(), end.getDate());
        if (e < s) [s, e] = [e, s];

        let count = 0;
        let current = new Date(s);

        while (current <= e) {
          const day = current.getDay(); // 0=Sun..6=Sat
          if (allowedWeekdays.has(day)) {
            count++;
          }
          current.setDate(current.getDate() + 1);
        }

        if (!includeEnd && count > 0 && allowedWeekdays.has(e.getDay())) {
          count -= 1;
        }
        return count;
      }

      function getIncludeEnd() {
        const radios = document.querySelectorAll('input[name="include-end"]');
        for (const r of radios) {
          if (r.checked) return r.value === "include";
        }
        return false;
      }

      function getAllowedWeekdays() {
        const boxes = document.querySelectorAll(".weekday-filter");
        const set = new Set();
        boxes.forEach((b) => {
          if (b.checked) {
            const d = Number(b.dataset.day);
            if (!Number.isNaN(d)) set.add(d);
          }
        });
        return set;
      }

      function showError(msg) {
        const el = document.getElementById("error-message");
        el.textContent = msg;
        el.classList.remove("hidden");
      }

      function clearError() {
        const el = document.getElementById("error-message");
        el.textContent = "";
        el.classList.add("hidden");
      }

      function updateResults() {
        clearError();
        const startVal = document.getElementById("start-date").value;
        const endVal = document.getElementById("end-date").value;
        const start = parseDateInput(startVal);
        const end = parseDateInput(endVal);

        if (!start || !end) {
          showError("Please select both a start date and an end date.");
          return;
        }

        let s = start;
        let e = end;
        let swapped = false;
        if (e < s) {
          [s, e] = [e, s];
          swapped = true;
        }

        const includeEnd = getIncludeEnd();
        const totalDays = diffInDays(s, e, includeEnd);

        if (totalDays < 0) {
          showError("The end date must be different from the start date.");
          return;
        }

        // Summary
        const summaryCard = document.getElementById("summary-card");
        const summaryRange = document.getElementById("summary-range");
        const summaryMain = document.getElementById("summary-main");
        const summaryNote = document.getElementById("summary-note");

        summaryCard.classList.remove("hidden");
        summaryRange.textContent = `${formatDateHuman(s)} → ${formatDateHuman(
          e
        )}${swapped ? " (dates were reordered)" : ""}`;
        summaryMain.textContent = `${totalDays.toLocaleString()} day${
          Math.abs(totalDays) === 1 ? "" : "s"
        } ${includeEnd ? "(end date included)" : ""}`;
        summaryNote.textContent =
          "See the detailed breakdown below for weeks, months, years, and working days.";

        // Total difference
        document.getElementById("res-days").textContent =
          totalDays.toLocaleString();
        document.getElementById("res-weeks").textContent = (
          totalDays / 7
        ).toFixed(2);
        document.getElementById("res-months-approx").textContent = (
          totalDays / 30.44
        ).toFixed(2);
        document.getElementById("res-years-approx").textContent = (
          totalDays / 365.25
        ).toFixed(3);

        // Calendar breakdown (exclusive of end date for consistency)
        const cal = calendarDiff(s, e);
        document.getElementById("res-years").textContent = cal.years.toString();
        document.getElementById("res-months").textContent =
          cal.months.toString();
        document.getElementById("res-days-rem").textContent =
          cal.days.toString();

        // Working days
        const allowedWeekdays = getAllowedWeekdays();
        const workingDays = allowedWeekdays.size
          ? countWorkingDays(s, e, includeEnd, allowedWeekdays)
          : 0;
        document.getElementById("res-working-days").textContent =
          workingDays.toLocaleString();
      }

      document.addEventListener("DOMContentLoaded", () => {
        const today = new Date();
        const startInput = document.getElementById("start-date");
        const endInput = document.getElementById("end-date");

        // Default: today and today
        const isoToday = today.toISOString().slice(0, 10);
        startInput.value = isoToday;
        endInput.value = isoToday;

        document
          .getElementById("calculate-btn")
          .addEventListener("click", updateResults);

        document.getElementById("reset-btn").addEventListener("click", () => {
          const now = new Date();
          const iso = now.toISOString().slice(0, 10);
          startInput.value = iso;
          endInput.value = iso;
          document.querySelector(
            'input[name="include-end"][value="exclude"]'
          ).checked = true;
          document.getElementById("exclude-weekends").checked = false;
          document.getElementById("weekday-options").classList.add("hidden");
          document.querySelectorAll(".weekday-filter").forEach((b) => {
            const d = Number(b.dataset.day);
            b.checked = d >= 1 && d <= 5;
          });
          clearError();
          document.getElementById("summary-card").classList.add("hidden");
          [
            "res-days",
            "res-weeks",
            "res-months-approx",
            "res-years-approx",
            "res-years",
            "res-months",
            "res-days-rem",
            "res-working-days",
          ].forEach((id) => {
            document.getElementById(id).textContent = "–";
          });
        });

        document.getElementById("swap-dates").addEventListener("click", () => {
          const a = startInput.value;
          const b = endInput.value;
          startInput.value = b;
          endInput.value = a;
        });

        document.getElementById("today-range").addEventListener("click", () => {
          const t = new Date();
          const isoT = t.toISOString().slice(0, 10);
          startInput.value = isoT;
          // set end date 30 days from today
          const future = new Date(t);
          future.setDate(future.getDate() + 30);
          endInput.value = future.toISOString().slice(0, 10);
        });

        document.getElementById("past-range").addEventListener("click", () => {
          const t = new Date();
          const isoT = t.toISOString().slice(0, 10);
          endInput.value = isoT;
          const past = new Date(t);
          past.setDate(past.getDate() - 30);
          startInput.value = past.toISOString().slice(0, 10);
        });

        document
          .getElementById("toggle-weekdays")
          .addEventListener("click", () => {
            const box = document.getElementById("weekday-options");
            box.classList.toggle("hidden");
          });

        document
          .getElementById("exclude-weekends")
          .addEventListener("change", (e) => {
            const checked = e.target.checked;
            document.querySelectorAll(".weekday-filter").forEach((b) => {
              const d = Number(b.dataset.day);
              if (d === 0 || d === 6) {
                b.checked = !checked;
              }
            });
          });
      });
    </script>

