    <script>
      // Standard CPC sizes (mm²) commonly used in BS 7671 contexts
      const standardSizes = [
        0.5, 0.75, 1, 1.5, 2.5, 4, 6, 10, 16, 25, 35, 50, 70, 95, 120, 150, 185,
        240,
      ];

      const modeEl = document.getElementById("mode");
      const materialEl = document.getElementById("material");
      const insulationEl = document.getElementById("insulation");
      const kWrapper = document.getElementById("k-wrapper");
      const kValueEl = document.getElementById("kValue");
      const faultCurrentEl = document.getElementById("faultCurrent");
      const timeEl = document.getElementById("time");
      const areaEl = document.getElementById("area");
      const roundEl = document.getElementById("roundToStandard");
      const resultTextEl = document.getElementById("resultText");
      const resultDetailEl = document.getElementById("resultDetail");
      const kInfoEl = document.getElementById("kInfo");
      const calcBtn = document.getElementById("calculateBtn");

      function getDefaultK(material, insulation) {
        // Approximate typical k values (A·√s/mm²)
        if (material === "copper") {
          if (insulation === "pvc") return 115;
          if (insulation === "xlpe") return 143;
          if (insulation === "bare") return 226;
        } else if (material === "aluminium") {
          if (insulation === "pvc") return 76;
          if (insulation === "xlpe") return 94;
          if (insulation === "bare") return 125;
        }
        return null;
      }

      function updateKInfo() {
        const material = materialEl.value;
        const insulation = insulationEl.value;
        const kDefault = getDefaultK(material, insulation);
        if (insulation === "custom") {
          kInfoEl.textContent =
            "Enter the k factor from the appropriate BS 7671 table for your conductor material, insulation and temperature assumptions.";
        } else if (kDefault) {
          kInfoEl.textContent =
            "Using k ≈ " +
            kDefault +
            " A·√s/mm² (typical value for " +
            material +
            ", " +
            insulation.toUpperCase() +
            "). Always confirm against the latest BS 7671 tables.";
        } else {
          kInfoEl.textContent = "";
        }
      }

      function onInsulationChange() {
        if (insulationEl.value === "custom") {
          kWrapper.classList.remove("hidden");
        } else {
          kWrapper.classList.add("hidden");
        }
        updateKInfo();
      }

      function onMaterialChange() {
        updateKInfo();
      }

      function onModeChange() {
        const mode = modeEl.value;
        // Show/hide input fields depending on which variable is being solved
        document.getElementById("input-I").classList.remove("hidden");
        document.getElementById("input-t").classList.remove("hidden");
        document.getElementById("input-S").classList.remove("hidden");

        if (mode === "S") {
          // Solve for S: need I and t
          areaEl.parentElement.classList.add("opacity-60");
          areaEl.disabled = true;
          faultCurrentEl.disabled = false;
          timeEl.disabled = false;
        } else if (mode === "I") {
          faultCurrentEl.parentElement.classList.add("opacity-60");
          faultCurrentEl.disabled = true;
          areaEl.disabled = false;
          timeEl.disabled = false;
          areaEl.parentElement.classList.remove("opacity-60");
          timeEl.parentElement.classList.remove("opacity-60");
        } else if (mode === "t") {
          timeEl.parentElement.classList.add("opacity-60");
          timeEl.disabled = true;
          areaEl.disabled = false;
          faultCurrentEl.disabled = false;
          areaEl.parentElement.classList.remove("opacity-60");
          faultCurrentEl.parentElement.classList.remove("opacity-60");
        }

        // Reset classes for others
        if (mode !== "S") {
          areaEl.parentElement.classList.remove("opacity-60");
        }
        if (mode !== "I") {
          faultCurrentEl.parentElement.classList.remove("opacity-60");
        }
        if (mode !== "t") {
          timeEl.parentElement.classList.remove("opacity-60");
        }

        resultTextEl.textContent = "Enter values and click “Calculate”.";
        resultDetailEl.textContent = "";
      }

      function roundUpToStandard(size) {
        for (let i = 0; i < standardSizes.length; i++) {
          if (size <= standardSizes[i]) return standardSizes[i];
        }
        return size;
      }

      function calculate() {
        const mode = modeEl.value;
        const material = materialEl.value;
        const insulation = insulationEl.value;

        let k;
        if (insulation === "custom") {
          k = parseFloat(kValueEl.value);
          if (!isFinite(k) || k <= 0) {
            resultTextEl.textContent =
              "Please enter a valid positive k factor.";
            resultDetailEl.textContent = "";
            return;
          }
        } else {
          k = getDefaultK(material, insulation);
          if (!k) {
            resultTextEl.textContent =
              "Unable to determine k factor for the selected options.";
            resultDetailEl.textContent = "";
            return;
          }
        }

        const I = parseFloat(faultCurrentEl.value);
        const t = parseFloat(timeEl.value);
        const S = parseFloat(areaEl.value);

        let result,
          detail = "";

        if (mode === "S") {
          if (!isFinite(I) || I <= 0 || !isFinite(t) || t <= 0) {
            resultTextEl.textContent =
              "Please enter positive values for fault current and disconnection time.";
            resultDetailEl.textContent = "";
            return;
          }
          const Smin = Math.sqrt(I * I * t) / k;
          let displayS = Smin;
          if (roundEl.checked) {
            const rounded = roundUpToStandard(Smin);
            detail =
              "Calculated minimum S = " +
              Smin.toFixed(3) +
              " mm². Rounded up to next standard size.";
            displayS = rounded;
          } else {
            detail =
              "Calculated minimum cross-sectional area from S = √(I² × t) / k.";
          }
          result =
            "Minimum protective conductor size S ≈ " +
            displayS.toFixed(3) +
            " mm²";
        } else if (mode === "I") {
          if (!isFinite(S) || S <= 0 || !isFinite(t) || t <= 0) {
            resultTextEl.textContent =
              "Please enter positive values for conductor area and disconnection time.";
            resultDetailEl.textContent = "";
            return;
          }
          const Imax = (k * S) / Math.sqrt(t);
          result =
            "Maximum permissible fault current I ≈ " + Imax.toFixed(1) + " A";
          detail = "From I = (k × S) / √t.";
        } else if (mode === "t") {
          if (!isFinite(S) || S <= 0 || !isFinite(I) || I <= 0) {
            resultTextEl.textContent =
              "Please enter positive values for conductor area and fault current.";
            resultDetailEl.textContent = "";
            return;
          }
          const tmax = Math.pow((k * S) / I, 2);
          result =
            "Maximum permissible disconnection time t ≈ " +
            tmax.toFixed(3) +
            " s";
          detail = "From t = (k × S / I)².";
        }

        resultTextEl.textContent = result;
        resultDetailEl.textContent = detail;
      }

      insulationEl.addEventListener("change", onInsulationChange);
      materialEl.addEventListener("change", onMaterialChange);
      modeEl.addEventListener("change", onModeChange);
      calcBtn.addEventListener("click", calculate);

      // Initialise UI
      onInsulationChange();
      onModeChange();
      updateKInfo();
    </script>

