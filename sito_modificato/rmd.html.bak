    <script>
      // --- IRS Uniform Lifetime Table (2023+) partial factors ---
      // Source: IRS Pub. 590-B (rounded to 1 decimal where applicable)
      const UNIFORM_TABLE = {
        73: 26.5,
        74: 25.5,
        75: 24.7,
        76: 23.7,
        77: 22.9,
        78: 22.0,
        79: 21.1,
        80: 20.2,
        81: 19.4,
        82: 18.5,
        83: 17.7,
        84: 16.8,
        85: 16.0,
        86: 15.2,
        87: 14.4,
        88: 13.7,
        89: 12.9,
        90: 12.2,
        91: 11.5,
        92: 10.8,
        93: 10.1,
        94: 9.5,
        95: 8.9,
        96: 8.4,
        97: 7.8,
        98: 7.3,
        99: 6.8,
        100: 6.4,
        101: 6.0,
        102: 5.6,
        103: 5.2,
        104: 4.9,
        105: 4.6,
        106: 4.3,
        107: 4.1,
        108: 3.9,
        109: 3.7,
        110: 3.5,
        111: 3.4,
        112: 3.3,
        113: 3.1,
        114: 3.0,
        115: 2.9,
        116: 2.8,
        117: 2.7,
        118: 2.5,
        119: 2.3,
        120: 2.0,
      };

      // Simplified Joint Life table: approximate factors for age 73-90 when spouse is >10 years younger.
      // These are rough approximations for educational use only.
      const JOINT_TABLE_APPROX = {
        73: 28.8,
        74: 27.8,
        75: 26.8,
        76: 25.8,
        77: 24.8,
        78: 23.8,
        79: 22.8,
        80: 21.8,
        81: 20.9,
        82: 20.0,
        83: 19.1,
        84: 18.2,
        85: 17.3,
        86: 16.5,
        87: 15.7,
        88: 14.9,
        89: 14.1,
        90: 13.4,
      };

      // Simplified Single Life table for inherited accounts (selected ages)
      const SINGLE_TABLE_APPROX = {
        30: 53.3,
        40: 43.6,
        50: 36.2,
        60: 29.9,
        70: 24.7,
        73: 22.9,
        75: 21.5,
        80: 18.7,
        85: 15.9,
        90: 13.2,
      };

      function interpolateTable(table, age) {
        age = Math.floor(age);
        if (table[age] !== undefined) return table[age];

        // Find nearest lower and higher ages
        const ages = Object.keys(table)
          .map(Number)
          .sort((a, b) => a - b);
        if (age < ages[0]) return table[ages[0]];
        if (age > ages[ages.length - 1]) return table[ages[ages.length - 1]];

        let lower = ages[0],
          upper = ages[ages.length - 1];
        for (let i = 0; i < ages.length - 1; i++) {
          if (age >= ages[i] && age <= ages[i + 1]) {
            lower = ages[i];
            upper = ages[i + 1];
            break;
          }
        }
        const fLower = table[lower];
        const fUpper = table[upper];
        const t = (age - lower) / (upper - lower);
        return fLower + t * (fUpper - fLower);
      }

      function getRmdStartAge(dob) {
        // Simplified SECURE / SECURE 2.0 logic based on year of birth
        const year = dob.getFullYear();
        const month = dob.getMonth() + 1;
        const day = dob.getDate();

        const bornBeforeJuly11949 =
          year < 1949 ||
          (year === 1949 && (month < 7 || (month === 7 && day < 1)));

        if (bornBeforeJuly11949) return 70.5;
        if (year === 1949 && (month > 6 || (month === 7 && day >= 1)))
          return 72;
        if (year === 1950) return 72;
        if (year >= 1951 && year <= 1959) return 73;
        if (year >= 1960) return 75;
        return 73;
      }

      function calculateAgeOnDate(dob, onDate) {
        let age = onDate.getFullYear() - dob.getFullYear();
        const m = onDate.getMonth() - dob.getMonth();
        if (m < 0 || (m === 0 && onDate.getDate() < dob.getDate())) {
          age--;
        }
        return age;
      }

      function formatCurrency(value) {
        if (!isFinite(value)) return "–";
        return value.toLocaleString("en-US", {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 2,
        });
      }

      function formatPercent(value) {
        if (!isFinite(value)) return "–";
        return value.toFixed(2) + "%";
      }

      // Initialize year dropdown and default account row
      document.addEventListener("DOMContentLoaded", () => {
        const yearSelect = document.getElementById("rmd-year");
        const currentYear = new Date().getFullYear();
        for (let y = currentYear - 1; y <= currentYear + 20; y++) {
          const opt = document.createElement("option");
          opt.value = y;
          opt.textContent = y;
          if (y === currentYear) opt.selected = true;
          yearSelect.appendChild(opt);
        }

        addAccountRow(); // initial account

        document
          .getElementById("add-account")
          .addEventListener("click", addAccountRow);
        document
          .getElementById("calculate-btn")
          .addEventListener("click", handleCalculate);
        document
          .getElementById("reset-btn")
          .addEventListener("click", resetForm);

        document
          .getElementById("table-type")
          .addEventListener("change", toggleSpouseSection);
        document
          .getElementById("owner-type")
          .addEventListener("change", ownerTypeChanged);
      });

      function ownerTypeChanged() {
        const ownerType = document.getElementById("owner-type").value;
        const tableType = document.getElementById("table-type");
        if (ownerType === "inherited") {
          tableType.value = "single";
        } else if (tableType.value === "single") {
          tableType.value = "uniform";
        }
        toggleSpouseSection();
      }

      function toggleSpouseSection() {
        const tableType = document.getElementById("table-type").value;
        const spouseSection = document.getElementById("spouse-section");
        spouseSection.classList.toggle("hidden", tableType !== "joint");
      }

      let accountCounter = 0;
      function addAccountRow() {
        const container = document.getElementById("accounts-container");
        const rowId = ++accountCounter;

        const row = document.createElement("div");
        row.className = "grid grid-cols-12 gap-2 items-end";
        row.dataset.accountId = rowId;

        row.innerHTML = `
      <div class="col-span-6">
        <label class="input-label" for="account-name-${rowId}">Account name</label>
        <input id="account-name-${rowId}" type="text" class="input-field" placeholder="e.g., Traditional IRA #1" />
      </div>
      <div class="col-span-5">
        <label class="input-label" for="account-balance-${rowId}">Balance as of Dec 31 (USD)</label>
        <input id="account-balance-${rowId}" type="number" min="0" step="0.01" class="input-field" placeholder="e.g., 250000" />
      </div>
      <div class="col-span-1 flex items-center justify-end pb-2">
        <button type="button" class="text-gray-400 hover:text-red-600 text-xl font-bold leading-none remove-account" title="Remove account">&times;</button>
      </div>
    `;

        container.appendChild(row);

        row.querySelector(".remove-account").addEventListener("click", () => {
          if (
            document.querySelectorAll("#accounts-container > div").length > 1
          ) {
            row.remove();
          } else {
            // clear instead of remove last row
            row.querySelector(`#account-name-${rowId}`).value = "";
            row.querySelector(`#account-balance-${rowId}`).value = "";
          }
        });
      }

      function handleCalculate() {
        const errorEl = document.getElementById("calc-error");
        errorEl.classList.add("hidden");
        errorEl.textContent = "";

        const dobStr = document.getElementById("dob").value;
        const rmdYear = parseInt(document.getElementById("rmd-year").value, 10);
        const ownerType = document.getElementById("owner-type").value;
        const tableType = document.getElementById("table-type").value;

        if (!dobStr) {
          showError("Please enter your date of birth.");
          return;
        }
        const dob = new Date(dobStr + "T00:00:00");

        if (tableType === "joint") {
          const spouseDobStr = document.getElementById("spouse-dob").value;
          const spouseBeneficiary =
            document.getElementById("spouse-beneficiary").value;
          if (!spouseDobStr) {
            showError(
              "Please enter your spouse’s date of birth for the Joint Life table."
            );
            return;
          }
          if (spouseBeneficiary !== "yes") {
            showError(
              "Joint Life table generally requires your spouse to be the sole beneficiary for the entire year."
            );
            return;
          }
        }

        const accountRows = Array.from(
          document.querySelectorAll("#accounts-container > div")
        );
        const accounts = [];
        let totalBalance = 0;
        for (const row of accountRows) {
          const id = row.dataset.accountId;
          const name =
            document.getElementById(`account-name-${id}`).value.trim() ||
            `Account ${id}`;
          const balStr = document.getElementById(`account-balance-${id}`).value;
          const bal = parseFloat(balStr);
          if (!isNaN(bal) && bal > 0) {
            accounts.push({ name, balance: bal });
            totalBalance += bal;
          }
        }

        if (totalBalance <= 0) {
          showError(
            "Please enter at least one positive prior year-end balance."
          );
          return;
        }

        // Determine if RMDs are required for this year
        const rmdStartAge = getRmdStartAge(dob);
        const asOfDate = new Date(rmdYear, 11, 31); // Dec 31 of RMD year
        const ageThisYear = calculateAgeOnDate(dob, asOfDate);

        if (ownerType === "owner" && ageThisYear + 0.01 < rmdStartAge) {
          // Not yet required
          showResults({
            totalRmd: 0,
            factor: null,
            totalBalance,
            accounts,
            status: `Based on your date of birth, RMDs are not yet required for ${rmdYear} under the simplified SECURE / SECURE 2.0 rules used here.`,
            tableType,
            ownerType,
            ageThisYear,
            rmdStartAge,
          });
          return;
        }

        // Determine life expectancy factor
        let factor = null;
        let factorLabel = "";

        if (tableType === "uniform") {
          factor =
            UNIFORM_TABLE[ageThisYear] ??
            interpolateTable(UNIFORM_TABLE, ageThisYear);
          factorLabel = `Uniform Lifetime Table, age ${ageThisYear}`;
        } else if (tableType === "joint") {
          const spouseDob = new Date(
            document.getElementById("spouse-dob").value + "T00:00:00"
          );
          const spouseAge = calculateAgeOnDate(spouseDob, asOfDate);
          const ageDiff = ageThisYear - spouseAge;
          factor =
            JOINT_TABLE_APPROX[ageThisYear] ??
            interpolateTable(JOINT_TABLE_APPROX, ageThisYear);
          factorLabel = `Approximate Joint Life Table, owner age ${ageThisYear}, spouse age ${spouseAge} (age diff ${ageDiff} years)`;
        } else if (tableType === "single") {
          factor =
            SINGLE_TABLE_APPROX[ageThisYear] ??
            interpolateTable(SINGLE_TABLE_APPROX, ageThisYear);
          factorLabel = `Approximate Single Life Table, age ${ageThisYear}`;
        }

        if (!factor || !isFinite(factor) || factor <= 0) {
          showError(
            "Unable to determine a life expectancy factor for the selected age and table. Please verify your inputs."
          );
          return;
        }

        const totalRmd = totalBalance / factor;
        const status = `This estimate uses a life expectancy factor of ${factor.toFixed(
          1
        )} for age ${ageThisYear} in ${rmdYear}.`;

        showResults({
          totalRmd,
          factor,
          totalBalance,
          accounts,
          status,
          tableType,
          ownerType,
          ageThisYear,
          rmdStartAge,
        });
      }

      function showError(msg) {
        const errorEl = document.getElementById("calc-error");
        errorEl.textContent = msg;
        errorEl.classList.remove("hidden");
        document.getElementById("results-section").classList.add("hidden");
      }

      function showResults({
        totalRmd,
        factor,
        totalBalance,
        accounts,
        status,
        tableType,
        ownerType,
        ageThisYear,
        rmdStartAge,
      }) {
        const resultsSection = document.getElementById("results-section");
        resultsSection.classList.remove("hidden");

        const totalRmdEl = document.getElementById("result-total-rmd");
        const factorEl = document.getElementById("result-factor");
        const factorLabelEl = document.getElementById("result-factor-label");
        const rateEl = document.getElementById("result-rate");
        const statusEl = document.getElementById("result-status");

        if (!factor || totalRmd === 0) {
          totalRmdEl.textContent = "$0";
          factorEl.textContent = "–";
          factorLabelEl.textContent = "";
          rateEl.textContent = "–";
          statusEl.textContent = status;
          document
            .getElementById("per-account-results")
            .classList.add("hidden");
          return;
        }

        totalRmdEl.textContent = formatCurrency(totalRmd);
        factorEl.textContent = factor.toFixed(1);
        factorLabelEl.textContent = factor
          ? (factorLabelEl.textContent = "")
          : "";

        const rate = (totalRmd / totalBalance) * 100;
        rateEl.textContent = formatPercent(rate);

        let extra = "";
        if (ownerType === "owner") {
          extra = ` Under current law, your RMDs are assumed to start at approximately age ${rmdStartAge}.`;
        } else {
          extra =
            " This is a simplified estimate for an inherited account and may not reflect all IRS rules.";
        }

        statusEl.textContent = status + extra;

        // Per-account breakdown (pro-rata)
        const perAccountSection = document.getElementById(
          "per-account-results"
        );
        const tbody = document.getElementById("per-account-tbody");
        tbody.innerHTML = "";

        if (accounts.length > 1 && totalRmd > 0) {
          accounts.forEach((acc) => {
            const share = totalRmd * (acc.balance / totalBalance);
            const tr = document.createElement("tr");
            tr.innerHTML = `
          <td class="px-3 py-1 border-b text-gray-800">${acc.name}</td>
          <td class="px-3 py-1 border-b text-right text-gray-700 result-number">${formatCurrency(
            acc.balance
          )}</td>
          <td class="px-3 py-1 border-b text-right text-gray-900 font-medium result-number">${formatCurrency(
            share
          )}</td>
        `;
            tbody.appendChild(tr);
          });
          perAccountSection.classList.remove("hidden");
        } else {
          perAccountSection.classList.add("hidden");
        }
      }

      function resetForm() {
        document.getElementById("dob").value = "";
        document.getElementById("owner-type").value = "owner";
        document.getElementById("table-type").value = "uniform";
        document.getElementById("spouse-dob").value = "";
        document.getElementById("spouse-beneficiary").value = "yes";
        toggleSpouseSection();

        const container = document.getElementById("accounts-container");
        container.innerHTML = "";
        accountCounter = 0;
        addAccountRow();

        document.getElementById("calc-error").classList.add("hidden");
        document.getElementById("results-section").classList.add("hidden");
      }
    </script>

    <script>
      window.MathJax = {
        tex: {
          inlineMath: [
