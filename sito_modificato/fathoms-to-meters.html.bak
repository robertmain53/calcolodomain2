    <script>
      (function () {
        const FATHOM_TO_METER = 1.8288;
        const fathomsInput = document.getElementById("fathomsInput");
        const metersInput = document.getElementById("metersInput");
        const resultText = document.getElementById("resultText");
        const swapBtn = document.getElementById("swapBtn");
        const clearBtn = document.getElementById("clearBtn");
        const quickTableBody = document.getElementById("quickTableBody");

        function formatNumber(value, decimals) {
          if (!isFinite(value)) return "";
          return Number(value.toFixed(decimals)).toString();
        }

        function updateFromFathoms() {
          const v = parseFloat(fathomsInput.value);
          if (isNaN(v)) {
            metersInput.value = "";
            resultText.classList.add("hidden");
            return;
          }
          const meters = v * FATHOM_TO_METER;
          metersInput.value = formatNumber(meters, 6);
          resultText.textContent = `${v} fathom${
            v === 1 ? "" : "s"
          } = ${formatNumber(meters, 4)} meter${
            Math.abs(meters) === 1 ? "" : "s"
          }`;
          resultText.classList.remove("hidden");
        }

        function updateFromMeters() {
          const v = parseFloat(metersInput.value);
          if (isNaN(v)) {
            fathomsInput.value = "";
            resultText.classList.add("hidden");
            return;
          }
          const fathoms = v / FATHOM_TO_METER;
          fathomsInput.value = formatNumber(fathoms, 6);
          resultText.textContent = `${v} meter${
            v === 1 ? "" : "s"
          } = ${formatNumber(fathoms, 4)} fathom${
            Math.abs(fathoms) === 1 ? "" : "s"
          }`;
          resultText.classList.remove("hidden");
        }

        fathomsInput.addEventListener("input", function () {
          if (document.activeElement === fathomsInput) {
            updateFromFathoms();
          }
        });

        metersInput.addEventListener("input", function () {
          if (document.activeElement === metersInput) {
            updateFromMeters();
          }
        });

        swapBtn.addEventListener("click", function () {
          const f = fathomsInput.value;
          const m = metersInput.value;
          fathomsInput.value = m;
          metersInput.value = f;
          if (document.activeElement === metersInput) {
            updateFromMeters();
          } else {
            updateFromFathoms();
          }
        });

        clearBtn.addEventListener("click", function () {
          fathomsInput.value = "";
          metersInput.value = "";
          resultText.classList.add("hidden");
        });

        // Populate quick table for 1–20 fathoms (step 1), then 30, 40, 50
        const baseValues = [];
        for (let i = 1; i <= 20; i++) baseValues.push(i);
        baseValues.push(30, 40, 50);

        baseValues.forEach((f) => {
          const m = f * FATHOM_TO_METER;
          const ft = f * 6;
          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="px-3 py-1 border-b">${f}</td>
        <td class="px-3 py-1 border-b">${formatNumber(m, 4)}</td>
        <td class="px-3 py-1 border-b">${formatNumber(ft, 0)}</td>
      `;
          quickTableBody.appendChild(tr);
        });
      })();
    </script>

