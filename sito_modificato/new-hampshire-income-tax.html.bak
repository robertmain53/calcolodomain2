    <script>
      // Simple NH income tax calculator logic
      (function () {
        const yearSelect = document.getElementById("nh-tax-year");
        const statusSelect = document.getElementById("nh-filing-status");
        const interestInput = document.getElementById("nh-interest");
        const dividendsInput = document.getElementById("nh-dividends");
        const otherIncomeInput = document.getElementById("nh-other-income");
        const thresholdOverrideInput = document.getElementById(
          "nh-threshold-override"
        );
        const resetThresholdBtn = document.getElementById("nh-reset-threshold");
        const calcBtn = document.getElementById("nh-calc-btn");

        const totalIdEl = document.getElementById("nh-total-id");
        const thresholdEl = document.getElementById("nh-threshold");
        const taxableBaseEl = document.getElementById("nh-taxable-base");
        const taxRateEl = document.getElementById("nh-tax-rate");
        const taxDueEl = document.getElementById("nh-tax-due");
        const effIdEl = document.getElementById("nh-effective-id");
        const effAllEl = document.getElementById("nh-effective-all");

        // Example rates and thresholds (illustrative only)
        const nhRates = {
          2024: 0.04,
          2025: 0.03,
          2026: 0.02,
        };

        const nhThresholds = {
          single: 2400,
          married: 4800,
          hoh: 3600,
        };

        function getStatusKey() {
          const v = statusSelect.value;
          if (v === "married") return "married";
          if (v === "hoh") return "hoh";
          return "single";
        }

        function formatMoney(value) {
          return value.toLocaleString("en-US", {
            style: "currency",
            currency: "USD",
          });
        }

        function formatPercent(value) {
          return (value * 100).toFixed(1) + "%";
        }

        function getThreshold() {
          const override = parseFloat(thresholdOverrideInput.value);
          if (!isNaN(override) && override >= 0) {
            return override;
          }
          return nhThresholds[getStatusKey()] || 0;
        }

        function calculate() {
          const year = parseInt(yearSelect.value, 10);
          const rate = nhRates[year] ?? 0;

          const interest = parseFloat(interestInput.value) || 0;
          const dividends = parseFloat(dividendsInput.value) || 0;
          const otherIncome = parseFloat(otherIncomeInput.value) || 0;

          const totalID = interest + dividends;
          const threshold = getThreshold();
          const taxableBase = Math.max(0, totalID - threshold);
          const tax = taxableBase * rate;
          const totalIncome = totalID + otherIncome;

          totalIdEl.textContent = formatMoney(totalID);
          thresholdEl.textContent = formatMoney(threshold);
          taxableBaseEl.textContent = formatMoney(taxableBase);
          taxRateEl.textContent = (rate * 100).toFixed(1) + "%";
          taxDueEl.textContent = formatMoney(tax);

          const effId = totalID > 0 ? tax / totalID : 0;
          const effAll = totalIncome > 0 ? tax / totalIncome : 0;

          effIdEl.textContent = formatPercent(effId);
          effAllEl.textContent = formatPercent(effAll);
        }

        calcBtn.addEventListener("click", calculate);
        resetThresholdBtn.addEventListener("click", function () {
          thresholdOverrideInput.value = "";
          calculate();
        });

        // Auto-recalculate on key changes
        [
          yearSelect,
          statusSelect,
          interestInput,
          dividendsInput,
          otherIncomeInput,
          thresholdOverrideInput,
        ].forEach((el) => {
          el.addEventListener("change", calculate);
          el.addEventListener("input", function () {
            // Debounce slightly
            clearTimeout(el._nhTimer);
            el._nhTimer = setTimeout(calculate, 200);
          });
        });

        // Initial calculation
        calculate();
      })();

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
