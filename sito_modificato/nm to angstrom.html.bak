            <script>
              document.addEventListener('DOMContentLoaded', () => {
                const mInput = document.getElementById('m-input');
                const umInput = document.getElementById('um-input');
                const nmInput = document.getElementById('nm-input');
                const aInput = document.getElementById('angstrom-input');
                const sciOutput = document.getElementById('sci-output');
                
                const inputs = [mInput, umInput, nmInput, aInput];
                
                // Formattatore
                function formatNum(num, precision = 9) {
                    if (isNaN(num) || num === 0) return '';
                    return parseFloat(num.toPrecision(precision)).toString();
                }

                function update(sourceUnit, value) {
                    if (isNaN(value) || value === 0) {
                        inputs.forEach(input => input.value = '');
                        sciOutput.textContent = '-';
                        return;
                    }

                    let m = 0; // Base unit

                    // 1. Converti l'input sorgente in Metri
                    if (sourceUnit === 'm') {
                        m = value;
                    } else if (sourceUnit === 'um') {
                        m = value / 1_000_000;
                    } else if (sourceUnit === 'nm') {
                        m = value / 1_000_000_000;
                    } else if (sourceUnit === 'a') {
                        m = value / 10_000_000_000;
                    }
                    
                    // 2. Aggiorna tutti gli altri campi
                    const values = {
                        m: m,
                        um: m * 1_000_000,
                        nm: m * 1_000_000_000,
                        a: m * 10_000_000_000
                    };
                    
                    for (const input of inputs) {
                        const unit = input.dataset.unit;
                        if (unit !== sourceUnit) {
                            input.value = formatNum(values[unit]);
                        }
                    }
                    
                    // 3. Aggiorna la notazione scientifica
                    sciOutput.textContent = `${m.toExponential(6)} m`;
                }

                inputs.forEach(input => {
                  input.addEventListener('input', (e) => {
                    const sourceUnit = e.target.dataset.unit;
                    const value = parseFloat(e.target.value);
                    update(sourceUnit, value);
                  });
                });
              });
            </script>
            
