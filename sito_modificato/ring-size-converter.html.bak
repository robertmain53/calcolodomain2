    <script>
      // Ring size reference data (subset covering common sizes, can be extended)
      const ringSizeData = [
        // US, UK, EU, IT, JP, CN, diameter_mm
        { us: 3, uk: "F", eu: 44, it: 4, jp: 4, cn: 6, d: 14.1 },
        { us: 3.5, uk: "G", eu: 45, it: 5, jp: 5, cn: 7, d: 14.5 },
        { us: 4, uk: "H", eu: 46, it: 6, jp: 7, cn: 9, d: 14.9 },
        { us: 4.5, uk: "I", eu: 47, it: 7, jp: 8, cn: 10, d: 15.3 },
        { us: 5, uk: "J 1/2", eu: 49, it: 9, jp: 9, cn: 11, d: 15.7 },
        { us: 5.5, uk: "K 1/2", eu: 50, it: 10, jp: 10, cn: 12, d: 16.1 },
        { us: 6, uk: "L 1/2", eu: 51, it: 11, jp: 11, cn: 13, d: 16.5 },
        { us: 6.5, uk: "M 1/2", eu: 53, it: 13, jp: 13, cn: 15, d: 16.9 },
        { us: 7, uk: "O", eu: 54, it: 14, jp: 14, cn: 16, d: 17.3 },
        { us: 7.5, uk: "P", eu: 55, it: 15, jp: 15, cn: 17, d: 17.7 },
        { us: 8, uk: "Q", eu: 57, it: 17, jp: 16, cn: 18, d: 18.1 },
        { us: 8.5, uk: "Q 1/2", eu: 58, it: 18, jp: 18, cn: 20, d: 18.5 },
        { us: 9, uk: "R 1/2", eu: 59, it: 19, jp: 19, cn: 21, d: 18.9 },
        { us: 9.5, uk: "S 1/2", eu: 61, it: 21, jp: 20, cn: 22, d: 19.4 },
        { us: 10, uk: "T 1/2", eu: 62, it: 22, jp: 22, cn: 24, d: 19.8 },
        { us: 10.5, uk: "U 1/2", eu: 63, it: 23, jp: 23, cn: 25, d: 20.2 },
        { us: 11, uk: "V 1/2", eu: 65, it: 25, jp: 24, cn: 26, d: 20.6 },
        { us: 11.5, uk: "W 1/2", eu: 66, it: 26, jp: 26, cn: 28, d: 21.0 },
        { us: 12, uk: "Y", eu: 67, it: 27, jp: 27, cn: 29, d: 21.4 },
        { us: 12.5, uk: "Z", eu: 69, it: 29, jp: 28, cn: 30, d: 21.8 },
        { us: 13, uk: "Z+1", eu: 70, it: 30, jp: 29, cn: 31, d: 22.2 },
      ];

      const inputModeEl = document.getElementById("input-mode");
      const unitWrapper = document.getElementById("unit-wrapper");
      const unitSelect = document.getElementById("unit-select");
      const sizeSelectWrapper = document.getElementById("size-select-wrapper");
      const sizeSelect = document.getElementById("size-select");
      const sizeInputWrapper = document.getElementById("size-input-wrapper");
      const sizeInput = document.getElementById("size-input");
      const convertBtn = document.getElementById("convert-btn");
      const errorMsg = document.getElementById("error-msg");
      const showHalfSizesEl = document.getElementById("show-half-sizes");
      const tableBody = document.getElementById("size-table-body");

      const resultEls = {
        us: document.getElementById("result-us"),
        uk: document.getElementById("result-uk"),
        eu: document.getElementById("result-eu"),
        it: document.getElementById("result-it"),
        jp: document.getElementById("result-jp"),
        cn: document.getElementById("result-cn"),
        diameter: document.getElementById("result-diameter"),
        circumference: document.getElementById("result-circumference"),
      };

      function formatDiameter(d) {
        if (!d) return "–";
        const mm = d.toFixed(2);
        const inches = (d / 25.4).toFixed(3);
        return `${mm} mm (${inches} in)`;
      }

      function formatCircumference(d) {
        if (!d) return "–";
        const c = Math.PI * d;
        const mm = c.toFixed(2);
        const inches = (c / 25.4).toFixed(3);
        return `${mm} mm (${inches} in)`;
      }

      function populateSizeSelect() {
        const mode = inputModeEl.value;
        sizeSelect.innerHTML = "";
        if (["diameter", "circumference"].includes(mode)) return;

        let values;
        if (mode === "us") {
          values = [...new Set(ringSizeData.map((r) => r.us))];
        } else if (mode === "uk") {
          values = [...new Set(ringSizeData.map((r) => r.uk))];
        } else if (mode === "eu") {
          values = [...new Set(ringSizeData.map((r) => r.eu))];
        } else if (mode === "it") {
          values = [...new Set(ringSizeData.map((r) => r.it))];
        } else if (mode === "jp") {
          values = [...new Set(ringSizeData.map((r) => r.jp))];
        }
        values.forEach((v) => {
          const opt = document.createElement("option");
          opt.value = v;
          opt.textContent = v;
          sizeSelect.appendChild(opt);
        });
      }

      function renderTable() {
        const showHalf = showHalfSizesEl.checked;
        tableBody.innerHTML = "";
        ringSizeData.forEach((row, idx) => {
          const isHalf = (row.us * 2) % 2 !== 0; // e.g. 5.5
          if (!showHalf && isHalf) return;

          const tr = document.createElement("tr");
          tr.dataset.index = idx;
          tr.className = "hover:bg-blue-50 cursor-pointer";

          const c = Math.PI * row.d;
          const cells = [
            row.us,
            row.uk,
            row.eu,
            row.it,
            row.jp,
            row.cn,
            row.d.toFixed(2),
            c.toFixed(1),
          ];
          cells.forEach((text) => {
            const td = document.createElement("td");
            td.className = "px-2 py-1 whitespace-nowrap";
            td.textContent = text;
            tr.appendChild(td);
          });

          tr.addEventListener("click", () => {
            highlightRow(idx);
            updateResultsFromRow(row);
          });

          tableBody.appendChild(tr);
        });
      }

      function highlightRow(index) {
        document.querySelectorAll("#size-table-body tr").forEach((tr) => {
          tr.classList.remove("size-row-active");
        });
        const target = document.querySelector(
          `#size-table-body tr[data-index="${index}"]`
        );
        if (target) {
          target.classList.add("size-row-active");
          target.scrollIntoView({ block: "center", behavior: "smooth" });
        }
      }

      function updateResultsFromRow(row) {
        resultEls.us.textContent = row.us ?? "–";
        resultEls.uk.textContent = row.uk ?? "–";
        resultEls.eu.textContent = row.eu ?? "–";
        resultEls.it.textContent = row.it ?? "–";
        resultEls.jp.textContent = row.jp ?? "–";
        resultEls.cn.textContent = row.cn ?? "–";
        resultEls.diameter.textContent = formatDiameter(row.d);
        resultEls.circumference.textContent = formatCircumference(row.d);
      }

      function findClosestByDiameter(diameterMm) {
        let closest = ringSizeData[0];
        let minDiff = Math.abs(ringSizeData[0].d - diameterMm);
        ringSizeData.forEach((row) => {
          const diff = Math.abs(row.d - diameterMm);
          if (diff < minDiff) {
            minDiff = diff;
            closest = row;
          }
        });
        return closest;
      }

      function onModeChange() {
        const mode = inputModeEl.value;
        errorMsg.classList.add("hidden");
        if (mode === "diameter" || mode === "circumference") {
          unitWrapper.classList.remove("hidden");
          sizeInputWrapper.classList.remove("hidden");
          sizeSelectWrapper.classList.add("hidden");
        } else {
          unitWrapper.classList.add("hidden");
          sizeInputWrapper.classList.add("hidden");
          sizeSelectWrapper.classList.remove("hidden");
          populateSizeSelect();
        }
      }

      function convert() {
        errorMsg.classList.add("hidden");
        const mode = inputModeEl.value;
        let targetRow = null;

        if (mode === "diameter" || mode === "circumference") {
          const val = parseFloat(sizeInput.value);
          if (isNaN(val) || val <= 0) {
            errorMsg.textContent = "Please enter a positive numeric value.";
            errorMsg.classList.remove("hidden");
            return;
          }
          let diameterMm;
          if (mode === "diameter") {
            diameterMm = unitSelect.value === "mm" ? val : val * 25.4;
          } else {
            const circumferenceMm =
              unitSelect.value === "mm" ? val : val * 25.4;
            diameterMm = circumferenceMm / Math.PI;
          }
          targetRow = findClosestByDiameter(diameterMm);
        } else {
          const selected = sizeSelect.value;
          if (!selected) {
            errorMsg.textContent = "Please select a size.";
            errorMsg.classList.remove("hidden");
            return;
          }
          if (mode === "us") {
            const num = parseFloat(selected);
            targetRow = ringSizeData.find((r) => r.us === num);
          } else if (mode === "uk") {
            targetRow = ringSizeData.find((r) => r.uk === selected);
          } else if (mode === "eu") {
            const num = parseInt(selected, 10);
            targetRow = ringSizeData.find((r) => r.eu === num);
          } else if (mode === "it") {
            const num = parseInt(selected, 10);
            targetRow = ringSizeData.find((r) => r.it === num);
          } else if (mode === "jp") {
            const num = parseInt(selected, 10);
            targetRow = ringSizeData.find((r) => r.jp === num);
          }
        }

        if (!targetRow) {
          errorMsg.textContent =
            "No matching size found in the reference table.";
          errorMsg.classList.remove("hidden");
          return;
        }

        updateResultsFromRow(targetRow);
        const index = ringSizeData.indexOf(targetRow);
        if (index >= 0) highlightRow(index);
      }

      // Initialize
      inputModeEl.addEventListener("change", onModeChange);
      showHalfSizesEl.addEventListener("change", renderTable);
      convertBtn.addEventListener("click", convert);

      // Default state
      populateSizeSelect();
      renderTable();
      // Preselect a common size (US 7)
      const defaultRow = ringSizeData.find((r) => r.us === 7);
      if (defaultRow) {
        updateResultsFromRow(defaultRow);
        const idx = ringSizeData.indexOf(defaultRow);
        highlightRow(idx);
        sizeSelect.value = defaultRow.us;
      }
    </script>

