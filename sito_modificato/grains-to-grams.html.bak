  <script>
    // Exact constants
    const G_PER_GRAIN = 0.06479891; // g per grain (exact)
    const MG_PER_GRAIN = 64.79891;  // mg per grain (exact)

    // Elements
    const grInput = document.getElementById('gr-input');
    const gOut    = document.getElementById('g-out');
    const mgOut   = document.getElementById('mg-out');

    const gInput  = document.getElementById('g-input');
    const grOut   = document.getElementById('gr-out');

    const roundingSel = document.getElementById('rounding');
    const presetBtns  = document.querySelectorAll('.pill');
    const tblBody     = document.getElementById('tbl-body');

    let isUpdating = false;

    function getDp(){ const v = parseInt(roundingSel.value,10); return isNaN(v)?0:v; }
    function roundTo(x, dp){ if(!isFinite(x) || dp===0) return x; const f = Math.pow(10,dp); return Math.round(x*f)/f; }
    function fmt(x, d){ if(!isFinite(x)) return '—'; return x.toLocaleString(undefined, {maximumFractionDigits:d}); }

    function updateFromGr(){
      if(isUpdating) return; isUpdating = true;
      const gr = parseFloat(grInput.value || 0);
      const g  = gr * G_PER_GRAIN;
      const mg = gr * MG_PER_GRAIN;

      const dp = getDp();
      gOut.textContent  = fmt(roundTo(g, dp), dp);
      mgOut.textContent = fmt(roundTo(mg, dp), dp);

      // reflect other side
      gInput.value = roundTo(g, dp);
      grOut.textContent = fmt(roundTo(gr, dp), dp);

      isUpdating = false;
    }

    function updateFromG(){
      if(isUpdating) return; isUpdating = true;
      const g  = parseFloat(gInput.value || 0);
      const gr = g / G_PER_GRAIN;

      const dp = getDp();
      grOut.textContent = fmt(roundTo(gr, dp), dp);

      // reflect other side
      grInput.value = roundTo(gr, dp);
      gOut.textContent  = fmt(roundTo(g, dp), dp);
      mgOut.textContent = fmt(roundTo(g * 1000, dp), dp);

      isUpdating = false;
    }

    grInput.addEventListener('input', updateFromGr);
    gInput.addEventListener('input', updateFromG);
    roundingSel.addEventListener('change', ()=>{
      if (document.activeElement === gInput) updateFromG(); else updateFromGr();
      buildTable();
    });

    // Presets
    presetBtns.forEach(btn=>{
      btn.addEventListener('click', ()=>{
        presetBtns.forEach(b=>b.classList.remove('pill-active'));
        btn.classList.add('pill-active');
        const mode = btn.dataset.mode, val = btn.dataset.val;
        if(mode === 'gr'){ grInput.value = val; updateFromGr(); }
        else { gInput.value = val; updateFromG(); }
      });
    });

    // Build quick table
    function buildTable(){
      const dp = getDp();
      const rows = [1,5,10,20,50,100,250,500,1000];
      tblBody.innerHTML = '';
      rows.forEach(gr=>{
        const g = gr * G_PER_GRAIN;
        const mg = gr * MG_PER_GRAIN;
        const tr = document.createElement('tr');
        tr.innerHTML = `
          <td class="py-1 px-2">${fmt(gr,0)}</td>
          <td class="py-1 px-2">${fmt(roundTo(g, dp), dp)} g</td>
          <td class="py-1 px-2">${fmt(roundTo(mg, dp), dp)} mg</td>
        `;
        tblBody.appendChild(tr);
      });
    }

    // Init
    buildTable();
    updateFromGr();
  </script>

