    <script>
      // Utility functions
      function sizeToBytes(value, unit) {
        const v = Number(value);
        if (!isFinite(v) || v < 0) return 0;
        const map = { KB: 1e3, MB: 1e6, GB: 1e9, TB: 1e12 };
        return v * (map[unit] || 1);
      }

      function speedToBps(value, unit) {
        const v = Number(value);
        if (!isFinite(v) || v <= 0) return 0;
        switch (unit) {
          case "Kbps":
            return v * 1e3;
          case "Mbps":
            return v * 1e6;
          case "Gbps":
            return v * 1e9;
          case "MBps":
            return v * 8e6; // 1 MB/s = 8 Mbps
          default:
            return v;
        }
      }

      function formatBytes(bytes) {
        if (!isFinite(bytes) || bytes <= 0) return "0 B";
        const units = ["B", "KB", "MB", "GB", "TB"];
        let i = 0;
        let v = bytes;
        while (v >= 1000 && i < units.length - 1) {
          v / 1000;
          i++;
        }
        return v.toFixed(v < 10 ? 2 : v < 100 ? 1 : 0) + " " + units[i];
      }

      function formatBps(bps) {
        if (!isFinite(bps) || bps <= 0) return "—";
        const units = ["bps", "Kbps", "Mbps", "Gbps"];
        let i = 0;
        let v = bps;
        while (v >= 1000 && i < units.length - 1) {
          v / 1000;
          i++;
        }
        return v.toFixed(v < 10 ? 2 : v < 100 ? 1 : 0) + " " + units[i];
      }

      function formatDuration(seconds) {
        if (!isFinite(seconds) || seconds <= 0) return "0 seconds";
        const s = Math.round(seconds);
        const days = Math.floor(s / 86400);
        const hours = Math.floor((s % 86400) / 3600);
        const mins = Math.floor((s % 3600) / 60);
        const secs = s % 60;

        const parts = [];
        if (days) parts.push(days + " day" + (days !== 1 ? "s" : ""));
        if (hours) parts.push(hours + " hour" + (hours !== 1 ? "s" : ""));
        if (mins) parts.push(mins + " min");
        if (!days && !hours && secs) parts.push(secs + " s");

        return parts.join(" ");
      }

      function formatShortDuration(seconds) {
        if (!isFinite(seconds) || seconds <= 0) return "0 s";
        if (seconds < 60) return seconds.toFixed(1) + " s";
        if (seconds < 3600) return (seconds / 60).toFixed(1) + " min";
        if (seconds < 86400) return (seconds / 3600).toFixed(2) + " h";
        return (seconds / 86400).toFixed(2) + " days";
      }

      // DOM elements
      const modeDownloadBtn = document.getElementById("mode-download");
      const modeUploadBtn = document.getElementById("mode-upload");
      const fileSizeInput = document.getElementById("file-size");
      const fileUnitSelect = document.getElementById("file-unit");
      const speedInput = document.getElementById("speed");
      const speedUnitSelect = document.getElementById("speed-unit");
      const overheadSelect = document.getElementById("overhead");
      const efficiencySelect = document.getElementById("efficiency");
      const calcBtn = document.getElementById("calculate-btn");
      const swapBtn = document.getElementById("swap-btn");

      const resultMain = document.getElementById("result-main");
      const resultSub = document.getElementById("result-sub");
      const breakdownSize = document.getElementById("breakdown-size");
      const breakdownSpeed = document.getElementById("breakdown-speed");
      const breakdownRaw = document.getElementById("breakdown-raw");
      const breakdownAdjusted = document.getElementById("breakdown-adjusted");

      const speedPresetsToggle = document.getElementById(
        "speed-presets-toggle"
      );
      const speedPresets = document.getElementById("speed-presets");

      const multiPanel = document.getElementById("multi-files-panel");
      const addFileBtn = document.getElementById("add-file-btn");
      const addFileRowBtn = document.getElementById("add-file-row");
      const closeMultiFilesBtn = document.getElementById("close-multi-files");
      const multiFilesList = document.getElementById("multi-files-list");
      const multiFilesTotal = document.getElementById("multi-files-total");

      let currentMode = "download"; // or 'upload'
      let lastComputedSeconds = null;
      let lastComputedSizeBytes = null;

      // Mode toggle
      function setMode(mode) {
        currentMode = mode;
        if (mode === "download") {
          modeDownloadBtn.classList.add(
            "bg-blue-600",
            "text-white",
            "border-blue-600"
          );
          modeDownloadBtn.classList.remove(
            "bg-white",
            "text-gray-700",
            "border-gray-300"
          );
          modeUploadBtn.classList.remove(
            "bg-blue-600",
            "text-white",
            "border-blue-600"
          );
          modeUploadBtn.classList.add(
            "bg-white",
            "text-gray-700",
            "border-gray-300"
          );
          resultSub.textContent =
            "Estimated download time based on your connection speed.";
        } else {
          modeUploadBtn.classList.add(
            "bg-blue-600",
            "text-white",
            "border-blue-600"
          );
          modeUploadBtn.classList.remove(
            "bg-white",
            "text-gray-700",
            "border-gray-300"
          );
          modeDownloadBtn.classList.remove(
            "bg-blue-600",
            "text-white",
            "border-blue-600"
          );
          modeDownloadBtn.classList.add(
            "bg-white",
            "text-gray-700",
            "border-gray-300"
          );
          resultSub.textContent =
            "Estimated upload time based on your connection speed.";
        }
      }

      modeDownloadBtn.addEventListener("click", () => setMode("download"));
      modeUploadBtn.addEventListener("click", () => setMode("upload"));

      // Speed presets
      speedPresetsToggle.addEventListener("click", () => {
        speedPresets.classList.toggle("hidden");
      });

      speedPresets.addEventListener("click", (e) => {
        const btn = e.target.closest("button[data-speed]");
        if (!btn) return;
        speedInput.value = btn.getAttribute("data-speed");
        speedUnitSelect.value = btn.getAttribute("data-unit");
      });

      // Multiple files handling
      function createFileRow() {
        const row = document.createElement("div");
        row.className = "flex gap-2 items-center";
        row.innerHTML = `
      <input type="number" min="0" step="any" class="flex-1 px-2 py-1 border border-gray-300 rounded-md text-xs" placeholder="Size">
      <select class="w-20 px-1 py-1 border border-gray-300 rounded-md text-xs">
        <option value="KB">KB</option>
        <option value="MB" selected>MB</option>
        <option value="GB">GB</option>
        <option value="TB">TB</option>
      </select>
      <button type="button" class="text-xs text-red-500 hover:text-red-700">Remove</button>
    `;
        return row;
      }

      function updateMultiTotal() {
        let totalBytes = 0;
        const rows = multiFilesList.querySelectorAll("div.flex");
        rows.forEach((row) => {
          const input = row.querySelector("input");
          const select = row.querySelector("select");
          const val = parseFloat(input.value);
          if (isFinite(val) && val > 0) {
            totalBytes += sizeToBytes(val, select.value);
          }
        });
        multiFilesTotal.textContent = formatBytes(totalBytes);
        if (totalBytes > 0) {
          // sync to main input using current unit
          const unit = fileUnitSelect.value;
          let divisor = 1;
          if (unit === "KB") divisor = 1e3;
          else if (unit === "MB") divisor = 1e6;
          else if (unit === "GB") divisor = 1e9;
          else if (unit === "TB") divisor = 1e12;
          fileSizeInput.value = (totalBytes / divisor)
            .toFixed(3)
            .replace(/\.?0+$/, "");
        }
      }

      addFileBtn.addEventListener("click", () => {
        multiPanel.classList.remove("hidden");
        if (!multiFilesList.children.length) {
          multiFilesList.appendChild(createFileRow());
        }
      });

      addFileRowBtn.addEventListener("click", () => {
        multiFilesList.appendChild(createFileRow());
      });

      closeMultiFilesBtn.addEventListener("click", () => {
        multiPanel.classList.add("hidden");
      });

      multiFilesList.addEventListener("input", updateMultiTotal);
      multiFilesList.addEventListener("click", (e) => {
        if (e.target.tagName === "BUTTON") {
          const row = e.target.closest("div.flex");
          if (row) {
            row.remove();
            updateMultiTotal();
          }
        }
      });

      // Core calculation
      function calculateTime() {
        const sizeVal = parseFloat(fileSizeInput.value);
        const speedVal = parseFloat(speedInput.value);
        if (
          !isFinite(sizeVal) ||
          sizeVal <= 0 ||
          !isFinite(speedVal) ||
          speedVal <= 0
        ) {
          resultMain.textContent = "—";
          resultSub.textContent =
            "Please enter a positive file size and speed.";
          breakdownSize.textContent = "—";
          breakdownSpeed.textContent = "—";
          breakdownRaw.textContent = "—";
          breakdownAdjusted.textContent = "—";
          lastComputedSeconds = null;
          lastComputedSizeBytes = null;
          return;
        }

        const sizeBytes = sizeToBytes(sizeVal, fileUnitSelect.value);
        const sizeBits = sizeBytes * 8;
        const speedBps = speedToBps(speedVal, speedUnitSelect.value);

        if (speedBps <= 0) {
          resultMain.textContent = "—";
          resultSub.textContent = "Speed must be greater than zero.";
          return;
        }

        const rawSeconds = sizeBits / speedBps;

        const overhead = parseFloat(overheadSelect.value) / 100;
        const efficiency = parseFloat(efficiencySelect.value);
        const effectiveSpeed = speedBps * (1 - overhead) * efficiency;
        const adjustedSeconds = sizeBits / effectiveSpeed;

        lastComputedSeconds = adjustedSeconds;
        lastComputedSizeBytes = sizeBytes;

        resultMain.textContent = formatDuration(adjustedSeconds);
        resultSub.textContent =
          (currentMode === "download"
            ? "Estimated download time: "
            : "Estimated upload time: ") +
          formatShortDuration(adjustedSeconds) +
          " (with overhead & efficiency).";

        breakdownSize.textContent = formatBytes(sizeBytes);
        breakdownSpeed.textContent = formatBps(effectiveSpeed) + " (effective)";
        breakdownRaw.textContent = formatDuration(rawSeconds);
        breakdownAdjusted.textContent = formatDuration(adjustedSeconds);
      }

      calcBtn.addEventListener("click", calculateTime);

      // Auto-calc on input changes (debounced)
      let calcTimeout = null;
      ["input", "change"].forEach((evt) => {
        document.addEventListener(evt, (e) => {
          if (
            ![
              "file-size",
              "file-unit",
              "speed",
              "speed-unit",
              "overhead",
              "efficiency",
            ].includes(e.target.id)
          )
            return;
          clearTimeout(calcTimeout);
          calcTimeout = setTimeout(calculateTime, 300);
        });
      });

      // Swap: use result time to compute required speed
      swapBtn.addEventListener("click", () => {
        if (!lastComputedSeconds || !lastComputedSizeBytes) {
          calculateTime();
          if (!lastComputedSeconds) return;
        }
        const targetSeconds = lastComputedSeconds;
        const sizeBits = lastComputedSizeBytes * 8;

        const overhead = parseFloat(overheadSelect.value) / 100;
        const efficiency = parseFloat(efficiencySelect.value);
        const effectiveSpeedNeeded = sizeBits / targetSeconds;
        const rawSpeedNeeded =
          effectiveSpeedNeeded / ((1 - overhead) * efficiency);

        // Express in Mbps by default
        const speedMbps = rawSpeedNeeded / 1e6;
        speedUnitSelect.value = "Mbps";
        speedInput.value = speedMbps.toFixed(speedMbps < 10 ? 2 : 1);

        resultSub.textContent =
          "Required " +
          (currentMode === "download" ? "download" : "upload") +
          " speed to hit this time: " +
          speedMbps.toFixed(1) +
          " Mbps (approx).";
      });

      // Initial mode
      setMode("download");
    </script>

