    <script>
      function clamp(value, min, max) {
        return Math.min(Math.max(value, min), max);
      }

      function estimateWCRatio(fck, exposure) {
        // Base on strength: simple inverse relation
        let wcStrength = clamp(0.9 - fck / 100, 0.3, 0.65);

        // Durability caps
        let wcCap;
        if (exposure === "severe") wcCap = 0.45;
        else if (exposure === "moderate") wcCap = 0.5;
        else wcCap = 0.55;

        return Math.min(wcStrength, wcCap);
      }

      function estimateWaterContent(slump, aggSize) {
        // Base water for 75 mm slump, 20 mm agg
        let base = 185;
        // Slump adjustment: +3 kg per 25 mm above 75, -3 per 25 below
        let slumpDelta = ((slump - 75) / 25) * 3;
        // Aggregate size adjustment: -5 kg for 25 mm, -10 for 40 mm, +5 for 10 mm
        let aggAdj = 0;
        if (aggSize == 10) aggAdj = 5;
        else if (aggSize == 25) aggAdj = -5;
        else if (aggSize == 40) aggAdj = -10;

        let water = base + slumpDelta + aggAdj;
        return clamp(water, 140, 220);
      }

      function estimateStrengthFromWC(wc) {
        // Simple inverse relation: fck ≈ 80 / (wc + 0.25)
        return 80 / (wc + 0.25);
      }

      document
        .getElementById("calculateBtn")
        .addEventListener("click", function () {
          const exposure = document.getElementById("exposureClass").value;
          const fck = parseFloat(document.getElementById("fck").value) || 30;
          const slump =
            parseFloat(document.getElementById("slump").value) || 75;
          const aggSize =
            parseFloat(document.getElementById("aggSize").value) || 20;

          const sgCement =
            parseFloat(document.getElementById("cementDensity").value) || 3.15;
          const sgFA =
            parseFloat(document.getElementById("faDensity").value) || 2.65;
          const sgCA =
            parseFloat(document.getElementById("caDensity").value) || 2.7;
          const airPct =
            parseFloat(document.getElementById("airContent").value) || 2;

          const faMoist =
            parseFloat(document.getElementById("faMoisture").value) || 2;
          const caMoist =
            parseFloat(document.getElementById("caMoisture").value) || 1;
          const admPct =
            parseFloat(document.getElementById("admixtureDosage").value) || 0.8;
          const batchVol =
            parseFloat(document.getElementById("batchVolume").value) || 1;

          // 1. w/c ratio
          const wc = estimateWCRatio(fck, exposure);
          document.getElementById("wcRatioOut").textContent = wc.toFixed(2);

          // 2. Water content (kg/m3) before admixture reduction
          let water = estimateWaterContent(slump, aggSize);

          // Water reduction from admixture: assume 10% per 1% dosage (simplified)
          const waterReductionFactor = 1 - clamp(admPct * 0.1, 0, 0.25);
          const waterEff = water * waterReductionFactor;

          // 3. Cement content
          const cement = waterEff / wc;

          // 4. Admixture mass (kg/m3)
          const admMass = cement * (admPct / 100);

          // 5. Volumes (m3)
          const vWater = waterEff / 1000; // density ~1000 kg/m3
          const vCement = cement / (sgCement * 1000);
          const vAdmixture = admMass / (1.1 * 1000); // assume SG ~1.1
          const vAir = airPct / 100;

          const vAggTotal = 1 - (vWater + vCement + vAdmixture + vAir);

          // Fine/coarse split based on agg size
          let faFrac;
          if (aggSize <= 10) faFrac = 0.45;
          else if (aggSize <= 20) faFrac = 0.4;
          else faFrac = 0.35;

          const vFA = vAggTotal * faFrac;
          const vCA = vAggTotal * (1 - faFrac);

          const faMass = vFA * sgFA * 1000;
          const caMass = vCA * sgCA * 1000;

          // 6. Moisture water from aggregates
          const waterFromFA = faMass * (faMoist / 100);
          const waterFromCA = caMass * (caMoist / 100);
          const waterFromAgg = waterFromFA + waterFromCA;

          const batchWaterToAdd = Math.max(waterEff - waterFromAgg, 0);

          // 7. Estimated strength from final w/c
          const estStrength = estimateStrengthFromWC(wc);

          // 8. Density estimate
          const totalMass = waterEff + cement + faMass + caMass + admMass;
          const density = totalMass / 1.0; // per m3

          // Output key parameters
          document.getElementById("waterPerM3Out").textContent =
            waterEff.toFixed(1);
          document.getElementById("cementPerM3Out").textContent =
            cement.toFixed(1);
          document.getElementById("strengthOut").textContent =
            estStrength.toFixed(1);
          document.getElementById("densityOut").textContent =
            density.toFixed(0) + " kg/m³";

          // Slump class
          let slumpClass = "Low";
          if (slump >= 50 && slump <= 100) slumpClass = "Medium";
          else if (slump > 100) slumpClass = "High";
          document.getElementById("slumpClassOut").textContent = slumpClass;

          // Quality checks
          let durMsg;
          if (exposure === "severe" && wc <= 0.45)
            durMsg = "OK for severe exposure (w/c ≤ 0.45)";
          else if (exposure === "moderate" && wc <= 0.5)
            durMsg = "OK for moderate exposure (w/c ≤ 0.50)";
          else if (exposure === "normal" && wc <= 0.55)
            durMsg = "OK for mild exposure (w/c ≤ 0.55)";
          else
            durMsg = "Check durability – w/c may be too high for this exposure";

          document.getElementById("durabilityCheck").textContent = durMsg;

          let workMsg;
          if (slump < 50)
            workMsg = "Low workability – may be difficult to place";
          else if (slump <= 100)
            workMsg = "Typical workability for structural concrete";
          else workMsg = "High workability – consider segregation risk";
          document.getElementById("workabilityCheck").textContent = workMsg;

          const margin = estStrength - fck;
          let marginMsg =
            margin >= 3
              ? `Estimated strength is about ${margin.toFixed(
                  1
                )} MPa above target`
              : `Estimated strength is close to target (margin ≈ ${margin.toFixed(
                  1
                )} MPa)`;
          document.getElementById("strengthMargin").textContent = marginMsg;

          // Mix table outputs per m3
          document.getElementById("waterPerM3Cell").textContent =
            waterEff.toFixed(1);
          document.getElementById("cementPerM3Cell").textContent =
            cement.toFixed(1);
          document.getElementById("faPerM3Cell").textContent =
            faMass.toFixed(1);
          document.getElementById("caPerM3Cell").textContent =
            caMass.toFixed(1);
          document.getElementById("admPerM3Cell").textContent =
            admMass.toFixed(2);

          // Per batch (kg)
          document.getElementById("waterPerBatchCell").textContent = (
            (batchWaterToAdd * batchVol) /
            1.0
          ).toFixed(1);
          document.getElementById("cementPerBatchCell").textContent = (
            cement * batchVol
          ).toFixed(1);
          document.getElementById("faPerBatchCell").textContent = (
            faMass * batchVol
          ).toFixed(1);
          document.getElementById("caPerBatchCell").textContent = (
            caMass * batchVol
          ).toFixed(1);
          document.getElementById("admPerBatchCell").textContent = (
            admMass * batchVol
          ).toFixed(2);
        });

      // Initialize on load
      window.addEventListener("load", () => {
        document.getElementById("calculateBtn").click();
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
