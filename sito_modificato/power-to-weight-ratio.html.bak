    <script>
      // Tab switching
      const modeTabs = document.querySelectorAll(".mode-tab");
      const cyclingPanel = document.getElementById("cycling-panel");
      const vehiclePanel = document.getElementById("vehicle-panel");

      modeTabs.forEach((tab) => {
        tab.addEventListener("click", () => {
          modeTabs.forEach((t) => {
            t.classList.remove("border-blue-600", "text-blue-600");
            t.classList.add("border-transparent", "text-gray-600");
          });
          tab.classList.add("border-blue-600", "text-blue-600");
          tab.classList.remove("text-gray-600");

          const mode = tab.getAttribute("data-mode");
          if (mode === "cycling") {
            cyclingPanel.classList.remove("hidden");
            vehiclePanel.classList.add("hidden");
          } else {
            vehiclePanel.classList.remove("hidden");
            cyclingPanel.classList.add("hidden");
          }
        });
      });

      // Utility functions
      function round(value, decimals) {
        const factor = Math.pow(10, decimals);
        return Math.round(value * factor) / factor;
      }

      // CYCLING CALCULATOR
      const cyclingPowerInput = document.getElementById("cycling-power");
      const cyclingWeightInput = document.getElementById("cycling-weight");
      const cyclingWeightUnit = document.getElementById("cycling-weight-unit");
      const cyclingCalcBtn = document.getElementById("cycling-calc-btn");
      const cyclingResetBtn = document.getElementById("cycling-reset-btn");
      const cyclingResults = document.getElementById("cycling-results");
      const cyclingWkgSpan = document.getElementById("cycling-wkg");
      const cyclingClassSpan = document.getElementById("cycling-class");
      const cyclingClassBadge = document.getElementById("cycling-class-badge");
      const cycling20minSpan = document.getElementById("cycling-20min");
      const cyclingClimbNote = document.getElementById("cycling-climb-note");

      function classifyCyclingWkg(wkg) {
        if (wkg < 1.5)
          return { label: "Beginner", color: "bg-gray-200 text-gray-800" };
        if (wkg < 2.5)
          return {
            label: "Recreational",
            color: "bg-green-100 text-green-800",
          };
        if (wkg < 3.5)
          return {
            label: "Strong club rider",
            color: "bg-blue-100 text-blue-800",
          };
        if (wkg < 4.5)
          return {
            label: "Competitive amateur",
            color: "bg-purple-100 text-purple-800",
          };
        if (wkg < 5.5)
          return {
            label: "Elite amateur",
            color: "bg-orange-100 text-orange-800",
          };
        return {
          label: "Professional level",
          color: "bg-red-100 text-red-800",
        };
      }

      function cyclingClimbInsight(wkg) {
        if (wkg < 2)
          return "Focus on base endurance and easy weight loss; climbs will feel challenging but will improve quickly with consistency.";
        if (wkg < 3)
          return "You can ride most local climbs steadily; structured intervals will help you move into strong club territory.";
        if (wkg < 4)
          return "You should be competitive on group ride climbs and shorter hill segments.";
        if (wkg < 5)
          return "You are in serious amateur territory; long mountain passes at tempo should be manageable.";
        if (wkg < 5.5)
          return "You are close to elite level; you can climb strongly in most amateur races.";
        return "This is world‑class climbing ability; you are comparable to professional climbers on long ascents.";
      }

      cyclingCalcBtn.addEventListener("click", () => {
        const power = parseFloat(cyclingPowerInput.value);
        const weight = parseFloat(cyclingWeightInput.value);
        const unit = cyclingWeightUnit.value;

        if (!power || power <= 0 || !weight || weight <= 0) {
          alert("Please enter positive values for both power and weight.");
          return;
        }

        let weightKg = weight;
        if (unit === "lb") {
          weightKg = weight * 0.45359237;
        }

        const wkg = power / weightKg;
        const wkgRounded = round(wkg, 2);
        cyclingWkgSpan.textContent = wkgRounded.toFixed(2);

        const classification = classifyCyclingWkg(wkg);
        cyclingClassSpan.textContent = classification.label;
        cyclingClassBadge.textContent = "Cycling";
        cyclingClassBadge.className = "badge ml-2 " + classification.color;

        const power20min = power * 1.05; // simple approximation
        cycling20minSpan.textContent = Math.round(power20min);

        cyclingClimbNote.textContent = cyclingClimbInsight(wkg);

        cyclingResults.classList.remove("hidden");
      });

      cyclingResetBtn.addEventListener("click", () => {
        cyclingPowerInput.value = "";
        cyclingWeightInput.value = "";
        cyclingResults.classList.add("hidden");
      });

      // VEHICLE CALCULATOR
      const vehiclePowerInput = document.getElementById("vehicle-power");
      const vehiclePowerUnit = document.getElementById("vehicle-power-unit");
      const vehicleWeightInput = document.getElementById("vehicle-weight");
      const vehicleWeightUnit = document.getElementById("vehicle-weight-unit");
      const vehicleCalcBtn = document.getElementById("vehicle-calc-btn");
      const vehicleResetBtn = document.getElementById("vehicle-reset-btn");
      const vehicleResults = document.getElementById("vehicle-results");
      const vehicleHpPerTonSpan = document.getElementById("vehicle-hp-per-ton");
      const vehicleHpPerKgSpan = document.getElementById("vehicle-hp-per-kg");
      const vehicleWPerKgSpan = document.getElementById("vehicle-w-per-kg");
      const vehicleClassSpan = document.getElementById("vehicle-class");
      const vehicleClassBadge = document.getElementById("vehicle-class-badge");
      const vehicleNote = document.getElementById("vehicle-note");

      function classifyVehicle(hpPerTon) {
        if (hpPerTon < 80)
          return {
            label: "Underpowered",
            note: "Suitable for city driving; acceleration will feel modest.",
            color: "bg-gray-200 text-gray-800",
          };
        if (hpPerTon < 120)
          return {
            label: "Average",
            note: "Typical modern family car performance.",
            color: "bg-green-100 text-green-800",
          };
        if (hpPerTon < 180)
          return {
            label: "Brisk",
            note: "Good overtaking and highway acceleration.",
            color: "bg-blue-100 text-blue-800",
          };
        if (hpPerTon < 250)
          return {
            label: "Sporty",
            note: "Hot hatch or sports sedan territory.",
            color: "bg-purple-100 text-purple-800",
          };
        if (hpPerTon < 350)
          return {
            label: "High performance",
            note: "Comparable to many sports cars.",
            color: "bg-orange-100 text-orange-800",
          };
        return {
          label: "Supercar level",
          note: "Extremely rapid acceleration; traction becomes the limiting factor.",
          color: "bg-red-100 text-red-800",
        };
      }

      vehicleCalcBtn.addEventListener("click", () => {
        const powerVal = parseFloat(vehiclePowerInput.value);
        const weightVal = parseFloat(vehicleWeightInput.value);
        const pUnit = vehiclePowerUnit.value;
        const wUnit = vehicleWeightUnit.value;

        if (!powerVal || powerVal <= 0 || !weightVal || weightVal <= 0) {
          alert("Please enter positive values for both power and weight.");
          return;
        }

        // Convert power to hp and W
        let powerHp, powerW;
        if (pUnit === "hp") {
          powerHp = powerVal;
          powerW = powerVal * 745.699872;
        } else if (pUnit === "kW") {
          powerHp = powerVal * 1.34102209;
          powerW = powerVal * 1000;
        } else {
          powerW = powerVal;
          powerHp = powerVal / 745.699872;
        }

        // Convert weight to kg
        let weightKg = weightVal;
        if (wUnit === "lb") {
          weightKg = weightVal * 0.45359237;
        }

        const hpPerKg = powerHp / weightKg;
        const wPerKg = powerW / weightKg;
        const hpPerTon = powerHp / (weightKg / 1000);

        vehicleHpPerKgSpan.textContent = round(hpPerKg, 4).toFixed(4);
        vehicleWPerKgSpan.textContent = round(wPerKg, 1).toFixed(1);
        vehicleHpPerTonSpan.textContent = round(hpPerTon, 1).toFixed(1);

        const classification = classifyVehicle(hpPerTon);
        vehicleClassSpan.textContent = classification.label;
        vehicleClassBadge.textContent = "Vehicle";
        vehicleClassBadge.className = "badge ml-2 " + classification.color;
        vehicleNote.textContent = classification.note;

        vehicleResults.classList.remove("hidden");
      });

      vehicleResetBtn.addEventListener("click", () => {
        vehiclePowerInput.value = "";
        vehicleWeightInput.value = "";
        vehicleResults.classList.add("hidden");
      });

      // Mobile menu accessibility
      const mobileToggle = document.getElementById("mobile-menu-toggle");
      const mobileMenu = document.getElementById("mobile-menu");
      if (mobileToggle && mobileMenu) {
        mobileToggle.addEventListener("click", () => {
          const expanded =
            mobileToggle.getAttribute("aria-expanded") === "true";
          mobileToggle.setAttribute("aria-expanded", String(!expanded));
          mobileMenu.classList.toggle("hidden");
        });
      }

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
