    <script>
      // Basic utility
      function deg2rad(d) {
        return (d * Math.PI) / 180;
      }

      function calculateSlopeStability() {
        const H = parseFloat(document.getElementById("slopeHeight").value);
        const betaDeg = parseFloat(document.getElementById("slopeAngle").value);
        const gamma = parseFloat(document.getElementById("unitWeight").value);
        const nSlices = parseInt(
          document.getElementById("numSlices").value,
          10
        );

        const c = parseFloat(document.getElementById("cohesion").value);
        const phiDeg = parseFloat(document.getElementById("phi").value);
        const ru = parseFloat(
          document.getElementById("porePressureRatio").value
        );
        const kh = parseFloat(document.getElementById("seismicCoeff").value);

        const R = parseFloat(document.getElementById("circleRadius").value);
        const xc = parseFloat(document.getElementById("circleCenterX").value);
        const yc = parseFloat(document.getElementById("circleCenterY").value);

        const errorEl = document.getElementById("calcError");
        errorEl.textContent = "";

        if ([H, betaDeg, gamma, nSlices, c, phiDeg, R].some((v) => isNaN(v))) {
          errorEl.textContent = "Please fill in all required numeric inputs.";
          return null;
        }
        if (H <= 0 || R <= 0 || gamma <= 0 || nSlices < 3) {
          errorEl.textContent =
            "Height, radius, unit weight must be positive and slices ≥ 3.";
          return null;
        }

        const beta = deg2rad(betaDeg);
        const phi = deg2rad(phiDeg);

        // Define slope surface line: toe at (0,0), surface up along angle beta
        // We'll intersect the circle with the slope surface to find entry/exit points.
        // Parametric slope surface: x = s cosβ, y = s sinβ, s >= 0
        // Circle: (x - xc)^2 + (y - yc)^2 = R^2
        // Solve for s: quadratic a s^2 + b s + c0 = 0
        const cosb = Math.cos(beta),
          sinb = Math.sin(beta);
        const a = 1; // cos^2 + sin^2
        const bq = -2 * (xc * cosb + yc * sinb);
        const c0 = xc * xc + yc * yc - R * R;
        const disc = bq * bq - 4 * a * c0;
        if (disc <= 0) {
          errorEl.textContent =
            "The specified circle does not intersect the slope surface. Adjust R or center.";
          return null;
        }
        const s1 = (-bq - Math.sqrt(disc)) / (2 * a);
        const s2 = (-bq + Math.sqrt(disc)) / (2 * a);
        const sEnter = Math.min(s1, s2);
        const sExit = Math.max(s1, s2);
        if (sExit <= 0) {
          errorEl.textContent =
            "The slip surface intersection lies outside the slope extent. Adjust geometry.";
          return null;
        }

        // Coordinates of entry and exit points on slope surface
        const x1 = sEnter * cosb;
        const y1 = sEnter * sinb;
        const x2 = sExit * cosb;
        const y2 = sExit * sinb;

        // Discretize along arc between these two points
        // Compute corresponding angles on circle
        function angleOfPoint(x, y) {
          return Math.atan2(y - yc, x - xc);
        }
        let theta1 = angleOfPoint(x1, y1);
        let theta2 = angleOfPoint(x2, y2);

        // Ensure we move along the arc under the slope (assume smaller absolute difference)
        if (theta2 < theta1) {
          const tmp = theta1;
          theta1 = theta2;
          theta2 = tmp;
        }

        const dtheta = (theta2 - theta1) / nSlices;

        let sumResist = 0;
        let sumDrive = 0;

        for (let i = 0; i < nSlices; i++) {
          const thLeft = theta1 + i * dtheta;
          const thRight = theta1 + (i + 1) * dtheta;
          const thMid = 0.5 * (thLeft + thRight);

          // Base endpoints on circle
          const xLeft = xc + R * Math.cos(thLeft);
          const yLeft = yc + R * Math.sin(thLeft);
          const xRight = xc + R * Math.cos(thRight);
          const yRight = yc + R * Math.sin(thRight);

          const baseLength = Math.hypot(xRight - xLeft, yRight - yLeft);
          const alpha = Math.atan2(yRight - yLeft, xRight - xLeft); // base inclination

          // Approximate slice width as horizontal projection
          const bHoriz = Math.abs(xRight - xLeft);

          // Top of slice: intersection with slope surface line
          // For each x, y_surface = x tanβ (since line passes through origin)
          const yTopLeft = xLeft * Math.tan(beta);
          const yTopRight = xRight * Math.tan(beta);

          // Approximate slice area as trapezoid between top and base
          const hLeft = yTopLeft - yLeft;
          const hRight = yTopRight - yRight;
          const avgHeight = Math.max(0, 0.5 * (hLeft + hRight));
          const area = avgHeight * bHoriz;

          const W = gamma * area; // kN per unit thickness

          // Pore pressure approximation using ru: u ≈ ru * γ * H
          const u = ru * gamma * H;
          const U = u * baseLength; // resultant pore pressure force normal to base (simplified)

          // Normal and shear components of weight on base
          const N = W * Math.cos(alpha) - U; // effective normal force
          const T = W * Math.sin(alpha) + kh * W * Math.cos(alpha); // driving shear incl. pseudo-static

          const sigmaEff = N / baseLength; // kPa (per unit thickness)
          const tauf = c + sigmaEff * Math.tan(phi); // shear strength
          const R_i = tauf * baseLength;
          const S_i = T;

          sumResist += R_i;
          sumDrive += S_i;
        }

        if (sumDrive <= 0) {
          errorEl.textContent =
            "Driving forces are zero or negative; check geometry and inputs.";
          return null;
        }

        const F = sumResist / sumDrive;
        return {
          F,
          geometry: { x1, y1, x2, y2, xc, yc, R, theta1, theta2 },
          slices: nSlices,
        };
      }

      function updateResults() {
        const result = calculateSlopeStability();
        const fosEl = document.getElementById("fosValue");
        const interpEl = document.getElementById("fosInterpretation");

        if (!result) {
          fosEl.textContent = "–";
          interpEl.textContent = "";
          drawSlope(null);
          return;
        }

        const F = result.F;
        fosEl.textContent = F.toFixed(2);

        let msg;
        if (F < 1.0) {
          msg =
            "Predicted unstable: driving forces exceed resisting forces (F < 1.0). Consider flattening the slope, improving drainage, or adding reinforcement.";
        } else if (F < 1.2) {
          msg =
            "Marginal stability: factor of safety is close to 1.0. Small changes in conditions could trigger failure.";
        } else if (F < 1.5) {
          msg =
            "Moderate stability: within the range often used for long-term slopes, depending on codes and risk level.";
        } else {
          msg =
            "High stability: factor of safety is relatively conservative for many static conditions. Always confirm with project-specific criteria.";
        }
        interpEl.textContent = msg;

        drawSlope(result.geometry);
      }

      function drawSlope(geom) {
        const canvas = document.getElementById("slopeCanvas");
        if (!canvas.getContext) return;
        const ctx = canvas.getContext("2d");
        const w = canvas.width;
        const h = canvas.height;

        // Clear
        ctx.clearRect(0, 0, w, h);

        // Axes scaling: we map x in [-H, 2H], y in [-H, H*1.5]
        const H =
          parseFloat(document.getElementById("slopeHeight").value) || 10;
        const xMin = -H,
          xMax = 2 * H;
        const yMin = -H,
          yMax = 1.5 * H;

        function xToPx(x) {
          return ((x - xMin) / (xMax - xMin)) * w;
        }
        function yToPx(y) {
          return h - ((y - yMin) / (yMax - yMin)) * h;
        }

        // Draw ground line (y=0)
        ctx.strokeStyle = "#9ca3af";
        ctx.lineWidth = 1;
        ctx.beginPath();
        ctx.moveTo(xToPx(xMin), yToPx(0));
        ctx.lineTo(xToPx(xMax), yToPx(0));
        ctx.stroke();

        // Draw slope surface
        const betaDeg =
          parseFloat(document.getElementById("slopeAngle").value) || 30;
        const beta = deg2rad(betaDeg);
        const cosb = Math.cos(beta),
          sinb = Math.sin(beta);
        const sMax = 2 * H;
        const xSlopeEnd = sMax * cosb;
        const ySlopeEnd = sMax * sinb;

        ctx.strokeStyle = "#4b5563";
        ctx.lineWidth = 2;
        ctx.beginPath();
        ctx.moveTo(xToPx(0), yToPx(0));
        ctx.lineTo(xToPx(xSlopeEnd), yToPx(ySlopeEnd));
        ctx.stroke();

        if (!geom) return;

        const { xc, yc, R, theta1, theta2 } = geom;

        // Draw slip circle arc
        ctx.strokeStyle = "#2563eb";
        ctx.lineWidth = 2;
        ctx.beginPath();
        const steps = 80;
        for (let i = 0; i <= steps; i++) {
          const th = theta1 + (theta2 - theta1) * (i / steps);
          const x = xc + R * Math.cos(th);
          const y = yc + R * Math.sin(th);
          const px = xToPx(x);
          const py = yToPx(y);
          if (i === 0) ctx.moveTo(px, py);
          else ctx.lineTo(px, py);
        }
        ctx.stroke();

        // Draw center
        ctx.fillStyle = "#2563eb";
        ctx.beginPath();
        ctx.arc(xToPx(xc), yToPx(yc), 3, 0, 2 * Math.PI);
        ctx.fill();

        // Draw slice boundaries along arc
        const nSlices =
          parseInt(document.getElementById("numSlices").value, 10) || 20;
        ctx.strokeStyle = "rgba(37,99,235,0.4)";
        ctx.lineWidth = 1;
        for (let i = 1; i < nSlices; i++) {
          const th = theta1 + (theta2 - theta1) * (i / nSlices);
          const xBase = xc + R * Math.cos(th);
          const yBase = yc + R * Math.sin(th);
          const xTop = xBase;
          const yTop = xBase * Math.tan(beta); // slope surface
          ctx.beginPath();
          ctx.moveTo(xToPx(xBase), yToPx(yBase));
          ctx.lineTo(xToPx(xTop), yToPx(yTop));
          ctx.stroke();
        }
      }

      // Preset slip surfaces
      function applyPreset(type) {
        const H =
          parseFloat(document.getElementById("slopeHeight").value) || 10;
        const betaDeg =
          parseFloat(document.getElementById("slopeAngle").value) || 30;
        const beta = deg2rad(betaDeg);

        let R, xc, yc;
        if (type === "shallow") {
          R = 1.5 * H;
          xc = 0.3 * H;
          yc = -0.5 * H;
        } else if (type === "deep") {
          R = 3.0 * H;
          xc = 0.0;
          yc = -2.0 * H;
        } else if (type === "toe") {
          R = 2.0 * H;
          xc = 0.0;
          yc = -H;
        }

        document.getElementById("circleRadius").value = R.toFixed(2);
        document.getElementById("circleCenterX").value = xc.toFixed(2);
        document.getElementById("circleCenterY").value = yc.toFixed(2);
        updateResults();
      }

      function resetDefaults() {
        document.getElementById("slopeHeight").value = 10;
        document.getElementById("slopeAngle").value = 30;
        document.getElementById("unitWeight").value = 18;
        document.getElementById("numSlices").value = 20;

        document.getElementById("cohesion").value = 10;
        document.getElementById("phi").value = 30;
        document.getElementById("porePressureRatio").value = 0.0;
        document.getElementById("seismicCoeff").value = 0.0;

        document.getElementById("circleRadius").value = 18;
        document.getElementById("circleCenterX").value = 0;
        document.getElementById("circleCenterY").value = -8;

        document.getElementById("calcError").textContent = "";
        updateResults();
      }

      document.addEventListener("DOMContentLoaded", () => {
        document
          .getElementById("calcBtn")
          .addEventListener("click", updateResults);
        document
          .getElementById("resetBtn")
          .addEventListener("click", resetDefaults);

        document
          .getElementById("presetShallow")
          .addEventListener("click", () => applyPreset("shallow"));
        document
          .getElementById("presetDeep")
          .addEventListener("click", () => applyPreset("deep"));
        document
          .getElementById("presetToe")
          .addEventListener("click", () => applyPreset("toe"));

        // Initial draw
        resetDefaults();
      });
