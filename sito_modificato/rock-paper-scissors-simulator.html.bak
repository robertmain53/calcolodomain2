    <script>
      // Mode toggle
      const playSection = document.getElementById("play-section");
      const experimentSection = document.getElementById("experiment-section");
      const modePlayBtn = document.getElementById("mode-play");
      const modeExperimentBtn = document.getElementById("mode-experiment");

      modePlayBtn.addEventListener("click", () => {
        playSection.classList.remove("hidden");
        experimentSection.classList.add("hidden");
        modePlayBtn.classList.add("bg-white", "shadow-sm", "text-blue-600");
        modeExperimentBtn.classList.remove(
          "bg-white",
          "shadow-sm",
          "text-blue-600"
        );
        modeExperimentBtn.classList.add("text-gray-600");
      });

      modeExperimentBtn.addEventListener("click", () => {
        playSection.classList.add("hidden");
        experimentSection.classList.remove("hidden");
        modeExperimentBtn.classList.add(
          "bg-white",
          "shadow-sm",
          "text-blue-600"
        );
        modePlayBtn.classList.remove("bg-white", "shadow-sm", "text-blue-600");
        modePlayBtn.classList.add("text-gray-600");
      });

      // Play mode logic
      const playButtons = document.querySelectorAll(
        "#play-section button[data-move]"
      );
      const playUserMove = document.getElementById("play-user-move");
      const playComputerMove = document.getElementById("play-computer-move");
      const playOutcome = document.getElementById("play-outcome");
      const playWins = document.getElementById("play-wins");
      const playLosses = document.getElementById("play-losses");
      const playDraws = document.getElementById("play-draws");
      const playTotal = document.getElementById("play-total");
      const playStreak = document.getElementById("play-streak");
      const playHistory = document.getElementById("play-history");
      const playReset = document.getElementById("play-reset");

      let playStats = {
        wins: 0,
        losses: 0,
        draws: 0,
        total: 0,
        currentWinStreak: 0,
      };
      let playHistoryData = [];

      function randomMove() {
        const moves = ["rock", "paper", "scissors"];
        const idx = Math.floor(Math.random() * 3);
        return moves[idx];
      }

      function outcomeText(result) {
        if (result === "win") return "You win!";
        if (result === "lose") return "You lose!";
        return "Draw.";
      }

      function compareMoves(user, comp) {
        if (user === comp) return "draw";
        if (
          (user === "rock" && comp === "scissors") ||
          (user === "paper" && comp === "rock") ||
          (user === "scissors" && comp === "paper")
        )
          return "win";
        return "lose";
      }

      function capitalize(str) {
        return str.charAt(0).toUpperCase() + str.slice(1);
      }

      function updatePlayUI() {
        playWins.textContent = playStats.wins;
        playLosses.textContent = playStats.losses;
        playDraws.textContent = playStats.draws;
        playTotal.textContent = playStats.total;
        playStreak.textContent = playStats.currentWinStreak;

        playHistory.innerHTML = "";
        if (playHistoryData.length === 0) {
          const p = document.createElement("p");
          p.className = "text-gray-400";
          p.textContent = "No rounds played yet.";
          playHistory.appendChild(p);
          return;
        }
        playHistoryData
          .slice()
          .reverse()
          .forEach((item, index) => {
            const row = document.createElement("div");
            row.className =
              "flex justify-between border-b border-gray-100 pb-0.5";
            row.innerHTML = `
        <span class="text-gray-500">#${playStats.total - index}</span>
        <span>${capitalize(item.user)} vs ${capitalize(item.comp)}</span>
        <span class="${
          item.result === "win"
            ? "text-green-700"
            : item.result === "lose"
            ? "text-red-700"
            : "text-gray-600"
        } font-medium">
          ${
            item.result === "win"
              ? "Win"
              : item.result === "lose"
              ? "Loss"
              : "Draw"
          }
        </span>
      `;
            playHistory.appendChild(row);
          });
      }

      playButtons.forEach((btn) => {
        btn.addEventListener("click", () => {
          const userMove = btn.getAttribute("data-move");
          const compMove = randomMove();
          const result = compareMoves(userMove, compMove);

          playUserMove.textContent = capitalize(userMove);
          playComputerMove.textContent = capitalize(compMove);
          playOutcome.textContent = outcomeText(result);
          playOutcome.classList.remove(
            "text-gray-700",
            "text-green-700",
            "text-red-700"
          );
          if (result === "win") playOutcome.classList.add("text-green-700");
          else if (result === "lose") playOutcome.classList.add("text-red-700");
          else playOutcome.classList.add("text-gray-700");

          playStats.total++;
          if (result === "win") {
            playStats.wins++;
            playStats.currentWinStreak++;
          } else if (result === "lose") {
            playStats.losses++;
            playStats.currentWinStreak = 0;
          } else {
            playStats.draws++;
          }

          playHistoryData.push({ user: userMove, comp: compMove, result });
          if (playHistoryData.length > 20) playHistoryData.shift();
          updatePlayUI();
        });
      });

      playReset.addEventListener("click", () => {
        playStats = {
          wins: 0,
          losses: 0,
          draws: 0,
          total: 0,
          currentWinStreak: 0,
        };
        playHistoryData = [];
        playUserMove.textContent = "‚Äì";
        playComputerMove.textContent = "‚Äì";
        playOutcome.textContent = "Make your first move!";
        playOutcome.className =
          "text-lg font-semibold text-center text-gray-700";
        updatePlayUI();
      });

      // Experiment mode logic
      const strategyA = document.getElementById("strategy-a");
      const strategyB = document.getElementById("strategy-b");
      const customA = document.getElementById("custom-a");
      const customB = document.getElementById("custom-b");
      const aRock = document.getElementById("a-rock");
      const aPaper = document.getElementById("a-paper");
      const aScissors = document.getElementById("a-scissors");
      const bRock = document.getElementById("b-rock");
      const bPaper = document.getElementById("b-paper");
      const bScissors = document.getElementById("b-scissors");
      const aSum = document.getElementById("a-sum");
      const bSum = document.getElementById("b-sum");
      const roundsInput = document.getElementById("rounds");
      const runSimBtn = document.getElementById("run-sim");
      const resetSimBtn = document.getElementById("reset-sim");
      const simWarning = document.getElementById("sim-warning");

      const simAWins = document.getElementById("sim-a-wins");
      const simBWins = document.getElementById("sim-b-wins");
      const simDraws = document.getElementById("sim-draws");
      const simAWinrate = document.getElementById("sim-a-winrate");
      const simBWinrate = document.getElementById("sim-b-winrate");
      const simDrawrate = document.getElementById("sim-drawrate");
      const simTotal = document.getElementById("sim-total");
      const simAStreak = document.getElementById("sim-a-streak");
      const simBStreak = document.getElementById("sim-b-streak");
      const simHistory = document.getElementById("sim-history");

      function updateSumDisplay(inputs, labelEl) {
        const sum = inputs.reduce(
          (acc, el) => acc + (parseFloat(el.value) || 0),
          0
        );
        labelEl.textContent = "Sum: " + sum.toFixed(2) + "%";
        labelEl.classList.toggle("text-red-600", Math.abs(sum - 100) > 0.5);
      }

      [aRock, aPaper, aScissors].forEach((el) => {
        el.addEventListener("input", () =>
          updateSumDisplay([aRock, aPaper, aScissors], aSum)
        );
      });
      [bRock, bPaper, bScissors].forEach((el) => {
        el.addEventListener("input", () =>
          updateSumDisplay([bRock, bPaper, bScissors], bSum)
        );
      });

      strategyA.addEventListener("change", () => {
        customA.classList.toggle("hidden", strategyA.value !== "custom");
      });
      strategyB.addEventListener("change", () => {
        customB.classList.toggle("hidden", strategyB.value !== "custom");
      });

      function getStrategyProb(strategy, r, p, s) {
        if (strategy === "always-rock") return { r: 1, p: 0, s: 0 };
        if (strategy === "always-paper") return { r: 0, p: 1, s: 0 };
        if (strategy === "always-scissors") return { r: 0, p: 0, s: 1 };
        if (strategy === "custom") {
          const total = r + p + s || 0;
          if (total <= 0) return { r: 1 / 3, p: 1 / 3, s: 1 / 3 };
          return { r: r / total, p: p / total, s: s / total };
        }
        // random
        return { r: 1 / 3, p: 1 / 3, s: 1 / 3 };
      }

      function drawFromProb(prob) {
        const x = Math.random();
        if (x < prob.r) return "rock";
        if (x < prob.r + prob.p) return "paper";
        return "scissors";
      }

      function resetSimUI() {
        simAWins.textContent = "0";
        simBWins.textContent = "0";
        simDraws.textContent = "0";
        simAWinrate.textContent = "0.0%";
        simBWinrate.textContent = "0.0%";
        simDrawrate.textContent = "0.0%";
        simTotal.textContent = "0";
        simAStreak.textContent = "0";
        simBStreak.textContent = "0";
        simHistory.innerHTML = "";
        simWarning.classList.add("hidden");
      }

      resetSimBtn.addEventListener("click", resetSimUI);

      runSimBtn.addEventListener("click", () => {
        simWarning.classList.add("hidden");
        const rounds = parseInt(roundsInput.value, 10);
        if (!Number.isFinite(rounds) || rounds < 1 || rounds > 100000) {
          simWarning.textContent =
            "Please enter a number of rounds between 1 and 100,000.";
          simWarning.classList.remove("hidden");
          return;
        }

        const aR = parseFloat(aRock.value) || 0;
        const aP = parseFloat(aPaper.value) || 0;
        const aS = parseFloat(aScissors.value) || 0;
        const bR = parseFloat(bRock.value) || 0;
        const bP = parseFloat(bPaper.value) || 0;
        const bS = parseFloat(bScissors.value) || 0;

        const probA = getStrategyProb(strategyA.value, aR, aP, aS);
        const probB = getStrategyProb(strategyB.value, bR, bP, bS);

        let aWins = 0,
          bWins = 0,
          draws = 0;
        let longestAStreak = 0,
          longestBStreak = 0;
        let currentAStreak = 0,
          currentBStreak = 0;
        const historySample = [];

        for (let i = 0; i < rounds; i++) {
          const moveA = drawFromProb(probA);
          const moveB = drawFromProb(probB);
          const res = compareMoves(moveA, moveB); // from play mode

          if (res === "win") {
            aWins++;
            currentAStreak++;
            currentBStreak = 0;
            if (currentAStreak > longestAStreak)
              longestAStreak = currentAStreak;
          } else if (res === "lose") {
            bWins++;
            currentBStreak++;
            currentAStreak = 0;
            if (currentBStreak > longestBStreak)
              longestBStreak = currentBStreak;
          } else {
            draws++;
            currentAStreak = 0;
            currentBStreak = 0;
          }

          if (i < 50) {
            historySample.push({ round: i + 1, a: moveA, b: moveB, res });
          }
        }

        const total = rounds;
        const aRate = total ? (aWins / total) * 100 : 0;
        const bRate = total ? (bWins / total) * 100 : 0;
        const dRate = total ? (draws / total) * 100 : 0;

        simAWins.textContent = aWins;
        simBWins.textContent = bWins;
        simDraws.textContent = draws;
        simAWinrate.textContent = aRate.toFixed(1) + "%";
        simBWinrate.textContent = bRate.toFixed(1) + "%";
        simDrawrate.textContent = dRate.toFixed(1) + "%";
        simTotal.textContent = total;
        simAStreak.textContent = longestAStreak;
        simBStreak.textContent = longestBStreak;

        simHistory.innerHTML = "";
        historySample.forEach((item) => {
          const row = document.createElement("div");
          row.className =
            "flex justify-between border-b border-gray-100 pb-0.5";
          row.innerHTML = `
        <span class="text-gray-500">#${item.round}</span>
        <span>A: ${capitalize(item.a)} vs B: ${capitalize(item.b)}</span>
        <span class="${
          item.res === "win"
            ? "text-green-700"
            : item.res === "lose"
            ? "text-red-700"
            : "text-gray-600"
        } font-medium">
          ${
            item.res === "win"
              ? "A wins"
              : item.res === "lose"
              ? "B wins"
              : "Draw"
          }
        </span>
      `;
          simHistory.appendChild(row);
        });
      });

      // Simple FAQ toggle (in addition to global script_faq)
      document
        .querySelectorAll("#faq-container .faq-question")
        .forEach((btn) => {
          btn.addEventListener("click", () => {
            const answer = btn.parentElement.querySelector(".faq-answer");
            const icon = btn.querySelector("span:last-child");
            const isHidden = answer.classList.contains("hidden");
            document
              .querySelectorAll("#faq-container .faq-answer")
              .forEach((a) => a.classList.add("hidden"));
            document
              .querySelectorAll("#faq-container .faq-question span:last-child")
              .forEach((i) => (i.textContent = "+"));
            if (isHidden) {
              answer.classList.remove("hidden");
              icon.textContent = "‚àí";
            }
          });
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
