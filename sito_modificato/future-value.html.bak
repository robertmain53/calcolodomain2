    <script>
      function formatMoney(value) {
        if (!isFinite(value)) return "—";
        return value.toLocaleString(undefined, {
          minimumFractionDigits: 2,
          maximumFractionDigits: 2,
        });
      }

      function getSelectedTiming() {
        const radios = document.querySelectorAll('input[name="paymentTiming"]');
        for (const r of radios) if (r.checked) return r.value;
        return "end";
      }

      function setMode(mode) {
        const tabs = document.querySelectorAll(".fv-tab");
        tabs.forEach((tab) => {
          if (tab.dataset.mode === mode) {
            tab.classList.remove("tab-inactive");
            tab.classList.add("tab-active");
          } else {
            tab.classList.remove("tab-active");
            tab.classList.add("tab-inactive");
          }
        });

        const paymentRow = document.getElementById("payment-row");
        const growthRow = document.getElementById("growth-row");

        if (mode === "lump") {
          paymentRow.classList.add("hidden");
          growthRow.classList.add("hidden");
        } else if (mode === "annuity") {
          paymentRow.classList.remove("hidden");
          growthRow.classList.add("hidden");
        } else {
          paymentRow.classList.remove("hidden");
          growthRow.classList.remove("hidden");
        }

        document.getElementById("fv-form").dataset.mode = mode;
      }

      function calculateFV() {
        const mode = document.getElementById("fv-form").dataset.mode || "lump";

        const pv = parseFloat(document.getElementById("pv").value) || 0;
        const pmt = parseFloat(document.getElementById("pmt").value) || 0;
        const rate = parseFloat(document.getElementById("rate").value) || 0;
        const years = parseFloat(document.getElementById("years").value) || 0;
        const compound = parseInt(
          document.getElementById("compound").value,
          10
        );
        const paymentFreq = parseInt(
          document.getElementById("paymentFreq").value,
          10
        );
        const growthRate =
          parseFloat(document.getElementById("growthRate").value) || 0;
        const timing = getSelectedTiming();

        const r = rate / 100;

        let fvPV = 0;
        let fvPmt = 0;
        let nComp = years * (compound > 0 ? compound : 1);
        let nPay = years * paymentFreq;

        // Lump sum component
        if (compound === 0) {
          // Continuous compounding
          fvPV = pv * Math.exp(r * years);
        } else {
          const iComp = r / compound;
          fvPV = pv * Math.pow(1 + iComp, compound * years);
        }

        // Annuity / growing annuity component
        if (mode === "annuity" || mode === "growing") {
          const iPay = r / paymentFreq;
          const g = mode === "growing" ? growthRate / 100 : 0;
          const n = nPay;

          if (n > 0 && pmt !== 0) {
            if (mode === "annuity" || Math.abs(iPay - g) < 1e-7) {
              // Ordinary or near-equal rates: use standard annuity formula or step simulation
              if (mode === "annuity") {
                if (iPay === 0) {
                  fvPmt = pmt * n;
                } else {
                  fvPmt = (pmt * (Math.pow(1 + iPay, n) - 1)) / iPay;
                }
              } else {
                // Growing with i ≈ g: simulate period by period
                let balance = 0;
                let contrib = pmt;
                for (let k = 0; k < n; k++) {
                  if (timing === "begin") {
                    balance += contrib;
                    balance *= 1 + iPay;
                  } else {
                    balance *= 1 + iPay;
                    balance += contrib;
                  }
                  contrib *= 1 + g;
                }
                fvPmt = balance;
              }

              if (mode === "annuity" && timing === "begin") {
                fvPmt *= 1 + iPay;
              }
            } else {
              // Growing annuity closed-form
              const factor =
                (Math.pow(1 + iPay, n) - Math.pow(1 + g, n)) / (iPay - g);
              fvPmt = pmt * factor;
              if (timing === "begin") {
                fvPmt *= 1 + iPay;
              }
            }
          }
        }

        const fvTotal = fvPV + fvPmt;

        // Total contributions
        let totalContrib = pv;
        if (mode === "annuity" || mode === "growing") {
          const g = mode === "growing" ? growthRate / 100 : 0;
          const n = nPay;
          if (mode === "annuity" || g === 0) {
            totalContrib += pmt * n;
          } else {
            // Sum of geometric series for growing contributions
            if (g === 0) {
              totalContrib += pmt * n;
            } else {
              totalContrib += (pmt * (1 - Math.pow(1 + g, n))) / (1 - (1 + g));
            }
          }
        }

        const interestEarned = fvTotal - totalContrib;

        // Update UI
        document.getElementById("fv-result").textContent =
          "$" + formatMoney(fvTotal);
        document.getElementById("contrib-result").textContent =
          "$" + formatMoney(totalContrib);
        document.getElementById("interest-result").textContent =
          "$" + formatMoney(interestEarned);

        // Effective annual rate line
        let ear = 0;
        if (compound === 0) {
          ear = Math.exp(r) - 1;
        } else if (compound > 0) {
          ear = Math.pow(1 + r / compound, compound) - 1;
        }
        const earLine = document.getElementById("effective-rate-line");
        earLine.textContent =
          isFinite(ear) && ear > -1
            ? `Effective annual rate (including compounding): ${(
                ear * 100
              ).toFixed(3)}%`
            : "";

        // Build simple yearly schedule (simulation)
        buildSchedule({
          pv,
          pmt,
          r,
          years,
          compound,
          paymentFreq,
          growthRate: growthRate / 100,
          timing,
          mode,
        });

        document.getElementById("results-section").classList.remove("hidden");
      }

      function buildSchedule(params) {
        const tbody = document.getElementById("schedule-body");
        tbody.innerHTML = "";

        const {
          pv,
          pmt,
          r,
          years,
          compound,
          paymentFreq,
          growthRate,
          timing,
          mode,
        } = params;
        const totalYears = Math.floor(years);
        if (totalYears <= 0) return;

        const stepsPerYear = Math.max(compound > 0 ? compound : 1, paymentFreq);
        const dt = 1 / stepsPerYear;
        const iStep = compound === 0 ? 0 : r / stepsPerYear; // for continuous, approximate with many small steps

        let balance = pv;
        let contribLevel = pmt;

        for (let year = 1; year <= totalYears; year++) {
          const startBalance = balance;
          let contribThisYear = 0;
          let interestThisYear = 0;

          for (let step = 0; step < stepsPerYear; step++) {
            const isPaymentStep =
              mode === "lump"
                ? false
                : step % Math.round(stepsPerYear / paymentFreq) === 0;

            if (mode !== "lump" && isPaymentStep && timing === "begin") {
              balance += contribLevel;
              contribThisYear += contribLevel;
              if (mode === "growing") contribLevel *= 1 + growthRate;
            }

            // Interest
            let interestStep;
            if (compound === 0) {
              // approximate continuous compounding over small dt
              interestStep = balance * (Math.exp(r * dt) - 1);
            } else {
              interestStep = balance * iStep;
            }
            balance += interestStep;
            interestThisYear += interestStep;

            if (mode !== "lump" && isPaymentStep && timing === "end") {
              balance += contribLevel;
              contribThisYear += contribLevel;
              if (mode === "growing") contribLevel *= 1 + growthRate;
            }
          }

          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="px-2 py-1 border-b text-left">${year}</td>
        <td class="px-2 py-1 border-b text-right">$${formatMoney(
          startBalance
        )}</td>
        <td class="px-2 py-1 border-b text-right">$${formatMoney(
          contribThisYear
        )}</td>
        <td class="px-2 py-1 border-b text-right">$${formatMoney(
          interestThisYear
        )}</td>
        <td class="px-2 py-1 border-b text-right">$${formatMoney(balance)}</td>
      `;
          tbody.appendChild(tr);
        }
      }

      document.addEventListener("DOMContentLoaded", () => {
        // Initialize mode
        setMode("annuity");
        document.getElementById("fv-form").dataset.mode = "annuity";

        // Tab handlers
        document.querySelectorAll(".fv-tab").forEach((tab) => {
          tab.addEventListener("click", () => {
            setMode(tab.dataset.mode);
          });
        });

        document
          .getElementById("calc-btn")
          .addEventListener("click", calculateFV);

        document.getElementById("reset-btn").addEventListener("click", () => {
          document.getElementById("pv").value = 10000;
          document.getElementById("pmt").value = 200;
          document.getElementById("rate").value = 7;
          document.getElementById("years").value = 20;
          document.getElementById("compound").value = 12;
          document.getElementById("paymentFreq").value = 12;
          document.getElementById("growthRate").value = 3;
          document.querySelector(
            'input[name="paymentTiming"][value="end"]'
          ).checked = true;
          setMode("annuity");
          document.getElementById("results-section").classList.add("hidden");
        });

        // Auto-calc on load for quick demo
        calculateFV();
      });
    </script>

