    <script>
      // Basic shape library (US units)
      const shapeLibraryUS = {
        W12x26: { Zx: 36.8, Sx: 30.6, Ix: 184, Aw: 3.06 },
        W18x35: { Zx: 72.6, Sx: 60.5, Ix: 548, Aw: 4.12 },
        W24x55: { Zx: 139, Sx: 116, Ix: 1390, Aw: 6.47 },
      };

      const E_US = 29000; // ksi
      const E_SI = 200000; // MPa

      const $ = (id) => document.getElementById(id);

      function updateUnits() {
        const unit = $("unitSystem").value;
        if (unit === "US") {
          $("fyUnit").textContent = "(Fy, ksi)";
          $("ZxUnit").textContent = "(Zx, in³)";
          $("SxUnit").textContent = "(Sx, in³)";
          $("IxUnit").textContent = "(Ix, in⁴)";
          $("AwUnit").textContent = "(Aw, in²)";
          $("LbUnit").textContent = "(Lb, ft)";
          $("LbeamUnit").textContent = "(L, ft)";
          $("wUnit").textContent = "(k/ft)";
          $("pUnit").textContent = "(k)";
        } else {
          $("fyUnit").textContent = "(Fy, MPa)";
          $("ZxUnit").textContent = "(Zx, mm³)";
          $("SxUnit").textContent = "(Sx, mm³)";
          $("IxUnit").textContent = "(Ix, mm⁴)";
          $("AwUnit").textContent = "(Aw, mm²)";
          $("LbUnit").textContent = "(Lb, m)";
          $("LbeamUnit").textContent = "(L, m)";
          $("wUnit").textContent = "(kN/m)";
          $("pUnit").textContent = "(kN)";
        }
      }

      function applyShapeFromLibrary() {
        const shape = $("shapeSelect").value;
        const unit = $("unitSystem").value;
        if (shape === "custom") {
          ["Zx", "Sx", "Ix", "Aw"].forEach((id) => ($(id).value = ""));
          return;
        }
        const props = shapeLibraryUS[shape];
        if (!props) return;

        if (unit === "US") {
          $("Zx").value = props.Zx;
          $("Sx").value = props.Sx;
          $("Ix").value = props.Ix;
          $("Aw").value = props.Aw;
        } else {
          // convert in-based to mm-based (1 in = 25.4 mm)
          const mm = 25.4;
          $("Zx").value = (props.Zx * Math.pow(mm, 3)).toFixed(0);
          $("Sx").value = (props.Sx * Math.pow(mm, 3)).toFixed(0);
          $("Ix").value = (props.Ix * Math.pow(mm, 4)).toFixed(0);
          $("Aw").value = (props.Aw * Math.pow(mm, 2)).toFixed(0);
        }
      }

      function resetResults() {
        $("resultsSummary").classList.add("hidden");
        $("noResultsMessage").classList.remove("hidden");
      }

      function runDesign() {
        const unit = $("unitSystem").value;
        const method = $("designMethod").value;
        const fy = parseFloat($("fy").value);
        const Zx = parseFloat($("Zx").value);
        const Sx = parseFloat($("Sx").value);
        const Ix = parseFloat($("Ix").value);
        const Aw = parseFloat($("Aw").value);
        const Lb = parseFloat($("Lb").value);
        const Lbeam = parseFloat($("Lbeam").value);
        const loadType = $("loadType").value;
        const w = parseFloat($("wLoad").value) || 0;
        const P = parseFloat($("pLoad").value) || 0;
        const deflLimitSel = $("deflectionLimit").value;
        const deflRatio = parseFloat($("deflectionRatio").value) || 360;
        const useServiceForDefl = $("deflectionUsesService").checked;

        if (!fy || !Zx || !Sx || !Ix || !Aw || !Lb || !Lbeam) {
          alert("Please enter all required section and geometry values.");
          return;
        }

        // Convert to consistent base units
        let E, L_in, Lb_in, w_klf, P_k;
        if (unit === "US") {
          E = E_US; // ksi
          L_in = Lbeam * 12;
          Lb_in = Lb * 12;
          w_klf = w;
          P_k = P;
        } else {
          // SI: convert to equivalent US for formulas, then convert back for display
          // 1 MPa = 0.145038 ksi, 1 mm = 0.0393701 in, 1 kN = 0.224809 kips
          const MPa_to_ksi = 0.145038;
          const mm_to_in = 0.0393701;
          const kN_to_k = 0.224809;
          E = E_SI * MPa_to_ksi;
          L_in = Lbeam / 0.0254;
          Lb_in = Lb / 0.0254;
          w_klf = w * kN_to_k * 3.28084; // kN/m -> k/ft
          P_k = P * kN_to_k;
        }

        // Internal forces
        let Mu, Vu;
        if (loadType === "UDL") {
          Mu = (w_klf * Math.pow(Lbeam, 2)) / 8; // k-ft
          Vu = (w_klf * Lbeam) / 2; // k
        } else {
          Mu = (P_k * Lbeam) / 4; // k-ft
          Vu = P_k / 2; // k
        }
        const Mu_in = Mu * 12; // k-in

        // Flexural strength (simplified)
        const phi_b = method === "LRFD" ? 0.9 : 1.0;
        const Omega_b = method === "ASD" ? 1.67 : 1.0;

        const Mp = fy * Zx; // k-in
        const My = fy * Sx; // k-in

        // crude LTB reduction: if Lb > 20 ft equivalent, reduce capacity
        let Mn = Math.min(Mp, 1.1 * My);
        const Lb_ft_equiv = Lb_in / 12;
        if (Lb_ft_equiv > 20) {
          const factor = Math.max(0.6, 20 / Lb_ft_equiv);
          Mn *= factor;
        }

        let phiMn, Mn_over_Omega;
        if (method === "LRFD") {
          phiMn = phi_b * Mn;
        } else {
          Mn_over_Omega = Mn / Omega_b;
        }

        const Mu_req = Mu_in;
        const flexCap = method === "LRFD" ? phiMn : Mn_over_Omega;
        const flexUtil = Mu_req / flexCap;

        // Shear strength (very simplified)
        const phi_v = method === "LRFD" ? 0.9 : 1.0;
        const Omega_v = method === "ASD" ? 1.67 : 1.0;
        const Vn = 0.6 * fy * Aw; // k (approx, ignoring units nuance)
        const Vcap = method === "LRFD" ? phi_v * Vn : Vn / Omega_v;
        const shearUtil = Vu / Vcap;

        // Deflection
        let w_service = w_klf;
        let P_service = P_k;
        if (!useServiceForDefl && method === "LRFD") {
          // assume factored load ~ 1.6*service for live load dominated
          w_service = w_klf / 1.6;
          P_service = P_k / 1.6;
        }

        let delta_max_in = 0;
        if (loadType === "UDL") {
          // w in k/ft -> convert to k/in
          const w_k_in = w_service / 12;
          delta_max_in = (5 * w_k_in * Math.pow(L_in, 4)) / (384 * E * Ix);
        } else {
          delta_max_in = (P_service * Math.pow(L_in, 3)) / (48 * E * Ix);
        }

        let ratio = deflRatio;
        if (deflLimitSel === "L360") ratio = 360;
        if (deflLimitSel === "L240") ratio = 240;
        if (deflLimitSel === "L480") ratio = 480;

        const delta_allow_in = L_in / ratio;
        const deflUtil = delta_max_in / delta_allow_in;

        // Overall status
        const maxUtil = Math.max(flexUtil, shearUtil, deflUtil);
        const ok = maxUtil <= 1.0 + 1e-6;
        let governing = "Flexure";
        if (shearUtil >= flexUtil && shearUtil >= deflUtil) governing = "Shear";
        if (deflUtil >= flexUtil && deflUtil >= shearUtil)
          governing = "Deflection";

        // Display results
        $("resultsSummary").classList.remove("hidden");
        $("noResultsMessage").classList.add("hidden");

        const statusBadge = $("overallStatusBadge");
        statusBadge.textContent = ok ? "PASS" : "FAIL";
        statusBadge.className =
          "result-badge " + (ok ? "result-badge-ok" : "result-badge-ng");
        $("overallStatusText").textContent = ok
          ? "Beam OK for given loads"
          : "Beam does NOT satisfy all checks";
        $("governingLimitState").textContent =
          "Governing limit state: " +
          governing +
          " (max D/C ≈ " +
          maxUtil.toFixed(2) +
          ")";

        $("flexureUtil").textContent = flexUtil.toFixed(2);
        $("shearUtil").textContent = shearUtil.toFixed(2);
        $("deflUtil").textContent = deflUtil.toFixed(2);

        const unitMoment = unit === "US" ? "k-ft" : "kN·m";
        const unitShear = unit === "US" ? "k" : "kN";
        const unitDefl = unit === "US" ? "in" : "mm";

        // Convert capacities back to display units
        let Mu_disp = Mu;
        let flexCap_disp = flexCap / 12; // k-ft
        let Vu_disp = Vu;
        let Vcap_disp = Vcap;
        let delta_max_disp = delta_max_in;
        let delta_allow_disp = delta_allow_in;

        if (unit === "SI") {
          const k_to_kN = 4.44822;
          const ft_to_m = 0.3048;
          const in_to_mm = 25.4;
          Mu_disp = Mu * k_to_kN * ft_to_m;
          flexCap_disp = flexCap_disp * k_to_kN * ft_to_m;
          Vu_disp = Vu * k_to_kN;
          Vcap_disp = Vcap * k_to_kN;
          delta_max_disp = delta_max_in * in_to_mm;
          delta_allow_disp = delta_allow_in * in_to_mm;
        }

        $("flexureCapacity").innerHTML =
          "Mu = " +
          Mu_disp.toFixed(2) +
          " " +
          unitMoment +
          ", φMn/ (Mn/Ω) = " +
          flexCap_disp.toFixed(2) +
          " " +
          unitMoment;

        $("shearCapacity").innerHTML =
          "Vu = " +
          Vu_disp.toFixed(2) +
          " " +
          unitShear +
          ", V<sub>cap</sub> = " +
          Vcap_disp.toFixed(2) +
          " " +
          unitShear;

        $("deflValues").innerHTML =
          "Δ = " +
          delta_max_disp.toFixed(3) +
          " " +
          unitDefl +
          ", Δ<sub>allow</sub> = " +
          delta_allow_disp.toFixed(3) +
          " " +
          unitDefl;

        // Details
        const unitLen = unit === "US" ? "ft" : "m";
        const unitLenIn = unit === "US" ? "in" : "mm";
        const unitZ = unit === "US" ? "in³" : "mm³";
        const unitI = unit === "US" ? "in⁴" : "mm⁴";
        const unitA = unit === "US" ? "in²" : "mm²";

        let L_disp = Lbeam;
        let Lb_disp = Lb;
        let Zx_disp = Zx;
        let Sx_disp = Sx;
        let Ix_disp = Ix;
        let Aw_disp = Aw;

        $("detailL").textContent = L_disp.toFixed(2) + " " + unitLen;
        $("detailLb").textContent = Lb_disp.toFixed(2) + " " + unitLen;
        $("detailZx").textContent = Zx_disp.toFixed(1) + " " + unitZ;
        $("detailSx").textContent = Sx_disp.toFixed(1) + " " + unitZ;
        $("detailIx").textContent = Ix_disp.toFixed(1) + " " + unitI;
        $("detailAw").textContent = Aw_disp.toFixed(2) + " " + unitA;

        $("detailForces").innerHTML =
          "Load case: " +
          (loadType === "UDL" ? "Uniform load" : "Point load at midspan") +
          ". Mu = " +
          Mu_disp.toFixed(2) +
          " " +
          unitMoment +
          ", Vu = " +
          Vu_disp.toFixed(2) +
          " " +
          unitShear +
          ".";

        $("detailFlexure").innerHTML =
          "Plastic moment Mp = Fy · Zx = " +
          (Mp / 12).toFixed(2) +
          " " +
          unitMoment +
          ". Elastic yield moment My = Fy · Sx = " +
          (My / 12).toFixed(2) +
          " " +
          unitMoment +
          ".<br>Nominal strength Mn (with simplified LTB) ≈ " +
          (Mn / 12).toFixed(2) +
          " " +
          unitMoment +
          ". Design strength = " +
          flexCap_disp.toFixed(2) +
          " " +
          unitMoment +
          ", demand/capacity = " +
          flexUtil.toFixed(2) +
          ".";

        $("detailShear").innerHTML =
          "Nominal shear strength Vn ≈ 0.6 · Fy · Aw = " +
          Vn.toFixed(2) +
          " k (approx.). " +
          "Design shear capacity = " +
          Vcap_disp.toFixed(2) +
          " " +
          unitShear +
          ", demand/capacity = " +
          shearUtil.toFixed(2) +
          ".";

        $("detailDeflection").innerHTML =
          "Maximum deflection Δ = " +
          delta_max_disp.toFixed(3) +
          " " +
          unitDefl +
          ", allowable Δ<sub>allow</sub> = " +
          delta_allow_disp.toFixed(3) +
          " " +
          unitDefl +
          " (L/" +
          ratio.toFixed(0) +
          "). Demand/capacity = " +
          deflUtil.toFixed(2) +
          ".";
      }

      document.addEventListener("DOMContentLoaded", () => {
        updateUnits();
        applyShapeFromLibrary();
        resetResults();

        $("unitSystem").addEventListener("change", () => {
          updateUnits();
          applyShapeFromLibrary();
          resetResults();
        });

        $("shapeSelect").addEventListener("change", () => {
          applyShapeFromLibrary();
          resetResults();
        });

        $("deflectionLimit").addEventListener("change", () => {
          const sel = $("deflectionLimit").value;
          $("deflectionRatio").disabled = sel !== "custom";
          if (sel === "L360") $("deflectionRatio").value = 360;
          if (sel === "L240") $("deflectionRatio").value = 240;
          if (sel === "L480") $("deflectionRatio").value = 480;
        });

        $("runDesign").addEventListener("click", runDesign);

        $("resetForm").addEventListener("click", () => {
          $("designMethod").value = "LRFD";
          $("unitSystem").value = "US";
          $("fy").value = 50;
          $("shapeSelect").value = "W12x26";
          updateUnits();
          applyShapeFromLibrary();
          $("Lb").value = 10;
          $("Lbeam").value = 20;
          $("loadType").value = "UDL";
          $("wLoad").value = 1.0;
          $("pLoad").value = 0;
          $("deflectionLimit").value = "L360";
          $("deflectionRatio").value = 360;
          $("deflectionUsesService").checked = true;
          resetResults();
        });
      });
    </script>

