    <script>
      // Unit system: true = imperial (ft/gal), false = metric (m/L)
      let isImperial = true;

      const unitLabels = document.querySelectorAll(".unit-label");
      const btnImperial = document.getElementById("btn-imperial");
      const btnMetric = document.getElementById("btn-metric");

      function updateUnitLabels() {
        unitLabels.forEach((el) => {
          el.textContent = isImperial ? "(ft)" : "(m)";
        });
      }

      btnImperial.addEventListener("click", () => {
        isImperial = true;
        btnImperial.classList.add("toggle-btn-active");
        btnMetric.classList.remove("toggle-btn-active");
        updateUnitLabels();
      });

      btnMetric.addEventListener("click", () => {
        isImperial = false;
        btnMetric.classList.add("toggle-btn-active");
        btnImperial.classList.remove("toggle-btn-active");
        updateUnitLabels();
      });

      updateUnitLabels();

      // Shape tabs
      const shapeTabs = document.querySelectorAll(".shape-tab");
      const shapePanels = document.querySelectorAll(".shape-panel");

      shapeTabs.forEach((tab) => {
        tab.addEventListener("click", () => {
          const shape = tab.getAttribute("data-shape");
          shapeTabs.forEach((t) => {
            t.classList.remove(
              "border-blue-600",
              "bg-blue-50",
              "text-blue-700",
              "font-medium"
            );
            t.classList.add("border-gray-300", "bg-white", "text-gray-700");
          });
          tab.classList.add(
            "border-blue-600",
            "bg-blue-50",
            "text-blue-700",
            "font-medium"
          );
          tab.classList.remove("border-gray-300", "bg-white", "text-gray-700");

          shapePanels.forEach((panel) => {
            panel.classList.toggle("hidden", panel.id !== "shape-" + shape);
          });
        });
      });

      // Steps toggle
      const toggleStepsBtn = document.getElementById("toggle-steps");
      const stepsPanel = document.getElementById("steps-panel");
      let stepsVisible = false;
      toggleStepsBtn.addEventListener("click", () => {
        stepsVisible = !stepsVisible;
        stepsPanel.classList.toggle("hidden", !stepsVisible);
        toggleStepsBtn.querySelector("span").textContent =
          (stepsVisible ? "− Hide" : "+ Include") +
          " steps / bench volume (optional)";
      });

      // Calculator logic
      const btnCalc = document.getElementById("btn-calc");
      const btnReset = document.getElementById("btn-reset");
      const errorMsg = document.getElementById("error-msg");
      const resultsSection = document.getElementById("results-section");
      const resultMain = document.getElementById("result-main");
      const resultMainAlt = document.getElementById("result-main-alt");
      const resultBreakdown = document.getElementById("result-breakdown");

      function parseVal(id) {
        const el = document.getElementById(id);
        if (!el) return null;
        const v = parseFloat(el.value);
        return isNaN(v) || v <= 0 ? null : v;
      }

      function calcStepsVolume() {
        if (stepsPanel.classList.contains("hidden")) return 0;
        const L = parseVal("steps-length");
        const W = parseVal("steps-width");
        const D = parseVal("steps-depth");
        if (!L || !W || !D) return 0;
        return L * W * D; // cubic ft or m
      }

      function getActiveShape() {
        let shape = "rect";
        shapeTabs.forEach((tab) => {
          if (tab.classList.contains("border-blue-600")) {
            shape = tab.getAttribute("data-shape");
          }
        });
        return shape;
      }

      btnCalc.addEventListener("click", () => {
        errorMsg.textContent = "";
        let Vpool = 0;
        let shape = getActiveShape();

        try {
          if (shape === "rect") {
            const L = parseVal("rect-length");
            const W = parseVal("rect-width");
            const ds = parseVal("rect-depth-shallow");
            const dd = parseVal("rect-depth-deep");
            if (!L || !W || !ds || !dd)
              throw new Error(
                "Please enter length, width, shallow and deep depths for the rectangular pool."
              );
            const davg = (ds + dd) / 2;
            Vpool = L * W * davg;
          } else if (shape === "round") {
            const D = parseVal("round-diameter");
            const d = parseVal("round-depth");
            if (!D || !d)
              throw new Error(
                "Please enter diameter and average depth for the round pool."
              );
            const r = D / 2;
            Vpool = Math.PI * r * r * d;
          } else if (shape === "oval") {
            const L = parseVal("oval-length");
            const W = parseVal("oval-width");
            const ds = parseVal("oval-depth-shallow");
            const dd = parseVal("oval-depth-deep");
            if (!L || !W || !ds || !dd)
              throw new Error(
                "Please enter length, width, shallow and deep depths for the oval pool."
              );
            const davg = (ds + dd) / 2;
            Vpool = (Math.PI / 4) * L * W * davg;
          } else if (shape === "kidney") {
            const L = parseVal("kidney-length");
            const W = parseVal("kidney-width");
            const ds = parseVal("kidney-depth-shallow");
            const dd = parseVal("kidney-depth-deep");
            if (!L || !W || !ds || !dd)
              throw new Error(
                "Please enter average length, width, shallow and deep depths for the kidney/irregular pool."
              );
            const davg = (ds + dd) / 2;
            Vpool = L * W * davg;
          }

          const Vsteps = calcStepsVolume();
          const Vtotal = Vpool + Vsteps;

          if (Vtotal <= 0 || !isFinite(Vtotal))
            throw new Error(
              "Please check your inputs. Volume could not be calculated."
            );

          let mainStr = "";
          let altStr = "";
          let breakdownStr = "";

          if (isImperial) {
            const gallons = Vtotal * 7.48;
            const liters = gallons * 3.78541;
            mainStr =
              gallons.toLocaleString(undefined, { maximumFractionDigits: 0 }) +
              " gallons (US)";
            altStr =
              liters.toLocaleString(undefined, { maximumFractionDigits: 0 }) +
              " liters";
            breakdownStr =
              "Pool: " +
              (Vpool * 7.48).toLocaleString(undefined, {
                maximumFractionDigits: 0,
              }) +
              " gal";
            if (Vsteps > 0) {
              breakdownStr +=
                " + Steps/bench: " +
                (Vsteps * 7.48).toLocaleString(undefined, {
                  maximumFractionDigits: 0,
                }) +
                " gal";
            }
          } else {
            const cubicMeters = Vtotal;
            const liters = cubicMeters * 1000;
            const gallons = liters / 3.78541;
            mainStr =
              liters.toLocaleString(undefined, { maximumFractionDigits: 0 }) +
              " liters";
            altStr =
              gallons.toLocaleString(undefined, { maximumFractionDigits: 0 }) +
              " gallons (US)";
            breakdownStr =
              "Pool: " +
              (Vpool * 1000).toLocaleString(undefined, {
                maximumFractionDigits: 0,
              }) +
              " L";
            if (Vsteps > 0) {
              breakdownStr +=
                " + Steps/bench: " +
                (Vsteps * 1000).toLocaleString(undefined, {
                  maximumFractionDigits: 0,
                }) +
                " L";
            }
          }

          resultMain.textContent = mainStr;
          resultMainAlt.textContent = altStr;
          resultBreakdown.textContent = breakdownStr;
          resultsSection.classList.remove("hidden");
        } catch (e) {
          errorMsg.textContent = e.message;
          resultsSection.classList.add("hidden");
        }
      });

      btnReset.addEventListener("click", () => {
        document
          .querySelectorAll("#calculator-forms input")
          .forEach((inp) => (inp.value = ""));
        document
          .querySelectorAll("#steps-panel input")
          .forEach((inp) => (inp.value = ""));
        errorMsg.textContent = "";
        resultsSection.classList.add("hidden");
      });

      // Simple FAQ toggle (in addition to global script_faq)
      document
        .querySelectorAll("#faq-container .faq-question")
        .forEach((btn) => {
          btn.addEventListener("click", () => {
            const answer = btn.parentElement.querySelector(".faq-answer");
            const icon = btn.querySelector("span:last-child");
            const isHidden = answer.classList.contains("hidden");
            answer.classList.toggle("hidden", !isHidden);
            icon.textContent = isHidden ? "−" : "+";
          });
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
