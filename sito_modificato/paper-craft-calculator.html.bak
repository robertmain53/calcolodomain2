    <script>
      // Utility: unit handling
      let currentUnit = "in"; // 'in' or 'cm'

      function toCurrentUnit(valueInInches) {
        return currentUnit === "in" ? valueInInches : valueInInches * 2.54;
      }

      function fromCurrentUnit(value) {
        return currentUnit === "in" ? value : value / 2.54;
      }

      function formatNumber(num, decimals = 2) {
        if (!isFinite(num)) return "-";
        return parseFloat(num.toFixed(decimals));
      }

      // Mode switching
      const modeButtons = document.querySelectorAll(".mode-btn");
      const modeSections = {
        card: document.getElementById("mode-card"),
        box: document.getElementById("mode-box"),
        envelope: document.getElementById("mode-envelope"),
      };
      let currentMode = "card";

      modeButtons.forEach((btn) => {
        btn.addEventListener("click", () => {
          currentMode = btn.dataset.mode;
          modeButtons.forEach((b) =>
            b.classList.remove(
              "toggle-btn-active",
              "border-blue-600",
              "text-blue-700"
            )
          );
          btn.classList.add(
            "toggle-btn-active",
            "border-blue-600",
            "text-blue-700"
          );
          Object.keys(modeSections).forEach((m) => {
            modeSections[m].classList.toggle("hidden", m !== currentMode);
          });
        });
      });

      // Unit toggle
      const unitInBtn = document.getElementById("unit-in");
      const unitCmBtn = document.getElementById("unit-cm");

      function setUnit(unit) {
        if (unit === currentUnit) return;
        // Convert all numeric inputs between units
        const inputs = document.querySelectorAll(
          '#paperCraftForm input[type="number"]'
        );
        inputs.forEach((input) => {
          const val = parseFloat(input.value);
          if (!isNaN(val)) {
            const inches = currentUnit === "in" ? val : val / 2.54;
            const newVal = unit === "in" ? inches : inches * 2.54;
            input.value = formatNumber(newVal, 2);
          }
        });
        currentUnit = unit;
        if (unit === "in") {
          unitInBtn.classList.add(
            "toggle-btn-active",
            "border-blue-600",
            "text-blue-700"
          );
          unitCmBtn.classList.remove(
            "toggle-btn-active",
            "border-blue-600",
            "text-blue-700"
          );
        } else {
          unitCmBtn.classList.add(
            "toggle-btn-active",
            "border-blue-600",
            "text-blue-700"
          );
          unitInBtn.classList.remove(
            "toggle-btn-active",
            "border-blue-600",
            "text-blue-700"
          );
        }
      }

      unitInBtn.addEventListener("click", () => setUnit("in"));
      unitCmBtn.addEventListener("click", () => setUnit("cm"));

      // Sheet presets
      const sheetPreset = document.getElementById("sheetPreset");
      const sheetWidth = document.getElementById("sheetWidth");
      const sheetHeight = document.getElementById("sheetHeight");

      sheetPreset.addEventListener("change", () => {
        let wIn = 8.5,
          hIn = 11;
        if (sheetPreset.value === "letter") {
          wIn = 8.5;
          hIn = 11;
        } else if (sheetPreset.value === "a4") {
          wIn = 8.27;
          hIn = 11.69;
        } else if (sheetPreset.value === "twelve") {
          wIn = 12;
          hIn = 12;
        } else {
          return;
        }
        sheetWidth.value = formatNumber(toCurrentUnit(wIn), 2);
        sheetHeight.value = formatNumber(toCurrentUnit(hIn), 2);
      });

      // Core layout calculation (rectangular packing, no rotation for now)
      function computePiecesPerSheet(
        sheetW,
        sheetH,
        pieceW,
        pieceH,
        margin,
        kerf
      ) {
        const usableW = Math.max(sheetW - 2 * margin, 0);
        const usableH = Math.max(sheetH - 2 * margin, 0);
        const pitchX = pieceW + kerf;
        const pitchY = pieceH + kerf;
        if (pitchX <= 0 || pitchY <= 0)
          return { across: 0, down: 0, perSheet: 0 };
        const across = Math.floor(usableW / pitchX);
        const down = Math.floor(usableH / pitchY);
        const perSheet = Math.max(across * down, 0);
        return { across, down, perSheet, usableW, usableH, pitchX, pitchY };
      }

      function buildLayoutGrid(across, down) {
        const grid = document.getElementById("layoutGrid");
        grid.innerHTML = "";
        const maxCols = 20;
        const maxRows = 20;
        const cols = Math.min(across || 0, maxCols);
        const rows = Math.min(down || 0, maxRows);
        const totalCells = maxCols * maxRows;
        for (let r = 0; r < maxRows; r++) {
          for (let c = 0; c < maxCols; c++) {
            const cell = document.createElement("div");
            cell.className = "layout-cell";
            if (r < rows && c < cols) {
              cell.classList.add("used");
            } else {
              cell.classList.add("waste");
            }
            grid.appendChild(cell);
          }
        }
      }

      function showError(msg) {
        const error = document.getElementById("errorMsg");
        error.textContent = msg;
        error.classList.remove("hidden");
        document.getElementById("resultsSection").classList.add("hidden");
      }

      function clearError() {
        const error = document.getElementById("errorMsg");
        error.textContent = "";
        error.classList.add("hidden");
      }

      document.getElementById("calculateBtn").addEventListener("click", () => {
        clearError();

        const sW = parseFloat(sheetWidth.value);
        const sH = parseFloat(sheetHeight.value);
        const margin = parseFloat(document.getElementById("margin").value) || 0;
        const kerf = parseFloat(document.getElementById("kerf").value) || 0;

        if (!sW || !sH || sW <= 0 || sH <= 0) {
          showError("Please enter a valid sheet width and height.");
          return;
        }
        if (margin * 2 >= sW || margin * 2 >= sH) {
          showError("Margin is too large for the selected sheet size.");
          return;
        }

        let pieceW,
          pieceH,
          qtyNeeded,
          detailHtml = "";
        const summaryList = document.getElementById("summaryList");
        const detailList = document.getElementById("detailList");
        summaryList.innerHTML = "";
        detailList.innerHTML = "";

        if (currentMode === "card") {
          const type = document.getElementById("cardType").value;
          const fW = parseFloat(document.getElementById("cardWidth").value);
          const fH = parseFloat(document.getElementById("cardHeight").value);
          qtyNeeded =
            parseInt(document.getElementById("cardQty").value, 10) || 0;
          if (!fW || !fH || fW <= 0 || fH <= 0 || qtyNeeded <= 0) {
            showError(
              "Please enter valid finished card dimensions and quantity."
            );
            return;
          }
          if (type === "panel") {
            pieceW = fW;
            pieceH = fH;
            detailHtml += `<p>Flat panel size: <strong>${formatNumber(
              pieceW
            )} × ${formatNumber(pieceH)} ${currentUnit}</strong></p>`;
          } else {
            // folded: assume fold along height (top fold) -> double height
            pieceW = fW;
            pieceH = 2 * fH;
            detailHtml += `<p>Folded card cut size: <strong>${formatNumber(
              pieceW
            )} × ${formatNumber(
              pieceH
            )} ${currentUnit}</strong> (fold along the long side).</p>`;
          }
        } else if (currentMode === "box") {
          const L = parseFloat(document.getElementById("boxInnerL").value);
          const W = parseFloat(document.getElementById("boxInnerW").value);
          const H = parseFloat(document.getElementById("boxInnerH").value);
          const flap = parseFloat(document.getElementById("boxFlap").value);
          const clearance =
            parseFloat(document.getElementById("boxLidClearance").value) || 0;
          qtyNeeded =
            parseInt(document.getElementById("boxQty").value, 10) || 0;
          if (
            !L ||
            !W ||
            !H ||
            !flap ||
            L <= 0 ||
            W <= 0 ||
            H <= 0 ||
            flap <= 0 ||
            qtyNeeded <= 0
          ) {
            showError(
              "Please enter valid box inner dimensions, flap width and quantity."
            );
            return;
          }
          const baseNetW = L + 2 * H + flap;
          const baseNetH = W + 2 * H;
          const lidNetW = L + clearance + 2 * H + flap;
          const lidNetH = W + clearance + 2 * H;

          // For layout, we consider the larger of base/lid nets
          pieceW = Math.max(baseNetW, lidNetW);
          pieceH = Math.max(baseNetH, lidNetH);

          detailHtml += `<p>Base net size: <strong>${formatNumber(
            baseNetW
          )} × ${formatNumber(baseNetH)} ${currentUnit}</strong></p>`;
          detailHtml += `<p>Lid net size (with clearance): <strong>${formatNumber(
            lidNetW
          )} × ${formatNumber(lidNetH)} ${currentUnit}</strong></p>`;
          detailHtml += `<p class="mt-2 text-gray-600 text-xs">Each net includes side walls and a glue flap. Score along all fold lines before assembling.</p>`;
        } else if (currentMode === "envelope") {
          const cW = parseFloat(document.getElementById("envCardW").value);
          const cH = parseFloat(document.getElementById("envCardH").value);
          const allowance =
            parseFloat(document.getElementById("envAllowance").value) || 0;
          qtyNeeded =
            parseInt(document.getElementById("envQty").value, 10) || 0;
          if (!cW || !cH || cW <= 0 || cH <= 0 || qtyNeeded <= 0) {
            showError(
              "Please enter valid card dimensions and envelope quantity."
            );
            return;
          }
          const cardMax = Math.max(cW, cH);
          const cardMin = Math.min(cW, cH);
          const squareSize = cardMax + cardMin + 2 * allowance;
          const firstScore = (squareSize - cardMin) / 2 - allowance;

          pieceW = squareSize;
          pieceH = squareSize;

          detailHtml += `<p>Cut a square: <strong>${formatNumber(
            squareSize
          )} × ${formatNumber(squareSize)} ${currentUnit}</strong></p>`;
          detailHtml += `<p>Score the first side at: <strong>${formatNumber(
            firstScore
          )} ${currentUnit}</strong> from each edge, then rotate 90° and repeat.</p>`;
          detailHtml += `<p class="mt-2 text-gray-600 text-xs">Fold in the side flaps first, then bottom, then top. Trim tips if desired.</p>`;
        }

        if (!pieceW || !pieceH || pieceW <= 0 || pieceH <= 0) {
          showError(
            "Calculated piece size is invalid. Please check your inputs."
          );
          return;
        }

        const layout = computePiecesPerSheet(
          sW,
          sH,
          pieceW,
          pieceH,
          margin,
          kerf
        );
        if (layout.perSheet === 0) {
          showError(
            "The piece does not fit on the sheet with the current settings. Try reducing dimensions, margin, or kerf."
          );
          return;
        }

        const sheetsNeeded = Math.ceil(qtyNeeded / layout.perSheet);
        const totalPieces = layout.perSheet * sheetsNeeded;
        const sheetArea = sW * sH;
        const pieceArea = pieceW * pieceH;
        const usedAreaPerSheet = layout.perSheet * pieceArea;
        const usagePct =
          sheetArea > 0 ? (usedAreaPerSheet / sheetArea) * 100 : 0;

        // Summary
        const unitLabel = currentUnit === "in" ? "in" : "cm";
        summaryList.innerHTML = `
      <div class="flex justify-between"><dt>Mode</dt><dd class="font-medium capitalize">${currentMode}</dd></div>
      <div class="flex justify-between"><dt>Sheet size</dt><dd class="font-medium">${formatNumber(
        sW
      )} × ${formatNumber(sH)} ${unitLabel}</dd></div>
      <div class="flex justify-between"><dt>Piece size</dt><dd class="font-medium">${formatNumber(
        pieceW
      )} × ${formatNumber(pieceH)} ${unitLabel}</dd></div>
      <div class="flex justify-between"><dt>Pieces per sheet</dt><dd class="font-medium">${
        layout.perSheet
      } (${layout.across} across × ${layout.down} down)</dd></div>
      <div class="flex justify-between"><dt>Quantity needed</dt><dd class="font-medium">${qtyNeeded}</dd></div>
      <div class="flex justify-between"><dt>Sheets required</dt><dd class="font-medium">${sheetsNeeded}</dd></div>
      <div class="flex justify-between"><dt>Total pieces produced</dt><dd class="font-medium">${totalPieces}</dd></div>
      <div class="flex justify-between"><dt>Paper usage per sheet</dt><dd class="font-medium">${formatNumber(
        usagePct,
        1
      )}%</dd></div>
    `;

        detailList.innerHTML = detailHtml;

        buildLayoutGrid(layout.across, layout.down);
        document.getElementById("resultsSection").classList.remove("hidden");
      });

      // Initial layout for default values
      window.addEventListener("load", () => {
        document.getElementById("calculateBtn").click();
      });

      // Mobile menu script (simple)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", () => {
          const menu = document.getElementById("mobile-menu");
          const expanded = menu.classList.contains("hidden") ? "true" : "false";
          menu.classList.toggle("hidden");
          document
            .getElementById("mobile-menu-toggle")
            .setAttribute("aria-expanded", expanded);
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
