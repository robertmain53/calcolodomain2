    <script>
      // Utility functions
      function formatCurrency(value) {
        if (!isFinite(value)) return "$0";
        const abs = Math.abs(value);
        const sign = value < 0 ? "-" : "";
        return (
          sign +
          "$" +
          abs.toLocaleString(undefined, {
            minimumFractionDigits: 0,
            maximumFractionDigits: 2,
          })
        );
      }

      function getPayPeriodsPerYear(freq) {
        switch (freq) {
          case "weekly":
            return 52;
          case "biweekly":
            return 26;
          case "semimonthly":
            return 24;
          case "monthly":
            return 12;
          case "annual":
            return 1;
          default:
            return 12;
        }
      }

      // DOM elements
      const incomeTypeEl = document.getElementById("income-type");
      const payFrequencyEl = document.getElementById("pay-frequency");
      const incomeAmountEl = document.getElementById("income-amount");
      const monthlyIncomeDisplay = document.getElementById(
        "monthly-income-display"
      );

      const needsPercentEl = document.getElementById("needs-percent");
      const wantsPercentEl = document.getElementById("wants-percent");
      const savingsPercentEl = document.getElementById("savings-percent");
      const needsPercentDisplay = document.getElementById(
        "needs-percent-display"
      );
      const wantsPercentDisplay = document.getElementById(
        "wants-percent-display"
      );
      const savingsPercentDisplay = document.getElementById(
        "savings-percent-display"
      );
      const percentWarning = document.getElementById("percent-warning");
      const resetDefaultsBtn = document.getElementById("reset-defaults");

      const needsAmountDisplay = document.getElementById(
        "needs-amount-display"
      );
      const wantsAmountDisplay = document.getElementById(
        "wants-amount-display"
      );
      const savingsAmountDisplay = document.getElementById(
        "savings-amount-display"
      );
      const needsShareDisplay = document.getElementById("needs-share-display");
      const wantsShareDisplay = document.getElementById("wants-share-display");
      const savingsShareDisplay = document.getElementById(
        "savings-share-display"
      );

      const needsMonthlyCell = document.getElementById("needs-monthly-cell");
      const wantsMonthlyCell = document.getElementById("wants-monthly-cell");
      const savingsMonthlyCell = document.getElementById(
        "savings-monthly-cell"
      );
      const needsPerpayCell = document.getElementById("needs-perpay-cell");
      const wantsPerpayCell = document.getElementById("wants-perpay-cell");
      const savingsPerpayCell = document.getElementById("savings-perpay-cell");
      const needsAnnualCell = document.getElementById("needs-annual-cell");
      const wantsAnnualCell = document.getElementById("wants-annual-cell");
      const savingsAnnualCell = document.getElementById("savings-annual-cell");
      const totalMonthlyCell = document.getElementById("total-monthly-cell");
      const totalPerpayCell = document.getElementById("total-perpay-cell");
      const totalAnnualCell = document.getElementById("total-annual-cell");

      const needsRowsContainer = document.getElementById("needs-expense-rows");
      const wantsRowsContainer = document.getElementById("wants-expense-rows");
      const savingsRowsContainer = document.getElementById(
        "savings-expense-rows"
      );
      const addRowButtons = document.querySelectorAll(".add-row-btn");
      const clearExpensesBtn = document.getElementById("clear-expenses");

      const needsActualTotalEl = document.getElementById("needs-actual-total");
      const wantsActualTotalEl = document.getElementById("wants-actual-total");
      const savingsActualTotalEl = document.getElementById(
        "savings-actual-total"
      );
      const needsDiffText = document.getElementById("needs-diff-text");
      const wantsDiffText = document.getElementById("wants-diff-text");
      const savingsDiffText = document.getElementById("savings-diff-text");

      // Expense row helpers
      function createExpenseRow(section, placeholder) {
        const row = document.createElement("div");
        row.className = "grid grid-cols-2 gap-2 items-center";
        row.innerHTML = `
      <input type="text" class="border-gray-300 rounded-md shadow-sm text-xs px-2 py-1" placeholder="${placeholder}">
      <div class="flex items-center justify-end gap-1">
        <span class="text-xs text-gray-500">$</span>
        <input type="number" min="0" step="0.01" class="border-gray-300 rounded-md shadow-sm text-xs px-2 py-1 w-24 text-right expense-amount" value="">
        <button type="button" class="text-xs text-gray-400 hover:text-red-500 remove-row-btn" title="Remove">✕</button>
      </div>
    `;
        // Attach listeners
        const amountInput = row.querySelector(".expense-amount");
        amountInput.addEventListener("input", () => {
          updateAll();
        });
        const removeBtn = row.querySelector(".remove-row-btn");
        removeBtn.addEventListener("click", () => {
          row.remove();
          updateAll();
        });
        return row;
      }

      function initDefaultRows() {
        const defaultNeeds = [
          "Rent / mortgage",
          "Utilities",
          "Groceries",
          "Transport",
          "Insurance",
          "Minimum debt payments",
        ];
        const defaultWants = [
          "Dining out",
          "Streaming / subscriptions",
          "Shopping",
          "Entertainment",
          "Travel",
        ];
        const defaultSavings = [
          "Emergency fund",
          "Retirement",
          "Investments",
          "Extra debt payments",
        ];

        defaultNeeds.forEach((ph) =>
          needsRowsContainer.appendChild(createExpenseRow("needs", ph))
        );
        defaultWants.forEach((ph) =>
          wantsRowsContainer.appendChild(createExpenseRow("wants", ph))
        );
        defaultSavings.forEach((ph) =>
          savingsRowsContainer.appendChild(createExpenseRow("savings", ph))
        );
      }

      // Percent slider logic: keep total at 100 by auto-adjusting the last moved slider's complement
      function enforcePercentTotal(changedEl) {
        const n = parseInt(needsPercentEl.value, 10) || 0;
        const w = parseInt(wantsPercentEl.value, 10) || 0;
        const s = parseInt(savingsPercentEl.value, 10) || 0;
        let total = n + w + s;

        if (total === 100) {
          percentWarning.classList.add("hidden");
          return;
        }

        // Auto-adjust the last slider in the trio (excluding the one just changed)
        let others = [];
        if (changedEl === needsPercentEl) {
          others = [wantsPercentEl, savingsPercentEl];
        } else if (changedEl === wantsPercentEl) {
          others = [needsPercentEl, savingsPercentEl];
        } else {
          others = [needsPercentEl, wantsPercentEl];
        }

        const diff = total - 100; // positive if too high
        // Adjust the second slider in "others" first to feel more stable
        const targetEl = others[1];

        let newVal = parseInt(targetEl.value, 10) - diff;
        if (newVal < 0) newVal = 0;
        if (newVal > 100) newVal = 100;
        targetEl.value = newVal;

        // Recompute; if still off due to clamping, adjust the remaining slider
        const n2 = parseInt(needsPercentEl.value, 10) || 0;
        const w2 = parseInt(wantsPercentEl.value, 10) || 0;
        const s2 = parseInt(savingsPercentEl.value, 10) || 0;
        let total2 = n2 + w2 + s2;
        if (total2 !== 100) {
          const lastEl = others[0];
          let lastVal = parseInt(lastEl.value, 10) - (total2 - 100);
          if (lastVal < 0) lastVal = 0;
          if (lastVal > 100) lastVal = 100;
          lastEl.value = lastVal;
        }

        percentWarning.classList.remove("hidden");
      }

      function getMonthlyIncome() {
        const perPay = parseFloat(incomeAmountEl.value) || 0;
        const freq = payFrequencyEl.value;
        const perYear = perPay * getPayPeriodsPerYear(freq);
        return perYear / 12;
      }

      function sumSection(container) {
        let sum = 0;
        container.querySelectorAll(".expense-amount").forEach((input) => {
          const v = parseFloat(input.value);
          if (!isNaN(v) && v >= 0) sum += v;
        });
        return sum;
      }

      function updateAll() {
        const monthlyIncome = getMonthlyIncome();
        monthlyIncomeDisplay.textContent = formatCurrency(monthlyIncome);

        const nPct = parseInt(needsPercentEl.value, 10) || 0;
        const wPct = parseInt(wantsPercentEl.value, 10) || 0;
        const sPct = parseInt(savingsPercentEl.value, 10) || 0;

        needsPercentDisplay.textContent = nPct;
        wantsPercentDisplay.textContent = wPct;
        savingsPercentDisplay.textContent = sPct;

        const needsTarget = monthlyIncome * (nPct / 100);
        const wantsTarget = monthlyIncome * (wPct / 100);
        const savingsTarget = monthlyIncome * (sPct / 100);

        needsAmountDisplay.textContent = formatCurrency(needsTarget);
        wantsAmountDisplay.textContent = formatCurrency(wantsTarget);
        savingsAmountDisplay.textContent = formatCurrency(savingsTarget);

        needsShareDisplay.textContent = `${nPct}% of income`;
        wantsShareDisplay.textContent = `${wPct}% of income`;
        savingsShareDisplay.textContent = `${sPct}% of income`;

        const freq = payFrequencyEl.value;
        const perPayPeriods = getPayPeriodsPerYear(freq);
        const perPayIncome = perPayPeriods
          ? (monthlyIncome * 12) / perPayPeriods
          : 0;

        needsMonthlyCell.textContent = formatCurrency(needsTarget);
        wantsMonthlyCell.textContent = formatCurrency(wantsTarget);
        savingsMonthlyCell.textContent = formatCurrency(savingsTarget);

        needsPerpayCell.textContent = formatCurrency(
          perPayIncome * (nPct / 100)
        );
        wantsPerpayCell.textContent = formatCurrency(
          perPayIncome * (wPct / 100)
        );
        savingsPerpayCell.textContent = formatCurrency(
          perPayIncome * (sPct / 100)
        );

        needsAnnualCell.textContent = formatCurrency(needsTarget * 12);
        wantsAnnualCell.textContent = formatCurrency(wantsTarget * 12);
        savingsAnnualCell.textContent = formatCurrency(savingsTarget * 12);

        totalMonthlyCell.textContent = formatCurrency(monthlyIncome);
        totalPerpayCell.textContent = formatCurrency(perPayIncome);
        totalAnnualCell.textContent = formatCurrency(monthlyIncome * 12);

        // Actual expenses
        const needsActual = sumSection(needsRowsContainer);
        const wantsActual = sumSection(wantsRowsContainer);
        const savingsActual = sumSection(savingsRowsContainer);

        needsActualTotalEl.textContent = formatCurrency(needsActual);
        wantsActualTotalEl.textContent = formatCurrency(wantsActual);
        savingsActualTotalEl.textContent = formatCurrency(savingsActual);

        const needsDiff = needsActual - needsTarget;
        const wantsDiff = wantsActual - wantsTarget;
        const savingsDiff = savingsActual - savingsTarget;

        function diffMessage(diff, label) {
          if (Math.abs(diff) < 0.01)
            return `You are right on target for ${label}.`;
          if (diff > 0)
            return `You are about ${formatCurrency(
              diff
            )} over the ${label} target.`;
          return `You are about ${formatCurrency(
            -diff
          )} under the ${label} target.`;
        }

        needsDiffText.textContent = diffMessage(needsDiff, "needs");
        wantsDiffText.textContent = diffMessage(wantsDiff, "wants");
        savingsDiffText.textContent = diffMessage(
          savingsDiff,
          "savings & debt"
        );
      }

      // Event listeners
      [incomeTypeEl, payFrequencyEl].forEach((el) => {
        el.addEventListener("change", updateAll);
      });
      incomeAmountEl.addEventListener("input", updateAll);

      [needsPercentEl, wantsPercentEl, savingsPercentEl].forEach((el) => {
        el.addEventListener("input", (e) => {
          enforcePercentTotal(e.target);
          updateAll();
        });
      });

      resetDefaultsBtn.addEventListener("click", () => {
        needsPercentEl.value = 50;
        wantsPercentEl.value = 30;
        savingsPercentEl.value = 20;
        percentWarning.classList.add("hidden");
        updateAll();
      });

      addRowButtons.forEach((btn) => {
        btn.addEventListener("click", () => {
          const section = btn.getAttribute("data-section");
          let container, placeholder;
          if (section === "needs") {
            container = needsRowsContainer;
            placeholder = "New need";
          } else if (section === "wants") {
            container = wantsRowsContainer;
            placeholder = "New want";
          } else {
            container = savingsRowsContainer;
            placeholder = "New savings item";
          }
          container.appendChild(createExpenseRow(section, placeholder));
          updateAll();
        });
      });

      clearExpensesBtn.addEventListener("click", () => {
        document.querySelectorAll(".expense-amount").forEach((input) => {
          input.value = "";
        });
        updateAll();
      });

      // Initialize
      initDefaultRows();
      updateAll();
    </script>

