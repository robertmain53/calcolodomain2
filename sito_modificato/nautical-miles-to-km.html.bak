    <script>
      (function () {
        const FACTOR = 1.852; // 1 nm = 1.852 km (exact)

        const nmInput = document.getElementById("nmInput");
        const kmInput = document.getElementById("kmInput");
        const swapBtn = document.getElementById("swapBtn");
        const clearBtn = document.getElementById("clearBtn");
        const summaryBox = document.getElementById("summaryBox");
        const tableBody = document.getElementById("nmKmTableBody");
        const presetButtons = document.querySelectorAll("[data-preset-nm]");

        let activeField = null; // 'nm' or 'km' to prevent feedback loops

        function formatNumber(value, decimals) {
          if (isNaN(value) || value === null) return "";
          return Number(value)
            .toFixed(decimals)
            .replace(/\.0+$/, "")
            .replace(/(\.\d*?[1-9])0+$/, "$1");
        }

        function updateSummary(from, fromValue, toValue) {
          if (fromValue === "" || isNaN(fromValue) || isNaN(toValue)) {
            summaryBox.textContent =
              "Enter a value above to see the conversion here.";
            return;
          }

          const fromLabel = from === "nm" ? "nautical miles" : "kilometers";
          const toLabel = from === "nm" ? "kilometers" : "nautical miles";
          const fromUnit = from === "nm" ? "nm" : "km";
          const toUnit = from === "nm" ? "km" : "nm";

          summaryBox.innerHTML = `
        <p><strong>${formatNumber(
          fromValue,
          6
        )} ${fromUnit}</strong> is equal to
        <strong>${formatNumber(toValue, 6)} ${toUnit}</strong>.</p>
        <p class="mt-1 text-xs text-gray-600">
          Using exact factor: 1 nm = 1.852 km (${
            from === "nm" ? "km = nm × 1.852" : "nm = km ÷ 1.852"
          }).
        </p>
      `;
        }

        function convertFromNm() {
          if (activeField === "km") return;
          activeField = "nm";
          const nmVal = parseFloat(nmInput.value);
          if (isNaN(nmVal)) {
            kmInput.value = "";
            updateSummary("nm", "", "");
            activeField = null;
            return;
          }
          const kmVal = nmVal * FACTOR;
          kmInput.value = formatNumber(kmVal, 6);
          updateSummary("nm", nmVal, kmVal);
          activeField = null;
        }

        function convertFromKm() {
          if (activeField === "nm") return;
          activeField = "km";
          const kmVal = parseFloat(kmInput.value);
          if (isNaN(kmVal)) {
            nmInput.value = "";
            updateSummary("km", "", "");
            activeField = null;
            return;
          }
          const nmVal = kmVal / FACTOR;
          nmInput.value = formatNumber(nmVal, 6);
          updateSummary("km", kmVal, nmVal);
          activeField = null;
        }

        nmInput.addEventListener("input", convertFromNm);
        kmInput.addEventListener("input", convertFromKm);

        swapBtn.addEventListener("click", function () {
          const nmVal = nmInput.value;
          const kmVal = kmInput.value;
          nmInput.value = kmVal;
          kmInput.value = nmVal;
          if (nmInput.value !== "") {
            convertFromNm();
          } else if (kmInput.value !== "") {
            convertFromKm();
          } else {
            updateSummary("nm", "", "");
          }
        });

        clearBtn.addEventListener("click", function () {
          nmInput.value = "";
          kmInput.value = "";
          updateSummary("nm", "", "");
          nmInput.focus();
        });

        presetButtons.forEach((btn) => {
          btn.addEventListener("click", () => {
            const nmVal = parseFloat(btn.getAttribute("data-preset-nm"));
            nmInput.value = nmVal;
            convertFromNm();
            nmInput.focus();
          });
        });

        function populateTable() {
          const presetValues = [1, 2, 3, 5, 10, 20, 50, 100, 250, 500, 1000];
          const rows = presetValues
            .map((nm) => {
              const km = nm * FACTOR;
              return `
          <tr>
            <td class="px-3 py-2 whitespace-nowrap">${formatNumber(
              nm,
              3
            )} nm</td>
            <td class="px-3 py-2 whitespace-nowrap">${formatNumber(
              km,
              3
            )} km</td>
          </tr>
        `;
            })
            .join("");
          tableBody.innerHTML = rows;
        }

        populateTable();
      })();
    </script>

