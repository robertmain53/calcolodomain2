  <script>
    // mobile menu toggle
    const mobileToggle = document.getElementById('mobile-menu-toggle');
    const mobileMenu = document.getElementById('mobile-menu');
    if (mobileToggle && mobileMenu) {
      mobileToggle.addEventListener('click', () => {
        const isHidden = mobileMenu.classList.contains('hidden');
        mobileMenu.classList.toggle('hidden', !isHidden);
        mobileToggle.setAttribute('aria-expanded', String(isHidden));
      });
    }

    function toRadians(value, unit) {
      return unit === 'deg' ? (value * Math.PI / 180) : value;
    }

    function formatNumber(num) {
      if (isNaN(num)) return '–';
      return Number(num.toFixed(6)).toString();
    }

    document.getElementById('convert-btn').addEventListener('click', function() {
      const r = parseFloat(document.getElementById('radius').value);
      const theta = parseFloat(document.getElementById('theta').value);
      const unit = document.getElementById('angle-unit').value;

      if (isNaN(r) || isNaN(theta)) {
        alert('Please enter valid numeric values for r and θ.');
        return;
      }

      const thetaRad = toRadians(theta, unit);
      const x = r * Math.cos(thetaRad);
      const y = r * Math.sin(thetaRad);

      document.getElementById('res-x').textContent = formatNumber(x);
      document.getElementById('res-y').textContent = formatNumber(y);
    });
  </script>

