    <script>
      // Utility functions for IPv4
      function parseIp(ipStr) {
        const parts = ipStr.trim().split(".");
        if (parts.length !== 4) return null;
        let val = 0;
        for (let i = 0; i < 4; i++) {
          if (!/^\d+$/.test(parts[i])) return null;
          const n = Number(parts[i]);
          if (n < 0 || n > 255) return null;
          val = (val << 8) | n;
        }
        return val >>> 0;
      }

      function intToIp(intVal) {
        return [
          (intVal >>> 24) & 255,
          (intVal >>> 16) & 255,
          (intVal >>> 8) & 255,
          intVal & 255,
        ].join(".");
      }

      function cidrToMask(prefix) {
        if (prefix < 0 || prefix > 32) return null;
        if (prefix === 0) return 0;
        return (0xffffffff << (32 - prefix)) >>> 0;
      }

      function maskToCidr(maskInt) {
        let bits = 0;
        let seenZero = false;
        for (let i = 31; i >= 0; i--) {
          const bit = (maskInt >>> i) & 1;
          if (bit === 1) {
            if (seenZero) return null; // non-contiguous
            bits++;
          } else {
            seenZero = true;
          }
        }
        return bits;
      }

      function maskToWildcard(maskInt) {
        return ~maskInt >>> 0;
      }

      function ipToBinaryString(intVal) {
        const parts = [];
        for (let i = 3; i >= 0; i--) {
          const octet = (intVal >>> (i * 8)) & 255;
          parts.push(octet.toString(2).padStart(8, "0"));
        }
        return parts.join(" ");
      }

      function detectClass(ipInt) {
        const first = (ipInt >>> 24) & 255;
        if (first <= 127) return "Class A";
        if (first <= 191) return "Class B";
        if (first <= 223) return "Class C";
        if (first <= 239) return "Class D (multicast)";
        return "Class E (experimental)";
      }

      function detectType(ipInt) {
        const first = (ipInt >>> 24) & 255;
        const second = (ipInt >>> 16) & 255;

        // Private ranges
        const is10 = first === 10;
        const is172 = first === 172 && second >= 16 && second <= 31;
        const is192168 = first === 192 && second === 168;

        if (is10 || is172 || is192168) return "Private";
        if (first === 127) return "Loopback";
        if (first === 169 && second === 254) return "Link-local (APIPA)";
        if (first === 0 || first === 255) return "Special use";
        return "Public";
      }

      function reverseDns(ipInt) {
        const a = (ipInt >>> 24) & 255;
        const b = (ipInt >>> 16) & 255;
        const c = (ipInt >>> 8) & 255;
        const d = ipInt & 255;
        return [d, c, b, a].join(".") + ".in-addr.arpa";
      }

      function buildMaskOptions() {
        const select = document.getElementById("maskSelect");
        const common = [8, 16, 24, 25, 26, 27, 28, 29, 30];
        const all = [];
        for (let p = 0; p <= 32; p++) all.push(p);

        const groups = [
          { label: "Common prefixes", list: common },
          { label: "All prefixes", list: all },
        ];

        groups.forEach((group) => {
          const optgroup = document.createElement("optgroup");
          optgroup.label = group.label;
          group.list.forEach((prefix) => {
            const maskInt = cidrToMask(prefix);
            const opt = document.createElement("option");
            opt.value = prefix;
            opt.textContent = `/${prefix} – ${intToIp(maskInt)}`;
            optgroup.appendChild(opt);
          });
          select.appendChild(optgroup);
        });
        select.value = "24";
      }

      function parseInput() {
        const raw = document.getElementById("ipInput").value.trim();
        const maskSelect = document.getElementById("maskSelect");
        if (!raw) return { error: "Please enter an IPv4 address." };

        let ipPart = raw;
        let cidrPart = null;

        const cidrMatch = raw.match(/^(.+?)\/(\d{1,2})$/);
        if (cidrMatch) {
          ipPart = cidrMatch[1].trim();
          cidrPart = Number(cidrMatch[2]);
          if (cidrPart < 0 || cidrPart > 32) {
            return { error: "CIDR prefix must be between 0 and 32." };
          }
        }

        const ipInt = parseIp(ipPart);
        if (ipInt === null) return { error: "Invalid IPv4 address format." };

        let prefix = cidrPart;
        if (prefix == null) {
          prefix = Number(maskSelect.value);
          if (isNaN(prefix) || prefix < 0 || prefix > 32) {
            return { error: "Invalid subnet mask / CIDR prefix." };
          }
        }

        const maskInt = cidrToMask(prefix);
        if (maskInt === null)
          return { error: "Invalid subnet mask derived from CIDR." };

        return { ipInt, prefix, maskInt, ipStr: intToIp(ipInt) };
      }

      function formatNumber(n) {
        return n.toLocaleString("en-US");
      }

      function calculate() {
        const errorBox = document.getElementById("errorBox");
        errorBox.classList.add("hidden");
        errorBox.textContent = "";

        const parsed = parseInput();
        if (parsed.error) {
          errorBox.textContent = parsed.error;
          errorBox.classList.remove("hidden");
          clearResults(false);
          return;
        }

        const { ipInt, prefix, maskInt, ipStr } = parsed;

        const network = (ipInt & maskInt) >>> 0;
        const wildcard = maskToWildcard(maskInt);
        const broadcast = (network | wildcard) >>> 0;

        const total =
          prefix === 32
            ? 1
            : prefix === 0
            ? Math.pow(2, 32)
            : Math.pow(2, 32 - prefix);
        let usable;
        let firstHost, lastHost;

        if (prefix === 31) {
          usable = 2;
          firstHost = network;
          lastHost = broadcast;
        } else if (prefix === 32) {
          usable = 1;
          firstHost = lastHost = network;
        } else {
          usable = total - 2;
          firstHost = network + 1;
          lastHost = broadcast - 1;
        }

        // Fill summary
        document.getElementById("resIp").textContent = ipStr;
        document.getElementById("resNetwork").textContent = intToIp(network);
        document.getElementById("resBroadcast").textContent =
          intToIp(broadcast);
        document.getElementById("resFirstHost").textContent =
          firstHost != null ? intToIp(firstHost >>> 0) : "–";
        document.getElementById("resLastHost").textContent =
          lastHost != null ? intToIp(lastHost >>> 0) : "–";

        document.getElementById("resMask").textContent = intToIp(maskInt);
        document.getElementById("resCidr").textContent = "/" + prefix;
        document.getElementById("resWildcard").textContent = intToIp(wildcard);
        document.getElementById("resTotal").textContent = formatNumber(total);
        document.getElementById("resUsable").textContent = formatNumber(usable);

        document.getElementById("resClass").textContent = detectClass(ipInt);
        document.getElementById("resType").textContent = detectType(ipInt);
        document.getElementById("resReverse").textContent = reverseDns(ipInt);

        document.getElementById("binIp").textContent = ipToBinaryString(ipInt);
        document.getElementById("binMask").textContent =
          ipToBinaryString(maskInt);

        // Host listing (limit to 1024 addresses)
        const hostsSection = document.getElementById("hostsSection");
        const hostsBody = document.getElementById("hostsTableBody");
        const hostsNote = document.getElementById("hostsNote");
        hostsBody.innerHTML = "";

        const includeNetBroadcast = document.getElementById(
          "includeNetBroadcast"
        ).checked;

        if (total <= 1024) {
          let start = includeNetBroadcast ? network : firstHost;
          let end = includeNetBroadcast ? broadcast : lastHost;

          if (start == null || end == null) {
            hostsSection.classList.add("hidden");
            hostsNote.textContent = "";
            return;
          }

          let idx = 1;
          for (let addr = start; addr <= end; addr++) {
            const tr = document.createElement("tr");
            const role =
              addr === network && !includeNetBroadcast && prefix <= 30
                ? "Network"
                : addr === broadcast && !includeNetBroadcast && prefix <= 30
                ? "Broadcast"
                : addr === network && prefix <= 30
                ? "Network"
                : addr === broadcast && prefix <= 30
                ? "Broadcast"
                : "Host";

            tr.innerHTML = `
          <td class="px-3 py-1 text-gray-600">${idx}</td>
          <td class="px-3 py-1">${intToIp(addr >>> 0)}</td>
          <td class="px-3 py-1 text-gray-600">${role}</td>
        `;
            hostsBody.appendChild(tr);
            idx++;
          }
          hostsSection.classList.remove("hidden");
          hostsNote.textContent = `Showing ${formatNumber(
            end - start + 1
          )} address(es).`;
        } else {
          hostsSection.classList.add("hidden");
          hostsNote.textContent = "";
        }
      }

      function clearResults(clearInput = true) {
        if (clearInput) {
          document.getElementById("ipInput").value = "";
        }
        const fields = [
          "resIp",
          "resNetwork",
          "resBroadcast",
          "resFirstHost",
          "resLastHost",
          "resMask",
          "resCidr",
          "resWildcard",
          "resTotal",
          "resUsable",
          "resClass",
          "resType",
          "resReverse",
          "binIp",
          "binMask",
        ];
        fields.forEach((id) => {
          const el = document.getElementById(id);
          if (el) el.textContent = "–";
        });
        document.getElementById("hostsTableBody").innerHTML = "";
        document.getElementById("hostsSection").classList.add("hidden");
        document.getElementById("errorBox").classList.add("hidden");
        document.getElementById("errorBox").textContent = "";
      }

      document.addEventListener("DOMContentLoaded", () => {
        buildMaskOptions();

        const calcBtn = document.getElementById("calcBtn");
        const clearBtn = document.getElementById("clearBtn");
        const ipInput = document.getElementById("ipInput");
        const maskSelect = document.getElementById("maskSelect");
        const autoCalc = document.getElementById("autoCalc");

        calcBtn.addEventListener("click", calculate);
        clearBtn.addEventListener("click", () => clearResults(true));

        function maybeAutoCalc() {
          if (autoCalc.checked) {
            calculate();
          }
        }

        ipInput.addEventListener("input", maybeAutoCalc);
        maskSelect.addEventListener("change", maybeAutoCalc);

        ipInput.addEventListener("keydown", (e) => {
          if (e.key === "Enter") {
            e.preventDefault();
            calculate();
          }
        });

        // Prefill with a common example
        ipInput.value = "192.168.1.10/24";
        calculate();
      });
    </script>

