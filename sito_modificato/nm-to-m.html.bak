  <script>
    const nmInput = document.getElementById('nm-input');
    const mOutput = document.getElementById('m-output');
    const umOutput = document.getElementById('um-output');
    const mmOutput = document.getElementById('mm-output');
    const cmOutput = document.getElementById('cm-output');
    const sciOutput = document.getElementById('sci-output');

    const mInput = document.getElementById('m-input');
    const nmOutput = document.getElementById('nm-output');

    const presetBtns = document.querySelectorAll('.pill');

    let isUpdating = false;

    function formatSci(num) {
      if (num === 0) return "0";
      const exp = Math.floor(Math.log10(Math.abs(num)));
      const mant = num / Math.pow(10, exp);
      return mant.toFixed(2) + "×10^" + exp;
    }

    function updateFromNm() {
      if (isUpdating) return;
      isUpdating = true;
      const nm = parseFloat(nmInput.value || 0);
      const m = nm / 1e9;
      const um = nm / 1e3;
      const mm = nm / 1e6;
      const cm = nm / 1e7;

      mOutput.value = m.toPrecision(6);
      umOutput.textContent = um.toPrecision(6);
      mmOutput.textContent = mm.toPrecision(6);
      cmOutput.textContent = cm.toPrecision(6);
      sciOutput.textContent = formatSci(m).replace("^", "⁻").replace("10⁻", "10^"); // small hack

      // also update reverse box
      mInput.value = m;
      nmOutput.value = nm;
      isUpdating = false;
    }

    function updateFromMeters() {
      if (isUpdating) return;
      isUpdating = true;
      const m = parseFloat(mInput.value || 0);
      const nm = m * 1e9;
      nmOutput.value = Math.round(nm);
      // reflect upwards
      nmInput.value = Math.round(nm);
      updateFromNm();
      isUpdating = false;
    }

    presetBtns.forEach(btn => {
      btn.addEventListener('click', () => {
        presetBtns.forEach(b => b.classList.remove('pill-active'));
        btn.classList.add('pill-active');
        const v = parseFloat(btn.dataset.val);
        nmInput.value = v;
        updateFromNm();
      });
    });

    nmInput.addEventListener('input', updateFromNm);
    mInput.addEventListener('input', updateFromMeters);

    // initial
    updateFromNm();
  </script>

