            <script>
              document.addEventListener('DOMContentLoaded', function() {
                const radiusInput = document.getElementById('radius-input');
                const heightInput = document.getElementById('height-input');
                const calculateBtn = document.getElementById('calculate-btn');
                const resultsContainer = document.getElementById('results-container');
                const errorMessage = document.getElementById('error-message');
                
                const resultVolume = document.getElementById('result-volume');
                const resultSlantHeight = document.getElementById('result-slant-height');
                const resultBaseArea = document.getElementById('result-base-area');
                const resultLateralArea = document.getElementById('result-lateral-area');
                const resultTotalArea = document.getElementById('result-total-area');
                const stepByStep = document.getElementById('step-by-step');

                // --- Helper Functions ---
                function parseInput(val) {
                    const num = parseFloat(val);
                    return (num >= 0 && !isNaN(num)) ? num : NaN;
                }
                
                function formatNumber(num) {
                    if (Math.abs(num) < 1e-10) return '0';
                    return parseFloat(num.toFixed(6)).toString();
                }

                function showError(message) {
                    errorMessage.textContent = `Error: ${message}`;
                    resultsContainer.classList.add('hidden');
                }

                // --- Main Calculation Logic ---
                calculateBtn.addEventListener('click', () => {
                    const r = parseInput(radiusInput.value);
                    const h = parseInput(heightInput.value);
                    
                    try {
                        if (isNaN(r) || isNaN(h)) {
                            throw new Error('Please enter valid, non-negative numbers for Radius (r) and Height (h).');
                        }
                        
                        errorMessage.textContent = '';
                        
                        // 1. Slant Height (l)
                        const l = Math.sqrt(r * r + h * h);
                        
                        // 2. Base Area (B)
                        const B = Math.PI * r * r;
                        
                        // 3. Lateral Area (L)
                        const L = Math.PI * r * l;
                        
                        // 4. Total Surface Area (A)
                        const A = B + L;
                        
                        // 5. Volume (V)
                        const V = (1/3) * B * h;
                        
                        // --- Output Construction ---
                        
                        // A. Slant Height Step
                        let steps = `
                            <h4 class="font-semibold text-lg">1. Calculate Slant Height ($l$):</h4>
                            <p>Using Pythagorean Theorem: $l = \\sqrt{r^2 + h^2}$</p>
                            <p class="formula-box text-center">$l = \\sqrt{${formatNumber(r)}^2 + ${formatNumber(h)}^2} = \\sqrt{${formatNumber(r*r)} + ${formatNumber(h*h)}} = \\sqrt{${formatNumber(r*r + h*h)}} \\approx ${formatNumber(l)}$</p>
                            <p>$l = ${formatNumber(l)}$</p>
                        `;

                        // B. Base Area Step
                         steps += `
                            <h4 class="font-semibold text-lg mt-4">2. Calculate Base Area ($B$):</h4>
                            <p>Formula: $B = \\pi r^2$</p>
                            <p class="formula-box text-center">$B = \\pi (${formatNumber(r)})^2 = ${formatNumber(r*r)} \\pi \\approx ${formatNumber(B)}$</p>
                            <p>$B = ${formatNumber(B)}$</p>
                        `;

                        // C. Lateral Area Step
                         steps += `
                            <h4 class="font-semibold text-lg mt-4">3. Calculate Lateral Surface Area ($L$):</h4>
                            <p>Formula: $L = \\pi r l$</p>
                            <p class="formula-box text-center">$L = \\pi (${formatNumber(r)}) (${formatNumber(l)}) = ${formatNumber(r * l)} \\pi \\approx ${formatNumber(L)}$</p>
                            <p>$L = ${formatNumber(L)}$</p>
                        `;
                        
                        // D. Total Area Step
                         steps += `
                            <h4 class="font-semibold text-lg mt-4">4. Calculate Total Surface Area ($A$):</h4>
                            <p>Formula: $A = B + L$</p>
                            <p class="formula-box text-center">$A = ${formatNumber(B)} + ${formatNumber(L)} \\approx ${formatNumber(A)}$</p>
                            <p>$A = ${formatNumber(A)}$</p>
                        `;

                        // E. Volume Step
                         steps += `
                            <h4 class="font-semibold text-lg mt-4">5. Calculate Volume ($V$):</h4>
                            <p>Formula: $V = \\frac{1}{3} B h$</p>
                            <p class="formula-box text-center">$V = \\frac{1}{3} (${formatNumber(B)}) (${formatNumber(h)}) \\approx ${formatNumber(V)}$</p>
                            <p>$V = ${formatNumber(V)}$</p>
                        `;


                        // Update result containers
                        resultSlantHeight.textContent = formatNumber(l);
                        resultBaseArea.textContent = formatNumber(B);
                        resultLateralArea.textContent = formatNumber(L);
                        resultTotalArea.textContent = formatNumber(A);
                        resultVolume.textContent = formatNumber(V);
                        stepByStep.innerHTML = steps;
                        resultsContainer.classList.remove('hidden');

                        // Re-render MathJax
                        if (window.MathJax) {
                            window.MathJax.typesetPromise([stepByStep, resultVolume]);
                        }

                    } catch (e) {
                        showError(e.message);
                    }
                });

                // Initial calculation (for example values)
                calculateBtn.click();
            </script>

