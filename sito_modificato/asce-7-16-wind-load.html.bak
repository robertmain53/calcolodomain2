    <script>
      // Utility: approximate Kz based on exposure and height (simplified from ASCE 7-16)
      function getKz(exposure, h) {
        h = Math.max(15, Math.min(h, 150)); // clamp for our table
        let a, zmin;
        if (exposure === "B") {
          a = 0.67;
          zmin = 15;
        } else if (exposure === "C") {
          a = 0.85;
          zmin = 15;
        } else {
          // D
          a = 1.03;
          zmin = 15;
        }
        const z = Math.max(h, zmin);
        // Simplified power law: Kz = 2.01 * (z/zg)^(2/alpha) but we use a tuned exponent
        const alpha = exposure === "B" ? 7 : exposure === "C" ? 9.5 : 11.5;
        const zg = exposure === "B" ? 1200 : exposure === "C" ? 900 : 700;
        const Kz = 2.01 * Math.pow(z / zg, 2 / alpha);
        return Math.max(0.4, Math.min(Kz, 2.5));
      }

      // Typical importance factor by risk category (ultimate)
      function getIw(risk) {
        switch (risk) {
          case "I":
            return 0.87;
          case "III":
            return 1.15;
          case "IV":
            return 1.15;
          case "II":
          default:
            return 1.0;
        }
      }

      // Representative GCp values for MWFRS (simplified)
      function getGCpSet() {
        return {
          windwardWall: 0.8,
          leewardWall: -0.5,
          roofEdge: -1.3,
          roofInterior: -0.9,
        };
      }

      function formatPsf(value) {
        if (!isFinite(value)) return "-";
        return value.toFixed(1) + " psf";
      }

      function buildResultsHtml(data) {
        const {
          qz,
          kz,
          iw,
          kd,
          kzt,
          v,
          gcpi,
          gcps,
          pWindward,
          pLeeward,
          pRoofEdge,
          pRoofInterior,
        } = data;

        return `
      <div class="border border-blue-100 rounded-lg p-4 bg-blue-50">
        <div class="flex items-center justify-between mb-2">
          <div>
            <p class="result-badge text-blue-700 font-semibold">Summary</p>
            <p class="text-sm text-gray-700">Design wind pressures at mean roof height</p>
          </div>
        </div>
        <dl class="grid md:grid-cols-3 gap-4 text-sm mt-2">
          <div class="bg-white rounded-md p-3 shadow-sm">
            <dt class="text-gray-500">Velocity pressure q<sub>z</sub></dt>
            <dd class="text-lg font-semibold text-gray-900">${qz.toFixed(
              1
            )} psf</dd>
          </div>
          <div class="bg-white rounded-md p-3 shadow-sm">
            <dt class="text-gray-500">Basic wind speed V</dt>
            <dd class="text-lg font-semibold text-gray-900">${v.toFixed(
              1
            )} mph</dd>
          </div>
          <div class="bg-white rounded-md p-3 shadow-sm">
            <dt class="text-gray-500">Exposure coefficient K<sub>z</sub></dt>
            <dd class="text-lg font-semibold text-gray-900">${kz.toFixed(
              3
            )}</dd>
          </div>
        </dl>
      </div>

      <div class="mt-4 grid md:grid-cols-2 gap-4 text-sm">
        <div class="border rounded-lg p-4">
          <p class="result-badge text-gray-700 font-semibold mb-2">Input factors</p>
          <ul class="space-y-1 text-gray-700">
            <li><span class="font-medium">Importance factor I<sub>w</sub>:</span> ${iw.toFixed(
              2
            )}</li>
            <li><span class="font-medium">Directionality factor K<sub>d</sub>:</span> ${kd.toFixed(
              2
            )}</li>
            <li><span class="font-medium">Topographic factor K<sub>zt</sub>:</span> ${kzt.toFixed(
              2
            )}</li>
            <li><span class="font-medium">Internal pressure GC<sub>pi</sub>:</span> ${gcpi.toFixed(
              2
            )}</li>
          </ul>
        </div>
        <div class="border rounded-lg p-4">
          <p class="result-badge text-gray-700 font-semibold mb-2">External coefficients (GC<sub>p</sub>)</p>
          <ul class="space-y-1 text-gray-700">
            <li><span class="font-medium">Windward wall:</span> ${gcps.windwardWall.toFixed(
              2
            )}</li>
            <li><span class="font-medium">Leeward wall:</span> ${gcps.leewardWall.toFixed(
              2
            )}</li>
            <li><span class="font-medium">Roof edge zone:</span> ${gcps.roofEdge.toFixed(
              2
            )}</li>
            <li><span class="font-medium">Roof interior zone:</span> ${gcps.roofInterior.toFixed(
              2
            )}</li>
          </ul>
        </div>
      </div>

      <div class="mt-6">
        <p class="result-badge text-gray-700 font-semibold mb-2">Design pressures (net, including internal pressure)</p>
        <div class="overflow-x-auto">
          <table class="min-w-full text-sm border border-gray-200">
            <thead class="bg-gray-100">
              <tr>
                <th class="px-3 py-2 text-left font-semibold text-gray-700 border-b">Surface</th>
                <th class="px-3 py-2 text-right font-semibold text-gray-700 border-b">Pressure p (psf)</th>
                <th class="px-3 py-2 text-left font-semibold text-gray-700 border-b">Sense</th>
              </tr>
            </thead>
            <tbody>
              <tr class="border-b">
                <td class="px-3 py-2">Windward wall</td>
                <td class="px-3 py-2 text-right">${formatPsf(pWindward)}</td>
                <td class="px-3 py-2">${
                  pWindward >= 0
                    ? "Pressure (toward surface)"
                    : "Suction (away from surface)"
                }</td>
              </tr>
              <tr class="border-b">
                <td class="px-3 py-2">Leeward wall</td>
                <td class="px-3 py-2 text-right">${formatPsf(pLeeward)}</td>
                <td class="px-3 py-2">${
                  pLeeward >= 0 ? "Pressure" : "Suction"
                }</td>
              </tr>
              <tr class="border-b">
                <td class="px-3 py-2">Roof edge zone</td>
                <td class="px-3 py-2 text-right">${formatPsf(pRoofEdge)}</td>
                <td class="px-3 py-2">${
                  pRoofEdge >= 0 ? "Pressure" : "Suction"
                }</td>
              </tr>
              <tr>
                <td class="px-3 py-2">Roof interior zone</td>
                <td class="px-3 py-2 text-right">${formatPsf(
                  pRoofInterior
                )}</td>
                <td class="px-3 py-2">${
                  pRoofInterior >= 0 ? "Pressure" : "Suction"
                }</td>
              </tr>
            </tbody>
          </table>
        </div>
        <p class="mt-2 text-xs text-gray-500">
          Positive values act toward the surface (pressure); negative values act away from the surface (suction).
          Use the more critical of positive and negative internal pressure cases for design.
        </p>
      </div>

      <div class="mt-4 text-xs text-gray-500">
        <p><strong>Note:</strong> Coefficients are representative for educational and preliminary design use only. For final design, use exact values from ASCE 7-16 for your building geometry, height, and exposure, and consult a licensed professional engineer.</p>
      </div>
    `;
      }

      document
        .getElementById("calculateBtn")
        .addEventListener("click", function () {
          const v = parseFloat(document.getElementById("v").value);
          const risk = document.getElementById("riskCategory").value;
          const exposure = document.getElementById("exposure").value;
          const h = parseFloat(document.getElementById("height").value);
          let iwInput = parseFloat(
            document.getElementById("importanceFactor").value
          );
          let kd = parseFloat(document.getElementById("kd").value);
          let kzt = parseFloat(document.getElementById("kzt").value);
          const gcpi = parseFloat(document.getElementById("gcpi").value);

          if (!isFinite(v) || !isFinite(h)) {
            alert(
              "Please enter at least basic wind speed and mean roof height."
            );
            return;
          }

          if (!isFinite(iwInput)) iwInput = getIw(risk);
          if (!isFinite(kd)) kd = 0.85;
          if (!isFinite(kzt)) kzt = 1.0;

          const kz = getKz(exposure, h);
          const iw = iwInput;

          const qz = 0.00256 * kz * kzt * kd * v * v * iw;

          const gcps = getGCpSet();
          // Assume G ~ 0.85 and qi ~ qz; net p ≈ qz * (GCp - GCpi)
          const pWindward = qz * (gcps.windwardWall - gcpi);
          const pLeeward = qz * (gcps.leewardWall - gcpi);
          const pRoofEdge = qz * (gcps.roofEdge - gcpi);
          const pRoofInterior = qz * (gcps.roofInterior - gcpi);

          const resultsHtml = buildResultsHtml({
            qz,
            kz,
            iw,
            kd,
            kzt,
            v,
            gcpi,
            gcps,
            pWindward,
            pLeeward,
            pRoofEdge,
            pRoofInterior,
          });

          const container = document.getElementById("resultsContainer");
          container.innerHTML = resultsHtml;
          container.scrollIntoView({ behavior: "smooth", block: "start" });
        });

      document
        .getElementById("resetBtn")
        .addEventListener("click", function () {
          document.getElementById("resultsContainer").innerHTML =
            '<p class="text-sm text-gray-500">Enter the required inputs and click “Calculate wind loads” to see velocity pressure and design pressures.</p>';
        });

      // Mobile menu script (from template)
      document.addEventListener("DOMContentLoaded", function () {
        const toggle = document.getElementById("mobile-menu-toggle");
        const menu = document.getElementById("mobile-menu");
        if (toggle && menu) {
          toggle.addEventListener("click", () => {
            const isHidden = menu.classList.contains("hidden");
            menu.classList.toggle("hidden", !isHidden);
            toggle.setAttribute("aria-expanded", String(isHidden));
          });
        }
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
