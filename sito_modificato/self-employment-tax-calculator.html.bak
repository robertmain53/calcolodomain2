          },
          brackets: {
            single: [
              [0, 0.1],
              [12000, 0.12],
              [49000, 0.22],
              [104000, 0.24],
              [198000, 0.32],
              [251000, 0.35],
              [628000, 0.37],
            ],
            mfj: [
              [0, 0.1],
              [24000, 0.12],
              [98000, 0.22],
              [208000, 0.24],
              [396000, 0.32],
              [502000, 0.35],
              [756000, 0.37],
            ],
            mfs: [
              [0, 0.1],
              [12000, 0.12],
              [49000, 0.22],
              [104000, 0.24],
              [198000, 0.32],
              [251000, 0.35],
              [378000, 0.37],
            ],
            hoh: [
              [0, 0.1],
              [17000, 0.12],
              [65000, 0.22],
              [104000, 0.24],
              [198000, 0.32],
              [251000, 0.35],
              [628000, 0.37],
            ],
          },
          ssWageBase: 173400,
          addlMedicareThreshold: {
            single: 200000,
            mfj: 250000,
            mfs: 125000,
            hoh: 200000,
          },
        },
      };

      function formatMoney(v) {
        if (isNaN(v)) return "$0";
        const sign = v < 0 ? "-" : "";
        const n = Math.abs(v);
        return (
          sign +
          n.toLocaleString(undefined, {
            style: "currency",
            currency: "USD",
            maximumFractionDigits: 0,
          })
        );
      }

      function calcIncomeTax(taxable, year, status) {
        if (taxable <= 0) return 0;
        const brackets = TAX_PARAMS[year].brackets[status];
        let tax = 0;
        for (let i = brackets.length - 1; i >= 0; i--) {
          const [threshold, rate] = brackets[i];
          if (taxable > threshold) {
            tax += (taxable - threshold) * rate;
            taxable = threshold;
          }
        }
        return tax;
      }

      function calculate() {
        const year = parseInt(document.getElementById("taxYear").value, 10);
        const status = document.getElementById("filingStatus").value;

        const seIncome =
          parseFloat(document.getElementById("seIncome").value) || 0;
        const w2Wages =
          parseFloat(document.getElementById("w2Wages").value) || 0;
        const otherIncome =
          parseFloat(document.getElementById("otherIncome").value) || 0;
        const aboveLineDed =
          parseFloat(document.getElementById("aboveLineDed").value) || 0;
        const itemizedDed =
          parseFloat(document.getElementById("itemizedDed").value) || 0;
        const ssOverride = parseFloat(
          document.getElementById("ssWageBase").value
        );
        const childCredit =
          parseFloat(document.getElementById("childCredit").value) || 0;

        const deductionType = document.querySelector(
          'input[name="deductionType"]:checked'
        ).value;
        const params = TAX_PARAMS[year];

        const ssWageBase =
          !isNaN(ssOverride) && ssOverride > 0 ? ssOverride : params.ssWageBase;

        const netEarnings = seIncome * 0.9235;

        const totalEarnedForSS = w2Wages + netEarnings;
        const remainingSSBase = Math.max(0, ssWageBase - w2Wages);
        const ssTaxable = Math.min(netEarnings, remainingSSBase);
        const ssTax = ssTaxable * 0.124;

        const medicareTax = netEarnings * 0.029;

        const addlThreshold = params.addlMedicareThreshold[status];
        const totalEarnedForMedicare = w2Wages + netEarnings;
        const addlMedicareBase = Math.max(
          0,
          totalEarnedForMedicare - addlThreshold
        );
        const addlMedicareTax = addlMedicareBase * 0.009;

        const seTax = ssTax + medicareTax + addlMedicareTax;

        const deductibleSETax = (ssTax + medicareTax) * 0.5;

        const grossIncome = w2Wages + seIncome + otherIncome;
        const agi = grossIncome - aboveLineDed - deductibleSETax;

        const standardDed = params.standardDeduction[status];
        const deduction =
          deductionType === "itemized" ? Math.max(itemizedDed, 0) : standardDed;

        const taxableIncome = Math.max(0, agi - deduction);

        let incomeTax = calcIncomeTax(taxableIncome, year, status);
        incomeTax = Math.max(0, incomeTax - childCredit);

        const totalTax = seTax + incomeTax;
        const netIncome = grossIncome - totalTax;

        document.getElementById("seTaxTotal").textContent = formatMoney(seTax);
        document.getElementById("incomeTaxTotal").textContent =
          formatMoney(incomeTax);
        document.getElementById("totalTax").textContent = formatMoney(totalTax);
        document.getElementById("netIncome").textContent =
          formatMoney(netIncome);
        document.getElementById("warningText").classList.remove("hidden");

        const tbody = document.getElementById("breakdownBody");
        tbody.innerHTML = "";

        function addRow(label, amount) {
          const tr = document.createElement("tr");
          const td1 = document.createElement("td");
          const td2 = document.createElement("td");
          td1.className = "px-3 py-2 text-gray-700";
          td2.className = "px-3 py-2 text-right text-gray-800";
          td1.textContent = label;
          td2.textContent = formatMoney(amount);
          tr.appendChild(td1);
          tr.appendChild(td2);
          tbody.appendChild(tr);
        }

        addRow("Net self-employment income (after expenses)", seIncome);
        addRow("Net earnings subject to SE tax (92.35%)", netEarnings);
        addRow("W-2 wages", w2Wages);
        addRow(
          "Social Security wage base used",
          Math.min(ssWageBase, totalEarnedForSS)
        );
        addRow("Social Security SE tax (12.4%)", ssTax);
        addRow("Medicare SE tax (2.9%)", medicareTax);
        addRow("Additional Medicare tax (0.9%)", addlMedicareTax);
        addRow("Total self-employment tax", seTax);
        addRow("Deductible half of SE tax", deductibleSETax);
        addRow("Other above-the-line deductions", aboveLineDed);
        addRow("Adjusted Gross Income (AGI)", agi);
        addRow(
          deductionType === "itemized"
            ? "Itemized deductions"
            : "Standard deduction",
          deduction
        );
        addRow("Taxable income", taxableIncome);
        addRow("Federal income tax (before credits)", incomeTax + childCredit);
        addRow("Credits (child tax credit & others)", -childCredit);
        addRow("Federal income tax (after credits)", incomeTax);
        addRow("Total federal tax (SE + income tax)", totalTax);
        addRow("Net income after federal tax", netIncome);
      }

      document.getElementById("calcBtn").addEventListener("click", calculate);

      document.getElementById("resetBtn").addEventListener("click", () => {
        document.getElementById("seIncome").value = 60000;
        document.getElementById("w2Wages").value = 0;
        document.getElementById("otherIncome").value = 0;
        document.getElementById("aboveLineDed").value = 0;
        document.getElementById("itemizedDed").value = 20000;
        document.getElementById("ssWageBase").value = "";
        document.getElementById("childCredit").value = 0;
        document.getElementById("filingStatus").value = "mfj";
        document.getElementById("taxYear").value = "2025";
        document.querySelector(
          'input[name="deductionType"][value="standard"]'
        ).checked = true;
        document.getElementById("itemizedWrapper").classList.add("hidden");
        document.getElementById("seTaxTotal").textContent = "$0";
        document.getElementById("incomeTaxTotal").textContent = "$0";
        document.getElementById("totalTax").textContent = "$0";
        document.getElementById("netIncome").textContent = "$0";
        document.getElementById("breakdownBody").innerHTML = "";
        document.getElementById("warningText").classList.add("hidden");
      });

      document.querySelectorAll('input[name="deductionType"]').forEach((r) => {
        r.addEventListener("change", () => {
          const type = document.querySelector(
            'input[name="deductionType"]:checked'
          ).value;
          document
            .getElementById("itemizedWrapper")
            .classList.toggle("hidden", type !== "itemized");
        });
      });

      window.addEventListener("DOMContentLoaded", calculate);
    </script>

