    <script>
      // TFSA annual limits (federal) by year
      const TFSA_LIMITS = {
        2009: 5000,
        2010: 5000,
        2011: 5000,
        2012: 5000,
        2013: 5500,
        2014: 5500,
        2015: 10000,
        2016: 5500,
        2017: 5500,
        2018: 5500,
        2019: 6000,
        2020: 6000,
        2021: 6000,
        2022: 6000,
        2023: 6500,
        2024: 7000,
      };

      function getTfsaLimit(year, includeFuture) {
        if (TFSA_LIMITS[year]) return TFSA_LIMITS[year];
        if (!includeFuture) return 0;
        // assume last known limit for future years
        const years = Object.keys(TFSA_LIMITS)
          .map(Number)
          .sort((a, b) => a - b);
        const lastYear = years[years.length - 1];
        return TFSA_LIMITS[lastYear];
      }

      function formatCurrency(value) {
        return value.toLocaleString("en-CA", {
          style: "currency",
          currency: "CAD",
          maximumFractionDigits: 0,
        });
      }

      function formatCurrency0(value) {
        return value.toLocaleString("en-CA", {
          style: "currency",
          currency: "CAD",
          maximumFractionDigits: 0,
        });
      }

      function initYearSelectors() {
        const startYearSelect = document.getElementById("startYear");
        const currentYearSelect = document.getElementById("currentYear");
        const now = new Date();
        const thisYear = now.getFullYear();
        const minYear = 2009;
        const maxYear = thisYear + 40; // allow long projections

        for (let y = minYear; y <= thisYear; y++) {
          const opt = document.createElement("option");
          opt.value = y;
          opt.textContent = y;
          if (y === 2009) opt.selected = true;
          startYearSelect.appendChild(opt);
        }

        for (let y = thisYear; y <= maxYear; y++) {
          const opt = document.createElement("option");
          opt.value = y;
          opt.textContent = y;
          if (y === thisYear + 30) opt.selected = true; // default 30-year projection
          currentYearSelect.appendChild(opt);
        }
      }

      function updateSliders() {
        const rate = document.getElementById("rateOfReturn");
        const rateOut = document.getElementById("rateOfReturnOutput");
        const infl = document.getElementById("inflationRate");
        const inflOut = document.getElementById("inflationRateOutput");
        rateOut.textContent = rate.value + "%";
        inflOut.textContent = infl.value + "%";
      }

      function calculateProjection() {
        const age = parseFloat(document.getElementById("age").value) || 30;
        const startYear = parseInt(
          document.getElementById("startYear").value,
          10
        );
        const endYear = parseInt(
          document.getElementById("currentYear").value,
          10
        );
        const currentBalance = Math.max(
          0,
          parseFloat(document.getElementById("currentBalance").value) || 0
        );
        let unusedRoom = Math.max(
          0,
          parseFloat(document.getElementById("unusedRoom").value) || 0
        );
        const annualContributionInput = Math.max(
          0,
          parseFloat(document.getElementById("annualContribution").value) || 0
        );
        const contributionFrequency = document.getElementById(
          "contributionFrequency"
        ).value;
        const annualWithdrawal = Math.max(
          0,
          parseFloat(document.getElementById("annualWithdrawal").value) || 0
        );
        const rate =
          (parseFloat(document.getElementById("rateOfReturn").value) || 0) /
          100;
        const inflation =
          (parseFloat(document.getElementById("inflationRate").value) || 0) /
          100;
        const includeFutureLimits = document.getElementById(
          "includeFutureLimits"
        ).checked;

        const projectionBody = document.getElementById("projectionTableBody");
        projectionBody.innerHTML = "";

        let balance = currentBalance;
        let totalContributions = 0;
        let totalWithdrawals = 0;
        let overcontributionFlag = false;

        const monthlyRate = rate / 12;
        const startProjectionYear = new Date().getFullYear(); // for "years from now" in real value
        const yearsSpan = endYear - startProjectionYear;

        for (let year = startYear; year <= endYear; year++) {
          const row = document.createElement("tr");
          const limit = getTfsaLimit(year, includeFutureLimits);

          // Contribution room at start of year
          let roomStart = unusedRoom + limit;

          // Planned contributions for this year
          let contribThisYear = 0;
          if (contributionFrequency === "annual") {
            contribThisYear = annualContributionInput;
          } else {
            contribThisYear = annualContributionInput * 12;
          }

          // Check overcontribution risk (approximate)
          if (contribThisYear > roomStart + 1e-6) {
            overcontributionFlag = true;
          }

          // Simulate growth with monthly contributions and mid-year withdrawal
          const startBalance = balance;
          let tempBalance = balance;

          if (contributionFrequency === "monthly") {
            const monthlyContribution = annualContributionInput;
            for (let m = 1; m <= 12; m++) {
              tempBalance *= 1 + monthlyRate;
              tempBalance += monthlyContribution;
              // mid-year withdrawal approximation at month 6
              if (annualWithdrawal > 0 && m === 6) {
                tempBalance = Math.max(0, tempBalance - annualWithdrawal);
              }
            }
          } else {
            // annual contribution at year-end, withdrawal mid-year
            // grow for 6 months, then withdraw, then 6 more months, then contribute
            for (let m = 1; m <= 6; m++) {
              tempBalance *= 1 + monthlyRate;
            }
            if (annualWithdrawal > 0) {
              tempBalance = Math.max(0, tempBalance - annualWithdrawal);
            }
            for (let m = 7; m <= 12; m++) {
              tempBalance *= 1 + monthlyRate;
            }
            tempBalance += annualContributionInput;
          }

          balance = tempBalance;
          totalContributions += contribThisYear;
          totalWithdrawals += annualWithdrawal;

          // Update contribution room at end of year
          // Room_end = roomStart - contributions + withdrawals (added next year)
          const roomEnd = roomStart - contribThisYear; // withdrawals added to next year's unusedRoom
          unusedRoom = Math.max(0, roomEnd + annualWithdrawal);

          // Build table row
          const cells = [
            year,
            formatCurrency0(startBalance),
            formatCurrency0(contribThisYear),
            formatCurrency0(annualWithdrawal),
            formatCurrency0(balance),
            formatCurrency0(Math.max(0, roomEnd)),
          ];

          cells.forEach((val, idx) => {
            const td = document.createElement("td");
            td.className =
              "px-3 py-2 border-b text-xs " +
              (idx === 0 ? "text-left" : "text-right");
            td.textContent = val;
            row.appendChild(td);
          });

          projectionBody.appendChild(row);
        }

        // Summary
        const summaryFinalBalance = document.getElementById(
          "summaryFinalBalance"
        );
        const summaryFinalBalanceReal = document.getElementById(
          "summaryFinalBalanceReal"
        );
        const summaryTotalContributions = document.getElementById(
          "summaryTotalContributions"
        );
        const summaryTotalGrowth =
          document.getElementById("summaryTotalGrowth");
        const summaryRoomEnd = document.getElementById("summaryRoomEnd");
        const summaryOvercontrib =
          document.getElementById("summaryOvercontrib");
        const warningText = document.getElementById("warningText");

        summaryFinalBalance.textContent = formatCurrency0(balance);

        // Real value (today's dollars) using inflation and yearsSpan (if positive)
        let realBalance = balance;
        if (inflation > 0 && yearsSpan > 0) {
          realBalance = balance / Math.pow(1 + inflation, yearsSpan);
        }
        summaryFinalBalanceReal.textContent =
          "In today’s dollars: " + formatCurrency0(realBalance);

        summaryTotalContributions.textContent =
          formatCurrency0(totalContributions);
        const growth =
          balance - currentBalance - totalContributions + totalWithdrawals;
        summaryTotalGrowth.textContent =
          "Investment growth: " + formatCurrency0(growth);

        summaryRoomEnd.textContent = formatCurrency0(Math.max(0, unusedRoom));

        if (overcontributionFlag) {
          summaryOvercontrib.textContent =
            "Overcontribution risk: projected in at least one year";
          summaryOvercontrib.classList.remove("text-emerald-800");
          summaryOvercontrib.classList.add("text-red-700");
          warningText.classList.remove("hidden");
        } else {
          summaryOvercontrib.textContent =
            "Overcontribution risk: none projected";
          summaryOvercontrib.classList.remove("text-red-700");
          summaryOvercontrib.classList.add("text-emerald-800");
          warningText.classList.add("hidden");
        }
      }

      function resetForm() {
        document.getElementById("age").value = 30;
        document.getElementById("currentBalance").value = 20000;
        document.getElementById("unusedRoom").value = 10000;
        document.getElementById("annualContribution").value = 6500;
        document.getElementById("annualWithdrawal").value = 0;
        document.getElementById("rateOfReturn").value = 5;
        document.getElementById("inflationRate").value = 2;
        document.getElementById("contributionFrequency").value = "annual";
        document.getElementById("includeFutureLimits").checked = true;
        updateSliders();
        calculateProjection();
      }

      document.addEventListener("DOMContentLoaded", () => {
        initYearSelectors();
        updateSliders();
        calculateProjection();

        document
          .getElementById("rateOfReturn")
          .addEventListener("input", updateSliders);
        document
          .getElementById("inflationRate")
          .addEventListener("input", updateSliders);

        document
          .getElementById("calculateBtn")
          .addEventListener("click", (e) => {
            e.preventDefault();
            calculateProjection();
          });

        document.getElementById("resetBtn").addEventListener("click", (e) => {
          e.preventDefault();
          resetForm();
        });
      });

