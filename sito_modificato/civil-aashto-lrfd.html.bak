    <script>
      // Toggle custom factors visibility
      const loadComboEl = document.getElementById("loadCombo");
      const customFactorsEl = document.getElementById("customFactors");

      loadComboEl.addEventListener("change", () => {
        if (loadComboEl.value === "user") {
          customFactorsEl.classList.remove("hidden");
        } else {
          customFactorsEl.classList.add("hidden");
        }
      });

      const spanLengthEl = document.getElementById("spanLength");
      const sectionModulusEl = document.getElementById("sectionModulus");
      const materialFyEl = document.getElementById("materialFy");
      const deadLoadEl = document.getElementById("deadLoad");
      const liveLoadEl = document.getElementById("liveLoad");
      const impactFactorEl = document.getElementById("impactFactor");
      const phiDEl = document.getElementById("phiD");
      const phiLEl = document.getElementById("phiL");
      const phiIMEl = document.getElementById("phiIM");

      const resSection = document.getElementById("resultsSection");
      const resDeadLoad = document.getElementById("resDeadLoad");
      const resLiveLoad = document.getElementById("resLiveLoad");
      const resImpactLoad = document.getElementById("resImpactLoad");
      const resWu = document.getElementById("resWu");
      const resMu = document.getElementById("resMu");
      const resMuPerFt = document.getElementById("resMuPerFt");
      const resFb = document.getElementById("resFb");
      const resFn = document.getElementById("resFn");
      const resUtilization = document.getElementById("resUtilization");
      const resStatus = document.getElementById("resStatus");

      function parseInput(el) {
        const v = parseFloat(el.value);
        return isFinite(v) ? v : NaN;
      }

      function calculateAASHTO() {
        const L = parseInput(spanLengthEl); // ft
        const S = parseInput(sectionModulusEl); // in^3
        const Fn = parseInput(materialFyEl); // ksi
        const D = parseInput(deadLoadEl); // k/ft
        const LL = parseInput(liveLoadEl); // k/ft
        const IM = parseInput(impactFactorEl); // %

        if ([L, S, Fn, D, LL, IM].some((v) => !isFinite(v) || v <= 0)) {
          alert("Please enter positive numeric values for all inputs.");
          return;
        }

        let gammaD, gammaL, gammaIM;
        switch (loadComboEl.value) {
          case "strengthI":
            gammaD = 1.25;
            gammaL = 1.75;
            gammaIM = 1.75;
            break;
          case "serviceI":
            gammaD = 1.0;
            gammaL = 1.0;
            gammaIM = 1.0;
            break;
          case "strengthII":
            gammaD = 1.25;
            gammaL = 1.35;
            gammaIM = 1.35;
            break;
          case "user":
            gammaD = parseInput(phiDEl);
            gammaL = parseInput(phiLEl);
            gammaIM = parseInput(phiIMEl);
            if ([gammaD, gammaL, gammaIM].some((v) => !isFinite(v) || v <= 0)) {
              alert("Please enter valid custom load factors γD, γL, and γIM.");
              return;
            }
            break;
          default:
            gammaD = 1.25;
            gammaL = 1.75;
            gammaIM = 1.75;
        }

        // Factored loads
        const wD = gammaD * D;
        const wL = gammaL * LL;
        const wIM = gammaIM * LL * (IM / 100);
        const wu = wD + wL + wIM; // k/ft

        // Max moment for simply supported beam with uniform load
        const Mu = (wu * L * L) / 8; // kip-ft
        const MuPerFt = Mu / L; // kip-ft/ft

        // Flexural stress
        const Mu_kip_in = Mu * 12; // kip-in
        const fb = Mu_kip_in / S; // ksi
        const utilization = fb / Fn;

        // Update UI
        resDeadLoad.textContent = wD.toFixed(3);
        resLiveLoad.textContent = wL.toFixed(3);
        resImpactLoad.textContent = wIM.toFixed(3);
        resWu.textContent = wu.toFixed(3);
        resMu.textContent = Mu.toFixed(2);
        resMuPerFt.textContent = MuPerFt.toFixed(2);
        resFb.textContent = fb.toFixed(2);
        resFn.textContent = Fn.toFixed(2);
        resUtilization.textContent = utilization.toFixed(2);

        resUtilization.classList.remove(
          "text-green-700",
          "text-amber-700",
          "text-red-700"
        );
        resStatus.classList.remove(
          "text-green-700",
          "text-amber-700",
          "text-red-700"
        );

        if (utilization < 0.8) {
          resUtilization.classList.add("text-green-700");
          resStatus.classList.add("text-green-700");
          resStatus.textContent =
            "Utilization is below 0.8 — section is relatively conservative for this combination.";
        } else if (utilization <= 1.0) {
          resUtilization.classList.add("text-amber-700");
          resStatus.classList.add("text-amber-700");
          resStatus.textContent =
            "Utilization is between 0.8 and 1.0 — section is efficiently used; verify all other limit states.";
        } else {
          resUtilization.classList.add("text-red-700");
          resStatus.classList.add("text-red-700");
          resStatus.textContent =
            "Utilization exceeds 1.0 — section is overstressed for this combination and must be redesigned.";
        }

        resSection.classList.remove("hidden");
      }

      document
        .getElementById("btnCalculate")
        .addEventListener("click", calculateAASHTO);

      document.getElementById("btnReset").addEventListener("click", () => {
        spanLengthEl.value = 120;
        sectionModulusEl.value = 3000;
        materialFyEl.value = 50;
        deadLoadEl.value = 2.5;
        liveLoadEl.value = 4.0;
        impactFactorEl.value = 33;
        loadComboEl.value = "strengthI";
        customFactorsEl.classList.add("hidden");
        phiDEl.value = 1.25;
        phiLEl.value = 1.75;
        phiIMEl.value = 1.75;
        resSection.classList.add("hidden");
      });
    </script>

