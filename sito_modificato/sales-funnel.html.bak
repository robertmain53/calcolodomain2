    <script>
      // Sales Funnel Calculator Logic
      const defaultStages = [
        { name: "Leads", volume: 1000, conv: 30 },
        { name: "MQL", volume: null, conv: 40 },
        { name: "SQL", volume: null, conv: 50 },
        { name: "Opportunity", volume: null, conv: 25 },
        { name: "Closed-Won", volume: null, conv: 100 },
      ];

      let stages = JSON.parse(JSON.stringify(defaultStages));

      const funnelBody = document.getElementById("funnel-body");
      const avgDealSizeEl = document.getElementById("avg-deal-size");
      const revenueTargetEl = document.getElementById("revenue-target");

      const summaryTopLeadsEl = document.getElementById("summary-top-leads");
      const summaryDealsEl = document.getElementById("summary-deals");
      const summaryOverallConvEl = document.getElementById(
        "summary-overall-conv"
      );
      const summaryRevenueEl = document.getElementById("summary-revenue");
      const summaryTargetEl = document.getElementById("summary-target");
      const summaryDealsNeededEl = document.getElementById(
        "summary-deals-needed"
      );
      const summaryLeadsNeededEl = document.getElementById(
        "summary-leads-needed"
      );

      const whatIfStageSelect = document.getElementById("whatif-stage");
      const whatIfDeltaRange = document.getElementById("whatif-delta");
      const whatIfDeltaLabel = document.getElementById("whatif-delta-label");
      const whatIfDealsEl = document.getElementById("whatif-deals");
      const whatIfRevenueEl = document.getElementById("whatif-revenue");

      function formatNumber(n) {
        if (!isFinite(n)) return "0";
        return n.toLocaleString(undefined, { maximumFractionDigits: 0 });
      }

      function formatCurrency(n) {
        if (!isFinite(n)) return "$0";
        return n.toLocaleString(undefined, {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 0,
        });
      }

      function renderStages() {
        funnelBody.innerHTML = "";
        stages.forEach((stage, index) => {
          const row = document.createElement("tr");
          row.className = "stage-row";

          const isFirst = index === 0;
          const isLast = index === stages.length - 1;

          row.innerHTML = `
        <td class="px-3 py-2 align-middle">
          <div class="flex items-center gap-2">
            <input type="text" data-type="name" data-index="${index}" value="${
            stage.name
          }" class="w-32 md:w-40 text-sm border-gray-300 rounded-md focus:ring-blue-500 focus:border-blue-500" />
            ${
              stages.length > 2
                ? `<button type="button" data-action="remove" data-index="${index}" class="text-xs text-red-500 hover:text-red-700" title="Remove stage">&times;</button>`
                : ""
            }
          </div>
        </td>
        <td class="px-3 py-2 text-right align-middle">
          <input type="number" min="0" step="1" data-type="volume" data-index="${index}" ${
            isFirst ? "" : "disabled"
          } value="${
            stage.volume !== null ? stage.volume : ""
          }" class="w-24 md:w-28 text-right border-gray-300 rounded-md focus:ring-blue-500 focus:border-blue-500 ${
            isFirst ? "" : "bg-gray-50 text-gray-500"
          }" />
        </td>
        <td class="px-3 py-2 text-right align-middle">
          <input type="number" min="0" max="100" step="0.1" data-type="conv" data-index="${index}" value="${
            stage.conv
          }" class="w-20 md:w-24 text-right border-gray-300 rounded-md focus:ring-blue-500 focus:border-blue-500" />
        </td>
        <td class="px-3 py-2 text-right align-middle">
          <span data-type="next-volume" data-index="${index}" class="inline-block min-w-[3rem] text-right text-gray-800 font-medium">0</span>
        </td>
      `;
          funnelBody.appendChild(row);
        });

        attachStageListeners();
        populateWhatIfStageOptions();
        recalc();
      }

      function attachStageListeners() {
        funnelBody
          .querySelectorAll('input[data-type="name"]')
          .forEach((input) => {
            input.addEventListener("input", (e) => {
              const idx = parseInt(e.target.dataset.index, 10);
              stages[idx].name = e.target.value || `Stage ${idx + 1}`;
              populateWhatIfStageOptions();
            });
          });

        funnelBody
          .querySelectorAll('input[data-type="volume"]')
          .forEach((input) => {
            input.addEventListener("input", (e) => {
              const idx = parseInt(e.target.dataset.index, 10);
              const val = parseFloat(e.target.value);
              stages[idx].volume = isNaN(val) ? 0 : Math.max(0, val);
              recalc();
            });
          });

        funnelBody
          .querySelectorAll('input[data-type="conv"]')
          .forEach((input) => {
            input.addEventListener("input", (e) => {
              const idx = parseInt(e.target.dataset.index, 10);
              let val = parseFloat(e.target.value);
              if (isNaN(val)) val = 0;
              val = Math.min(100, Math.max(0, val));
              stages[idx].conv = val;
              e.target.value = val;
              recalc();
            });
          });

        funnelBody
          .querySelectorAll('button[data-action="remove"]')
          .forEach((btn) => {
            btn.addEventListener("click", (e) => {
              const idx = parseInt(e.currentTarget.dataset.index, 10);
              stages.splice(idx, 1);
              renderStages();
            });
          });
      }

      function recalc() {
        // Forward propagate volumes
        for (let i = 0; i < stages.length; i++) {
          if (i === 0) {
            if (stages[i].volume == null || isNaN(stages[i].volume))
              stages[i].volume = 0;
          } else {
            const prev = stages[i - 1];
            const conv = (prev.conv || 0) / 100;
            stages[i].volume = (prev.volume || 0) * conv;
          }
        }

        // Update DOM volumes
        stages.forEach((stage, index) => {
          const nextSpan = funnelBody.querySelector(
            `span[data-type="next-volume"][data-index="${index}"]`
          );
          if (nextSpan) {
            if (index === stages.length - 1) {
              nextSpan.textContent = "-";
            } else {
              nextSpan.textContent = formatNumber(
                stages[index + 1].volume || 0
              );
            }
          }
          if (index === 0) {
            const volInput = funnelBody.querySelector(
              `input[data-type="volume"][data-index="${index}"]`
            );
            if (volInput) volInput.value = stage.volume || 0;
          }
        });

        const topLeads = stages[0]?.volume || 0;
        const deals = stages[stages.length - 1]?.volume || 0;
        const overallConv = topLeads > 0 ? deals / topLeads : 0;

        const avgDealSize = parseFloat(avgDealSizeEl.value) || 0;
        const revenue = deals * avgDealSize;

        summaryTopLeadsEl.textContent = formatNumber(topLeads);
        summaryDealsEl.textContent = formatNumber(deals);
        summaryOverallConvEl.textContent =
          (overallConv * 100).toFixed(2).replace(/\.00$/, "") + "%";
        summaryRevenueEl.textContent = formatCurrency(revenue);

        const target = parseFloat(revenueTargetEl.value) || 0;
        summaryTargetEl.textContent = formatCurrency(target);

        let dealsNeeded = 0;
        let leadsNeeded = 0;
        if (avgDealSize > 0) {
          dealsNeeded = target / avgDealSize;
          if (overallConv > 0) {
            leadsNeeded = dealsNeeded / overallConv;
          }
        }
        summaryDealsNeededEl.textContent = formatNumber(Math.ceil(dealsNeeded));
        summaryLeadsNeededEl.textContent = formatNumber(Math.ceil(leadsNeeded));

        updateWhatIf();
      }

      function populateWhatIfStageOptions() {
        const currentValue = whatIfStageSelect.value;
        whatIfStageSelect.innerHTML = "";
        stages.forEach((stage, index) => {
          if (index === stages.length - 1) return; // last stage has no next stage
          const opt = document.createElement("option");
          opt.value = index;
          opt.textContent = stage.name || `Stage ${index + 1}`;
          whatIfStageSelect.appendChild(opt);
        });
        if (whatIfStageSelect.options.length > 0) {
          const idx = Array.from(whatIfStageSelect.options).findIndex(
            (o) => o.value === currentValue
          );
          whatIfStageSelect.selectedIndex = idx >= 0 ? idx : 0;
        }
      }

      function updateWhatIf() {
        if (stages.length < 2 || whatIfStageSelect.options.length === 0) {
          whatIfDealsEl.textContent = "–";
          whatIfRevenueEl.textContent = "–";
          return;
        }

        const stageIndex = parseInt(whatIfStageSelect.value || "0", 10);
        const delta = parseFloat(whatIfDeltaRange.value) || 0;
        whatIfDeltaLabel.textContent = `+${delta}%`;

        // Current baseline
        const topLeads = stages[0]?.volume || 0;
        const dealsCurrent = stages[stages.length - 1]?.volume || 0;
        const avgDealSize = parseFloat(avgDealSizeEl.value) || 0;
        const revenueCurrent = dealsCurrent * avgDealSize;

        // Clone stages and apply improved conversion to selected stage
        const cloned = JSON.parse(JSON.stringify(stages));
        const newConv = Math.min(100, cloned[stageIndex].conv + delta);
        cloned[stageIndex].conv = newConv;

        // Recalculate volumes on cloned funnel
        for (let i = 0; i < cloned.length; i++) {
          if (i === 0) {
            cloned[i].volume = topLeads;
          } else {
            const prev = cloned[i - 1];
            const conv = (prev.conv || 0) / 100;
            cloned[i].volume = (prev.volume || 0) * conv;
          }
        }

        const dealsImproved = cloned[cloned.length - 1]?.volume || 0;
        const revenueImproved = dealsImproved * avgDealSize;

        whatIfDealsEl.textContent = `${formatNumber(
          dealsCurrent
        )} → ${formatNumber(dealsImproved)}`;
        whatIfRevenueEl.textContent = `${formatCurrency(
          revenueCurrent
        )} → ${formatCurrency(revenueImproved)}`;
      }

      document.getElementById("add-stage-btn").addEventListener("click", () => {
        const newIndex = stages.length - 1;
        const newStage = {
          name: `Stage ${newIndex + 1}`,
          volume: null,
          conv: 50,
        };
        stages.splice(stages.length - 1, 0, newStage);
        renderStages();
      });

      document
        .getElementById("reset-defaults-btn")
        .addEventListener("click", () => {
          stages = JSON.parse(JSON.stringify(defaultStages));
          avgDealSizeEl.value = 5000;
          revenueTargetEl.value = 250000;
          renderStages();
        });

      avgDealSizeEl.addEventListener("input", recalc);
      revenueTargetEl.addEventListener("input", recalc);
      whatIfStageSelect.addEventListener("change", updateWhatIf);
      whatIfDeltaRange.addEventListener("input", updateWhatIf);

      // Initial render
      renderStages();

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
