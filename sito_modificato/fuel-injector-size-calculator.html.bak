    <script>
      // Tab switching
      const tabSizeFromHp = document.getElementById("tab-size-from-hp");
      const tabHpFromSize = document.getElementById("tab-hp-from-size");
      const panelSizeFromHp = document.getElementById("panel-size-from-hp");
      const panelHpFromSize = document.getElementById("panel-hp-from-size");

      function activateTab(tab) {
        if (tab === "size") {
          tabSizeFromHp.classList.add("tab-active", "border-blue-600");
          tabHpFromSize.classList.remove("tab-active", "border-blue-600");
          panelSizeFromHp.classList.remove("hidden");
          panelHpFromSize.classList.add("hidden");
        } else {
          tabHpFromSize.classList.add("tab-active", "border-blue-600");
          tabSizeFromHp.classList.remove("tab-active", "border-blue-600");
          panelHpFromSize.classList.remove("hidden");
          panelSizeFromHp.classList.add("hidden");
        }
      }

      tabSizeFromHp.addEventListener("click", () => activateTab("size"));
      tabHpFromSize.addEventListener("click", () => activateTab("hp"));

      // Sync BSFC with fuel type presets
      function setupBsfcSync(selectId, bsfcId) {
        const sel = document.getElementById(selectId);
        const bsfcInput = document.getElementById(bsfcId);
        sel.addEventListener("change", () => {
          const opt = sel.options[sel.selectedIndex];
          const preset = parseFloat(opt.getAttribute("data-bsfc"));
          if (!isNaN(preset) && sel.value !== "custom") {
            bsfcInput.value = preset.toFixed(2);
          }
        });
      }
      setupBsfcSync("fuelType", "bsfc");
      setupBsfcSync("fuelType2", "bsfc2");

      // Fuel density factors for cc/min conversion
      const fuelCcPerLb = {
        gasoline_na: 10.5,
        gasoline_boosted: 10.5,
        e85: 9.8,
        methanol: 7.9,
        custom: 10.5,
      };

      function getFuelKey(selectEl) {
        const val = selectEl.value || "custom";
        return fuelCcPerLb[val] ? val : "custom";
      }

      // Injector size from HP
      document.getElementById("btnCalcSize").addEventListener("click", () => {
        const hpInput = parseFloat(document.getElementById("hpInput").value);
        const hpType = document.getElementById("hpType").value;
        const drivetrainLoss =
          parseFloat(document.getElementById("drivetrainLoss").value) || 0;
        const cylinders = parseInt(
          document.getElementById("cylinders").value,
          10
        );
        const bsfc = parseFloat(document.getElementById("bsfc").value);
        const dutyCycle = parseFloat(
          document.getElementById("dutyCycle").value
        );
        const fuelSelect = document.getElementById("fuelType");
        const fuelKey = getFuelKey(fuelSelect);
        const ccPerLb = fuelCcPerLb[fuelKey];

        const errorEl = document.getElementById("sizeError");
        const resultsEl = document.getElementById("sizeResults");
        errorEl.classList.add("hidden");
        resultsEl.classList.add("hidden");

        if (
          isNaN(hpInput) ||
          hpInput <= 0 ||
          isNaN(cylinders) ||
          cylinders <= 0 ||
          isNaN(bsfc) ||
          bsfc <= 0 ||
          isNaN(dutyCycle) ||
          dutyCycle <= 0
        ) {
          errorEl.textContent =
            "Please enter valid positive values for horsepower, cylinders, BSFC and duty cycle.";
          errorEl.classList.remove("hidden");
          return;
        }

        let crankHp = hpInput;
        if (hpType === "wheel") {
          const lossFrac = Math.min(Math.max(drivetrainLoss / 100, 0), 0.5);
          crankHp = hpInput / (1 - lossFrac);
        }

        const dutyFrac = Math.min(Math.max(dutyCycle / 100, 0.1), 1);
        const totalFuelLbHr = crankHp * bsfc;
        const injSizeLbHr = totalFuelLbHr / (cylinders * dutyFrac);
        const injSizeCcMin = injSizeLbHr * ccPerLb;

        document.getElementById("resSizeLbHr").textContent =
          injSizeLbHr.toFixed(1);
        document.getElementById("resSizeCcMin").textContent =
          injSizeCcMin.toFixed(0);
        document.getElementById("resCrankHp").textContent = crankHp.toFixed(0);
        document.getElementById("resTotalFlowLbHr").textContent =
          totalFuelLbHr.toFixed(1);
        document.getElementById("resDutyUsed").textContent = (
          dutyFrac * 100
        ).toFixed(0);

        resultsEl.classList.remove("hidden");
      });

      document.getElementById("btnResetSize").addEventListener("click", () => {
        document.getElementById("hpInput").value = 400;
        document.getElementById("hpType").value = "crank";
        document.getElementById("drivetrainLoss").value = 15;
        document.getElementById("cylinders").value = 8;
        document.getElementById("fuelType").value = "gasoline_na";
        document.getElementById("bsfc").value = 0.5;
        document.getElementById("dutyCycle").value = 85;
        document.getElementById("sizeResults").classList.add("hidden");
        document.getElementById("sizeError").classList.add("hidden");
      });

      // HP from injector size
      document.getElementById("btnCalcHp").addEventListener("click", () => {
        const injSize = parseFloat(document.getElementById("injSize").value);
        const injUnit = document.getElementById("injSizeUnit").value;
        const cylinders = parseInt(
          document.getElementById("hpCylinders2").value,
          10
        );
        const bsfc = parseFloat(document.getElementById("bsfc2").value);
        const dutyCycle = parseFloat(
          document.getElementById("dutyCycle2").value
        );
        const drivetrainLoss =
          parseFloat(document.getElementById("drivetrainLoss2").value) || 0;
        const fuelSelect = document.getElementById("fuelType2");
        const fuelKey = getFuelKey(fuelSelect);
        const ccPerLb = fuelCcPerLb[fuelKey];

        const errorEl = document.getElementById("hpError");
        const resultsEl = document.getElementById("hpResults");
        errorEl.classList.add("hidden");
        resultsEl.classList.add("hidden");

        if (
          isNaN(injSize) ||
          injSize <= 0 ||
          isNaN(cylinders) ||
          cylinders <= 0 ||
          isNaN(bsfc) ||
          bsfc <= 0 ||
          isNaN(dutyCycle) ||
          dutyCycle <= 0
        ) {
          errorEl.textContent =
            "Please enter valid positive values for injector size, cylinders, BSFC and duty cycle.";
          errorEl.classList.remove("hidden");
          return;
        }

        let injLbHr = injSize;
        if (injUnit === "ccmin") {
          injLbHr = injSize / ccPerLb;
        }

        const dutyFrac = Math.min(Math.max(dutyCycle / 100, 0.1), 1);
        const totalFuelLbHr = injLbHr * cylinders * dutyFrac;
        const hpCrank = totalFuelLbHr / bsfc;
        const lossFrac = Math.min(Math.max(drivetrainLoss / 100, 0), 0.5);
        const hpWheel = hpCrank * (1 - lossFrac);

        document.getElementById("resHpCrank").textContent = hpCrank.toFixed(0);
        document.getElementById("resHpWheel").textContent = hpWheel.toFixed(0);
        document.getElementById("resHpSizeLbHr").textContent =
          injLbHr.toFixed(1);
        document.getElementById("resHpSizeCcMin").textContent = (
          injLbHr * ccPerLb
        ).toFixed(0);
        document.getElementById("resHpTotalFlow").textContent =
          totalFuelLbHr.toFixed(1);
        document.getElementById("resHpDutyUsed").textContent = (
          dutyFrac * 100
        ).toFixed(0);

        resultsEl.classList.remove("hidden");
      });

      document.getElementById("btnResetHp").addEventListener("click", () => {
        document.getElementById("injSize").value = 100;
        document.getElementById("injSizeUnit").value = "lbhr";
        document.getElementById("hpCylinders2").value = 8;
        document.getElementById("fuelType2").value = "gasoline_na";
        document.getElementById("bsfc2").value = 0.5;
        document.getElementById("dutyCycle2").value = 85;
        document.getElementById("drivetrainLoss2").value = 15;
        document.getElementById("hpResults").classList.add("hidden");
        document.getElementById("hpError").classList.add("hidden");
