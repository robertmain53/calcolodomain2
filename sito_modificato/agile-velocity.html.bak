    <script>
      // Utility functions
      function parseNumber(value) {
        const n = parseFloat(value);
        return isNaN(n) ? null : n;
      }

      function mean(arr) {
        if (!arr.length) return null;
        return arr.reduce((a, b) => a + b, 0) / arr.length;
      }

      function median(arr) {
        if (!arr.length) return null;
        const sorted = [...arr].sort((a, b) => a - b);
        const mid = Math.floor(sorted.length / 2);
        if (sorted.length % 2 === 0) {
          return (sorted[mid - 1] + sorted[mid]) / 2;
        }
        return sorted[mid];
      }

      function stdDev(arr) {
        if (arr.length < 2) return 0;
        const m = mean(arr);
        const variance =
          arr.reduce((sum, x) => sum + Math.pow(x - m, 2), 0) /
          (arr.length - 1);
        return Math.sqrt(variance);
      }

      function formatNumber(value, decimals = 2) {
        if (value === null || value === undefined || isNaN(value)) return "–";
        return value.toFixed(decimals).replace(/\.00$/, "");
      }

      function addDays(date, days) {
        const d = new Date(date.getTime());
        d.setDate(d.getDate() + days);
        return d;
      }

      function addWeeks(date, weeks) {
        return addDays(date, weeks * 7);
      }

      function formatDate(date) {
        if (!(date instanceof Date) || isNaN(date.getTime())) return "–";
        return date.toISOString().slice(0, 10);
      }

      // Sprint rows management
      const sprintRowsTbody = document.getElementById("sprint-rows");
      const addSprintRowBtn = document.getElementById("add-sprint-row");
      const resetSprintsBtn = document.getElementById("reset-sprints");
      const calculateBtn = document.getElementById("calculate-velocity");

      function createSprintRow(index, completed = "", planned = "") {
        const tr = document.createElement("tr");
        tr.className = index % 2 === 0 ? "bg-white" : "bg-gray-50";

        tr.innerHTML = `
      <td class="px-3 py-2 text-xs text-gray-500">Sprint ${index}</td>
      <td class="px-3 py-2">
        <input type="number" min="0" step="0.1"
               class="w-full border border-gray-300 rounded-md px-2 py-1 text-xs focus:ring-1 focus:ring-blue-500 focus:border-transparent sprint-completed"
               placeholder="e.g. 24" value="${completed}">
      </td>
      <td class="px-3 py-2">
        <input type="number" min="0" step="0.1"
               class="w-full border border-gray-300 rounded-md px-2 py-1 text-xs focus:ring-1 focus:ring-blue-500 focus:border-transparent sprint-planned"
               placeholder="optional" value="${planned}">
      </td>
      <td class="px-2 py-2 text-center">
        <button type="button" class="text-xs text-red-600 hover:underline remove-sprint-row">✕</button>
      </td>
    `;
        return tr;
      }

      function renumberSprintRows() {
        const rows = sprintRowsTbody.querySelectorAll("tr");
        rows.forEach((row, idx) => {
          const labelCell = row.querySelector("td:first-child");
          if (labelCell) labelCell.textContent = "Sprint " + (idx + 1);
          row.className = idx % 2 === 0 ? "bg-white" : "bg-gray-50";
        });
      }

      function addSprintRow(completed = "", planned = "") {
        const index = sprintRowsTbody.querySelectorAll("tr").length + 1;
        const row = createSprintRow(index, completed, planned);
        sprintRowsTbody.appendChild(row);
      }

      function resetSprintRows() {
        sprintRowsTbody.innerHTML = "";
        // Provide a few default empty rows
        for (let i = 0; i < 5; i++) {
          addSprintRow();
        }
      }

      addSprintRowBtn.addEventListener("click", () => {
        addSprintRow();
      });

      resetSprintsBtn.addEventListener("click", () => {
        resetSprintRows();
        clearResults();
      });

      sprintRowsTbody.addEventListener("click", (e) => {
        if (e.target.classList.contains("remove-sprint-row")) {
          const row = e.target.closest("tr");
          if (row) {
            row.remove();
            renumberSprintRows();
          }
        }
      });

      function clearResults() {
        document.getElementById("avg-velocity").textContent = "–";
        document.getElementById("median-velocity").textContent = "–";
        document.getElementById("velocity-variation").textContent = "–";
        document.getElementById("velocity-variation-label").textContent =
          "Coefficient of variation";
        document.getElementById("forecast-rows").innerHTML =
          '<tr><td class="px-3 py-2 text-gray-500" colspan="4">Enter sprint history and backlog to see forecast.</td></tr>';
        document.getElementById("forecast-note").textContent = "";
        document.getElementById("avg-plan-completion").textContent = "–";
        document.getElementById("sprints-hit-plan").textContent = "–";
        document.getElementById("avg-plan-delta").textContent = "–";
      }

      // Main calculation
      calculateBtn.addEventListener("click", () => {
        const completedInputs =
          sprintRowsTbody.querySelectorAll(".sprint-completed");
        const plannedInputs =
          sprintRowsTbody.querySelectorAll(".sprint-planned");

        const completedVelocities = [];
        const plannedValues = [];

        completedInputs.forEach((input, idx) => {
          const completed = parseNumber(input.value);
          const planned = parseNumber(plannedInputs[idx].value);

          if (completed !== null) {
            completedVelocities.push(completed);
            plannedValues.push(planned);
          }
        });

        if (!completedVelocities.length) {
          clearResults();
          alert(
            "Please enter at least one sprint with completed story points."
          );
          return;
        }

        const avgVel = mean(completedVelocities);
        const medVel = median(completedVelocities);
        const sdVel = stdDev(completedVelocities);
        const cv = avgVel ? sdVel / avgVel : 0;

        document.getElementById("avg-velocity").textContent =
          formatNumber(avgVel);
        document.getElementById("median-velocity").textContent =
          formatNumber(medVel);

        let stabilityLabel = "";
        if (cv < 0.15) {
          stabilityLabel = "Stable velocity (low variation)";
        } else if (cv < 0.3) {
          stabilityLabel = "Moderate variation";
        } else {
          stabilityLabel = "High variation – forecasts less reliable";
        }
        document.getElementById("velocity-variation").textContent =
          formatNumber(cv * 100, 1) + "%";
        document.getElementById("velocity-variation-label").textContent =
          stabilityLabel;

        // Forecast
        const backlogPoints = parseNumber(
          document.getElementById("backlog-points").value
        );
        const sprintLength = parseNumber(
          document.getElementById("sprint-length").value
        );
        const sprintLengthUnit =
          document.getElementById("sprint-length-unit").value;
        const startDateStr = document.getElementById("start-date").value;
        const startDate = startDateStr
          ? new Date(startDateStr + "T00:00:00")
          : null;

        const forecastTbody = document.getElementById("forecast-rows");
        forecastTbody.innerHTML = "";

        if (backlogPoints === null || backlogPoints <= 0) {
          forecastTbody.innerHTML =
            '<tr><td class="px-3 py-2 text-gray-500" colspan="4">Enter remaining backlog size to see forecast.</td></tr>';
          document.getElementById("forecast-note").textContent =
            "Backlog size is required for sprint and date-based forecasts.";
        } else {
          // Define scenarios: optimistic (max), typical (avg), conservative (pessimistic)
          const maxVel = Math.max(...completedVelocities);
          const minVel = Math.min(...completedVelocities);
          const conservativeVel = Math.max(avgVel - sdVel, minVel * 0.7); // avoid zero/negative
          const optimisticVel = maxVel;
          const typicalVel = avgVel;

          const scenarios = [
            { name: "Optimistic", vel: optimisticVel },
            { name: "Typical", vel: typicalVel },
            { name: "Conservative", vel: conservativeVel },
          ];

          scenarios.forEach((s) => {
            if (!s.vel || s.vel <= 0) return;
            const sprintsNeeded = backlogPoints / s.vel;
            let completionDateText = "–";
            if (startDate && sprintLength && sprintLength > 0) {
              let totalDays;
              if (sprintLengthUnit === "weeks") {
                totalDays = sprintLength * 7 * sprintsNeeded;
              } else {
                totalDays = sprintLength * sprintsNeeded;
              }
              const completionDate = addDays(startDate, Math.round(totalDays));
              completionDateText = formatDate(completionDate);
            }

            const tr = document.createElement("tr");
            tr.innerHTML = `
          <td class="px-3 py-2 text-gray-800">${s.name}</td>
          <td class="px-3 py-2 text-right text-gray-800">${formatNumber(
            s.vel
          )}</td>
          <td class="px-3 py-2 text-right text-gray-800">${formatNumber(
            sprintsNeeded,
            1
          )}</td>
          <td class="px-3 py-2 text-right text-gray-800">${completionDateText}</td>
        `;
            forecastTbody.appendChild(tr);
          });

          document.getElementById("forecast-note").textContent =
            "Forecasts are approximate and assume similar team composition, estimation practice, and working conditions.";
        }

        // Planned vs actual
        const planRatios = [];
        const deltas = [];
        let hitCount = 0;
        let planCount = 0;

        completedVelocities.forEach((completed, idx) => {
          const planned = plannedValues[idx];
          if (planned !== null && planned > 0) {
            const ratio = completed / planned;
            planRatios.push(ratio);
            deltas.push(completed - planned);
            planCount++;
            if (ratio >= 1) hitCount++;
          }
        });

        if (!planCount) {
          document.getElementById("avg-plan-completion").textContent = "–";
          document.getElementById("sprints-hit-plan").textContent = "–";
          document.getElementById("avg-plan-delta").textContent = "–";
        } else {
          const avgRatio = mean(planRatios);
          const avgDelta = mean(deltas);
          document.getElementById("avg-plan-completion").textContent =
            formatNumber(avgRatio * 100, 1) + "%";
          document.getElementById("sprints-hit-plan").textContent =
            hitCount + " / " + planCount;
          const sign = avgDelta >= 0 ? "+" : "";
          document.getElementById("avg-plan-delta").textContent =
            sign + formatNumber(avgDelta, 1) + " pts per sprint";
        }
      });

      // Initialize default rows on load
      resetSprintRows();
    </script>

