    <script>
      // Utility: clamp non-negative integers
      function parseNonNegativeInt(value) {
        const n = parseInt(value, 10);
        if (isNaN(n) || n < 0) return 0;
        return n;
      }

      function setTodayIfEmpty() {
        const startInput = document.getElementById("start-date");
        if (!startInput.value) {
          const today = new Date();
          const yyyy = today.getFullYear();
          const mm = String(today.getMonth() + 1).padStart(2, "0");
          const dd = String(today.getDate()).padStart(2, "0");
          startInput.value = `${yyyy}-${mm}-${dd}`;
        }
      }

      function applyPresetDays(totalDays) {
        // Clear Y/M and distribute into weeks + days
        document.getElementById("years").value = 0;
        document.getElementById("months").value = 0;
        const weeks = Math.floor(totalDays / 7);
        const days = totalDays % 7;
        document.getElementById("weeks").value = weeks;
        document.getElementById("days").value = days;
      }

      function calculateDueDate() {
        const startInput = document.getElementById("start-date").value;
        const op =
          document.querySelector('input[name="operation"]:checked')?.value ||
          "add";

        const years = parseNonNegativeInt(
          document.getElementById("years").value
        );
        const months = parseNonNegativeInt(
          document.getElementById("months").value
        );
        const weeks = parseNonNegativeInt(
          document.getElementById("weeks").value
        );
        const days = parseNonNegativeInt(document.getElementById("days").value);

        const resultMain = document.getElementById("result-main");
        const resultWeekday = document.getElementById("result-weekday");
        const resultDetails = document.getElementById("result-details");
        const resultStatus = document.getElementById("result-status");

        if (!startInput) {
          resultMain.textContent = "Please select a valid start date.";
          resultWeekday.textContent = "";
          resultDetails.textContent = "";
          resultStatus.textContent = "";
          return;
        }

        if (years === 0 && months === 0 && weeks === 0 && days === 0) {
          resultMain.textContent =
            "Enter at least one non-zero value for years, months, weeks, or days.";
          resultWeekday.textContent = "";
          resultDetails.textContent = "";
          resultStatus.textContent = "";
          return;
        }

        const startDate = new Date(startInput + "T00:00:00");
        if (isNaN(startDate.getTime())) {
          resultMain.textContent =
            "The start date is invalid. Please choose another date.";
          resultWeekday.textContent = "";
          resultDetails.textContent = "";
          resultStatus.textContent = "";
          return;
        }

        // Clone date
        const resultDate = new Date(startDate.getTime());

        const sign = op === "subtract" ? -1 : 1;

        // Apply years and months using calendar logic
        const newYear = resultDate.getFullYear() + sign * years;
        const newMonth = resultDate.getMonth() + sign * months;
        resultDate.setFullYear(newYear, newMonth);

        // Apply days (weeks converted to days)
        const totalDays = weeks * 7 + days;
        resultDate.setDate(resultDate.getDate() + sign * totalDays);

        const optionsDate = { year: "numeric", month: "long", day: "numeric" };
        const optionsWeekday = { weekday: "long" };
        const formattedDate = resultDate.toLocaleDateString(
          undefined,
          optionsDate
        );
        const weekday = resultDate.toLocaleDateString(
          undefined,
          optionsWeekday
        );

        resultMain.textContent =
          op === "add"
            ? `Due date: ${formattedDate}`
            : `Calculated start date: ${formattedDate}`;
        resultWeekday.textContent = `This falls on a ${weekday}.`;

        const totalShiftDays = sign * totalDays; // days from weeks+days only
        const parts = [];
        if (years)
          parts.push(`${sign * years} year${Math.abs(years) !== 1 ? "s" : ""}`);
        if (months)
          parts.push(
            `${sign * months} month${Math.abs(months) !== 1 ? "s" : ""}`
          );
        if (weeks)
          parts.push(`${sign * weeks} week${Math.abs(weeks) !== 1 ? "s" : ""}`);
        if (days)
          parts.push(`${sign * days} day${Math.abs(days) !== 1 ? "s" : ""}`);

        resultDetails.textContent =
          `From ${startDate.toLocaleDateString(undefined, optionsDate)} ` +
          `${op === "add" ? "plus" : "minus"} ${parts.join(", ")}.`;

        const now = new Date();
        resultStatus.textContent = `Calculated at ${now.toLocaleTimeString()}`;
      }

      document.addEventListener("DOMContentLoaded", () => {
        setTodayIfEmpty();

        document
          .getElementById("calculate-btn")
          .addEventListener("click", calculateDueDate);

        document.getElementById("reset-btn").addEventListener("click", () => {
          setTodayIfEmpty();
          document.getElementById("years").value = 0;
          document.getElementById("months").value = 0;
          document.getElementById("weeks").value = 0;
          document.getElementById("days").value = 0;
          document.querySelector(
            'input[name="operation"][value="add"]'
          ).checked = true;

          document.getElementById("result-main").textContent =
            "Select a date and interval, then click Calculate.";
          document.getElementById("result-weekday").textContent = "";
          document.getElementById("result-details").textContent = "";
          document.getElementById("result-status").textContent = "";
        });

        // Preset buttons for days
        document.querySelectorAll("button[data-preset]").forEach((btn) => {
          btn.addEventListener("click", () => {
            const days = parseInt(btn.getAttribute("data-preset"), 10);
            if (!isNaN(days)) {
              applyPresetDays(days);
            }
          });
        });

        // Pregnancy preset: 280 days
        document
          .getElementById("preset-pregnancy")
          .addEventListener("click", () => {
            applyPresetDays(280);
          });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
