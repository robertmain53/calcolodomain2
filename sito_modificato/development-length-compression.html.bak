    <script>
      // Bar size database (US and metric diameters)
      const barDataUS = {
        "#3": 0.375,
        "#4": 0.5,
        "#5": 0.625,
        "#6": 0.75,
        "#7": 0.875,
        "#8": 1.0,
        "#9": 1.128,
        "#10": 1.27,
        "#11": 1.41,
      };
      const barDataMetric = {
        "#3": 9.5,
        "#4": 12.7,
        "#5": 15.9,
        "#6": 19.1,
        "#7": 22.2,
        "#8": 25.4,
        "#9": 28.7,
        "#10": 32.3,
        "#11": 35.8,
      };

      const unitSystemEl = document.getElementById("unit-system");
      const barSizeEl = document.getElementById("bar-size");
      const dbEl = document.getElementById("db");
      const fcEl = document.getElementById("fc");
      const fyEl = document.getElementById("fy");
      const errorMsgEl = document.getElementById("error-msg");

      const dbUnitLabel = document.getElementById("db-unit-label");
      const fcUnitLabel = document.getElementById("fc-unit-label");
      const fyUnitLabel = document.getElementById("fy-unit-label");
      const ldUnitLabel = document.getElementById("ld-unit-label");
      const ldUnitLabelSmall = document.getElementById("ld-unit-label-small");
      const ldBasicUnit = document.getElementById("ld-basic-unit");
      const ldMinUnit = document.getElementById("ld-min-unit");
      const ldFinalUnit = document.getElementById("ld-final-unit");

      const resultsCard = document.getElementById("results-card");
      const ldMainEl = document.getElementById("ld-main");
      const ldInDbEl = document.getElementById("ld-in-db");
      const ldBasicEl = document.getElementById("ld-basic");
      const ldMinEl = document.getElementById("ld-min");
      const ldFinalEl = document.getElementById("ld-final");
      const stepByStepEl = document.getElementById("step-by-step");

      function updateUnits() {
        const system = unitSystemEl.value;
        if (system === "us") {
          dbUnitLabel.textContent = "(in)";
          fcUnitLabel.textContent = "(psi)";
          fyUnitLabel.textContent = "(ksi)";
          ldUnitLabel.textContent = "in";
          ldUnitLabelSmall.textContent = "in";
          ldBasicUnit.textContent = "in";
          ldMinUnit.textContent = "in";
          ldFinalUnit.textContent = "in";
          // convert db if it looks like metric (rough heuristic)
          if (barSizeEl.value !== "custom" && barDataUS[barSizeEl.value]) {
            dbEl.value = barDataUS[barSizeEl.value];
          }
        } else {
          dbUnitLabel.textContent = "(mm)";
          fcUnitLabel.textContent = "(MPa)";
          fyUnitLabel.textContent = "(MPa)";
          ldUnitLabel.textContent = "mm";
          ldUnitLabelSmall.textContent = "mm";
          ldBasicUnit.textContent = "mm";
          ldMinUnit.textContent = "mm";
          ldFinalUnit.textContent = "mm";
          if (barSizeEl.value !== "custom" && barDataMetric[barSizeEl.value]) {
            dbEl.value = barDataMetric[barSizeEl.value];
          }
        }
      }

      unitSystemEl.addEventListener("change", updateUnits);

      barSizeEl.addEventListener("change", () => {
        const system = unitSystemEl.value;
        const size = barSizeEl.value;
        if (size === "custom") return;
        if (system === "us" && barDataUS[size]) {
          dbEl.value = barDataUS[size];
        } else if (system === "metric" && barDataMetric[size]) {
          dbEl.value = barDataMetric[size];
        }
      });

      function roundTo(value, step) {
        return Math.ceil(value / step) * step;
      }

      document.getElementById("calc-btn").addEventListener("click", () => {
        errorMsgEl.textContent = "";
        const system = unitSystemEl.value;
        let db = parseFloat(dbEl.value);
        let fc = parseFloat(fcEl.value);
        let fy = parseFloat(fyEl.value);

        if (!(db > 0 && fc > 0 && fy > 0)) {
          errorMsgEl.textContent =
            "Please enter positive values for d\u2090, f'\u2090 and f\u2090.";
          resultsCard.classList.add("hidden");
          return;
        }

        // basic validation ranges
        if (system === "us") {
          if (fc < 2000 || fc > 10000) {
            errorMsgEl.textContent =
              "f'c should typically be between 2,000 and 10,000 psi.";
          }
          if (fy < 40 || fy > 80) {
            errorMsgEl.textContent =
              "f\u2090 is usually between 40 and 80 ksi for reinforcing steel.";
          }
        } else {
          if (fc < 10 || fc > 70) {
            errorMsgEl.textContent =
              "f'c should typically be between 10 and 70 MPa.";
          }
          if (fy < 300 || fy > 600) {
            errorMsgEl.textContent =
              "f\u2090 is usually between 300 and 600 MPa for reinforcing steel.";
          }
        }

        const barType = document.querySelector(
          'input[name="bar-type"]:checked'
        ).value;
        const confinement = document.querySelector(
          'input[name="confinement"]:checked'
        ).value;
        const lightweight = document.getElementById("lightweight").checked;
        const epoxy = document.getElementById("epoxy").checked;

        // lambda for lightweight concrete (simplified)
        let lambda = 1.0;
        if (lightweight) lambda = 0.75;

        let ldBasic, ldMin1, ldMin2, ldControl;
        let notes = [];

        if (system === "us") {
          // Convert fc to ksi for sqrt if needed
          const fcKsi = fc / 1000.0;
          const sqrtFc = Math.sqrt(fcKsi * 1000); // keep psi under sqrt for consistency

          // Basic ld in inches (approximate)
          ldBasic = ((0.02 * fy) / Math.sqrt(fc)) * db * (1 / lambda);

          // Minimums
          ldMin1 = (0.0003 * fy * 1000 * db) / 1000; // simplify to fy*db*0.0003
          ldMin2 = 8 * db;

          // For plain bars, enforce higher multiple of db
          if (barType === "plain") {
            ldMin2 = Math.max(ldMin2, 12 * db);
            notes.push(
              "Plain bar: minimum multiple of d\u2090 increased (e.g., ≥ 12 d\u2090)."
            );
          }

          ldControl = Math.max(ldBasic, ldMin1, ldMin2);
        } else {
          // Metric implementation
          const sqrtFc = Math.sqrt(fc);

          ldBasic = ((0.15 * fy) / sqrtFc) * db * (1 / lambda); // mm

          ldMin1 = 0.06 * fy * db; // mm
          ldMin2 = 8 * db; // mm

          if (barType === "plain") {
            ldMin2 = Math.max(ldMin2, 12 * db);
            notes.push(
              "Plain bar: minimum multiple of d\u2090 increased (e.g., ≥ 12 d\u2090)."
            );
          }

          ldControl = Math.max(ldBasic, ldMin1, ldMin2);
        }

        // Round for reporting
        const step = system === "us" ? 0.5 : 5; // 0.5 in or 5 mm
        const ldRounded = roundTo(ldControl, step);

        const ldInDb = ldRounded / db;

        // Populate results
        ldMainEl.textContent = ldRounded.toFixed(2);
        ldInDbEl.textContent = ldInDb.toFixed(1);

        ldBasicEl.textContent = ldBasic.toFixed(2);
        ldMinEl.textContent = Math.max(ldMin1, ldMin2).toFixed(2);
        ldFinalEl.textContent = ldRounded.toFixed(2);

        // Step-by-step explanation
        const unitLen = system === "us" ? "in" : "mm";
        const unitFc = system === "us" ? "psi" : "MPa";
        const unitFy = system === "us" ? "ksi" : "MPa";

        let html = "";
        html += `<p><strong>Inputs</strong></p>`;
        html += `<ul class="list-disc ml-5">`;
        html += `<li>Bar diameter d<sub>b</sub> = ${db.toFixed(
          3
        )} ${unitLen}</li>`;
        html += `<li>Concrete strength f'<sub>c</sub> = ${fc.toFixed(
          0
        )} ${unitFc}</li>`;
        html += `<li>Steel yield strength f<sub>y</sub> = ${fy.toFixed(
          1
        )} ${unitFy}</li>`;
        html += `<li>Bar type: ${
          barType === "deformed" ? "Deformed" : "Plain"
        }</li>`;
        html += `<li>Confinement: ${
          confinement === "ties" ? "Ties" : "Spiral"
        }</li>`;
        html += `<li>Concrete type: ${
          lightweight
            ? "Lightweight (λ ≈ " + lambda.toFixed(2) + ")"
            : "Normal-weight (λ = 1.0)"
        }</li>`;
        if (epoxy) {
          html += `<li>Epoxy-coated bar (no additional factor applied for compression ℓ<sub>d</sub>).</li>`;
        }
        html += `</ul>`;

        html += `<p class="mt-2"><strong>Step 1 – Basic development length</strong><br>`;
        if (system === "us") {
          html += `ℓ<sub>dc,basic</sub> = 0.02 · (f<sub>y</sub> / √f'<sub>c</sub>) · d<sub>b</sub> / λ ≈ ${ldBasic.toFixed(
            2
          )} ${unitLen}</p>`;
        } else {
          html += `ℓ<sub>dc,basic</sub> = 0.15 · (f<sub>y</sub> / √f'<sub>c</sub>) · d<sub>b</sub> / λ ≈ ${ldBasic.toFixed(
            2
          )} ${unitLen}</p>`;
        }

        html += `<p><strong>Step 2 – Minimum code limits</strong><br>`;
        html += `Limit 1: ℓ<sub>dc</sub> ≥ ${ldMin1.toFixed(2)} ${unitLen}<br>`;
        html += `Limit 2: ℓ<sub>dc</sub> ≥ ${ldMin2.toFixed(2)} ${unitLen}</p>`;

        html += `<p><strong>Step 3 – Controlling value</strong><br>`;
        html += `ℓ<sub>dc,control</sub> = max(basic, limits) ≈ ${ldControl.toFixed(
          2
        )} ${unitLen}<br>`;
        html += `Rounded up for reporting: ℓ<sub>dc</sub> = ${ldRounded.toFixed(
          2
        )} ${unitLen} ≈ ${ldInDb.toFixed(1)} d<sub>b</sub>.</p>`;

        if (notes.length > 0) {
          html += `<p><strong>Notes:</strong></p><ul class="list-disc ml-5">`;
          notes.forEach((n) => {
            html += `<li>${n}</li>`;
          });
          html += `</ul>`;
        }

        html += `<p class="text-xs text-gray-500 mt-2">This is an approximate implementation of ACI 318 development length in compression for straight bars. Always verify against the exact provisions of the governing ACI 318 edition and your project specifications.</p>`;

        stepByStepEl.innerHTML = html;
        resultsCard.classList.remove("hidden");
      });

      document.getElementById("reset-btn").addEventListener("click", () => {
        unitSystemEl.value = "us";
        barSizeEl.value = "#5";
        dbEl.value = barDataUS["#5"];
        fcEl.value = 4000;
        fyEl.value = 60;
        document.querySelector(
          'input[name="bar-type"][value="deformed"]'
        ).checked = true;
        document.querySelector(
          'input[name="confinement"][value="ties"]'
        ).checked = true;
        document.getElementById("lightweight").checked = false;
        document.getElementById("epoxy").checked = false;
        errorMsgEl.textContent = "";
        resultsCard.classList.add("hidden");
        updateUnits();
      });

      // Initialize
      updateUnits();
    </script>

