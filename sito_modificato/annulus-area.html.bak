    <script>
      // Annulus calculator logic
      (function () {
        const unitSelect = document.getElementById("unit-select");
        const solveForSelect = document.getElementById("solve-for");
        const inputModeSelect = document.getElementById("input-mode");

        const outerInput = document.getElementById("outer-input");
        const innerInput = document.getElementById("inner-input");
        const areaInput = document.getElementById("area-input");
        const areaInputWrapper = document.getElementById("area-input-wrapper");

        const unitLabels = document.querySelectorAll(".unit-label");
        const unitLabelArea = document.querySelector(".unit-label-area");

        const calcBtn = document.getElementById("calculate-btn");
        const resetBtn = document.getElementById("reset-btn");
        const errorMsg = document.getElementById("error-msg");

        const resultsCard = document.getElementById("results-card");
        const resultMain = document.getElementById("result-main");
        const resultSecondary = document.getElementById("result-secondary");
        const resultDetails = document.getElementById("result-details");

        function updateUnitLabels() {
          const u = unitSelect.value;
          unitLabels.forEach((el) => (el.textContent = u));
          if (unitLabelArea) unitLabelArea.textContent = u + "\u00b2";
        }

        function updateVisibility() {
          const solveFor = solveForSelect.value;

          if (solveFor === "area") {
            areaInputWrapper.classList.add("hidden");
            innerInput.parentElement.classList.remove("hidden");
            outerInput.parentElement.classList.remove("hidden");
          } else if (solveFor === "innerRadius") {
            areaInputWrapper.classList.remove("hidden");
            innerInput.parentElement.classList.add("hidden");
            outerInput.parentElement.classList.remove("hidden");
          } else if (solveFor === "outerRadius") {
            areaInputWrapper.classList.remove("hidden");
            innerInput.parentElement.classList.remove("hidden");
            outerInput.parentElement.classList.add("hidden");
          }

          const mode = inputModeSelect.value;
          if (mode === "radius") {
            document.querySelector(
              'label[for="outer-input"]'
            ).childNodes[0].textContent =
              solveFor === "outerRadius"
                ? "Outer radius R "
                : "Outer radius R ";
            document.querySelector(
              'label[for="inner-input"]'
            ).childNodes[0].textContent =
              solveFor === "innerRadius"
                ? "Inner radius r "
                : "Inner radius r ";
          } else {
            document.querySelector(
              'label[for="outer-input"]'
            ).childNodes[0].textContent =
              solveFor === "outerRadius"
                ? "Outer diameter D "
                : "Outer diameter D ";
            document.querySelector(
              'label[for="inner-input"]'
            ).childNodes[0].textContent =
              solveFor === "innerRadius"
                ? "Inner diameter d "
                : "Inner diameter d ";
          }

          clearError();
        }

        function clearError() {
          errorMsg.textContent = "";
        }

        function showError(msg) {
          errorMsg.textContent = msg;
          resultsCard.classList.add("hidden");
        }

        function parsePositive(value, name) {
          const v = parseFloat(value);
          if (isNaN(v) || !isFinite(v)) {
            throw new Error("Please enter a valid number for " + name + ".");
          }
          if (v <= 0) {
            throw new Error(name + " must be greater than 0.");
          }
          return v;
        }

        function formatNumber(x, decimals = 4) {
          if (!isFinite(x)) return "—";
          if (Math.abs(x) >= 1e5 || Math.abs(x) <= 1e-3) {
            return x.toExponential(4);
          }
          return parseFloat(x.toFixed(decimals)).toString();
        }

        function calculate() {
          clearError();
          const solveFor = solveForSelect.value;
          const mode = inputModeSelect.value;
          const unit = unitSelect.value;

          try {
            let R = null,
              r = null,
              A = null;

            if (solveFor === "area") {
              const outer = parsePositive(
                outerInput.value,
                mode === "radius" ? "outer radius" : "outer diameter"
              );
              const inner = parsePositive(
                innerInput.value,
                mode === "radius" ? "inner radius" : "inner diameter"
              );

              if (mode === "radius") {
                R = outer;
                r = inner;
              } else {
                R = outer / 2;
                r = inner / 2;
              }

              if (r >= R) {
                throw new Error(
                  "Inner " +
                    (mode === "radius" ? "radius" : "diameter") +
                    " must be smaller than outer " +
                    (mode === "radius" ? "radius" : "diameter") +
                    "."
                );
              }

              A = Math.PI * (R * R - r * r);

              const areaUnit = unit + "\u00b2";
              resultMain.textContent =
                "A = " + formatNumber(A, 4) + " " + areaUnit;
              resultSecondary.textContent =
                "Annulus area from " +
                (mode === "radius" ? "radii" : "diameters") +
                ".";
              resultDetails.textContent =
                "Using A = \u03c0(R² - r²) with R = " +
                formatNumber(R, 4) +
                " " +
                unit +
                " and r = " +
                formatNumber(r, 4) +
                " " +
                unit +
                ".";
            } else if (solveFor === "innerRadius") {
              const outer = parsePositive(
                outerInput.value,
                mode === "radius" ? "outer radius" : "outer diameter"
              );
              const areaVal = parsePositive(areaInput.value, "area");

              if (mode === "radius") {
                R = outer;
              } else {
                R = outer / 2;
              }
              A = areaVal;

              const term = R * R - A / Math.PI;
              if (term <= 0) {
                throw new Error(
                  "Area is too large for the given outer " +
                    (mode === "radius" ? "radius" : "diameter") +
                    ". It would require a negative inner radius."
                );
              }
              r = Math.sqrt(term);

              const lengthUnit = unit;
              const areaUnit = unit + "\u00b2";

              const displayInner = mode === "radius" ? r : 2 * r;
              const labelInner = mode === "radius" ? "r" : "d";

              resultMain.textContent =
                labelInner +
                " = " +
                formatNumber(displayInner, 4) +
                " " +
                lengthUnit;
              resultSecondary.textContent =
                "Inner " +
                (mode === "radius" ? "radius" : "diameter") +
                " from area and outer " +
                (mode === "radius" ? "radius" : "diameter") +
                ".";
              resultDetails.textContent =
                "Using r = √(R² - A/π) with R = " +
                formatNumber(R, 4) +
                " " +
                lengthUnit +
                " and A = " +
                formatNumber(A, 4) +
                " " +
                areaUnit +
                ".";
            } else if (solveFor === "outerRadius") {
              const inner = parsePositive(
                innerInput.value,
                mode === "radius" ? "inner radius" : "inner diameter"
              );
              const areaVal = parsePositive(areaInput.value, "area");

              if (mode === "radius") {
                r = inner;
              } else {
                r = inner / 2;
              }
              A = areaVal;

              const term = r * r + A / Math.PI;
              R = Math.sqrt(term);

              const lengthUnit = unit;
              const areaUnit = unit + "\u00b2";

              const displayOuter = mode === "radius" ? R : 2 * R;
              const labelOuter = mode === "radius" ? "R" : "D";

              resultMain.textContent =
                labelOuter +
                " = " +
                formatNumber(displayOuter, 4) +
                " " +
                lengthUnit;
              resultSecondary.textContent =
                "Outer " +
                (mode === "radius" ? "radius" : "diameter") +
                " from area and inner " +
                (mode === "radius" ? "radius" : "diameter") +
                ".";
              resultDetails.textContent =
                "Using R = √(r² + A/π) with r = " +
                formatNumber(r, 4) +
                " " +
                lengthUnit +
                " and A = " +
                formatNumber(A, 4) +
                " " +
                areaUnit +
                ".";
            }

            resultsCard.classList.remove("hidden");
          } catch (err) {
            showError(err.message);
          }
        }

        function reset() {
          outerInput.value = "";
          innerInput.value = "";
          areaInput.value = "";
          resultsCard.classList.add("hidden");
          clearError();
        }

        unitSelect.addEventListener("change", () => {
          updateUnitLabels();
          if (!resultsCard.classList.contains("hidden")) {
            calculate();
          }
        });

        solveForSelect.addEventListener("change", () => {
          updateVisibility();
          resultsCard.classList.add("hidden");
        });

        inputModeSelect.addEventListener("change", () => {
          updateVisibility();
          resultsCard.classList.add("hidden");
        });

        calcBtn.addEventListener("click", calculate);
        resetBtn.addEventListener("click", reset);

        [outerInput, innerInput, areaInput].forEach((el) => {
          el.addEventListener("keydown", (e) => {
            if (e.key === "Enter") {
              e.preventDefault();
              calculate();
            }
          });
        });

        updateUnitLabels();
        updateVisibility();
      })();

      // Simple FAQ toggle (in addition to global script_faq if present)
      document.addEventListener("DOMContentLoaded", function () {
        document.querySelectorAll(".faq-question").forEach((btn) => {
          btn.addEventListener("click", () => {
            const answer = btn.nextElementSibling;
            const toggle = btn.querySelector(".faq-toggle");
            const isHidden = answer.classList.contains("hidden");
            document
              .querySelectorAll(".faq-answer")
              .forEach((a) => a.classList.add("hidden"));
            document
              .querySelectorAll(".faq-toggle")
              .forEach((t) => (t.textContent = "+"));
            if (isHidden) {
              answer.classList.remove("hidden");
              if (toggle) toggle.textContent = "−";
            }
          });
        });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
