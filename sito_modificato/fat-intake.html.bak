    <script>
      // Utility conversions
      function toKg(weight, unit) {
        return unit === "lb" ? weight * 0.45359237 : weight;
      }
      function toCm(height, unit) {
        return unit === "in" ? height * 2.54 : height;
      }

      function round(value, decimals) {
        const factor = Math.pow(10, decimals);
        return Math.round(value * factor) / factor;
      }

      function calculateCalories() {
        const sex = document.getElementById("sex").value;
        const age = parseFloat(document.getElementById("age").value) || 0;
        const height = parseFloat(document.getElementById("height").value) || 0;
        const weight = parseFloat(document.getElementById("weight").value) || 0;
        const heightUnit = document.getElementById("height-unit").value;
        const weightUnit = document.getElementById("weight-unit").value;
        const activity = parseFloat(document.getElementById("activity").value);
        const goal = document.getElementById("goal").value;

        const wKg = toKg(weight, weightUnit);
        const hCm = toCm(height, heightUnit);

        if (!age || !wKg || !hCm) return null;

        let rmr;
        if (sex === "male") {
          rmr = 10 * wKg + 6.25 * hCm - 5 * age + 5;
        } else if (sex === "female") {
          rmr = 10 * wKg + 6.25 * hCm - 5 * age - 161;
        } else {
          // average of male/female constants
          rmr = 10 * wKg + 6.25 * hCm - 5 * age - 78;
        }

        let tdee = rmr * activity;

        switch (goal) {
          case "loss-10":
            tdee *= 0.9;
            break;
          case "loss-20":
            tdee *= 0.8;
            break;
          case "gain-10":
            tdee *= 1.1;
            break;
          case "gain-20":
            tdee *= 1.2;
            break;
          default:
            break;
        }

        return tdee;
      }

      function updateFatPercentDisplay() {
        const slider = document.getElementById("fat-percent");
        const display = document.getElementById("fat-percent-display");
        if (slider && display) {
          display.textContent = slider.value + "%";
        }
      }

      function calculateFatIntake() {
        const caloriesOutput = document.getElementById("calories-output");
        const totalFatOutput = document.getElementById("total-fat-output");
        const totalFatKcalText = document.getElementById("total-fat-kcal-text");
        const satFatOutput = document.getElementById("sat-fat-output");
        const satFatKcalText = document.getElementById("sat-fat-kcal-text");
        const omega3Output = document.getElementById("omega3-output");
        const omega6Output = document.getElementById("omega6-output");

        const summaryTotalFat = document.getElementById("summary-total-fat");
        const summarySatFat = document.getElementById("summary-sat-fat");
        const summaryOmega3 = document.getElementById("summary-omega3");
        const summaryOmega6 = document.getElementById("summary-omega6");

        const fatPercent = parseFloat(
          document.getElementById("fat-percent").value
        );
        const customCaloriesInput = document.getElementById("custom-calories");
        let calories;

        if (
          customCaloriesInput &&
          customCaloriesInput.dataset.useCustom === "true"
        ) {
          calories = parseFloat(customCaloriesInput.value);
        } else {
          calories = calculateCalories();
        }

        if (!calories || calories < 500) {
          caloriesOutput.textContent = "—";
          totalFatOutput.textContent = "—";
          totalFatKcalText.textContent = "";
          satFatOutput.textContent = "—";
          satFatKcalText.textContent = "";
          omega3Output.textContent = "—";
          omega6Output.textContent = "—";
          summaryTotalFat.textContent = "—";
          summarySatFat.textContent = "—";
          summaryOmega3.textContent = "—";
          summaryOmega6.textContent = "—";
          return;
        }

        const kcalRounded = Math.round(calories);
        caloriesOutput.textContent = kcalRounded.toLocaleString();

        // Total fat
        const kcalFromFat = calories * (fatPercent / 100);
        const gramsFat = kcalFromFat / 9;
        const gramsFatRounded = round(gramsFat, 1);
        totalFatOutput.textContent = gramsFatRounded + " g/day";
        totalFatKcalText.textContent = `${Math.round(
          kcalFromFat
        )} kcal from fat (${fatPercent}% of total calories).`;
        summaryTotalFat.textContent = `${gramsFatRounded} g/day`;

        // Saturated fat: default 8% of calories (between 7–10%)
        const satPercent = 8;
        const kcalSat = calories * (satPercent / 100);
        const gramsSat = kcalSat / 9;
        const gramsSatRounded = round(gramsSat, 1);
        satFatOutput.textContent = gramsSatRounded + " g/day";
        satFatKcalText.textContent = `${Math.round(
          kcalSat
        )} kcal from saturated fat (${satPercent}% of calories).`;
        summarySatFat.textContent = `${gramsSatRounded} g/day (≈${satPercent}% kcal)`;

        // Omega-3 and omega-6 (approximate)
        // Assume ~5–10% of total fat as omega-3 and 15–25% as omega-6
        const omega3Min = gramsFat * 0.05;
        const omega3Max = gramsFat * 0.1;
        const omega6Min = gramsFat * 0.15;
        const omega6Max = gramsFat * 0.25;

        omega3Output.textContent = `${round(omega3Min, 1)}–${round(
          omega3Max,
          1
        )} g/day (approx.)`;
        omega6Output.textContent = `${round(omega6Min, 1)}–${round(
          omega6Max,
          1
        )} g/day (approx.)`;

        summaryOmega3.textContent = omega3Output.textContent;
        summaryOmega6.textContent = omega6Output.textContent;
      }

      document.addEventListener("DOMContentLoaded", function () {
        updateFatPercentDisplay();
        calculateFatIntake();

        const fatSlider = document.getElementById("fat-percent");
        if (fatSlider) {
          fatSlider.addEventListener("input", function () {
            updateFatPercentDisplay();
          });
          fatSlider.addEventListener("change", calculateFatIntake);
        }

        const inputsToWatch = [
          "sex",
          "age",
          "height",
          "weight",
          "height-unit",
          "weight-unit",
          "activity",
          "goal",
        ];
        inputsToWatch.forEach((id) => {
          const el = document.getElementById(id);
          if (el) {
            el.addEventListener("change", function () {
              // If user changes core inputs, stop forcing custom calories
              const customCaloriesInput =
                document.getElementById("custom-calories");
              if (customCaloriesInput) {
                delete customCaloriesInput.dataset.useCustom;
              }
              calculateFatIntake();
            });
            if (el.tagName === "INPUT") {
              el.addEventListener("input", function () {
                const customCaloriesInput =
                  document.getElementById("custom-calories");
                if (customCaloriesInput) {
                  delete customCaloriesInput.dataset.useCustom;
                }
              });
            }
          }
        });

        const useCustomBtn = document.getElementById("use-custom-calories");
        if (useCustomBtn) {
          useCustomBtn.addEventListener("click", function () {
            const customCaloriesInput =
              document.getElementById("custom-calories");
            if (!customCaloriesInput) return;
            const val = parseFloat(customCaloriesInput.value);
            if (!val || val < 800 || val > 6000) {
              alert(
                "Please enter a realistic daily calorie value between 800 and 6000."
              );
              return;
            }
            customCaloriesInput.dataset.useCustom = "true";
            calculateFatIntake();
          });
        }

        const calcBtn = document.getElementById("calculate-btn");
        if (calcBtn) {
          calcBtn.addEventListener("click", calculateFatIntake);
        }
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
