<script>
  // Nutrient definitions: base ranges for adult males 19–50 (approximate, simplified)
  const baseNutrients = [
    { id: 'vitA', name: 'Vitamin A', unit: 'mcg RAE', baseMin: 700, baseMax: 900 },
    { id: 'vitC', name: 'Vitamin C', unit: 'mg', baseMin: 75, baseMax: 120 },
    { id: 'vitD', name: 'Vitamin D', unit: 'IU', baseMin: 600, baseMax: 2000 },
    { id: 'vitE', name: 'Vitamin E', unit: 'mg', baseMin: 10, baseMax: 20 },
    { id: 'vitK', name: 'Vitamin K', unit: 'mcg', baseMin: 90, baseMax: 120 },
    { id: 'b1', name: 'Thiamin (B1)', unit: 'mg', baseMin: 1.1, baseMax: 1.5 },
    { id: 'b2', name: 'Riboflavin (B2)', unit: 'mg', baseMin: 1.1, baseMax: 1.6 },
    { id: 'b3', name: 'Niacin (B3)', unit: 'mg', baseMin: 14, baseMax: 20 },
    { id: 'b6', name: 'Vitamin B6', unit: 'mg', baseMin: 1.3, baseMax: 2 },
    { id: 'folate', name: 'Folate', unit: 'mcg DFE', baseMin: 300, baseMax: 500 },
    { id: 'b12', name: 'Vitamin B12', unit: 'mcg', baseMin: 2, baseMax: 6 },
    { id: 'calcium', name: 'Calcium', unit: 'mg', baseMin: 900, baseMax: 1300 },
    { id: 'iron', name: 'Iron', unit: 'mg', baseMin: 8, baseMax: 18 },
    { id: 'magnesium', name: 'Magnesium', unit: 'mg', baseMin: 300, baseMax: 420 },
    { id: 'zinc', name: 'Zinc', unit: 'mg', baseMin: 8, baseMax: 15 }
  ];

  function createIntakeTable() {
    const tbody = document.getElementById('intake-table-body');
    tbody.innerHTML = '';
    baseNutrients.forEach(n => {
      const tr = document.createElement('tr');
      tr.innerHTML = `
        <td class="px-2 py-2 text-gray-800">${n.name}</td>
        <td class="px-2 py-2">
          <input type="number" step="0.1" min="0" id="intake-${n.id}" class="w-24 border-gray-300 rounded-md shadow-sm focus:ring-blue-500 focus:border-blue-500 px-1 py-0.5 text-xs">
        </td>
        <td class="px-2 py-2 text-gray-600">${n.unit}</td>
      `;
      tbody.appendChild(tr);
    });
  }

  function adjustRangesForProfile() {
    const age = parseInt(document.getElementById('age').value, 10);
    const sex = document.querySelector('input[name="sex"]:checked')?.value || 'male';
    const pregStatus = document.getElementById('pregnancyStatus').value;

    return baseNutrients.map(n => {
      let min = n.baseMin;
      let max = n.baseMax;

      // Age adjustments (very simplified)
      if (age >= 51 && age <= 70) {
        if (n.id === 'vitD') { min = 800; max = 2000; }
        if (n.id === 'calcium') { min = 1000; max = 1300; }
      } else if (age > 70) {
        if (n.id === 'vitD') { min = 800; max = 3000; }
        if (n.id === 'calcium') { min = 1200; max = 1500; }
      } else if (age >= 14 && age <= 18) {
        if (n.id === 'calcium') { min = 1200; max = 1500; }
        if (n.id === 'iron' && sex === 'female') { min = 15; max = 18; }
      }

      // Sex differences (simplified)
      if (sex === 'female') {
        if (n.id === 'vitA') { min = 600; max = 800; }
        if (n.id === 'vitC') { min = 65; max = 110; }
        if (n.id === 'iron' && age >= 19 && age <= 50) { min = 15; max = 20; }
        if (n.id === 'magnesium') { min = 260; max = 360; }
        if (n.id === 'zinc') { min = 8; max = 12; }
      }

      // Pregnancy / lactation (only for females 14–50)
      const inReproAge = age >= 14 && age <= 50;
      if (sex === 'female' && inReproAge) {
        if (pregStatus === 'pregnant') {
          if (n.id === 'vitA') { min += 70; max += 100; }
          if (n.id === 'vitC') { min += 10; max += 20; }
          if (n.id === 'vitD') { min = Math.max(min, 600); }
          if (n.id === 'folate') { min = 600; max = 800; }
          if (n.id === 'iron') { min = 27; max = 30; }
          if (n.id === 'b12') { min += 0.2; max += 0.5; }
        } else if (pregStatus === 'lactating') {
          if (n.id === 'vitA') { min += 200; max += 250; }
          if (n.id === 'vitC') { min += 30; max += 40; }
          if (n.id === 'vitD') { min = Math.max(min, 600); }
          if (n.id === 'folate') { min = 500; max = 700; }
          if (n.id === 'b12') { min += 0.4; max += 0.8; }
          if (n.id === 'calcium') { min = Math.max(min, 1000); }
        }
      }

      return { ...n, min, max };
    });
  }

  function classifyIntake(intake, min, max) {
    if (intake == null || isNaN(intake)) return { label: 'No intake entered', color: 'text-gray-500', badgeClass: 'bg-gray-100 border-gray-200 text-gray-700' };
    if (intake < min) return { label: 'Below range', color: 'text-amber-700', badgeClass: 'bg-amber-100 border-amber-200 text-amber-800' };
    if (intake > max) return { label: 'Above range', color: 'text-red-700', badgeClass: 'bg-red-100 border-red-200 text-red-800' };
    return { label: 'Within range', color: 'text-green-700', badgeClass: 'bg-green-100 border-green-200 text-green-800' };
  }

  function calculateAndRender() {
    const adjusted = adjustRangesForProfile();
    const dietPattern = document.getElementById('dietPattern').value;
    const tbody = document.getElementById('results-table-body');
    tbody.innerHTML = '';

    adjusted.forEach(n => {
      const intakeVal = parseFloat(document.getElementById(`intake-${n.id}`)?.value);
      const status = classifyIntake(intakeVal, n.min, n.max);

      // Diet-based highlight (for vegan/vegetarian)
      let dietNote = '';
      if (dietPattern === 'vegetarian' || dietPattern === 'vegan') {
        if (['b12', 'vitD', 'iron', 'zinc', 'calcium'].includes(n.id)) {
          dietNote = ' • higher attention on plant-based diets';
        }
      }

      const tr = document.createElement('tr');
      tr.innerHTML = `
        <td class="px-2 py-2 text-gray-800">${n.name}</td>
        <td class="px-2 py-2 text-gray-700">${n.min.toFixed( (n.min < 10 && n.min % 1 !== 0) ? 1 : 0 )}–${n.max.toFixed( (n.max < 10 && n.max % 1 !== 0) ? 1 : 0 )} ${n.unit}</td>
        <td class="px-2 py-2 text-gray-700">${isNaN(intakeVal) ? '<span class="text-gray-400">—</span>' : intakeVal + ' ' + n.unit}</td>
        <td class="px-2 py-2">
          <span class="badge ${status.badgeClass}">${status.label}</span>
          ${dietNote ? `<span class="block text-[0.65rem] text-purple-700 mt-0.5">${dietNote}</span>` : ''}
        </td>
      `;
      tbody.appendChild(tr);
    });
  }

  document.addEventListener('DOMContentLoaded', () => {
    createIntakeTable();
    calculateAndRender();

    document.getElementById('calculate-btn').addEventListener('click', calculateAndRender);
    document.getElementById('age').addEventListener('change', calculateAndRender);
    document.getElementById('pregnancyStatus').addEventListener('change', calculateAndRender);
    document.getElementById('dietPattern').addEventListener('change', calculateAndRender);
    document.querySelectorAll('input[name="sex"]').forEach(r => r.addEventListener('change', calculateAndRender));

    // Simple FAQ toggle (in addition to global script_faq, safe if both run)
    document.querySelectorAll('#faq-container .faq-question').forEach(btn => {
      btn.addEventListener('click', () => {
        const answer = btn.parentElement.querySelector('.faq-answer');
        const isHidden = answer.classList.contains('hidden');
        document.querySelectorAll('#faq-container .faq-answer').forEach(a => a.classList.add('hidden'));
        document.querySelectorAll('#faq-container .faq-question span:last-child').forEach(icon => icon.textContent = '+');
        if (isHidden) {
          answer.classList.remove('hidden');
          btn.querySelector('span:last-child').textContent = '−';
        }
      });
    });
  });

  window.MathJax = {
    tex: { inlineMath: [['\\(','\\)'], ['$', '$']], displayMath: [['$','$'], ['\\[','\\]']] },
    svg: { fontCache: 'global' }
  };
</script>
