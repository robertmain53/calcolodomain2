    const hhdInput = document.getElementById('hhd-input');
    const gallonsUsOutput = document.getElementById('gallons-us-output');
    const gallonsImpOutput = document.getElementById('gallons-imp-output');

    const gallonsInput = document.getElementById('gallons-input');
    const hhdOutput = document.getElementById('hhd-output');

    const hhdLabel = document.getElementById('hhd-label');

    let currentUs = 63;     // default US hogshead
    let currentImp = 52.5;  // default UK hogshead
    let isUpdating = false;

    function updateFromHogshead() {
      if (isUpdating) return;
      isUpdating = true;
      const h = parseFloat(hhdInput.value || 0);

      // US
      if (currentUs && currentUs !== 'none') {
        gallonsUsOutput.value = +(h * currentUs).toFixed(2);
      } else {
        gallonsUsOutput.value = '';
      }

      // IMP
      if (currentImp && currentImp !== 'none') {
        gallonsImpOutput.value = +(h * currentImp).toFixed(2);
      } else {
        gallonsImpOutput.value = '';
      }

      isUpdating = false;
    }

    function updateFromGallons() {
      if (isUpdating) return;
      isUpdating = true;
      const g = parseFloat(gallonsInput.value || 0);

      // Prefer US if defined, else imperial
      let divisor = currentUs && currentUs !== 'none' ? currentUs : (currentImp && currentImp !== 'none' ? currentImp : null);
      const hhd = divisor ? g / divisor : 0;
      hhdOutput.value = +hhd.toFixed(4);
      isUpdating = false;
    }

    presetBtns.forEach(btn => {
      btn.addEventListener('click', () => {
        presetBtns.forEach(b => b.classList.remove('mode-pill-active'));
        btn.classList.add('mode-pill-active');

        const us = btn.dataset.us;
        const imp = btn.dataset.imp;

        currentUs = us === 'none' ? null : parseFloat(us);
        currentImp = imp === 'none' ? null : parseFloat(imp);

        let labelText = '1 hogshead = ';
        if (currentUs) labelText += currentUs + ' US gal';
        if (currentUs && currentImp) labelText += ' and ';
        if (currentImp) labelText += currentImp + ' imp gal';
        hhdLabel.textContent = labelText;

        updateFromHogshead();
        updateFromGallons();
      });
    });

    hhdInput.addEventListener('input', updateFromHogshead);
    gallonsInput.addEventListener('input', updateFromGallons);

    // init with defaults
    updateFromHogshead();
    updateFromGallons();

    // fill table static (for common US 63 / UK 52.5)
    document.getElementById('tbl-0-25-us').textContent = (0.25 * 63).toFixed(2);
    document.getElementById('tbl-0-25-imp').textContent = (0.25 * 52.5).toFixed(2);
    document.getElementById('tbl-0-5-us').textContent = (0.5 * 63).toFixed(2);
    document.getElementById('tbl-0-5-imp').textContent = (0.5 * 52.5).toFixed(2);
    document.getElementById('tbl-1-us').textContent = (1 * 63).toFixed(2);
    document.getElementById('tbl-1-imp').textContent = (1 * 52.5).toFixed(2);
    document.getElementById('tbl-2-us').textContent = (2 * 63).toFixed(2);
    document.getElementById('tbl-2-imp').textContent = (2 * 52.5).toFixed(2);
    document.getElementById('tbl-5-us').textContent = (5 * 63).toFixed(2);
    document.getElementById('tbl-5-imp').textContent = (5 * 52.5).toFixed(2);
  </script>

