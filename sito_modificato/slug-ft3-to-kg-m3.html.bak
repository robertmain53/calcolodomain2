    <script>
      (function () {
        const FACTOR = 515.3788; // 1 slug/ft^3 = 515.3788 kg/m^3

        const dirSlugToKgBtn = document.getElementById("dir-slug-to-kg");
        const dirKgToSlugBtn = document.getElementById("dir-kg-to-slug");
        const inputValueEl = document.getElementById("input-value");
        const outputValueEl = document.getElementById("output-value");
        const inputUnitLabelEl = document.getElementById("input-unit-label");
        const outputUnitLabelEl = document.getElementById("output-unit-label");
        const factorLabelEl = document.getElementById("factor-label");
        const decimalsEl = document.getElementById("decimals");
        const inputErrorEl = document.getElementById("input-error");
        const swapBtn = document.getElementById("swap-btn");
        const quickTableBody = document.getElementById("quick-table-body");
        const presetButtons = document.querySelectorAll(".preset-btn");

        let direction = "slug-to-kg"; // or 'kg-to-slug'

        function formatNumber(value) {
          const decimals = parseInt(decimalsEl.value, 10) || 3;
          if (!isFinite(value)) return "";
          return value.toFixed(decimals);
        }

        function updateLabels() {
          if (direction === "slug-to-kg") {
            inputUnitLabelEl.textContent = "slug/ft³";
            outputUnitLabelEl.textContent = "kg/m³";
            factorLabelEl.textContent = "1 slug/ft³ = 515.3788 kg/m³";
          } else {
            inputUnitLabelEl.textContent = "kg/m³";
            outputUnitLabelEl.textContent = "slug/ft³";
            factorLabelEl.textContent = "1 kg/m³ = 0.0019390 slug/ft³";
          }
        }

        function convert() {
          const raw = inputValueEl.value.trim();
          if (raw === "") {
            outputValueEl.value = "";
            inputErrorEl.classList.add("hidden");
            return;
          }

          const val = Number(raw);
          if (!isFinite(val)) {
            inputErrorEl.classList.remove("hidden");
            outputValueEl.value = "";
            return;
          }

          inputErrorEl.classList.add("hidden");

          let result;
          if (direction === "slug-to-kg") {
            result = val * FACTOR;
          } else {
            result = val / FACTOR;
          }
          outputValueEl.value = formatNumber(result);
        }

        function setDirection(newDir) {
          direction = newDir;
          if (direction === "slug-to-kg") {
            dirSlugToKgBtn.classList.add("toggle-active");
            dirSlugToKgBtn.classList.remove("bg-white", "text-blue-600");
            dirKgToSlugBtn.classList.remove("toggle-active");
            dirKgToSlugBtn.classList.add("bg-white", "text-blue-600");
          } else {
            dirKgToSlugBtn.classList.add("toggle-active");
            dirKgToSlugBtn.classList.remove("bg-white", "text-blue-600");
            dirSlugToKgBtn.classList.remove("toggle-active");
            dirSlugToKgBtn.classList.add("bg-white", "text-blue-600");
          }
          updateLabels();
          convert();
        }

        function swapUnits() {
          // Swap direction but keep current numeric values logically consistent
          if (outputValueEl.value.trim() !== "") {
            inputValueEl.value = outputValueEl.value;
          }
          setDirection(
            direction === "slug-to-kg" ? "kg-to-slug" : "slug-to-kg"
          );
          convert();
        }

        function buildQuickTable() {
          const baseValuesSlug = [0.001, 0.002, 0.005, 0.01, 1.0, 1.94, 2.0];
          quickTableBody.innerHTML = "";
          baseValuesSlug.forEach((v) => {
            const kg = v * FACTOR;
            const tr = document.createElement("tr");
            tr.innerHTML = `
          <td class="px-3 py-1 border-b text-gray-700">${v}</td>
          <td class="px-3 py-1 border-b text-gray-700">${kg.toFixed(3)}</td>
        `;
            quickTableBody.appendChild(tr);
          });
        }

        // Event listeners
        dirSlugToKgBtn.addEventListener("click", () =>
          setDirection("slug-to-kg")
        );
        dirKgToSlugBtn.addEventListener("click", () =>
          setDirection("kg-to-slug")
        );
        inputValueEl.addEventListener("input", convert);
        decimalsEl.addEventListener("change", convert);
        swapBtn.addEventListener("click", swapUnits);

        presetButtons.forEach((btn) => {
          btn.addEventListener("click", () => {
            const val = btn.getAttribute("data-preset");
            if (direction !== "slug-to-kg") {
              // Presets are defined in slug/ft³; ensure direction matches
              setDirection("slug-to-kg");
            }
            inputValueEl.value = val;
            convert();
          });
        });

        // Initialize
        updateLabels();
        buildQuickTable();
      })();
    </script>

