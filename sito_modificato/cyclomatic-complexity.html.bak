    <script>
      // Tab switching
      const tabCode = document.getElementById("tab-code");
      const tabGraph = document.getElementById("tab-graph");
      const panelCode = document.getElementById("panel-code");
      const panelGraph = document.getElementById("panel-graph");

      tabCode.addEventListener("click", () => {
        tabCode.classList.add("border-blue-600", "text-blue-600");
        tabCode.classList.remove("text-gray-600");
        tabGraph.classList.remove("border-blue-600", "text-blue-600");
        tabGraph.classList.add("text-gray-600");
        panelCode.classList.remove("hidden");
        panelGraph.classList.add("hidden");
      });

      tabGraph.addEventListener("click", () => {
        tabGraph.classList.add("border-blue-600", "text-blue-600");
        tabGraph.classList.remove("text-gray-600");
        tabCode.classList.remove("border-blue-600", "text-blue-600");
        tabCode.classList.add("text-gray-600");
        panelGraph.classList.remove("hidden");
        panelCode.classList.add("hidden");
      });

      // Utility: strip comments (very simple heuristics)
      function stripComments(code, langProfile) {
        if (!code) return "";
        let result = code;

        if (langProfile === "python") {
          // Remove # comments
          result = result.replace(/#.*$/gm, "");
          // Remove triple-quoted strings (often used as docstrings)
          result = result.replace(/("""|''')[\s\S]*?\1/gm, "");
        } else if (langProfile === "matlab") {
          result = result.replace(/%.*$/gm, "");
        } else {
          // C-like: // and /* */
          result = result.replace(/\/\/.*$/gm, "");
          result = result.replace(/\/\*[\s\S]*?\*\//gm, "");
        }
        return result;
      }

      function countDecisions(code, langProfile, countLogical) {
        const counts = {
          ifs: 0,
          fors: 0,
          whiles: 0,
          cases: 0,
          catches: 0,
          ternary: 0,
          logicalOps: 0,
        };
        if (!code.trim()) return counts;

        const cLike = langProfile === "c_like" || langProfile === "generic";
        const pyLike = langProfile === "python";
        const matlabLike = langProfile === "matlab";

        // Basic regexes (word boundaries to avoid partial matches)
        if (cLike || pyLike || matlabLike) {
          counts.ifs = (code.match(/\bif\b/g) || []).length;
          counts.fors = (code.match(/\bfor\b/g) || []).length;
          counts.whiles = (code.match(/\bwhile\b/g) || []).length;
        }

        if (cLike) {
          counts.cases = (code.match(/\bcase\b/g) || []).length;
          counts.catches = (code.match(/\bcatch\b/g) || []).length;
          counts.ternary = (code.match(/\?/g) || []).length;
          if (countLogical) {
            counts.logicalOps =
              (code.match(/&&/g) || []).length +
              (code.match(/\|\|/g) || []).length;
          }
        } else if (pyLike) {
          // Python: elif counts as additional decision
          const elifs = (code.match(/\belif\b/g) || []).length;
          counts.ifs += elifs;
          // try/except
          counts.catches = (code.match(/\bexcept\b/g) || []).length;
          if (countLogical) {
            counts.logicalOps =
              (code.match(/\band\b/g) || []).length +
              (code.match(/\bor\b/g) || []).length;
          }
        } else if (matlabLike) {
          // elseif in MATLAB
          const elseifs = (code.match(/\belseif\b/g) || []).length;
          counts.ifs += elseifs;
          if (countLogical) {
            counts.logicalOps =
              (code.match(/&&/g) || []).length +
              (code.match(/\|\|/g) || []).length;
          }
          // switch/case
          counts.cases = (code.match(/\bcase\b/g) || []).length;
        }

        return counts;
      }

      function classifyComplexity(vg) {
        if (vg == null || isNaN(vg)) {
          return {
            label: "N/A",
            band: "none",
            text: "Run the calculator to see the complexity rating.",
          };
        }
        if (vg <= 10) {
          return {
            label: "Low",
            band: "low",
            text: `Cyclomatic complexity ${vg} is low. The function is simple and should be easy to understand and test.`,
          };
        } else if (vg <= 20) {
          return {
            label: "Moderate",
            band: "medium",
            text: `Cyclomatic complexity ${vg} is moderate. Ensure you have good test coverage and consider refactoring if it grows further.`,
          };
        } else if (vg <= 50) {
          return {
            label: "High",
            band: "high",
            text: `Cyclomatic complexity ${vg} is high. Strongly consider splitting this function into smaller, focused units.`,
          };
        } else {
          return {
            label: "Very high",
            band: "high",
            text: `Cyclomatic complexity ${vg} is very high. The code is likely hard to test and maintain; refactoring is highly recommended.`,
          };
        }
      }

      function updateBadge(band) {
        const badge = document.getElementById("result-badge");
        badge.classList.remove("badge-low", "badge-medium", "badge-high");
        if (band === "low") {
          badge.classList.add("badge-low");
          badge.textContent = "Low complexity";
        } else if (band === "medium") {
          badge.classList.add("badge-medium");
          badge.textContent = "Moderate complexity";
        } else if (band === "high") {
          badge.classList.add("badge-high");
          badge.textContent = "High complexity";
        } else {
          badge.textContent = "No data";
        }
      }

      // Code mode handlers
      const btnCalcCode = document.getElementById("btn-calc-code");
      const btnClearCode = document.getElementById("btn-clear-code");
      const btnLoadExample = document.getElementById("btn-load-example");
      const codeInput = document.getElementById("code-input");
      const codeError = document.getElementById("code-error");

      const resultVg = document.getElementById("result-vg");
      const resultRating = document.getElementById("result-rating");
      const interpretationText = document.getElementById("interpretation-text");
      const panelBreakdown = document.getElementById("panel-breakdown");

      const countIfEl = document.getElementById("count-if");
      const countForEl = document.getElementById("count-for");
      const countWhileEl = document.getElementById("count-while");
      const countCaseEl = document.getElementById("count-case");
      const countCatchEl = document.getElementById("count-catch");
      const countTernaryEl = document.getElementById("count-ternary");
      const countLogicalOpEl = document.getElementById("count-logical-op");

      btnLoadExample.addEventListener("click", () => {
        const example = `int classify(int x, int y) {
    if (x > 0 && y > 0) {
        if (x > y) {
            return 1;
        } else if (x == y) {
            return 2;
        } else {
            return 3;
        }
    } else if (x < 0 || y < 0) {
        return 4;
    } else {
        switch (x) {
            case 0:
                return 5;
            case 1:
                return 6;
            default:
                return 7;
        }
    }
}`;
        codeInput.value = example;
      });

      btnClearCode.addEventListener("click", () => {
        codeInput.value = "";
        codeError.classList.add("hidden");
      });

      btnCalcCode.addEventListener("click", () => {
        codeError.classList.add("hidden");
        const rawCode = codeInput.value || "";
        if (!rawCode.trim()) {
          codeError.textContent = "Please paste some code first.";
          codeError.classList.remove("hidden");
          return;
        }
        const langProfile = document.getElementById("language-select").value;
        const countLogical = document.getElementById("count-logical").checked;
        const ignoreComments =
          document.getElementById("ignore-comments").checked;

        let code = rawCode;
        if (ignoreComments) {
          code = stripComments(code, langProfile);
        }

        const counts = countDecisions(code, langProfile, countLogical);
        const totalDecisions =
          counts.ifs +
          counts.fors +
          counts.whiles +
          counts.cases +
          counts.catches +
          counts.ternary +
          counts.logicalOps;
        const vg = 1 + totalDecisions;

        // Update breakdown
        countIfEl.textContent = counts.ifs;
        countForEl.textContent = counts.fors;
        countWhileEl.textContent = counts.whiles;
        countCaseEl.textContent = counts.cases;
        countCatchEl.textContent = counts.catches;
        countTernaryEl.textContent = counts.ternary;
        countLogicalOpEl.textContent = counts.logicalOps;
        panelBreakdown.classList.remove("hidden");

        // Update main result
        resultVg.textContent = vg.toString();
        const cls = classifyComplexity(vg);
        resultRating.textContent = cls.label;
        interpretationText.textContent = cls.text;
        updateBadge(cls.band);
      });

      // Graph mode handlers
      const btnCalcGraph = document.getElementById("btn-calc-graph");
      const nodesInput = document.getElementById("nodes-input");
      const edgesInput = document.getElementById("edges-input");
      const componentsInput = document.getElementById("components-input");
      const graphError = document.getElementById("graph-error");

      btnCalcGraph.addEventListener("click", () => {
        graphError.classList.add("hidden");
        const N = parseInt(nodesInput.value, 10);
        const E = parseInt(edgesInput.value, 10);
        const P = parseInt(componentsInput.value, 10);

        if (isNaN(N) || isNaN(E) || isNaN(P) || N <= 0 || E < 0 || P <= 0) {
          graphError.textContent =
            "Please enter valid non-negative integers (N > 0, E ≥ 0, P > 0).";
          graphError.classList.remove("hidden");
          return;
        }

        const vg = E - N + 2 * P;
        resultVg.textContent = vg.toString();
        const cls = classifyComplexity(vg);
        resultRating.textContent = cls.label;
        interpretationText.textContent =
          `Using V(G) = E − N + 2P with N = ${N}, E = ${E}, P = ${P}, the cyclomatic complexity is ${vg}. ` +
          cls.text;
        updateBadge(cls.band);
      });
    </script>

