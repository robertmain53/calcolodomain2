    <script>
      (function () {
        const unitSystemEl = document.getElementById("unit-system");
        const crackGeometryEl = document.getElementById("crack-geometry");
        const stressInputEl = document.getElementById("stress-input");
        const crackLengthEl = document.getElementById("crack-length-input");
        const widthEl = document.getElementById("width-input");
        const kcEl = document.getElementById("kc-input");

        const stressUnitEl = document.getElementById("stress-unit");
        const lengthUnitEl = document.getElementById("length-unit");
        const widthUnitEl = document.getElementById("width-unit");
        const kcUnitEl = document.getElementById("kc-unit");

        const computeBtn = document.getElementById("compute-btn");
        const resetBtn = document.getElementById("reset-btn");

        const resultsCard = document.getElementById("results-card");
        const modeOutput = document.getElementById("mode-output");
        const fOutput = document.getElementById("f-output");
        const kOutput = document.getElementById("k-output");
        const kcSummary = document.getElementById("kc-summary");
        const kcOutput = document.getElementById("kc-output");
        const validityWarning = document.getElementById("validity-warning");
        const widthGroup = document.getElementById("width-group");
        const geometryNote = document.getElementById("geometry-note");

        function updateUnits() {
          const system = unitSystemEl.value;
          if (system === "si") {
            stressUnitEl.textContent = "MPa";
            lengthUnitEl.textContent = "m";
            widthUnitEl.textContent = "m";
            kcUnitEl.textContent = "MPa·√m";
          } else {
            stressUnitEl.textContent = "ksi";
            lengthUnitEl.textContent = "in";
            widthUnitEl.textContent = "in";
            kcUnitEl.textContent = "ksi·√in";
          }
        }

        function getModeLabel(key) {
          if (key.includes("modeI")) return "Mode I (opening)";
          if (key.includes("modeII")) return "Mode II (sliding)";
          if (key.includes("modeIII")) return "Mode III (tearing)";
          return "Mode I";
        }

        function requiresWidth(key) {
          return key !== "through_modeI";
        }

        function geometryInfo(key) {
          switch (key) {
            case "edge_modeI":
            case "edge_modeII":
            case "edge_modeIII":
              return {
                note: "Single edge crack in finite width plate. Polynomial geometry factor valid approximately for 0 ≤ a/W ≤ 0.6.",
                maxRatio: 0.6,
              };
            case "center_modeI":
              return {
                note: "Center crack in finite width plate. sec(πa/W) geometry factor accurate for a/W up to about 0.5.",
                maxRatio: 0.5,
              };
            case "through_modeI":
              return {
                note: "Through-thickness crack in an infinite plate. Geometry factor F = 1.0 (no width required).",
                maxRatio: null,
              };
            default:
              return { note: "", maxRatio: null };
          }
        }

        function computeGeometryFactorEdge(aOverW) {
          const x = aOverW;
          return (
            1.12 -
            0.231 * x +
            10.55 * x * x -
            21.72 * Math.pow(x, 3) +
            30.39 * Math.pow(x, 4)
          );
        }

        function computeGeometryFactorCenter(aOverW) {
          const ratio = aOverW;
          const arg = Math.PI * ratio;
          const sec = 1 / Math.cos(arg);
          return Math.sqrt(sec);
        }

        function computeK() {
          const geomKey = crackGeometryEl.value;
          const sigma = parseFloat(stressInputEl.value);
          const a = parseFloat(crackLengthEl.value);
          const W = parseFloat(widthEl.value);

          if (!isFinite(sigma) || !isFinite(a) || sigma <= 0 || a <= 0) {
            alert("Please enter positive values for stress and crack length.");
            return;
          }

          let F = 1.0;
          let aOverW = null;
          const info = geometryInfo(geomKey);

          if (requiresWidth(geomKey)) {
            if (!isFinite(W) || W <= 0) {
              alert("Please enter a positive plate width W.");
              return;
            }
            aOverW = a / W;
            if (aOverW <= 0) {
              alert("Crack length a must be less than plate width W.");
              return;
            }
            if (
              geomKey === "edge_modeI" ||
              geomKey === "edge_modeII" ||
              geomKey === "edge_modeIII"
            ) {
              F = computeGeometryFactorEdge(aOverW);
            } else if (geomKey === "center_modeI") {
              if (aOverW >= 0.5) {
                validityWarning.classList.remove("hidden");
              } else {
                validityWarning.classList.add("hidden");
              }
              F = computeGeometryFactorCenter(aOverW);
            }
            if (info.maxRatio !== null) {
              if (aOverW > info.maxRatio) {
                validityWarning.classList.remove("hidden");
              } else {
                validityWarning.classList.add("hidden");
              }
            } else {
              validityWarning.classList.add("hidden");
            }
          } else {
            // through-thickness infinite plate
            validityWarning.classList.add("hidden");
            F = 1.0;
          }

          // For mode II and III we use same F as mode I for this geometry
          const base = sigma * Math.sqrt(Math.PI * a);
          const K = base * F;

          modeOutput.textContent = getModeLabel(geomKey);
          fOutput.textContent = isFinite(F) ? F.toFixed(4) : "—";

          const unitLabel = unitSystemEl.value === "si" ? "MPa·√m" : "ksi·√in";
          kOutput.textContent = isFinite(K)
            ? K.toFixed(3) + " " + unitLabel
            : "—";

          const kcVal = parseFloat(kcEl.value);
          if (isFinite(kcVal) && kcVal > 0) {
            const ratio = K / kcVal;
            let status;
            if (ratio < 0.7) {
              status = "Below fracture toughness (comfortable margin).";
            } else if (ratio < 1.0) {
              status = "Approaching fracture toughness (reduced margin).";
            } else {
              status = "Exceeds fracture toughness (unstable fracture likely).";
            }
            kcOutput.textContent =
              "K / Kc = " + ratio.toFixed(3) + " – " + status;
            kcSummary.classList.remove("hidden");
          } else {
            kcSummary.classList.add("hidden");
          }

          resultsCard.classList.remove("hidden");
        }

        function resetForm() {
          stressInputEl.value = "";
          crackLengthEl.value = "";
          widthEl.value = "";
          kcEl.value = "";
          resultsCard.classList.add("hidden");
          validityWarning.classList.add("hidden");
        }

        unitSystemEl.addEventListener("change", updateUnits);

        crackGeometryEl.addEventListener("change", function () {
          const key = crackGeometryEl.value;
          const info = geometryInfo(key);
          geometryNote.textContent =
            info.note ||
            "Geometry factor F is computed from standard polynomial approximations. Validity ranges are indicated below.";
          if (requiresWidth(key)) {
            widthGroup.classList.remove("hidden");
          } else {
            widthGroup.classList.add("hidden");
          }
          resultsCard.classList.add("hidden");
          validityWarning.classList.add("hidden");
        });

        computeBtn.addEventListener("click", computeK);
        resetBtn.addEventListener("click", resetForm);

        updateUnits();
      })();
    </script>
  
