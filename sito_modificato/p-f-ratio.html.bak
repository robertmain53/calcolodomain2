    <script>
      // P/F ratio calculator logic
      function classifyARDS(pf, peep) {
        if (!isFinite(pf) || pf <= 0) {
          return {
            label: "No result",
            description:
              "Enter valid PaO2 and FiO2 values to calculate the P/F ratio.",
            color: "bg-gray-100 text-gray-700",
          };
        }

        const hasPEEP = peep !== null && !isNaN(peep) && peep >= 5;

        if (pf > 300) {
          return {
            label: "No ARDS (P/F > 300)",
            description: hasPEEP
              ? "Oxygenation is within the normal range. ARDS criteria are not met based on P/F ratio."
              : "P/F > 300 suggests normal oxygenation. Formal ARDS criteria also require PEEP/CPAP ≥ 5 cmH₂O.",
            color: "bg-green-100 text-green-800",
          };
        } else if (pf > 200) {
          return {
            label: "Mild ARDS (P/F 201–300)",
            description: hasPEEP
              ? "Consistent with mild ARDS by Berlin definition, assuming imaging and timing criteria are also met."
              : "P/F is in the mild ARDS range, but PEEP/CPAP ≥ 5 cmH₂O is required for formal ARDS classification.",
            color: "bg-yellow-100 text-yellow-800",
          };
        } else if (pf > 100) {
          return {
            label: "Moderate ARDS (P/F 101–200)",
            description: hasPEEP
              ? "Consistent with moderate ARDS, assuming bilateral opacities and non-cardiogenic cause of respiratory failure."
              : "P/F is in the moderate ARDS range; confirm that PEEP/CPAP ≥ 5 cmH₂O and other ARDS criteria are satisfied.",
            color: "bg-orange-100 text-orange-800",
          };
        } else {
          return {
            label: "Severe ARDS (P/F ≤ 100)",
            description: hasPEEP
              ? "Consistent with severe ARDS. This is associated with high mortality and often requires advanced ventilatory support."
              : "P/F ≤ 100 is in the severe ARDS range; ensure PEEP/CPAP ≥ 5 cmH₂O and confirm full ARDS criteria.",
            color: "bg-red-100 text-red-800",
          };
        }
      }

      function updateSummary(pao2, pao2Unit, fio2, peep) {
        const summaryEl = document.getElementById("input-summary");
        const pao2Text = pao2
          ? pao2.toString() + " " + (pao2Unit === "kpa" ? "kPa" : "mmHg")
          : "—";
        const fio2Text = fio2 ? fio2.toFixed(2) + " (fraction)" : "—";
        const peepText =
          (peep || peep === 0) && !isNaN(peep) ? peep + " cmH₂O" : "—";

        summaryEl.innerHTML = `
      <li>PaO₂: ${pao2Text}</li>
      <li>FiO₂: ${fio2Text}</li>
      <li>PEEP/CPAP: ${peepText}</li>
    `;
      }

      document.addEventListener("DOMContentLoaded", function () {
        const pao2Input = document.getElementById("pao2-value");
        const pao2UnitSelect = document.getElementById("pao2-unit");
        const fio2Input = document.getElementById("fio2-value");
        const peepInput = document.getElementById("peep-value");
        const resultEl = document.getElementById("pf-result");
        const ardsBadge = document.getElementById("ards-badge");
        const ardsText = document.getElementById("ards-text");
        const calculateBtn = document.getElementById("calculate-btn");
        const resetBtn = document.getElementById("reset-btn");

        // Preset FiO2 buttons
        document.querySelectorAll(".fio2-preset-btn").forEach((btn) => {
          btn.addEventListener("click", () => {
            const val = parseFloat(btn.getAttribute("data-fio2-preset"));
            if (!isNaN(val)) {
              fio2Input.value = val.toFixed(2);
            }
          });
        });

        function calculate() {
          const pao2Raw = parseFloat(pao2Input.value);
          const pao2Unit = pao2UnitSelect.value;
          const fio2Raw = parseFloat(fio2Input.value);
          const peepRaw =
            peepInput.value === "" ? null : parseFloat(peepInput.value);

          updateSummary(
            isNaN(pao2Raw) ? null : pao2Raw,
            pao2Unit,
            isNaN(fio2Raw) ? null : fio2Raw,
            peepRaw
          );

          if (
            isNaN(pao2Raw) ||
            pao2Raw <= 0 ||
            isNaN(fio2Raw) ||
            fio2Raw <= 0 ||
            fio2Raw > 1.0
          ) {
            resultEl.textContent = "—";
            ardsBadge.textContent = "Invalid or incomplete input";
            ardsBadge.className = "badge bg-gray-100 text-gray-700 mb-2";
            ardsText.textContent =
              "Please enter a positive PaO₂ value and FiO₂ between 0.21 and 1.0.";
            return;
          }

          // Convert PaO2 to mmHg if needed
          let pao2mmHg = pao2Raw;
          if (pao2Unit === "kpa") {
            pao2mmHg = pao2Raw * 7.5;
          }

          const pfRatio = pao2mmHg / fio2Raw;

          if (!isFinite(pfRatio) || pfRatio <= 0) {
            resultEl.textContent = "—";
            ardsBadge.textContent = "No result";
            ardsBadge.className = "badge bg-gray-100 text-gray-700 mb-2";
            ardsText.textContent =
              "Unable to compute a valid P/F ratio with the provided values.";
            return;
          }

          resultEl.textContent = pfRatio.toFixed(0);

          const classification = classifyARDS(pfRatio, peepRaw);
          ardsBadge.textContent = classification.label;
          ardsBadge.className = "badge mb-2 " + classification.color;
          ardsText.textContent = classification.description;
        }

        calculateBtn.addEventListener("click", calculate);

        // Allow quick recalculation on Enter key in any main input
        [pao2Input, fio2Input, peepInput].forEach((el) => {
          el.addEventListener("keydown", (e) => {
            if (e.key === "Enter") {
              e.preventDefault();
              calculate();
            }
          });
        });

        resetBtn.addEventListener("click", () => {
          pao2Input.value = "";
          fio2Input.value = "";
          peepInput.value = "";
          resultEl.textContent = "—";
          ardsBadge.textContent = "No result yet";
          ardsBadge.className = "badge bg-gray-100 text-gray-700 mb-2";
          ardsText.textContent = "Enter PaO₂ and FiO₂ to see ARDS severity.";
          updateSummary(null, "mmhg", null, null);
        });

        // Initialize summary
        updateSummary(null, "mmhg", null, null);
      });

      // MathJax config
      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
