    <script>
      // Black–Scholes Greeks implementation
      function normPdf(x) {
        return Math.exp(-0.5 * x * x) / Math.sqrt(2 * Math.PI);
      }

      function normCdf(x) {
        // Abramowitz & Stegun approximation
        var k = 1 / (1 + 0.2316419 * Math.abs(x));
        var a1 = 0.31938153,
          a2 = -0.356563782,
          a3 = 1.781477937,
          a4 = -1.821255978,
          a5 = 1.330274429;
        var poly = ((((a5 * k + a4) * k + a3) * k + a2) * k + a1) * k;
        var approx = 1 - normPdf(x) * poly;
        return x >= 0 ? approx : 1 - approx;
      }

      function computeGreeks(params) {
        var S = params.S,
          K = params.K,
          T = params.T,
          r = params.r,
          q = params.q,
          sigma = params.sigma,
          isCall = params.isCall;

        if (T <= 0 || sigma <= 0 || S <= 0 || K <= 0) {
          throw new Error(
            "All inputs must be positive and time/volatility must be greater than zero."
          );
        }

        var sqrtT = Math.sqrt(T);
        var d1 =
          (Math.log(S / K) + (r - q + 0.5 * sigma * sigma) * T) /
          (sigma * sqrtT);
        var d2 = d1 - sigma * sqrtT;

        var Nd1 = normCdf(isCall ? d1 : -d1);
        var Nd2 = normCdf(isCall ? d2 : -d2);
        var pdfD1 = normPdf(d1);

        var discQ = Math.exp(-q * T);
        var discR = Math.exp(-r * T);

        var price;
        if (isCall) {
          price = S * discQ * normCdf(d1) - K * discR * normCdf(d2);
        } else {
          price = K * discR * normCdf(-d2) - S * discQ * normCdf(-d1);
        }

        var delta = isCall ? discQ * normCdf(d1) : discQ * (normCdf(d1) - 1);
        var gamma = (discQ * pdfD1) / (S * sigma * sqrtT);

        // Theta per year
        var theta;
        if (isCall) {
          theta =
            -(S * discQ * pdfD1 * sigma) / (2 * sqrtT) -
            r * K * discR * normCdf(d2) +
            q * S * discQ * normCdf(d1);
        } else {
          theta =
            -(S * discQ * pdfD1 * sigma) / (2 * sqrtT) +
            r * K * discR * normCdf(-d2) -
            q * S * discQ * normCdf(-d1);
        }
        var thetaPerDay = theta / 365.0;

        var vega = S * discQ * pdfD1 * sqrtT; // per 1.00 vol
        var vegaPerPct = vega / 100.0;

        var rho;
        if (isCall) {
          rho = K * T * discR * normCdf(d2);
        } else {
          rho = -K * T * discR * normCdf(-d2);
        }
        var rhoPerPct = rho / 100.0;

        return {
          price: price,
          delta: delta,
          gamma: gamma,
          thetaPerDay: thetaPerDay,
          vegaPerPct: vegaPerPct,
          rhoPerPct: rhoPerPct,
          d1: d1,
          d2: d2,
        };
      }

      function formatNumber(x, decimals) {
        if (!isFinite(x)) return "–";
        return x.toFixed(decimals);
      }

      document.addEventListener("DOMContentLoaded", function () {
        var isCall = true;

        var callBtn = document.getElementById("call-btn");
        var putBtn = document.getElementById("put-btn");
        callBtn.addEventListener("click", function () {
          isCall = true;
          callBtn.className =
            "flex-1 px-3 py-2 rounded border border-blue-600 bg-blue-50 text-blue-700 font-medium";
          putBtn.className =
            "flex-1 px-3 py-2 rounded border border-gray-300 bg-white text-gray-700";
        });
        putBtn.addEventListener("click", function () {
          isCall = false;
          putBtn.className =
            "flex-1 px-3 py-2 rounded border border-blue-600 bg-blue-50 text-blue-700 font-medium";
          callBtn.className =
            "flex-1 px-3 py-2 rounded border border-gray-300 bg-white text-gray-700";
        });

        document
          .getElementById("calc-btn")
          .addEventListener("click", function () {
            var errorEl = document.getElementById("error-msg");
            errorEl.classList.add("hidden");
            errorEl.textContent = "";

            try {
              var S = parseFloat(document.getElementById("spot").value);
              var K = parseFloat(document.getElementById("strike").value);
              var timeVal = parseFloat(document.getElementById("time").value);
              var timeUnit = document.getElementById("time-unit").value;
              var rPct = parseFloat(document.getElementById("rate").value);
              var volPct = parseFloat(document.getElementById("vol").value);
              var qPct = parseFloat(
                document.getElementById("div-yield").value || "0"
              );

              if (
                [S, K, timeVal, rPct, volPct].some(function (v) {
                  return isNaN(v);
                })
              ) {
                throw new Error("Please fill in all required numeric fields.");
              }

              var T = timeUnit === "days" ? timeVal / 365.0 : timeVal;
              var r = rPct / 100.0;
              var sigma = volPct / 100.0;
              var q = (isNaN(qPct) ? 0 : qPct) / 100.0;

              var res = computeGreeks({
                S: S,
                K: K,
                T: T,
                r: r,
                q: q,
                sigma: sigma,
                isCall: isCall,
              });

              document.getElementById("res-price").textContent = formatNumber(
                res.price,
                4
              );
              document.getElementById("res-delta").textContent = formatNumber(
                res.delta,
                4
              );
              document.getElementById("res-gamma").textContent = formatNumber(
                res.gamma,
                6
              );
              document.getElementById("res-theta").textContent = formatNumber(
                res.thetaPerDay,
                4
              );
              document.getElementById("res-vega").textContent = formatNumber(
                res.vegaPerPct,
                4
              );
              document.getElementById("res-rho").textContent = formatNumber(
                res.rhoPerPct,
                4
              );
              document.getElementById("res-d1").textContent = formatNumber(
                res.d1,
                4
              );
              document.getElementById("res-d2").textContent = formatNumber(
                res.d2,
                4
              );
            } catch (e) {
              errorEl.textContent = e.message;
              errorEl.classList.remove("hidden");
            }
          });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
