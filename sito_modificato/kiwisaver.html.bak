    <script>
      // KiwiSaver calculator logic
      function formatNZD(value) {
        return (
          "NZ$" + value.toLocaleString("en-NZ", { maximumFractionDigits: 0 })
        );
      }

      function calculateKiwiSaverProjection() {
        const errorEl = document.getElementById("error-message");
        errorEl.textContent = "";

        const age = parseFloat(document.getElementById("age").value);
        const retirementAge = parseFloat(
          document.getElementById("retirement-age").value
        );
        const salaryStart = parseFloat(document.getElementById("salary").value);
        const salaryGrowth =
          parseFloat(document.getElementById("salary-growth").value) / 100;
        const employeeRate =
          parseFloat(document.getElementById("employee-rate").value) / 100;
        const employerRate =
          parseFloat(document.getElementById("employer-rate").value) / 100;
        const includeGovt = document.getElementById("include-govt").checked;
        const initialBalance =
          parseFloat(document.getElementById("initial-balance").value) || 0;

        const returnRate =
          parseFloat(document.getElementById("return-rate").value) / 100;
        const feeRate =
          parseFloat(document.getElementById("fee-rate").value) / 100;
        const pir = parseFloat(document.getElementById("pir").value);
        const inflation =
          parseFloat(document.getElementById("inflation").value) / 100;

        if (isNaN(age) || isNaN(retirementAge) || isNaN(salaryStart)) {
          errorEl.textContent = "Please enter your age and salary.";
          return;
        }
        if (retirementAge <= age) {
          errorEl.textContent =
            "Retirement age must be greater than current age.";
          return;
        }
        if (salaryStart < 0) {
          errorEl.textContent = "Salary cannot be negative.";
          return;
        }

        const years = Math.floor(retirementAge - age);
        let balance = initialBalance;
        let salary = salaryStart;

        let totalEmployee = 0;
        let totalEmployer = 0;
        let totalGovt = 0;
        let totalFees = 0;
        let totalTax = 0;
        let totalReturns = 0;

        for (let i = 0; i < years; i++) {
          // Contributions for the year
          const employeeContrib = salary * employeeRate;
          const employerContrib = salary * employerRate;
          let govtContrib = 0;
          if (includeGovt && employeeContrib > 0) {
            // Government contribution: 50c per $1 up to $521.43 if at least $1,042.86 contributed
            const eligibleGovt = Math.min(521.43, 0.5 * employeeContrib);
            govtContrib = eligibleGovt;
          }

          totalEmployee += employeeContrib;
          totalEmployer += employerContrib;
          totalGovt += govtContrib;

          balance += employeeContrib + employerContrib + govtContrib;

          // Investment growth, fees, tax
          const grossReturn = balance * returnRate;
          const fees = balance * feeRate;
          const taxableReturn = Math.max(grossReturn, 0);
          const tax = taxableReturn * pir;
          const netGrowth = grossReturn - fees - tax;

          totalReturns += grossReturn;
          totalFees += fees;
          totalTax += tax;

          balance += netGrowth;

          // Increase salary for next year
          salary *= 1 + salaryGrowth;
        }

        const nominalBalance = balance;
        let realBalance = null;
        if (!isNaN(inflation) && inflation > 0) {
          realBalance = nominalBalance / Math.pow(1 + inflation, years);
        }

        const resultsContainer = document.getElementById("results-container");
        resultsContainer.innerHTML = "";

        const summaryCard = document.createElement("div");
        summaryCard.className =
          "bg-blue-50 border border-blue-100 rounded-md p-4 mb-3";
        summaryCard.innerHTML = `
      <h3 class="text-lg font-semibold text-blue-800 mb-2">Projected KiwiSaver balance at age ${retirementAge}</h3>
      <p class="text-2xl font-bold text-blue-900 mb-1">${formatNZD(
        nominalBalance
      )}</p>
      ${
        realBalance
          ? `<p class="text-sm text-blue-800 mb-1">≈ ${formatNZD(
              realBalance
            )} in today's dollars (assuming ${(inflation * 100).toFixed(
              1
            )}% inflation)</p>`
          : ""
      }
      <p class="text-xs text-blue-700">This is an estimate only. Actual returns, fees, tax and contributions will vary.</p>
    `;
        resultsContainer.appendChild(summaryCard);

        const breakdownCard = document.createElement("div");
        breakdownCard.className =
          "bg-gray-50 border border-gray-200 rounded-md p-4";
        breakdownCard.innerHTML = `
      <h3 class="text-md font-semibold mb-2">Breakdown over ${years} year${
          years !== 1 ? "s" : ""
        }</h3>
      <div class="results-grid text-sm">
        <div>
          <div class="text-gray-500">Employee contributions</div>
          <div class="font-semibold">${formatNZD(totalEmployee)}</div>
        </div>
        <div>
          <div class="text-gray-500">Employer contributions</div>
          <div class="font-semibold">${formatNZD(totalEmployer)}</div>
        </div>
        <div>
          <div class="text-gray-500">Government contributions</div>
          <div class="font-semibold">${formatNZD(totalGovt)}</div>
        </div>
        <div>
          <div class="text-gray-500">Total contributions</div>
          <div class="font-semibold">${formatNZD(
            totalEmployee + totalEmployer + totalGovt + initialBalance
          )}</div>
        </div>
        <div>
          <div class="text-gray-500">Gross investment returns</div>
          <div class="font-semibold">${formatNZD(totalReturns)}</div>
        </div>
        <div>
          <div class="text-gray-500">Fees paid</div>
          <div class="font-semibold text-red-700">− ${formatNZD(
            totalFees
          )}</div>
        </div>
        <div>
          <div class="text-gray-500">Tax on returns (PIR)</div>
          <div class="font-semibold text-red-700">− ${formatNZD(totalTax)}</div>
        </div>
        <div>
          <div class="text-gray-500">Net investment gain</div>
          <div class="font-semibold">${formatNZD(
            totalReturns - totalFees - totalTax
          )}</div>
        </div>
      </div>
    `;
        resultsContainer.appendChild(breakdownCard);

        const note = document.createElement("p");
        note.className = "mt-2 text-xs text-gray-500";
        note.textContent =
          "Assumptions: annual compounding, constant contribution rates, simplified employer tax (ESCT) and government contribution rules. This is not financial advice.";
        resultsContainer.appendChild(note);
      }

      document.addEventListener("DOMContentLoaded", function () {
        const calcBtn = document.getElementById("calculate-btn");
        const resetBtn = document.getElementById("reset-btn");

        if (calcBtn) {
          calcBtn.addEventListener("click", calculateKiwiSaverProjection);
        }

        if (resetBtn) {
          resetBtn.addEventListener("click", function () {
            document.getElementById("age").value = 30;
            document.getElementById("retirement-age").value = 65;
            document.getElementById("salary").value = 70000;
            document.getElementById("salary-growth").value = 2.0;
            document.getElementById("employee-rate").value = 6;
            document.getElementById("employer-rate").value = 3.0;
            document.getElementById("include-govt").checked = true;
            document.getElementById("initial-balance").value = 20000;
            document.getElementById("return-rate").value = 5.0;
            document.getElementById("fee-rate").value = 1.0;
            document.getElementById("pir").value = 0.28;
            document.getElementById("inflation").value = 2.0;
            document.getElementById("results-container").innerHTML =
              '<p class="text-sm text-gray-600">Enter your details above and click <span class="font-semibold">Calculate KiwiSaver projection</span> to see your estimated balance.</p>';
            document.getElementById("error-message").textContent = "";
          });
        }
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
