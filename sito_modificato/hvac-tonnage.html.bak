    <script>
      // Tab switching
      document.querySelectorAll(".hvac-tab").forEach((btn) => {
        btn.addEventListener("click", () => {
          const mode = btn.getAttribute("data-mode");
          document.querySelectorAll(".hvac-tab").forEach((b) => {
            b.classList.remove("border-blue-600", "text-blue-600");
            b.classList.add("border-transparent", "text-gray-600");
          });
          btn.classList.add("border-blue-600", "text-blue-600");
          btn.classList.remove("border-transparent", "text-gray-600");

          document
            .querySelectorAll(".hvac-mode")
            .forEach((sec) => sec.classList.add("hidden"));
          const active = document.getElementById("mode-" + mode);
          if (active) active.classList.remove("hidden");
        });
      });

      function parseNumber(id) {
        const el = document.getElementById(id);
        if (!el) return NaN;
        const v = parseFloat(el.value);
        return isNaN(v) ? NaN : v;
      }

      function formatNumber(n, decimals) {
        return n.toLocaleString(undefined, {
          maximumFractionDigits: decimals,
          minimumFractionDigits: decimals,
        });
      }

      // Area-based sizing
      document.getElementById("btn-calc-area").addEventListener("click", () => {
        const areaVal = parseNumber("area");
        const areaUnit = document.getElementById("area-unit").value;
        const climate = document.getElementById("climate").value;
        const insulation = document.getElementById("insulation").value;
        const sun = document.getElementById("sun").value;
        const occupants = parseNumber("occupants");
        const ceiling = parseNumber("ceiling-height");

        if (!areaVal || areaVal <= 0) {
          alert("Please enter a valid conditioned floor area.");
          return;
        }

        // Convert area to sq ft if needed
        let areaSqft = areaVal;
        if (areaUnit === "sqm") {
          areaSqft = areaVal * 10.7639;
        }

        // Base BTU/ft² by climate
        let basePerSqft;
        switch (climate) {
          case "cool":
            basePerSqft = 15;
            break;
          case "moderate":
            basePerSqft = 18;
            break;
          case "warm":
            basePerSqft = 22;
            break;
          case "hot":
            basePerSqft = 25;
            break;
          default:
            basePerSqft = 18;
        }

        // Insulation factor
        let insFactor = 1.0;
        if (insulation === "poor") insFactor = 1.15;
        else if (insulation === "good") insFactor = 0.85;

        // Solar factor
        let sunFactor = 1.0;
        if (sun === "low") sunFactor = 0.95;
        else if (sun === "high") sunFactor = 1.1;

        // Ceiling height factor (relative to 8 ft)
        let heightFactor = 1.0;
        if (!isNaN(ceiling) && ceiling > 0) {
          heightFactor = ceiling / 8.0;
        }

        const baseBtu = areaSqft * basePerSqft;
        const adjustedBtu = baseBtu * insFactor * sunFactor * heightFactor;

        // Occupant load
        let occ = isNaN(occupants) ? 0 : Math.max(occupants, 0);
        const extraPeople = Math.max(occ - 2, 0);
        const peopleBtu = extraPeople * 600;

        const totalBtu = adjustedBtu + peopleBtu;
        const tons = totalBtu / 12000;

        // Recommended range: ±0.25 ton, snapped to 0.5 increments
        function snapHalf(x) {
          return Math.round(x * 2) / 2;
        }
        const minTons = Math.max(0.5, snapHalf(Math.max(tons - 0.25, 0.5)));
        const maxTons = snapHalf(tons + 0.25);

        const sqftPerTon = areaSqft / tons;

        document.getElementById("res-tons-min").textContent = formatNumber(
          minTons,
          1
        );
        document.getElementById("res-tons-max").textContent = formatNumber(
          maxTons,
          1
        );
        document.getElementById("res-btu").textContent =
          Math.round(totalBtu).toLocaleString();
        document.getElementById("res-sqft-per-ton").textContent = isFinite(
          sqftPerTon
        )
          ? Math.round(sqftPerTon).toLocaleString()
          : "—";

        document.getElementById("area-results").classList.remove("hidden");
      });

      // BTU <-> tons conversion
      document
        .getElementById("btn-btu-to-tons")
        .addEventListener("click", () => {
          const btu = parseNumber("btu-input");
          if (!btu || btu <= 0) {
            alert("Please enter a valid BTU/hr value.");
            return;
          }
          const tons = btu / 12000;
          const summary = `${btu.toLocaleString()} BTU/hr ≈ ${formatNumber(
            tons,
            2
          )} tons of cooling.`;
          document.getElementById("btu-summary").textContent = summary;
          document.getElementById("btu-results").classList.remove("hidden");
        });

      document
        .getElementById("btn-tons-to-btu")
        .addEventListener("click", () => {
          const tons = parseNumber("tons-input");
          if (!tons || tons <= 0) {
            alert("Please enter a valid tonnage value.");
            return;
          }
          const btu = tons * 12000;
          const summary = `${formatNumber(tons, 2)} tons ≈ ${Math.round(
            btu
          ).toLocaleString()} BTU/hr of cooling capacity.`;
          document.getElementById("btu-summary").textContent = summary;
          document.getElementById("btu-results").classList.remove("hidden");
        });

      // Existing unit analysis
      document
        .getElementById("btn-check-unit")
        .addEventListener("click", () => {
          let btu = parseNumber("existing-btu");
          const model = document.getElementById("existing-model").value.trim();
          const area = parseNumber("existing-area");

          if ((!btu || btu <= 0) && !model) {
            alert(
              "Enter either a BTU/hr rating or a model number to estimate tonnage."
            );
            return;
          }

          // Try to infer BTU from model number if BTU not provided
          if ((!btu || btu <= 0) && model) {
            const match = model.match(/(\d{2})/g);
            if (match) {
              // Look for a typical BTU code (18, 24, 30, 36, 42, 48, 60)
              const codes = match.map((x) => parseInt(x, 10));
              const typical = [18, 24, 30, 36, 42, 48, 60];
              let found = null;
              for (const c of codes) {
                if (typical.includes(c)) {
                  found = c;
                  break;
                }
              }
              if (found !== null) {
                btu = found * 1000;
              }
            }
          }

          if (!btu || btu <= 0) {
            alert(
              "Could not infer BTU from the model number. Please enter the BTU/hr if available."
            );
            return;
          }

          const tons = btu / 12000;
          const tonLine = `Estimated unit size: about ${formatNumber(
            tons,
            2
          )} tons (${Math.round(btu).toLocaleString()} BTU/hr).`;
          document.getElementById("existing-tonnage-line").textContent =
            tonLine;

          let assessment = "";
          let sqftPerTonLine = "";

          if (area && area > 0) {
            const sqftPerTon = area / tons;
            sqftPerTonLine = `This unit serves about ${Math.round(
              area
            ).toLocaleString()} sq ft, or roughly ${Math.round(
              sqftPerTon
            ).toLocaleString()} sq ft per ton.`;

            if (sqftPerTon < 400) {
              assessment =
                "This suggests the system may be oversized for a typical home, especially in moderate or cool climates.";
            } else if (sqftPerTon > 800) {
              assessment =
                "This suggests the system may be undersized for a typical home, particularly in warm or hot climates.";
            } else {
              assessment =
                "This falls within a common residential range (roughly 400–800 sq ft per ton), but a Manual J load calculation is needed for a definitive answer.";
            }
          } else {
            sqftPerTonLine =
              "Enter the approximate square footage served by this unit to compare size vs. area.";
            assessment =
              "Use the area-based sizing tab to see how this unit compares to a rule-of-thumb recommendation.";
          }

          document.getElementById("existing-sqft-per-ton-line").textContent =
            sqftPerTonLine;
          document.getElementById("existing-assessment").textContent =
            assessment;
          document
            .getElementById("existing-results")
            .classList.remove("hidden");
        });

      // Simple FAQ toggle (in addition to global script_faq if present)
      document
        .querySelectorAll("#faq-container .faq-question")
        .forEach((btn) => {
          btn.addEventListener("click", () => {
            const answer = btn.parentElement.querySelector(".faq-answer");
            const icon = btn.querySelector("svg");
            const isHidden = answer.classList.contains("hidden");
            document
              .querySelectorAll("#faq-container .faq-answer")
              .forEach((a) => a.classList.add("hidden"));
            document
              .querySelectorAll("#faq-container .faq-question svg")
              .forEach((i) => i.classList.remove("rotate-180"));
            if (isHidden) {
              answer.classList.remove("hidden");
              icon.classList.add("rotate-180");
            }
          });
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
