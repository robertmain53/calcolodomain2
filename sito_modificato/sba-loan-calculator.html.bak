    <script>
      // Utility formatting
      function fmtMoney(v) {
        if (!isFinite(v)) return "$0";
        return (
          "$" +
          v.toLocaleString(undefined, {
            minimumFractionDigits: 2,
            maximumFractionDigits: 2,
          })
        );
      }
      function fmtPercent(v, digits = 2) {
        if (!isFinite(v)) return "0%";
        return v.toFixed(digits) + "%";
      }

      // Amortization generator with optional extra payment
      function buildSchedule(principal, annualRate, years, extra, startDate) {
        const r = annualRate / 100 / 12;
        const n = Math.round(years * 12);
        if (principal <= 0 || r < 0 || n <= 0)
          return { payment: 0, schedule: [], totalInterest: 0, months: 0 };

        const basePayment =
          r === 0
            ? principal / n
            : (principal * (r * Math.pow(1 + r, n))) / (Math.pow(1 + r, n) - 1);

        let balance = principal;
        let month = 0;
        let totalInterest = 0;
        const schedule = [];
        const date = startDate ? new Date(startDate) : null;

        while (balance > 0 && month < n + 600) {
          // safety cap
          const interest = balance * r;
          let principalPay = basePayment - interest;
          let extraPay = extra || 0;

          if (principalPay < 0) principalPay = 0;

          if (principalPay + extraPay > balance + interest) {
            const totalNeeded = balance + interest;
            if (totalNeeded <= interest) {
              principalPay = 0;
              extraPay = 0;
            } else {
              principalPay = totalNeeded - interest;
              extraPay = 0;
            }
          }

          const payment = principalPay + interest + extraPay;
          balance = balance - principalPay - extraPay;
          if (balance < 1e-6) balance = 0;

          totalInterest += interest;

          let payDate = "";
          if (date) {
            const d = new Date(date);
            d.setMonth(d.getMonth() + month);
            payDate = d.toLocaleDateString(undefined, {
              year: "numeric",
              month: "short",
            });
          }

          schedule.push({
            num: month + 1,
            date: payDate,
            payment,
            principal: principalPay + extraPay,
            interest,
            extra: extraPay,
            balance,
          });

          if (balance <= 0) break;
          month++;
        }

        return {
          payment: basePayment,
          schedule,
          totalInterest,
          months: schedule.length,
        };
      }

      // SBA 7(a) guarantee fee (simplified tiered)
      function calcSba7aFee(guaranteedAmount) {
        if (guaranteedAmount <= 0) return { fee: 0, effectiveRate: 0 };
        let remaining = guaranteedAmount;
        let fee = 0;

        const tier1 = Math.min(remaining, 1000000);
        fee += tier1 * 0.0;
        remaining -= tier1;

        if (remaining > 0) {
          const tier2 = Math.min(remaining, 1000000);
          fee += tier2 * 0.035;
          remaining -= tier2;
        }

        if (remaining > 0) {
          fee += remaining * 0.0375;
        }

        return { fee, effectiveRate: (fee / guaranteedAmount) * 100 };
      }

      // SBA 7(a) logic
      function initSba7a() {
        const tab7a = document.getElementById("tab-7a");
        const tab504 = document.getElementById("tab-504");
        const panel7a = document.getElementById("panel-7a");
        const panel504 = document.getElementById("panel-504");

        tab7a.addEventListener("click", () => {
          tab7a.classList.add("tab-active");
          tab504.classList.remove("tab-active");
          panel7a.classList.remove("hidden");
          panel504.classList.add("hidden");
        });
        tab504.addEventListener("click", () => {
          tab504.classList.add("tab-active");
          tab7a.classList.remove("tab-active");
          panel504.classList.remove("hidden");
          panel7a.classList.add("hidden");
        });

        const feeModeEl = document.getElementById("sba7a-fee-mode");
        const feeCustomWrapper = document.getElementById(
          "sba7a-fee-custom-wrapper"
        );
        feeModeEl.addEventListener("change", () => {
          feeCustomWrapper.classList.toggle(
            "hidden",
            feeModeEl.value !== "custom"
          );
        });

        document.getElementById("sba7a-calc").addEventListener("click", () => {
          const amount =
            parseFloat(document.getElementById("sba7a-amount").value) || 0;
          const rate =
            parseFloat(document.getElementById("sba7a-rate").value) || 0;
          const term =
            parseFloat(document.getElementById("sba7a-term").value) || 0;
          const guaranteePct = Math.min(
            Math.max(
              parseFloat(document.getElementById("sba7a-guarantee").value) || 0,
              0
            ),
            100
          );
          const feeMode = feeModeEl.value;
          const feeCustomPct =
            parseFloat(document.getElementById("sba7a-fee-custom").value) || 0;
          const extra =
            parseFloat(document.getElementById("sba7a-extra").value) || 0;
          const startDate =
            document.getElementById("sba7a-start").value || null;

          if (amount <= 0 || rate < 0 || term <= 0) {
            alert(
              "Please enter a positive loan amount, interest rate, and term."
            );
            return;
          }

          const guaranteedAmount = amount * (guaranteePct / 100);
          let fee = 0;
          let feeRate = 0;

          if (feeMode === "auto") {
            const res = calcSba7aFee(guaranteedAmount);
            fee = res.fee;
            feeRate = res.effectiveRate;
          } else if (feeMode === "custom") {
            fee = guaranteedAmount * (feeCustomPct / 100);
            feeRate = feeCustomPct;
          } // 'none' => fee stays 0

          const schedBase = buildSchedule(amount, rate, term, 0, startDate);
          const schedExtra =
            extra > 0
              ? buildSchedule(amount, rate, term, extra, startDate)
              : schedBase;

          const monthlyEl = document.getElementById("sba7a-monthly");
          const monthlyNoteEl = document.getElementById("sba7a-monthly-note");
          const interestEl = document.getElementById("sba7a-interest");
          const termEffEl = document.getElementById("sba7a-term-effective");
          const feeAmountEl = document.getElementById("sba7a-fee-amount");
          const feeRateEl = document.getElementById("sba7a-fee-rate");
          const summaryEl = document.getElementById("sba7a-summary");
          const resultsBox = document.getElementById("sba7a-results");
          const summaryBox = document.getElementById("sba7a-summary");
          const schedWrapper = document.getElementById(
            "sba7a-schedule-wrapper"
          );
          const schedBody = document.getElementById("sba7a-schedule-body");

          monthlyEl.textContent = fmtMoney(schedBase.payment);
          if (extra > 0) {
            monthlyNoteEl.textContent =
              "Plus " + fmtMoney(extra) + " extra each month.";
          } else {
            monthlyNoteEl.textContent = "";
          }

          const totalInterestBase = schedBase.totalInterest;
          const totalInterestExtra = schedExtra.totalInterest;
          const monthsBase = schedBase.months;
          const monthsExtra = schedExtra.months;

          interestEl.textContent = fmtMoney(totalInterestExtra);
          const yearsEff = monthsExtra / 12;
          termEffEl.textContent =
            extra > 0
              ? `Paid off in about ${yearsEff.toFixed(
                  1
                )} years (${monthsExtra} months).`
              : `Paid off in ${term.toFixed(1)} years (${monthsBase} months).`;

          feeAmountEl.textContent = fmtMoney(fee);
          if (fee > 0) {
            feeRateEl.textContent = `Effective fee: ${fmtPercent(
              feeRate
            )} of guaranteed amount (${fmtMoney(guaranteedAmount)}).`;
          } else if (feeMode === "none") {
            feeRateEl.textContent =
              "Guarantee fee not included (for comparison only).";
          } else {
            feeRateEl.textContent = "No guarantee fee based on current tiers.";
          }

          let summaryText = `On a $${amount.toLocaleString()} SBA 7(a) loan at ${rate.toFixed(
            2
          )}% for ${term.toFixed(
            1
          )} years, your estimated monthly payment is ${fmtMoney(
            schedBase.payment
          )}.`;
          if (extra > 0) {
            const interestSaved = totalInterestBase - totalInterestExtra;
            const monthsSaved = monthsBase - monthsExtra;
            summaryText += ` With an extra ${fmtMoney(
              extra
            )} each month, you could pay the loan off about ${monthsSaved} months sooner and save roughly ${fmtMoney(
              interestSaved
            )} in interest.`;
          }
          if (fee > 0) {
            summaryText += ` The estimated SBA guarantee fee is ${fmtMoney(
              fee
            )}, which is typically financed or paid at closing.`;
          }

          summaryEl.textContent = summaryText;

          // Build schedule table (using extra schedule if applicable)
          schedBody.innerHTML = "";
          const schedule = schedExtra.schedule;
          schedule.forEach((row) => {
            const tr = document.createElement("tr");
            tr.innerHTML = `
          <td class="px-2 py-1">${row.num}</td>
          <td class="px-2 py-1">${row.date || "-"}</td>
          <td class="px-2 py-1 text-right">${fmtMoney(row.payment)}</td>
          <td class="px-2 py-1 text-right">${fmtMoney(row.principal)}</td>
          <td class="px-2 py-1 text-right">${fmtMoney(row.interest)}</td>
          <td class="px-2 py-1 text-right">${
            row.extra ? fmtMoney(row.extra) : "$0.00"
          }</td>
          <td class="px-2 py-1 text-right">${fmtMoney(row.balance)}</td>
        `;
            schedBody.appendChild(tr);
          });

          resultsBox.classList.remove("hidden");
          summaryBox.classList.remove("hidden");
          schedWrapper.classList.remove("hidden");
        });

        document.getElementById("sba7a-reset").addEventListener("click", () => {
          document.getElementById("sba7a-amount").value = 500000;
          document.getElementById("sba7a-rate").value = 8.25;
          document.getElementById("sba7a-term").value = 10;
          document.getElementById("sba7a-guarantee").value = 75;
          document.getElementById("sba7a-fee-mode").value = "auto";
          document.getElementById("sba7a-fee-custom").value = 3.5;
          document.getElementById("sba7a-extra").value = 0;
          document.getElementById("sba7a-start").value = "";
          document
            .getElementById("sba7a-fee-custom-wrapper")
            .classList.add("hidden");
          document.getElementById("sba7a-results").classList.add("hidden");
          document.getElementById("sba7a-summary").classList.add("hidden");
          document
            .getElementById("sba7a-schedule-wrapper")
            .classList.add("hidden");
        });
      }

      // SBA 504 logic
      function initSba504() {
        document.getElementById("sba504-calc").addEventListener("click", () => {
          const project =
            parseFloat(document.getElementById("sba504-project").value) || 0;
          let equityPct =
            parseFloat(document.getElementById("sba504-equity").value) || 0;
          let firstPct =
            parseFloat(document.getElementById("sba504-first-share").value) ||
            0;
          const firstRate =
            parseFloat(document.getElementById("sba504-first-rate").value) || 0;
          const firstTerm =
            parseFloat(document.getElementById("sba504-first-term").value) || 0;
          const sbaRate =
            parseFloat(document.getElementById("sba504-sba-rate").value) || 0;
          const sbaTerm =
            parseFloat(document.getElementById("sba504-sba-term").value) || 0;
          const extra =
            parseFloat(document.getElementById("sba504-extra").value) || 0;

          if (
            project <= 0 ||
            firstRate < 0 ||
            firstTerm <= 0 ||
            sbaRate < 0 ||
            sbaTerm <= 0
          ) {
            alert(
              "Please enter a positive project cost, interest rates, and terms."
            );
            return;
          }

          // Normalize shares if they don't sum to <= 100
          const totalPct = equityPct + firstPct;
          if (totalPct > 100) {
            const scale = 100 / totalPct;
            equityPct *= scale;
            firstPct *= scale;
          }
          const sbaPct = Math.max(0, 100 - equityPct - firstPct);

          const equityAmt = project * (equityPct / 100);
          const firstAmt = project * (firstPct / 100);
          const sbaAmt = project * (sbaPct / 100);

          const extraFirst = extra * (firstAmt / (firstAmt + sbaAmt || 1));
          const extraSba = extra * (sbaAmt / (firstAmt + sbaAmt || 1));

          const schedFirst = buildSchedule(
            firstAmt,
            firstRate,
            firstTerm,
            extraFirst,
            null
          );
          const schedSba = buildSchedule(
            sbaAmt,
            sbaRate,
            sbaTerm,
            extraSba,
            null
          );

          const monthlyCombined = schedFirst.payment + schedSba.payment;
          const totalInterest =
            schedFirst.totalInterest + schedSba.totalInterest;

          const resBox = document.getElementById("sba504-results");
          const monthlyEl = document.getElementById("sba504-monthly");
          const breakdownEl = document.getElementById("sba504-breakdown");
          const interestEl = document.getElementById("sba504-interest");
          const termsEl = document.getElementById("sba504-terms");
          const structEl = document.getElementById("sba504-structure");
          const schedWrapper = document.getElementById(
            "sba504-schedule-wrapper"
          );
          const firstBody = document.getElementById("sba504-first-body");
          const sbaBody = document.getElementById("sba504-sba-body");

          monthlyEl.textContent = fmtMoney(monthlyCombined);
          breakdownEl.textContent = `First mortgage: ${fmtMoney(
            schedFirst.payment
          )} + SBA debenture: ${fmtMoney(schedSba.payment)}${
            extra > 0
              ? " (plus " + fmtMoney(extra) + " extra split between both loans)"
              : ""
          }.`;
          interestEl.textContent = fmtMoney(totalInterest);
          termsEl.textContent = `First mortgage term: ${firstTerm.toFixed(
            1
          )} years; SBA debenture term: ${sbaTerm.toFixed(1)} years.`;

          structEl.textContent = `Equity: ${fmtMoney(equityAmt)} (${fmtPercent(
            equityPct,
            1
          )}), First mortgage: ${fmtMoney(firstAmt)} (${fmtPercent(
            firstPct,
            1
          )}), SBA debenture: ${fmtMoney(sbaAmt)} (${fmtPercent(sbaPct, 1)}).`;

          // Build schedules
          firstBody.innerHTML = "";
          schedFirst.schedule.forEach((row) => {
            const tr = document.createElement("tr");
            tr.innerHTML = `
          <td class="px-2 py-1">${row.num}</td>
          <td class="px-2 py-1 text-right">${fmtMoney(row.payment)}</td>
          <td class="px-2 py-1 text-right">${fmtMoney(row.principal)}</td>
          <td class="px-2 py-1 text-right">${fmtMoney(row.interest)}</td>
          <td class="px-2 py-1 text-right">${fmtMoney(row.balance)}</td>
        `;
            firstBody.appendChild(tr);
          });

          sbaBody.innerHTML = "";
          schedSba.schedule.forEach((row) => {
            const tr = document.createElement("tr");
            tr.innerHTML = `
          <td class="px-2 py-1">${row.num}</td>
          <td class="px-2 py-1 text-right">${fmtMoney(row.payment)}</td>
          <td class="px-2 py-1 text-right">${fmtMoney(row.principal)}</td>
          <td class="px-2 py-1 text-right">${fmtMoney(row.interest)}</td>
          <td class="px-2 py-1 text-right">${fmtMoney(row.balance)}</td>
        `;
            sbaBody.appendChild(tr);
          });

          resBox.classList.remove("hidden");
          schedWrapper.classList.remove("hidden");
        });

        document
          .getElementById("sba504-reset")
          .addEventListener("click", () => {
            document.getElementById("sba504-project").value = 1000000;
            document.getElementById("sba504-equity").value = 10;
            document.getElementById("sba504-first-share").value = 50;
            document.getElementById("sba504-first-rate").value = 7.5;
            document.getElementById("sba504-first-term").value = 25;
            document.getElementById("sba504-sba-rate").value = 6.0;
            document.getElementById("sba504-sba-term").value = 25;
            document.getElementById("sba504-extra").value = 0;
            document.getElementById("sba504-results").classList.add("hidden");
            document
              .getElementById("sba504-schedule-wrapper")
              .classList.add("hidden");
          });
      }

      document.addEventListener("DOMContentLoaded", () => {
        initSba7a();
        initSba504();
      });

