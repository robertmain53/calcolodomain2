    <script>
      (function () {
        const g0 = 9.80665; // m/s^2

        const modeButtons = {
          thrust: document.getElementById("mode-thrust"),
          propellant: document.getElementById("mode-propellant"),
        };

        function setMode(mode) {
          if (mode === "thrust") {
            modeButtons.thrust.classList.add("bg-blue-600", "text-white");
            modeButtons.thrust.classList.remove(
              "bg-white",
              "text-blue-600",
              "border",
              "border-blue-300"
            );
            modeButtons.propellant.classList.remove(
              "bg-blue-600",
              "text-white"
            );
            modeButtons.propellant.classList.add(
              "bg-white",
              "text-blue-600",
              "border",
              "border-blue-300"
            );
          } else {
            modeButtons.propellant.classList.add("bg-blue-600", "text-white");
            modeButtons.propellant.classList.remove(
              "bg-white",
              "text-blue-600",
              "border",
              "border-blue-300"
            );
            modeButtons.thrust.classList.remove("bg-blue-600", "text-white");
            modeButtons.thrust.classList.add(
              "bg-white",
              "text-blue-600",
              "border",
              "border-blue-300"
            );
          }
        }

        modeButtons.thrust.addEventListener("click", () => setMode("thrust"));
        modeButtons.propellant.addEventListener("click", () =>
          setMode("propellant")
        );

        const $ = (id) => document.getElementById(id);

        function toSIThrust(value, unit) {
          if (isNaN(value)) return NaN;
          if (unit === "N") return value;
          if (unit === "kN") return value * 1000;
          if (unit === "lbf") return value * 4.4482216153;
          return NaN;
        }

        function fromSIThrust(N) {
          if (!isFinite(N)) return "–";
          const kN = N / 1000;
          const lbf = N / 4.4482216153;
          return `${N.toFixed(1)} N  |  ${kN.toFixed(3)} kN  |  ${lbf.toFixed(
            1
          )} lbf`;
        }

        function toSIMass(value, unit) {
          if (isNaN(value)) return NaN;
          if (unit === "kg") return value;
          if (unit === "lb") return value * 0.45359237;
          return NaN;
        }

        function fromSIMass(kg) {
          if (!isFinite(kg)) return "–";
          const lb = kg / 0.45359237;
          return `${kg.toFixed(2)} kg  |  ${lb.toFixed(2)} lb`;
        }

        function toSIMassFlow(value, unit) {
          if (isNaN(value)) return NaN;
          if (unit === "kg_s") return value;
          if (unit === "lb_s") return value * 0.45359237;
          return NaN;
        }

        function fromSIMassFlow(kg_s) {
          if (!isFinite(kg_s)) return "–";
          const lb_s = kg_s / 0.45359237;
          return `${kg_s.toFixed(3)} kg/s  |  ${lb_s.toFixed(3)} lb/s`;
        }

        function toSITime(value, unit) {
          if (isNaN(value)) return NaN;
          if (unit === "s") return value;
          if (unit === "min") return value * 60;
          return NaN;
        }

        function fromSITime(s) {
          if (!isFinite(s)) return "–";
          const min = s / 60;
          if (s < 120) {
            return `${s.toFixed(2)} s`;
          }
          return `${s.toFixed(1)} s  |  ${min.toFixed(2)} min`;
        }

        function toSIPressure(value, unit) {
          if (isNaN(value)) return NaN;
          if (unit === "Pa") return value;
          if (unit === "kPa") return value * 1000;
          if (unit === "atm") return value * 101325;
          return NaN;
        }

        function fromSIPressure(Pa) {
          if (!isFinite(Pa)) return "–";
          const kPa = Pa / 1000;
          const atm = Pa / 101325;
          return `${kPa.toFixed(2)} kPa  |  ${atm.toFixed(3)} atm`;
        }

        function formatIspSeconds(isp_s) {
          if (!isFinite(isp_s)) return "–";
          const ve = isp_s * g0;
          return `${isp_s.toFixed(1)} s  |  vₑ = ${ve.toFixed(1)} m/s`;
        }

        function formatVe(ve) {
          if (!isFinite(ve)) return "–";
          const isp = ve / g0;
          return `${ve.toFixed(1)} m/s  |  Isp = ${isp.toFixed(1)} s`;
        }

        function formatImpulse(Ns) {
          if (!isFinite(Ns)) return "–";
          if (Ns < 1e3) return `${Ns.toFixed(1)} N·s`;
          const kNs = Ns / 1e3;
          const MNs = Ns / 1e6;
          if (kNs < 1e3) return `${kNs.toFixed(3)} kN·s`;
          return `${MNs.toFixed(3)} MN·s`;
        }

        function formatDeltaV(ms) {
          if (!isFinite(ms)) return "–";
          if (ms < 1000) return `${ms.toFixed(1)} m/s`;
          const km = ms / 1000;
          return `${ms.toFixed(1)} m/s  |  ${km.toFixed(3)} km/s`;
        }

        function formatRatio(r) {
          if (!isFinite(r)) return "–";
          return r.toFixed(3);
        }

        function formatTWR(twr) {
          if (!isFinite(twr)) return "–";
          return twr.toFixed(2) + " g₀";
        }

        function calculate() {
          const errorEl = $("errorMsg");
          errorEl.textContent = "";

          const F_in = parseFloat($("thrust").value);
          const F_unit = $("thrustUnit").value;
          const isp_in = parseFloat($("isp").value);
          const ispUnit = $("ispUnit").value;
          const mdot_in = parseFloat($("mdot").value);
          const mdotUnit = $("mdotUnit").value;
          const tburn_in = parseFloat($("burnTime").value);
          const tburnUnit = $("burnTimeUnit").value;
          const mprop_in = parseFloat($("propMass").value);
          const mpropUnit = $("propMassUnit").value;
          const m0_in = parseFloat($("m0").value);
          const m0Unit = $("m0Unit").value;
          const mf_in = parseFloat($("mf").value);
          const mfUnit = $("mfUnit").value;
          const pa_in = parseFloat($("ambientPressure").value);
          const paUnit = $("ambientPressureUnit").value;

          let F = isNaN(F_in) ? NaN : toSIThrust(F_in, F_unit);
          let isp_s = NaN;
          let ve = NaN;

          if (!isNaN(isp_in)) {
            if (ispUnit === "s") {
              isp_s = isp_in;
              ve = isp_s * g0;
            } else if (ispUnit === "mps") {
              ve = isp_in;
              isp_s = ve / g0;
            }
          }

          let mdot = isNaN(mdot_in) ? NaN : toSIMassFlow(mdot_in, mdotUnit);
          let tburn = isNaN(tburn_in) ? NaN : toSITime(tburn_in, tburnUnit);
          let mprop = isNaN(mprop_in) ? NaN : toSIMass(mprop_in, mpropUnit);
          let m0 = isNaN(m0_in) ? NaN : toSIMass(m0_in, m0Unit);
          let mf = isNaN(mf_in) ? NaN : toSIMass(mf_in, mfUnit);
          let pa = isNaN(pa_in) ? NaN : toSIPressure(pa_in, paUnit);

          // Basic validation: need at least two of (F, isp, mdot) to solve engine performance
          if (isNaN(F) && isNaN(isp_s) && isNaN(mdot)) {
            errorEl.textContent =
              "Enter at least thrust, specific impulse, or mass flow rate.";
          }

          // Solve for missing among F, isp_s, mdot where possible
          // Use relationships: F = isp_s * g0 * mdot
          if (!isNaN(F) && !isNaN(isp_s) && isNaN(mdot)) {
            mdot = F / (isp_s * g0);
          } else if (!isNaN(F) && !isNaN(mdot) && isNaN(isp_s)) {
            isp_s = F / (mdot * g0);
            ve = isp_s * g0;
          } else if (!isNaN(isp_s) && !isNaN(mdot) && isNaN(F)) {
            F = isp_s * g0 * mdot;
          }

          // If ve known but isp_s NaN
          if (!isNaN(ve) && isNaN(isp_s)) {
            isp_s = ve / g0;
          }
          if (!isNaN(isp_s) && isNaN(ve)) {
            ve = isp_s * g0;
          }

          // Relate mprop, mdot, tburn
          if (!isNaN(mdot) && !isNaN(tburn) && isNaN(mprop)) {
            mprop = mdot * tburn;
          } else if (!isNaN(mprop) && !isNaN(mdot) && isNaN(tburn)) {
            tburn = mprop / mdot;
          } else if (!isNaN(mprop) && !isNaN(tburn) && isNaN(mdot)) {
            mdot = mprop / tburn;
          }

          // Relate m0, mf, mprop
          if (!isNaN(m0) && !isNaN(mprop) && isNaN(mf)) {
            mf = m0 - mprop;
          } else if (!isNaN(mf) && !isNaN(mprop) && isNaN(m0)) {
            m0 = mf + mprop;
          } else if (!isNaN(m0) && !isNaN(mf) && isNaN(mprop)) {
            mprop = m0 - mf;
          }

          // Total impulse
          let Itot = NaN;
          if (!isNaN(F) && !isNaN(tburn)) {
            Itot = F * tburn;
          } else if (!isNaN(isp_s) && !isNaN(mprop)) {
            Itot = isp_s * g0 * mprop;
          }

          // Mass ratio and delta-v
          let massRatio = NaN;
          let deltaV = NaN;
          if (!isNaN(m0) && !isNaN(mf) && m0 > mf && !isNaN(ve)) {
            massRatio = m0 / mf;
            deltaV = ve * Math.log(massRatio);
          }

          // Thrust-to-weight at liftoff
          let twr = NaN;
          if (!isNaN(F) && !isNaN(m0) && m0 > 0) {
            twr = F / (m0 * g0);
          }

          // Update results
          $("resThrust").textContent = fromSIThrust(F);
          $("resIsp").textContent = formatIspSeconds(isp_s);
          $("resVe").textContent = formatVe(ve);
          $("resMdot").textContent = fromSIMassFlow(mdot);
          $("resBurnTime").textContent = fromSITime(tburn);
          $("resPropMass").textContent = fromSIMass(mprop);
          $("resItot").textContent = formatImpulse(Itot);

          $("resM0").textContent = fromSIMass(m0);
          $("resMf").textContent = fromSIMass(mf);
          $("resMassRatio").textContent = formatRatio(massRatio);
          $("resDeltaV").textContent = formatDeltaV(deltaV);
          $("resTWR").textContent = formatTWR(twr);

          // Ambient pressure info (optional)
          if (!isNaN(pa)) {
            // Could be used in future for sea-level vs vacuum comparison
            // For now we just ensure it's converted and silently ignored in outputs
            console.log("Ambient pressure (Pa):", pa);
          }
        }

        $("calculateBtn").addEventListener("click", calculate);

        $("resetBtn").addEventListener("click", () => {
          document.getElementById("rocket-form").reset();
          $("errorMsg").textContent = "";
          const resultIds = [
            "resThrust",
            "resIsp",
            "resVe",
            "resMdot",
            "resBurnTime",
            "resPropMass",
            "resItot",
            "resM0",
            "resMf",
            "resMassRatio",
            "resDeltaV",
            "resTWR",
          ];
          resultIds.forEach((id) => {
            $(id).textContent = "–";
          });
        });
      })();
    </script>

