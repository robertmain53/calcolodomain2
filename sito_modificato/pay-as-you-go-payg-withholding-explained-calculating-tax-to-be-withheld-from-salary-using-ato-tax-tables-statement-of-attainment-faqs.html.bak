    <script>
      // Simple PAYG approximation logic
      function periodsPerYear(freq) {
        switch (freq) {
          case "weekly":
            return 52;
          case "fortnightly":
            return 26;
          case "monthly":
            return 12;
          case "quarterly":
            return 4;
          case "annually":
            return 1;
          default:
            return 52;
        }
      }

      function residentTaxAnnual(income) {
        // Approximate Australian resident marginal tax rates (no offsets)
        if (income <= 18200) return 0;
        if (income <= 45000) return (income - 18200) * 0.19;
        if (income <= 120000) return 5092 + (income - 45000) * 0.325;
        if (income <= 180000) return 29467 + (income - 120000) * 0.37;
        return 51667 + (income - 180000) * 0.45;
      }

      function nonResidentTaxAnnual(income) {
        // Simplified non-resident rates (no tax-free threshold)
        if (income <= 120000) return income * 0.325;
        if (income <= 180000) return 39000 + (income - 120000) * 0.37;
        return 61200 + (income - 180000) * 0.45;
      }

      function helpRepaymentAnnual(income) {
        // Very rough HELP/HECS approximation based on income bands
        if (income < 50000) return 0;
        if (income < 70000) return income * 0.01;
        if (income < 90000) return income * 0.02;
        if (income < 110000) return income * 0.03;
        if (income < 130000) return income * 0.04;
        if (income < 150000) return income * 0.05;
        return income * 0.06;
      }

      function formatAUD(value) {
        if (!isFinite(value)) return "–";
        return "A$" + value.toFixed(2).replace(/\B(?=(\d{3})+(?!\d))/g, ",");
      }

      document.addEventListener("DOMContentLoaded", function () {
        const grossInput = document.getElementById("gross-pay");
        const freqSelect = document.getElementById("pay-frequency");
        const residencySelect = document.getElementById("residency");
        const hasHelpCheckbox = document.getElementById("has-help");
        const ignoreMedicareCheckbox =
          document.getElementById("ignore-medicare");
        const calcBtn = document.getElementById("calc-btn");

        const resGross = document.getElementById("res-gross");
        const resPayg = document.getElementById("res-payg");
        const resMedicare = document.getElementById("res-medicare");
        const resHelp = document.getElementById("res-help");
        const resTotalTax = document.getElementById("res-total-tax");
        const resNet = document.getElementById("res-net");
        const resEffectiveRate = document.getElementById("res-effective-rate");

        function getTaxFreeThresholdClaimed() {
          const radios = document.querySelectorAll('input[name="tft"]');
          for (const r of radios) {
            if (r.checked) return r.value === "yes";
          }
          return true;
        }

        function calculate() {
          const gross = parseFloat(grossInput.value);
          if (!isFinite(gross) || gross <= 0) {
            resGross.textContent = "–";
            resPayg.textContent = "–";
            resMedicare.textContent = "–";
            resHelp.textContent = "–";
            resTotalTax.textContent = "–";
            resNet.textContent = "–";
            resEffectiveRate.textContent = "Effective tax rate: –";
            return;
          }

          const freq = freqSelect.value;
          const periods = periodsPerYear(freq);
          const residency = residencySelect.value;
          const tftClaimed = getTaxFreeThresholdClaimed();
          const hasHelp = hasHelpCheckbox.checked;
          const ignoreMedicare = ignoreMedicareCheckbox.checked;

          const annualIncome = gross * periods;

          let taxableIncome = annualIncome;
          let baseTaxAnnual = 0;

          if (residency === "resident") {
            // Resident tax with tax-free threshold
            baseTaxAnnual = residentTaxAnnual(annualIncome);
            if (!tftClaimed) {
              // Approximate "no tax-free threshold" by taxing from first dollar at marginal rates
              // We do this by adding back the tax that would have been saved on the first $18,200
              const thresholdTaxSaved = residentTaxAnnual(18200);
              baseTaxAnnual += thresholdTaxSaved;
            }
          } else {
            baseTaxAnnual = nonResidentTaxAnnual(annualIncome);
          }

          let medicareAnnual = 0;
          if (!ignoreMedicare && residency === "resident") {
            medicareAnnual = taxableIncome * 0.02; // simple 2% levy
          }

          let helpAnnual = 0;
          if (hasHelp) {
            helpAnnual = helpRepaymentAnnual(annualIncome);
          }

          const totalAnnualTax = baseTaxAnnual + medicareAnnual + helpAnnual;
          const paygPerPeriod = totalAnnualTax / periods;
          const medicarePerPeriod = medicareAnnual / periods;
          const helpPerPeriod = helpAnnual / periods;
          const netPerPeriod = gross - paygPerPeriod;

          resGross.textContent = formatAUD(gross);
          resPayg.textContent = formatAUD(paygPerPeriod);
          resMedicare.textContent =
            medicarePerPeriod > 0 ? formatAUD(medicarePerPeriod) : "A$0.00";
          resHelp.textContent =
            helpPerPeriod > 0 ? formatAUD(helpPerPeriod) : "A$0.00";
          resTotalTax.textContent = formatAUD(paygPerPeriod);
          resNet.textContent = formatAUD(netPerPeriod);

          const effectiveRate = (paygPerPeriod / gross) * 100;
          resEffectiveRate.textContent =
            "Effective tax rate this period: " + effectiveRate.toFixed(1) + "%";
        }

        calcBtn.addEventListener("click", calculate);
        grossInput.addEventListener("keyup", function (e) {
          if (e.key === "Enter") calculate();
        });
      });
    </script>

