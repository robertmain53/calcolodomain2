<script>
  // Tab handling
  const tabs = {
    'tab-1rm': 'panel-1rm',
    'tab-volume': 'panel-volume',
    'tab-progress': 'panel-progress'
  };
  Object.keys(tabs).forEach(tabId => {
    document.getElementById(tabId).addEventListener('click', () => {
      Object.keys(tabs).forEach(id => {
        const btn = document.getElementById(id);
        const panel = document.getElementById(tabs[id]);
        if (id === tabId) {
          btn.classList.add('tab-active');
          panel.classList.remove('hidden');
        } else {
          btn.classList.remove('tab-active');
          panel.classList.add('hidden');
        }
      });
    });
  });

  // 1RM calculation
  function estimate1RM(weight, reps) {
    if (reps <= 0 || weight <= 0) return null;
    const w = weight;
    const r = reps;
    const epley = w * (1 + r / 30);
    const brzycki = r >= 37 ? null : w * (36 / (37 - r));
    const lander = w * (100 / (101.3 - 2.671 * r));
    const lombardi = w * Math.pow(r, 0.10);
    const oconnor = w * (1 + 0.025 * r);
    const values = [epley, brzycki, lander, lombardi, oconnor].filter(v => Number.isFinite(v) && v > 0);
    if (!values.length) return null;
    const avg = values.reduce((a, b) => a + b, 0) / values.length;
    return avg;
  }

  function roundTo(weight, unit) {
    const step = unit === 'lb' ? 5 : 2.5;
    return Math.round(weight / step) * step;
  }

  function updatePercentTable(oneRM, unit) {
    const wrapper = document.getElementById('percent-table-wrapper');
    const tbody = document.getElementById('percent-table-body');
    if (!oneRM || oneRM <= 0) {
      wrapper.classList.add('hidden');
      tbody.innerHTML = '';
      return;
    }
    const rows = [
      { pct: 0.5, reps: '10–15+', goal: 'Technique / warm-up' },
      { pct: 0.6, reps: '8–12', goal: 'Hypertrophy / work capacity' },
      { pct: 0.7, reps: '6–10', goal: 'Strength + size' },
      { pct: 0.8, reps: '3–6', goal: 'Heavy strength work' },
      { pct: 0.85, reps: '3–5', goal: 'Heavy strength work' },
      { pct: 0.9, reps: '1–3', goal: 'Peaking / testing' }
    ];
    tbody.innerHTML = rows.map(row => {
      const w = roundTo(oneRM * row.pct, unit);
      return `<tr>
        <td class="px-3 py-2">${Math.round(row.pct * 100)}%</td>
        <td class="px-3 py-2">${row.reps}</td>
        <td class="px-3 py-2 font-medium">${isNaN(w) ? '–' : w + ' ' + unit}</td>
        <td class="px-3 py-2">${row.goal}</td>
      </tr>`;
    }).join('');
    wrapper.classList.remove('hidden');
  }

  document.getElementById('calc-1rm-btn').addEventListener('click', () => {
    const weight = parseFloat(document.getElementById('weight-input').value);
    const reps = parseInt(document.getElementById('reps-input').value, 10);
    const unit = document.getElementById('unit-select').value;
    const rpeVal = document.getElementById('rpe-select').value;

    const oneRM = estimate1RM(weight, reps);
    const oneRmDisplay = document.getElementById('one-rm-display');
    const oneRmRange = document.getElementById('one-rm-range');
    const tmPercent = parseFloat(document.getElementById('tm-percent').value);
    const tmDisplay = document.getElementById('training-max-display');
    const suggestedFocus = document.getElementById('suggested-focus');
    const fatigueNote = document.getElementById('fatigue-note');

    if (!oneRM) {
      oneRmDisplay.textContent = '–';
      oneRmRange.textContent = 'Please enter a valid weight and reps.';
      tmDisplay.textContent = '–';
      suggestedFocus.textContent = 'Unable to estimate. Try a hard set of 3–10 reps.';
      fatigueNote.textContent = '';
      updatePercentTable(null, unit);
      return;
    }

    const oneRmRounded = roundTo(oneRM, unit);
    oneRmDisplay.textContent = oneRmRounded + ' ' + unit;

    const low = roundTo(oneRM * 0.97, unit);
    const high = roundTo(oneRM * 1.03, unit);
    oneRmRange.textContent = `Approx. range: ${low}–${high} ${unit} (±3%)`;

    const tm = roundTo(oneRM * tmPercent, unit);
    tmDisplay.textContent = tm + ' ' + unit;

    // Suggested focus based on reps and RPE
    let focus = '';
    if (reps <= 3) focus = 'Heavy strength / neural focus.';
    else if (reps <= 6) focus = 'Strength with some hypertrophy.';
    else if (reps <= 10) focus = 'Hypertrophy and work capacity.';
    else focus = 'Endurance / high-rep hypertrophy.';

    let fatigue = '';
    if (rpeVal) {
      const rpe = parseFloat(rpeVal);
      if (rpe >= 9.5) fatigue = 'Very fatiguing set. Use sparingly and ensure adequate recovery.';
      else if (rpe >= 8) fatigue = 'Hard but sustainable. Good for main working sets.';
      else fatigue = 'Moderate effort. Good for warm-ups or volume work.';
    }

    suggestedFocus.textContent = focus;
    fatigueNote.textContent = fatigue;

    updatePercentTable(oneRM, unit);
  });

  document.getElementById('tm-percent').addEventListener('change', () => {
    // Recalculate training max if 1RM already exists
    const current = document.getElementById('one-rm-display').textContent.split(' ')[0];
    const unit = document.getElementById('unit-select').value;
    const oneRM = parseFloat(current);
    if (!oneRM || isNaN(oneRM)) return;
    const tmPercent = parseFloat(document.getElementById('tm-percent').value);
    const tmDisplay = document.getElementById('training-max-display');
    const tm = roundTo(oneRM * tmPercent, unit);
    tmDisplay.textContent = tm + ' ' + unit;
  });

  // Volume & Tonnage
  const volumeSets = [];
  function renderVolumeTable() {
    const tbody = document.getElementById('volume-table-body');
    const unit = document.getElementById('unit-select').value;
    tbody.innerHTML = volumeSets.map((s, idx) => {
      const tonnage = s.weight * s.reps * s.sets;
      return `<tr>
        <td class="px-3 py-2">${idx + 1}</td>
        <td class="px-3 py-2">${s.weight} ${unit}</td>
        <td class="px-3 py-2">${s.sets}</td>
        <td class="px-3 py-2">${s.reps}</td>
        <td class="px-3 py-2">${tonnage.toFixed(1)} ${unit}</td>
        <td class="px-3 py-2 text-right">
          <button type="button" data-idx="${idx}" class="text-xs text-red-600 hover:underline remove-set-btn">Remove</button>
        </td>
      </tr>`;
    }).join('');

    let totalSets = 0, totalReps = 0, totalTonnage = 0;
    volumeSets.forEach(s => {
      totalSets += s.sets;
      totalReps += s.sets * s.reps;
      totalTonnage += s.weight * s.reps * s.sets;
    });
    document.getElementById('total-sets').textContent = totalSets;
    document.getElementById('total-reps').textContent = totalReps;
    document.getElementById('total-tonnage').textContent = totalTonnage.toFixed(1) + ' ' + unit;

    document.querySelectorAll('.remove-set-btn').forEach(btn => {
      btn.addEventListener('click', () => {
        const idx = parseInt(btn.getAttribute('data-idx'), 10);
        volumeSets.splice(idx, 1);
        renderVolumeTable();
      });
    });
  }

  document.getElementById('add-set-btn').addEventListener('click', () => {
    const w = parseFloat(document.getElementById('vol-weight').value);
    const r = parseInt(document.getElementById('vol-reps').value, 10);
    const s = parseInt(document.getElementById('vol-sets').value, 10);
    if (!w || !r || !s || w <= 0 || r <= 0 || s <= 0) return;
    volumeSets.push({ weight: w, reps: r, sets: s });
    renderVolumeTable();
  });

  document.getElementById('clear-sets-btn').addEventListener('click', () => {
    volumeSets.length = 0;
    renderVolumeTable();
  });

  // Progress comparison
  document.getElementById('compare-btn').addEventListener('click', () => {
    const unit = document.getElementById('unit-select').value;
    const aw = parseFloat(document.getElementById('a-weight').value);
    const ar = parseInt(document.getElementById('a-reps').value, 10);
    const bw = parseFloat(document.getElementById('b-weight').value);
    const br = parseInt(document.getElementById('b-reps').value, 10);

    const a1rm = estimate1RM(aw, ar);
    const b1rm = estimate1RM(bw, br);

    const summary = document.getElementById('compare-summary');
    const details = document.getElementById('compare-details');
    const a1rmEl = document.getElementById('a-1rm');
    const b1rmEl = document.getElementById('b-1rm');
    const changeEl = document.getElementById('strength-change');

    if (!a1rm || !b1rm) {
      summary.textContent = 'Enter valid weight and reps for both sessions to compare.';
      details.classList.add('hidden');
      return;
    }

    const aRounded = roundTo(a1rm, unit);
    const bRounded = roundTo(b1rm, unit);
    a1rmEl.textContent = aRounded + ' ' + unit;
    b1rmEl.textContent = bRounded + ' ' + unit;

    const diff = b1rm - a1rm;
    const pct = (diff / a1rm) * 100;
    const pctRounded = pct.toFixed(1);
    let text;
    if (pct > 0) text = `~${pctRounded}% stronger than Session A.`;
    else if (pct < 0) text = `~${Math.abs(pctRounded)}% below Session A.`;
    else text = 'No estimated change in strength.';

    changeEl.textContent = text;
    summary.textContent = 'Comparison based on estimated 1RM from each performance.';
    details.classList.remove('hidden');
  });

  document.querySelectorAll('#faq-container .faq-question').forEach(btn => {
    btn.addEventListener('click', () => {
      const answer = btn.nextElementSibling;
      const icon = btn.querySelector('span:last-child');
      const isHidden = answer.classList.contains('hidden');
      document.querySelectorAll('#faq-container .faq-question + div').forEach(div => div.classList.add('hidden'));
      document.querySelectorAll('#faq-container .faq-question span:last-child').forEach(i => i.textContent = '+');
      if (isHidden) {
        answer.classList.remove('hidden');
        icon.textContent = '−';
      } else {
        answer.classList.add('hidden');
        icon.textContent = '+';
      }
    });
  });

  // Keep tonnage unit label in sync with main unit selector
  document.getElementById('unit-select').addEventListener('change', () => {
    renderVolumeTable();
  });
</script>

