    <script>
      // --- Empirical Formula Calculator Logic ---

      const elementsBody = document.getElementById("elements-body");
      const addRowBtn = document.getElementById("add-row");
      const loadExampleBtn = document.getElementById("load-example");
      const modePercentBtn = document.getElementById("mode-percent");
      const modeMassBtn = document.getElementById("mode-mass");
      const headerAmountLabel = document.getElementById("header-amount-label");
      const sumInfo = document.getElementById("sum-info");
      const roundingSelect = document.getElementById("rounding-select");
      const molarMassInput = document.getElementById("molar-mass");
      const calculateBtn = document.getElementById("calculate-btn");
      const errorMsg = document.getElementById("error-msg");
      const resultsSection = document.getElementById("results-section");
      const empiricalFormulaEl = document.getElementById("empirical-formula");
      const molecularWrapper = document.getElementById("molecular-wrapper");
      const molecularFormulaEl = document.getElementById("molecular-formula");
      const stepsContainer = document.getElementById("steps-container");
      const ratioWarning = document.getElementById("ratio-warning");
      const resultModeBadge = document.getElementById("result-mode-badge");

      let mode = "percent"; // 'percent' or 'mass'

      const defaultAtomicMasses = {
        H: 1.008,
        He: 4.003,
        Li: 6.941,
        Be: 9.012,
        B: 10.81,
        C: 12.01,
        N: 14.01,
        O: 16.0,
        F: 19.0,
        Ne: 20.18,
        Na: 22.99,
        Mg: 24.31,
        Al: 26.98,
        Si: 28.09,
        P: 30.97,
        S: 32.07,
        Cl: 35.45,
        Ar: 39.95,
        K: 39.1,
        Ca: 40.08,
        Fe: 55.85,
        Cu: 63.55,
        Zn: 65.38,
        Br: 79.9,
        Ag: 107.87,
        I: 126.9,
        Ba: 137.33,
        Au: 196.97,
        Hg: 200.59,
        Pb: 207.2,
      };

      function createRow(symbol = "", amount = "", mass = "") {
        const tr = document.createElement("tr");

        tr.innerHTML = `
      <td class="px-2 py-1 border-b">
        <input type="text" maxlength="2" class="element-symbol w-20 border border-gray-300 rounded px-2 py-1 text-sm" placeholder="C" value="${symbol}">
      </td>
      <td class="px-2 py-1 border-b">
        <input type="number" step="0.0001" min="0" class="element-amount w-28 border border-gray-300 rounded px-2 py-1 text-sm" placeholder="${
          mode === "percent" ? "e.g. 40.0" : "e.g. 4.50"
        }" value="${amount}">
      </td>
      <td class="px-2 py-1 border-b">
        <input type="number" step="0.0001" min="0" class="element-mass w-28 border border-gray-300 rounded px-2 py-1 text-sm" placeholder="auto" value="${mass}">
      </td>
      <td class="px-2 py-1 border-b">
        <button type="button" class="remove-row text-xs text-red-600 hover:underline">Remove</button>
      </td>
    `;

        elementsBody.appendChild(tr);
      }

      function initDefaultRows() {
        elementsBody.innerHTML = "";
        createRow("C", "40.0", defaultAtomicMasses["C"]);
        createRow("H", "6.7", defaultAtomicMasses["H"]);
        createRow("O", "53.3", defaultAtomicMasses["O"]);
        updateSumInfo();
      }

      function setMode(newMode) {
        mode = newMode;
        if (mode === "percent") {
          modePercentBtn.classList.add("bg-blue-600", "text-white");
          modePercentBtn.classList.remove("bg-gray-100", "text-gray-700");
          modeMassBtn.classList.remove("bg-blue-600", "text-white");
          modeMassBtn.classList.add("bg-gray-100", "text-gray-700");
          headerAmountLabel.textContent = "Percent (%)";
          resultModeBadge.textContent = "Percent mode";
          resultModeBadge.classList.remove("bg-green-100", "text-green-700");
          resultModeBadge.classList.add("bg-blue-100", "text-blue-700");
        } else {
          modeMassBtn.classList.add("bg-blue-600", "text-white");
          modeMassBtn.classList.remove("bg-gray-100", "text-gray-700");
          modePercentBtn.classList.remove("bg-blue-600", "text-white");
          modePercentBtn.classList.add("bg-gray-100", "text-gray-700");
          headerAmountLabel.textContent = "Mass (g)";
          resultModeBadge.textContent = "Mass mode";
          resultModeBadge.classList.remove("bg-blue-100", "text-blue-700");
          resultModeBadge.classList.add("bg-green-100", "text-green-700");
        }

        // Update placeholders
        document.querySelectorAll(".element-amount").forEach((input) => {
          input.placeholder = mode === "percent" ? "e.g. 40.0" : "e.g. 4.50";
        });
        updateSumInfo();
      }

      function updateSumInfo() {
        const amounts = Array.from(document.querySelectorAll(".element-amount"))
          .map((inp) => parseFloat(inp.value))
          .filter((v) => !isNaN(v) && v >= 0);

        if (mode === "percent") {
          const sum = amounts.reduce((a, b) => a + b, 0);
          if (sum > 0) {
            sumInfo.textContent = `Current total: ${sum.toFixed(
              2
            )}%. The calculator will normalize to 100% if needed.`;
          } else {
            sumInfo.textContent = "";
          }
        } else {
          sumInfo.textContent =
            "Enter masses in grams. They do not need to sum to any particular value.";
        }
      }

      function parseRows() {
        const rows = [];
        const trs = elementsBody.querySelectorAll("tr");
        trs.forEach((tr) => {
          const symbol = tr.querySelector(".element-symbol").value.trim();
          const amountStr = tr.querySelector(".element-amount").value.trim();
          const massStr = tr.querySelector(".element-mass").value.trim();
          if (!symbol && !amountStr && !massStr) return;

          const amount = parseFloat(amountStr);
          const atomicMass = massStr
            ? parseFloat(massStr)
            : defaultAtomicMasses[symbol] || NaN;

          rows.push({ tr, symbol, amount, atomicMass });
        });
        return rows;
      }

      function clearErrors() {
        errorMsg.classList.add("hidden");
        errorMsg.textContent = "";
        document
          .querySelectorAll(".element-symbol, .element-amount, .element-mass")
          .forEach((inp) => {
            inp.classList.remove("input-error");
          });
      }

      function showError(message, inputs = []) {
        errorMsg.textContent = message;
        errorMsg.classList.remove("hidden");
        inputs.forEach((inp) => inp.classList.add("input-error"));
      }

      function closestSimpleFraction(x, tol) {
        // Try integers first
        const nearestInt = Math.round(x);
        if (Math.abs(x - nearestInt) <= tol) return nearestInt;

        const candidates = [1 / 2, 1 / 3, 2 / 3, 1 / 4, 3 / 4, 1.5, 2.5, 3.5];
        for (const frac of candidates) {
          const k = x / frac;
          const kRound = Math.round(k);
          if (kRound > 0 && Math.abs(k - kRound) <= tol) {
            return frac * kRound;
          }
        }
        return x;
      }

      function formatFormula(formulaObj) {
        // formulaObj: { C: 6, H: 12, O: 6 }
        return Object.entries(formulaObj)
          .sort((a, b) => a[0].localeCompare(b[0]))
          .map(([sym, n]) => (n === 1 ? sym : sym + n))
          .join("");
      }

      function calculate() {
        clearErrors();
        ratioWarning.classList.add("hidden");
        stepsContainer.innerHTML = "";
        empiricalFormulaEl.textContent = "";
        molecularWrapper.classList.add("hidden");
        molecularFormulaEl.textContent = "";

        const rows = parseRows();
        if (rows.length === 0) {
          showError("Please enter at least one element with data.");
          return;
        }

        // Validate symbols and data
        for (const { tr, symbol, amount, atomicMass } of rows) {
          const symInput = tr.querySelector(".element-symbol");
          const amtInput = tr.querySelector(".element-amount");
          const massInput = tr.querySelector(".element-mass");

          if (!symbol.match(/^[A-Z][a-z]?$/)) {
            showError("Use valid element symbols (e.g., C, H, O, Na, Cl).", [
              symInput,
            ]);
            return;
          }
          if (isNaN(amount) || amount <= 0) {
            showError(
              "All elements must have a positive percent or mass value.",
              [amtInput]
            );
            return;
          }
          if (isNaN(atomicMass) || atomicMass <= 0) {
            showError(
              "Please provide a valid atomic mass for each element (or use a common element with a default value).",
              [massInput]
            );
            return;
          }
        }

        // Normalize if percent mode
        let amounts = rows.map((r) => r.amount);
        if (mode === "percent") {
          const sum = amounts.reduce((a, b) => a + b, 0);
          if (sum <= 0) {
            showError("Total percent must be greater than 0.");
            return;
          }
          amounts = amounts.map((v) => (v * 100) / sum);
        }

        const roundingTol = parseFloat(roundingSelect.value) || 0.05;

        // Step 1: convert to moles
        const moles = amounts.map((amt, i) => amt / rows[i].atomicMass);

        // Step 2: divide by smallest
        const minMoles = Math.min(...moles);
        const ratiosRaw = moles.map((m) => m / minMoles);

        // Step 3: adjust to nearest simple fraction / integer
        const ratiosAdjusted = ratiosRaw.map((r) =>
          closestSimpleFraction(r, roundingTol)
        );

        // Check if any ratio is still far from integer
        const multipliers = [];
        let maxMultiplier = 1;
        const finalInts = ratiosAdjusted.map((r) => {
          const rounded = Math.round(r);
          if (Math.abs(r - rounded) > roundingTol) {
            // try scaling by small integers 2-6
            let best = r;
            let bestMult = 1;
            for (let k = 2; k <= 6; k++) {
              const scaled = r * k;
              const scaledRound = Math.round(scaled);
              if (Math.abs(scaled - scaledRound) <= roundingTol) {
                best = scaledRound;
                bestMult = k;
                break;
              }
            }
            multipliers.push(bestMult);
            maxMultiplier = Math.max(maxMultiplier, bestMult);
            return best;
          } else {
            multipliers.push(1);
            return rounded;
          }
        });

        if (maxMultiplier > 3) {
          ratioWarning.classList.remove("hidden");
        }

        // Build empirical formula object
        const empirical = {};
        finalInts.forEach((n, i) => {
          if (n <= 0) return;
          empirical[rows[i].symbol] = n;
        });

        if (Object.keys(empirical).length === 0) {
          showError(
            "Could not determine a valid empirical formula. Check your inputs."
          );
          return;
        }

        const empiricalFormulaStr = formatFormula(empirical);
        empiricalFormulaEl.textContent = empiricalFormulaStr;

        // Molecular formula if molar mass provided
        const molarMass = parseFloat(molarMassInput.value);
        if (!isNaN(molarMass) && molarMass > 0) {
          // empirical mass
          let empMass = 0;
          for (const [sym, n] of Object.entries(empirical)) {
            const row = rows.find((r) => r.symbol === sym);
            empMass +=
              n * (row ? row.atomicMass : defaultAtomicMasses[sym] || 0);
          }
          if (empMass > 0) {
            const nRatio = molarMass / empMass;
            const nInt = Math.round(nRatio);
            if (Math.abs(nRatio - nInt) <= 0.1 && nInt >= 1) {
              const molecular = {};
              for (const [sym, n] of Object.entries(empirical)) {
                molecular[sym] = n * nInt;
              }
              molecularFormulaEl.textContent = formatFormula(molecular);
              molecularWrapper.classList.remove("hidden");
            }
          }
        }

        // Build step-by-step explanation
        const stepLines = [];

        stepLines.push("<p><strong>1. Convert input to moles</strong></p>");
        stepLines.push("<ul>");
        rows.forEach((row, i) => {
          const basis =
            mode === "percent"
              ? `${amounts[i].toFixed(4)} g (from normalized percent)`
              : `${amounts[i].toFixed(4)} g`;
          stepLines.push(
            `<li>${row.symbol}: moles = ${basis} ÷ ${row.atomicMass.toFixed(
              4
            )} g/mol = ${moles[i].toFixed(4)} mol</li>`
          );
        });
        stepLines.push("</ul>");

        stepLines.push(
          "<p><strong>2. Divide all mole values by the smallest</strong></p>"
        );
        stepLines.push(
          `<p>Smallest mole value = ${minMoles.toFixed(4)} mol</p>`
        );
        stepLines.push("<ul>");
        rows.forEach((row, i) => {
          stepLines.push(
            `<li>${row.symbol}: ${moles[i].toFixed(4)} ÷ ${minMoles.toFixed(
              4
            )} = ${ratiosRaw[i].toFixed(4)}</li>`
          );
        });
        stepLines.push("</ul>");

        stepLines.push(
          "<p><strong>3. Adjust ratios to simple whole numbers</strong></p>"
        );
        stepLines.push("<ul>");
        rows.forEach((row, i) => {
          stepLines.push(
            `<li>${row.symbol}: ratio ≈ ${ratiosRaw[i].toFixed(
              4
            )} → adjusted ≈ ${ratiosAdjusted[i].toFixed(
              4
            )} → final subscript = ${finalInts[i]}</li>`
          );
        });
        stepLines.push("</ul>");

        stepLines.push(
          "<p><strong>4. Write the empirical formula</strong></p>"
        );
        stepLines.push(
          `<p>Empirical formula = <code>${empiricalFormulaStr}</code></p>`
        );

        if (
          !isNaN(molarMass) &&
          molarMass > 0 &&
          molecularWrapper.classList.contains("hidden") === false
        ) {
          stepLines.push(
            "<p><strong>5. Use molar mass to find molecular formula</strong></p>"
          );
          stepLines.push(
            `<p>Empirical formula mass = ${empMass.toFixed(3)} g/mol</p>`
          );
          const nRatio = molarMass / empMass;
          const nInt = Math.round(nRatio);
          stepLines.push(
            `<p>n = molar mass ÷ empirical mass = ${molarMass.toFixed(
              3
            )} ÷ ${empMass.toFixed(3)} ≈ ${nRatio.toFixed(3)} ≈ ${nInt}</p>`
          );
          stepLines.push(
            "<p>Multiply all empirical subscripts by n to get the molecular formula shown above.</p>"
          );
        }

        stepsContainer.innerHTML = stepLines.join("\n");
        resultsSection.classList.remove("hidden");
      }

      // Event listeners
      addRowBtn.addEventListener("click", () => {
        createRow();
      });

      loadExampleBtn.addEventListener("click", () => {
        elementsBody.innerHTML = "";
        if (mode === "percent") {
          // Classic CH2O example
          createRow("C", "40.0", defaultAtomicMasses["C"]);
          createRow("H", "6.7", defaultAtomicMasses["H"]);
          createRow("O", "53.3", defaultAtomicMasses["O"]);
        } else {
          // Mass example: 1.80 g C, 0.30 g H, 2.40 g O
          createRow("C", "1.80", defaultAtomicMasses["C"]);
          createRow("H", "0.30", defaultAtomicMasses["H"]);
          createRow("O", "2.40", defaultAtomicMasses["O"]);
        }
        updateSumInfo();
      });

      elementsBody.addEventListener("click", (e) => {
        if (e.target.classList.contains("remove-row")) {
          const tr = e.target.closest("tr");
          tr.remove();
          updateSumInfo();
        }
      });

      elementsBody.addEventListener("input", (e) => {
        if (e.target.classList.contains("element-amount")) {
          updateSumInfo();
        }
      });

      modePercentBtn.addEventListener("click", () => setMode("percent"));
      modeMassBtn.addEventListener("click", () => setMode("mass"));
      calculateBtn.addEventListener("click", calculate);

      // Initialize
      initDefaultRows();
    </script>

