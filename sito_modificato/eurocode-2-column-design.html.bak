    <script>
      // Utility functions
      function roundTo(value, decimals) {
        if (!isFinite(value)) return NaN;
        const factor = Math.pow(10, decimals);
        return Math.round(value * factor) / factor;
      }

      function updateBadge(util) {
        const badge = document.getElementById("utilisation-badge");
        if (!isFinite(util)) {
          badge.textContent = "No result";
          badge.className =
            "text-xs font-semibold px-2 py-1 rounded-full result-badge-warn";
          return;
        }
        if (util <= 0.8) {
          badge.textContent =
            "OK – utilisation " + (util * 100).toFixed(0) + "%";
          badge.className =
            "text-xs font-semibold px-2 py-1 rounded-full result-badge-ok";
        } else if (util <= 1.0) {
          badge.textContent =
            "High – utilisation " + (util * 100).toFixed(0) + "%";
          badge.className =
            "text-xs font-semibold px-2 py-1 rounded-full result-badge-warn";
        } else {
          badge.textContent =
            "Not OK – utilisation " + (util * 100).toFixed(0) + "%";
          badge.className =
            "text-xs font-semibold px-2 py-1 rounded-full result-badge-fail";
        }
      }

      document
        .getElementById("reset-btn")
        .addEventListener("click", function () {
          document.getElementById("ec2-column-form").reset();
          document.getElementById("fck").value = "25";
          document.getElementById("fyk").value = "500";
          document.getElementById("gamma-c").value = "1.5";
          document.getElementById("gamma-s").value = "1.15";
          document.getElementById("braced").value = "braced";
          document.getElementById("bar-diameter").value = "16";
          document.getElementById("rho-min-factor").value = "yes";
          document.getElementById("rho-user").value = "0.02";

          document.getElementById("results-summary").classList.add("hidden");
          document.getElementById("results-details").classList.add("hidden");
          document.getElementById("results-note").classList.add("hidden");
        });

      document
        .getElementById("calculate-btn")
        .addEventListener("click", function () {
          // Read inputs
          const b =
            parseFloat(document.getElementById("col-width").value) / 1000; // m
          const h =
            parseFloat(document.getElementById("col-depth").value) / 1000; // m
          const L0 = parseFloat(document.getElementById("col-length").value); // m
          const cnom =
            parseFloat(document.getElementById("cover").value) / 1000; // m

          const fck = parseFloat(document.getElementById("fck").value); // MPa
          const fyk = parseFloat(document.getElementById("fyk").value); // MPa
          const gammaC = parseFloat(document.getElementById("gamma-c").value);
          const gammaS = parseFloat(document.getElementById("gamma-s").value);

          const Ned = parseFloat(document.getElementById("ned").value); // kN
          const Med = parseFloat(document.getElementById("med").value); // kNm
          const M0Ed = parseFloat(document.getElementById("first-order").value); // kNm
          const frameType = document.getElementById("braced").value;

          const phiBar =
            parseFloat(document.getElementById("bar-diameter").value) / 1000; // m
          const nBars = parseInt(document.getElementById("num-bars").value, 10);
          const useRhoMin =
            document.getElementById("rho-min-factor").value === "yes";
          const rhoUser = parseFloat(document.getElementById("rho-user").value);

          // Basic validation
          if (!b || !h || !L0 || !fck || !fyk || !Ned || !nBars) {
            alert("Please fill in all mandatory fields with valid numbers.");
            return;
          }

          // Section properties
          const Ac = b * h; // m2
          const I = (b * Math.pow(h, 3)) / 12; // m4, bending about strong axis
          const i = Math.sqrt(I / Ac); // m

          // Material design strengths
          const alphaCC = 0.85; // generic
          const fcd = (alphaCC * fck) / gammaC; // MPa
          const fyd = fyk / gammaS; // MPa

          // Convert to consistent units (N, m)
          const fcdN = fcd * 1e6; // N/m2
          const fydN = fyd * 1e6; // N/m2

          // Steel area
          const AsProv = (nBars * Math.PI * Math.pow(phiBar, 2)) / 4; // m2
          const rhoProv = AsProv / Ac;

          // Slenderness
          const lambda = L0 / i;

          // Approximate Ecm (EC2 Table 3.1) in MPa
          let Ecm;
          if (fck <= 20) Ecm = 27e3;
          else if (fck <= 25) Ecm = 30e3;
          else if (fck <= 30) Ecm = 33e3;
          else if (fck <= 35) Ecm = 34e3;
          else if (fck <= 40) Ecm = 35e3;
          else if (fck <= 45) Ecm = 36e3;
          else if (fck <= 50) Ecm = 37e3;
          else Ecm = 37e3;
          const EcmN = Ecm * 1e6; // N/m2

          // Critical load Ncr (Euler)
          const Ncr = (Math.PI * Math.PI * EcmN * I) / (L0 * L0); // N

          // Slenderness limit (simplified)
          // Use a conservative expression: lambda_lim = 20 * (0.7 + 0.3 * (Ned/Nrd0)) approx.
          // First estimate Nrd0 ignoring steel:
          const Nrd0 = 0.6 * fcdN * Ac; // N, very rough
          const nedRatio = Math.min((Ned * 1e3) / Nrd0, 1.0);
          const lambdaLim = 20 * (0.7 + 0.3 * nedRatio); // dimensionless

          // Second-order check
          let secondOrderRequired = lambda > lambdaLim;
          // For unbraced frames, be more conservative
          if (frameType === "unbraced" && lambda > 10)
            secondOrderRequired = true;

          // Amplification factor k
          let kFactor = 1.0;
          if (secondOrderRequired && Ncr > 0) {
            const ratio = (Ned * 1e3) / Ncr;
            if (ratio < 0.9) {
              kFactor = 1 / (1 - ratio);
            } else {
              kFactor = 3.0; // cap to avoid blow-up
            }
          }
          const Med2 = kFactor * M0Ed; // kNm

          // Axial resistance (simplified)
          const nu = 0.6; // reduction factor for columns
          const NRdN = nu * fcdN * Ac + AsProv * fydN; // N
          const NRd = NRdN / 1e3; // kN
          const etaN = Ned / NRd;

          // Minimum reinforcement ratio
          let rhoMin;
          if (useRhoMin) {
            const term = (0.1 * (Ned * 1e3)) / (Ac * fcdN);
            rhoMin = Math.max(term, 0.002);
          } else {
            rhoMin = rhoUser;
          }

          // Approximate required steel area from axial capacity (ignoring bending)
          const AsReqN = Math.max((Ned * 1e3 - nu * fcdN * Ac) / fydN, 0);
          const AsReq = AsReqN; // m2

          // Update UI
          document.getElementById("results-summary").classList.remove("hidden");
          document.getElementById("results-details").classList.remove("hidden");
          document.getElementById("results-note").classList.remove("hidden");

          document.getElementById("ned-val").textContent = roundTo(Ned, 1);
          document.getElementById("nrd-val").textContent = isFinite(NRd)
            ? roundTo(NRd, 1)
            : "–";
          document.getElementById("eta-n").textContent = isFinite(etaN)
            ? roundTo(etaN, 2)
            : "–";

          document.getElementById("lambda-val").textContent = roundTo(
            lambda,
            1
          );
          document.getElementById("lambda-lim-val").textContent = roundTo(
            lambdaLim,
            1
          );
          document.getElementById("second-order-flag").textContent =
            secondOrderRequired ? "Yes" : "No";
          document.getElementById("k-factor-val").textContent = roundTo(
            kFactor,
            2
          );
          document.getElementById("med2-val").textContent = roundTo(Med2, 1);

          document.getElementById("as-prov-val").textContent = roundTo(
            AsProv * 1e6,
            0
          ); // mm2
          document.getElementById("as-req-val").textContent = isFinite(AsReq)
            ? roundTo(AsReq * 1e6, 0)
            : "–"; // mm2
          document.getElementById("rho-prov-val").textContent = roundTo(
            rhoProv,
            4
          );
          document.getElementById("rho-min-val").textContent = roundTo(
            rhoMin,
            4
          );

          // Status messages
          const statusAxial = document.getElementById("status-axial");
          const statusSlenderness =
            document.getElementById("status-slenderness");
          const statusRebar = document.getElementById("status-rebar");

          if (etaN <= 1.0) {
            statusAxial.textContent =
              "Axial compression check: OK (ηN = " +
              roundTo(etaN, 2) +
              " ≤ 1.0)";
          } else {
            statusAxial.textContent =
              "Axial compression check: NOT OK (ηN = " +
              roundTo(etaN, 2) +
              " > 1.0)";
          }

          if (secondOrderRequired) {
            statusSlenderness.textContent =
              "Slenderness / second-order: λ = " +
              roundTo(lambda, 1) +
              " > λlim = " +
              roundTo(lambdaLim, 1) +
              " → second-order effects included (k = " +
              roundTo(kFactor, 2) +
              ").";
          } else {
            statusSlenderness.textContent =
              "Slenderness / second-order: λ = " +
              roundTo(lambda, 1) +
              " ≤ λlim = " +
              roundTo(lambdaLim, 1) +
              " → second-order effects may be neglected.";
          }

          if (rhoProv >= rhoMin && rhoProv <= 0.04) {
            statusRebar.textContent =
              "Longitudinal reinforcement: OK (ρprov = " +
              roundTo(rhoProv, 3) +
              " between ρmin = " +
              roundTo(rhoMin, 3) +
              " and ρmax ≈ 0.04).";
          } else if (rhoProv < rhoMin) {
            statusRebar.textContent =
              "Longitudinal reinforcement: NOT OK (ρprov = " +
              roundTo(rhoProv, 3) +
              " < ρmin = " +
              roundTo(rhoMin, 3) +
              ").";
          } else {
            statusRebar.textContent =
              "Longitudinal reinforcement: NOT OK (ρprov = " +
              roundTo(rhoProv, 3) +
              " > ρmax ≈ 0.04).";
          }

          // Summary text
          let summary =
            "Design axial resistance N_Rd ≈ " +
            roundTo(NRd, 1) +
            " kN, utilisation η_N = " +
            (isFinite(etaN) ? roundTo(etaN, 2) : "–") +
            ". ";
          summary +=
            "Slenderness λ = " +
            roundTo(lambda, 1) +
            (secondOrderRequired
              ? " > λ_lim → second-order effects considered (k = " +
                roundTo(kFactor, 2) +
                "). "
              : " ≤ λ_lim → second-order effects may be neglected. ");
          summary +=
            "Provided reinforcement ratio ρ_l,prov = " +
            roundTo(rhoProv, 3) +
            ", minimum ρ_l,min = " +
            roundTo(rhoMin, 3) +
            ".";

          document.getElementById("summary-text").textContent = summary;

          updateBadge(etaN);
        });
    </script>

