    <script>
      // Utility conversions
      function toFeet(value, unit) {
        if (!value || value <= 0) return 0;
        switch (unit) {
          case "ft":
            return value;
          case "yd":
            return value * 3;
          case "m":
            return value * 3.280839895;
          default:
            return value;
        }
      }

      function toFeetDepth(value, unit) {
        if (!value || value <= 0) return 0;
        switch (unit) {
          case "ft":
            return value;
          case "in":
            return value / 12;
          case "cm":
            return value / 30.48;
          case "m":
            return value * 3.280839895;
          default:
            return value;
        }
      }

      function volumeFromMode(mode) {
        if (mode === "rect") {
          const length = parseFloat(document.getElementById("length").value);
          const width = parseFloat(document.getElementById("width").value);
          const depth = parseFloat(document.getElementById("depth").value);
          const lengthUnit = document.getElementById("length-unit").value;
          const widthUnit = document.getElementById("width-unit").value;
          const depthUnit = document.getElementById("depth-unit").value;
          const count =
            parseFloat(document.getElementById("areas-count").value) || 1;

          const L = toFeet(length, lengthUnit);
          const W = toFeet(width, widthUnit);
          const D = toFeetDepth(depth, depthUnit);

          if (L <= 0 || W <= 0 || D <= 0) return 0;
          return L * W * D * count;
        }

        if (mode === "circle") {
          const radius = parseFloat(document.getElementById("radius").value);
          const depth = parseFloat(
            document.getElementById("depth-circle").value
          );
          const radiusUnit = document.getElementById("radius-unit").value;
          const depthUnit = document.getElementById("depth-circle-unit").value;
          const count =
            parseFloat(document.getElementById("areas-count-circle").value) ||
            1;

          const R = toFeet(radius, radiusUnit);
          const D = toFeetDepth(depth, depthUnit);

          if (R <= 0 || D <= 0) return 0;
          const area = Math.PI * R * R;
          return area * D * count;
        }

        if (mode === "custom") {
          const vol = parseFloat(
            document.getElementById("custom-volume").value
          );
          const unit = document.getElementById("custom-volume-unit").value;
          if (!vol || vol <= 0) return 0;
          switch (unit) {
            case "ft3":
              return vol;
            case "yd3":
              return vol * 27;
            case "m3":
              return vol * 35.3146667;
            default:
              return vol;
          }
        }

        return 0;
      }

      function getDensityLbFt3() {
        const densityVal = parseFloat(document.getElementById("density").value);
        const densityUnit = document.getElementById("density-unit").value;
        if (!densityVal || densityVal <= 0) return 105; // default
        if (densityUnit === "lbft3") return densityVal;
        // kg/m3 -> lb/ft3
        if (densityUnit === "kgm3") {
          return densityVal * 0.0624279606;
        }
        return densityVal;
      }

      function getBagWeightLb() {
        const bagVal = parseFloat(document.getElementById("bag-weight").value);
        const bagUnit = document.getElementById("bag-weight-unit").value;
        if (!bagVal || bagVal <= 0) return 50;
        if (bagUnit === "lb") return bagVal;
        if (bagUnit === "kg") return bagVal * 2.2046226218;
        return bagVal;
      }

      function formatNumber(num, decimals) {
        if (!isFinite(num)) return "0";
        return num.toLocaleString(undefined, {
          maximumFractionDigits: decimals,
          minimumFractionDigits: 0,
        });
      }

      function updateResults() {
        const activeModeBtn = document.querySelector(
          ".mode-tab.border-blue-600"
        );
        const mode = activeModeBtn
          ? activeModeBtn.getAttribute("data-mode")
          : "rect";
        const volFt3 = volumeFromMode(mode);
        const errorEl = document.getElementById("error-msg");

        if (volFt3 <= 0) {
          errorEl.classList.remove("hidden");
          // Reset results
          document.getElementById("res-ft3").textContent = "0";
          document.getElementById("res-yd3").textContent = "0";
          document.getElementById("res-m3").textContent = "0";
          document.getElementById("res-tons").textContent = "0";
          document.getElementById("res-lb").textContent = "0";
          document.getElementById("res-kg").textContent = "0";
          document.getElementById("res-bags").textContent = "0";
          return;
        }

        errorEl.classList.add("hidden");

        const yd3 = volFt3 / 27;
        const m3 = volFt3 / 35.3146667;

        const densityLbFt3 = getDensityLbFt3();
        const weightLb = volFt3 * densityLbFt3;
        const weightTons = weightLb / 2000;
        const weightKg = weightLb * 0.45359237;

        const bagWeightLb = getBagWeightLb();
        const bags = weightLb / bagWeightLb;

        // Update DOM
        document.getElementById("res-ft3").textContent = formatNumber(
          volFt3,
          1
        );
        document.getElementById("res-yd3").textContent = formatNumber(yd3, 2);
        document.getElementById("res-m3").textContent = formatNumber(m3, 2);
        document.getElementById("res-tons").textContent = formatNumber(
          weightTons,
          2
        );
        document.getElementById("res-lb").textContent = formatNumber(
          weightLb,
          0
        );
        document.getElementById("res-kg").textContent = formatNumber(
          weightKg,
          0
        );
        document.getElementById("res-bags").textContent = formatNumber(
          Math.ceil(bags),
          0
        );

        const bagInputVal =
          parseFloat(document.getElementById("bag-weight").value) || 50;
        const bagUnit = document.getElementById("bag-weight-unit").value;
        document.getElementById("res-bag-size").textContent =
          bagInputVal + " " + bagUnit;
      }

      // Mode switching
      document.querySelectorAll(".mode-tab").forEach((btn) => {
        btn.addEventListener("click", () => {
          document.querySelectorAll(".mode-tab").forEach((b) => {
            b.classList.remove("border-blue-600", "text-blue-600");
            b.classList.add("border-transparent", "text-gray-500");
          });
          btn.classList.remove("border-transparent", "text-gray-500");
          btn.classList.add("border-blue-600", "text-blue-600");

          const mode = btn.getAttribute("data-mode");
          document
            .getElementById("gravel-form-rect")
            .classList.toggle("hidden", mode !== "rect");
          document
            .getElementById("gravel-form-circle")
            .classList.toggle("hidden", mode !== "circle");
          document
            .getElementById("gravel-form-custom")
            .classList.toggle("hidden", mode !== "custom");

          document.getElementById("error-msg").classList.add("hidden");
        });
      });

      // Depth presets
      document.querySelectorAll(".depth-preset").forEach((btn) => {
        btn.addEventListener("click", () => {
          const depth = parseFloat(btn.getAttribute("data-depth"));
          const depthInput = document.getElementById("depth");
          const depthUnit = document.getElementById("depth-unit");
          depthUnit.value = "in";
          depthInput.value = depth;
        });
      });

      // Calculate & reset buttons
      document
        .getElementById("calculate-btn")
        .addEventListener("click", updateResults);

      document.getElementById("reset-btn").addEventListener("click", () => {
        [
          "length",
          "width",
          "depth",
          "radius",
          "depth-circle",
          "custom-volume",
        ].forEach((id) => {
          const el = document.getElementById(id);
          if (el) el.value = "";
        });
        document.getElementById("areas-count").value = 1;
        document.getElementById("areas-count-circle").value = 1;
        document.getElementById("density").value = 105;
        document.getElementById("density-unit").value = "lbft3";
        document.getElementById("bag-weight").value = 50;
        document.getElementById("bag-weight-unit").value = "lb";
        document.getElementById("error-msg").classList.add("hidden");
        updateResults();
      });

      // Auto-update on Enter key
      ["gravel-form-rect", "gravel-form-circle", "gravel-form-custom"].forEach(
        (formId) => {
          const form = document.getElementById(formId);
          form.addEventListener("submit", (e) => {
            e.preventDefault();
            updateResults();
          });
        }
      );

      // Initialize
      updateResults();
    </script>

