    <script>
      // Utility: fraction display for yards
      function formatYards(yards) {
        const rounded = Math.round(yards * 8) / 8; // nearest 1/8
        const whole = Math.floor(rounded);
        const frac = rounded - whole;
        const fracMap = {
          0: "",
          0.125: "1/8",
          0.25: "1/4",
          0.375: "3/8",
          0.5: "1/2",
          0.625: "5/8",
          0.75: "3/4",
          0.875: "7/8",
        };
        const key =
          Object.keys(fracMap).find(
            (k) => Math.abs(frac - parseFloat(k)) < 1e-6
          ) || "0";
        const fracStr = fracMap[key];
        if (whole === 0 && fracStr) return fracStr + " yd";
        if (!fracStr) return whole.toFixed(2) + " yd";
        return whole + " " + fracStr + " yd";
      }

      const unitSystemEl = document.getElementById("unit-system");
      const fabricWidthEl = document.getElementById("fabric-width");
      const fabricWidthCustomEl = document.getElementById(
        "fabric-width-custom"
      );
      const fabricWidthUnitEl = document.getElementById("fabric-width-unit");
      const extraPercentEl = document.getElementById("extra-percent");
      const roundingEl = document.getElementById("rounding");
      const layoutModeEl = document.getElementById("layout-mode");
      const piecesBodyEl = document.getElementById("pieces-body");
      const addPieceBtn = document.getElementById("add-piece");
      const clearPiecesBtn = document.getElementById("clear-pieces");
      const resultMainEl = document.getElementById("result-main");
      const resultAltEl = document.getElementById("result-alt");
      const resultNotesEl = document.getElementById("result-notes");
      const resultBreakdownEl = document.getElementById("result-breakdown");

      let pieces = [];
      let pieceIdCounter = 1;

      function getFabricWidth() {
        let val = fabricWidthEl.value;
        let width = 0;
        if (val === "custom") {
          width = parseFloat(fabricWidthCustomEl.value || "0");
        } else {
          width = parseFloat(val);
        }
        if (isNaN(width) || width <= 0) return 0;
        // Convert metric width to cm if metric, inches if imperial
        if (unitSystemEl.value === "metric") {
          // if user selected 110 or 140, assume cm already
          return width;
        } else {
          // imperial: if width > 100, assume cm and convert to inches; otherwise inches
          if (width > 100) return width / 2.54;
          return width;
        }
      }

      function renderPieces() {
        piecesBodyEl.innerHTML = "";
        if (pieces.length === 0) {
          resultBreakdownEl.innerHTML = "<li>No pieces added yet.</li>";
          return;
        }
        pieces.forEach((piece) => {
          const row = document.createElement("div");
          row.className =
            "grid grid-cols-12 gap-2 px-3 py-2 items-center text-xs";
          row.dataset.id = piece.id;

          row.innerHTML = `
        <div class="col-span-3">
          <input type="text" class="w-full border-gray-300 rounded-md shadow-sm focus:ring-blue-500 focus:border-blue-500 piece-name" value="${
            piece.name
          }">
        </div>
        <div class="col-span-2 text-right">
          <input type="number" min="0" step="0.1" class="w-full text-right border-gray-300 rounded-md shadow-sm focus:ring-blue-500 focus:border-blue-500 piece-length" value="${
            piece.length
          }">
        </div>
        <div class="col-span-2 text-right">
          <input type="number" min="0" step="0.1" class="w-full text-right border-gray-300 rounded-md shadow-sm focus:ring-blue-500 focus:border-blue-500 piece-width" value="${
            piece.width
          }">
        </div>
        <div class="col-span-2 text-right">
          <input type="number" min="1" step="1" class="w-full text-right border-gray-300 rounded-md shadow-sm focus:ring-blue-500 focus:border-blue-500 piece-qty" value="${
            piece.qty
          }">
        </div>
        <div class="col-span-2 text-right">
          <input type="checkbox" class="piece-fold" ${
            piece.onFold ? "checked" : ""
          }>
        </div>
        <div class="col-span-1 text-right">
          <button type="button" class="text-red-500 hover:text-red-700 piece-remove">&times;</button>
        </div>
      `;
          piecesBodyEl.appendChild(row);
        });
      }

      function recalc() {
        const fabricWidth = getFabricWidth();
        if (!fabricWidth || pieces.length === 0) {
          resultMainEl.textContent =
            unitSystemEl.value === "imperial" ? "0.00 yd" : "0.00 m";
          resultAltEl.textContent =
            unitSystemEl.value === "imperial" ? "0.00 m" : "0.00 yd";
          resultNotesEl.textContent =
            "Add pattern pieces and fabric width to see yardage.";
          return;
        }

        const extraPct = parseFloat(extraPercentEl.value || "0");
        const rounding = parseFloat(roundingEl.value || "0.125");
        const layoutMode = layoutModeEl.value;

        // Update pieces from DOM
        pieces.forEach((piece) => {
          const row = piecesBodyEl.querySelector(`div[data-id="${piece.id}"]`);
          if (!row) return;
          piece.name = row.querySelector(".piece-name").value || piece.name;
          piece.length = parseFloat(
            row.querySelector(".piece-length").value || "0"
          );
          piece.width = parseFloat(
            row.querySelector(".piece-width").value || "0"
          );
          piece.qty = parseInt(
            row.querySelector(".piece-qty").value || "1",
            10
          );
          piece.onFold = row.querySelector(".piece-fold").checked;
        });

        // Filter invalid pieces
        const validPieces = pieces.filter(
          (p) => p.length > 0 && p.width > 0 && p.qty > 0
        );
        if (validPieces.length === 0) {
          resultMainEl.textContent =
            unitSystemEl.value === "imperial" ? "0.00 yd" : "0.00 m";
          resultAltEl.textContent =
            unitSystemEl.value === "imperial" ? "0.00 m" : "0.00 yd";
          resultNotesEl.textContent =
            "Please enter positive dimensions for your pattern pieces.";
          resultBreakdownEl.innerHTML =
            "<li>No valid pieces to calculate.</li>";
          return;
        }

        // Approximate packing: sum area and divide by width, with simple efficiency factor
        let totalArea = 0;
        let breakdownHtml = "";
        validPieces.forEach((p) => {
          const effWidth = p.onFold ? p.width * 2 : p.width;
          const area = p.length * effWidth * p.qty;
          totalArea += area;
          breakdownHtml += `<li>${p.name || "Piece"}: ${
            p.qty
          } × ${p.length.toFixed(1)} × ${p.width.toFixed(1)} ${
            unitSystemEl.value === "imperial" ? "in" : "cm"
          }${p.onFold ? " (on fold)" : ""}</li>`;
        });

        // Layout efficiency factor
        let efficiency = 0.85; // base
        if (layoutMode === "single") efficiency = 0.8;
        if (layoutMode === "double") efficiency = 0.9;

        const effectiveWidth = fabricWidth * efficiency;
        let totalLength = totalArea / effectiveWidth; // in inches or cm

        // Add extra percentage
        if (!isNaN(extraPct) && extraPct > 0) {
          totalLength *= 1 + extraPct / 100;
        }

        let mainStr, altStr, notes;
        if (unitSystemEl.value === "imperial") {
          const yardsRaw = totalLength / 36;
          const yardsRounded = Math.ceil(yardsRaw / rounding) * rounding;
          mainStr = formatYards(yardsRounded);
          const meters = yardsRounded * 0.9144;
          altStr = meters.toFixed(2) + " m";
          notes = `Based on fabric width ≈ ${fabricWidth.toFixed(
            1
          )} in and layout efficiency of ${(efficiency * 100).toFixed(0)}%.`;
        } else {
          const metersRaw = totalLength / 100;
          const metersRounded = Math.ceil(metersRaw / rounding) * rounding;
          mainStr = metersRounded.toFixed(2) + " m";
          const yards = metersRounded / 0.9144;
          altStr = formatYards(yards);
          notes = `Based on fabric width ≈ ${fabricWidth.toFixed(
            0
          )} cm and layout efficiency of ${(efficiency * 100).toFixed(0)}%.`;
        }

        resultMainEl.textContent = mainStr;
        resultAltEl.textContent = altStr;
        resultNotesEl.textContent = notes;
        resultBreakdownEl.innerHTML = breakdownHtml;
      }

      // Event listeners
      unitSystemEl.addEventListener("change", () => {
        fabricWidthUnitEl.textContent =
          unitSystemEl.value === "imperial" ? "in" : "cm";
        recalc();
      });

      fabricWidthEl.addEventListener("change", () => {
        if (fabricWidthEl.value === "custom") {
          fabricWidthCustomEl.classList.remove("hidden");
          fabricWidthCustomEl.focus();
        } else {
          fabricWidthCustomEl.classList.add("hidden");
        }
        recalc();
      });

      fabricWidthCustomEl.addEventListener("input", recalc);
      extraPercentEl.addEventListener("input", recalc);
      roundingEl.addEventListener("change", recalc);
      layoutModeEl.addEventListener("change", recalc);

      addPieceBtn.addEventListener("click", () => {
        pieces.push({
          id: pieceIdCounter++,
          name: "Piece " + pieceIdCounter,
          length: 10,
          width: 10,
          qty: 1,
          onFold: false,
        });
        renderPieces();
        recalc();
      });

      clearPiecesBtn.addEventListener("click", () => {
        pieces = [];
        renderPieces();
        recalc();
      });

      piecesBodyEl.addEventListener("input", (e) => {
        if (
          e.target.classList.contains("piece-name") ||
          e.target.classList.contains("piece-length") ||
          e.target.classList.contains("piece-width") ||
          e.target.classList.contains("piece-qty")
        ) {
          recalc();
        }
      });

      piecesBodyEl.addEventListener("change", (e) => {
        if (e.target.classList.contains("piece-fold")) {
          recalc();
        }
      });

      piecesBodyEl.addEventListener("click", (e) => {
        if (e.target.classList.contains("piece-remove")) {
          const row = e.target.closest("div[data-id]");
          const id = parseInt(row.dataset.id, 10);
          pieces = pieces.filter((p) => p.id !== id);
          renderPieces();
          recalc();
        }
      });

      // Presets
      document.querySelectorAll("button[data-preset]").forEach((btn) => {
        btn.addEventListener("click", () => {
          const preset = btn.getAttribute("data-preset");
          pieces = [];
          if (preset === "skirt") {
            pieces.push(
              {
                id: pieceIdCounter++,
                name: "Skirt front",
                length: 24,
                width: 30,
                qty: 1,
                onFold: true,
              },
              {
                id: pieceIdCounter++,
                name: "Skirt back",
