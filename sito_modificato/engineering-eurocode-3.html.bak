    <script>
      // Tabs
      document.querySelectorAll(".ec3-tab").forEach((btn) => {
        btn.addEventListener("click", () => {
          const target = btn.getAttribute("data-tab");
          document.querySelectorAll(".ec3-tab").forEach((b) => {
            b.classList.remove("border-blue-600", "text-blue-600");
            b.classList.add("border-transparent", "text-gray-600");
          });
          btn.classList.add("border-blue-600", "text-blue-600");
          btn.classList.remove("border-transparent", "text-gray-600");

          document
            .querySelectorAll(".ec3-panel")
            .forEach((p) => p.classList.add("hidden"));
          document.getElementById(target).classList.remove("hidden");
        });
      });

      // Helper: clamp
      function clamp(val, min, max) {
        return Math.min(Math.max(val, min), max);
      }

      // 1) Section classification
      document.getElementById("btn-classify").addEventListener("click", () => {
        const fy = parseFloat(document.getElementById("cls-fy").value) || 355;
        const sigF =
          parseFloat(document.getElementById("cls-sig-f").value) || 250;
        const sigW =
          parseFloat(document.getElementById("cls-sig-w").value) || 250;
        const c = parseFloat(document.getElementById("cls-c").value) || 80;
        const tf = parseFloat(document.getElementById("cls-tf").value) || 15;
        const hw = parseFloat(document.getElementById("cls-hw").value) || 400;
        const tw = parseFloat(document.getElementById("cls-tw").value) || 8;

        const eps = Math.sqrt(235 / fy);

        // Simplified k-factors (approximate, for internal compression parts)
        function limitsFlange() {
          const k1 = 9 * (235 / fy); // Class 1
          const k2 = 10 * (235 / fy); // Class 2
          const k3 = 14 * (235 / fy); // Class 3
          return [k1 * eps, k2 * eps, k3 * eps];
        }
        function limitsWeb() {
          const k1 = 33 * (235 / fy);
          const k2 = 38 * (235 / fy);
          const k3 = 42 * (235 / fy);
          return [k1 * eps, k2 * eps, k3 * eps];
        }

        const lamF = c / tf;
        const lamW = hw / tw;
        const [lf1, lf2, lf3] = limitsFlange();
        const [lw1, lw2, lw3] = limitsWeb();

        function classify(lam, l1, l2, l3) {
          if (lam <= l1) return 1;
          if (lam <= l2) return 2;
          if (lam <= l3) return 3;
          return 4;
        }

        const classF = classify(lamF, lf1, lf2, lf3);
        const classW = classify(lamW, lw1, lw2, lw3);
        const governing = Math.max(classF, classW);

        document.getElementById("cls-lam-f").textContent = lamF.toFixed(1);
        document.getElementById("cls-lam-w").textContent = lamW.toFixed(1);
        document.getElementById("cls-lim1-f").textContent = lf1.toFixed(1);
        document.getElementById("cls-lim2-f").textContent = lf2.toFixed(1);
        document.getElementById("cls-lim3-f").textContent = lf3.toFixed(1);
        document.getElementById("cls-lim1-w").textContent = lw1.toFixed(1);
        document.getElementById("cls-lim2-w").textContent = lw2.toFixed(1);
        document.getElementById("cls-lim3-w").textContent = lw3.toFixed(1);
        document.getElementById("cls-class-f").textContent = "Class " + classF;
        document.getElementById("cls-class-w").textContent = "Class " + classW;
        document.getElementById("cls-governing").textContent =
          "Class " + governing;

        document.getElementById("cls-results").classList.remove("hidden");
      });

      // 2) Bending resistance
      document.getElementById("btn-bending").addEventListener("click", () => {
        const fy = parseFloat(document.getElementById("bend-fy").value) || 355;
        const gamma =
          parseFloat(document.getElementById("bend-gamma").value) || 1.0;
        const cls = parseInt(document.getElementById("bend-class").value, 10);
        const Wpl_cm3 =
          parseFloat(document.getElementById("bend-wpl").value) || 1000;
        const Wel_cm3 =
          parseFloat(document.getElementById("bend-wel").value) || 900;
        const MEd = parseFloat(document.getElementById("bend-med").value) || 0;

        // Convert cm³ to m³: 1 cm³ = 1e-6 m³
        const Wpl = Wpl_cm3 * 1e-6;
        const Wel = Wel_cm3 * 1e-6;

        const Mpl_Rd = (Wpl * fy) / gamma / 1e6; // Nmm to kNm: divide by 1e6
        const Mel_Rd = (Wel * fy) / gamma / 1e6;

        let MRd;
        if (cls === 1 || cls === 2) {
          MRd = Mpl_Rd;
        } else if (cls === 3) {
          MRd = Mel_Rd;
        } else {
          MRd = NaN;
        }

        let util = NaN;
        let status =
          "Class 4 not supported – use effective section properties.";
        if (!isNaN(MRd) && MRd > 0) {
          util = MEd / MRd;
          if (util <= 1.0) {
            status = "OK (within resistance)";
          } else {
            status = "NOT OK (exceeds resistance)";
          }
        }

        document.getElementById("bend-mpl").textContent = isNaN(Mpl_Rd)
          ? "-"
          : Mpl_Rd.toFixed(1);
        document.getElementById("bend-mel").textContent = isNaN(Mel_Rd)
          ? "-"
          : Mel_Rd.toFixed(1);
        document.getElementById("bend-mrd").textContent = isNaN(MRd)
          ? "-"
          : MRd.toFixed(1);
        document.getElementById("bend-util").textContent = isNaN(util)
          ? "-"
          : util.toFixed(2);
        document.getElementById("bend-status").textContent = status;

        document.getElementById("bend-results").classList.remove("hidden");
      });

      // 3) Axial & buckling
      document.getElementById("btn-axial").addEventListener("click", () => {
        const fy = parseFloat(document.getElementById("ax-fy").value) || 355;
        const gamma =
          parseFloat(document.getElementById("ax-gamma").value) || 1.0;
        const alpha =
          parseFloat(document.getElementById("ax-alpha").value) || 0.21;
        const A_cm2 =
          parseFloat(document.getElementById("ax-area").value) || 80;
        const i_cm = parseFloat(document.getElementById("ax-i").value) || 8.0;
        const Lcr_m =
          parseFloat(document.getElementById("ax-lcr").value) || 3.0;
        const NEd = parseFloat(document.getElementById("ax-ned").value) || 0;

        const E = 210000; // MPa

        // Convert units: A cm² -> m², i cm -> m
        const A = A_cm2 * 1e-4;
        const i = i_cm * 1e-2;
        const Lcr = Lcr_m;

        const Npl_Rd = (A * fy) / gamma / 1e3; // N to kN

        const lambdaBar = (Lcr / i) * Math.sqrt(fy / (Math.PI * Math.PI * E));

        const phi =
          0.5 * (1 + alpha * (lambdaBar - 0.2) + lambdaBar * lambdaBar);
        const sqrtTerm = Math.max(phi * phi - lambdaBar * lambdaBar, 0);
        let chi = 1.0;
        if (lambdaBar > 0) {
          chi = 1 / (phi + Math.sqrt(sqrtTerm));
          chi = clamp(chi, 0, 1);
        }

        const Nb_Rd = chi * Npl_Rd;
        const util = Nb_Rd > 0 ? NEd / Nb_Rd : NaN;
        const status =
          util <= 1 ? "OK (within resistance)" : "NOT OK (exceeds resistance)";

        document.getElementById("ax-lambda").textContent = lambdaBar.toFixed(3);
        document.getElementById("ax-chi").textContent = chi.toFixed(3);
        document.getElementById("ax-npl").textContent = Npl_Rd.toFixed(1);
        document.getElementById("ax-nbrd").textContent = Nb_Rd.toFixed(1);
        document.getElementById("ax-util").textContent = isNaN(util)
          ? "-"
          : util.toFixed(2);
        document.getElementById("ax-status").textContent = status;

        document.getElementById("ax-results").classList.remove("hidden");
      });

      // Simple FAQ toggle (in addition to global script_faq)
      document
        .querySelectorAll("#faq-container .faq-question")
        .forEach((btn) => {
          btn.addEventListener("click", () => {
            const ans = btn.parentElement.querySelector(".faq-answer");
            const icon = btn.querySelector("span:last-child");
            const isHidden = ans.classList.contains("hidden");
            document
              .querySelectorAll("#faq-container .faq-answer")
              .forEach((a) => a.classList.add("hidden"));
            document
              .querySelectorAll("#faq-container .faq-question span:last-child")
              .forEach((i) => (i.textContent = "+"));
            if (isHidden) {
              ans.classList.remove("hidden");
              icon.textContent = "−";
            }
          });
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
