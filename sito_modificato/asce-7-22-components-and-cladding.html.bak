    <script>
      // Simple Kz approximation based on exposure and height (low-rise, educational only)
      function getKz(exposure, z) {
        z = Math.max(15, Math.min(z, 500));
        if (exposure === "B") {
          if (z <= 30) return 0.7;
          if (z <= 60) return 0.84;
          if (z <= 120) return 0.96;
          if (z <= 200) return 1.03;
          return 1.07;
        } else if (exposure === "C") {
          if (z <= 15) return 0.85;
          if (z <= 30) return 1.0;
          if (z <= 60) return 1.14;
          if (z <= 120) return 1.26;
          if (z <= 200) return 1.32;
          return 1.4;
        } else {
          // D
          if (z <= 15) return 1.03;
          if (z <= 30) return 1.12;
          if (z <= 60) return 1.2;
          if (z <= 120) return 1.28;
          if (z <= 200) return 1.33;
          return 1.4;
        }
      }

      // Approximate GCp values for common C&C zones (educational only)
      function getApproxGCp(surfaceType, zone, At) {
        const small = At <= 10; // small components
        const med = At > 10 && At <= 100;
        const large = At > 100;

        // Base values (roughly representative, not exact ASCE 7-22)
        if (surfaceType === "wall") {
          if (zone === "wall_zone4") {
            if (small) return { pos: 0.8, neg: -1.1 };
            if (med) return { pos: 0.7, neg: -1.0 };
            return { pos: 0.6, neg: -0.9 };
          } else {
            // zone 5 corners
            if (small) return { pos: 1.0, neg: -1.5 };
            if (med) return { pos: 0.9, neg: -1.3 };
            return { pos: 0.8, neg: -1.1 };
          }
        }

        if (surfaceType === "roof_low_slope") {
          if (zone === "roof_zone1") {
            if (small) return { pos: 0.9, neg: -1.3 };
            if (med) return { pos: 0.8, neg: -1.1 };
            return { pos: 0.7, neg: -0.9 };
          } else if (zone === "roof_zone2") {
            if (small) return { pos: 1.1, neg: -2.0 };
            if (med) return { pos: 1.0, neg: -1.7 };
            return { pos: 0.9, neg: -1.5 };
          } else {
            // zone 3 corners
            if (small) return { pos: 1.3, neg: -3.0 };
            if (med) return { pos: 1.2, neg: -2.5 };
            return { pos: 1.1, neg: -2.0 };
          }
        }

        if (surfaceType === "roof_steep") {
          if (zone === "roof_zone1") {
            if (small) return { pos: 0.9, neg: -1.5 };
            if (med) return { pos: 0.8, neg: -1.3 };
            return { pos: 0.7, neg: -1.1 };
          } else if (zone === "roof_zone2") {
            if (small) return { pos: 1.1, neg: -2.2 };
            if (med) return { pos: 1.0, neg: -1.9 };
            return { pos: 0.9, neg: -1.6 };
          } else {
            // zone 3 corners
            if (small) return { pos: 1.4, neg: -3.3 };
            if (med) return { pos: 1.3, neg: -2.8 };
            return { pos: 1.2, neg: -2.3 };
          }
        }

        // Fallback
        return { pos: 0.8, neg: -1.0 };
      }

      function formatPsf(value) {
        if (!isFinite(value)) return "-";
        return value.toFixed(1) + " psf";
      }

      document.getElementById("calcBtn").addEventListener("click", function () {
        const V = parseFloat(document.getElementById("windSpeed").value) || 0;
        const exposure = document.getElementById("exposure").value;
        const z = parseFloat(document.getElementById("height").value) || 0;
        const Kzt = parseFloat(document.getElementById("kzt").value) || 1.0;
        const Kd = parseFloat(document.getElementById("kd").value) || 0.85;
        const G =
          parseFloat(document.getElementById("gustEffect").value) || 0.85;
        const CpiAbs = Math.abs(
          parseFloat(document.getElementById("cpi").value) || 0.18
        );
        const surfaceType = document.getElementById("surfaceType").value;
        const zone = document.getElementById("zone").value;
        const At =
          parseFloat(document.getElementById("tributaryArea").value) || 10;

        if (V <= 0 || z <= 0) {
          alert("Please enter positive values for wind speed and height.");
          return;
        }

        const Kz = getKz(exposure, z);
        const qz = 0.00256 * Kz * Kzt * Kd * V * V;
        const qh = qz; // for low-rise, using same height as mean roof

        const gcp = getApproxGCp(surfaceType, zone, At);

        // External pressures (positive and negative)
        const pextPos = qh * gcp.pos;
        const pextNeg = qh * gcp.neg;

        // Internal pressures (±Cpi)
        const pintPos = qh * G * CpiAbs;
        const pintNeg = -qh * G * CpiAbs;

        // Net pressures for four combinations; pick worst magnitude
        const combos = [
          pextPos - pintPos,
          pextPos - pintNeg,
          pextNeg - pintPos,
          pextNeg - pintNeg,
        ];
        let pnetWorst = combos[0];
        combos.forEach((v) => {
          if (Math.abs(v) > Math.abs(pnetWorst)) pnetWorst = v;
        });

        // Update UI
        document.getElementById("kzOut").textContent = Kz.toFixed(3);
        document.getElementById("qzOut").textContent = formatPsf(qz);
        document.getElementById("qhOut").textContent = formatPsf(qh);

        document.getElementById("gcpOut").textContent =
          "GCp+ = " + gcp.pos.toFixed(2) + ", GCp− = " + gcp.neg.toFixed(2);

        document.getElementById("pextOut").textContent =
          "Max: " +
          formatPsf(Math.max(pextPos, pextNeg)) +
          ", Min: " +
          formatPsf(Math.min(pextPos, pextNeg));

        document.getElementById("pintOut").textContent =
          "±" + formatPsf(Math.abs(pintPos));

        document.getElementById("pnetOut").textContent = formatPsf(pnetWorst);
      });

      // Auto-update Cpi when building type changes (helper)
      document
        .getElementById("buildingType")
        .addEventListener("change", function () {
          const type = this.value;
          const cpiSelect = document.getElementById("cpi");
          if (type === "enclosed") cpiSelect.value = "0.18";
          else if (type === "partially_enclosed") cpiSelect.value = "0.55";
          else cpiSelect.value = "0.0";
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
