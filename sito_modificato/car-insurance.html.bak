        function calculateInsurance() {
            const carValue = parseFloat(document.getElementById('carValue').value) || 0;
            const driverAge = parseInt(document.getElementById('driverAge').value) || 0;
            const location = document.getElementById('location').value;

            let baseRate = 500;
            let rateFactor = 0.05;
            let ageFactor = 20;
            let locationFactor = 100; // Simplified

            // Simplified insurance calculation
            const insuranceCost = baseRate + (carValue * rateFactor) + (ageFactor * driverAge) + locationFactor;

            document.getElementById('insuranceCost').textContent = `$${insuranceCost.toFixed(2)}`;
        }
    </script>


