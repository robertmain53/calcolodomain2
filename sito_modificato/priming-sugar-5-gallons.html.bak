    <script>
      // Utility: convert F to C
      function fToC(f) {
        return ((f - 32) * 5) / 9;
      }

      // Residual CO2 (volumes) as function of temp in F (empirical fit)
      function residualCO2FromTempF(Tf) {
        return 3.0378 - 0.050062 * Tf + 0.00026555 * Tf * Tf;
      }

      // Get beer volume in liters from input
      function getVolumeLiters() {
        const vol = parseFloat(document.getElementById("volume").value) || 0;
        const unit = document.getElementById("volume-unit").value;
        if (unit === "gal") {
          return vol * 3.78541;
        }
        return vol; // already liters
      }

      // Style preset handling
      const stylePresetEl = document.getElementById("style-preset");
      const targetCo2El = document.getElementById("target-co2");
      const targetCo2DisplayEl = document.getElementById("target-co2-display");
      const styleRangeEl = document.getElementById("style-range");

      function updateTargetDisplay() {
        targetCo2DisplayEl.textContent = parseFloat(targetCo2El.value).toFixed(
          2
        );
      }

      stylePresetEl.addEventListener("change", () => {
        const val = stylePresetEl.value;
        const selected = stylePresetEl.options[stylePresetEl.selectedIndex];
        const range = selected.getAttribute("data-range");
        if (val !== "custom" && val) {
          targetCo2El.value = val;
          updateTargetDisplay();
          styleRangeEl.textContent =
            "Preset range for this style: " +
            (range || "") +
            " volumes of CO₂.";
        } else {
          styleRangeEl.textContent =
            "Choose a style to auto-fill a reasonable carbonation level, or leave as custom.";
        }
      });

      targetCo2El.addEventListener("input", () => {
        updateTargetDisplay();
        // When user manually adjusts, set preset to custom
        stylePresetEl.value = "custom";
        styleRangeEl.textContent = "Custom carbonation level selected.";
      });

      updateTargetDisplay();

      // Main calculation
      document.getElementById("calc-btn").addEventListener("click", () => {
        const volL = getVolumeLiters();
        const tempVal = parseFloat(document.getElementById("beer-temp").value);
        const tempUnit = document.getElementById("temp-unit").value;
        const sugarType = document.getElementById("sugar-type").value;
        const targetVolumes = parseFloat(targetCo2El.value);

        if (!volL || volL <= 0 || !tempVal || !targetVolumes) {
          alert(
            "Please enter valid volume, temperature, and target carbonation."
          );
          return;
        }

        // Convert temperature to F for residual CO2 formula
        const tempF = tempUnit === "C" ? (tempVal * 9) / 5 + 32 : tempVal;
        const residual = residualCO2FromTempF(tempF);
        let deltaV = targetVolumes - residual;
        if (deltaV < 0) deltaV = 0;

        // Base: corn sugar (dextrose) grams
        // grams = 15.195 * volume(L) * deltaV  (approximation)
        let gramsCorn = 15.195 * volL * deltaV;
        if (gramsCorn < 0) gramsCorn = 0;

        let grams;
        let sugarLabel;
        let density; // g per cup, approximate

        switch (sugarType) {
          case "table":
            // Table sugar is slightly more fermentable and denser: need ~5% less by weight
            grams = gramsCorn * 0.95;
            sugarLabel = "table sugar (sucrose)";
            density = 200; // g per cup (approx)
            break;
          case "dme":
            // DME is less fermentable: need ~25% more by weight
            grams = gramsCorn * 1.25;
            sugarLabel = "DME (dry malt extract)";
            density = 120; // g per cup (approx)
            break;
          case "corn":
          default:
            grams = gramsCorn;
            sugarLabel = "corn sugar (dextrose)";
            density = 190; // g per cup (approx)
            break;
        }

        const ounces = grams / 28.3495;
        const cups = grams / density;

        // Update UI
        document.getElementById("res-grams").textContent = grams.toFixed(1);
        document.getElementById("res-ounces").textContent = ounces.toFixed(2);
        document.getElementById("res-cups").textContent = cups.toFixed(2);

        const volDisplay =
          document.getElementById("volume").value +
          " " +
          (document.getElementById("volume-unit").value === "gal"
            ? "US gal"
            : "L");

        const summary =
          `For ${volDisplay} of beer at about ${tempVal.toFixed(
            1
          )} °${tempUnit}, ` +
          `to reach approximately ${targetVolumes.toFixed(
            2
          )} volumes of CO₂, ` +
          `use about ${grams.toFixed(1)} g (${ounces.toFixed(
            2
          )} oz, ~${cups.toFixed(2)} cups) of ${sugarLabel}.`;

        document.getElementById("res-summary").textContent = summary;

        const warningEl = document.getElementById("res-warning");
        if (targetVolumes > 3.0) {
          warningEl.textContent =
            "High carbonation level selected. Use only bottles rated for high pressure (e.g., champagne bottles) and ensure fermentation is fully complete to avoid bottle bombs.";
        } else {
          warningEl.textContent = "";
        }

        document.getElementById("results-content").classList.remove("hidden");
      });

      // Temperature unit change helper: keep numeric value but change meaning? Safer to convert.
      document.getElementById("temp-unit").addEventListener("change", (e) => {
        const unit = e.target.value;
        const input = document.getElementById("beer-temp");
        let val = parseFloat(input.value);
        if (!isNaN(val)) {
          if (unit === "C") {
            // convert F -> C
            input.value = fToC(val).toFixed(1);
          } else {
            // convert C -> F
            input.value = ((val * 9) / 5 + 32).toFixed(1);
          }
        }
      });
    </script>

