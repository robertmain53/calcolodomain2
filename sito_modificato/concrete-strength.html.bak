    <script>
      // Tab switching
      const tabs = {
        "tab-mix": "panel-mix",
        "tab-maturity": "panel-maturity",
        "tab-class": "panel-class",
      };
      Object.keys(tabs).forEach((tabId) => {
        document.getElementById(tabId).addEventListener("click", () => {
          // Toggle tab button styles
          Object.keys(tabs).forEach((id) => {
            const btn = document.getElementById(id);
            if (id === tabId) {
              btn.classList.add("tab-active");
            } else {
              btn.classList.remove("tab-active");
            }
          });
          // Toggle panels
          Object.values(tabs).forEach((panelId) => {
            document.getElementById(panelId).classList.add("hidden");
          });
          document.getElementById(tabs[tabId]).classList.remove("hidden");
        });
      });

      // Helper: MPa to psi
      function mpaToPsi(mpa) {
        return mpa * 145.0377;
      }

      function ratingFromMpa(mpa) {
        if (mpa < 15) return "Very low";
        if (mpa < 25) return "Low / residential";
        if (mpa < 35) return "Normal structural";
        if (mpa < 50) return "High strength";
        return "Very high / special";
      }

      // Mix design calculator
      document.getElementById("btn-calc-wc").addEventListener("click", () => {
        const wc = parseFloat(document.getElementById("wc-ratio").value);
        const f28ref = parseFloat(document.getElementById("wc-f28").value);
        const age = parseFloat(document.getElementById("wc-age").value);
        const beta = parseFloat(document.getElementById("wc-beta").value);

        if (
          isNaN(wc) ||
          isNaN(f28ref) ||
          isNaN(age) ||
          isNaN(beta) ||
          wc <= 0 ||
          f28ref <= 0 ||
          age <= 0 ||
          beta <= 0
        ) {
          alert("Please enter valid positive numbers for all fields.");
          return;
        }

        const wcRef = 0.5;
        const k = 5.0; // empirical constant
        const f28 = f28ref * Math.exp(k * (wcRef - wc));
        const fAge = f28 * (age / (age + beta));

        const mpa = fAge;
        const psi = mpaToPsi(mpa);

        document.getElementById("wc-result-mpa").textContent = mpa.toFixed(1);
        document.getElementById("wc-result-psi").textContent = psi.toFixed(0);
        document.getElementById("wc-result-rating").textContent =
          ratingFromMpa(mpa);
        document.getElementById("wc-result-note").textContent =
          "Approximate compressive strength at " +
          age.toFixed(1) +
          " days for w/c = " +
          wc.toFixed(2) +
          ".";
      });

      // Maturity calculator
      document.getElementById("btn-calc-mat").addEventListener("click", () => {
        const f28 = parseFloat(document.getElementById("mat-f28").value);
        const age = parseFloat(document.getElementById("mat-age").value);
        const temp = parseFloat(document.getElementById("mat-temp").value);
        const n = parseFloat(document.getElementById("mat-alpha").value);

        if (
          isNaN(f28) ||
          isNaN(age) ||
          isNaN(temp) ||
          isNaN(n) ||
          f28 <= 0 ||
          age <= 0 ||
          n <= 0
        ) {
          alert("Please enter valid positive numbers for all fields.");
          return;
        }

        const Tref = 20.0;
        const tEff = age * Math.exp(0.06 * (temp - Tref));
        const f = f28 * (tEff / (tEff + n));
        const percent = (f / f28) * 100;

        const mpa = f;
        const psi = mpaToPsi(mpa);

        document.getElementById("mat-result-mpa").textContent = mpa.toFixed(1);
        document.getElementById("mat-result-psi").textContent = psi.toFixed(0);
        document.getElementById("mat-result-percent").textContent =
          percent.toFixed(0) + " %";
      });

      // Strength class lookup
      const classData = {
        "C16/20": {
          cyl: 16,
          cube: 20,
          usage: "Lightly loaded slabs, paths, blinding",
        },
        "C20/25": {
          cyl: 20,
          cube: 25,
          usage: "Residential slabs, foundations",
        },
        "C25/30": { cyl: 25, cube: 30, usage: "General structural concrete" },
        "C30/37": {
          cyl: 30,
          cube: 37,
          usage: "Heavier duty slabs, beams, columns",
        },
        "C35/45": {
          cyl: 35,
          cube: 45,
          usage: "Bridges, parking decks, aggressive exposure",
        },
        "C40/50": {
          cyl: 40,
          cube: 50,
          usage: "High‑rise columns, heavily loaded members",
        },
        "C50/60": {
          cyl: 50,
          cube: 60,
          usage: "Very high strength / special structures",
        },
      };

      document
        .getElementById("btn-calc-class")
        .addEventListener("click", () => {
          const cls = document.getElementById("class-select").value;
          if (!cls || !classData[cls]) {
            alert("Please select a concrete strength class.");
            return;
          }
          const data = classData[cls];
          document.getElementById("class-cylinder-mpa").textContent =
            data.cyl.toFixed(0);
          document.getElementById("class-cube-mpa").textContent =
            data.cube.toFixed(0);
          document.getElementById("class-cylinder-psi").textContent = mpaToPsi(
            data.cyl
          ).toFixed(0);
          document.getElementById("class-usage").textContent = data.usage;
          document.getElementById("class-note").textContent =
            cls +
            " corresponds to a characteristic cylinder strength of " +
            data.cyl.toFixed(0) +
            " MPa (≈ " +
            mpaToPsi(data.cyl).toFixed(0) +
            " psi) and cube strength of " +
            data.cube.toFixed(0) +
            " MPa.";
        });

      // Mobile menu toggle (from template)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const expanded = this.getAttribute("aria-expanded") === "true";
          this.setAttribute("aria-expanded", String(!expanded));
          menu.classList.toggle("hidden");
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
