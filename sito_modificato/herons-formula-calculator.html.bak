            <script>
              document.addEventListener('DOMContentLoaded', function() {
                const sideA = document.getElementById('side-a');
                const sideB = document.getElementById('side-b');
                const sideC = document.getElementById('side-c');
                const calculateBtn = document.getElementById('calculate-btn');
                const resultsContainer = document.getElementById('results-container');
                const errorMessage = document.getElementById('error-message');
                
                const resultPerimeter = document.getElementById('result-perimeter');
                const resultSemiPerimeter = document.getElementById('result-semi-perimeter');
                const resultArea = document.getElementById('result-area');
                const stepByStep = document.getElementById('step-by-step');

                // --- Helper Functions ---
                function parseInput(val) {
                    const num = parseFloat(val);
                    return (num > 0 && !isNaN(num)) ? num : NaN;
                }
                
                function formatNumber(num) {
                    if (Math.abs(num) < 1e-10) return '0';
                    return parseFloat(num.toFixed(6)).toString();
                }

                function showError(message) {
                    errorMessage.textContent = `Error: ${message}`;
                    resultsContainer.classList.add('hidden');
                }

                // --- Main Calculation Logic ---
                calculateBtn.addEventListener('click', () => {
                    const a = parseInput(sideA.value);
                    const b = parseInput(sideB.value);
                    const c = parseInput(sideC.value);
                    
                    try {
                        if (isNaN(a) || isNaN(b) || isNaN(c)) {
                            throw new Error('Please enter three valid, positive side lengths.');
                        }

                        // 1. Triangle Inequality Check
                        if (a + b <= c || a + c <= b || b + c <= a) {
                            throw new Error('Invalid Triangle: The sum of any two sides must be greater than the third side ($a+b>c$, etc.).');
                        }

                        // 2. Calculate Perimeter and Semi-Perimeter
                        const P = a + b + c;
                        const s = P / 2;
                        
                        // 3. Calculate the terms inside the square root
                        const termA = s - a;
                        const termB = s - b;
                        const termC = s - c;
                        
                        const radicand = s * termA * termB * termC;
                        
                        if (radicand < 0) {
                             // This should be caught by inequality check, but for robustness:
                            throw new Error('Area cannot be calculated (Radicand is negative). Check side lengths.');
                        }
                        
                        // 4. Calculate Area
                        const Area = Math.sqrt(radicand);
                        
                        // --- Output Construction ---
                        const fA = formatNumber(a), fB = formatNumber(b), fC = formatNumber(c);
                        const fP = formatNumber(P), fS = formatNumber(s);
                        const fTermA = formatNumber(termA), fTermB = formatNumber(termB), fTermC = formatNumber(termC);
                        const fRadicand = formatNumber(radicand), fArea = formatNumber(Area);

                        // A. Semi-Perimeter Step
                        let steps = `
                            <h4 class="font-semibold text-lg">1. Calculate Semi-Perimeter ($s$):</h4>
                            <p>Formula: $s = \\frac{a + b + c}{2}$</p>
                            <p class="formula-box text-center">$s = \\frac{${fA} + ${fB} + ${fC}}{2} = \\frac{${fP}}{2} = ${fS}$</p>
                            <p>$s = ${fS}$</p>
                        `;

                        // B. Area Step
                         steps += `
                            <h4 class="font-semibold text-lg mt-4">2. Calculate Area ($\\mathcal{A}$):</h4>
                            <p>Formula: $\\mathcal{A} = \\sqrt{s(s-a)(s-b)(s-c)}$</p>
                            <p>Calculate internal terms: $(s-a) = ${fS} - ${fA} = ${fTermA}$; $(s-b) = ${fS} - ${fB} = ${fTermB}$; $(s-c) = ${fS} - ${fC} = ${fTermC}$</p>
                            <p>Substitution:</p>
                            <p class="formula-box text-center">$\\mathcal{A} = \\sqrt{${fS} \\times ${fTermA} \\times ${fTermB} \\times ${fTermC}} = \\sqrt{${fRadicand}}$</p>
                            <p>Result: $\\mathcal{A} \\approx ${fArea}$</p>
                        `;

                        // 5. Update DOM
                        resultPerimeter.textContent = fP;
                        resultSemiPerimeter.textContent = fS;
                        resultArea.textContent = fArea;
                        stepByStep.innerHTML = steps;
                        resultsContainer.classList.remove('hidden');

                        // Re-render MathJax
                        if (window.MathJax) {
                            window.MathJax.typesetPromise([stepByStep, resultSemiPerimeter, resultArea]);
                        }

                    } catch (e) {
                        showError(e.message);
                    }
                });

                // Initial calculation (for example values)
                calculateBtn.click();
            </script>

