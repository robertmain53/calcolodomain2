    <script>
      // Alloy presets
      const ec9AlloySelect = document.getElementById("ec9-alloy");
      const fyInput = document.getElementById("ec9-fy");
      const fuInput = document.getElementById("ec9-fu");

      ec9AlloySelect.addEventListener("change", () => {
        const val = ec9AlloySelect.value;
        if (val === "6060T6") {
          fyInput.value = 190;
          fuInput.value = 215;
          fyInput.readOnly = false;
          fuInput.readOnly = false;
        } else if (val === "6082T6") {
          fyInput.value = 260;
          fuInput.value = 300;
          fyInput.readOnly = false;
          fuInput.readOnly = false;
        } else {
          fyInput.readOnly = false;
          fuInput.readOnly = false;
        }
      });

      function toNumber(id) {
        const v = parseFloat(
          document.getElementById(id).value.replace(",", ".")
        );
        return isNaN(v) ? 0 : v;
      }

      function formatNumber(val, decimals) {
        if (!isFinite(val)) return "–";
        return val.toFixed(decimals);
      }

      document.getElementById("ec9-calc-btn").addEventListener("click", () => {
        const gammaM1 = toNumber("ec9-gamma-m1");
        const gammaM2 = toNumber("ec9-gamma-m2");
        const fy = toNumber("ec9-fy"); // MPa
        const fu = toNumber("ec9-fu"); // MPa (not used directly but kept for extension)

        const A_cm2 = toNumber("ec9-A");
        const Wy_cm3 = toNumber("ec9-Wy");
        const Aw_cm2 = toNumber("ec9-Aw");

        const MEd = toNumber("ec9-MEd"); // kNm
        const NEd = toNumber("ec9-NEd"); // kN
        const VEd = toNumber("ec9-VEd"); // kN
        const chi = toNumber("ec9-chi") || 1.0;

        // Unit conversions
        const A = A_cm2 * 1e-4; // cm² -> m²
        const Wy = Wy_cm3 * 1e-6; // cm³ -> m³
        const Aw = Aw_cm2 * 1e-4; // cm² -> m²

        // Design strengths (MPa)
        const f0_2d = fy / gammaM1;
        const fv_d = fy / (Math.sqrt(3) * gammaM2);

        // Resistances
        // M_Rd in kNm: f0_2d [MPa = N/mm²] * Wy [m³] * 1e6 / 1e3
        const MRd = f0_2d * Wy * 1e3;

        // N_Rd in kN: chi * f0_2d [MPa] * A [m²] * 1e6 / 1e3
        const NRd = chi * f0_2d * A * 1e3;

        // V_Rd in kN: fv_d [MPa] * Aw [m²] * 1e6 / 1e3
        const VRd = fv_d * Aw * 1e3;

        const etaM = MRd > 0 ? MEd / MRd : NaN;
        const etaN = NRd > 0 ? NEd / NRd : NaN;
        const etaV = VRd > 0 ? VEd / VRd : NaN;

        document.getElementById("ec9-MRd").textContent =
          MRd > 0 ? formatNumber(MRd, 2) : "–";
        document.getElementById("ec9-NRd").textContent =
          NRd > 0 ? formatNumber(NRd, 1) : "–";
        document.getElementById("ec9-VRd").textContent =
          VRd > 0 ? formatNumber(VRd, 1) : "–";

        document.getElementById("ec9-etaM").textContent = isFinite(etaM)
          ? formatNumber(etaM, 2)
          : "–";
        document.getElementById("ec9-etaN").textContent = isFinite(etaN)
          ? formatNumber(etaN, 2)
          : "–";
        document.getElementById("ec9-etaV").textContent = isFinite(etaV)
          ? formatNumber(etaV, 2)
          : "–";

        const statusEl = document.getElementById("ec9-status");
        const maxEta = Math.max(
          isFinite(etaM) ? etaM : 0,
          isFinite(etaN) ? etaN : 0,
          isFinite(etaV) ? etaV : 0
        );

        statusEl.classList.remove(
          "hidden",
          "bg-green-50",
          "border-green-400",
          "text-green-700",
          "bg-red-50",
          "border-red-400",
          "text-red-700"
        );
        if (maxEta <= 1.0) {
          statusEl.classList.add(
            "bg-green-50",
            "border",
            "border-green-400",
            "text-green-700"
          );
          statusEl.innerHTML = `<strong>OK:</strong> Maximum utilisation η = ${formatNumber(
            maxEta,
            2
          )} ≤ 1.0. Member passes this simplified EC9 check.`;
        } else {
          statusEl.classList.add(
            "bg-red-50",
            "border",
            "border-red-400",
            "text-red-700"
          );
          statusEl.innerHTML = `<strong>Not OK:</strong> Maximum utilisation η = ${formatNumber(
            maxEta,
            2
          )} &gt; 1.0. Increase section size, reduce span or loads, or refine the design.`;
        }
      });

      // Mobile menu toggle
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", () => {
          const menu = document.getElementById("mobile-menu");
          const expanded = menu.classList.contains("hidden") ? "true" : "false";
          menu.classList.toggle("hidden");
          document
            .getElementById("mobile-menu-toggle")
            .setAttribute("aria-expanded", expanded);
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
