    <script>
      // Accordion behavior
      document.querySelectorAll(".accordion-header").forEach((btn) => {
        btn.addEventListener("click", () => {
          const body = btn.nextElementSibling;
          body.classList.toggle("hidden");
        });
      });

      // FAQ simple toggle (in addition to global script_faq)
      document.querySelectorAll("#faq .faq-question").forEach((btn) => {
        btn.addEventListener("click", () => {
          const ans = btn.nextElementSibling;
          ans.classList.toggle("hidden");
        });
      });

      function parseNum(id) {
        const v = parseFloat(document.getElementById(id).value);
        return isNaN(v) ? null : v;
      }

      function formatPercent(v) {
        if (v === null || !isFinite(v)) return "–";
        return (v * 100).toFixed(1) + "%";
      }

      function formatRatio(v, decimals = 2) {
        if (v === null || !isFinite(v)) return "–";
        return v.toFixed(decimals);
      }

      function classifyNetMargin(m) {
        if (m == null || !isFinite(m)) return "";
        if (m < 0.02) return "Weak margin";
        if (m < 0.1) return "OK margin";
        return "Strong margin";
      }

      function classifyROE(r) {
        if (r == null || !isFinite(r)) return "";
        if (r < 0.08) return "Low ROE";
        if (r < 0.18) return "Healthy ROE";
        return "Very strong ROE";
      }

      function classifyCurrentRatio(r) {
        if (r == null || !isFinite(r)) return "";
        if (r < 1) return "Below 1.0 – potential liquidity risk";
        if (r <= 2) return "Comfortable liquidity";
        return "High – check for idle cash/inventory";
      }

      function classifyDebtEquity(r) {
        if (r == null || !isFinite(r)) return "";
        if (r < 0.3) return "Low leverage";
        if (r <= 1.5) return "Moderate leverage";
        return "High leverage – higher risk";
      }

      function classifyInterestCoverage(r) {
        if (r == null || !isFinite(r)) return "";
        if (r < 1) return "Coverage &lt;1 – unsustainable";
        if (r < 3) return "Thin coverage";
        return "Comfortable coverage";
      }

      function classifyPE(r) {
        if (r == null || !isFinite(r) || r <= 0) return "";
        if (r < 10) return "Low multiple – may be undervalued or low growth";
        if (r <= 25) return "Typical range";
        return "High multiple – priced for growth";
      }

      document.getElementById("calculateBtn").addEventListener("click", () => {
        const revenue = parseNum("revenue");
        const cogs = parseNum("cogs");
        const ebit = parseNum("operatingIncome");
        const netIncome = parseNum("netIncome");
        const totalAssets = parseNum("totalAssets");
        const totalEquity = parseNum("totalEquity");
        const totalDebt = parseNum("totalDebt");
        const currentAssets = parseNum("currentAssets");
        const inventory = parseNum("inventory");
        const currentLiabilities = parseNum("currentLiabilities");
        const interestExpense = parseNum("interestExpense");
        const shares = parseNum("sharesOutstanding");
        const price = parseNum("sharePrice");

        const keyInputs = [
          revenue,
          netIncome,
          totalAssets,
          totalEquity,
          currentAssets,
          currentLiabilities,
          totalDebt,
          shares,
          price,
        ].filter((v) => v !== null);
        const warning = document.getElementById("inputWarning");
        if (keyInputs.length === 0) {
          warning.classList.remove("hidden");
          return;
        } else {
          warning.classList.add("hidden");
        }

        // Profitability
        const netMargin =
          revenue && netIncome != null ? netIncome / revenue : null;
        const roe =
          totalEquity && netIncome != null ? netIncome / totalEquity : null;
        const roa =
          totalAssets && netIncome != null ? netIncome / totalAssets : null;
        const grossMargin =
          revenue && cogs != null ? (revenue - cogs) / revenue : null;

        document.getElementById("netMarginValue").textContent =
          formatPercent(netMargin);
        document.getElementById("netMarginTag").textContent =
          classifyNetMargin(netMargin);
        document.getElementById("roeValue").textContent = formatPercent(roe);
        document.getElementById("roeTag").textContent = classifyROE(roe);
        document.getElementById("roaValue").textContent = formatPercent(roa);
        document.getElementById("roaTag").textContent =
          roa != null && isFinite(roa)
            ? roa < 0.03
              ? "Low ROA"
              : roa < 0.08
              ? "OK ROA"
              : "Strong ROA"
            : "";
        document.getElementById("grossMarginValue").textContent =
          formatPercent(grossMargin);
        document.getElementById("grossMarginTag").textContent =
          grossMargin != null && isFinite(grossMargin)
            ? grossMargin < 0.2
              ? "Thin margin"
              : grossMargin < 0.4
              ? "Healthy"
              : "Very strong"
            : "";

        // Liquidity
        const currentRatio =
          currentAssets && currentLiabilities
            ? currentAssets / currentLiabilities
            : null;
        const quickRatio =
          currentAssets != null && inventory != null && currentLiabilities
            ? (currentAssets - inventory) / currentLiabilities
            : null;

        document.getElementById("currentRatioValue").textContent =
          formatRatio(currentRatio);
        document.getElementById("currentRatioTag").textContent =
          classifyCurrentRatio(currentRatio);
        document.getElementById("quickRatioValue").textContent =
          formatRatio(quickRatio);
        document.getElementById("quickRatioTag").textContent =
          quickRatio != null && isFinite(quickRatio)
            ? quickRatio < 1
              ? "Below 1.0"
              : "OK"
            : "";

        // Leverage
        const debtEquity =
          totalDebt != null && totalEquity ? totalDebt / totalEquity : null;
        const debtRatio =
          totalDebt != null && totalAssets ? totalDebt / totalAssets : null;
        const interestCoverage =
          ebit != null && interestExpense ? ebit / interestExpense : null;

        document.getElementById("debtEquityValue").textContent =
          formatRatio(debtEquity);
        document.getElementById("debtEquityTag").textContent =
          classifyDebtEquity(debtEquity);
        document.getElementById("debtRatioValue").textContent =
          formatRatio(debtRatio);
        document.getElementById("debtRatioTag").textContent =
          debtRatio != null && isFinite(debtRatio)
            ? debtRatio < 0.3
              ? "Low leverage"
              : debtRatio < 0.6
              ? "Moderate"
              : "High"
            : "";
        document.getElementById("interestCoverageValue").textContent =
          formatRatio(interestCoverage);
        document.getElementById("interestCoverageTag").textContent =
          classifyInterestCoverage(interestCoverage);

        // Efficiency
        const assetTurnover =
          revenue && totalAssets ? revenue / totalAssets : null;
        const inventoryTurnover = cogs && inventory ? cogs / inventory : null;

        document.getElementById("assetTurnoverValue").textContent =
          formatRatio(assetTurnover);
        document.getElementById("assetTurnoverTag").textContent =
          assetTurnover != null && isFinite(assetTurnover)
            ? assetTurnover < 0.5
              ? "Asset-heavy / low turnover"
              : "OK / high turnover"
            : "";
        document.getElementById("inventoryTurnoverValue").textContent =
          formatRatio(inventoryTurnover);
        document.getElementById("inventoryTurnoverTag").textContent =
          inventoryTurnover != null && isFinite(inventoryTurnover)
            ? inventoryTurnover < 3
              ? "Slow-moving inventory"
              : "Healthy turnover"
            : "";

        // Valuation
        const eps = netIncome != null && shares ? netIncome / shares : null;
        const marketCap = price && shares ? price * shares : null;
        const pe = price && eps ? price / eps : null;
        const pb = marketCap && totalEquity ? marketCap / totalEquity : null;
        const ps = marketCap && revenue ? marketCap / revenue : null;
        const evEbit =
          marketCap && totalDebt != null && ebit
            ? (marketCap + totalDebt) / ebit
            : null;

        document.getElementById("peValue").textContent = formatRatio(pe);
        document.getElementById("peTag").textContent = classifyPE(pe);
        document.getElementById("pbValue").textContent = formatRatio(pb);
        document.getElementById("pbTag").textContent =
          pb != null && isFinite(pb)
            ? pb < 1
              ? "Below book value"
              : "Above book value"
            : "";
        document.getElementById("psValue").textContent = formatRatio(ps);
        document.getElementById("psTag").textContent =
          ps != null && isFinite(ps) ? (ps < 2 ? "Low P/S" : "High P/S") : "";
        document.getElementById("evEbitValue").textContent =
          formatRatio(evEbit);
        document.getElementById("evEbitTag").textContent =
          evEbit != null && isFinite(evEbit)
            ? evEbit < 8
              ? "Low multiple"
              : evEbit < 15
              ? "Typical"
              : "High multiple"
            : "";

        // Summary
        const strengths = [];
        const risks = [];

        if (netMargin != null && netMargin >= 0.1)
          strengths.push("Strong net profit margin");
        if (roe != null && roe >= 0.15) strengths.push("High return on equity");
        if (currentRatio != null && currentRatio >= 1.2 && currentRatio <= 2)
          strengths.push("Comfortable liquidity (current ratio)");
        if (debtEquity != null && debtEquity < 1)
          strengths.push("Moderate or low leverage");
        if (interestCoverage != null && interestCoverage >= 3)
          strengths.push("Comfortable interest coverage");

        if (netMargin != null && netMargin < 0.03)
          risks.push("Very thin or negative net margin");
        if (roe != null && roe < 0.08) risks.push("Low return on equity");
        if (currentRatio != null && currentRatio < 1)
          risks.push("Current ratio below 1.0 – potential liquidity stress");
        if (debtEquity != null && debtEquity > 2)
          risks.push("High debt-to-equity – leverage risk");
        if (interestCoverage != null && interestCoverage < 1.5)
          risks.push("Weak interest coverage – debt service risk");

        let html = '<div class="text-sm">';
        if (strengths.length) {
          html +=
            '<p class="font-semibold mb-1">Potential strengths:</p><ul class="list-disc ml-5 mb-2">';
          strengths.forEach((s) => (html += "<li>" + s + "</li>"));
          html += "</ul>";
        }
        if (risks.length) {
          html +=
            '<p class="font-semibold mb-1">Potential red flags:</p><ul class="list-disc ml-5 mb-2">';
          risks.forEach((r) => (html += "<li>" + r + "</li>"));
          html += "</ul>";
        }
        if (!strengths.length && !risks.length) {
          html +=
            "<p>We computed some ratios, but none clearly fall into strong or weak buckets based on generic thresholds. Compare them to sector peers and your own history for deeper insight.</p>";
        }
        html +=
          '<p class="mt-2 text-xs text-blue-900/80">Note: These classifications are generic and for educational purposes only. Always use sector-specific benchmarks and professional judgment.</p></div>';
        document.getElementById("summaryPanel").innerHTML = html;
      });

      document.getElementById("resetBtn").addEventListener("click", () => {
        document
          .querySelectorAll('input[type="number"]')
          .forEach((i) => (i.value = ""));
        document
          .querySelectorAll(
            "#netMarginValue,#roeValue,#roaValue,#grossMarginValue,#currentRatioValue,#quickRatioValue,#debtEquityValue,#debtRatioValue,#interestCoverageValue,#assetTurnoverValue,#inventoryTurnoverValue,#peValue,#pbValue,#psValue,#evEbitValue"
          )
          .forEach((el) => (el.textContent = "–"));
        document
          .querySelectorAll(
            "#netMarginTag,#roeTag,#roaTag,#grossMarginTag,#currentRatioTag,#quickRatioTag,#debtEquityTag,#debtRatioTag,#interestCoverageTag,#assetTurnoverTag,#inventoryTurnoverTag,#peTag,#pbTag,#psTag,#evEbitTag"
          )
          .forEach((el) => (el.textContent = ""));
        document.getElementById("summaryPanel").innerHTML =
          "Enter your data and click “Calculate Ratios” to see a summary of strengths and red flags.";
        document.getElementById("inputWarning").classList.add("hidden");
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
