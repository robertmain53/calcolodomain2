                  <input
                    id="revenue"
                    type="number"
                    step="any"
                    class="w-full border rounded px-3 py-2 mb-2"
                    placeholder="e.g. 1,000,000"
                  />

                  <label class="block text-sm mb-1" for="cogs"
                    >Cost of Goods Sold (COGS)</label
                  >
                  <input
                    id="cogs"
                    type="number"
                    step="any"
                    class="w-full border rounded px-3 py-2 mb-2"
                  />

                  <label class="block text-sm mb-1" for="operatingIncome"
                    >Operating Income (EBIT)</label
                  >
                  <input
                    id="operatingIncome"
                    type="number"
                    step="any"
                    class="w-full border rounded px-3 py-2 mb-2"
                  />

                  <label class="block text-sm mb-1" for="netIncome"
                    >Net Income</label
                  >
                  <input
                    id="netIncome"
                    type="number"
                    step="any"
                    class="w-full border rounded px-3 py-2 mb-2"
                  />

                  <label class="block text-sm mb-1" for="interestExpense"
                    >Interest Expense</label
                  >
                  <input
                    id="interestExpense"
                    type="number"
                    step="any"
                    class="w-full border rounded px-3 py-2 mb-2"
                  />

                  <label class="block text-sm mb-1" for="taxExpense"
                    >Income Tax Expense</label
                  >
                  <input
                    id="taxExpense"
                    type="number"
                    step="any"
                    class="w-full border rounded px-3 py-2"
                  />
                </div>

                <!-- Balance Sheet & Market Data -->
                <div>
                  <h3 class="font-semibold mb-2">
                    Balance Sheet &amp; Market Data
                  </h3>
                  <label class="block text-sm mb-1" for="totalAssets"
                    >Total Assets</label
                  >
                  <input
                    id="totalAssets"
                    type="number"
                    step="any"
                    class="w-full border rounded px-3 py-2 mb-2"
                  />

                  <label class="block text-sm mb-1" for="totalEquity"
                    >Total Equity</label
                  >
                  <input
                    id="totalEquity"
                    type="number"
                    step="any"
                    class="w-full border rounded px-3 py-2 mb-2"
                  />

                  <label class="block text-sm mb-1" for="totalDebt"
                    >Total Debt (Interest-bearing)</label
                  >
                  <input
                    id="totalDebt"
                    type="number"
                    step="any"
                    class="w-full border rounded px-3 py-2 mb-2"
                  />

                  <label class="block text-sm mb-1" for="currentAssets"
                    >Current Assets</label
                  >
                  <input
                    id="currentAssets"
                    type="number"
                    step="any"
                    class="w-full border rounded px-3 py-2 mb-2"
                  />

                  <label class="block text-sm mb-1" for="inventory"
                    >Inventory</label
                  >
                  <input
                    id="inventory"
                    type="number"
                    step="any"
                    class="w-full border rounded px-3 py-2 mb-2"
                  />

                  <label class="block text-sm mb-1" for="currentLiabilities"
                    >Current Liabilities</label
                  >
                  <input
                    id="currentLiabilities"
                    type="number"
                    step="any"
                    class="w-full border rounded px-3 py-2 mb-2"
