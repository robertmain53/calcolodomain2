              <div class="space-y-4">
                <!-- Profitability -->
                <div class="border rounded-lg">
                  <button
                    type="button"
                    class="w-full flex justify-between items-center px-4 py-2 bg-gray-50 hover:bg-gray-100 rounded-t-lg accordion-header"
                  >
