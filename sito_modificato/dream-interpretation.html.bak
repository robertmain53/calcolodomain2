    <script>
      // Symbol button toggling
      const symbolButtons = document.querySelectorAll(".symbol-btn");
      const activeSymbols = new Set();

      symbolButtons.forEach((btn) => {
        btn.addEventListener("click", () => {
          const sym = btn.getAttribute("data-symbol");
          if (activeSymbols.has(sym)) {
            activeSymbols.delete(sym);
            btn.classList.remove(
              "bg-blue-100",
              "border-blue-500",
              "text-blue-700"
            );
            btn.classList.add("bg-gray-100", "text-gray-700");
          } else {
            activeSymbols.add(sym);
            btn.classList.add(
              "bg-blue-100",
              "border-blue-500",
              "text-blue-700"
            );
            btn.classList.remove("bg-gray-100", "text-gray-700");
          }
        });
      });

      const analyzeBtn = document.getElementById("analyzeBtn");
      const clearBtn = document.getElementById("clearBtn");
      const errorMsg = document.getElementById("errorMsg");
      const resultsSection = document.getElementById("resultsSection");
      const themesContainer = document.getElementById("themesContainer");
      const meaningsContainer = document.getElementById("meaningsContainer");
      const symbolsContainer = document.getElementById("symbolsContainer");
      const questionsList = document.getElementById("questionsList");

      function createTag(text, tone = "neutral") {
        const span = document.createElement("span");
        span.className =
          "inline-flex items-center px-2.5 py-1 rounded-full text-xs font-medium";
        if (tone === "positive") {
          span.classList.add("bg-green-100", "text-green-800");
        } else if (tone === "warning") {
          span.classList.add("bg-yellow-100", "text-yellow-800");
        } else {
          span.classList.add("bg-gray-100", "text-gray-800");
        }
        span.textContent = text;
        return span;
      }

      function analyzeDream() {
        const dreamText = document.getElementById("dreamText").value.trim();
        const lifeContext = document.getElementById("lifeContext").value.trim();
        const dreamType = document.getElementById("dreamType").value;
        const mainEmotion = document.getElementById("mainEmotion").value;

        if (!dreamText) {
          errorMsg.classList.remove("hidden");
          resultsSection.classList.add("hidden");
          return;
        }
        errorMsg.classList.add("hidden");

        // Reset containers
        themesContainer.innerHTML = "";
        meaningsContainer.innerHTML = "";
        symbolsContainer.innerHTML = "";
        questionsList.innerHTML = "";

        const lowerText = dreamText.toLowerCase();

        // Detect themes
        const themes = [];

        if (
          dreamType === "nightmare" ||
          /nightmare|terrified|horrible|scary/.test(lowerText)
        ) {
          themes.push({ label: "Nightmare / distress", tone: "warning" });
        }
        if (
          dreamType === "anxiety" ||
          /stress|stressed|anxious|pressure|deadline|overwhelmed/.test(
            lowerText
          )
        ) {
          themes.push({ label: "Stress & anxiety", tone: "warning" });
        }
        if (
          dreamType === "recurring" ||
          /every night|again and again|same dream/.test(lowerText)
        ) {
          themes.push({ label: "Recurring pattern", tone: "neutral" });
        }
        if (
          /ex\b|ex-boyfriend|ex-girlfriend|former partner/.test(lowerText) ||
          dreamType === "relationship"
        ) {
          themes.push({ label: "Relationship / attachment", tone: "neutral" });
        }
        if (
          /work|office|boss|meeting|deadline|exam|test|school|classroom|unprepared/.test(
            lowerText
          ) ||
          dreamType === "work" ||
          dreamType === "exam"
        ) {
          themes.push({ label: "Performance & expectations", tone: "neutral" });
        }
        if (
          /fly|flying|float|floating|levitate/.test(lowerText) ||
          dreamType === "flying" ||
          activeSymbols.has("flying")
        ) {
          themes.push({ label: "Freedom vs. control", tone: "positive" });
        }
        if (
          /fall|falling|drop/.test(lowerText) ||
          dreamType === "falling" ||
          activeSymbols.has("falling")
        ) {
          themes.push({
            label: "Loss of control / insecurity",
            tone: "warning",
          });
        }
        if (
          /house|home|room|apartment/.test(lowerText) ||
          activeSymbols.has("house")
        ) {
          themes.push({
            label: "Sense of self / personal space",
            tone: "neutral",
          });
        }
        if (
          /water|ocean|sea|river|lake|drown|drowning|wave|flood/.test(
            lowerText
          ) ||
          activeSymbols.has("water")
        ) {
          themes.push({ label: "Emotional intensity", tone: "neutral" });
        }
        if (
          /dead|death|funeral|grave|cemetery/.test(lowerText) ||
          activeSymbols.has("death")
        ) {
          themes.push({ label: "Endings & transitions", tone: "neutral" });
        }

        if (themes.length === 0) {
          themes.push({ label: "Mixed / unclear themes", tone: "neutral" });
        }

        themes.forEach((t) =>
          themesContainer.appendChild(createTag(t.label, t.tone))
        );

        // Meanings (high-level)
        const meaningParagraphs = [];

        // Emotion-based
        if (
          mainEmotion === "fear" ||
          /terrified|scared|afraid|horror/.test(lowerText)
        ) {
          meaningParagraphs.push(
            "The strong presence of fear suggests your mind may be rehearsing situations where you feel unsafe, judged, or out of control. Rather than predicting danger, the dream likely mirrors worries you already carry into sleep."
          );
        } else if (mainEmotion === "anxiety") {
          meaningParagraphs.push(
            "Anxiety in dreams often amplifies everyday pressures. Your brain may be exaggerating “what if” scenarios to help you emotionally process deadlines, responsibilities, or uncertainty."
          );
        } else if (mainEmotion === "joy" || mainEmotion === "relief") {
          meaningParagraphs.push(
            "Positive emotions in the dream point toward a sense of possibility, relief, or integration. Even if some images were strange, your emotional tone suggests resilience and capacity to cope."
          );
        } else if (mainEmotion === "sadness") {
          meaningParagraphs.push(
            "Sadness in dreams can reflect grief, disappointment, or unexpressed feelings. The dream may be giving you a safe space to feel what is harder to express when awake."
          );
        }

        // Context-based
        if (lifeContext) {
          meaningParagraphs.push(
            "Because you added life context, it is especially useful to compare the dream scenes with real situations you described. Look for parallels in roles, power dynamics, or emotions rather than literal events."
          );
        } else {
          meaningParagraphs.push(
            "Without explicit life context, focus on the emotional “flavor” of the dream. Ask where in your waking life you feel something similar, even if the details are different."
          );
        }

        // Specific patterns
        if (
          activeSymbols.has("chased") ||
          /chase|chased|pursue|pursued/.test(lowerText) ||
          dreamType === "chase"
        ) {
          meaningParagraphs.push(
            "Being chased often symbolizes something you are avoiding: a decision, a conversation, a responsibility, or a feeling. The chaser may represent a part of yourself (anger, guilt, ambition) that you find hard to face directly."
          );
        }
        if (
          activeSymbols.has("teeth") ||
          /teeth|tooth|dentist/.test(lowerText) ||
          dreamType === "teeth"
        ) {
          meaningParagraphs.push(
            "Teeth-related dreams are frequently linked to appearance, communication, or a sense of power. They can arise when you worry about how others see you, or when you feel you are not being heard."
          );
        }
        if (
          activeSymbols.has("exam") ||
          /exam|test|quiz|unprepared|late for class/.test(lowerText) ||
          dreamType === "exam"
        ) {
          meaningParagraphs.push(
            "Exam or unpreparedness dreams are classic performance-anxiety dreams. They rarely relate to actual exams and more often reflect fear of being evaluated, exposed, or “found out” in some area of life."
          );
        }
        if (
          activeSymbols.has("naked") ||
          /naked|nude|no clothes/.test(lowerText)
        ) {
          meaningParagraphs.push(
            "Appearing naked or exposed in public often symbolizes vulnerability, shame, or fear of judgment. It may point to situations where you feel emotionally “uncovered” or not fully prepared."
          );
        }
        if (
          activeSymbols.has("pregnancy") ||
          /pregnant|pregnancy|baby|birth|giving birth/.test(lowerText)
        ) {
          meaningParagraphs.push(
            "Pregnancy or birth in dreams can symbolize new ideas, projects, or phases of life “growing” inside you. It does not necessarily predict literal pregnancy."
          );
        }

        // If nothing else
        if (meaningParagraphs.length === 0) {
          meaningParagraphs.push(
            "Your dream combines several elements that are best understood in relation to your own life. Rather than a fixed meaning, treat it as a symbolic story about how you are currently experiencing yourself and the world."
          );
        }

        meaningParagraphs.forEach((text) => {
          const p = document.createElement("p");
          p.textContent = text;
          meaningsContainer.appendChild(p);
        });

        // Symbol insights
        const symbolInsights = [];

        activeSymbols.forEach((sym) => {
          if (sym === "falling") {
            symbolInsights.push(
              "Falling: commonly linked to loss of control, insecurity, or fear of failure. Ask where you feel unsupported or afraid of “falling short.”"
            );
          } else if (sym === "flying") {
            symbolInsights.push(
              "Flying: can express freedom, ambition, or escape. Notice whether you felt joyful and powerful, or scared and out of control."
            );
          } else if (sym === "teeth") {
            symbolInsights.push(
              "Teeth / mouth: often associated with communication, appearance, and personal power. Consider whether you are holding back words or worried about how you come across."
            );
          } else if (sym === "chased") {
            symbolInsights.push(
              "Being chased: may reflect avoidance of a task, truth, or emotion. The chaser can symbolize a part of you that wants attention."
            );
          } else if (sym === "naked") {
            symbolInsights.push(
              "Naked in public: typically about vulnerability, shame, or fear of exposure. Where do you feel “on display” in waking life?"
            );
          } else if (sym === "exam") {
            symbolInsights.push(
              "Exam / test: linked to evaluation and self-worth. You may be questioning whether you are “good enough” in some role."
            );
          } else if (sym === "house") {
            symbolInsights.push(
              "House / rooms: often represent the self. New or hidden rooms can symbolize unexplored aspects of your identity or potential."
            );
          } else if (sym === "water") {
            symbolInsights.push(
              "Water / ocean: classic symbol for emotion. Calm water suggests emotional balance; storms or floods suggest feeling overwhelmed."
            );
          } else if (sym === "death") {
            symbolInsights.push(
              "Death / dead person: usually about endings, transitions, or letting go, rather than literal death."
            );
          } else if (sym === "animals") {
            symbolInsights.push(
              "Animals: can represent instincts, drives, or traits you associate with that animal (loyalty, aggression, playfulness, etc.)."
            );
          } else if (sym === "vehicle") {
            symbolInsights.push(
              "Car / vehicle: often symbolizes your life direction or sense of control over where you are headed."
            );
          } else if (sym === "lost") {
            symbolInsights.push(
              "Lost / can’t find way: may mirror confusion about life direction, identity, or decisions."
            );
          } else if (sym === "ex") {
            symbolInsights.push(
              "Ex-partner: often reflects unresolved feelings, patterns from that relationship, or how you relate to intimacy now."
            );
          } else if (sym === "childhood") {
            symbolInsights.push(
              "Childhood place: can point to early experiences, beliefs, or emotional patterns that are being revisited."
            );
          } else if (sym === "pregnancy") {
            symbolInsights.push(
              "Pregnancy / birth: can symbolize new beginnings, creativity, or responsibilities developing in your life."
            );
          }
        });

        // Also infer symbols from text if not already selected
        if (
          /teeth|tooth|dentist/.test(lowerText) &&
          !activeSymbols.has("teeth")
        ) {
          symbolInsights.push(
            "Teeth / mouth: often associated with communication, appearance, and personal power. Consider whether you are holding back words or worried about how you come across."
          );
        }
        if (
          /house|home|room|apartment/.test(lowerText) &&
          !activeSymbols.has("house")
        ) {
          symbolInsights.push(
            "House / rooms: often represent the self. New or hidden rooms can symbolize unexplored aspects of your identity or potential."
          );
        }
        if (
          /water|ocean|sea|river|lake|drown|drowning|wave|flood/.test(
            lowerText
          ) &&
          !activeSymbols.has("water")
        ) {
          symbolInsights.push(
            "Water / ocean: classic symbol for emotion. Calm water suggests emotional balance; storms or floods suggest feeling overwhelmed."
          );
        }

        if (symbolInsights.length === 0) {
          symbolInsights.push(
            "No specific symbols were highlighted. Focus on the overall emotional tone and storyline of the dream rather than individual images."
          );
        }

        symbolInsights.forEach((text) => {
          const p = document.createElement("p");
          p.textContent = text;
          symbolsContainer.appendChild(p);
        });

        // Reflection questions
        const questions = [
          "What part of this dream felt most emotionally intense or memorable to you?",
          "Where in your current life do you feel a similar emotion to what you felt in the dream?",
          "If each main character or symbol represented a part of you, what might each part want or fear?",
          "Is there a decision, conversation, or change you have been postponing that this dream might be pointing toward?",
        ];

        if (
          dreamType === "recurring" ||
          /every night|again and again|same dream/.test(lowerText)
        ) {
          questions.push(
            "What has stayed the same in your life since this dream first appeared, and what has changed?"
          );
        }
        if (
          activeSymbols.has("chased") ||
          /chase|chased|pursue|pursued/.test(lowerText)
        ) {
          questions.push(
            "If you turned around and faced what was chasing you, what do you imagine it would say or ask of you?"
          );
        }
        if (
          activeSymbols.has("house") ||
          /house|home|room|apartment/.test(lowerText)
        ) {
          questions.push(
            "How does the dream house compare to your real living situation or your inner sense of “home”?"
          );
        }

        questions.forEach((q) => {
          const li = document.createElement("li");
          li.textContent = q;
          questionsList.appendChild(li);
        });

        resultsSection.classList.remove("hidden");
        resultsSection.scrollIntoView({ behavior: "smooth", block: "start" });
      }

      analyzeBtn.addEventListener("click", analyzeDream);

      clearBtn.addEventListener("click", () => {
        document.getElementById("dreamText").value = "";
        document.getElementById("lifeContext").value = "";
        document.getElementById("dreamType").value = "";
        document.getElementById("mainEmotion").value = "";
        errorMsg.classList.add("hidden");
        resultsSection.classList.add("hidden");
        themesContainer.innerHTML = "";
        meaningsContainer.innerHTML = "";
        symbolsContainer.innerHTML = "";
        questionsList.innerHTML = "";
        activeSymbols.clear();
        symbolButtons.forEach((btn) => {
          btn.classList.remove(
            "bg-blue-100",
            "border-blue-500",
            "text-blue-700"
          );
          btn.classList.add("bg-gray-100", "text-gray-700");
        });
      });

      // Mobile menu script (simple)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", () => {
          const menu = document.getElementById("mobile-menu");
          const expanded = menu.classList.contains("hidden") ? "true" : "false";
          menu.classList.toggle("hidden");
          document
            .getElementById("mobile-menu-toggle")
            .setAttribute("aria-expanded", expanded);
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
