    <script>
      // Standard normal PDF
      function normPDF(x) {
        return Math.exp(-0.5 * x * x) / Math.sqrt(2 * Math.PI);
      }

      // Standard normal CDF using Abramowitz-Stegun approximation
      function normCDF(x) {
        var sign = x < 0 ? -1 : 1;
        x = Math.abs(x) / Math.sqrt(2);
        var t = 1 / (1 + 0.3275911 * x);
        var a1 = 0.254829592,
          a2 = -0.284496736,
          a3 = 1.421413741,
          a4 = -1.453152027,
          a5 = 1.061405429;
        var erf =
          1 -
          ((((a5 * t + a4) * t + a3) * t + a2) * t + a1) * t * Math.exp(-x * x);
        return 0.5 * (1 + sign * erf);
      }

      function blackScholes(S, K, T, r, q, sigma) {
        var sqrtT = Math.sqrt(T);
        var d1 =
          (Math.log(S / K) + (r - q + 0.5 * sigma * sigma) * T) /
          (sigma * sqrtT);
        var d2 = d1 - sigma * sqrtT;

        var Nd1 = normCDF(d1);
        var Nd2 = normCDF(d2);
        var Nminusd1 = normCDF(-d1);
        var Nminusd2 = normCDF(-d2);

        var discR = Math.exp(-r * T);
        var discQ = Math.exp(-q * T);

        var call = S * discQ * Nd1 - K * discR * Nd2;
        var put = K * discR * Nminusd2 - S * discQ * Nminusd1;

        // Greeks
        var pdfd1 = normPDF(d1);
        var gamma = (discQ * pdfd1) / (S * sigma * sqrtT);
        var vega = S * discQ * pdfd1 * sqrtT; // per 1.0 volatility
        var vegaPct = vega / 100; // per 1% volatility

        var thetaCallYear =
          -(S * discQ * pdfd1 * sigma) / (2 * sqrtT) -
          r * K * discR * Nd2 +
          q * S * discQ * Nd1;
        var thetaPutYear =
          -(S * discQ * pdfd1 * sigma) / (2 * sqrtT) +
          r * K * discR * Nminusd2 -
          q * S * discQ * Nminusd1;
        var thetaCallDay = thetaCallYear / 365;
        var thetaPutDay = thetaPutYear / 365;

        var rhoCall = (K * T * discR * Nd2) / 100; // per 1% change in r
        var rhoPut = (-K * T * discR * Nminusd2) / 100;

        var deltaCall = discQ * Nd1;
        var deltaPut = -discQ * Nminusd1;

        return {
          call: call,
          put: put,
          d1: d1,
          d2: d2,
          deltaCall: deltaCall,
          deltaPut: deltaPut,
          gamma: gamma,
          vegaPct: vegaPct,
          thetaCallDay: thetaCallDay,
          thetaPutDay: thetaPutDay,
          rhoCall: rhoCall,
          rhoPut: rhoPut,
        };
      }

      function impliedVol(targetPrice, S, K, T, r, q, isCall) {
        var tol = 1e-6;
        var maxIter = 100;
        var low = 0.001; // 0.1% annual vol
        var high = 5.0; // 500% annual vol

        // Check if target is within bounds
        var bsLow = blackScholes(S, K, T, r, q, low);
        var priceLow = isCall ? bsLow.call : bsLow.put;
        var bsHigh = blackScholes(S, K, T, r, q, high);
        var priceHigh = isCall ? bsHigh.call : bsHigh.put;

        if (targetPrice < priceLow || targetPrice > priceHigh) {
          return null;
        }

        var mid, priceMid;
        for (var i = 0; i < maxIter; i++) {
          mid = 0.5 * (low + high);
          var bsMid = blackScholes(S, K, T, r, q, mid);
          priceMid = isCall ? bsMid.call : bsMid.put;

          if (Math.abs(priceMid - targetPrice) < tol) {
            return mid;
          }

          if (priceMid > targetPrice) {
            high = mid;
          } else {
            low = mid;
          }
        }
        return mid;
      }

      function formatNumber(x, decimals) {
        if (!isFinite(x)) return "–";
        return x.toFixed(decimals);
      }

      function calculate() {
        var errorEl = document.getElementById("errorMessage");
        errorEl.classList.add("hidden");
        errorEl.textContent = "";

        var S = parseFloat(document.getElementById("underlyingPrice").value);
        var K = parseFloat(document.getElementById("strikePrice").value);
        var Tinput = parseFloat(
          document.getElementById("timeToMaturity").value
        );
        var timeUnit = document.getElementById("timeUnit").value;
        var rPct = parseFloat(document.getElementById("riskFreeRate").value);
        var qPct =
          parseFloat(document.getElementById("dividendYield").value) || 0;
        var sigmaPct = parseFloat(document.getElementById("volatility").value);
        var marketPrice = parseFloat(
          document.getElementById("marketPrice").value
        );
        var optionTypeIV = document.getElementById("optionTypeIV").value;

        if (!(S > 0) || !(K > 0) || !(Tinput > 0) || !(sigmaPct > 0)) {
          errorEl.textContent =
            "Please enter positive values for S, K, time to expiration, and volatility.";
          errorEl.classList.remove("hidden");
          return;
        }

        var T = timeUnit === "days" ? Tinput / 365.0 : Tinput;
        var r = (rPct || 0) / 100.0;
        var q = (qPct || 0) / 100.0;
        var sigma = sigmaPct / 100.0;

        var res = blackScholes(S, K, T, r, q, sigma);

        document.getElementById("callPrice").textContent = formatNumber(
          res.call,
          4
        );
        document.getElementById("putPrice").textContent = formatNumber(
          res.put,
          4
        );
        document.getElementById("d1Value").textContent = formatNumber(
          res.d1,
          4
        );
        document.getElementById("d2Value").textContent = formatNumber(
          res.d2,
          4
        );

        document.getElementById("callDelta").textContent = formatNumber(
          res.deltaCall,
          4
        );
        document.getElementById("putDelta").textContent = formatNumber(
          res.deltaPut,
          4
        );
        document.getElementById("gamma").textContent = formatNumber(
          res.gamma,
          6
        );
        document.getElementById("vega").textContent = formatNumber(
          res.vegaPct,
          4
        );
        document.getElementById("callTheta").textContent = formatNumber(
          res.thetaCallDay,
          6
        );
        document.getElementById("putTheta").textContent = formatNumber(
          res.thetaPutDay,
          6
        );
        document.getElementById("callRho").textContent = formatNumber(
          res.rhoCall,
          4
        );
        document.getElementById("putRho").textContent = formatNumber(
          res.rhoPut,
          4
        );

        var ivResultEl = document.getElementById("ivResult");
        var ivNoteEl = document.getElementById("ivNote");
        ivNoteEl.classList.add("hidden");

        if (!isNaN(marketPrice) && marketPrice > 0) {
          var isCall = optionTypeIV === "call";
          var iv = impliedVol(marketPrice, S, K, T, r, q, isCall);
          if (iv === null) {
            ivResultEl.textContent =
              "No implied volatility found for this market price within [0.1%, 500%].";
          } else {
            ivResultEl.textContent =
              "Implied volatility: " + (iv * 100).toFixed(4) + " %";
            ivNoteEl.classList.remove("hidden");
          }
        } else {
          ivResultEl.textContent =
            "Enter a market price to compute implied volatility.";
        }
      }

      document
        .getElementById("calculateBtn")
        .addEventListener("click", function (e) {
          e.preventDefault();
          calculate();
        });

      document
        .getElementById("resetBtn")
        .addEventListener("click", function () {
          document.getElementById("underlyingPrice").value = 100;
          document.getElementById("strikePrice").value = 100;
          document.getElementById("timeToMaturity").value = 0.5;
          document.getElementById("timeUnit").value = "years";
          document.getElementById("riskFreeRate").value = 2;
          document.getElementById("dividendYield").value = 0;
          document.getElementById("volatility").value = 20;
          document.getElementById("marketPrice").value = "";
          document.getElementById("optionTypeIV").value = "call";
          document.getElementById("errorMessage").classList.add("hidden");
          document.getElementById("errorMessage").textContent = "";

          [
            "callPrice",
            "putPrice",
            "d1Value",
            "d2Value",
            "callDelta",
            "putDelta",
            "gamma",
            "vega",
            "callTheta",
            "putTheta",
            "callRho",
            "putRho",
          ].forEach(function (id) {
            document.getElementById(id).textContent = "–";
          });
          document.getElementById("ivResult").textContent =
            "Enter a market price to compute implied volatility.";
          document.getElementById("ivNote").classList.add("hidden");
        });

      // Auto-calc on load with defaults
      window.addEventListener("DOMContentLoaded", calculate);
    </script>

