            <script>
                // Costanti geometriche per il pentagono regolare
                const SIDES = 5;
                const HALF_ANGLE_RAD = Math.PI / SIDES; // 36 degrees in radians (360/10)
                
                // Relazioni trigonometriche (costanti)
                const TAN_36 = Math.tan(HALF_ANGLE_RAD);
                const SIN_36 = Math.sin(HALF_ANGLE_RAD);

                // Funzione per formattare i numeri
                function formatNum(num, precision = 4) {
                    if (isNaN(num) || num <= 0) return '-';
                    return parseFloat(num.toFixed(precision)).toString();
                }

                function calculateProperties(s, a, R) {
                    let side, apothem, radius;
                    
                    // 1. Determine the primary input unit and calculate the others
                    if (!isNaN(s)) {
                        side = s;
                        apothem = side / (2 * TAN_36);
                        radius = side / (2 * SIN_36);
                    } else if (!isNaN(a)) {
                        apothem = a;
                        side = apothem * (2 * TAN_36);
                        radius = apothem / Math.cos(HALF_ANGLE_RAD);
                    } else if (!isNaN(R)) {
                        radius = R;
                        side = radius * (2 * SIN_36);
                        apothem = radius * Math.cos(HALF_ANGLE_RAD);
                    } else {
                        return { error: 'Please enter a value for the Side, Apothem, or Circumradius.' };
                    }
                    
                    // 2. Calculate area and perimeter
                    const perimeter = side * SIDES;
                    const area = (perimeter * apothem) / 2;
                    const internalAngle = 108; // Fixed for regular pentagon

                    // 3. Check validity
                    if (side <= 0) {
                        return { error: 'Input must be greater than zero.' };
                    }

                    return { side, apothem, radius, perimeter, area, internalAngle };
                }

                document.getElementById('calculate-btn').addEventListener('click', () => {
                    const sideInput = document.getElementById('side-input');
                    const apothemInput = document.getElementById('apothem-input');
                    const radiusInput = document.getElementById('circumradius-input');

                    const s = parseFloat(sideInput.value);
                    const a = parseFloat(apothemInput.value);
                    const R = parseFloat(radiusInput.value);
                    
                    // Determina l'input valido (solo 1 permesso)
                    const inputs = [s, a, R].filter(v => !isNaN(v) && v > 0);
                    
                    if (inputs.length !== 1) {
                        document.getElementById('input-error').textContent = 'Please enter a value for ONLY ONE of the fields (Side, Apothem, or Circumradius).';
                        document.getElementById('input-error').hidden = false;
                        document.getElementById('results-output').hidden = true;
                        return;
                    }
                    
                    // Reset inputs to avoid confusion
                    [sideInput, apothemInput, radiusInput].forEach(input => input.value = '');
                    
                    const results = calculateProperties(s, a, R);

                    if (results.error) {
                        document.getElementById('input-error').textContent = results.error;
                        document.getElementById('input-error').hidden = false;
                        document.getElementById('results-output').hidden = true;
                        return;
                    }
                    
                    document.getElementById('input-error').hidden = true;
                    document.getElementById('results-output').hidden = false;

                    // Populate outputs and re-input the calculated values into the fields
                    sideInput.value = formatNum(results.side);
                    apothemInput.value = formatNum(results.apothem);
                    radiusInput.value = formatNum(results.radius);

                    document.getElementById('out-area').textContent = formatNum(results.area);
                    document.getElementById('out-perimeter').textContent = formatNum(results.perimeter);
                    document.getElementById('out-apothem').textContent = formatNum(results.apothem);
                    document.getElementById('out-radius').textContent = formatNum(results.radius);
                    document.getElementById('out-angle').textContent = '108°';
                    
                    // Rende le formule LaTeX
                    if (window.MathJax) {
                       MathJax.typesetPromise();
                    }
                });
                
                // Set default example (Side = 5)
                document.getElementById('side-input').value = 5;
                document.getElementById('calculate-btn').click();
            });
            </script>
            
