    <script>
      // Masonry Wall Design Calculator Logic
      (function () {
        const unitSystemEl = document.getElementById("unitSystem");
        const supportConditionEl = document.getElementById("supportCondition");
        const loadCaseEl = document.getElementById("loadCase");

        const wallHeightEl = document.getElementById("wallHeight");
        const wallLengthEl = document.getElementById("wallLength");
        const wallThicknessEl = document.getElementById("wallThickness");
        const kEl = document.getElementById("effectiveLengthFactor");

        const axialLoadEl = document.getElementById("axialLoad");
        const uniformPressureEl = document.getElementById("uniformPressure");

        const faAllowEl = document.getElementById("allowableCompStress");
        const ftAllowEl = document.getElementById("allowableFlexStress");
        const eEl = document.getElementById("modulusElasticity");

        const calcBtn = document.getElementById("calcButton");
        const resetBtn = document.getElementById("resetButton");
        const resultsContainer = document.getElementById("resultsContainer");

        const units = {
          us: {
            wallHeight: "ft",
            wallLength: "ft",
            wallThickness: "in",
            axialLoad: "kips",
            uniformPressure: "kip/ft²",
            fa: "psi",
            ft: "psi",
            e: "ksi",
          },
          si: {
            wallHeight: "m",
            wallLength: "m",
            wallThickness: "mm",
            axialLoad: "kN",
            uniformPressure: "kN/m²",
            fa: "MPa",
            ft: "MPa",
            e: "GPa",
          },
        };

        function updateUnitLabels() {
          const sys = unitSystemEl.value;
          document.getElementById("wallHeightUnit").textContent =
            units[sys].wallHeight;
          document.getElementById("wallLengthUnit").textContent =
            units[sys].wallLength;
          document.getElementById("wallThicknessUnit").textContent =
            units[sys].wallThickness;
          document.getElementById("axialLoadUnit").textContent =
            units[sys].axialLoad;
          document.getElementById("uniformPressureUnit").textContent =
            units[sys].uniformPressure;
          document.getElementById("allowableCompStressUnit").textContent =
            units[sys].fa;
          document.getElementById("allowableFlexStressUnit").textContent =
            units[sys].ft;
          document.getElementById("modulusElasticityUnit").textContent =
            units[sys].e;
        }

        function setDefaultK() {
          const cond = supportConditionEl.value;
          let k = 1.0;
          if (cond === "fixedPinned") k = 0.7;
          if (cond === "fixedFixed") k = 0.5;
          kEl.value = k.toFixed(2);
        }

        function parseVal(el) {
          const v = parseFloat(el.value);
          return isNaN(v) ? null : v;
        }

        function showError(msg) {
          resultsContainer.innerHTML =
            '<p class="text-red-700 text-sm">' + msg + "</p>";
        }

        function formatNumber(val, decimals) {
          return Number(val).toFixed(decimals);
        }

        function calculate() {
          const sys = unitSystemEl.value;

          let h = parseVal(wallHeightEl);
          let L = parseVal(wallLengthEl);
          let t = parseVal(wallThicknessEl);
          let K = parseVal(kEl);

          let P = parseVal(axialLoadEl);
          let w = parseVal(uniformPressureEl);

          let Fa = parseVal(faAllowEl);
          let Ft = parseVal(ftAllowEl);
          let E = parseVal(eEl);

          if ([h, L, t, K, P, w, Fa, Ft, E].some((v) => v === null || v <= 0)) {
            showError("Please enter positive numeric values for all inputs.");
            return;
          }

          // Convert to consistent base units
          let h_m, L_m, t_m, P_N, w_N_m2, Fa_Pa, Ft_Pa, E_Pa;

          if (sys === "us") {
            // ft, in, kips, psi, ksi
            h_m = h * 0.3048;
            L_m = L * 0.3048;
            t_m = t * 0.0254;
            P_N = P * 4448.2216; // 1 kip = 4448.2216 N
            w_N_m2 = w * 47.880258; // 1 kip/ft² ≈ 47.880258 kN/m²? Actually 1 kip/ft² = 47.880258 kPa = 47,880.258 N/m²
            w_N_m2 = w * 47880.258;
            Fa_Pa = Fa * 6894.757; // psi to Pa
            Ft_Pa = Ft * 6894.757;
            E_Pa = E * 6894757000; // ksi to Pa
          } else {
            // m, mm, kN, MPa, GPa
            h_m = h;
            L_m = L;
            t_m = t / 1000.0;
            P_N = P * 1000.0;
            w_N_m2 = w * 1000.0;
            Fa_Pa = Fa * 1e6;
            Ft_Pa = Ft * 1e6;
            E_Pa = E * 1e9;
          }

          // Effective height and slenderness
          const h_eff = K * h_m;
          const slenderness = h_eff / t_m;

          // Section properties per meter length of wall
          const b = 1.0; // 1 m strip
          const A = t_m * b;
          const I = (b * Math.pow(t_m, 3)) / 12.0;
          const S = I / (t_m / 2.0);

          // Axial stress
          const fa = P_N / A; // Pa

          // Out-of-plane bending from uniform pressure
          // w_N_m2 acts over span L_m, line load q = w * 1m
          const q = w_N_m2 * 1.0; // N/m
          const Mmax = (q * Math.pow(L_m, 2)) / 8.0; // N·m
          const fb = Mmax / S; // Pa

          // Simple interaction
          const ua = fa / Fa_Pa;
          const ub = Math.abs(fb) / Ft_Pa;
          const u_comb = ua + ub;

          // Convert back to display units
          let fa_disp, fb_disp, Fa_disp, Ft_disp;
          if (sys === "us") {
            fa_disp = fa / 6894.757;
            fb_disp = fb / 6894.757;
            Fa_disp = Fa;
            Ft_disp = Ft;
          } else {
            fa_disp = fa / 1e6;
            fb_disp = fb / 1e6;
            Fa_disp = Fa;
            Ft_disp = Ft;
          }

          const slenderLimit = 30; // generic reference
          const passesSlender = slenderness <= slenderLimit;
          const passesAxial = ua <= 1.0;
          const passesBend = ub <= 1.0;
          const passesComb = u_comb <= 1.0;

          const stressUnit = sys === "us" ? "psi" : "MPa";

          let html = "";

          html += '<div class="border rounded-md p-3 bg-gray-50 text-sm mb-3">';
          html += "<p><strong>Summary:</strong> ";
          if (passesSlender && passesAxial && passesBend && passesComb) {
            html +=
              '<span class="result-good font-semibold">Wall PASSES all simplified ASD checks.</span>';
          } else {
            html +=
              '<span class="result-bad font-semibold">Wall does NOT pass one or more simplified ASD checks.</span>';
          }
          html += "</p>";
          html += "</div>";

          html += '<div class="grid md:grid-cols-2 gap-4 text-sm">';

          // Geometry card
          html += '<div class="border rounded-md p-3">';
          html += '<h3 class="font-semibold mb-2">Geometry & Slenderness</h3>';
          html += '<ul class="space-y-1">';
          html +=
            "<li>Effective height h<sub>eff</sub>: <strong>" +
            formatNumber(h_eff, 3) +
            " m</strong></li>";
          html +=
            "<li>Thickness t: <strong>" +
            formatNumber(t_m, 3) +
            " m</strong></li>";
          html +=
            "<li>Slenderness h<sub>eff</sub>/t: <strong>" +
            formatNumber(slenderness, 1) +
            "</strong> ";
          html += passesSlender
            ? '<span class="result-good">(≤ ' + slenderLimit + ")</span>"
            : '<span class="result-bad">(> ' + slenderLimit + ")</span>";
          html += "</li>";
          html += "</ul>";
          html += "</div>";

          // Axial card
          html += '<div class="border rounded-md p-3">';
          html += '<h3 class="font-semibold mb-2">Axial Compression</h3>';
          html += '<ul class="space-y-1">';
          html +=
            "<li>Axial stress f<sub>a</sub>: <strong>" +
            formatNumber(fa_disp, 2) +
            " " +
            stressUnit +
            "</strong></li>";
          html +=
            "<li>Allowable F<sub>a</sub>: <strong>" +
            formatNumber(Fa_disp, 2) +
            " " +
            stressUnit +
            "</strong></li>";
          html +=
            "<li>Utilization f<sub>a</sub>/F<sub>a</sub>: <strong>" +
            formatNumber(ua, 2) +
            "</strong> ";
          html += passesAxial
            ? '<span class="result-good">(OK)</span>'
            : '<span class="result-bad">(NG)</span>';
          html += "</li>";
          html += "</ul>";
          html += "</div>";

          // Bending card
          html += '<div class="border rounded-md p-3">';
          html += '<h3 class="font-semibold mb-2">Out-of-Plane Bending</h3>';
          html += '<ul class="space-y-1">';
          html +=
            "<li>Bending stress f<sub>b</sub>: <strong>" +
            formatNumber(fb_disp, 2) +
            " " +
            stressUnit +
            "</strong></li>";
          html +=
            "<li>Allowable F<sub>t</sub>: <strong>" +
            formatNumber(Ft_disp, 2) +
            " " +
            stressUnit +
            "</strong></li>";
          html +=
            "<li>Utilization |f<sub>b</sub>|/F<sub>t</sub>: <strong>" +
            formatNumber(ub, 2) +
            "</strong> ";
          html += passesBend
            ? '<span class="result-good">(OK)</span>'
            : '<span class="result-bad">(NG)</span>';
          html += "</li>";
          html += "</ul>";
          html += "</div>";

          // Combined card
          html += '<div class="border rounded-md p-3">';
          html +=
            '<h3 class="font-semibold mb-2">Combined Axial + Bending</h3>';
          html += '<ul class="space-y-1">';
          html +=
            "<li>Interaction: f<sub>a</sub>/F<sub>a</sub> + |f<sub>b</sub>|/F<sub>t</sub> = <strong>" +
            formatNumber(u_comb, 2) +
            "</strong> ";
          html += passesComb
            ? '<span class="result-good">(≤ 1.0 OK)</span>'
            : '<span class="result-bad">(> 1.0 NG)</span>';
          html += "</li>";
          html += "</ul>";
          html +=
            '<p class="mt-2 text-xs text-gray-500">This is a simplified linear interaction check for educational use. Refer to your masonry code for exact interaction equations.</p>';
          html += "</div>";

          html += "</div>";

          resultsContainer.innerHTML = html;
        }

        function resetForm() {
          unitSystemEl.value = "us";
          supportConditionEl.value = "pinned";
          loadCaseEl.value = "service";

          wallHeightEl.value = 10;
          wallLengthEl.value = 12;
          wallThicknessEl.value = 8;
          axialLoadEl.value = 20;
          uniformPressureEl.value = 0.05;
          faAllowEl.value = 250;
          ftAllowEl.value = 30;
          eEl.value = 900;

          updateUnitLabels();
          setDefaultK();
          resultsContainer.innerHTML =
            '<p class="text-gray-500 text-sm">Enter inputs and click “Run Masonry Wall Check” to see slenderness, stresses, and utilization ratios.</p>';
        }

        unitSystemEl.addEventListener("change", () => {
          updateUnitLabels();
        });

        supportConditionEl.addEventListener("change", setDefaultK);
        calcBtn.addEventListener("click", calculate);
        resetBtn.addEventListener("click", resetForm);

        // Initialize
        updateUnitLabels();
        setDefaultK();
      })();
    </script>

