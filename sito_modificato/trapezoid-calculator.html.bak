            <script>
                // Funzione per formattare i numeri
                function formatNum(num, precision = 4) {
                    if (isNaN(num)) return '-';
                    return parseFloat(num.toFixed(precision)).toString();
                }

                // Main calculation function
                function calculateTrapezoid(a, b, c, d, h, p) {
                    const abs_a = Math.abs(a);
                    const abs_b = Math.abs(b);
                    const abs_c = Math.abs(c);
                    const abs_d = Math.abs(d);

                    if (abs_a <= 0 || abs_b <= 0 || abs_c <= 0 || abs_d <= 0) {
                        return { error: 'All side lengths (a, b, c, d) must be greater than zero.' };
                    }
                    if (abs_a === abs_b) {
                        return { error: 'Bases a and b must be of different lengths for a non-parallelogram trapezoid. For a parallelogram, use a=b, but ensure h is provided.' };
                    }
                    if (abs_a > abs_b) { // Ensure 'b' is the longer base for the projection
                        // For simplicity, swap a and b internally for the analysis.
                        return calculateTrapezoid(abs_b, abs_a, abs_c, abs_d, h, p);
                    }

                    const diff_b_a = abs_b - abs_a;
                    let calculated_h = h;
                    let type = 'Generic';

                    // --- Area and Perimeter ---
                    const perimeter = abs_a + abs_b + abs_c + abs_d;
                    let area = null;

                    // --- Analyze Type ---
                    if (Math.abs(abs_c - abs_d) < 1e-9) {
                        type = 'Isosceles';
                    }

                    // --- Calculate Height (if not provided) ---
                    if (isNaN(calculated_h) || calculated_h <= 0) {
                        // If it is not isosceles, h is hard to compute from sides alone
                        if (type === 'Isosceles') {
                            const x = diff_b_a / 2; // Proiezione del lato obliquo
                            if (abs_c * abs_c < x * x) {
                                return { error: 'Side c is too short for an Isosceles trapezoid with these bases.' };
                            }
                            calculated_h = Math.sqrt(abs_c * abs_c - x * x);
                            
                        } else {
                            // Formula complessa per trapezio scaleno (usando area dei triangoli)
                            const s = (abs_b - abs_a + abs_c + abs_d) / 2;
                            const term1 = Math.sqrt(s * (s - abs_c) * (s - abs_d) * (s - (abs_b - abs_a)));
                            calculated_h = (2 / (abs_b - abs_a)) * term1;
                        }
                    }

                    // --- Final Area Calculation ---
                    if (!isNaN(calculated_h) && calculated_h > 0) {
                        area = ((abs_a + abs_b) / 2) * calculated_h;
                    }
                    
                    // --- Calculate Angles (using calculated_h) ---
                    let alpha, beta, gamma, delta;
                    
                    // Proiezione sinistra
                    const x_left = Math.sqrt(abs_c * abs_c - calculated_h * calculated_h);
                    alpha = Math.acos(x_left / abs_c) * (180 / Math.PI); // Angolo tra c e a/b
                    beta = 180 - alpha; // The angle adjacent to base 'b' is supplementary

                    // Proiezione destra
                    const x_right = Math.sqrt(abs_d * abs_d - calculated_h * calculated_h);
                    delta = Math.acos(x_right / abs_d) * (180 / Math.PI);
                    gamma = 180 - delta;
                    
                    // Check if it is a right trapezoid
                    if (Math.abs(x_left) < 1e-9 || Math.abs(x_right) < 1e-9) {
                        type = 'Right (90°)';
                    }

                    return {
                        area: area,
                        perimeter: perimeter,
                        height: calculated_h,
                        type: type,
                        angles: { alpha: alpha, beta: beta, gamma: gamma, delta: delta },
                        // Diagonals and other properties require a more complex coordinate approach; we skip it for now
                    };
                }


                const inputFields = {
                    a: document.getElementById('base-a-input'),
                    b: document.getElementById('base-b-input'),
                    c: document.getElementById('side-c-input'),
                    d: document.getElementById('side-d-input'),
                    h: document.getElementById('height-h-input'),
                    p: document.getElementById('precision-input')
                };
                const outputFields = {
                    area: document.getElementById('area-output'),
                    perimeter: document.getElementById('perimeter-output'),
                    height: document.getElementById('height-output'),
                    type: document.getElementById('type-output'),
                    alpha: document.getElementById('angle-alpha-output'),
                    beta: document.getElementById('angle-beta-output'),
                    gamma: document.getElementById('angle-gamma-output'),
                    delta: document.getElementById('angle-delta-output'),
                    resultsBox: document.getElementById('results-output'),
                    errorBox: document.getElementById('input-error')
                };

                function displayError(message) {
                    outputFields.resultsBox.hidden = true;
                    outputFields.errorBox.textContent = message;
                    outputFields.errorBox.hidden = false;
                }

                function displayResults(results, precision) {
                    outputFields.errorBox.hidden = true;
                    outputFields.resultsBox.hidden = false;
                    
                    outputFields.area.textContent = formatNum(results.area, precision);
                    outputFields.perimeter.textContent = formatNum(results.perimeter, precision);
                    outputFields.height.textContent = formatNum(results.height, precision);
                    outputFields.type.textContent = results.type;

                    outputFields.alpha.textContent = formatNum(results.angles.alpha, precision) + '°';
                    outputFields.beta.textContent = formatNum(results.angles.beta, precision) + '°';
                    outputFields.gamma.textContent = formatNum(results.angles.gamma, precision) + '°';
                    outputFields.delta.textContent = formatNum(results.angles.delta, precision) + '°';
                }

                document.getElementById('calculate-btn').addEventListener('click', () => {
                    const a = parseFloat(inputFields.a.value);
                    const b = parseFloat(inputFields.b.value);
                    const c = parseFloat(inputFields.c.value);
                    const d = parseFloat(inputFields.d.value);
                    const h = parseFloat(inputFields.h.value);
                    const p = parseInt(inputFields.p.value) || 4;

                    const results = calculateTrapezoid(a, b, c, d, h, p);

                    if (results.error) {
                        displayError(results.error);
                    } else {
                        displayResults(results, p);
                    }
                });
                
                // Set default example (Isosceles trapezoid)
                inputFields.a.value = 10;
                inputFields.b.value = 14;
                inputFields.c.value = 5;
                inputFields.d.value = 5;
                inputFields.h.value = 4; // Not strictly needed, but good for quick check
                document.getElementById('calculate-btn').click();
            });
            </script>
            
