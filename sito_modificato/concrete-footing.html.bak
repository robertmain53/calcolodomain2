<script>
  document.addEventListener('DOMContentLoaded', function () {
    document.querySelectorAll('#faq .faq-question').forEach(function (btn) {
      btn.addEventListener('click', function () {
        const answer = btn.parentElement.querySelector('.faq-answer');
        const icon = btn.querySelector('span:last-child');
        const isHidden = answer.classList.contains('hidden');
        document.querySelectorAll('#faq .faq-answer').forEach(a => a.classList.add('hidden'));
        document.querySelectorAll('#faq .faq-question span:last-child').forEach(i => i.textContent = '+');
        if (isHidden) {
          answer.classList.remove('hidden');
          icon.textContent = '−';
        }
      });
    });
  });

  // Concrete footing calculator logic
  document.addEventListener('DOMContentLoaded', function () {
    const unitRadios = document.querySelectorAll('.unit-radio');
    const shapeSelect = document.getElementById('shape');
    const rectInputs = document.getElementById('rectInputs');
    const roundInputs = document.getElementById('roundInputs');

    const deadLoad = document.getElementById('deadLoad');
    const liveLoad = document.getElementById('liveLoad');
    const numFootings = document.getElementById('numFootings');
    const soilCapacity = document.getElementById('soilCapacity');
    const safetyFactor = document.getElementById('safetyFactor');

    const width = document.getElementById('width');
    const length = document.getElementById('length');
    const thickness = document.getElementById('thickness');
    const diameter = document.getElementById('diameter');
    const thicknessRound = document.getElementById('thicknessRound');

    const calcBtn = document.getElementById('calcBtn');
    const resetBtn = document.getElementById('resetBtn');
    const statusMsg = document.getElementById('statusMsg');
    const results = document.getElementById('results');

    const resTotalLoad = document.getElementById('resTotalLoad');
    const resReqArea = document.getElementById('resReqArea');
    const resActualArea = document.getElementById('resActualArea');
    const resBearing = document.getElementById('resBearing');
    const bearingCheck = document.getElementById('bearingCheck');
    const resVolPer = document.getElementById('resVolPer');
    const resVolTotal = document.getElementById('resVolTotal');
    const resBags = document.getElementById('resBags');
    const rebarSuggestion = document.getElementById('rebarSuggestion');

    const unitLabels = {
      deadLoadUnit: document.getElementById('deadLoadUnit'),
      liveLoadUnit: document.getElementById('liveLoadUnit'),
      soilCapacityUnit: document.getElementById('soilCapacityUnit'),
      widthUnit: document.getElementById('widthUnit'),
      lengthUnit: document.getElementById('lengthUnit'),
      thicknessUnit: document.getElementById('thicknessUnit'),
      diameterUnit: document.getElementById('diameterUnit'),
      thicknessRoundUnit: document.getElementById('thicknessRoundUnit')
    };

    function getUnitSystem() {
      return document.querySelector('.unit-radio:checked').value;
    }

    function updateUnitLabels() {
      const u = getUnitSystem();
      if (u === 'imperial') {
        unitLabels.deadLoadUnit.textContent = 'lb';
        unitLabels.liveLoadUnit.textContent = 'lb';
        unitLabels.soilCapacityUnit.textContent = 'psf';
        unitLabels.widthUnit.textContent = 'ft';
        unitLabels.lengthUnit.textContent = 'ft';
        unitLabels.thicknessUnit.textContent = 'ft';
        unitLabels.diameterUnit.textContent = 'ft';
        unitLabels.thicknessRoundUnit.textContent = 'ft';
        if (!results.classList.contains('hidden')) {
          calculate(); // refresh results in new units
        }
      } else {
        unitLabels.deadLoadUnit.textContent = 'kN';
        unitLabels.liveLoadUnit.textContent = 'kN';
        unitLabels.soilCapacityUnit.textContent = 'kPa';
        unitLabels.widthUnit.textContent = 'm';
        unitLabels.lengthUnit.textContent = 'm';
        unitLabels.thicknessUnit.textContent = 'm';
        unitLabels.diameterUnit.textContent = 'm';
        unitLabels.thicknessRoundUnit.textContent = 'm';
        if (!results.classList.contains('hidden')) {
          calculate();
        }
      }
    }

    unitRadios.forEach(r => r.addEventListener('change', updateUnitLabels));

    shapeSelect.addEventListener('change', function () {
      if (shapeSelect.value === 'square') {
        rectInputs.classList.remove('hidden');
        roundInputs.classList.add('hidden');
      } else {
        rectInputs.classList.add('hidden');
        roundInputs.classList.remove('hidden');
      }
    });

    function parsePositive(value) {
      const v = parseFloat(value);
      return isFinite(v) && v > 0 ? v : NaN;
    }

    function calculate() {
      const u = getUnitSystem();
      statusMsg.textContent = '';
      results.classList.add('hidden');

      const Pdead = parsePositive(deadLoad.value);
      const Plive = parsePositive(liveLoad.value);
      const nFoot = parsePositive(numFootings.value);
      const qAllow = parsePositive(soilCapacity.value);
      const SF = parsePositive(safetyFactor.value);

      if (isNaN(Pdead) || isNaN(Plive) || isNaN(nFoot) || isNaN(qAllow) || isNaN(SF)) {
        statusMsg.textContent = 'Please enter valid positive values for loads, soil capacity, safety factor, and number of footings.';
        statusMsg.classList.remove('text-green-700');
        statusMsg.classList.add('text-red-600');
        return;
      }

      let B, L, t, D;
      if (shapeSelect.value === 'square') {
        B = parsePositive(width.value);
        L = parsePositive(length.value);
        t = parsePositive(thickness.value);
        if (isNaN(B) || isNaN(L) || isNaN(t)) {
          statusMsg.textContent = 'Please enter valid footing width, length, and thickness.';
          statusMsg.classList.remove('text-green-700');
          statusMsg.classList.add('text-red-600');
          return;
        }
      } else {
        D = parsePositive(diameter.value);
        t = parsePositive(thicknessRound.value);
        if (isNaN(D) || isNaN(t)) {
          statusMsg.textContent = 'Please enter valid footing diameter and thickness.';
          statusMsg.classList.remove('text-green-700');
          statusMsg.classList.add('text-red-600');
          return;
        }
      }

      const P = Pdead + Plive; // total load per footing

      // Convert to consistent base units
      let Pbase, qAllowBase, areaUnitLabel, pressureUnitLabel, volUnitLabel, volTotalLabel;
      if (u === 'imperial') {
        Pbase = P; // lb
        qAllowBase = qAllow; // psf
        areaUnitLabel = 'ft²';
        pressureUnitLabel = 'psf';
        volUnitLabel = 'ft³';
        volTotalLabel = 'yd³';
      } else {
        Pbase = P * 1000; // kN -> N (approx, 1 kN = 1000 N)
        qAllowBase = qAllow * 1000; // kPa -> N/m² (1 kPa = 1000 N/m²)
        areaUnitLabel = 'm²';
        pressureUnitLabel = 'kPa';
        volUnitLabel = 'm³';
        volTotalLabel = 'm³';
      }

      const qDesign = qAllow / SF;
      const Areq = P / qDesign; // in ft² or m² directly in user units

      let Aactual, Vper;
      if (shapeSelect.value === 'square') {
        Aactual = B * L;
        Vper = B * L * t;
      } else {
        const radius = D / 2;
        Aactual = Math.PI * radius * radius;
        Vper = Aactual * t;
      }

      const qActual = P / Aactual;

      // Volume total and bags
      const Vtotal = Vper * nFoot;
      let bags;
      if (u === 'imperial') {
        // 80 lb bag ≈ 0.6 ft³
        bags = Vtotal / 0.6;
      } else {
        // 25 kg bag ≈ 0.012 m³ (approx)
        bags = Vtotal / 0.012;
      }

      // Populate results
      resTotalLoad.textContent = u === 'imperial'
        ? P.toFixed(0) + ' lb per footing'
        : P.toFixed(2) + ' kN per footing';

      resReqArea.textContent = Areq.toFixed(3) + ' ' + areaUnitLabel;
      resActualArea.textContent = Aactual.toFixed(3) + ' ' + areaUnitLabel;

      resBearing.textContent = u === 'imperial'
        ? qActual.toFixed(0) + ' psf'
        : qActual.toFixed(1) + ' kPa';

      const ok = qActual <= qDesign;
      bearingCheck.textContent = ok
        ? 'OK: Bearing pressure is within allowable limits (with safety factor).'
        : 'Warning: Bearing pressure exceeds allowable limit. Increase footing size or improve soil.';
      bearingCheck.classList.toggle('text-green-700', ok);
      bearingCheck.classList.toggle('text-red-600', !ok);

      if (u === 'imperial') {
        const VtotalYd3 = Vtotal / 27;
        resVolPer.textContent = Vper.toFixed(2) + ' ft³';
        resVolTotal.textContent = Vtotal.toFixed(2) + ' ft³ (' + VtotalYd3.toFixed(2) + ' yd³)';
      } else {
        resVolPer.textContent = Vper.toFixed(3) + ' m³';
        resVolTotal.textContent = Vtotal.toFixed(3) + ' m³';
      }

      resBags.textContent = Math.ceil(bags) + (u === 'imperial' ? ' bags of 80 lb mix (approx.)' : ' bags of 25 kg mix (approx.)');

      // Rebar suggestion (very simplified)
      let suggestion;
      if (shapeSelect.value === 'square') {
        if (u === 'imperial') {
          suggestion = 'Place at least 2–3 #4 bars (1/2 in) each way near the bottom of the footing, with about 3 in cover from soil and edges. For larger footings, use 4 bars each way.';
        } else {
          suggestion = 'Place at least 2–3 12 mm bars each way near the bottom of the footing, with about 75 mm cover from soil and edges. For larger footings, use 4 bars each way.';
        }
      } else {
        if (u === 'imperial') {
          suggestion = 'Use 3–4 #4 bars radiating from the center plus a small #3 or #4 ring near the perimeter, all with about 3 in cover from soil and edges.';
        } else {
          suggestion = 'Use 3–4 12 mm bars radiating from the center plus a small 10–12 mm ring near the perimeter, all with about 75 mm cover from soil and edges.';
        }
      }
      rebarSuggestion.textContent = suggestion;

      results.classList.remove('hidden');
      statusMsg.textContent = 'Calculation updated.';
      statusMsg.classList.remove('text-red-600');
      statusMsg.classList.add('text-green-700');
    }

    calcBtn.addEventListener('click', calculate);

    resetBtn.addEventListener('click', function () {
      deadLoad.value = 2000;
      liveLoad.value = 1500;
      numFootings.value = 4;
      soilCapacity.value = 2000;
      safetyFactor.value = 1.5;
      width.value = 2;
      length.value = 2;
      thickness.value = 0.75;
      diameter.value = 2;
      thicknessRound.value = 0.75;
      shapeSelect.value = 'square';
      rectInputs.classList.remove('hidden');
      roundInputs.classList.add('hidden');
      statusMsg.textContent = '';
      results.classList.add('hidden');
    });
  });

  window.MathJax = {
    tex: { inlineMath: [['\\(','\\)'], ['$', '$']], displayMath: [['$','$'], ['\\[','\\]']] },
    svg: { fontCache: 'global' }
  };
</script>
