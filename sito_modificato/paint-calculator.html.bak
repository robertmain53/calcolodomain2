    <script>
      // Paint calculator logic
      (function () {
        const roomsContainer = document.getElementById("roomsContainer");
        const roomTemplate = document.getElementById("roomTemplate");
        const addRoomBtn = document.getElementById("addRoomBtn");
        const unitButtons = document.querySelectorAll(".unit-toggle");
        const coverageInput = document.getElementById("coverageInput");
        const coverageUnitLabel = document.getElementById("coverageUnitLabel");
        const wastePercentInput = document.getElementById("wastePercent");

        const totalWallAreaEl = document.getElementById("totalWallArea");
        const totalCeilingAreaEl = document.getElementById("totalCeilingArea");
        const totalTrimLengthEl = document.getElementById("totalTrimLength");

        const areaUnitLabel = document.getElementById("areaUnitLabel");
        const areaUnitLabel2 = document.getElementById("areaUnitLabel2");
        const lengthUnitLabel = document.getElementById("lengthUnitLabel");

        const paintWallsGallonsEl =
          document.getElementById("paintWallsGallons");
        const paintWallsLitersEl = document.getElementById("paintWallsLiters");
        const paintCeilingsGallonsEl = document.getElementById(
          "paintCeilingsGallons"
        );
        const paintCeilingsLitersEl = document.getElementById(
          "paintCeilingsLiters"
        );
        const paintTrimGallonsEl = document.getElementById("paintTrimGallons");
        const paintTrimLitersEl = document.getElementById("paintTrimLiters");
        const paintTotalGallonsEl =
          document.getElementById("paintTotalGallons");
        const paintTotalLitersEl = document.getElementById("paintTotalLiters");

        const canBreakdownImperialEl = document.getElementById(
          "canBreakdownImperial"
        );
        const canBreakdownMetricEl =
          document.getElementById("canBreakdownMetric");

        let currentUnits = "imperial"; // 'imperial' or 'metric'

        function addRoom(initial) {
          const clone = roomTemplate.content.cloneNode(true);
          roomsContainer.appendChild(clone);
          updateRoomIndices();
          if (!initial) recalc();
        }

        function updateRoomIndices() {
          const cards = roomsContainer.querySelectorAll(".room-card");
          cards.forEach((card, idx) => {
            const indexSpan = card.querySelector(".room-index");
            if (indexSpan) indexSpan.textContent = idx + 1;
          });
          updateUnitLabels();
        }

        function getRoomsData() {
          const cards = roomsContainer.querySelectorAll(".room-card");
          const rooms = [];
          cards.forEach((card) => {
            const getVal = (selector, fallback = 0) => {
              const el = card.querySelector(selector);
              const v = parseFloat(el && el.value);
              return isNaN(v) ? fallback : v;
            };

            const perimeter = getVal(".room-perimeter");
            const height = getVal(".room-height");
            const ceilingLength = getVal(".room-ceiling-length");
            const ceilingWidth = getVal(".room-ceiling-width");
            const doorsCount = getVal(".room-doors-count");
            const doorWidth = getVal(".room-door-width");
            const doorHeight = getVal(".room-door-height");
            const windowsCount = getVal(".room-windows-count");
            const windowWidth = getVal(".room-window-width");
            const windowHeight = getVal(".room-window-height");
            const trimLength = getVal(".room-trim-length");
            const wallCoats = Math.max(0, getVal(".room-wall-coats", 0));
            const ceilingCoats = Math.max(0, getVal(".room-ceiling-coats", 0));
            const trimCoats = Math.max(0, getVal(".room-trim-coats", 0));

            const wallArea = perimeter * height;
            const openingsArea =
              doorsCount * doorWidth * doorHeight +
              windowsCount * windowWidth * windowHeight;
            const netWallArea = Math.max(0, wallArea - openingsArea);
            const ceilingArea =
              ceilingLength > 0 && ceilingWidth > 0
                ? ceilingLength * ceilingWidth
                : 0;

            rooms.push({
              card,
              perimeter,
              height,
              wallArea,
              openingsArea,
              netWallArea,
              ceilingArea,
              trimLength,
              wallCoats,
              ceilingCoats,
              trimCoats,
            });
          });
          return rooms;
        }

        function recalc() {
          const coverage = parseFloat(coverageInput.value);
          const wastePercent = parseFloat(wastePercentInput.value) || 0;
          const rooms = getRoomsData();

          let totalWallArea = 0;
          let totalCeilingArea = 0;
          let totalTrimLength = 0;
          let wallAreaCoated = 0;
          let ceilingAreaCoated = 0;
          let trimAreaCoated = 0;

          const trimHeightEquivalent = currentUnits === "imperial" ? 0.5 : 0.15; // 6 in or 0.15 m

          rooms.forEach((room) => {
            totalWallArea += room.netWallArea;
            totalCeilingArea += room.ceilingArea;
            totalTrimLength += room.trimLength;

            wallAreaCoated += room.netWallArea * room.wallCoats;
            ceilingAreaCoated += room.ceilingArea * room.ceilingCoats;
            trimAreaCoated +=
              room.trimLength * trimHeightEquivalent * room.trimCoats;

            const unitArea = currentUnits === "imperial" ? "ft²" : "m²";
            const unitLen = currentUnits === "imperial" ? "ft" : "m";
            const summary = `Walls: ${room.netWallArea.toFixed(
              1
            )} ${unitArea}, Ceilings: ${room.ceilingArea.toFixed(
              1
            )} ${unitArea}, Trim: ${room.trimLength.toFixed(1)} ${unitLen}`;
            const summaryEl = room.card.querySelector(".room-summary");
            if (summaryEl) summaryEl.textContent = summary;
          });

          totalWallAreaEl.textContent = totalWallArea.toFixed(1);
          totalCeilingAreaEl.textContent = totalCeilingArea.toFixed(1);
          totalTrimLengthEl.textContent = totalTrimLength.toFixed(1);

          if (!coverage || coverage <= 0) {
            paintWallsGallonsEl.textContent =
              paintWallsLitersEl.textContent =
              paintCeilingsGallonsEl.textContent =
              paintCeilingsLitersEl.textContent =
              paintTrimGallonsEl.textContent =
              paintTrimLitersEl.textContent =
              paintTotalGallonsEl.textContent =
              paintTotalLitersEl.textContent =
                "0.00";
            canBreakdownImperialEl.textContent = "0 × 1 gal, 0 × 1 qt (US)";
            canBreakdownMetricEl.textContent = "0 × 10 L, 0 × 2.5 L";
            return;
          }

          const wasteFactor = 1 + wastePercent / 100;

          const wallGallons = (wallAreaCoated / coverage) * wasteFactor;
          const ceilingGallons = (ceilingAreaCoated / coverage) * wasteFactor;
          const trimGallons = (trimAreaCoated / coverage) * wasteFactor;

          const totalGallons = wallGallons + ceilingGallons + trimGallons;
          const gallonsToLiters = 3.78541;
          const totalLiters = totalGallons * gallonsToLiters;

          paintWallsGallonsEl.textContent = wallGallons.toFixed(2);
          paintWallsLitersEl.textContent = (
            wallGallons * gallonsToLiters
          ).toFixed(2);
          paintCeilingsGallonsEl.textContent = ceilingGallons.toFixed(2);
          paintCeilingsLitersEl.textContent = (
            ceilingGallons * gallonsToLiters
          ).toFixed(2);
          paintTrimGallonsEl.textContent = trimGallons.toFixed(2);
          paintTrimLitersEl.textContent = (
            trimGallons * gallonsToLiters
          ).toFixed(2);
          paintTotalGallonsEl.textContent = totalGallons.toFixed(2);
          paintTotalLitersEl.textContent = totalLiters.toFixed(2);

          // Can breakdown (imperial)
          const gal = totalGallons;
          const num1Gal = Math.floor(gal);
          const remainingGal = gal - num1Gal;
          const numQuarts = Math.ceil(remainingGal * 4); // 4 quarts per gallon
          canBreakdownImperialEl.textContent = `${num1Gal} × 1 gal, ${numQuarts} × 1 qt (US)`;

          // Can breakdown (metric) - 10 L and 2.5 L
          let litersLeft = totalLiters;
          const num10L = Math.floor(litersLeft / 10);
          litersLeft -= num10L * 10;
          const num2_5L = Math.ceil(litersLeft / 2.5);
          canBreakdownMetricEl.textContent = `${num10L} × 10 L, ${num2_5L} × 2.5 L`;
        }

        function updateUnitLabels() {
          const isImperial = currentUnits === "imperial";
          areaUnitLabel.textContent = areaUnitLabel2.textContent = isImperial
            ? "ft²"
            : "m²";
          lengthUnitLabel.textContent = isImperial ? "ft" : "m";
          coverageUnitLabel.textContent = isImperial
            ? "ft² per gallon"
            : "m² per liter";

          const cards = roomsContainer.querySelectorAll(".room-card");
          cards.forEach((card) => {
            card
              .querySelectorAll(
                ".unit-label-perimeter, .unit-label-length, .unit-label-width, .unit-label-height"
              )
              .forEach((el) => {
                el.textContent = el.classList.contains("unit-label-height")
                  ? isImperial
                    ? "ft"
                    : "m"
                  : isImperial
                  ? "ft"
                  : "m";
              });
          });

          recalc();
        }

        unitButtons.forEach((btn) => {
          btn.addEventListener("click", () => {
            const units = btn.getAttribute("data-units");
            if (units === currentUnits) return;
            currentUnits = units;
            unitButtons.forEach((b) => {
              if (b.getAttribute("data-units") === units) {
                b.classList.add("bg-blue-600", "text-white", "border-blue-600");
                b.classList.remove(
                  "bg-white",
                  "text-gray-700",
                  "border-gray-300"
                );
              } else {
                b.classList.remove(
                  "bg-blue-600",
                  "text-white",
                  "border-blue-600"
                );
                b.classList.add("bg-white", "text-gray-700", "border-gray-300");
              }
            });

            // Convert existing numeric values between systems
            const factorLength = 0.3048; // ft to m
            const cards = roomsContainer.querySelectorAll(".room-card");
            cards.forEach((card) => {
              const convertField = (selector) => {
                const el = card.querySelector(selector);
                if (!el) return;
                const v = parseFloat(el.value);
                if (isNaN(v) || v === 0) return;
                if (units === "metric") {
                  el.value = (v * factorLength).toFixed(2);
                } else {
                  el.value = (v / factorLength).toFixed(2);
                }
              };
              convertField(".room-perimeter");
              convertField(".room-height");
              convertField(".room-ceiling-length");
              convertField(".room-ceiling-width");
              convertField(".room-door-width");
              convertField(".room-door-height");
              convertField(".room-window-width");
              convertField(".room-window-height");
              convertField(".room-trim-length");
            });

            // Convert coverage
            const cov = parseFloat(coverageInput.value);
            if (!isNaN(cov) && cov > 0) {
              if (units === "metric") {
                coverageInput.value = (cov * 0.092903).toFixed(1); // ft²/gal -> m²/gal approx, but we want m²/L; assume 1 gal ≈ 3.785 L
                // More precise: (ft²/gal) * (0.092903 m²/ft²) / 3.785 L/gal
                coverageInput.value = ((cov * 0.092903) / 3.78541).toFixed(1);
              } else {
                // m²/L -> ft²/gal
                coverageInput.value = ((cov * 3.78541) / 0.092903).toFixed(0);
              }
            }

            updateUnitLabels();
          });
        });

        roomsContainer.addEventListener("input", function (e) {
          if (e.target.matches("input")) recalc();
        });
        coverageInput.addEventListener("input", recalc);
        wastePercentInput.addEventListener("input", recalc);

        roomsContainer.addEventListener("click", function (e) {
          if (e.target.classList.contains("remove-room")) {
            const card = e.target.closest(".room-card");
            if (card) {
              card.remove();
              updateRoomIndices();
              recalc();
            }
          }
        });

        addRoomBtn.addEventListener("click", function () {
          addRoom(false);
        });

        // Initialize with one room
        addRoom(true);
        recalc();
      })();
    </script>

