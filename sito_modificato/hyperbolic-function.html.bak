    <script>
      // Mobile menu toggle
      const mobileToggle = document.getElementById("mobile-menu-toggle");
      const mobileMenu = document.getElementById("mobile-menu");
      if (mobileToggle && mobileMenu) {
        mobileToggle.addEventListener("click", () => {
          const expanded =
            mobileToggle.getAttribute("aria-expanded") === "true";
          mobileToggle.setAttribute("aria-expanded", String(!expanded));
          mobileMenu.classList.toggle("hidden");
        });
      }

      const fnSelect = document.getElementById("hyperbolic-fn-select");
      const xInput = document.getElementById("x-input");
      const calcBtn = document.getElementById("hyperbolic-calc-btn");
      const clearBtn = document.getElementById("hyperbolic-clear-btn");
      const resultEl = document.getElementById("hyperbolic-result");

      function formatNumber(x) {
        if (!isFinite(x)) return "NaN";
        const absx = Math.abs(x);
        if (absx !== 0 && (absx < 1e-6 || absx > 1e8)) {
          return x.toExponential(6);
        }
        return x.toFixed(6).replace(/\.?0+$/, "");
      }

      function showCalcError(msg) {
        if (!resultEl) return;
        resultEl.innerHTML = '<p class="text-red-600">' + msg + "</p>";
      }

      function calculateHyperbolic() {
        if (!fnSelect || !xInput || !resultEl) return;

        const fn = fnSelect.value;
        const xVal = parseFloat(xInput.value);

        if (!isFinite(xVal)) {
          showCalcError("Please enter a valid real number for x.");
          return;
        }

        const x = xVal;
        const ex = Math.exp(x);
        const e_minus_x = Math.exp(-x);

        // basic hyperbolic
        const sinh = (ex - e_minus_x) / 2;
        const cosh = (ex + e_minus_x) / 2;
        const tanh = cosh !== 0 ? sinh / cosh : NaN;
        const sech = cosh !== 0 ? 1 / cosh : NaN;
        const csch = sinh !== 0 ? 1 / sinh : NaN;
        const coth = sinh !== 0 ? cosh / sinh : NaN;

        let value = NaN;
        let identityDetails = "";
        let domainNote = "";
        let compositionNote = "";

        if (fn === "sinh") {
          value = sinh;
          const viaExp = (ex - e_minus_x) / 2;
          const diff = Math.abs(sinh - viaExp);
          identityDetails =
            "<p>Definition check: sinh(x) = (e^x − e^{−x}) / 2</p>" +
            '<ul class="list-disc ml-5">' +
            '<li>e^x ≈ <span class="font-mono">' +
            formatNumber(ex) +
            "</span></li>" +
            '<li>e^{−x} ≈ <span class="font-mono">' +
            formatNumber(e_minus_x) +
            "</span></li>" +
            '<li>(e^x − e^{−x}) / 2 ≈ <span class="font-mono">' +
            formatNumber(viaExp) +
            "</span></li>" +
            '<li>|sinh(x) − definition| ≈ <span class="font-mono">' +
            formatNumber(diff) +
            "</span></li>" +
            "</ul>";
        } else if (fn === "cosh") {
          value = cosh;
          const viaExp = (ex + e_minus_x) / 2;
          const diff = Math.abs(cosh - viaExp);
          identityDetails =
            "<p>Definition check: cosh(x) = (e^x + e^{−x}) / 2</p>" +
            '<ul class="list-disc ml-5">' +
            '<li>e^x ≈ <span class="font-mono">' +
            formatNumber(ex) +
            "</span></li>" +
            '<li>e^{−x} ≈ <span class="font-mono">' +
            formatNumber(e_minus_x) +
            "</span></li>" +
            '<li>(e^x + e^{−x}) / 2 ≈ <span class="font-mono">' +
            formatNumber(viaExp) +
            "</span></li>" +
            '<li>|cosh(x) − definition| ≈ <span class="font-mono">' +
            formatNumber(diff) +
            "</span></li>" +
            "</ul>";
        } else if (fn === "tanh") {
          value = tanh;
          const viaDef = cosh !== 0 ? sinh / cosh : NaN;
          const diff = Math.abs(value - viaDef);
          identityDetails =
            "<p>Definition check: tanh(x) = sinh(x) / cosh(x)</p>" +
            '<ul class="list-disc ml-5">' +
            '<li>sinh(x) ≈ <span class="font-mono">' +
            formatNumber(sinh) +
            "</span></li>" +
            '<li>cosh(x) ≈ <span class="font-mono">' +
            formatNumber(cosh) +
            "</span></li>" +
            '<li>sinh(x) / cosh(x) ≈ <span class="font-mono">' +
            formatNumber(viaDef) +
            "</span></li>" +
            '<li>|tanh(x) − definition| ≈ <span class="font-mono">' +
            formatNumber(diff) +
            "</span></li>" +
            "</ul>";
        } else if (fn === "sech") {
          value = sech;
          identityDetails =
            "<p>Definition: sech(x) = 1 / cosh(x)</p>" +
            '<ul class="list-disc ml-5">' +
            '<li>cosh(x) ≈ <span class="font-mono">' +
            formatNumber(cosh) +
            "</span></li>" +
            '<li>sech(x) ≈ <span class="font-mono">' +
            formatNumber(sech) +
            "</span></li>" +
            "</ul>";
          if (cosh === 0) {
            domainNote = "cosh(x) = 0, so sech(x) is undefined.";
          }
        } else if (fn === "csch") {
          value = csch;
          identityDetails =
            "<p>Definition: csch(x) = 1 / sinh(x)</p>" +
            '<ul class="list-disc ml-5">' +
            '<li>sinh(x) ≈ <span class="font-mono">' +
            formatNumber(sinh) +
            "</span></li>" +
            '<li>csch(x) ≈ <span class="font-mono">' +
            formatNumber(csch) +
            "</span></li>" +
            "</ul>";
          if (sinh === 0) {
            domainNote = "sinh(x) = 0, so csch(x) is undefined at this x.";
          }
        } else if (fn === "coth") {
          value = coth;
          identityDetails =
            "<p>Definition: coth(x) = cosh(x) / sinh(x)</p>" +
            '<ul class="list-disc ml-5">' +
            '<li>sinh(x) ≈ <span class="font-mono">' +
            formatNumber(sinh) +
            "</span></li>" +
            '<li>cosh(x) ≈ <span class="font-mono">' +
            formatNumber(cosh) +
            "</span></li>" +
            '<li>coth(x) ≈ <span class="font-mono">' +
            formatNumber(coth) +
            "</span></li>" +
            "</ul>";
          if (sinh === 0) {
            domainNote = "sinh(x) = 0, so coth(x) has a singularity at this x.";
          }
        } else if (fn === "asinh") {
          const val = Math.asinh(x);
          value = val;
          const viaLog = Math.log(x + Math.sqrt(x * x + 1));
          const diff = Math.abs(val - viaLog);
          const back = Math.sinh(val);
          const compDiff = Math.abs(back - x);
          identityDetails =
            "<p>Inverse definition: asinh(x) is the inverse of sinh(x).</p>" +
            '<ul class="list-disc ml-5">' +
            '<li>asinh(x) (built-in) ≈ <span class="font-mono">' +
            formatNumber(val) +
            "</span></li>" +
            '<li>ln(x + √(x² + 1)) ≈ <span class="font-mono">' +
            formatNumber(viaLog) +
            "</span></li>" +
            '<li>|asinh(x) − log-formula| ≈ <span class="font-mono">' +
            formatNumber(diff) +
            "</span></li>" +
            '<li>sinh(asinh(x)) ≈ <span class="font-mono">' +
            formatNumber(back) +
            "</span></li>" +
            '<li>|sinh(asinh(x)) − x| ≈ <span class="font-mono">' +
            formatNumber(compDiff) +
            "</span></li>" +
            "</ul>";
        } else if (fn === "acosh") {
          if (x < 1) {
            showCalcError("acosh(x) is only defined for x ≥ 1 (real values).");
            return;
          }
          const val = Math.acosh(x);
          value = val;
          const viaLog = Math.log(x + Math.sqrt(x * x - 1));
          const diff = Math.abs(val - viaLog);
          const back = Math.cosh(val);
          const compDiff = Math.abs(back - x);
          identityDetails =
            "<p>Inverse definition: acosh(x) is the inverse of cosh(x) on x ≥ 1.</p>" +
            '<ul class="list-disc ml-5">' +
            '<li>acosh(x) (built-in) ≈ <span class="font-mono">' +
            formatNumber(val) +
            "</span></li>" +
            '<li>ln(x + √(x² − 1)) ≈ <span class="font-mono">' +
            formatNumber(viaLog) +
            "</span></li>" +
            '<li>|acosh(x) − log-formula| ≈ <span class="font-mono">' +
            formatNumber(diff) +
            "</span></li>" +
            '<li>cosh(acosh(x)) ≈ <span class="font-mono">' +
            formatNumber(back) +
            "</span></li>" +
            '<li>|cosh(acosh(x)) − x| ≈ <span class="font-mono">' +
            formatNumber(compDiff) +
            "</span></li>" +
            "</ul>";
          domainNote = "Domain: x ≥ 1 for real acosh(x).";
        } else if (fn === "atanh") {
          if (Math.abs(x) >= 1) {
            showCalcError(
              "atanh(x) is only defined for |x| < 1 (real values)."
            );
            return;
          }
          const val = Math.atanh(x);
          value = val;
          const viaLog = 0.5 * Math.log((1 + x) / (1 - x));
          const diff = Math.abs(val - viaLog);
          const back = Math.tanh(val);
          const compDiff = Math.abs(back - x);
          identityDetails =
            "<p>Inverse definition: atanh(x) is the inverse of tanh(x) on |x| &lt; 1.</p>" +
            '<ul class="list-disc ml-5">' +
            '<li>atanh(x) (built-in) ≈ <span class="font-mono">' +
            formatNumber(val) +
            "</span></li>" +
            '<li>(1/2) · ln((1 + x) / (1 − x)) ≈ <span class="font-mono">' +
            formatNumber(viaLog) +
            "</span></li>" +
            '<li>|atanh(x) − log-formula| ≈ <span class="font-mono">' +
            formatNumber(diff) +
            "</span></li>" +
            '<li>tanh(atanh(x)) ≈ <span class="font-mono">' +
            formatNumber(back) +
            "</span></li>" +
            '<li>|tanh(atanh(x)) − x| ≈ <span class="font-mono">' +
            formatNumber(compDiff) +
            "</span></li>" +
            "</ul>";
          domainNote = "Domain: |x| &lt; 1 for real atanh(x).";
        } else if (fn === "acoth") {
          if (Math.abs(x) <= 1) {
            showCalcError(
              "acoth(x) is only defined for |x| > 1 (real values)."
            );
            return;
          }
          const val = 0.5 * Math.log((x + 1) / (x - 1));
          value = val;
          const back =
            (0.5 * (Math.exp(val) + Math.exp(-val))) /
            ((Math.exp(val) - Math.exp(-val)) / 2); // cosh/sinh
          const compDiff = Math.abs(back - x);
          identityDetails =
            "<p>Inverse definition: acoth(x) is the inverse of coth(x) on |x| &gt; 1.</p>" +
            '<ul class="list-disc ml-5">' +
            '<li>acoth(x) ≈ <span class="font-mono">' +
            formatNumber(val) +
            "</span></li>" +
            '<li>(1/2) · ln((x + 1) / (x − 1)) ≈ <span class="font-mono">' +
            formatNumber(val) +
            "</span></li>" +
            '<li>coth(acoth(x)) ≈ <span class="font-mono">' +
            formatNumber(back) +
            "</span></li>" +
            '<li>|coth(acoth(x)) − x| ≈ <span class="font-mono">' +
            formatNumber(compDiff) +
            "</span></li>" +
            "</ul>";
          domainNote = "Domain: |x| &gt; 1 for real acoth(x).";
        } else if (fn === "asech") {
          if (x <= 0 || x > 1) {
            showCalcError(
              "asech(x) is only defined for 0 < x ≤ 1 (real values)."
            );
            return;
          }
          const val = Math.acosh(1 / x);
          value = val;
          const back = 1 / Math.cosh(val);
          const compDiff = Math.abs(back - x);
          identityDetails =
            "<p>Inverse definition: asech(x) is the inverse of sech(x) on 0 &lt; x ≤ 1.</p>" +
            '<ul class="list-disc ml-5">' +
            '<li>asech(x) = acosh(1/x) ≈ <span class="font-mono">' +
            formatNumber(val) +
            "</span></li>" +
            '<li>sech(asech(x)) ≈ <span class="font-mono">' +
            formatNumber(back) +
            "</span></li>" +
            '<li>|sech(asech(x)) − x| ≈ <span class="font-mono">' +
            formatNumber(compDiff) +
            "</span></li>" +
            "</ul>";
          domainNote = "Domain: 0 &lt; x ≤ 1 for real asech(x).";
        } else if (fn === "acsch") {
          if (x === 0) {
            showCalcError("acsch(x) is not defined at x = 0 (real values).");
            return;
          }
          const val = Math.asinh(1 / x);
          value = val;
          const back = 1 / Math.sinh(val);
          const compDiff = Math.abs(back - x);
          identityDetails =
            "<p>Inverse definition: acsch(x) is the inverse of csch(x) for x ≠ 0.</p>" +
            '<ul class="list-disc ml-5">' +
            '<li>acsch(x) = asinh(1/x) ≈ <span class="font-mono">' +
            formatNumber(val) +
            "</span></li>" +
            '<li>csch(acsch(x)) ≈ <span class="font-mono">' +
            formatNumber(back) +
            "</span></li>" +
            '<li>|csch(acsch(x)) − x| ≈ <span class="font-mono">' +
            formatNumber(compDiff) +
            "</span></li>" +
            "</ul>";
          domainNote = "Domain: x ≠ 0 for real acsch(x).";
        } else {
          showCalcError("Unknown function. Please reload the page.");
          return;
        }

        if (!isFinite(value)) {
          showCalcError(
            "The function is undefined or overflows numerically at this x. Check the domain and try a different value."
          );
          return;
        }

        let html = '<div class="space-y-3">';
        html +=
          '<p><span class="font-semibold">Input:</span> x = <span class="font-mono">' +
          formatNumber(x) +
          "</span></p>";
        html +=
          '<p><span class="font-semibold">Selected function:</span> <span class="font-mono">' +
          fn +
          "(x)</span></p>";
        html +=
          '<p><span class="font-semibold">Value:</span> ' +
          '<span class="font-mono">' +
          fn +
          "(" +
          formatNumber(x) +
          ")</span> ≈ " +
          '<span class="font-mono">' +
          formatNumber(value) +
          "</span></p>";

        html += '<div class="mt-2">' + identityDetails + "</div>";

        if (domainNote) {
          html +=
            '<p class="mt-2 text-xs text-yellow-700">' + domainNote + "</p>";
        }

        html +=
          '<p class="mt-2 text-xs text-gray-600">Numerical values are approximate and rounded. Use them as a guide and, for formal proofs, rely on analytic identities.</p>';
        html += "</div>";

        resultEl.innerHTML = html;
      }

      if (calcBtn) {
        calcBtn.addEventListener("click", calculateHyperbolic);
      }

      if (clearBtn && resultEl) {
        clearBtn.addEventListener("click", () => {
          if (xInput) xInput.value = "";
          if (fnSelect) fnSelect.value = "sinh";
          resultEl.innerHTML =
            '<p class="text-gray-500">The numerical value, exponential definition, and identity checks will appear here after you click <span class="font-semibold">Calculate</span>.</p>';
        });
      }

      // Simple FAQ toggle
      document
        .querySelectorAll("#faq-container .faq-question")
        .forEach((btn) => {
          btn.addEventListener("click", () => {
            const answer = btn.parentElement.querySelector(".faq-answer");
            if (!answer) return;
            const isHidden = answer.classList.contains("hidden");
            answer.classList.toggle("hidden", !isHidden);
            const icon = btn.querySelector("span:last-child");
            if (icon) icon.textContent = isHidden ? "−" : "+";
          });
        });
    </script>

