<script="">
        function calculateCPP() {
            const mapValue = parseFloat(document.getElementById('map').value);
            const icpValue = parseFloat(document.getElementById('icp').value);
            const mapError = document.getElementById('mapError');
            const icpError = document.getElementById('icpError');
            const cppValue = document.getElementById('cppValue');

            mapError.textContent = '';
            icpError.textContent = '';

            if (isNaN(mapValue) || mapValue <= 0) {
                mapError.textContent = 'Please enter a valid MAP value greater than 0.';
                return;
            }

            if (isNaN(icpValue) || icpValue < 0) {
                icpError.textContent = 'Please enter a valid ICP value greater than or equal to 0.';
                return;
            }

            const cpp = mapValue - icpValue;
            cppValue.textContent = cpp.toFixed(2) + ' mmHg';
        }
    </script>


