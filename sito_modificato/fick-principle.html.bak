    <script>
      // Fick calculator logic
      (function () {
        const modeButtons = document.querySelectorAll(".mode-btn");
        const unitButtons = document.querySelectorAll(".unit-btn");

        const fieldVO2 = document.getElementById("field-vo2");
        const fieldCaO2 = document.getElementById("field-cao2");
        const fieldCvO2 = document.getElementById("field-cvo2");
        const fieldCO = document.getElementById("field-co");
        const fieldAV = document.getElementById("field-avdiff");

        const vo2Input = document.getElementById("vo2");
        const vo2Unit = document.getElementById("vo2-unit");
        const bsaInput = document.getElementById("bsa");
        const cao2Input = document.getElementById("cao2");
        const cvo2Input = document.getElementById("cvo2");
        const coInput = document.getElementById("co");
        const avdiffInput = document.getElementById("avdiff");

        const cao2UnitLabel = document.getElementById("cao2-unit-label");
        const cvo2UnitLabel = document.getElementById("cvo2-unit-label");
        const avdiffUnitLabel = document.getElementById("avdiff-unit-label");

        const errorEl = document.getElementById("fick-error");
        const resultsBox = document.getElementById("fick-results");
        const resultCO = document.getElementById("result-co");
        const resultCI = document.getElementById("result-ci");
        const resultVO2 = document.getElementById("result-vo2");
        const resultAV = document.getElementById("result-avdiff");
        const resultNotes = document.getElementById("result-notes");

        const calcBtn = document.getElementById("fick-calc-btn");
        const clearBtn = document.getElementById("fick-clear-btn");
        const exampleBtn = document.getElementById("fick-example-btn");

        let currentMode = "co"; // 'co', 'vo2', 'avdiff'
        let currentUnit = "si"; // 'si' (mL/L) or 'clinical' (mL/dL)

        function setMode(mode) {
          currentMode = mode;
          modeButtons.forEach((btn) => {
            if (btn.dataset.mode === mode) {
              btn.classList.remove(
                "border-gray-300",
                "text-gray-700",
                "bg-white"
              );
              btn.classList.add(
                "border-blue-600",
                "bg-blue-50",
                "text-blue-700",
                "font-medium"
              );
            } else {
              btn.classList.remove(
                "border-blue-600",
                "bg-blue-50",
                "text-blue-700",
                "font-medium"
              );
              btn.classList.add("border-gray-300", "text-gray-700", "bg-white");
            }
          });

          // Show/hide fields depending on what we're solving for
          fieldVO2.classList.remove("hidden");
          fieldCaO2.classList.remove("hidden");
          fieldCvO2.classList.remove("hidden");
          fieldCO.classList.add("hidden");
          fieldAV.classList.add("hidden");

          if (mode === "co") {
            // Solve for CO: need VO2, CaO2, CvO2
            vo2Input.disabled = false;
            cao2Input.disabled = false;
            cvo2Input.disabled = false;
            coInput.disabled = true;
            avdiffInput.disabled = true;
          } else if (mode === "vo2") {
            // Solve for VO2: need CO and A-V diff (via CaO2 & CvO2 or direct)
            fieldCO.classList.remove("hidden");
            vo2Input.disabled = true;
            coInput.disabled = false;
            cao2Input.disabled = false;
            cvo2Input.disabled = false;
            avdiffInput.disabled = true;
          } else if (mode === "avdiff") {
            // Solve for A-V diff: need VO2 and CO
            fieldAV.classList.remove("hidden");
            vo2Input.disabled = false;
            coInput.disabled = false;
            cao2Input.disabled = true;
            cvo2Input.disabled = true;
            avdiffInput.disabled = false;
          }

          clearResults();
          hideError();
        }

        function setUnit(unit) {
          currentUnit = unit;
          unitButtons.forEach((btn) => {
            if (btn.dataset.unit === unit) {
              btn.classList.remove(
                "border-gray-300",
                "text-gray-700",
                "bg-white"
              );
              btn.classList.add(
                "border-blue-600",
                "bg-blue-50",
                "text-blue-700",
                "font-medium"
              );
            } else {
              btn.classList.remove(
                "border-blue-600",
                "bg-blue-50",
                "text-blue-700",
                "font-medium"
              );
              btn.classList.add("border-gray-300", "text-gray-700", "bg-white");
            }
          });

          if (unit === "si") {
            cao2UnitLabel.textContent = "mL O₂/L";
            cvo2UnitLabel.textContent = "mL O₂/L";
            avdiffUnitLabel.textContent = "mL O₂/L";
          } else {
            cao2UnitLabel.textContent = "mL O₂/dL";
            cvo2UnitLabel.textContent = "mL O₂/dL";
            avdiffUnitLabel.textContent = "mL O₂/dL";
          }

          clearResults();
          hideError();
        }

        function showError(msg) {
          errorEl.textContent = msg;
          errorEl.classList.remove("hidden");
          resultsBox.classList.add("hidden");
        }

        function hideError() {
          errorEl.classList.add("hidden");
          errorEl.textContent = "";
        }

        function clearResults() {
          resultsBox.classList.add("hidden");
          resultCO.textContent = "";
          resultCI.textContent = "";
          resultVO2.textContent = "";
          resultAV.textContent = "";
          resultNotes.textContent = "";
        }

        function parsePositive(value, name) {
          const v = parseFloat(value);
          if (isNaN(v) || v <= 0) {
            throw new Error(name + " must be a positive number.");
          }
          return v;
        }

        function calculate() {
          hideError();
          clearResults();

          try {
            const unitFactor = currentUnit === "clinical" ? 10 : 1; // mL/dL -> mL/L

            let vo2, co, cao2, cvo2, avdiff;
            let ci = null;

            if (currentMode === "co") {
              vo2 = parsePositive(vo2Input.value, "Oxygen consumption (VO₂)");
              if (vo2Unit.value === "lmin") vo2 *= 1000; // L/min -> mL/min
              cao2 = parsePositive(
                cao2Input.value,
                "Arterial oxygen content (CaO₂)"
              );
              cvo2 = parsePositive(
                cvo2Input.value,
                "Mixed venous oxygen content (CvO₂)"
              );
              let diff = cao2 - cvo2;
              if (diff <= 0) {
                throw new Error(
                  "CaO₂ must be greater than CvO₂ to compute cardiac output."
                );
              }
              // Convert diff to mL/L if needed
              diff *= unitFactor;
              co = vo2 / diff; // L/min
              avdiff = diff; // mL/L

              const bsa = parseFloat(bsaInput.value);
              if (!isNaN(bsa) && bsa > 0) {
                ci = co / bsa;
              }
            } else if (currentMode === "vo2") {
              co = parsePositive(coInput.value, "Cardiac output (CO)");
              cao2 = parsePositive(
                cao2Input.value,
                "Arterial oxygen content (CaO₂)"
              );
              cvo2 = parsePositive(
                cvo2Input.value,
                "Mixed venous oxygen content (CvO₂)"
              );
              let diff = cao2 - cvo2;
              if (diff <= 0) {
                throw new Error(
                  "CaO₂ must be greater than CvO₂ to compute VO₂."
                );
              }
              diff *= unitFactor; // mL/L
              vo2 = co * diff; // mL/min
              avdiff = diff;

              const bsa = parseFloat(bsaInput.value);
              if (!isNaN(bsa) && bsa > 0) {
                ci = co / bsa;
              }
            } else if (currentMode === "avdiff") {
              vo2 = parsePositive(vo2Input.value, "Oxygen consumption (VO₂)");
              if (vo2Unit.value === "lmin") vo2 *= 1000;
              co = parsePositive(coInput.value, "Cardiac output (CO)");
              let diff = vo2 / co; // mL/L
              avdiff = diff;
              if (currentUnit === "clinical") {
                diff = diff / 10; // convert to mL/dL for display
              }
              // For this mode we don't know CaO2 and CvO2 individually, only the difference
              const bsa = parseFloat(bsaInput.value);
              if (!isNaN(bsa) && bsa > 0) {
                ci = co / bsa;
              }
            }

            // Populate results
            resultsBox.classList.remove("hidden");

            if (typeof co !== "undefined") {
              resultCO.textContent =
                "Cardiac output (CO): " + co.toFixed(2) + " L/min";
            }

            if (ci !== null) {
              resultCI.textContent =
                "Cardiac index (CI): " + ci.toFixed(2) + " L/min/m²";
            } else {
              resultCI.textContent =
                "Cardiac index (CI): enter BSA to compute.";
            }

            if (typeof vo2 !== "undefined") {
              resultVO2.textContent =
                "Oxygen consumption (VO₂): " + vo2.toFixed(0) + " mL/min";
            }

            if (typeof avdiff !== "undefined") {
              const displayDiff =
                currentUnit === "clinical" ? avdiff / 10 : avdiff;
              const unitLabel =
                currentUnit === "clinical" ? "mL O₂/dL" : "mL O₂/L";
              resultAV.textContent =
                "A–V O₂ content difference (CaO₂ − CvO₂): " +
                displayDiff.toFixed(1) +
                " " +
                unitLabel;
            }

            resultNotes.textContent =
              "Note: This calculation assumes steady-state conditions and accurate measurements of VO₂ and blood oxygen content. Always interpret results in clinical context.";
          } catch (err) {
            showError(err.message);
          }
        }

        function clearAll() {
          vo2Input.value = "";
          vo2Unit.value = "mlmin";
          bsaInput.value = "";
          cao2Input.value = "";
          cvo2Input.value = "";
          coInput.value = "";
          avdiffInput.value = "";
          hideError();
          clearResults();
        }

        function loadExample() {
          // Example from the worked example in the text
          setMode("co");
          setUnit("si");
          vo2Input.value = "250";
          vo2Unit.value = "mlmin";
          cao2Input.value = "200";
          cvo2Input.value = "150";
          bsaInput.value = "1.8";
          coInput.value = "";
          avdiffInput.value = "";
          hideError();
          clearResults();
        }

        modeButtons.forEach((btn) => {
          btn.addEventListener("click", () => setMode(btn.dataset.mode));
        });

        unitButtons.forEach((btn) => {
          btn.addEventListener("click", () => setUnit(btn.dataset.unit));
        });

        calcBtn.addEventListener("click", calculate);
        clearBtn.addEventListener("click", clearAll);
        exampleBtn.addEventListener("click", loadExample);

        // Initialize defaults
        setMode("co");
        setUnit("si");
      })();

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
