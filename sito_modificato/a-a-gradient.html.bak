    <script>
      // UI logic for mode toggles
      const pbModeEl = document.getElementById("pb-mode");
      const pbCustomRow = document.getElementById("pb-custom-row");
      const altitudeRow = document.getElementById("altitude-row");
      const fio2ModeEl = document.getElementById("fio2-mode");
      const fio2Row = document.getElementById("fio2-row");

      pbModeEl.addEventListener("change", () => {
        const mode = pbModeEl.value;
        pbCustomRow.classList.toggle("hidden", mode !== "custom");
        altitudeRow.classList.toggle("hidden", mode !== "altitude");
      });

      fio2ModeEl.addEventListener("change", () => {
        const mode = fio2ModeEl.value;
        fio2Row.classList.toggle("hidden", mode !== "custom");
      });

      function estimatePBFromAltitude(altMeters) {
        // Simple barometric formula approximation (valid up to ~11 km)
        const PB0 = 760; // mmHg at sea level
        const h = altMeters;
        return PB0 * Math.pow(1 - 2.25577e-5 * h, 5.25588);
      }

      function clamp(value, min, max) {
        return Math.min(max, Math.max(min, value));
      }

      document.getElementById("calc-btn").addEventListener("click", () => {
        const age = parseFloat(document.getElementById("age").value);
        const pbMode = pbModeEl.value;
        const fio2Mode = fio2ModeEl.value;
        const pbInput = parseFloat(document.getElementById("pb").value);
        const altitude = parseFloat(document.getElementById("altitude").value);
        const fio2Input = parseFloat(document.getElementById("fio2").value);
        const pao2Art = parseFloat(document.getElementById("pao2").value);
        const paco2 = parseFloat(document.getElementById("paco2").value);
        const rq = parseFloat(document.getElementById("rq").value);

        if (
          isNaN(age) ||
          age < 0 ||
          age > 120 ||
          isNaN(pao2Art) ||
          isNaN(paco2) ||
          isNaN(rq)
        ) {
          alert(
            "Please enter valid numeric values for age, PaO₂, PaCO₂, and R."
          );
          return;
        }

        let PB;
        if (pbMode === "sea") {
          PB = 760;
        } else if (pbMode === "custom") {
          if (isNaN(pbInput) || pbInput < 300 || pbInput > 900) {
            alert(
              "Please enter a barometric pressure between 300 and 900 mmHg."
            );
            return;
          }
          PB = pbInput;
        } else {
          if (isNaN(altitude) || altitude < -500 || altitude > 6000) {
            alert("Please enter an altitude between -500 and 6000 meters.");
            return;
          }
          PB = estimatePBFromAltitude(altitude);
        }

        let FiO2;
        if (fio2Mode === "room") {
          FiO2 = 0.21;
        } else {
          if (isNaN(fio2Input) || fio2Input < 0.21 || fio2Input > 1) {
            alert("Please enter FiO₂ between 0.21 and 1.0.");
            return;
          }
          FiO2 = fio2Input;
        }

        const PH2O = 47; // mmHg at 37°C
        const effectivePB = PB - PH2O;
        const safeRQ = clamp(rq, 0.5, 1.0);

        const PAO2 = FiO2 * effectivePB - paco2 / safeRQ;
        const AA = PAO2 - pao2Art;

        const normalAA = age / 4 + 4;

        const resultsCard = document.getElementById("results-card");
        const resPb = document.getElementById("res-pb");
        const resFio2 = document.getElementById("res-fio2");
        const resPAO2 = document.getElementById("res-pao2");
        const resPaO2Art = document.getElementById("res-paO2-arterial");
        const resAA = document.getElementById("res-aa");
        const resAANormal = document.getElementById("res-aa-normal");
        const resAAStatusText = document.getElementById("res-aa-status-text");
        const resAABadge = document.getElementById("res-aa-badge");
        const resInterp = document.getElementById("res-interpretation");

        resPb.textContent = PB.toFixed(0) + " mmHg";
        resFio2.textContent = FiO2.toFixed(2);
        resPAO2.textContent = PAO2.toFixed(1) + " mmHg";
        resPaO2Art.textContent = pao2Art.toFixed(1) + " mmHg";
        resAA.textContent = AA.toFixed(1) + " mmHg";
        resAANormal.textContent = normalAA.toFixed(1) + " mmHg";

        let statusText = "";
        let badgeText = "";
        let badgeClass = "";
        let interp = "";

        if (AA < -5) {
          statusText = "Lower than expected (check inputs)";
          badgeText = "Check values";
          badgeClass = "bg-yellow-100 text-yellow-800";
          interp =
            "The calculated A–a gradient is negative, which is physiologically unusual. This usually indicates a measurement or data entry error (e.g. PaO₂ higher than calculated PAO₂). Re-check FiO₂, PB, PaO₂, and PaCO₂.";
        } else {
          const ratio = AA / normalAA;
          if (ratio <= 1.1) {
            statusText = "Within expected range";
            badgeText = "Normal";
            badgeClass = "bg-green-100 text-green-800";
            interp =
              "The A–a gradient is within the expected range for age on room air at sea level. If the patient is hypoxemic, consider hypoventilation or low inspired oxygen (e.g. altitude) as potential causes.";
          } else if (ratio <= 2) {
            statusText = "Mildly elevated";
            badgeText = "Mild elevation";
            badgeClass = "bg-yellow-100 text-yellow-800";
            interp =
              "The A–a gradient is mildly elevated above the age-adjusted normal estimate. This suggests some impairment of gas exchange, such as mild V/Q mismatch (e.g. early pneumonia, mild pulmonary edema, small pulmonary emboli). Interpret in the full clinical context.";
          } else {
            statusText = "Significantly elevated";
            badgeText = "Marked elevation";
            badgeClass = "bg-red-100 text-red-800";
            interp =
              "The A–a gradient is markedly elevated compared with the age-adjusted normal estimate. This is consistent with significant V/Q mismatch, diffusion impairment, or shunt (e.g. pneumonia, ARDS, pulmonary edema, large pulmonary embolism). Urgent clinical assessment is warranted.";
          }
        }

        resAAStatusText.textContent = statusText;
        resAABadge.textContent = badgeText;
        resAABadge.className = "badge ml-2 " + badgeClass;
        resAABadge.classList.remove("hidden");
        resInterp.textContent = interp;

        resultsCard.classList.remove("hidden");
      });

      // Mobile menu script (simple)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const expanded = this.getAttribute("aria-expanded") === "true";
          this.setAttribute("aria-expanded", String(!expanded));
          menu.classList.toggle("hidden");
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
