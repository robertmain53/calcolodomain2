    <script>
      // eFG% calculator logic
      function formatPercent(value) {
        if (isNaN(value) || !isFinite(value)) return "–";
        return (value * 100).toFixed(1) + "%";
      }

      function interpretEFG(efg) {
        if (!isFinite(efg))
          return "Enter valid stats to see an interpretation.";
        if (efg < 0.45)
          return "This is an inefficient shooting performance for most levels.";
        if (efg < 0.5) return "Below-average shooting efficiency.";
        if (efg < 0.55) return "Around league-average shooting efficiency.";
        if (efg < 0.6) return "Very good shooting efficiency.";
        return "This is an elite eFG% for most levels of basketball.";
      }

      function updateCalculator() {
        const fgm = parseFloat(document.getElementById("fgm").value);
        const fg3m = parseFloat(document.getElementById("fg3m").value);
        const fga = parseFloat(document.getElementById("fga").value);
        const errorEl = document.getElementById("input-error");

        if ([fgm, fg3m, fga].some((v) => isNaN(v) || v < 0)) {
          errorEl.textContent = "All inputs must be zero or positive numbers.";
          errorEl.classList.remove("hidden");
          return;
        }
        if (fgm > fga) {
          errorEl.textContent =
            "Field goals made (FGM) cannot exceed field goal attempts (FGA).";
          errorEl.classList.remove("hidden");
          return;
        }
        if (fg3m > fgm) {
          errorEl.textContent =
            "3-point field goals made (3PM) cannot exceed total field goals made (FGM).";
          errorEl.classList.remove("hidden");
          return;
        }
        if (fga === 0) {
          errorEl.textContent =
            "Field goal attempts (FGA) must be greater than zero to compute eFG%.";
          errorEl.classList.remove("hidden");
          return;
        }

        errorEl.classList.add("hidden");

        const efg = (fgm + 0.5 * fg3m) / fga;
        const fgPct = fgm / fga;

        document.getElementById("efg-result").textContent = formatPercent(efg);
        document.getElementById(
          "efg-raw"
        ).textContent = `eFG = (${fgm} + 0.5 × ${fg3m}) ÷ ${fga} = ${efg.toFixed(
          3
        )}`;
        document.getElementById("efg-interpretation").textContent =
          interpretEFG(efg);
        document.getElementById(
          "fgp-comparison"
        ).textContent = `FG% = ${fgm} ÷ ${fga} = ${formatPercent(
          fgPct
        )}. eFG% is ${
          efg > fgPct ? "higher" : efg < fgPct ? "lower" : "the same"
        } because it ${
          efg > fgPct
            ? "rewards made 3-pointers."
            : "weights 3-pointers differently."
        }`;
      }

      document
        .getElementById("calculate-btn")
        .addEventListener("click", updateCalculator);

      document.getElementById("reset-btn").addEventListener("click", () => {
        document.getElementById("fgm").value = 8;
        document.getElementById("fg3m").value = 3;
        document.getElementById("fga").value = 15;
        document.getElementById("input-error").classList.add("hidden");
        updateCalculator();
      });

      ["fgm", "fg3m", "fga"].forEach((id) => {
        document.getElementById(id).addEventListener("input", () => {
          document.getElementById("input-error").classList.add("hidden");
        });
      });

      // Initialize with default example
      updateCalculator();

      // Simple FAQ toggle (in addition to global script_faq if needed)
      document
        .querySelectorAll("#faq-container .faq-question")
        .forEach((btn) => {
          btn.addEventListener("click", () => {
            const answer = btn.parentElement.querySelector(".faq-answer");
            const icon = btn.querySelector("span:last-child");
            const isHidden = answer.classList.contains("hidden");
            document
              .querySelectorAll("#faq-container .faq-answer")
              .forEach((a) => a.classList.add("hidden"));
            document
              .querySelectorAll("#faq-container .faq-question span:last-child")
              .forEach((i) => (i.textContent = "+"));
            if (isHidden) {
              answer.classList.remove("hidden");
              icon.textContent = "−";
            }
          });
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
