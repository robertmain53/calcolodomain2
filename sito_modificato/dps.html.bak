    <script>
      function parseNumber(id) {
        const el = document.getElementById(id);
        if (!el) return 0;
        const v = parseFloat(el.value.replace(",", "."));
        return isNaN(v) ? 0 : v;
      }

      function formatNumber(n) {
        if (!isFinite(n)) return "0";
        if (Math.abs(n) >= 1000000) return n.toFixed(0);
        if (Math.abs(n) >= 1000) return n.toFixed(1);
        return n.toFixed(2);
      }

      function computeBuild(build) {
        const baseDamage = parseNumber("baseDamage" + build);
        const aps = parseNumber("aps" + build);
        const critChancePct = parseNumber("critChance" + build);
        const critMulti = parseNumber("critMulti" + build);
        const uptimePct = parseNumber("uptime" + build);
        const skillDamage = parseNumber("skillDamage" + build);
        const skillCooldown = parseNumber("skillCooldown" + build);

        const C = Math.min(Math.max(critChancePct / 100, 0), 1);
        const M = Math.max(critMulti, 1);
        const U = Math.min(Math.max(uptimePct / 100, 0), 1);

        const avgHit = baseDamage * (1 - C) + baseDamage * M * C;
        const autoDps = avgHit * aps;

        let skillDps = 0;
        if (skillDamage > 0 && skillCooldown > 0) {
          skillDps = skillDamage / skillCooldown;
        }

        const rawDps = autoDps + skillDps;
        return { rawDps, uptime: U };
      }

      function updateDps() {
        const fightDuration = parseNumber("fightDuration");
        const enemyDRpct = parseNumber("enemyDR");
        const DR = Math.min(Math.max(enemyDRpct / 100, 0), 0.95); // cap at 95%

        const buildA = computeBuild("A");
        const buildB = computeBuild("B");

        const effDpsA = buildA.rawDps * buildA.uptime * (1 - DR);
        const effDpsB = buildB.rawDps * buildB.uptime * (1 - DR);

        const totalDamageA = effDpsA * fightDuration;
        const totalDamageB = effDpsB * fightDuration;

        document.getElementById("rawDpsA").textContent = formatNumber(
          buildA.rawDps
        );
        document.getElementById("rawDpsB").textContent = formatNumber(
          buildB.rawDps
        );
        document.getElementById("effDpsA").textContent = formatNumber(effDpsA);
        document.getElementById("effDpsB").textContent = formatNumber(effDpsB);
        document.getElementById("totalDamageA").textContent =
          formatNumber(totalDamageA);
        document.getElementById("totalDamageB").textContent =
          formatNumber(totalDamageB);

        // Comparison
        const labelA =
          document.getElementById("labelA").value.trim() || "Build A";
        const labelB =
          document.getElementById("labelB").value.trim() || "Build B";

        if (buildA.rawDps <= 0 && buildB.rawDps <= 0) {
          document.getElementById("comparisonSummary").textContent =
            "Enter stats for both builds to see which one has higher DPS.";
          document.getElementById("winnerLabel").textContent = "–";
          document.getElementById("dpsDiff").textContent = "–";
          document.getElementById("extraDamage").textContent = "–";
          return;
        }

        let winner, loser, effWinner, effLoser, labelWinner, labelLoser;
        if (effDpsA > effDpsB) {
          winner = "A";
          loser = "B";
          effWinner = effDpsA;
          effLoser = effDpsB;
          labelWinner = labelA;
          labelLoser = labelB;
        } else if (effDpsB > effDpsA) {
          winner = "B";
          loser = "A";
          effWinner = effDpsB;
          effLoser = effDpsA;
          labelWinner = labelB;
          labelLoser = labelA;
        } else {
          document.getElementById("comparisonSummary").textContent =
            "Both builds have the same effective DPS based on the current inputs.";
          document.getElementById("winnerLabel").textContent = "Tie";
          document.getElementById("dpsDiff").textContent = "0";
          document.getElementById("extraDamage").textContent = "0";
          return;
        }

        const diff = effWinner - effLoser;
        const pct = effLoser > 0 ? (diff / effLoser) * 100 : 0;
        const extraDamage = diff * fightDuration;

        document.getElementById("winnerLabel").textContent = labelWinner;
        document.getElementById("dpsDiff").textContent =
          formatNumber(diff) + " DPS (" + pct.toFixed(1) + "% higher)";
        document.getElementById("extraDamage").textContent =
          formatNumber(extraDamage) + " damage";

        document.getElementById("comparisonSummary").textContent =
          labelWinner +
          " deals about " +
          formatNumber(diff) +
          " more DPS than " +
          labelLoser +
          " and " +
          formatNumber(extraDamage) +
          " more total damage over " +
          fightDuration.toFixed(0) +
          " seconds.";
      }

      function resetBuild(build) {
        if (build === "A") {
          document.getElementById("labelA").value = "";
          document.getElementById("baseDamageA").value = 100;
          document.getElementById("apsA").value = 1.5;
          document.getElementById("critChanceA").value = 25;
          document.getElementById("critMultiA").value = 2.0;
          document.getElementById("uptimeA").value = 100;
          document.getElementById("skillDamageA").value = 0;
          document.getElementById("skillCooldownA").value = 0;
        } else {
          document.getElementById("labelB").value = "";
          document.getElementById("baseDamageB").value = 80;
          document.getElementById("apsB").value = 2.0;
          document.getElementById("critChanceB").value = 15;
          document.getElementById("critMultiB").value = 1.8;
          document.getElementById("uptimeB").value = 100;
          document.getElementById("skillDamageB").value = 0;
          document.getElementById("skillCooldownB").value = 0;
        }
        updateDps();
      }

      document.addEventListener("DOMContentLoaded", function () {
        const ids = [
          "fightDuration",
          "enemyDR",
          "labelA",
          "baseDamageA",
          "apsA",
          "critChanceA",
          "critMultiA",
          "uptimeA",
          "skillDamageA",
          "skillCooldownA",
          "labelB",
          "baseDamageB",
          "apsB",
          "critChanceB",
          "critMultiB",
          "uptimeB",
          "skillDamageB",
          "skillCooldownB",
        ];
        ids.forEach((id) => {
          const el = document.getElementById(id);
          if (!el) return;
          el.addEventListener("input", updateDps);
          el.addEventListener("change", updateDps);
        });
        updateDps();
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
