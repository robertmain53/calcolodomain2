    <script>
      // Utility conversions
      function toMeters(value, unit) {
        if (unit === "m") return value;
        if (unit === "ft") return value * 0.3048;
        return value;
      }
      function toMS(value, unit) {
        switch (unit) {
          case "ms":
            return value;
          case "kmh":
            return value / 3.6;
          case "mph":
            return value * 0.44704;
          case "kts":
            return value * 0.514444;
          default:
            return value;
        }
      }
      function formatPowerW(watts) {
        if (!isFinite(watts)) return "–";
        if (watts < 1e3) return watts.toFixed(1) + " W";
        if (watts < 1e6) return (watts / 1e3).toFixed(2) + " kW";
        if (watts < 1e9) return (watts / 1e6).toFixed(2) + " MW";
        return (watts / 1e9).toFixed(2) + " GW";
      }
      function formatEnergyKWh(kwh) {
        if (!isFinite(kwh)) return "–";
        if (kwh < 1e3) return kwh.toFixed(0) + " kWh";
        if (kwh < 1e6) return (kwh / 1e3).toFixed(2) + " MWh";
        if (kwh < 1e9) return (kwh / 1e6).toFixed(2) + " GWh";
        return (kwh / 1e9).toFixed(2) + " TWh";
      }

      function calculate() {
        const sizeMode = document.querySelector(
          'input[name="sizeMode"]:checked'
        ).value;
        const rotorSize = parseFloat(
          document.getElementById("rotor-size").value
        );
        const sizeUnit = document.getElementById("size-unit").value;
        const windSpeed = parseFloat(
          document.getElementById("wind-speed").value
        );
        const windUnit = document.getElementById("wind-unit").value;
        const rho = parseFloat(document.getElementById("air-density").value);
        const cp = parseFloat(document.getElementById("cp").value);
        const eta = parseFloat(document.getElementById("eta").value);
        const ratedKW = parseFloat(
          document.getElementById("rated-power").value
        );
        const cfPercent = parseFloat(
          document.getElementById("capacity-factor").value
        );
        const householdUse = parseFloat(
          document.getElementById("household-use").value
        );
        const hoursPerYear = parseFloat(
          document.getElementById("hours-per-year").value
        );

        if (
          !rotorSize ||
          rotorSize <= 0 ||
          !windSpeed ||
          windSpeed < 0 ||
          !rho ||
          rho <= 0
        ) {
          return;
        }

        // Geometry
        let sizeMeters = toMeters(rotorSize, sizeUnit);
        let radiusM = sizeMode === "diameter" ? sizeMeters / 2 : sizeMeters;
        const area = Math.PI * radiusM * radiusM;

        // Wind speed normalized
        const v = toMS(windSpeed, windUnit);

        // Power calculations (W)
        const pWind = 0.5 * rho * area * Math.pow(v, 3);
        const pRotor = cp * pWind;
        const pElec = eta * pRotor;

        // Annual energy from rated power and capacity factor
        const cf = isFinite(cfPercent) ? cfPercent / 100 : 0;
        const ratedW = ratedKW * 1000;
        const eYearKWh = (ratedW * cf * hoursPerYear) / 1000;
        const homes = householdUse > 0 ? eYearKWh / householdUse : NaN;

        // Update results
        document.getElementById("res-area").textContent =
          area.toFixed(1) + " m²";
        document.getElementById("res-wind").textContent = v.toFixed(2) + " m/s";
        document.getElementById("res-pwind").textContent = formatPowerW(pWind);
        document.getElementById("res-protor").textContent =
          formatPowerW(pRotor);
        document.getElementById("res-pelec").textContent = formatPowerW(pElec);

        document.getElementById("res-rated").textContent = isFinite(ratedKW)
          ? ratedKW.toFixed(1) + " kW"
          : "–";
        document.getElementById("res-cf").textContent = isFinite(cfPercent)
          ? cfPercent.toFixed(1) + " %"
          : "–";
        document.getElementById("res-energy").textContent =
          formatEnergyKWh(eYearKWh);
        document.getElementById("res-homes").textContent = isFinite(homes)
          ? homes.toFixed(0) + " homes"
          : "–";

        // Summary sentence
        const summary = [];
        if (isFinite(pElec)) {
          summary.push(
            `At a wind speed of ${v.toFixed(
              1
            )} m/s, this turbine would produce about ${formatPowerW(
              pElec
            )} of electrical power.`
          );
        }
        if (isFinite(eYearKWh) && eYearKWh > 0) {
          summary.push(
            `With a rated power of ${ratedKW.toFixed(
              1
            )} kW and a capacity factor of ${cfPercent.toFixed(
              1
            )}%, the estimated annual energy production is ${formatEnergyKWh(
              eYearKWh
            )}.`
          );
        }
        if (isFinite(homes) && homes > 0) {
          summary.push(
            `That is roughly enough electricity for about ${homes.toFixed(
              0
            )} average homes at ${householdUse.toFixed(0)} kWh per year each.`
          );
        }
        document.getElementById("summary-text").textContent = summary.join(" ");
      }

      document.addEventListener("DOMContentLoaded", function () {
        const cpInput = document.getElementById("cp");
        const etaInput = document.getElementById("eta");
        const cpVal = document.getElementById("cp-val");
        const etaVal = document.getElementById("eta-val");

        cpInput.addEventListener("input", () => {
          cpVal.textContent = parseFloat(cpInput.value).toFixed(3);
          calculate();
        });
        etaInput.addEventListener("input", () => {
          etaVal.textContent = parseFloat(etaInput.value).toFixed(2);
          calculate();
        });

        document
          .getElementById("density-preset")
          .addEventListener("change", function () {
            const airDensity = document.getElementById("air-density");
            switch (this.value) {
              case "sea":
                airDensity.value = 1.225;
                break;
              case "1000m":
                airDensity.value = 1.112;
                break;
              case "cold":
                airDensity.value = 1.3;
                break;
              default:
                break;
            }
            calculate();
          });

        // Size mode label
        const sizeLabel = document.getElementById("size-label");
        document.querySelectorAll(".size-mode").forEach((r) => {
          r.addEventListener("change", () => {
            sizeLabel.textContent =
              r.value === "diameter" ? "diameter" : "radius";
            calculate();
          });
        });

        // Presets
        document.querySelectorAll("[data-preset]").forEach((btn) => {
          btn.addEventListener("click", () => {
            const preset = btn.getAttribute("data-preset");
            const rotorSize = document.getElementById("rotor-size");
            const sizeUnit = document.getElementById("size-unit");
            const windSpeed = document.getElementById("wind-speed");
            const windUnit = document.getElementById("wind-unit");
            const ratedPower = document.getElementById("rated-power");
            const cf = document.getElementById("capacity-factor");
            const cp = document.getElementById("cp");
            const eta = document.getElementById("eta");

            if (preset === "small") {
              document.querySelector(
                'input[name="sizeMode"][value="diameter"]'
              ).checked = true;
              rotorSize.value = 7; // ~10 kW small turbine
              sizeUnit.value = "m";
              windSpeed.value = 6;
              windUnit.value = "ms";
              cp.value = 0.35;
              cpVal.textContent = "0.35";
              eta.value = 0.85;
              etaVal.textContent = "0.85";
              ratedPower.value = 10;
              cf.value = 25;
            } else if (preset === "onshore") {
              document.querySelector(
                'input[name="sizeMode"][value="diameter"]'
              ).checked = true;
              rotorSize.value = 120; // typical 3 MW
              sizeUnit.value = "m";
              windSpeed.value = 8;
              windUnit.value = "ms";
              cp.value = 0.45;
              cpVal.textContent = "0.45";
              eta.value = 0.9;
              etaVal.textContent = "0.90";
              ratedPower.value = 3000;
              cf.value = 35;
            } else if (preset === "offshore") {
              document.querySelector(
                'input[name="sizeMode"][value="diameter"]'
              ).checked = true;
              rotorSize.value = 170; // ~8 MW
              sizeUnit.value = "m";
              windSpeed.value = 9.5;
              windUnit.value = "ms";
              cp.value = 0.48;
              cpVal.textContent = "0.48";
              eta.value = 0.92;
              etaVal.textContent = "0.92";
              ratedPower.value = 8000;
              cf.value = 50;
            }
            calculate();
          });
        });

        document
          .getElementById("calculate-btn")
          .addEventListener("click", calculate);
        document.getElementById("wind-form").addEventListener("input", (e) => {
          if (["number", "select-one"].includes(e.target.type)) {
            calculate();
          }
        });

        document.getElementById("reset-btn").addEventListener("click", () => {
          document.getElementById("rotor-size").value = 100;
          document.getElementById("size-unit").value = "m";
          document.getElementById("wind-speed").value = 8;
          document.getElementById("wind-unit").value = "ms";
          document.getElementById("air-density").value = 1.225;
          document.getElementById("density-preset").value = "custom";
          document.querySelector(
            'input[name="sizeMode"][value="diameter"]'
          ).checked = true;
          document.getElementById("cp").value = 0.45;
          cpVal.textContent = "0.45";
          document.getElementById("eta").value = 0.9;
          etaVal.textContent = "0.90";
          document.getElementById("rated-power").value = 3000;
          document.getElementById("capacity-factor").value = 35;
          document.getElementById("household-use").value = 10000;
          document.getElementById("hours-per-year").value = 8760;
          calculate();
        });

        // Initial calculation
        calculate();
      });
    </script>

