    <script>
      (function () {
        const typeSelect = document.getElementById("barcode-type");
        const dataInput = document.getElementById("barcode-data");
        const moduleWidthInput = document.getElementById("module-width");
        const moduleHeightInput = document.getElementById("module-height");
        const marginInput = document.getElementById("margin");
        const showTextCheckbox = document.getElementById("show-text");
        const qrOptions = document.getElementById("qr-options");
        const qrECCSelect = document.getElementById("qr-ecc");
        const qrMarginInput = document.getElementById("qr-margin");
        const generateBtn = document.getElementById("generate-btn");
        const clearBtn = document.getElementById("clear-btn");
        const errorMessage = document.getElementById("error-message");
        const placeholderText = document.getElementById("placeholder-text");
        const output = document.getElementById("barcode-output");
        const downloadPngBtn = document.getElementById("download-png");
        const downloadSvgBtn = document.getElementById("download-svg");

        let lastType = null;

        function setError(msg) {
          if (!msg) {
            errorMessage.textContent = "";
            errorMessage.classList.add("hidden");
          } else {
            errorMessage.textContent = msg;
            errorMessage.classList.remove("hidden");
          }
        }

        function toggleQrOptions() {
          const type = typeSelect.value;
          if (type === "qrcode") {
            qrOptions.classList.remove("hidden");
          } else {
            qrOptions.classList.add("hidden");
          }
        }

        function validateData(type, value) {
          value = value.trim();
          if (!value) {
            return { ok: false, msg: "Please enter some data to encode." };
          }

          if (["ean13", "ean8", "upca", "upce", "itf"].includes(type)) {
            if (!/^[0-9]+$/.test(value)) {
              return {
                ok: false,
                msg: "This barcode type accepts digits only (0–9).",
              };
            }
          }

          if (type === "ean13") {
            if (value.length !== 12 && value.length !== 13) {
              return { ok: false, msg: "EAN-13 must be 12 or 13 digits long." };
            }
          } else if (type === "ean8") {
            if (value.length !== 7 && value.length !== 8) {
              return { ok: false, msg: "EAN-8 must be 7 or 8 digits long." };
            }
          } else if (type === "upca") {
            if (value.length !== 11 && value.length !== 12) {
              return { ok: false, msg: "UPC-A must be 11 or 12 digits long." };
            }
          } else if (type === "upce") {
            if (value.length < 6 || value.length > 8) {
              return { ok: false, msg: "UPC-E is typically 6–8 digits long." };
            }
          } else if (type === "itf") {
            if (value.length % 2 !== 0) {
              return {
                ok: false,
                msg: "ITF (Interleaved 2 of 5) requires an even number of digits.",
              };
            }
          }

          return { ok: true, msg: "" };
        }

        function computeEAN13CheckDigit(digits12) {
          let odd = 0,
            even = 0;
          for (let i = 0; i < 12; i++) {
            const d = parseInt(digits12.charAt(i), 10);
            if ((i + 1) % 2 === 0) even += d;
            else odd += d;
          }
          const total = odd + 3 * even;
          const cd = (10 - (total % 10)) % 10;
          return String(cd);
        }

        function computeEAN8CheckDigit(digits7) {
          let odd = 0,
            even = 0;
          for (let i = 0; i < 7; i++) {
            const d = parseInt(digits7.charAt(i), 10);
            if ((i + 1) % 2 === 0) even += d;
            else odd += d;
          }
          const total = 3 * odd + even;
          const cd = (10 - (total % 10)) % 10;
          return String(cd);
        }

        function computeUPCACheckDigit(digits11) {
          let odd = 0,
            even = 0;
          for (let i = 0; i < 11; i++) {
            const d = parseInt(digits11.charAt(i), 10);
            if ((i + 1) % 2 === 1) odd += d;
            else even += d;
          }
          const total = 3 * odd + even;
          const cd = (10 - (total % 10)) % 10;
          return String(cd);
        }

        function normalizeData(type, value) {
          value = value.trim();
          if (type === "ean13" && value.length === 12) {
            return value + computeEAN13CheckDigit(value);
          }
          if (type === "ean8" && value.length === 7) {
            return value + computeEAN8CheckDigit(value);
          }
          if (type === "upca" && value.length === 11) {
            return value + computeUPCACheckDigit(value);
          }
          return value;
        }

        function clearOutput() {
          placeholderText.classList.remove("hidden");
          output.classList.add("hidden");
          output.innerHTML = "";
          setError("");
          downloadPngBtn.disabled = true;
          downloadSvgBtn.disabled = true;
        }

        function generateBarcode() {
          const type = typeSelect.value;
          const rawData = dataInput.value;
          const validation = validateData(type, rawData);
          if (!validation.ok) {
            setError(validation.msg);
            clearOutput();
            return;
          }
          setError("");

          const width = parseFloat(moduleWidthInput.value) || 2;
          const height = parseFloat(moduleHeightInput.value) || 80;
          const margin = parseInt(marginInput.value, 10) || 0;
          const showText = showTextCheckbox.checked;
          const qrECC = qrECCSelect.value;
          const qrMargin = parseInt(qrMarginInput.value, 10) || 4;

          const data = normalizeData(type, rawData);

          placeholderText.classList.add("hidden");
          output.classList.remove("hidden");
          output.innerHTML = "";

          try {
            if (type === "qrcode") {
              const size = Math.max(80, Math.min(600, height));
              const qrContainer = document.createElement("div");
              qrContainer.id = "qrcode-container";
              qrContainer.className = "inline-block";
              output.appendChild(qrContainer);

              new QRCode(qrContainer, {
                text: data,
                width: size,
                height: size,
                correctLevel:
                  QRCode.CorrectLevel[qrECC] || QRCode.CorrectLevel.M,
                margin: qrMargin,
              });
            } else if (type === "datamatrix") {
              const canvas = document.createElement("canvas");
              canvas.id = "datamatrix-canvas";
              output.appendChild(canvas);
              const scale = Math.max(1, Math.min(10, Math.round(width)));
              const h = Math.max(20, Math.min(600, height));
              const bw = bwipjs.toCanvas(canvas, {
                bcid: "datamatrix",
                text: data,
                scale: scale,
                height: h,
                includetext: false,
                paddingwidth: margin,
                paddingheight: margin,
              });
            } else {
              const svg = document.createElementNS(
                "http://www.w3.org/2000/svg",
                "svg"
              );
              svg.id = "barcode-svg";
              output.appendChild(svg);

              let format = "CODE128";
              switch (type) {
                case "code39":
                  format = "CODE39";
                  break;
                case "ean13":
                  format = "EAN13";
                  break;
                case "ean8":
                  format = "EAN8";
                  break;
                case "upca":
                  format = "UPC";
                  break;
                case "upce":
                  format = "UPC";
                  break;
                case "itf":
                  format = "ITF";
                  break;
                case "code93":
                  format = "CODE93";
                  break;
                default:
                  format = "CODE128";
              }

              JsBarcode(svg, data, {
                format: format,
                width: width,
                height: height,
                margin: margin,
                displayValue: showText,
                fontSize: 14,
                textMargin: 4,
                lineColor: "#000000",
                background: "#ffffff",
              });
            }

            downloadPngBtn.disabled = false;
            downloadSvgBtn.disabled = false;
            lastType = type;
          } catch (e) {
            console.error(e);
            setError(
              "Unable to generate this barcode. Please adjust the data or settings."
            );
            clearOutput();
          }
        }

        function downloadPNG() {
          if (downloadPngBtn.disabled) return;

          const type = lastType || typeSelect.value;

          if (type === "qrcode") {
            const container = document.getElementById("qrcode-container");
            if (!container) return;
            const img = container.querySelector("img");
            const canvas = container.querySelector("canvas");
            if (img && img.src) {
              const link = document.createElement("a");
              link.href = img.src;
              link.download = "barcode.png";
              link.click();
            } else if (canvas) {
              const link = document.createElement("a");
              link.href = canvas.toDataURL("image/png");
              link.download = "barcode.png";
              link.click();
            }
          } else if (type === "datamatrix") {
            const canvas = document.getElementById("datamatrix-canvas");
            if (!canvas) return;
            const link = document.createElement("a");
            link.href = canvas.toDataURL("image/png");
            link.download = "barcode.png";
            link.click();
          } else {
            const svg = document.getElementById("barcode-svg");
            if (!svg) return;
            const serializer = new XMLSerializer();
            const svgStr = serializer.serializeToString(svg);
            const img = new Image();
            const svgBlob = new Blob([svgStr], {
              type: "image/svg+xml;charset=utf-8",
            });
            const url = URL.createObjectURL(svgBlob);
            img.onload = function () {
              const canvas = document.createElement("canvas");
              const bbox = svg.getBBox();
              canvas.width = Math.ceil(bbox.width + bbox.x);
              canvas.height = Math.ceil(bbox.height + bbox.y + 10);
              const ctx = canvas.getContext("2d");
              ctx.fillStyle = "#ffffff";
              ctx.fillRect(0, 0, canvas.width, canvas.height);
              ctx.drawImage(img, 0, 0);
              URL.revokeObjectURL(url);
              const link = document.createElement("a");
              link.href = canvas.toDataURL("image/png");
              link.download = "barcode.png";
              link.click();
            };
            img.src = url;
          }
        }

        function downloadSVG() {
          if (downloadSvgBtn.disabled) return;

          const type = lastType || typeSelect.value;

          if (type === "qrcode") {
            const container = document.getElementById("qrcode-container");
            if (!container) return;
            const canvas = container.querySelector("canvas");
            if (!canvas) {
              alert(
                "SVG export for QR Code is not available in this mode. Please use PNG."
              );
              return;
            }
            const dataUrl = canvas.toDataURL("image/png");
            const img = new Image();
            img.onload = function () {
              const svg = document.createElementNS(
                "http://www.w3.org/2000/svg",
                "svg"
              );
              svg.setAttribute("xmlns", "http://www.w3.org/2000/svg");
              svg.setAttribute("width", img.width);
              svg.setAttribute("height", img.height);
              const image = document.createElementNS(
                "http://www.w3.org/2000/svg",
                "image"
              );
              image.setAttributeNS(
                "http://www.w3.org/1999/xlink",
                "href",
                dataUrl
              );
              image.setAttribute("width", img.width);
              image.setAttribute("height", img.height);
              svg.appendChild(image);
              const serializer = new XMLSerializer();
              const svgStr = serializer.serializeToString(svg);
              const blob = new Blob([svgStr], {
                type: "image/svg+xml;charset=utf-8",
              });
              const url = URL.createObjectURL(blob);
              const link = document.createElement("a");
              link.href = url;
              link.download = "barcode.svg";
              link.click();
              URL.revokeObjectURL(url);
            };
            img.src = dataUrl;
          } else if (type === "datamatrix") {
            const canvas = document.getElementById("datamatrix-canvas");
            if (!canvas) return;
            const dataUrl = canvas.toDataURL("image/png");
            const img = new Image();
            img.onload = function () {
              const svg = document.createElementNS(
                "http://www.w3.org/2000/svg",
                "svg"
              );
              svg.setAttribute("xmlns", "http://www.w3.org/2000/svg");
              svg.setAttribute("width", img.width);
              svg.setAttribute("height", img.height);
              const image = document.createElementNS(
                "http://www.w3.org/2000/svg",
                "image"
              );
              image.setAttributeNS(
                "http://www.w3.org/1999/xlink",
                "href",
                dataUrl
              );
              image.setAttribute("width", img.width);
              image.setAttribute("height", img.height);
              svg.appendChild(image);
              const serializer = new XMLSerializer();
              const svgStr = serializer.serializeToString(svg);
              const blob = new Blob([svgStr], {
                type: "image/svg+xml;charset=utf-8",
              });
              const url = URL.createObjectURL(blob);
              const link = document.createElement("a");
              link.href = url;
              link.download = "barcode.svg";
              link.click();
              URL.revokeObjectURL(url);
            };
            img.src = dataUrl;
          } else {
            const svg = document.getElementById("barcode-svg");
            if (!svg) return;
            const serializer = new XMLSerializer();
            let svgStr = serializer.serializeToString(svg);
            if (!svgStr.match(/^<svg[^>]+xmlns=/)) {
              svgStr = svgStr.replace(
                /^<svg/,
                '<svg xmlns="http://www.w3.org/2000/svg"'
              );
            }
            const blob = new Blob([svgStr], {
              type: "image/svg+xml;charset=utf-8",
            });
            const url = URL.createObjectURL(blob);
            const link = document.createElement("a");
            link.href = url;
            link.download = "barcode.svg";
            link.click();
            URL.revokeObjectURL(url);
          }
        }

        generateBtn.addEventListener("click", generateBarcode);
        clearBtn.addEventListener("click", function () {
          dataInput.value = "";
          clearOutput();
        });
        typeSelect.addEventListener("change", function () {
          toggleQrOptions();
          setError("");
        });
        downloadPngBtn.addEventListener("click", downloadPNG);
        downloadSvgBtn.addEventListener("click", downloadSVG);

        toggleQrOptions();
      })();
    </script>
  
