    <script>
      // Core configuration for rulesets
      const RULESETS = {
        "5e-standard": {
          name: "D&D 5e Standard",
          start: 8,
          min: 8,
          max: 15,
          maxFinal: 20,
          points: 27,
          costs: { 8: 0, 9: 1, 10: 2, 11: 3, 12: 4, 13: 5, 14: 7, 15: 9 },
        },
        "5e-variant": {
          name: "D&D 5e Variant",
          start: 8,
          min: 8,
          max: 18,
          maxFinal: 20,
          points: 32,
          // simple extension: 16->11, 17->13, 18->15 (house rule)
          costs: {
            8: 0,
            9: 1,
            10: 2,
            11: 3,
            12: 4,
            13: 5,
            14: 7,
            15: 9,
            16: 11,
            17: 13,
            18: 15,
          },
        },
        pathfinder: {
          name: "Pathfinder 1e (20 pt)",
          start: 10,
          min: 7,
          max: 18,
          maxFinal: 20,
          points: 20,
          // PF 1e standard 20‑point buy
          costs: {
            7: -4,
            8: -2,
            9: -1,
            10: 0,
            11: 1,
            12: 2,
            13: 3,
            14: 5,
            15: 7,
            16: 10,
            17: 13,
            18: 17,
          },
        },
        custom: {
          name: "Custom",
          start: 8,
          min: 3,
          max: 18,
          maxFinal: 20,
          points: 27,
          costs: { 8: 0, 9: 1, 10: 2, 11: 3, 12: 4, 13: 5, 14: 7, 15: 9 },
        },
      };

      const ABILITIES = ["STR", "DEX", "CON", "INT", "WIS", "CHA"];

      const RACE_BONUSES = {
        human: { STR: 1, DEX: 1, CON: 1, INT: 1, WIS: 1, CHA: 1 },
        "variant-human": {}, // handled via custom presets
        "half-elf": { CHA: 2 },
        "hill-dwarf": { CON: 2, WIS: 1 },
        "mountain-dwarf": { CON: 2, STR: 2 },
        "high-elf": { DEX: 2, INT: 1 },
        "wood-elf": { DEX: 2, WIS: 1 },
        "lightfoot-halfling": { DEX: 2, CHA: 1 },
        "stout-halfling": { DEX: 2, CON: 1 },
        dragonborn: { STR: 2, CHA: 1 },
        tiefling: { CHA: 2, INT: 1 },
        "custom-2-1": {}, // user chooses
        "custom-1-1-1": {},
        none: {},
      };

      const state = {
        rulesetKey: "5e-standard",
        bases: {},
        bonuses: {},
      };

      function initState() {
        const rs = RULESETS[state.rulesetKey];
        ABILITIES.forEach((a) => {
          if (state.bases[a] == null) state.bases[a] = rs.start;
          if (state.bonuses[a] == null) state.bonuses[a] = 0;
        });
      }

      function scoreCost(score) {
        const rs = RULESETS[state.rulesetKey];
        const c = rs.costs[score];
        return c != null ? c - rs.costs[rs.start] : 0;
      }

      function totalPointsSpent() {
        const rs = RULESETS[state.rulesetKey];
        let total = 0;
        ABILITIES.forEach((a) => {
          const s = state.bases[a];
          if (s < rs.min || s > rs.max) return;
          const baseCost = rs.costs[rs.start] ?? 0;
          const c = rs.costs[s];
          if (c != null) total += c - baseCost;
        });
        return total;
      }

      function abilityMod(score) {
        return Math.floor((score - 10) / 2);
      }

      function renderAbilitiesTable() {
        const tbody = document.getElementById("abilitiesBody");
        tbody.innerHTML = "";
        const rs = RULESETS[state.rulesetKey];

        ABILITIES.forEach((a) => {
          const base = state.bases[a];
          const bonus = state.bonuses[a] || 0;
          const finalScore = base + bonus;
          const cost = scoreCost(base);
          const mod = abilityMod(finalScore);
          const row = document.createElement("tr");

          row.innerHTML = `
        <td class="px-3 py-2 font-medium text-gray-800">${a}</td>
        <td class="px-3 py-2 text-center">
          <div class="inline-flex items-center gap-1">
            <button type="button" class="px-1.5 py-0.5 border border-gray-300 rounded text-xs text-gray-700 hover:bg-gray-50" data-ability="${a}" data-delta="-1">−</button>
            <input type="number" inputmode="numeric" class="stat-input w-14 text-center border border-gray-300 rounded px-1 py-0.5 text-sm" id="base-${a}" value="${base}" min="${
            rs.min
          }" max="${rs.max}" aria-label="${a} base score" />
            <button type="button" class="px-1.5 py-0.5 border border-gray-300 rounded text-xs text-gray-700 hover:bg-gray-50" data-ability="${a}" data-delta="1">+</button>
          </div>
        </td>
        <td class="px-3 py-2 text-center text-gray-700">${cost}</td>
        <td class="px-3 py-2 text-center">
          <input type="number" inputmode="numeric" class="w-14 text-center border border-gray-300 rounded px-1 py-0.5 text-sm" id="bonus-${a}" value="${bonus}" aria-label="${a} racial or custom bonus" />
        </td>
        <td class="px-3 py-2 text-center font-semibold text-gray-800">${finalScore}</td>
        <td class="px-3 py-2 text-center">
          <span class="inline-flex items-center justify-center px-2 py-0.5 rounded-full text-xs ${
            mod >= 0 ? "bg-green-50 text-green-700" : "bg-red-50 text-red-700"
          }">
            ${mod >= 0 ? "+" + mod : mod}
          </span>
        </td>
      `;
          tbody.appendChild(row);
        });

        attachAbilityHandlers();
        updatePointsUI();
        updateSummary();
        updateShareString();
      }

      function attachAbilityHandlers() {
        document.querySelectorAll("button[data-ability]").forEach((btn) => {
          btn.addEventListener("click", () => {
            const ability = btn.getAttribute("data-ability");
            const delta = parseInt(btn.getAttribute("data-delta"), 10);
            adjustBaseScore(ability, delta);
          });
        });

        ABILITIES.forEach((a) => {
          const baseInput = document.getElementById(`base-${a}`);
          const bonusInput = document.getElementById(`bonus-${a}`);
          baseInput.addEventListener("change", () => {
            const rs = RULESETS[state.rulesetKey];
            let v = parseInt(baseInput.value, 10);
            if (isNaN(v)) v = rs.start;
            v = Math.max(rs.min, Math.min(rs.max, v));
            state.bases[a] = v;
            renderAbilitiesTable();
          });
          bonusInput.addEventListener("change", () => {
            let v = parseInt(bonusInput.value, 10);
            if (isNaN(v)) v = 0;
            state.bonuses[a] = v;
            renderAbilitiesTable();
          });
        });
      }

      function adjustBaseScore(ability, delta) {
        const rs = RULESETS[state.rulesetKey];
        let v = state.bases[ability] + delta;
        v = Math.max(rs.min, Math.min(rs.max, v));
        state.bases[ability] = v;
        renderAbilitiesTable();
      }

      function updatePointsUI() {
        const rs = RULESETS[state.rulesetKey];
        const spent = totalPointsSpent();
        const max = rs.points;
        const spentEl = document.getElementById("pointsSpent");
        const maxEl = document.getElementById("pointsMax");
        const bar = document.getElementById("pointsBar");
        const status = document.getElementById("pointsStatus");

        spentEl.textContent = spent;
        maxEl.textContent = max;

        const ratio = Math.max(0, Math.min(1, spent / max));
        bar.style.width = ratio * 100 + "%";

        const inputs = document.querySelectorAll(".stat-input");
        let invalid = false;
        inputs.forEach((inp) => inp.setAttribute("aria-invalid", "false"));

        if (spent < max) {
          status.textContent = "Within budget";
          status.className =
            "text-xs badge bg-green-100 text-green-700 border border-green-200";
        } else if (spent === max) {
          status.textContent = "Exactly spent";
          status.className =
            "text-xs badge bg-yellow-100 text-yellow-700 border border-yellow-200";
        } else {
          status.textContent = "Over budget";
          status.className =
            "text-xs badge bg-red-100 text-red-700 border border-red-200";
          invalid = true;
        }

        if (invalid) {
          inputs.forEach((inp) => inp.setAttribute("aria-invalid", "true"));
        }

        // Range / start labels
        document.getElementById("rangeLabel").textContent =
          rs.min + "–" + rs.max;
        document.getElementById("startValueLabel").textContent = rs.start;
        document.getElementById("maxFinalLabel").textContent = rs.maxFinal;

        // Cost table
        const costBody = document.getElementById("costTableBody");
        costBody.innerHTML = "";
        const scores = Object.keys(rs.costs)
          .map(Number)
          .sort((a, b) => a - b);
        scores.forEach((s) => {
          const tr = document.createElement("tr");
          tr.innerHTML = `<td class="px-2 py-1">${s}</td><td class="px-2 py-1">${
            rs.costs[s] - (rs.costs[rs.start] ?? 0)
          }</td>`;
          costBody.appendChild(tr);
        });
      }

      function applyRacePreset(key) {
        const bonuses = RACE_BONUSES[key] || {};
        ABILITIES.forEach((a) => {
          state.bonuses[a] = bonuses[a] || 0;
        });

        // Handle flexible presets
        if (key === "variant-human" || key === "custom-2-1") {
          // default: +2 to primary (STR) and +1 to secondary (CON)
          state.bonuses.STR = 2;
          state.bonuses.CON = 1;
        }
        if (key === "custom-1-1-1") {
          state.bonuses.STR = 1;
          state.bonuses.DEX = 1;
          state.bonuses.CON = 1;
          state.bonuses.INT = 0;
          state.bonuses.WIS = 0;
          state.bonuses.CHA = 0;
        }

        renderAbilitiesTable();
      }

      function applyBuildPreset(type) {
        const rs = RULESETS[state.rulesetKey];
        if (rs.name.indexOf("Pathfinder") !== -1) {
          // Simple PF-friendly presets
          if (type === "martial") {
            state.bases = {
              STR: 16,
              DEX: 12,
              CON: 14,
              INT: 10,
              WIS: 10,
              CHA: 8,
            };
          } else if (type === "caster") {
            state.bases = {
              STR: 8,
              DEX: 12,
              CON: 14,
              INT: 16,
              WIS: 10,
              CHA: 10,
            };
          } else {
            state.bases = {
              STR: 14,
              DEX: 14,
              CON: 14,
              INT: 10,
              WIS: 10,
              CHA: 10,
            };
          }
        } else {
          // 5e presets (27‑point friendly)
          if (type === "martial") {
            state.bases = {
              STR: 15,
              DEX: 10,
              CON: 15,
              INT: 8,
              WIS: 10,
              CHA: 8,
            };
          } else if (type === "caster") {
            state.bases = {
              STR: 8,
              DEX: 14,
              CON: 14,
              INT: 15,
              WIS: 10,
              CHA: 8,
            };
          } else {
            state.bases = {
              STR: 14,
              DEX: 14,
              CON: 14,
              INT: 10,
              WIS: 10,
              CHA: 8,
            };
          }
        }
        renderAbilitiesTable();
      }

      function updateSummary() {
        const rs = RULESETS[state.rulesetKey];
        const spent = totalPointsSpent();
        let lines = [];
        lines.push(`${rs.name} – ${spent}/${rs.points} points`);
        lines.push("");

        ABILITIES.forEach((a) => {
          const base = state.bases[a];
          const bonus = state.bonuses[a] || 0;
          const finalScore = base + bonus;
          const mod = abilityMod(finalScore);
          const bonusStr = bonus === 0 ? "" : ` (+${bonus})`;
          lines.push(
            `${a}: ${base}${bonusStr} = ${finalScore} (${
              mod >= 0 ? "+" + mod : mod
            })`
          );
        });

        document.getElementById("summaryBlock").textContent = lines.join("\n");
      }

      function updateShareString() {
        const rs = RULESETS[state.rulesetKey];
        const payload = {
          ruleset: state.rulesetKey,
          bases: state.bases,
          bonuses: state.bonuses,
        };
        const encoded = btoa(JSON.stringify(payload));
        const share = `ruleset=${rs.name}; build=${encoded}`;
        document.getElementById("shareableString").value = share;
      }

      function exportJson() {
        const payload = {
          ruleset: state.rulesetKey,
          rules: RULESETS[state.rulesetKey],
          bases: state.bases,
          bonuses: state.bonuses,
          pointsSpent: totalPointsSpent(),
        };
        const blob = new Blob([JSON.stringify(payload, null, 2)], {
          type: "application/json",
        });
        const url = URL.createObjectURL(blob);
        const a = document.createElement("a");
        a.href = url;
        a.download = "dnd-point-buy-build.json";
        document.body.appendChild(a);
        a.click();
        document.body.removeChild(a);
        URL.revokeObjectURL(url);
      }

      function copySummary() {
        const text = document.getElementById("summaryBlock").textContent;
        if (!navigator.clipboard) {
          const ta = document.createElement("textarea");
          ta.value = text;
          document.body.appendChild(ta);
          ta.select();
          document.execCommand("copy");
          document.body.removeChild(ta);
          return;
        }
        navigator.clipboard.writeText(text).catch(() => {});
      }

      function resetScores() {
        const rs = RULESETS[state.rulesetKey];
        ABILITIES.forEach((a) => {
          state.bases[a] = rs.start;
          state.bonuses[a] = 0;
        });
        document.getElementById("racePreset").value = "none";
        renderAbilitiesTable();
      }

      function initControls() {
        document.getElementById("ruleset").addEventListener("change", (e) => {
          state.rulesetKey = e.target.value;
          const rs = RULESETS[state.rulesetKey];
          document.getElementById("totalPoints").value = rs.points;
          initState();
          renderAbilitiesTable();
        });

        document
          .getElementById("totalPoints")
          .addEventListener("change", (e) => {
            let v = parseInt(e.target.value, 10);
            if (isNaN(v) || v <= 0) v = RULESETS[state.rulesetKey].points;
            RULESETS[state.rulesetKey].points = v;
            updatePointsUI();
            updateSummary();
            updateShareString();
          });

        document
          .getElementById("racePreset")
          .addEventListener("change", (e) => {
            applyRacePreset(e.target.value);
          });

        document.querySelectorAll(".preset-btn").forEach((btn) => {
          btn.addEventListener("click", () => {
            applyBuildPreset(btn.getAttribute("data-preset"));
          });
        });

        document
          .getElementById("copySummaryBtn")
          .addEventListener("click", copySummary);
        document
          .getElementById("exportJsonBtn")
          .addEventListener("click", exportJson);
        document
          .getElementById("resetBtn")
          .addEventListener("click", resetScores);
      }

      document.addEventListener("DOMContentLoaded", () => {
        initState();
        initControls();
        renderAbilitiesTable();
      });
    </script>

