    <script>
      // Sealant calculator logic
      (function () {
        const unitMetricBtn = document.getElementById("unit-metric");
        const unitImperialBtn = document.getElementById("unit-imperial");
        const jointWidthInput = document.getElementById("joint-width");
        const jointDepthInput = document.getElementById("joint-depth");
        const jointLengthInput = document.getElementById("joint-length");
        const jointCountInput = document.getElementById("joint-count");
        const jointTypeSelect = document.getElementById("joint-type");
        const packagePresetSelect = document.getElementById("package-preset");
        const packageVolumeInput = document.getElementById("package-volume");
        const wasteFactorInput = document.getElementById("waste-factor");

        const jointWidthUnit = document.getElementById("joint-width-unit");
        const jointDepthUnit = document.getElementById("joint-depth-unit");
        const jointLengthUnit = document.getElementById("joint-length-unit");
        const packageVolumeUnit = document.getElementById(
          "package-volume-unit"
        );

        const calculateBtn = document.getElementById("calculate-btn");
        const resultsContainer = document.getElementById("results-container");
        const errorMessage = document.getElementById("error-message");

        const resultVolumeMain = document.getElementById("result-volume-main");
        const resultVolumeAlt = document.getElementById("result-volume-alt");
        const resultTubesMain = document.getElementById("result-tubes-main");
        const resultTubesDetail = document.getElementById(
          "result-tubes-detail"
        );
        const resultNotes = document.getElementById("result-notes");

        let unitSystem = "metric"; // 'metric' or 'imperial'

        function setUnitSystem(system) {
          unitSystem = system;

          if (system === "metric") {
            unitMetricBtn.classList.add("bg-blue-600", "text-white");
            unitMetricBtn.classList.remove("bg-white", "text-gray-700");
            unitImperialBtn.classList.remove("bg-blue-600", "text-white");
            unitImperialBtn.classList.add("bg-white", "text-gray-700");

            jointWidthUnit.textContent = "mm";
            jointDepthUnit.textContent = "mm";
            jointLengthUnit.textContent = "m";
            packageVolumeUnit.textContent = "ml";

            // If current package preset is imperial, reset
            if (["10.1", "20"].includes(packagePresetSelect.value)) {
              packagePresetSelect.value = "";
            }
            if (!packagePresetSelect.value) {
              packageVolumeInput.value = 300;
            }
          } else {
            unitImperialBtn.classList.add("bg-blue-600", "text-white");
            unitImperialBtn.classList.remove("bg-white", "text-gray-700");
            unitMetricBtn.classList.remove("bg-blue-600", "text-white");
            unitMetricBtn.classList.add("bg-white", "text-gray-700");

            jointWidthUnit.textContent = "in";
            jointDepthUnit.textContent = "in";
            jointLengthUnit.textContent = "ft";
            packageVolumeUnit.textContent = "fl oz";

            if (["300", "600"].includes(packagePresetSelect.value)) {
              packagePresetSelect.value = "";
            }
            if (!packagePresetSelect.value) {
              packageVolumeInput.value = 10.1;
            }
          }
          resultsContainer.classList.add("hidden");
          errorMessage.classList.add("hidden");
        }

        function applyJointTypePreset() {
          const type = jointTypeSelect.value;
          if (!type) return;

          if (unitSystem === "metric") {
            switch (type) {
              case "window":
                jointWidthInput.value = 8;
                jointDepthInput.value = 8;
                break;
              case "façade":
                jointWidthInput.value = 15;
                jointDepthInput.value = 8;
                break;
              case "floor":
                jointWidthInput.value = 20;
                jointDepthInput.value = 10;
                break;
              case "sanitary":
                jointWidthInput.value = 6;
                jointDepthInput.value = 6;
                break;
            }
          } else {
            switch (type) {
              case "window":
                jointWidthInput.value = 0.3;
                jointDepthInput.value = 0.3;
                break;
              case "façade":
                jointWidthInput.value = 0.6;
                jointDepthInput.value = 0.3;
                break;
              case "floor":
                jointWidthInput.value = 0.75;
                jointDepthInput.value = 0.4;
                break;
              case "sanitary":
                jointWidthInput.value = 0.25;
                jointDepthInput.value = 0.25;
                break;
            }
          }
          resultsContainer.classList.add("hidden");
          errorMessage.classList.add("hidden");
        }

        function applyPackagePreset() {
          const val = packagePresetSelect.value;
          if (!val) return;

          if (unitSystem === "metric") {
            if (val === "300" || val === "600") {
              packageVolumeInput.value = Number(val);
            } else if (val === "10.1") {
              // 10.1 fl oz ≈ 299 ml
              packageVolumeInput.value = 300;
            } else if (val === "20") {
              // 20 fl oz ≈ 591 ml
              packageVolumeInput.value = 600;
            }
          } else {
            if (val === "10.1" || val === "20") {
              packageVolumeInput.value = Number(val);
            } else if (val === "300") {
              // 300 ml ≈ 10.1 fl oz
              packageVolumeInput.value = 10.1;
            } else if (val === "600") {
              // 600 ml ≈ 20.3 fl oz
              packageVolumeInput.value = 20;
            }
          }
          resultsContainer.classList.add("hidden");
          errorMessage.classList.add("hidden");
        }

        function formatNumber(num, decimals = 2) {
          if (!isFinite(num)) return "–";
          const fixed = num.toFixed(decimals);
          return parseFloat(fixed).toString();
        }

        function calculate() {
          errorMessage.classList.add("hidden");
          resultsContainer.classList.add("hidden");

          const w = parseFloat(jointWidthInput.value);
          const d = parseFloat(jointDepthInput.value);
          const L = parseFloat(jointLengthInput.value);
          const n = parseInt(jointCountInput.value, 10);
          const pkg = parseFloat(packageVolumeInput.value);
          const waste = parseFloat(wasteFactorInput.value);

          if ([w, d, L, n, pkg].some((v) => isNaN(v) || v <= 0)) {
            errorMessage.textContent =
              "Please enter positive values for width, depth, length, number of joints, and package volume.";
            errorMessage.classList.remove("hidden");
            return;
          }
          if (isNaN(waste) || waste < 0) {
            errorMessage.textContent =
              "Waste factor must be zero or a positive percentage.";
            errorMessage.classList.remove("hidden");
            return;
          }

          let totalVolumeMain; // ml or fl oz
          let totalVolumeAlt; // fl oz or ml

          if (unitSystem === "metric") {
            // width, depth in mm; length in m; result in ml
            const volumePerMeterMl = (w * d) / 1000; // ml/m
            const totalLength = L * n; // m
            totalVolumeMain = volumePerMeterMl * totalLength; // ml

            // convert to fl oz for alt
            totalVolumeAlt = totalVolumeMain / 29.5735;
          } else {
            // width, depth in inches; length in ft
            const lengthInches = L * 12;
            const totalLengthInches = lengthInches * n;
            const volumeIn3 = w * d * totalLengthInches; // cubic inches
            totalVolumeMain = volumeIn3 / 1.80469; // fl oz
            totalVolumeAlt = totalVolumeMain * 29.5735; // ml
          }

          const tubesTheoretical = totalVolumeMain / pkg;
          const tubesWithWaste = tubesTheoretical * (1 + (waste || 0) / 100);
          const tubesRounded = Math.ceil(tubesWithWaste);

          // Populate results
          if (unitSystem === "metric") {
            resultVolumeMain.textContent = `${formatNumber(
              totalVolumeMain,
              1
            )} ml`;
            resultVolumeAlt.textContent = `≈ ${formatNumber(
              totalVolumeAlt,
              1
            )} fl oz`;
          } else {
            resultVolumeMain.textContent = `${formatNumber(
              totalVolumeMain,
              2
            )} fl oz`;
            resultVolumeAlt.textContent = `≈ ${formatNumber(
              totalVolumeAlt,
              0
            )} ml`;
          }

          resultTubesMain.textContent = `${tubesRounded} ${
            tubesRounded === 1 ? "tube/sausage" : "tubes/sausages"
          }`;
          resultTubesDetail.textContent =
            `Theoretical: ${formatNumber(tubesTheoretical, 2)} units, ` +
            `with ${formatNumber(waste || 0, 0)}% waste: ${formatNumber(
              tubesWithWaste,
              2
            )} units (rounded up).`;

          const totalLengthDisplay = formatNumber(L * n, 2);
          const lengthUnit = unitSystem === "metric" ? "m" : "ft";
          const jointSizeDisplay =
            unitSystem === "metric"
              ? `${formatNumber(w, 1)} × ${formatNumber(d, 1)} mm`
              : `${formatNumber(w, 2)} × ${formatNumber(d, 2)} in`;

          resultNotes.innerHTML =
            `Based on a joint size of <strong>${jointSizeDisplay}</strong> and a total joint length of ` +
            `<strong>${totalLengthDisplay} ${lengthUnit}</strong>. ` +
            `Actual consumption can vary with substrate roughness, tooling, and application technique. ` +
            `Always round up and consider keeping one extra tube for touch-ups.`;

          resultsContainer.classList.remove("hidden");
        }

        // Event listeners
        unitMetricBtn.addEventListener("click", () => setUnitSystem("metric"));
        unitImperialBtn.addEventListener("click", () =>
          setUnitSystem("imperial")
        );
        jointTypeSelect.addEventListener("change", applyJointTypePreset);
        packagePresetSelect.addEventListener("change", applyPackagePreset);
        calculateBtn.addEventListener("click", calculate);

        // Enter key triggers calculation
        [
          jointWidthInput,
          jointDepthInput,
          jointLengthInput,
          jointCountInput,
          packageVolumeInput,
          wasteFactorInput,
        ].forEach((el) => {
          el.addEventListener("keydown", (e) => {
            if (e.key === "Enter") {
              e.preventDefault();
              calculate();
            }
          });
        });

        // Initialize
        setUnitSystem("metric");
      })();
    </script>

