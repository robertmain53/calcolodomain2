    <script>
      // Currency prefix handling
      const currencySelect = document.getElementById("currency");
      const currencyPrefixEls = [
        document.getElementById("currency-prefix"),
        document.getElementById("currency-prefix-begin"),
        document.getElementById("currency-prefix-end"),
      ];

      function updateCurrencyPrefix() {
        const map = {
          USD: "$",
          EUR: "€",
          GBP: "£",
          CAD: "$",
          AUD: "$",
          INR: "₹",
          other: "",
        };
        const symbol = map[currencySelect.value] || "";
        currencyPrefixEls.forEach((el) => {
          if (el) el.textContent = symbol;
        });
      }
      currencySelect.addEventListener("change", updateCurrencyPrefix);
      updateCurrencyPrefix();

      // Calculator logic
      const netSalesEl = document.getElementById("netSales");
      const periodDaysEl = document.getElementById("periodDays");
      const beginAREl = document.getElementById("beginAR");
      const endAREl = document.getElementById("endAR");
      const errorMsgEl = document.getElementById("errorMsg");
      const resultsSection = document.getElementById("resultsSection");
      const turnoverResultEl = document.getElementById("turnoverResult");
      const dsoResultEl = document.getElementById("dsoResult");
      const avgARResultEl = document.getElementById("avgARResult");
      const interpretationResultEl = document.getElementById(
        "interpretationResult"
      );

      function formatNumber(num, decimals = 2) {
        if (!isFinite(num)) return "–";
        return num.toLocaleString(undefined, {
          minimumFractionDigits: decimals,
          maximumFractionDigits: decimals,
        });
      }

      function interpret(turnover, dso) {
        if (!isFinite(turnover) || turnover <= 0)
          return "Unable to interpret. Please check your inputs.";
        if (dso < 0)
          return "Unable to interpret. Please check your period days.";

        if (dso <= 30) {
          return "Excellent collection efficiency. Customers are paying very quickly relative to typical net 30–60 terms.";
        } else if (dso <= 60) {
          return "Healthy receivables turnover. Collections are broadly aligned with common B2B credit terms.";
        } else if (dso <= 90) {
          return "Collections are on the slow side. Review credit terms, invoicing accuracy, and follow-up processes.";
        } else {
          return "Very slow collections and potential credit risk. Consider tightening credit policy and strengthening collections.";
        }
      }

      document.getElementById("calculateBtn").addEventListener("click", () => {
        errorMsgEl.textContent = "";
        resultsSection.classList.add("hidden");

        const netSales = parseFloat(netSalesEl.value);
        const periodDays = parseFloat(periodDaysEl.value);
        const beginAR =
          beginAREl.value === "" ? null : parseFloat(beginAREl.value);
        const endAR = parseFloat(endAREl.value);

        if (isNaN(netSales) || netSales < 0) {
          errorMsgEl.textContent =
            "Please enter a valid non-negative value for net credit sales.";
          return;
        }
        if (isNaN(endAR) || endAR <= 0) {
          errorMsgEl.textContent =
            "Please enter a positive ending accounts receivable balance.";
          return;
        }
        if (isNaN(periodDays) || periodDays <= 0) {
          errorMsgEl.textContent =
            "Please enter a positive number of days in the period.";
          return;
        }
        if (beginAR !== null && beginAR < 0) {
          errorMsgEl.textContent =
            "Beginning accounts receivable cannot be negative.";
          return;
        }

        let avgAR;
        if (beginAR !== null) {
          avgAR = (beginAR + endAR) / 2;
        } else {
          avgAR = endAR;
        }

        if (avgAR <= 0) {
          errorMsgEl.textContent =
            "Average accounts receivable must be greater than zero.";
          return;
        }

        const turnover = netSales / avgAR;
        const dso = periodDays / turnover;

        turnoverResultEl.textContent = formatNumber(turnover, 2) + " ×";
        dsoResultEl.textContent = formatNumber(dso, 1) + " days";
        avgARResultEl.textContent = formatNumber(avgAR, 2);
        interpretationResultEl.textContent = interpret(turnover, dso);

        resultsSection.classList.remove("hidden");
      });

      document.getElementById("resetBtn").addEventListener("click", () => {
        netSalesEl.value = "";
        periodDaysEl.value = 365;
        beginAREl.value = "";
        endAREl.value = "";
        errorMsgEl.textContent = "";
        resultsSection.classList.add("hidden");
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
