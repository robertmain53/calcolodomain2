    <script>
      // Approximate scoring tables by gender & age group
      const strengthParams = {
        male: {
          under30: { push: { min: 20, max: 60 }, sit: { min: 25, max: 55 } },
          "30to39": { push: { min: 18, max: 55 }, sit: { min: 22, max: 50 } },
          "40to49": { push: { min: 15, max: 50 }, sit: { min: 20, max: 45 } },
          "50plus": { push: { min: 12, max: 45 }, sit: { min: 18, max: 40 } },
        },
        female: {
          under30: { push: { min: 15, max: 45 }, sit: { min: 20, max: 50 } },
          "30to39": { push: { min: 13, max: 40 }, sit: { min: 18, max: 45 } },
          "40to49": { push: { min: 10, max: 35 }, sit: { min: 16, max: 40 } },
          "50plus": { push: { min: 8, max: 30 }, sit: { min: 14, max: 35 } },
        },
      };

      const runParams = {
        male: {
          under30: { best: 8.5 * 60, worst: 15 * 60 },
          "30to39": { best: 9 * 60, worst: 15.5 * 60 },
          "40to49": { best: 9.5 * 60, worst: 16 * 60 },
          "50plus": { best: 10 * 60, worst: 17 * 60 },
        },
        female: {
          under30: { best: 9.5 * 60, worst: 16 * 60 },
          "30to39": { best: 10 * 60, worst: 16.5 * 60 },
          "40to49": { best: 10.5 * 60, worst: 17 * 60 },
          "50plus": { best: 11 * 60, worst: 18 * 60 },
        },
      };

      function clamp(val, min, max) {
        return Math.min(Math.max(val, min), max);
      }

      function scoreStrength(reps, minReps, maxReps, maxPoints) {
        if (reps <= minReps) return 0;
        if (reps >= maxReps) return maxPoints;
        const ratio = (reps - minReps) / (maxReps - minReps);
        return ratio * maxPoints;
      }

      function scoreRun(timeSec, bestSec, worstSec, maxPoints) {
        if (timeSec <= bestSec) return maxPoints;
        if (timeSec >= worstSec) return 0;
        const ratio = (worstSec - timeSec) / (worstSec - bestSec);
        return ratio * maxPoints;
      }

      function updateTrainingHelper() {
        const targetPush = parseInt(
          document.getElementById("targetPushups").value || "0",
          10
        );
        const targetSit = parseInt(
          document.getElementById("targetSitups").value || "0",
          10
        );
        const pushWeekly = targetPush * 5; // 5 sessions/week
        const sitWeekly = (sitWeekly = targetSit * 5);
        document.getElementById("pushWeekly").textContent = isFinite(pushWeekly)
          ? pushWeekly
          : "–";
        document.getElementById("sitWeekly").textContent = isFinite(sitWeekly)
          ? sitWeekly
          : "–";
      }

      document.addEventListener("DOMContentLoaded", () => {
        const genderEl = document.getElementById("gender");
        const ageGroupEl = document.getElementById("ageGroup");
        const pushupsEl = document.getElementById("pushups");
        const situpsEl = document.getElementById("situps");
        const runMinEl = document.getElementById("runMin");
        const runSecEl = document.getElementById("runSec");
        const totalScoreEl = document.getElementById("totalScore");
        const pushScoreEl = document.getElementById("pushScore");
        const sitScoreEl = document.getElementById("sitScore");
        const runScoreEl = document.getElementById("runScore");
        const categoryBadgeEl = document.getElementById("categoryBadge");
        const calculateBtn = document.getElementById("calculateBtn");
        const resetBtn = document.getElementById("resetBtn");

        function categorize(score) {
          if (score >= 90)
            return { label: "Excellent", color: "bg-green-100 text-green-800" };
          if (score >= 75)
            return {
              label: "Satisfactory",
              color: "bg-yellow-100 text-yellow-800",
            };
          return { label: "Unsatisfactory", color: "bg-red-100 text-red-800" };
        }

        function calculate() {
          const gender = genderEl.value;
          const ageGroup = ageGroupEl.value;

          const push = clamp(parseInt(pushupsEl.value || "0", 10), 0, 200);
          const sit = clamp(parseInt(situpsEl.value || "0", 10), 0, 200);
          const runMin = clamp(parseInt(runMinEl.value || "0", 10), 5, 40);
          const runSec = clamp(parseInt(runSecEl.value || "0", 10), 0, 59);
          const runTotalSec = runMin * 60 + runSec;

          const sp = strengthParams[gender][ageGroup];
          const rp = runParams[gender][ageGroup];

          const pushScore = scoreStrength(push, sp.push.min, sp.push.max, 20);
          const sitScore = scoreStrength(sit, sp.sit.min, sp.sit.max, 20);
          const runScore = scoreRun(runTotalSec, rp.best, rp.worst, 60);

          const total = pushScore + sitScore + runScore;

          pushScoreEl.textContent = pushScore.toFixed(1);
          sitScoreEl.textContent = sitScore.toFixed(1);
          runScoreEl.textContent = runScore.toFixed(1);
          totalScoreEl.textContent = total.toFixed(1);

          const cat = categorize(total);
          categoryBadgeEl.textContent = cat.label;
          categoryBadgeEl.className = "badge " + cat.color;
        }

        calculateBtn.addEventListener("click", calculate);
        resetBtn.addEventListener("click", () => {
          genderEl.value = "male";
          ageGroupEl.value = "under30";
          pushupsEl.value = 30;
          situpsEl.value = 35;
          runMinEl.value = 12;
          runSecEl.value = 30;
          totalScoreEl.textContent = "0.0";
          pushScoreEl.textContent = "0.0";
          sitScoreEl.textContent = "0.0";
          runScoreEl.textContent = "0.0";
          categoryBadgeEl.textContent = "–";
          categoryBadgeEl.className = "badge bg-gray-200 text-gray-800";
        });

        // Training helper listeners
        document
          .getElementById("targetPushups")
          .addEventListener("input", updateTrainingHelper);
        document
          .getElementById("targetSitups")
          .addEventListener("input", updateTrainingHelper);
        updateTrainingHelper();

        // Initial calculation
        calculate();
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
