            <script>
              document.addEventListener('DOMContentLoaded', () => {
                const cupsInput = document.getElementById('cups-input');
                const flozInput = document.getElementById('floz-input');
                const tbspInput = document.getElementById('tbsp-input');
                const tspInput = document.getElementById('tsp-input');

                const inputs = [cupsInput, flozInput, tbspInput, tspInput];

                // Define everything in terms of 1 TEASPOON (our base unit)
                const CONVERSIONS = {
                    tsp: 1,
                    tbsp: 3,        // 1 tbsp = 3 tsp
                    floz: 6,        // 1 fl oz = 2 tbsp = 6 tsp
                    cup: 48         // 1 cup = 16 tbsp = 48 tsp
                };

                function formatNum(num) {
                    if (isNaN(num) || num === 0) return '';
                    // Rimuovi gli zeri finali non necessari
                    return parseFloat(num.toFixed(4)).toString();
                }
                
                function update(sourceUnit, sourceValue) {
                    if (isNaN(sourceValue) || sourceValue === 0) {
                        inputs.forEach(input => input.value = '');
                        return;
                    }

                    // 1. Convert the source value to the base unit (teaspoons)
                    const baseValueTsp = sourceValue * CONVERSIONS[sourceUnit];

                    // 2. Aggiorna tutti gli altri campi
                    inputs.forEach(input => {
                        const unit = input.dataset.unit;
                        if (unit !== sourceUnit) {
                            const convertedValue = baseValueTsp / CONVERSIONS[unit];
                            input.value = formatNum(convertedValue);
                        }
                    });
                }

                inputs.forEach(input => {
                  input.addEventListener('input', (e) => {
                    const sourceUnit = e.target.dataset.unit;
                    const value = parseFloat(e.target.value);
                    update(sourceUnit, value);
                  });
                });
              });
            </script>
            
