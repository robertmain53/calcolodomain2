    <script>
      // Utility formatting
      function fmtMoney(value) {
        if (!isFinite(value)) return "$0";
        const v = Math.round(value);
        return v.toLocaleString("en-US", {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 0,
        });
      }
      function fmtPercent(value) {
        if (!isFinite(value)) return "0%";
        return value.toFixed(1).replace(/\.0$/, "") + "%";
      }

      function getFundingFeeRate(serviceType, usageType, downPct, exempt) {
        if (exempt) return 0;
        // Approximate current VA purchase funding fee schedule
        let rate = 0;
        const isFirst = usageType === "first";
        const isRegular = serviceType === "regular";

        if (downPct < 5) {
          if (isFirst) {
            rate = isRegular ? 2.15 : 2.4;
          } else {
            rate = 3.3;
          }
        } else if (downPct < 10) {
          rate = 1.5;
        } else {
          rate = 1.25;
        }
        return rate;
      }

      function calculateVA() {
        const homePrice =
          parseFloat(document.getElementById("homePrice").value) || 0;
        const downAmountInput =
          parseFloat(document.getElementById("downPaymentAmount").value) || 0;
        const downPctInput =
          parseFloat(document.getElementById("downPaymentPercent").value) || 0;
        const rate =
          parseFloat(document.getElementById("interestRate").value) || 0;
        const termYears =
          parseInt(document.getElementById("loanTerm").value, 10) || 30;
        const taxAnnual =
          parseFloat(document.getElementById("propertyTax").value) || 0;
        const insAnnual =
          parseFloat(document.getElementById("homeInsurance").value) || 0;
        const hoaMonthly =
          parseFloat(document.getElementById("hoaDues").value) || 0;

        const serviceType = document.getElementById("serviceType").value;
        const usageType = document.getElementById("usageType").value;
        const fundingExempt =
          document.querySelector('input[name="fundingExempt"]:checked')
            ?.value === "yes";

        const countyLimit =
          parseFloat(document.getElementById("countyLimit").value) || 0;
        const incomeMonthly =
          parseFloat(document.getElementById("grossIncome").value) || 0;
        const otherDebts =
          parseFloat(document.getElementById("otherDebts").value) || 0;
        const targetDTI =
          parseFloat(document.getElementById("targetDTI").value) || 41;

        const errorEl = document.getElementById("calcError");

        if (homePrice <= 0 || rate <= 0 || termYears <= 0) {
          errorEl.classList.remove("hidden");
          return;
        } else {
          errorEl.classList.add("hidden");
        }

        // Sync down payment fields: if user entered % > 0, override amount
        let downAmount = downAmountInput;
        let downPct = downPctInput;
        if (downPct > 0 && homePrice > 0) {
          downAmount = homePrice * (downPct / 100);
        } else if (downAmount > 0 && homePrice > 0) {
          downPct = (downAmount / homePrice) * 100;
        } else {
          downAmount = 0;
          downPct = 0;
        }
        // Update fields to keep them in sync
        document.getElementById("downPaymentAmount").value =
          Math.round(downAmount);
        document.getElementById("downPaymentPercent").value = downPct
          .toFixed(1)
          .replace(/\.0$/, "");

        const baseLoan = Math.max(homePrice - downAmount, 0);
        const fundingRate = getFundingFeeRate(
          serviceType,
          usageType,
          downPct,
          fundingExempt
        );
        const fundingFee = baseLoan * (fundingRate / 100);
        const totalLoan = baseLoan + fundingFee;

        const n = termYears * 12;
        const monthlyRate = rate / 100 / 12;
        let piMonthly = 0;
        if (monthlyRate > 0 && n > 0) {
          const pow = Math.pow(1 + monthlyRate, n);
          piMonthly = (totalLoan * (monthlyRate * pow)) / (pow - 1);
        }

        const taxMonthly = taxAnnual / 12;
        const insMonthly = insAnnual / 12;
        const totalMonthly = piMonthly + taxMonthly + insMonthly + hoaMonthly;

        // DTI and affordability
        let dti = 0;
        let maxHousing = 0;
        let maxPrice = 0;

        if (incomeMonthly > 0) {
          dti = ((otherDebts + totalMonthly) / incomeMonthly) * 100;
          maxHousing = (targetDTI / 100) * incomeMonthly - otherDebts;
          if (maxHousing < 0) maxHousing = 0;

          // Estimate max price by solving for loan amount that yields PI = maxHousing - escrows - HOA
          const escrows = taxMonthly + insMonthly + hoaMonthly;
          const maxPI = Math.max(maxHousing - escrows, 0);
          if (monthlyRate > 0 && n > 0 && maxPI > 0) {
            const pow2 = Math.pow(1 + monthlyRate, n);
            const maxLoan = (maxPI * (pow2 - 1)) / (monthlyRate * pow2);
            // Assume same down payment percentage for affordability estimate
            const assumedDownPct = downPct / 100;
            maxPrice =
              assumedDownPct < 1 ? maxLoan / (1 - assumedDownPct) : maxLoan;
          }
        }

        // Entitlement estimates
        const fullEntitlement = countyLimit * 0.25;
        const usedEntitlement = totalLoan * 0.25;
        const remainingEntitlement = Math.max(
          fullEntitlement - usedEntitlement,
          0
        );

        // Update UI
        document.getElementById("baseLoanDisplay").textContent =
          fmtMoney(baseLoan);
        document.getElementById("fundingRateDisplay").textContent =
          fmtPercent(fundingRate);
        document.getElementById("fundingFeeDisplay").textContent =
          fmtMoney(fundingFee);
        document.getElementById("totalLoanDisplay").textContent =
          fmtMoney(totalLoan);

        document.getElementById("piMonthlyDisplay").textContent =
          fmtMoney(piMonthly);
        document.getElementById("taxMonthlyDisplay").textContent =
          fmtMoney(taxMonthly);
        document.getElementById("insMonthlyDisplay").textContent =
          fmtMoney(insMonthly);
        document.getElementById("hoaMonthlyDisplay").textContent =
          fmtMoney(hoaMonthly);
        document.getElementById("totalMonthlyDisplay").textContent =
          fmtMoney(totalMonthly);

        document.getElementById("dtiDisplay").textContent = fmtPercent(dti);
        document.getElementById("targetDtiDisplay").textContent =
          fmtPercent(targetDTI);
        document.getElementById("maxHousingDisplay").textContent =
          fmtMoney(maxHousing);
        document.getElementById("maxPriceDisplay").textContent =
          fmtMoney(maxPrice);

        document.getElementById("limitDisplay").textContent =
          fmtMoney(countyLimit);
        document.getElementById("fullEntitlementDisplay").textContent =
          fmtMoney(fullEntitlement);
        document.getElementById("usedEntitlementDisplay").textContent =
          fmtMoney(usedEntitlement);
        document.getElementById("remainingEntitlementDisplay").textContent =
          fmtMoney(remainingEntitlement);
      }

      document
        .getElementById("calculateBtn")
        .addEventListener("click", calculateVA);

      document.getElementById("resetBtn").addEventListener("click", () => {
        document.getElementById("homePrice").value = 400000;
        document.getElementById("downPaymentAmount").value = 0;
        document.getElementById("downPaymentPercent").value = 0;
        document.getElementById("interestRate").value = 6.25;
        document.getElementById("loanTerm").value = 30;
        document.getElementById("propertyTax").value = 4800;
        document.getElementById("homeInsurance").value = 1200;
        document.getElementById("hoaDues").value = 0;
        document.getElementById("serviceType").value = "regular";
        document.getElementById("usageType").value = "first";
        document
          .querySelectorAll('input[name="fundingExempt"]')
          .forEach((r) => {
            r.checked = r.value === "no";
          });
        document.getElementById("countyLimit").value = 766550;
        document.getElementById("grossIncome").value = 8000;
        document.getElementById("otherDebts").value = 500;
        document.getElementById("targetDTI").value = 41;
        document.getElementById("calcError").classList.add("hidden");
        calculateVA();
      });

      // Auto-recalculate on input changes (debounced)
      let vaTimer;
      document
        .querySelectorAll(
          '#homePrice,#downPaymentAmount,#downPaymentPercent,#interestRate,#loanTerm,#propertyTax,#homeInsurance,#hoaDues,#serviceType,#usageType,#countyLimit,#grossIncome,#otherDebts,#targetDTI,input[name="fundingExempt"]'
        )
        .forEach((el) => {
          el.addEventListener("input", () => {
            clearTimeout(vaTimer);
            vaTimer = setTimeout(calculateVA, 300);
          });
          el.addEventListener("change", () => {
            clearTimeout(vaTimer);
            vaTimer = setTimeout(calculateVA, 300);
          });
        });

      // Initial calculation
      calculateVA();
    </script>

