    <script>
      // Core data structures
      const state = {
        guests: [], // {id, name, side}
        tables: [], // {id, name, seats, shape, assignments: [guestId|null]}
        selectedGuestId: null,
      };

      const guestListEl = document.getElementById("guest-list");
      const tablesContainerEl = document.getElementById("tables-container");
      const guestCountEl = document.getElementById("guest-count");
      const tableCountEl = document.getElementById("table-count");
      const printableOutputEl = document.getElementById("printable-output");

      const statTotalGuestsEl = document.getElementById("stat-total-guests");
      const statSeatedGuestsEl = document.getElementById("stat-seated-guests");
      const statUnassignedGuestsEl = document.getElementById(
        "stat-unassigned-guests"
      );
      const statTotalSeatsEl = document.getElementById("stat-total-seats");
      const statEmptySeatsEl = document.getElementById("stat-empty-seats");

      function uid() {
        return Math.random().toString(36).slice(2, 10);
      }

      // Render functions
      function renderGuests() {
        guestListEl.innerHTML = "";
        if (!state.guests.length) {
          guestListEl.innerHTML =
            '<p class="text-xs text-gray-400 text-center mt-8">Add guests to start assigning seats.</p>';
          guestCountEl.textContent = "0 guests";
          return;
        }
        state.guests.forEach((g) => {
          const assigned = isGuestAssigned(g.id);
          const div = document.createElement("div");
          div.className =
            "guest-pill flex items-center justify-between px-2 py-1 rounded-md text-xs mb-1 " +
            (state.selectedGuestId === g.id
              ? "bg-blue-600 text-white"
              : assigned
              ? "bg-emerald-50 text-emerald-800 border border-emerald-200"
              : "bg-white text-gray-800 border border-gray-200");
          div.dataset.guestId = g.id;
          div.innerHTML = `
        <span class="truncate">${g.name}${
            g.side
              ? ' <span class="text-[10px] opacity-80">(' + g.side + ")</span>"
              : ""
          }</span>
        <button type="button" class="ml-2 text-[10px] ${
          state.selectedGuestId === g.id
            ? "text-blue-100"
            : "text-gray-400 hover:text-red-500"
        }" data-remove-guest="${g.id}">&times;</button>
      `;
          guestListEl.appendChild(div);
        });
        guestCountEl.textContent = `${state.guests.length} guest${
          state.guests.length !== 1 ? "s" : ""
        }`;
      }

      function renderTables() {
        tablesContainerEl.innerHTML = "";
        if (!state.tables.length) {
          tablesContainerEl.innerHTML =
            '<p class="text-xs text-gray-400 text-center col-span-full mt-8">Add tables to see them here.</p>';
          tableCountEl.textContent = "0 tables";
          return;
        }
        state.tables.forEach((table) => {
          const card = document.createElement("div");
          card.className =
            "border border-gray-200 rounded-md p-2 flex flex-col gap-2 bg-gray-50";
          card.dataset.tableId = table.id;

          const header = document.createElement("div");
          header.className = "flex items-center justify-between text-xs mb-1";
          header.innerHTML = `
        <div>
          <span class="font-semibold">${table.name || "Table"}</span>
          <span class="text-gray-500 ml-1">(${table.seats} seats, ${
            table.shape === "round" ? "round" : "rect"
          })</span>
        </div>
        <div class="flex gap-1">
          <button type="button" class="px-1.5 py-0.5 border border-gray-300 rounded text-[10px] text-gray-600 hover:bg-white" data-edit-table="${
            table.id
          }">Edit</button>
          <button type="button" class="px-1.5 py-0.5 border border-red-200 rounded text-[10px] text-red-600 hover:bg-red-50" data-delete-table="${
            table.id
          }">Delete</button>
        </div>
      `;
          card.appendChild(header);

          const layout = document.createElement("div");
          layout.className = "flex-1 flex items-center justify-center";
          layout.appendChild(renderTableLayout(table));
          card.appendChild(layout);

          tablesContainerEl.appendChild(card);
        });
        tableCountEl.textContent = `${state.tables.length} table${
          state.tables.length !== 1 ? "s" : ""
        }`;
      }

      function renderTableLayout(table) {
        const container = document.createElement("div");
        container.className =
          "relative w-full max-w-[220px] h-40 flex items-center justify-center";

        if (table.shape === "round") {
          const circle = document.createElement("div");
          circle.className =
            "relative w-28 h-28 rounded-full border border-gray-300 bg-white flex items-center justify-center text-[10px] text-gray-500";
          circle.textContent = table.name || "Table";
          container.appendChild(circle);

          const radius = 55;
          const centerX = 110;
          const centerY = 80;
          for (let i = 0; i < table.seats; i++) {
            const angle = (2 * Math.PI * i) / table.seats - Math.PI / 2;
            const x = centerX + radius * Math.cos(angle);
            const y = centerY + radius * Math.sin(angle);

            const seat = document.createElement("button");
            seat.type = "button";
            seat.className =
              "seat absolute w-7 h-7 rounded-full border text-[10px] flex items-center justify-center bg-white";
            seat.style.left = x - 14 + "px";
            seat.style.top = y - 14 + "px";
            seat.dataset.tableId = table.id;
            seat.dataset.seatIndex = i;

            const guestId = table.assignments[i] || null;
            if (guestId) {
              const guest = state.guests.find((g) => g.id === guestId);
              seat.textContent = guest ? guest.name.split(" ")[0] : "Guest";
              seat.classList.add(
                "border-emerald-500",
                "bg-emerald-50",
                "text-emerald-800"
              );
            } else {
              seat.textContent = i + 1;
              seat.classList.add("border-gray-300", "text-gray-500");
            }

            if (state.selectedGuestId && guestId === state.selectedGuestId) {
              seat.classList.add("seat-selected");
            }

            container.appendChild(seat);
          }
        } else {
          const rect = document.createElement("div");
          rect.className =
            "relative w-32 h-20 border border-gray-300 bg-white flex items-center justify-center text-[10px] text-gray-500";
          rect.textContent = table.name || "Table";
          container.appendChild(rect);

          const perSide = Math.ceil(table.seats / 4);
          let seatIndex = 0;

          function createSeat(x, y) {
            if (seatIndex >= table.seats) return;
            const seat = document.createElement("button");
            seat.type = "button";
            seat.className =
              "seat absolute w-7 h-7 rounded-full border text-[10px] flex items-center justify-center bg-white";
            seat.style.left = x + "px";
            seat.style.top = y + "px";
            seat.dataset.tableId = table.id;
            seat.dataset.seatIndex = seatIndex;

            const guestId = table.assignments[seatIndex] || null;
            if (guestId) {
              const guest = state.guests.find((g) => g.id === guestId);
              seat.textContent = guest ? guest.name.split(" ")[0] : "Guest";
              seat.classList.add(
                "border-emerald-500",
                "bg-emerald-50",
                "text-emerald-800"
              );
            } else {
              seat.textContent = seatIndex + 1;
              seat.classList.add("border-gray-300", "text-gray-500");
            }

            if (state.selectedGuestId && guestId === state.selectedGuestId) {
              seat.classList.add("seat-selected");
            }

            container.appendChild(seat);
            seatIndex++;
          }

          const leftX = 10;
          const rightX = 32 + 10 + 32; // approximate
          const topY = 10;
          const bottomY = 20 + 10 + 20;

          for (let i = 0; i < perSide; i++) {
            createSeat(leftX, topY + i * 18);
          }
          for (let i = 0; i < perSide; i++) {
            createSeat(rightX, topY + i * 18);
          }
          for (let i = 0; i < perSide; i++) {
            createSeat(leftX + 18 + i * 18, topY - 12);
          }
          for (let i = 0; i < perSide; i++) {
            createSeat(leftX + 18 + i * 18, bottomY + 4);
          }
        }

        return container;
      }

      function isGuestAssigned(guestId) {
        return state.tables.some((t) => t.assignments.includes(guestId));
      }

      function updateStatsAndPrintable() {
        const totalGuests = state.guests.length;
        let seatedGuestsSet = new Set();
        let totalSeats = 0;
        let emptySeats = 0;

        state.tables.forEach((t) => {
          totalSeats += t.seats;
          t.assignments.forEach((a) => {
            if (a) seatedGuestsSet.add(a);
            else emptySeats++;
          });
        });

        const seatedGuests = seatedGuestsSet.size;
        const unassignedGuests = totalGuests - seatedGuests;

        statTotalGuestsEl.textContent = totalGuests;
        statSeatedGuestsEl.textContent = seatedGuests;
        statUnassignedGuestsEl.textContent = unassignedGuests;
        statTotalSeatsEl.textContent = totalSeats;
        statEmptySeatsEl.textContent = emptySeats;

        // Printable summary
        let lines = [];
        state.tables.forEach((t) => {
          lines.push(
            `${t.name || "Table"} (${t.seats} seats, ${
              t.shape === "round" ? "round" : "rectangular"
            })`
          );
          t.assignments.forEach((guestId, idx) => {
            const seatNum = idx + 1;
            if (guestId) {
              const g = state.guests.find((gg) => gg.id === guestId);
              lines.push(`  Seat ${seatNum}: ${g ? g.name : "Guest"}`);
            } else {
              lines.push(`  Seat ${seatNum}: [empty]`);
            }
          });
          lines.push("");
        });

        const unassigned = state.guests.filter((g) => !isGuestAssigned(g.id));
        if (unassigned.length) {
          lines.push("Unassigned guests:");
          unassigned.forEach((g) => lines.push(`  - ${g.name}`));
        }

        printableOutputEl.value = lines.join("\n");
      }

      function rerenderAll() {
        renderGuests();
        renderTables();
        updateStatsAndPrintable();
      }

      // Event handlers
      document.getElementById("add-guest-btn").addEventListener("click", () => {
        const nameInput = document.getElementById("guest-name");
        const sideInput = document.getElementById("guest-side");
        const name = nameInput.value.trim();
        if (!name) return;
        state.guests.push({ id: uid(), name, side: sideInput.value || "" });
        nameInput.value = "";
        sideInput.value = "";
        rerenderAll();
      });

      document.getElementById("bulk-add-btn").addEventListener("click", () => {
        const bulk = document.getElementById("bulk-guests");
        const lines = bulk.value
          .split("\n")
          .map((l) => l.trim())
          .filter(Boolean);
        if (!lines.length) return;
        lines.forEach((name) => {
          state.guests.push({ id: uid(), name, side: "" });
        });
        bulk.value = "";
        rerenderAll();
      });

      document
        .getElementById("clear-guests-btn")
        .addEventListener("click", () => {
          if (!confirm("Remove all guests and clear seat assignments?")) return;
          state.guests = [];
          state.tables.forEach(
            (t) => (t.assignments = new Array(t.seats).fill(null))
          );
          state.selectedGuestId = null;
          rerenderAll();
        });

      document.getElementById("add-table-btn").addEventListener("click", () => {
        const name = document.getElementById("table-name").value.trim();
        const seats = parseInt(
          document.getElementById("table-seats").value,
          10
        );
        const shape = document.getElementById("table-shape").value;
        if (!seats || seats < 1) return;
        const table = {
          id: uid(),
          name: name || `Table ${state.tables.length + 1}`,
          seats,
          shape: shape === "rect" ? "rect" : "round",
          assignments: new Array(seats).fill(null),
        };
        state.tables.push(table);
        document.getElementById("table-name").value = "";
        rerenderAll();
      });

      document
        .getElementById("clear-tables-btn")
        .addEventListener("click", () => {
          if (!confirm("Remove all tables and seat assignments?")) return;
          state.tables = [];
          rerenderAll();
        });

      // Guest list click (select/remove)
      guestListEl.addEventListener("click", (e) => {
        const removeId = e.target.getAttribute("data-remove-guest");
        if (removeId) {
          // Remove guest and clear from assignments
          state.tables.forEach((t) => {
            t.assignments = t.assignments.map((a) =>
              a === removeId ? null : a
            );
          });
          state.guests = state.guests.filter((g) => g.id !== removeId);
          if (state.selectedGuestId === removeId) state.selectedGuestId = null;
          rerenderAll();
          return;
        }
        const pill = e.target.closest("[data-guest-id]");
        if (!pill) return;
        const id = pill.dataset.guestId;
        state.selectedGuestId = state.selectedGuestId === id ? null : id;
        rerenderAll();
      });

      // Tables container click (seats, edit, delete)
      tablesContainerEl.addEventListener("click", (e) => {
        const deleteId = e.target.getAttribute("data-delete-table");
        if (deleteId) {
          state.tables = state.tables.filter((t) => t.id !== deleteId);
          rerenderAll();
          return;
        }
        const editId = e.target.getAttribute("data-edit-table");
        if (editId) {
          const table = state.tables.find((t) => t.id === editId);
          if (!table) return;
          const newName = prompt("Table name/number:", table.name);
          if (newName !== null) table.name = newName.trim() || table.name;
          const newSeatsStr = prompt("Number of seats:", table.seats);
          if (newSeatsStr !== null) {
            const newSeats = parseInt(newSeatsStr, 10);
            if (newSeats && newSeats > 0 && newSeats <= 30) {
              const oldAssignments = table.assignments.slice(0, newSeats);
              table.seats = newSeats;
              table.assignments = new Array(newSeats).fill(null);
              for (let i = 0; i < oldAssignments.length; i++) {
                table.assignments[i] = oldAssignments[i];
              }
            }
          }
          const newShape = prompt("Shape (round/rect):", table.shape);
          if (newShape && (newShape === "round" || newShape === "rect")) {
            table.shape = newShape;
          }
          rerenderAll();
          return;
        }

        const seat = e.target.closest(".seat");
        if (seat) {
          const tableId = seat.dataset.tableId;
          const seatIndex = parseInt(seat.dataset.seatIndex, 10);
          const table = state.tables.find((t) => t.id === tableId);
          if (!table) return;

          const currentGuestId = table.assignments[seatIndex] || null;

          if (!state.selectedGuestId) {
            // No guest selected: toggle clear
            if (currentGuestId) {
              table.assignments[seatIndex] = null;
              rerenderAll();
            }
            return;
          }

          // If seat already has this guest, unassign
          if (currentGuestId === state.selectedGuestId) {
            table.assignments[seatIndex] = null;
            rerenderAll();
            return;
          }

          // Remove guest from any other seat
          state.tables.forEach((t) => {
            t.assignments = t.assignments.map((a) =>
              a === state.selectedGuestId ? null : a
            );
          });

          // Assign to this seat
          table.assignments[seatIndex] = state.selectedGuestId;
          rerenderAll();
        }
      });

      // Auto-fill tables
      document.getElementById("auto-fill-btn").addEventListener("click", () => {
        const unassignedGuests = state.guests.filter(
          (g) => !isGuestAssigned(g.id)
        );
        if (!unassignedGuests.length || !state.tables.length) return;

        let gi = 0;
        state.tables.forEach((t) => {
          for (let i = 0; i < t.seats && gi < unassignedGuests.length; i++) {
            if (!t.assignments[i]) {
              t.assignments[i] = unassignedGuests[gi].id;
              gi++;
            }
          }
        });
        rerenderAll();
      });

      document
        .getElementById("clear-assignments-btn")
        .addEventListener("click", () => {
          state.tables.forEach(
            (t) => (t.assignments = new Array(t.seats).fill(null))
          );
          rerenderAll();
        });

      // Export / Import JSON
      document
        .getElementById("export-json-btn")
        .addEventListener("click", () => {
          const data = JSON.stringify(state, null, 2);
          const blob = new Blob([data], { type: "application/json" });
          const url = URL.createObjectURL(blob);
          const a = document.createElement("a");
          a.href = url;
          a.download = "wedding-seating-chart.json";
          a.click();
          URL.revokeObjectURL(url);
        });

      document
        .getElementById("import-json-btn")
        .addEventListener("click", () => {
          const input = document.createElement("input");
          input.type = "file";
          input.accept = "application/json";
          input.onchange = (e) => {
            const file = e.target.files[0];
            if (!file) return;
            const reader = new FileReader();
            reader.onload = (ev) => {
              try {
                const data = JSON.parse(ev.target.result);
                if (
                  data &&
                  Array.isArray(data.guests) &&
                  Array.isArray(data.tables)
                ) {
                  state.guests = data.guests;
                  state.tables = data.tables;
                  state.selectedGuestId = null;
                  rerenderAll();
                } else {
                  alert("Invalid seating chart file.");
                }
              } catch (err) {
                alert("Could not read file.");
              }
            };
            reader.readAsText(file);
          };
          input.click();
        });

      document
        .getElementById("copy-printable-btn")
        .addEventListener("click", async () => {
          try {
            await navigator.clipboard.writeText(printableOutputEl.value);
            alert("Printable summary copied to clipboard.");
          } catch {
            printableOutputEl.select();
            document.execCommand("copy");
            alert("Printable summary copied. If not, please copy manually.");
          }
        });

      // Initial render
      rerenderAll();

      // Mobile menu script (from template)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", () => {
          const menu = document.getElementById("mobile-menu");
          const expanded = menu.classList.contains("hidden") ? "true" : "false";
          menu.classList.toggle("hidden");
          document
            .getElementById("mobile-menu-toggle")
            .setAttribute("aria-expanded", expanded);
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
