    <script>
      // Utility: format currency
      function formatCurrency(value) {
        if (isNaN(value) || value === null) return "$0";
        return (
          "$" + value.toLocaleString("en-US", { maximumFractionDigits: 0 })
        );
      }

      function parseAmount(input) {
        const v = parseFloat(input.value.replace(/,/g, ""));
        return isNaN(v) ? 0 : v;
      }

      const defaultOneTime = [
        {
          category: "Formation & legal",
          desc: "Incorporation, licenses, legal fees",
          amount: 1500,
        },
        {
          category: "Branding & website",
          desc: "Logo, design, initial website build",
          amount: 2500,
        },
        {
          category: "Equipment & hardware",
          desc: "Laptops, monitors, office setup",
          amount: 6000,
        },
        {
          category: "Initial marketing",
          desc: "Launch campaign, ads, collateral",
          amount: 3000,
        },
      ];

      const defaultMonthly = [
        {
          category: "Rent / coworking",
          desc: "Office or coworking space",
          amount: 1500,
        },
        {
          category: "Salaries & contractors",
          desc: "Founders, early hires, freelancers",
          amount: 12000,
        },
        {
          category: "Software & tools",
          desc: "SaaS, hosting, domain, email",
          amount: 600,
        },
        {
          category: "Marketing & sales",
          desc: "Ads, CRM, events",
          amount: 1500,
        },
        {
          category: "Insurance & admin",
          desc: "Insurance, accounting, misc.",
          amount: 800,
        },
      ];

      const oneTimeContainer = document.getElementById("one-time-rows");
      const monthlyContainer = document.getElementById("monthly-rows");

      function createRow(item, type) {
        const row = document.createElement("div");
        row.className = "flex items-center px-3 py-2 gap-2 text-sm";
        row.innerHTML = `
      <div class="w-2/5">
        <input type="text" class="w-full px-2 py-1 border border-gray-200 rounded text-sm focus:ring-1 focus:ring-blue-500 focus:border-blue-500"
               value="${
                 item.category || ""
               }" placeholder="Category (e.g., Legal)">
      </div>
      <div class="w-2/5">
        <input type="text" class="w-full px-2 py-1 border border-gray-200 rounded text-sm focus:ring-1 focus:ring-blue-500 focus:border-blue-500"
               value="${item.desc || ""}" placeholder="Description (optional)">
      </div>
      <div class="w-1/5 flex items-center justify-end gap-1">
        <span class="text-xs text-gray-400">$</span>
        <input type="number" min="0" step="50"
               class="amount-input w-24 px-2 py-1 border border-gray-200 rounded text-right text-sm focus:ring-1 focus:ring-blue-500 focus:border-blue-500"
               value="${item.amount != null ? item.amount : ""}">
        <button type="button" class="text-gray-300 hover:text-red-500 ml-1 remove-row" aria-label="Remove row">&times;</button>
      </div>
    `;
        const amountInput = row.querySelector(".amount-input");
        amountInput.addEventListener("input", recalcAll);
        row.querySelector(".remove-row").addEventListener("click", () => {
          row.remove();
          recalcAll();
        });
        return row;
      }

      function initRows() {
        oneTimeContainer.innerHTML = "";
        monthlyContainer.innerHTML = "";
        defaultOneTime.forEach((item) =>
          oneTimeContainer.appendChild(createRow(item, "one-time"))
        );
        defaultMonthly.forEach((item) =>
          monthlyContainer.appendChild(createRow(item, "monthly"))
        );
      }

      function sumContainer(container) {
        let total = 0;
        container.querySelectorAll(".amount-input").forEach((input) => {
          total += parseAmount(input);
        });
        return total;
      }

      function recalcAll() {
        const oneTimeTotal = sumContainer(oneTimeContainer);
        const monthlyTotal = sumContainer(monthlyContainer);
        const runwayMonths =
          parseInt(document.getElementById("runway-months").value, 10) || 0;
        const runwayCost = monthlyTotal * runwayMonths;
        const totalStartupBudget = oneTimeTotal + runwayCost;

        document.getElementById("one-time-subtotal").textContent =
          formatCurrency(oneTimeTotal);
        document.getElementById("monthly-subtotal").textContent =
          formatCurrency(monthlyTotal);
        document.getElementById("monthly-burn").textContent =
          formatCurrency(monthlyTotal);
        document.getElementById("runway-cost").textContent =
          formatCurrency(runwayCost);
        document.getElementById("total-startup-budget").textContent =
          formatCurrency(totalStartupBudget);

        document.getElementById("summary-one-time").textContent =
          formatCurrency(oneTimeTotal);
        document.getElementById("summary-runway").textContent =
          formatCurrency(runwayCost);
        document.getElementById("summary-total").textContent =
          formatCurrency(totalStartupBudget);

        const availableCapitalInput =
          document.getElementById("available-capital");
        const availableCapital = parseAmount(availableCapitalInput);
        const gap = availableCapital - totalStartupBudget;
        const gapEl = document.getElementById("capital-gap");
        const noteEl = document.getElementById("capital-gap-note");

        gapEl.textContent = formatCurrency(Math.abs(gap));
        if (!availableCapitalInput.value) {
          gapEl.textContent = "$0";
          noteEl.textContent =
            "Enter how much capital or funding you expect to have to see if you are over- or under-funded.";
          gapEl.className = "font-semibold text-slate-900";
        } else if (gap >= 0) {
          gapEl.textContent = formatCurrency(gap);
          gapEl.className = "font-semibold text-emerald-700";
          noteEl.textContent =
            "You appear to have enough capital to cover your planned startup budget, with a positive buffer.";
        } else {
          gapEl.textContent = "-" + formatCurrency(Math.abs(gap));
          gapEl.className = "font-semibold text-red-700";
          noteEl.textContent =
            "You may need additional funding, cost reductions, or a shorter runway to close this gap.";
        }
      }

      document.addEventListener("DOMContentLoaded", () => {
        initRows();
        recalcAll();

        document
          .getElementById("runway-months")
          .addEventListener("input", () => {
            const input = document.getElementById("runway-months");
            if (parseInt(input.value, 10) < 0) input.value = 0;
            recalcAll();
          });

        document
          .getElementById("available-capital")
          .addEventListener("input", recalcAll);

        document
          .getElementById("add-one-time-row")
          .addEventListener("click", () => {
            oneTimeContainer.appendChild(
              createRow({ category: "", desc: "", amount: null }, "one-time")
            );
          });

        document
          .getElementById("add-monthly-row")
          .addEventListener("click", () => {
            monthlyContainer.appendChild(
              createRow({ category: "", desc: "", amount: null }, "monthly")
            );
          });

        document.getElementById("reset-all").addEventListener("click", () => {
          document.getElementById("runway-months").value = 12;
          document.getElementById("available-capital").value = "";
          initRows();
          recalcAll();
        });

        document.getElementById("export-csv").addEventListener("click", () => {
          const runwayMonths =
            parseInt(document.getElementById("runway-months").value, 10) || 0;
          const rows = [];
          rows.push(["Type", "Category", "Description", "Amount (USD)"]);

          oneTimeContainer
            .querySelectorAll(".flex.items-center")
            .forEach((row) => {
              const inputs = row.querySelectorAll("input");
              const category = inputs[0].value || "";
              const desc = inputs[1].value || "";
              const amount = parseAmount(inputs[2]) || 0;
              if (category || desc || amount) {
                rows.push(["One-time", category, desc, amount]);
              }
            });

          monthlyContainer
            .querySelectorAll(".flex.items-center")
            .forEach((row) => {
              const inputs = row.querySelectorAll("input");
              const category = inputs[0].value || "";
              const desc = inputs[1].value || "";
              const amount = parseAmount(inputs[2]) || 0;
              if (category || desc || amount) {
                rows.push(["Monthly", category, desc, amount]);
              }
            });

          rows.push([]);
          rows.push(["Summary"]);
          rows.push(["Runway months", runwayMonths]);
          rows.push(["One-time total", sumContainer(oneTimeContainer)]);
          const monthlyTotal = sumContainer(monthlyContainer);
          rows.push(["Monthly burn", monthlyTotal]);
          rows.push(["Runway cost", monthlyTotal * runwayMonths]);
          rows.push([
            "Total startup budget",
            sumContainer(oneTimeContainer) + monthlyTotal * runwayMonths,
          ]);

          const csvContent = rows
            .map((r) =>
              r
                .map((field) => {
                  if (
                    typeof field === "string" &&
                    (field.includes(",") || field.includes('"'))
                  ) {
                    return '"' + field.replace(/"/g, '""') + '"';
                  }
                  return field;
                })
                .join(",")
            )
            .join("\r\n");

          const blob = new Blob([csvContent], {
            type: "text/csv;charset=utf-8;",
          });
          const url = URL.createObjectURL(blob);
          const a = document.createElement("a");
          a.href = url;
          a.download = "startup-costs-estimate.csv";
          document.body.appendChild(a);
          a.click();
          document.body.removeChild(a);
          URL.revokeObjectURL(url);
        });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
