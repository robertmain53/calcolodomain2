    <script>
    (function() {
      function normalizeMod(x, m) {
        var r = x % m;
        return (r + m) % m;
      }

      function extendedGcd(a, b) {
        var old_r = a, r = b;
        var old_s = 1, s = 0;
        var old_t = 0, t = 1;
        var steps = [];
        steps.push({ step: 0, q: null, r: old_r, s: old_s, t: old_t });
        var step = 1;

        while (r !== 0) {
          var q = Math.floor(old_r / r);
          var tmp_r = old_r - q * r;
          var tmp_s = old_s - q * s;
          var tmp_t = old_t - q * t;

          steps.push({ step: step, q: q, r: r, s: s, t: t });

          old_r = r; r = tmp_r;
          old_s = s; s = tmp_s;
          old_t = t; t = tmp_t;
          step++;
        }

        steps.push({ step: step, q: null, r: r, s: s, t: t });
        return { g: old_r, x: old_s, y: old_t, steps: steps };
      }

      function $(id) {
        return document.getElementById(id);
      }

      function clearChildren(el) {
        while (el.firstChild) {
          el.removeChild(el.firstChild);
        }
      }

      function setExample(a, m) {
        $('a-input').value = a;
        $('m-input').value = m;
        var errorEl = $('error-container');
        var resultContainer = $('result-container');
        var stepsContainer = $('steps-container');
        errorEl.classList.add('hidden');
        resultContainer.classList.add('hidden');
        stepsContainer.classList.add('hidden');
      }

      document.addEventListener('DOMContentLoaded', function() {
        var form = $('modinv-form');
        var errorEl = $('error-container');
        var resultContainer = $('result-container');
        var resultSummary = $('result-summary');
        var resultDetails = $('result-details');
        var verificationBox = $('verification-box');
        var stepsContainer = $('steps-container');
        var stepsTableBody = $('steps-table-body');
        var showStepsCheckbox = $('show-steps');
        var resetButton = $('reset-button');

        form.addEventListener('submit', function(e) {
          e.preventDefault();

          errorEl.classList.add('hidden');
          resultContainer.classList.add('hidden');
          stepsContainer.classList.add('hidden');
          clearChildren(stepsTableBody);
          verificationBox.innerHTML = '';

          var aVal = $('a-input').value.trim();
          var mVal = $('m-input').value.trim();

          if (aVal === '' || mVal === '') {
            errorEl.textContent = 'Please enter both a and the modulus m.';
            errorEl.classList.remove('hidden');
            return;
          }

          var a = Number(aVal);
          var m = Number(mVal);

          if (!Number.isFinite(a) || !Number.isInteger(a) || !Number.isFinite(m) || !Number.isInteger(m)) {
            errorEl.textContent = 'Both a and m must be integers.';
            errorEl.classList.remove('hidden');
            return;
          }

          if (Math.abs(a) > Number.MAX_SAFE_INTEGER || Math.abs(m) > Number.MAX_SAFE_INTEGER) {
            errorEl.textContent = 'For numerical stability, please use integers with absolute value less than 9,007,199,254,740,991.';
            errorEl.classList.remove('hidden');
            return;
          }

          if (m <= 1) {
            errorEl.textContent = 'The modulus m must be greater than 1.';
            errorEl.classList.remove('hidden');
            return;
          }

          if (a === 0) {
            errorEl.textContent = 'a = 0 does not have a modular inverse.';
            errorEl.classList.remove('hidden');
            return;
          }

          var aNorm = normalizeMod(a, m);
          var eg = extendedGcd(aNorm, m);
          var g = Math.abs(eg.g);

          if (g !== 1) {
            resultSummary.textContent = 'No modular inverse exists.';
            resultDetails.innerHTML =
              '<p>The greatest common divisor gcd(a, m) is ' + g +
              ', which is greater than 1. Only pairs (a, m) with gcd(a, m) = 1 admit a modular multiplicative inverse.</p>';
            resultContainer.classList.remove('hidden');
            return;
          }

          var inv = normalizeMod(eg.x, m);
          var check = normalizeMod(aNorm * inv, m);

          resultSummary.textContent =
            'The modular inverse of a = ' + a.toString() +
            ' modulo m = ' + m.toString() +
            ' is x = ' + inv.toString() + '.';

          var detailHtml = '';
          detailHtml += '<p>We first reduce a modulo m: a ≡ ' + aNorm.toString() +
            ' (mod ' + m.toString() + ').</p>';
          detailHtml += '<p>Using the extended Euclidean algorithm we find integers x and y such that ' +
            'a\' x + m y = gcd(a\', m) = 1. The coefficient x gives the modular inverse.</p>';
          resultDetails.innerHTML = detailHtml;

          verificationBox.innerHTML =
            '<div class="formula-box text-sm">' +
              '<p class="font-semibold mb-1">Verification</p>' +
              '<p>a\' = ' + aNorm.toString() + ', m = ' + m.toString() + ', x = ' + inv.toString() + '</p>' +
              '<p>(' + aNorm.toString() + ' × ' + inv.toString() + ') mod ' + m.toString() +
              ' = ' + check.toString() + '.</p>' +
              '<p>Since this equals 1, x is a valid modular inverse of a modulo m.</p>' +
            '</div>';

          resultContainer.classList.remove('hidden');

          if (showStepsCheckbox.checked && eg.steps && eg.steps.length > 0) {
            eg.steps.forEach(function(row) {
              if (row.q === null && row.step !== 0) {
                return;
              }
              var tr = document.createElement('tr');
              tr.className = 'border-b';

              var cells = [
                row.step,
                row.q === null ? '-' : row.q,
                row.r,
                row.s,
                row.t
              ];

              cells.forEach(function(val) {
                var td = document.createElement('td');
                td.className = 'px-3 py-1 text-right';
                td.textContent = val;
                tr.appendChild(td);
              });

              stepsTableBody.appendChild(tr);
            });

            stepsContainer.classList.remove('hidden');
          }
        });

        resetButton.addEventListener('click', function() {
          $('a-input').value = '';
          $('m-input').value = '';
          showStepsCheckbox.checked = false;
          errorEl.classList.add('hidden');
          resultContainer.classList.add('hidden');
          stepsContainer.classList.add('hidden');
          clearChildren(stepsTableBody);
          verificationBox.innerHTML = '';
        });

        var exampleButtons = document.querySelectorAll('.example-btn');
        exampleButtons.forEach(function(btn) {
          btn.addEventListener('click', function() {
            var a = this.getAttribute('data-a');
            var m = this.getAttribute('data-m');
            setExample(a, m);
          });
        });
      });
    })();
    </script>

