        function convertGPMtoLPS() {
            const gpm = parseFloat(document.getElementById('gpmInput').value);
            if (!isNaN(gpm)) {
                const lps = gpm * 0.0630902;
                document.getElementById('lpsResult').textContent = lps.toFixed(5);
            } else {
                document.getElementById('lpsResult').textContent = 'Invalid input';
            }
        }
