    <script>
      // NIHSS calculator logic
      (function () {
        const inputs = document.querySelectorAll(".nihss-item");
        const totalEl = document.getElementById("nihss-total");
        const interpEl = document.getElementById("nihss-interpretation");
        const form = document.getElementById("nihss-form");
        const expandAllBtn = document.getElementById("expand-all");
        const collapseAllBtn = document.getElementById("collapse-all");
        const resetBtn = document.getElementById("reset-form");

        function updateScore() {
          let total = 0;
          inputs.forEach((input) => {
            if (input.checked) {
              const v = parseInt(input.value, 10);
              if (!isNaN(v)) total += v;
            }
          });
          totalEl.textContent = total;

          let interpretation = "No stroke symptoms";
          if (total === 0) {
            interpretation = "No stroke symptoms";
          } else if (total >= 1 && total <= 4) {
            interpretation = "Minor stroke";
          } else if (total >= 5 && total <= 15) {
            interpretation = "Moderate stroke";
          } else if (total >= 16 && total <= 20) {
            interpretation = "Moderate to severe stroke";
          } else if (total >= 21) {
            interpretation = "Severe stroke";
          }
          interpEl.textContent = interpretation;
        }

        inputs.forEach((input) => {
          input.addEventListener("change", updateScore);
        });

        expandAllBtn.addEventListener("click", () => {
          document
            .querySelectorAll("#nihss-form details")
            .forEach((d) => (d.open = true));
        });

        collapseAllBtn.addEventListener("click", () => {
          document
            .querySelectorAll("#nihss-form details")
            .forEach((d) => (d.open = false));
        });

        resetBtn.addEventListener("click", () => {
          form.reset();
          updateScore();
        });

        // Initialize
        updateScore();
      })();
    </script>

