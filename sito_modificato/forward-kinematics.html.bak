    <script>
      // State
      let angleUnit = "deg"; // 'deg' or 'rad'

      function toRadians(value) {
        return angleUnit === "deg" ? (value * Math.PI) / 180 : value;
      }

      function formatNumber(value, decimals = 3) {
        if (!isFinite(value)) return "NaN";
        return parseFloat(value.toFixed(decimals)).toString();
      }

      function computeForwardKinematics() {
        const L1 = parseFloat(document.getElementById("link1-length").value);
        const L2 = parseFloat(document.getElementById("link2-length").value);
        const t1 = parseFloat(document.getElementById("theta1").value);
        const t2 = parseFloat(document.getElementById("theta2").value);

        if (isNaN(L1) || isNaN(L2) || isNaN(t1) || isNaN(t2)) {
          return;
        }

        const theta1 = toRadians(t1);
        const theta2 = toRadians(t2);

        const x1 = L1 * Math.cos(theta1);
        const y1 = L1 * Math.sin(theta1);

        const x = x1 + L2 * Math.cos(theta1 + theta2);
        const y = y1 + L2 * Math.sin(theta1 + theta2);

        const unit = document.getElementById("length-unit").value;

        document.getElementById("x-output").textContent = formatNumber(x);
        document.getElementById("y-output").textContent = formatNumber(y);
        document.getElementById("x1-output").textContent = formatNumber(x1);
        document.getElementById("y1-output").textContent = formatNumber(y1);
        document.getElementById("x-output-unit").textContent = unit;
        document.getElementById("y-output-unit").textContent = unit;

        drawArm(L1, L2, x1, y1, x, y);
      }

      function drawArm(L1, L2, x1, y1, x, y) {
        const canvas = document.getElementById("arm-canvas");
        if (!canvas || !canvas.getContext) return;
        const ctx = canvas.getContext("2d");

        const w = canvas.width;
        const h = canvas.height;

        ctx.clearRect(0, 0, w, h);

        const margin = 20;
        const maxLen = L1 + L2 || 1;
        const scale = Math.min(
          (w - 2 * margin) / (2 * maxLen),
          (h - 2 * margin) / (2 * maxLen)
        );

        const originX = w / 2;
        const originY = h / 2;

        function mapX(val) {
          return originX + val * scale;
        }
        function mapY(val) {
          return originY - val * scale;
        }

        // Axes
        ctx.strokeStyle = "#e5e7eb";
        ctx.lineWidth = 1;
        ctx.beginPath();
        ctx.moveTo(0, originY);
        ctx.lineTo(w, originY);
        ctx.moveTo(originX, 0);
        ctx.lineTo(originX, h);
        ctx.stroke();

        // Link 1
        ctx.strokeStyle = "#2563eb";
        ctx.lineWidth = 4;
        ctx.beginPath();
        ctx.moveTo(mapX(0), mapY(0));
        ctx.lineTo(mapX(x1), mapY(y1));
        ctx.stroke();

        // Link 2
        ctx.strokeStyle = "#16a34a";
        ctx.beginPath();
        ctx.moveTo(mapX(x1), mapY(y1));
        ctx.lineTo(mapX(x), mapY(y));
        ctx.stroke();

        // Joints
        ctx.fillStyle = "#111827";
        ctx.beginPath();
        ctx.arc(mapX(0), mapY(0), 4, 0, 2 * Math.PI);
        ctx.fill();

        ctx.beginPath();
        ctx.arc(mapX(x1), mapY(y1), 4, 0, 2 * Math.PI);
        ctx.fill();

        // End-effector
        ctx.fillStyle = "#dc2626";
        ctx.beginPath();
        ctx.arc(mapX(x), mapY(y), 5, 0, 2 * Math.PI);
        ctx.fill();
      }

      function setAngleUnit(unit) {
        if (unit === angleUnit) return;
        const theta1Input = document.getElementById("theta1");
        const theta2Input = document.getElementById("theta2");
        let t1 = parseFloat(theta1Input.value);
        let t2 = parseFloat(theta2Input.value);

        if (!isNaN(t1)) {
          if (unit === "deg") {
            t1 = (t1 * 180) / Math.PI;
          } else {
            t1 = (t1 * Math.PI) / 180;
          }
          theta1Input.value = formatNumber(t1, unit === "deg" ? 1 : 3);
        }
        if (!isNaN(t2)) {
          if (unit === "deg") {
            t2 = (t2 * 180) / Math.PI;
          } else {
            t2 = (t2 * Math.PI) / 180;
          }
          theta2Input.value = formatNumber(t2, unit === "deg" ? 1 : 3);
        }

        angleUnit = unit;
        document.getElementById("deg-btn").className =
          unit === "deg"
            ? "px-2 py-1 rounded border border-blue-600 text-blue-600 text-xs font-medium"
            : "px-2 py-1 rounded border border-gray-300 text-gray-600 text-xs";
        document.getElementById("rad-btn").className =
          unit === "rad"
            ? "px-2 py-1 rounded border border-blue-600 text-blue-600 text-xs font-medium"
            : "px-2 py-1 rounded border border-gray-300 text-gray-600 text-xs";
      }

      function resetCalculator() {
        document.getElementById("link1-length").value = 1;
        document.getElementById("link2-length").value = 0.8;
        document.getElementById("length-unit").value = "m";
        angleUnit = "deg";
        document.getElementById("theta1").value = 45;
        document.getElementById("theta2").value = 30;
        setAngleUnit("deg");
        computeForwardKinematics();
      }

      document.addEventListener("DOMContentLoaded", function () {
        document
          .getElementById("compute-btn")
          .addEventListener("click", computeForwardKinematics);
        document
          .getElementById("reset-btn")
          .addEventListener("click", resetCalculator);
        document
          .getElementById("deg-btn")
          .addEventListener("click", () => setAngleUnit("deg"));
        document
          .getElementById("rad-btn")
          .addEventListener("click", () => setAngleUnit("rad"));

        [
          "link1-length",
          "link2-length",
          "theta1",
          "theta2",
          "length-unit",
        ].forEach((id) => {
          const el = document.getElementById(id);
          el.addEventListener("input", computeForwardKinematics);
          el.addEventListener("change", computeForwardKinematics);
        });

        computeForwardKinematics();
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
