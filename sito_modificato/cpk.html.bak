    <script>
      // Simple normal CDF approximation (Abramowitz & Stegun)
      function normalCdf(z) {
        const sign = z < 0 ? -1 : 1;
        z = Math.abs(z) / Math.sqrt(2);
        const t = 1 / (1 + 0.3275911 * z);
        const a1 = 0.254829592,
          a2 = -0.284496736,
          a3 = 1.421413741,
          a4 = -1.453152027,
          a5 = 1.061405429;
        const erf =
          1 -
          ((((a5 * t + a4) * t + a3) * t + a2) * t + a1) * t * Math.exp(-z * z);
        return 0.5 * (1 + sign * erf);
      }

      function parseNumber(id) {
        const v = document.getElementById(id).value.trim();
        if (v === "") return null;
        const n = Number(v);
        return Number.isFinite(n) ? n : NaN;
      }

      function showError(msg) {
        const sec = document.getElementById("error-section");
        const box = document.getElementById("error-message");
        box.textContent = msg;
        sec.classList.remove("hidden");
      }

      function clearError() {
        document.getElementById("error-section").classList.add("hidden");
      }

      function setText(id, value, digits = 4) {
        const el = document.getElementById(id);
        if (value == null || !Number.isFinite(value)) {
          el.textContent = "–";
        } else {
          el.textContent = value.toFixed(digits);
        }
      }

      function capabilityBadge(cpk) {
        const badge = document.getElementById("capability-badge");
        if (!Number.isFinite(cpk) || cpk <= 0) {
          badge.textContent = "Not capable";
          badge.className = "badge bg-red-100 text-red-700";
        } else if (cpk < 1.0) {
          badge.textContent = "Poor capability";
          badge.className = "badge bg-red-100 text-red-700";
        } else if (cpk < 1.33) {
          badge.textContent = "Marginal";
          badge.className = "badge bg-yellow-100 text-yellow-700";
        } else if (cpk < 1.67) {
          badge.textContent = "Capable";
          badge.className = "badge bg-green-100 text-green-700";
        } else {
          badge.textContent = "Highly capable";
          badge.className = "badge bg-emerald-100 text-emerald-700";
        }
      }

      function parseRawData() {
        const txt = document.getElementById("raw-data").value;
        const parts = txt.split(/[\s,;]+/).filter(Boolean);
        const data = parts
          .map((p) => Number(p))
          .filter((v) => Number.isFinite(v));
        return data;
      }

      function mean(arr) {
        return arr.reduce((a, b) => a + b, 0) / arr.length;
      }

      function sampleStd(arr, m) {
        const n = arr.length;
        if (n < 2) return NaN;
        let s2 = 0;
        for (const x of arr) {
          const d = x - m;
          s2 += d * d;
        }
        return Math.sqrt(s2 / (n - 1));
      }

      function withinSigmaFromSubgroups(data, subgroupSize) {
        if (!subgroupSize || subgroupSize < 2) return NaN;
        const n = data.length;
        if (n < subgroupSize * 2) return NaN;
        const k = Math.floor(n / subgroupSize);
        if (k < 2) return NaN;
        const d2Table = {
          2: 1.128,
          3: 1.693,
          4: 2.059,
          5: 2.326,
          6: 2.534,
          7: 2.704,
          8: 2.847,
          9: 2.97,
          10: 3.078,
        };
        const d2 = d2Table[subgroupSize];
        if (!d2) return NaN;
        let sumR = 0;
        for (let i = 0; i < k; i++) {
          const start = i * subgroupSize;
          const group = data.slice(start, start + subgroupSize);
          const min = Math.min(...group);
          const max = Math.max(...group);
          sumR += max - min;
        }
        const Rbar = sumR / k;
        return Rbar / d2;
      }

      function calculateSummaryMode() {
        const LSL = parseNumber("lsl");
        const USL = parseNumber("usl");
        const mu = parseNumber("mean");
        const sigma = parseNumber("sigma");
        const n = parseNumber("n-summary");
        const assumeNormal = document.getElementById("assume-normal").checked;

        if (LSL == null || USL == null || mu == null || sigma == null) {
          showError("Please enter LSL, USL, mean, and standard deviation.");
          return null;
        }
        if (
          !Number.isFinite(LSL) ||
          !Number.isFinite(USL) ||
          !Number.isFinite(mu) ||
          !Number.isFinite(sigma) ||
          sigma <= 0
        ) {
          showError(
            "Check that LSL, USL, mean, and standard deviation are valid numbers (σ must be > 0)."
          );
          return null;
        }
        if (USL <= LSL) {
          showError(
            "Upper spec limit (USL) must be greater than lower spec limit (LSL)."
          );
          return null;
        }

        const sigmaWithin = sigma;
        const sigmaOverall = sigma;
        const Cp = (USL - LSL) / (6 * sigmaWithin);
        const Cpu = (USL - mu) / (3 * sigmaWithin);
        const Cpl = (mu - LSL) / (3 * sigmaWithin);
        const Cpk = Math.min(Cpu, Cpl);

        const Pp = (USL - LSL) / (6 * sigmaOverall);
        const Ppu = (USL - mu) / (3 * sigmaOverall);
        const Ppl = (mu - LSL) / (3 * sigmaOverall);
        const Ppk = Math.min(Ppu, Ppl);

        let Zu = null,
          Zl = null,
          pDef = null,
          dpmo = null;
        if (assumeNormal) {
          Zu = (USL - mu) / sigmaOverall;
          Zl = (mu - LSL) / sigmaOverall;
          const pHigh = 1 - normalCdf(Zu);
          const pLow = 1 - normalCdf(Zl);
          pDef = Math.max(0, pHigh + pLow);
          dpmo = pDef * 1e6;
        }

        return {
          mu,
          sigmaWithin,
          sigmaOverall,
          n: Number.isFinite(n) ? n : null,
          Cp,
          Cpk,
          Pp,
          Ppk,
          Zu,
          Zl,
          pDef,
          dpmo,
        };
      }

      function calculateRawMode() {
        const LSL = parseNumber("lsl-raw");
        const USL = parseNumber("usl-raw");
        const subgroupSize = parseNumber("subgroup-size");
        const assumeNormal =
          document.getElementById("assume-normal-raw").checked;
        const data = parseRawData();

        if (LSL == null || USL == null) {
          showError("Please enter LSL and USL.");
          return null;
        }
        if (!Number.isFinite(LSL) || !Number.isFinite(USL) || USL <= LSL) {
          showError(
            "Upper spec limit (USL) must be greater than lower spec limit (LSL), and both must be valid numbers."
          );
          return null;
        }
        if (!data || data.length < 2) {
          showError("Please paste at least two valid numeric measurements.");
          return null;
        }

        const n = data.length;
        const mu = mean(data);
        const sigmaOverall = sampleStd(data, mu);
        if (!Number.isFinite(sigmaOverall) || sigmaOverall <= 0) {
          showError(
            "Unable to compute a valid standard deviation from the data."
          );
          return null;
        }

        let sigmaWithin = sigmaOverall;
        if (Number.isFinite(subgroupSize) && subgroupSize >= 2) {
          const sW = withinSigmaFromSubgroups(data, subgroupSize);
          if (Number.isFinite(sW) && sW > 0) {
            sigmaWithin = sW;
          }
        }

        const Cp = (USL - LSL) / (6 * sigmaWithin);
        const Cpu = (USL - mu) / (3 * sigmaWithin);
        const Cpl = (mu - LSL) / (3 * sigmaWithin);
        const Cpk = Math.min(Cpu, Cpl);

        const Pp = (USL - LSL) / (6 * sigmaOverall);
        const Ppu = (USL - mu) / (3 * sigmaOverall);
        const Ppl = (mu - LSL) / (3 * sigmaOverall);
        const Ppk = Math.min(Ppu, Ppl);

        let Zu = null,
          Zl = null,
          pDef = null,
          dpmo = null;
        if (assumeNormal) {
          Zu = (USL - mu) / sigmaOverall;
          Zl = (mu - LSL) / sigmaOverall;
          const pHigh = 1 - normalCdf(Zu);
          const pLow = 1 - normalCdf(Zl);
          pDef = Math.max(0, pHigh + pLow);
          dpmo = pDef * 1e6;
        }

        return {
          mu,
          sigmaWithin,
          sigmaOverall,
          n,
          Cp,
          Cpk,
          Pp,
          Ppk,
          Zu,
          Zl,
          pDef,
          dpmo,
        };
      }

      function renderResults(res) {
        if (!res) return;
        clearError();
        document.getElementById("results-section").classList.remove("hidden");

        setText("res-cp", res.Cp);
        setText("res-cpk", res.Cpk);
        setText("res-pp", res.Pp);
        setText("res-ppk", res.Ppk);

        setText("res-zu", res.Zu);
        setText("res-zl", res.Zl);
        if (res.pDef != null && Number.isFinite(res.pDef)) {
          document.getElementById("res-percent-defect").textContent =
            (res.pDef * 100).toFixed(4) + " %";
          document.getElementById("res-dpmo").textContent = res.dpmo.toFixed(1);
        } else {
          document.getElementById("res-percent-defect").textContent = "–";
          document.getElementById("res-dpmo").textContent = "–";
        }

        setText("res-mean", res.mu);
        setText("res-sigma-within", res.sigmaWithin);
        setText("res-sigma-overall", res.sigmaOverall);
        if (res.n != null && Number.isFinite(res.n)) {
          document.getElementById("res-n").textContent = res.n.toString();
        } else {
          document.getElementById("res-n").textContent = "–";
        }

        capabilityBadge(res.Cpk);
      }

      document.getElementById("btn-calc").addEventListener("click", () => {
        clearError();
        const summaryActive = !document
          .getElementById("panel-summary")
          .classList.contains("hidden");
        const res = summaryActive ? calculateSummaryMode() : calculateRawMode();
        if (res) renderResults(res);
      });

      document.getElementById("btn-reset").addEventListener("click", () => {
        [
          "lsl",
          "usl",
          "mean",
          "sigma",
          "n-summary",
          "lsl-raw",
          "usl-raw",
          "raw-data",
          "subgroup-size",
        ].forEach((id) => {
          const el = document.getElementById(id);
          if (el) el.value = "";
        });
        document.getElementById("assume-normal").checked = true;
        document.getElementById("assume-normal-raw").checked = true;
        document.getElementById("results-section").classList.add("hidden");
        clearError();
      });

