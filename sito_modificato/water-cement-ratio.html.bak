    <script>
      // Mode toggle
      const modeWeightBtn = document.getElementById("mode-weight-btn");
      const modeVolumeBtn = document.getElementById("mode-volume-btn");
      const weightInputs = document.getElementById("weight-inputs");
      const volumeInputs = document.getElementById("volume-inputs");
      let currentMode = "weight";

      function setMode(mode) {
        currentMode = mode;
        if (mode === "weight") {
          weightInputs.classList.remove("hidden");
          volumeInputs.classList.add("hidden");
          modeWeightBtn.classList.add(
            "bg-blue-600",
            "text-white",
            "font-medium"
          );
          modeWeightBtn.classList.remove("bg-gray-100", "text-gray-700");
          modeVolumeBtn.classList.remove(
            "bg-blue-600",
            "text-white",
            "font-medium"
          );
          modeVolumeBtn.classList.add("bg-gray-100", "text-gray-700");
        } else {
          weightInputs.classList.add("hidden");
          volumeInputs.classList.remove("hidden");
          modeVolumeBtn.classList.add(
            "bg-blue-600",
            "text-white",
            "font-medium"
          );
          modeVolumeBtn.classList.remove("bg-gray-100", "text-gray-700");
          modeWeightBtn.classList.remove(
            "bg-blue-600",
            "text-white",
            "font-medium"
          );
          modeWeightBtn.classList.add("bg-gray-100", "text-gray-700");
        }
      }

      modeWeightBtn.addEventListener("click", () => setMode("weight"));
      modeVolumeBtn.addEventListener("click", () => setMode("volume"));

      // Calculation
      const calculateBtn = document.getElementById("calculate-btn");
      const wcValueEl = document.getElementById("wc-value");
      const wcBadgeEl = document.getElementById("wc-badge");
      const wcInterpEl = document.getElementById("wc-interpretation");
      const strengthEstimateEl = document.getElementById("strength-estimate");
      const strengthNoteEl = document.getElementById("strength-note");
      const durabilityLimitEl = document.getElementById("durability-limit");
      const durabilityStatusEl = document.getElementById("durability-status");

      function getDurabilityLimit(exposure) {
        switch (exposure) {
          case "normal":
            return 0.55;
          case "moderate":
            return 0.5;
          case "severe":
            return 0.45;
          case "marine":
            return 0.4;
          default:
            return 0.55;
        }
      }

      function getDurabilityLabel(exposure) {
        switch (exposure) {
          case "normal":
            return "Normal (interior / mild)";
          case "moderate":
            return "Moderate (weather, wet)";
          case "severe":
            return "Severe (freeze–thaw, deicing)";
          case "marine":
            return "Marine / very severe";
          default:
            return "Normal (interior / mild)";
        }
      }

      function estimateStrengthFromWc(wc) {
        // Simple empirical curve: f_c (MPa) ≈ 120 * e^(−2.5 * w/c)
        const fcMpa = 120 * Math.exp(-2.5 * wc);
        return fcMpa;
      }

      function formatNumber(value, decimals) {
        return Number.isFinite(value) ? value.toFixed(decimals) : "–";
      }

      calculateBtn.addEventListener("click", () => {
        const exposure = document.getElementById("exposure-class").value;
        const targetStrengthVal =
          parseFloat(document.getElementById("strength-value").value) || 0;
        const targetStrengthUnit =
          document.getElementById("strength-unit").value;

        let wc = null;

        if (currentMode === "weight") {
          const cementMass = parseFloat(
            document.getElementById("cement-mass").value
          );
          const waterMass = parseFloat(
            document.getElementById("water-mass").value
          );

          if (!cementMass || !waterMass || cementMass <= 0 || waterMass <= 0) {
            alert("Please enter positive cement and water masses.");
            return;
          }
          wc = waterMass / cementMass;
        } else {
          const cementVol = parseFloat(
            document.getElementById("cement-volume").value
          );
          const waterVol = parseFloat(
            document.getElementById("water-volume").value
          );
          const cementDensity = parseFloat(
            document.getElementById("cement-density").value
          );
          const waterDensity = parseFloat(
            document.getElementById("water-density").value
          );
          const cementVolUnit =
            document.getElementById("cement-volume-unit").value;
          const waterVolUnit =
            document.getElementById("water-volume-unit").value;

          if (!cementVol || !waterVol || cementVol <= 0 || waterVol <= 0) {
            alert("Please enter positive cement and water volumes.");
            return;
          }
          if (
            !cementDensity ||
            !waterDensity ||
            cementDensity <= 0 ||
            waterDensity <= 0
          ) {
            alert("Please enter valid densities for cement and water.");
            return;
          }

          // Convert ft³ to m³ if needed
          const ft3ToM3 = 0.0283168;
          const cementVolM3 =
            cementVolUnit === "ft3" ? cementVol * ft3ToM3 : cementVol;
          const waterVolM3 =
            waterVolUnit === "ft3" ? waterVol * ft3ToM3 : waterVol;

          const cementMass = cementVolM3 * cementDensity;
          const waterMass = waterVolM3 * waterDensity;

          wc = waterMass / cementMass;
        }

        if (!Number.isFinite(wc) || wc <= 0) {
          alert(
            "Unable to compute a valid water-cement ratio. Please check your inputs."
          );
          return;
        }

        // Update w/c display
        wcValueEl.textContent = formatNumber(wc, 3);

        // Badge & interpretation
        wcBadgeEl.classList.remove(
          "bg-gray-200",
          "text-gray-800",
          "bg-green-100",
          "text-green-800",
          "bg-yellow-100",
          "text-yellow-800",
          "bg-red-100",
          "text-red-800"
        );

        let badgeText = "";
        let interpText = "";

        if (wc < 0.35) {
          badgeText = "Very low w/c";
          wcBadgeEl.classList.add("bg-yellow-100", "text-yellow-800");
          interpText =
            "Very low water-cement ratio – high strength and durability, but workability may be poor without superplasticizer.";
        } else if (wc >= 0.35 && wc <= 0.5) {
          badgeText = "Good w/c";
          wcBadgeEl.classList.add("bg-green-100", "text-green-800");
          interpText =
            "Typical range for structural concrete – good balance of strength, durability, and workability.";
        } else if (wc > 0.5 && wc <= 0.6) {
          badgeText = "High w/c";
          wcBadgeEl.classList.add("bg-yellow-100", "text-yellow-800");
          interpText =
            "Relatively high water-cement ratio – acceptable for some non-structural or mild exposure applications.";
        } else {
          badgeText = "Very high w/c";
          wcBadgeEl.classList.add("bg-red-100", "text-red-800");
          interpText =
            "Very high water-cement ratio – expect low strength and poor durability. Consider reducing water or increasing cement.";
        }
        wcBadgeEl.textContent = badgeText;
        wcInterpEl.textContent = interpText;

        // Strength estimate
        const fcMpa = estimateStrengthFromWc(wc);
        const fcPsi = fcMpa * 145.038;
        strengthEstimateEl.textContent = `≈ ${formatNumber(
          fcMpa,
          1
        )} MPa (≈ ${formatNumber(fcPsi, 0)} psi)`;

        // Compare to target strength if provided
        if (targetStrengthVal > 0) {
          let targetMpa = targetStrengthVal;
          if (targetStrengthUnit === "psi") {
            targetMpa = targetStrengthVal / 145.038;
          }
          if (fcMpa >= targetMpa) {
            strengthNoteEl.textContent =
              "Estimated strength meets or exceeds the target, assuming proper materials and curing.";
          } else {
            strengthNoteEl.textContent =
              "Estimated strength is below the target – consider reducing w/c, increasing cement, or using admixtures.";
          }
        } else {
          strengthNoteEl.textContent =
            "Empirical estimate – actual strength depends on materials, curing, and mix design.";
        }

        // Durability check
        const limit = getDurabilityLimit(exposure);
        const label = getDurabilityLabel(exposure);
        durabilityLimitEl.textContent = `For “${label}” exposure, recommended max w/c ≈ ${limit.toFixed(
          2
        )}.`;

        durabilityStatusEl.classList.remove(
          "text-green-700",
          "text-yellow-700",
          "text-red-700"
        );

        if (wc <= limit) {
          durabilityStatusEl.textContent =
            "Your mix is within the recommended limit for this exposure class.";
          durabilityStatusEl.classList.add("text-green-700");
        } else if (wc <= limit + 0.05) {
          durabilityStatusEl.textContent =
            "Your mix is slightly above the recommended limit – durability may be reduced in aggressive environments.";
          durabilityStatusEl.classList.add("text-yellow-700");
        } else {
          durabilityStatusEl.textContent =
            "Your mix is well above the recommended limit – not suitable for this exposure without modification.";
          durabilityStatusEl.classList.add("text-red-700");
        }
      });

      // Initialize with default calculation
      window.addEventListener("DOMContentLoaded", () => {
        calculateBtn.click();
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
