    <script>
      // --- Rational arithmetic helpers ---
      function parseNumber(str) {
        str = String(str).trim();
        if (!str) return { n: 0, d: 1 };
        if (str.includes("/")) {
          const [a, b] = str.split("/").map((s) => s.trim());
          const n = parseFloat(a);
          const d = parseFloat(b);
          if (!isFinite(n) || !isFinite(d) || d === 0)
            throw new Error("Invalid fraction: " + str);
          return normalize({ n, d });
        }
        const v = parseFloat(str);
        if (!isFinite(v)) throw new Error("Invalid number: " + str);
        // represent decimals as fraction with denominator 10^k
        const s = str.replace(/^[+-]/, "");
        if (s.includes(".")) {
          const parts = s.split(".");
          const k = parts[1].length;
          const n = Math.round(v * Math.pow(10, k));
          const d = Math.pow(10, k);
          return normalize({ n, d });
        }
        return { n: v, d: 1 };
      }
      function gcd(a, b) {
        a = Math.abs(a);
        b = Math.abs(b);
        while (b) {
          const t = b;
          b = a % b;
          a = t;
        }
        return a || 1;
      }
      function normalize(fr) {
        if (fr.d < 0) {
          fr.n = -fr.n;
          fr.d = -fr.d;
        }
        if (fr.n === 0) {
          fr.d = 1;
          return fr;
        }
        const g = gcd(fr.n, fr.d);
        fr.n / g;
        fr.d / g;
        return fr;
      }
      function add(a, b) {
        return normalize({ n: a.n * b.d + b.n * a.d, d: a.d * b.d });
      }
      function sub(a, b) {
        return normalize({ n: a.n * b.d - b.n * a.d, d: a.d * b.d });
      }
      function mul(a, b) {
        return normalize({ n: a.n * b.n, d: a.d * b.d });
      }
      function div(a, b) {
        if (b.n === 0) throw new Error("Division by zero");
        return normalize({ n: a.n * b.d, d: a.d * b.n });
      }
      function isZero(a) {
        return a.n === 0;
      }
      function approx(a) {
        return a.n / a.d;
      }
      function fracToString(a) {
        if (a.d === 1) return String(a.n);
        return a.n + "/" + a.d;
      }

      // --- UI and solver ---
      const coeffMatrixEl = document.getElementById("coeff-matrix");
      const constColumnEl = document.getElementById("const-column");
      const varLabelsEl = document.getElementById("variable-labels");
      const varColumnEl = document.getElementById("var-column");
      const sizeButtons = document.querySelectorAll(".size-btn");
      const customToggle = document.getElementById("custom-size-toggle");
      const customPanel = document.getElementById("custom-size-panel");
      const rowsInput = document.getElementById("rows-input");
      const colsInput = document.getElementById("cols-input");
      const applySizeBtn = document.getElementById("apply-size");
      const solveBtn = document.getElementById("solve-btn");
      const clearBtn = document.getElementById("clear-btn");
      const randomBtn = document.getElementById("random-btn");
      const statusMsg = document.getElementById("status-msg");
      const resultsEl = document.getElementById("results");
      const solutionSummaryEl = document.getElementById("solution-summary");
      const solutionValuesEl = document.getElementById("solution-values");
      const specialCaseEl = document.getElementById("special-case");
      const stepsContainer = document.getElementById("steps-container");
      const stepsContent = document.getElementById("steps-content");
      const matrixDetails = document.getElementById("matrix-details");
      const matrixContent = document.getElementById("matrix-content");
      const solutionFormatEl = document.getElementById("solution-format");
      const stepsModeEl = document.getElementById("steps-mode");

      let currentRows = 2;
      let currentCols = 2;

      function variableNames(n) {
        const base = ["x", "y", "z", "w", "u", "v", "s", "t"];
        const vars = [];
        for (let i = 0; i < n; i++) {
          vars.push(base[i] || "x" + (i + 1));
        }
        return vars;
      }

      function buildGrid(rows, cols) {
        currentRows = rows;
        currentCols = cols;
        coeffMatrixEl.innerHTML = "";
        constColumnEl.innerHTML = "";

        coeffMatrixEl.style.gridTemplateColumns = `repeat(${cols}, minmax(3rem,4rem))`;
        constColumnEl.style.gridTemplateColumns = `minmax(3rem,4rem)`;

        for (let r = 0; r < rows; r++) {
          for (let c = 0; c < cols; c++) {
            const inp = document.createElement("input");
            inp.type = "text";
            inp.inputMode = "decimal";
            inp.className =
              "matrix-input border border-gray-300 rounded px-1 py-1 text-sm text-center";
            inp.dataset.row = r;
            inp.dataset.col = c;
            coeffMatrixEl.appendChild(inp);
          }
          const rhs = document.createElement("input");
          rhs.type = "text";
          rhs.inputMode = "decimal";
          rhs.className =
            "matrix-input border border-gray-300 rounded px-1 py-1 text-sm text-center";
          rhs.dataset.row = r;
          rhs.dataset.col = "rhs";
          constColumnEl.appendChild(rhs);
        }

        const vars = variableNames(cols);
        varLabelsEl.textContent = vars.join(", ");
        varColumnEl.textContent = "(" + vars.join(", ") + ")";

        resultsEl.classList.add("hidden");
        statusMsg.textContent = "";
      }

      function setSizeFromButton(btn) {
        sizeButtons.forEach((b) => {
          b.classList.remove("border-blue-600", "text-blue-600", "bg-blue-50");
          b.classList.add("border-gray-300", "text-gray-700");
        });
        btn.classList.remove("border-gray-300", "text-gray-700");
        btn.classList.add("border-blue-600", "text-blue-600", "bg-blue-50");
        const r = parseInt(btn.dataset.rows, 10);
        const c = parseInt(btn.dataset.cols, 10);
        buildGrid(r, c);
      }

      sizeButtons.forEach((btn) => {
        btn.addEventListener("click", () => {
          customPanel.classList.add("hidden");
          setSizeFromButton(btn);
        });
      });

      customToggle.addEventListener("click", () => {
        customPanel.classList.toggle("hidden");
      });

      applySizeBtn.addEventListener("click", () => {
        let r = parseInt(rowsInput.value, 10);
        let c = parseInt(colsInput.value, 10);
        if (!Number.isFinite(r) || r < 1) r = 1;
        if (!Number.isFinite(c) || c < 1) c = 1;
        if (r > 8) r = 8;
        if (c > 8) c = 8;
        rowsInput.value = r;
        colsInput.value = c;
        buildGrid(r, c);
        sizeButtons.forEach((b) => {
          b.classList.remove("border-blue-600", "text-blue-600", "bg-blue-50");
          b.classList.add("border-gray-300", "text-gray-700");
        });
      });

      function readSystem() {
        const A = [];
        const b = [];
        for (let r = 0; r < currentRows; r++) {
          const row = [];
          for (let c = 0; c < currentCols; c++) {
            const inp = coeffMatrixEl.querySelector(
              `input[data-row="${r}"][data-col="${c}"]`
            );
            const val = inp.value.trim();
            row.push(val ? parseNumber(val) : { n: 0, d: 1 });
          }
          const rhs = constColumnEl.querySelector(
            `input[data-row="${r}"][data-col="rhs"]`
          );
          const rhsVal = rhs.value.trim();
          b.push(rhsVal ? parseNumber(rhsVal) : { n: 0, d: 1 });
          A.push(row);
        }
        return { A, b };
      }

      function cloneMatrix(A) {
        return A.map((row) => row.map((fr) => ({ n: fr.n, d: fr.d })));
      }
      function cloneVector(v) {
        return v.map((fr) => ({ n: fr.n, d: fr.d }));
      }

      function gaussianElimination(Ain, bin) {
        const A = cloneMatrix(Ain);
        const b = cloneVector(bin);
        const m = A.length;
        const n = A[0].length;
        const steps = [];
        let rankA = 0;
        let rankAb = 0;

        let row = 0;
        for (let col = 0; col < n && row < m; col++) {
          // find pivot
          let pivotRow = -1;
          for (let r = row; r < m; r++) {
            if (!isZero(A[r][col])) {
              pivotRow = r;
              break;
            }
          }
          if (pivotRow === -1) continue;

          if (pivotRow !== row) {
            [A[pivotRow], A[row]] = [A[row], A[pivotRow]];
            [b[pivotRow], b[row]] = [b[row], b[pivotRow]];
            steps.push({
              type: "swap",
              row1: row,
              row2: pivotRow,
              A: cloneMatrix(A),
              b: cloneVector(b),
            });
          }

          const pivot = A[row][col];
          // normalize pivot row
          if (!(pivot.n === 1 && pivot.d === 1)) {
            for (let c = col; c < n; c++) {
              A[row][c] = div(A[row][c], pivot);
            }
            b[row] = div(b[row], pivot);
            steps.push({
              type: "scale",
              row: row,
              factor: { n: pivot.d, d: pivot.n },
              A: cloneMatrix(A),
              b: cloneVector(b),
            });
          }

          // eliminate below
          for (let r = row + 1; r < m; r++) {
            if (isZero(A[r][col])) continue;
            const factor = A[r][col];
            for (let c = col; c < n; c++) {
              A[r][c] = sub(A[r][c], mul(factor, A[row][c]));
            }
            b[r] = sub(b[r], mul(factor, b[row]));
            steps.push({
              type: "eliminate",
              src: row,
              target: r,
              factor,
              A: cloneMatrix(A),
              b: cloneVector(b),
            });
          }

          row++;
        }

        // compute ranks
        function rowIsZero(rowA) {
          return rowA.every(isZero);
        }
        rankA = A.reduce((acc, rowA) => acc + (rowIsZero(rowA) ? 0 : 1), 0);
        rankAb = A.reduce((acc, rowA, i) => {
          const allZero = rowIsZero(rowA) && isZero(b[i]);
          const inconsistent = rowIsZero(rowA) && !isZero(b[i]);
          if (inconsistent) return acc + 1;
          return acc + (rowIsZero(rowA) ? 0 : 1);
        }, 0);

        // back substitution if possible
        let solution = null;
        let type = "unique";
        if (rankA !== rankAb) {
          type = "none";
        } else if (rankA < n) {
          type = "infinite";
        } else {
          // unique solution
          solution = new Array(n).fill({ n: 0, d: 1 });
          for (let i = m - 1; i >= 0; i--) {
            // find leading 1
            let leadCol = -1;
            for (let c = 0; c < n; c++) {
              if (!isZero(A[i][c])) {
                leadCol = c;
                break;
              }
            }
            if (leadCol === -1) continue;
            let sum = { n: 0, d: 1 };
            for (let c = leadCol + 1; c < n; c++) {
              sum = add(sum, mul(A[i][c], solution[c]));
            }
            solution[leadCol] = sub(b[i], sum);
          }
        }

        return { type, A, b, steps, rankA, rankAb, solution };
      }

      function formatAugmentedMatrix(A, b) {
        const m = A.length;
        const n = A[0].length;
        let lines = [];
        for (let i = 0; i < m; i++) {
          const row = [];
          for (let j = 0; j < n; j++) {
            row.push(fracToString(A[i][j]));
          }
          lines.push("[ " + row.join("  ") + " | " + fracToString(b[i]) + " ]");
        }
        return lines.join("\n");
      }

      function formatSolution(solution) {
        const mode = solutionFormatEl.value;
        const vars = variableNames(solution.length);
        const lines = [];
        for (let i = 0; i < solution.length; i++) {
          const fr = solution[i];
          const dec = approx(fr);
          if (mode === "fraction-only") {
            lines.push(`${vars[i]} = ${fracToString(fr)}`);
          } else if (mode === "decimal-only") {
            lines.push(`${vars[i]} ≈ ${dec.toFixed(6)}`);
          } else {
            lines.push(
              `${vars[i]} = ${fracToString(fr)}  ≈  ${dec.toFixed(6)}`
            );
          }
        }
        return lines.join("\n");
      }

      function solveSystem() {
        statusMsg.textContent = "";
        try {
          const { A, b } = readSystem();
          const {
            type,
            A: Ar,
            b: br,
            steps,
            rankA,
            rankAb,
            solution,
          } = gaussianElimination(A, b);

          resultsEl.classList.remove("hidden");
          stepsContent.innerHTML = "";
          matrixContent.innerHTML = "";

          if (type === "none") {
            solutionSummaryEl.innerHTML =
              '<span class="text-red-600 font-semibold">No solution.</span> The system is inconsistent.';
            solutionValuesEl.textContent = "";
            specialCaseEl.textContent =
              "The augmented matrix has a row of the form [0 0 … 0 | b] with b ≠ 0, which means the equations contradict each other.";
          } else if (type === "infinite") {
            solutionSummaryEl.innerHTML =
              '<span class="text-amber-600 font-semibold">Infinitely many solutions.</span> The system is dependent.';
            solutionValuesEl.textContent = "";
            specialCaseEl.textContent =
              "Rank(A) = Rank([A|b]) < number of variables, so there are free variables. This calculator currently reports the rank information but does not parameterize the full solution set.";
          } else {
            solutionSummaryEl.innerHTML =
              '<span class="text-green-700 font-semibold">Unique solution found.</span>';
            solutionValuesEl.textContent = formatSolution(solution);
            specialCaseEl.textContent = "";
          }

          // matrix info
          const pre = document.createElement("pre");
          pre.textContent = formatAugmentedMatrix(Ar, br);
          matrixContent.appendChild(pre);
          const info = document.createElement("p");
          info.textContent = `rank(A) = ${rankA}, rank([A|b]) = ${rankAb}, variables = ${currentCols}`;
          matrixContent.appendChild(info);

          // steps
          if (stepsModeEl.value === "none") {
            stepsContainer.open = false;
            stepsContainer.classList.add("hidden");
          } else {
            stepsContainer.classList.remove("hidden");
            stepsContainer.open = false;
            steps.forEach((st, idx) => {
              const block = document.createElement("div");
              let desc = `Step ${idx + 1}: `;
              if (st.type === "swap") {
                desc += `Swap R${st.row1 + 1} and R${st.row2 + 1}`;
              } else if (st.type === "scale") {
                desc += `Scale R${st.row + 1} to make pivot 1`;
              } else if (st.type === "eliminate") {
                desc += `R${st.target + 1} ← R${
                  st.target + 1
                } − (${fracToString(st.factor)})·R${st.src + 1}`;
              }
              const p = document.createElement("div");
              p.textContent = desc;
              const preStep = document.createElement("pre");
              preStep.textContent = formatAugmentedMatrix(st.A, st.b);
              block.appendChild(p);
              block.appendChild(preStep);
              stepsContent.appendChild(block);
            });
          }

          statusMsg.textContent = "Solved.";
        } catch (e) {
          console.error(e);
          statusMsg.textContent = "Error: " + e.message;
          resultsEl.classList.add("hidden");
        }
      }

      solveBtn.addEventListener("click", solveSystem);

      clearBtn.addEventListener("click", () => {
        coeffMatrixEl
          .querySelectorAll("input")
          .forEach((inp) => (inp.value = ""));
        constColumnEl
          .querySelectorAll("input")
          .forEach((inp) => (inp.value = ""));
        resultsEl.classList.add("hidden");
        statusMsg.textContent = "";
      });

      randomBtn.addEventListener("click", () => {
        coeffMatrixEl.querySelectorAll("input").forEach((inp) => {
          const v = Math.floor(Math.random() * 9) - 4; // -4..4
          inp.value = v;
        });
        constColumnEl.querySelectorAll("input").forEach((inp) => {
          const v = Math.floor(Math.random() * 15) - 7;
          inp.value = v;
        });
        resultsEl.classList.add("hidden");
        statusMsg.textContent = "Random system generated.";
      });

      // Example presets
      document.querySelectorAll(".example-btn").forEach((btn) => {
        btn.addEventListener("click", () => {
          const ex = btn.dataset.example;
          if (ex === "2x2-unique") {
            buildGrid(2, 2);
            coeffMatrixEl.querySelector(
              'input[data-row="0"][data-col="0"]'
            ).value = "2";
            coeffMatrixEl.querySelector(
              'input[data-row="0"][data-col="1"]'
            ).value = "3";
            constColumnEl.querySelector('input[data-row="0"]').value = "7";
            coeffMatrixEl.querySelector(
              'input[data-row="1"][data-col="0"]'
            ).value = "-1";
            coeffMatrixEl.querySelector(
              'input[data-row="1"][data-col="1"]'
            ).value = "4";
            constColumnEl.querySelector('input[data-row="1"]').value = "1";
          } else if (ex === "2x2-parallel") {
            buildGrid(2, 2);
            coeffMatrixEl.querySelector(
              'input[data-row="0"][data-col="0"]'
            ).value = "1";
            coeffMatrixEl.querySelector(
              'input[data-row="0"][data-col="1"]'
            ).value = "2";
            constColumnEl.querySelector('input[data-row="0"]').value = "3";
            coeffMatrixEl.querySelector(
              'input[data-row="1"][data-col="0"]'
            ).value = "2";
            coeffMatrixEl.querySelector(
              'input[data-row="1"][data-col="1"]'
            ).value = "4";
            constColumnEl.querySelector('input[data-row="1"]').value = "8";
          } else if (ex === "2x2-dependent") {
            buildGrid(2, 2);
            coeffMatrixEl.querySelector(
              'input[data-row="0"][data-col="0"]'
            ).value = "1";
            coeffMatrixEl.querySelector(
              'input[data-row="0"][data-col="1"]'
            ).value = "2";
            constColumnEl.querySelector('input[data-row="0"]').value = "3";
            coeffMatrixEl.querySelector(
              'input[data-row="1"][data-col="0"]'
            ).value = "2";
            coeffMatrixEl.querySelector(
              'input[data-row="1"][data-col="1"]'
            ).value = "4";
            constColumnEl.querySelector('input[data-row="1"]').value = "6";
          } else if (ex === "3x3-unique") {
            buildGrid(3, 3);
            const vals = [
              [1, 1, 1, 6],
              [2, -1, 1, 3],
              [1, 2, -1, 3],
            ];
            for (let r = 0; r < 3; r++) {
              for (let c = 0; c < 3; c++) {
                coeffMatrixEl.querySelector(
                  `input[data-row="${r}"][data-col="${c}"]`
                ).value = vals[r][c];
              }
              constColumnEl.querySelector(`input[data-row="${r}"]`).value =
                vals[r][3];
            }
          }
          resultsEl.classList.add("hidden");
          statusMsg.textContent = "Example loaded.";
        });
      });

      // Initialize default 2x2 grid
      buildGrid(2, 2);

      // Mobile menu script (from template)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const expanded = this.getAttribute("aria-expanded") === "true";
          this.setAttribute("aria-expanded", String(!expanded));
          menu.classList.toggle("hidden");
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
