            <script>
              document.addEventListener('DOMContentLoaded', function() {
                const sA = document.getElementById('side-a');
                const sB = document.getElementById('side-b');
                const sC = document.getElementById('side-c');
                const aAlpha = document.getElementById('angle-alpha');
                const aBeta = document.getElementById('angle-beta');
                const calculateBtn = document.getElementById('calculate-btn');
                const resultsContainer = document.getElementById('results-container');
                const errorMessage = document.getElementById('error-message');
                const resultSides = document.getElementById('result-sides');
                const resultAngles = document.getElementById('result-angles');
                const stepByStep = document.getElementById('step-by-step');

                const R_TO_D = 180 / Math.PI;
                const D_TO_R = Math.PI / 180;
                
                // Helper functions
                function parseInput(val) {
                    const num = parseFloat(val);
                    return num > 0 ? num : NaN;
                }
                
                function formatNumber(num) {
                    if (Math.abs(num) < 1e-10) return '0';
                    return parseFloat(num.toFixed(4)).toString();
                }

                function showError(message) {
                    errorMessage.textContent = `Error: ${message}`;
                    resultsContainer.classList.add('hidden');
                }

                // Main Solver Logic
                calculateBtn.addEventListener('click', () => {
                    const a = parseInput(sA.value);
                    const b = parseInput(sB.value);
                    const c = parseInput(sC.value);
                    const alpha = parseInput(aAlpha.value);
                    const beta = parseInput(aBeta.value);

                    let unknowns = [a, b, c, alpha, beta].filter(n => isNaN(n)).length;
                    let knowns = 5 - unknowns;

                    let sides = { a, b, c };
                    let angles = { alpha, beta, gamma: 90 };
                    let steps = '';
                    let error = '';

                    try {
                        if (knowns < 2) {
                            throw new Error('Please enter at least two known values (e.g., two sides, or one side and one angle).');
                        }
                        if (knowns > 2) {
                            // Validate over-defined triangles
                            if (!isNaN(alpha) && !isNaN(beta) && formatNumber(alpha + beta) !== '90') {
                                throw new Error('Angles α and β must sum to 90 degrees.');
                            }
                            if (!isNaN(a) && !isNaN(b) && !isNaN(c) && formatNumber(a * a + b * b) !== formatNumber(c * c)) {
                                throw new Error('Side lengths violate the Pythagorean theorem.');
                            }
                        }
                        if ((!isNaN(alpha) && alpha >= 90) || (!isNaN(beta) && beta >= 90)) {
                            throw new Error('Acute angles (α and β) must be less than 90 degrees.');
                        }

                        // --- Solve for the third angle first ---
                        if (isNaN(angles.alpha) && !isNaN(angles.beta)) {
                            angles.alpha = 90 - angles.beta;
                            steps += `<p>1. Find $\\alpha$: $\\alpha = 90^\circ - \\beta = 90^\circ - ${formatNumber(angles.beta)}^\circ = ${formatNumber(angles.alpha)}^\circ$</p>`;
                        } else if (isNaN(angles.beta) && !isNaN(angles.alpha)) {
                            angles.beta = 90 - angles.alpha;
                            steps += `<p>1. Find $\\beta$: $\\beta = 90^\circ - \\alpha = 90^\circ - ${formatNumber(angles.alpha)}^\circ = ${formatNumber(angles.beta)}^\circ$</p>`;
                        }
                        
                        // --- Solve for the third side using Pythagorean ---
                        if (isNaN(sides.c) && !isNaN(sides.a) && !isNaN(sides.b)) {
                            sides.c = Math.sqrt(sides.a * sides.a + sides.b * sides.b);
                            steps += `<p>2. Find hypotenuse c (Pythagorean Theorem): $c = \\sqrt{a^2 + b^2} = \\sqrt{${formatNumber(sides.a)}^2 + ${formatNumber(sides.b)}^2} = ${formatNumber(sides.c)}$</p>`;
                        } else if (isNaN(sides.a) && !isNaN(sides.b) && !isNaN(sides.c)) {
                            sides.a = Math.sqrt(sides.c * sides.c - sides.b * sides.b);
                            steps += `<p>2. Find side a (Pythagorean Theorem): $a = \\sqrt{c^2 - b^2} = \\sqrt{${formatNumber(sides.c)}^2 - ${formatNumber(sides.b)}^2} = ${formatNumber(sides.a)}$</p>`;
                        } else if (isNaN(sides.b) && !isNaN(sides.a) && !isNaN(sides.c)) {
                            sides.b = Math.sqrt(sides.c * sides.c - sides.a * sides.a);
                            steps += `<p>2. Find side b (Pythagorean Theorem): $b = \\sqrt{c^2 - a^2} = \\sqrt{${formatNumber(sides.c)}^2 - ${formatNumber(sides.a)}^2} = ${formatNumber(sides.b)}$</p>`;
                        }

                        // Check for impossible side lengths
                        if ((!isNaN(sides.a) && !isNaN(sides.b) && !isNaN(sides.c) && sides.a * sides.a + sides.b * sides.b < sides.c * sides.c) ||
                            (isNaN(sides.c) && !isNaN(sides.a) && !isNaN(sides.b) && sides.a * sides.a + sides.b * sides.b < 0)
                        ) {
                             throw new Error('Invalid side lengths: Hypotenuse must be longer than either leg, and $a^2+b^2$ must be positive.');
                        }
                        
                        // --- Use SOHCAHTOA to solve for remaining angle or side ---
                        let trigStep = 3;
                        
                        // If sides are known, solve for angles
                        if (!isNaN(sides.a) && !isNaN(sides.c) && isNaN(angles.alpha)) {
                            angles.alpha = Math.asin(sides.a / sides.c) * R_TO_D;
                            angles.beta = 90 - angles.alpha;
                            steps += `<p>${trigStep}. Find $\\alpha$ (SOH): $\\alpha = \sin^{-1}(\\frac{a}{c}) = \sin^{-1}(\\frac{${formatNumber(sides.a)}}{${formatNumber(sides.c)}}) \approx ${formatNumber(angles.alpha)}^\circ$</p>`;
                        } else if (!isNaN(sides.b) && !isNaN(sides.c) && isNaN(angles.beta)) {
                            angles.beta = Math.asin(sides.b / sides.c) * R_TO_D;
                            angles.alpha = 90 - angles.beta;
                            steps += `<p>${trigStep}. Find $\\beta$ (SOH): $\\beta = \sin^{-1}(\\frac{b}{c}) = \sin^{-1}(\\frac{${formatNumber(sides.b)}}{${formatNumber(sides.c)}}) \approx ${formatNumber(angles.beta)}^\circ$</p>`;
                        } else if (!isNaN(sides.a) && !isNaN(sides.b) && isNaN(angles.alpha)) {
                            angles.alpha = Math.atan(sides.a / sides.b) * R_TO_D;
                            angles.beta = 90 - angles.alpha;
                            steps += `<p>${trigStep}. Find $\\alpha$ (TOA): $\\alpha = \tan^{-1}(\\frac{a}{b}) = \tan^{-1}(\\frac{${formatNumber(sides.a)}}{${formatNumber(sides.b)}}) \approx ${formatNumber(angles.alpha)}^\circ$</p>`;
                        }
                        
                        // If one side and one angle are known, solve for remaining sides
                        if (!isNaN(angles.alpha) && !isNaN(sides.a) && isNaN(sides.b)) {
                            sides.b = sides.a / Math.tan(angles.alpha * D_TO_R);
                            sides.c = sides.a / Math.sin(angles.alpha * D_TO_R);
                             steps += `<p>${trigStep}. Find sides b and c using $\\alpha$ (e.g., $b = a / \tan(\\alpha)$): $b \approx ${formatNumber(sides.b)}$, $c \approx ${formatNumber(sides.c)}$</p>`;
                        } else if (!isNaN(angles.alpha) && !isNaN(sides.b) && isNaN(sides.a)) {
                            sides.a = sides.b * Math.tan(angles.alpha * D_TO_R);
                            sides.c = sides.b / Math.cos(angles.alpha * D_TO_R);
                             steps += `<p>${trigStep}. Find sides a and c using $\\alpha$ (e.g., $a = b \cdot \tan(\\alpha)$): $a \approx ${formatNumber(sides.a)}$, $c \approx ${formatNumber(sides.c)}$</p>`;
                        } else if (!isNaN(angles.alpha) && !isNaN(sides.c) && isNaN(sides.a)) {
                            sides.a = sides.c * Math.sin(angles.alpha * D_TO_R);
                            sides.b = sides.c * Math.cos(angles.alpha * D_TO_R);
                             steps += `<p>${trigStep}. Find sides a and b using $\\alpha$ (e.g., $a = c \cdot \sin(\\alpha)$): $a \approx ${formatNumber(sides.a)}$, $b \approx ${formatNumber(sides.b)}$</p>`;
                        }
                        
                        // --- Finalize and Output ---
                        resultSides.innerHTML = `a = ${formatNumber(sides.a)}<br>b = ${formatNumber(sides.b)}<br>c = ${formatNumber(sides.c)}`;
                        resultAngles.innerHTML = `$\\alpha = ${formatNumber(angles.alpha)}^\\circ$<br>$\\beta = ${formatNumber(angles.beta)}^\\circ$<br>$\\gamma = 90^\\circ$`;
                        stepByStep.innerHTML = steps;

                        resultsContainer.classList.remove('hidden');
                        
                        // Re-render MathJax
                        if (window.MathJax) {
                            window.MathJax.typesetPromise([resultAngles, stepByStep]);
                        }

                    } catch (e) {
                        showError(e.message);
                    }
                });
                
                // Initial calculation (for example values)
                sA.value = 3;
                sB.value = 4;
                calculateBtn.click(); 
              });
            </script>

