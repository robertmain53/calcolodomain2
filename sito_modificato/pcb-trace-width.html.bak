    <script>
      // Constants
      const RHO_CU = 1.724e-8; // resistivity of copper (ohm·m)
      const OZ_TO_UM = 34.79; // approx µm per oz/ft²

      function milToMm(mil) {
        return mil * 0.0254;
      }
      function mmToMil(mm) {
        return mm / 0.0254;
      }

      function getCopperThicknessMm(oz) {
        return (oz * OZ_TO_UM) / 1000; // µm -> mm
      }

      function ipcCurrentFromArea(areaMil2, dT, layer) {
        const k = layer === "external" ? 0.048 : 0.024;
        return k * Math.pow(dT, 0.44) * Math.pow(areaMil2, 0.725);
      }

      function ipcAreaFromCurrent(I, dT, layer) {
        const k = layer === "external" ? 0.048 : 0.024;
        if (I <= 0 || dT <= 0) return 0;
        const base = I / (k * Math.pow(dT, 0.44));
        return Math.pow(base, 1 / 0.725);
      }

      function updateModeUI() {
        const mode = document.getElementById("calcMode").value;
        const widthGroup = document.getElementById("widthInputGroup");
        const currentInput = document.getElementById("current");
        const currentLabel = currentInput.previousElementSibling;

        if (mode === "widthFromCurrent") {
          widthGroup.classList.add("hidden");
          currentLabel.textContent = "Current (A)";
        } else {
          widthGroup.classList.remove("hidden");
          currentLabel.textContent = "Current limit (A, optional)";
        }
      }

      function updateUnitLabels() {
        const system = document.getElementById("unitSystem").value;
        document.getElementById("currentUnitLabel").textContent = "(A)";
        document.getElementById("widthUnitLabel").textContent =
          system === "metric" ? "(mm)" : "(mil)";
      }

      function suggestStandardWidth(widthMm) {
        if (!widthMm || !isFinite(widthMm)) return "–";
        const mil = mmToMil(widthMm);
        const standardsMil = [
          4, 5, 6, 8, 10, 12, 15, 20, 25, 30, 40, 50, 60, 80, 100, 150, 200,
        ];
        let chosen = standardsMil[standardsMil.length - 1];
        for (let s of standardsMil) {
          if (s >= mil) {
            chosen = s;
            break;
          }
        }
        const chosenMm = milToMm(chosen);
        return `${chosen.toFixed(0)} mil (≈ ${chosenMm.toFixed(3)} mm)`;
      }

      function calculate() {
        const unitSystem = document.getElementById("unitSystem").value;
        const layer = document.getElementById("layerType").value;
        const mode = document.getElementById("calcMode").value;
        const copperOz = parseFloat(
          document.getElementById("copperWeight").value
        );
        const dT = parseFloat(document.getElementById("tempRise").value);
        const Iinput = parseFloat(document.getElementById("current").value);
        const widthInputVal = parseFloat(
          document.getElementById("traceWidthInput").value
        );

        if (!dT || dT <= 0) {
          alert("Please enter a valid temperature rise.");
          return;
        }

        const thicknessMm = getCopperThicknessMm(copperOz);
        const thicknessMil = mmToMil(thicknessMm);

        let widthMm, widthMil, I, dTused;

        if (mode === "widthFromCurrent") {
          if (!Iinput || Iinput <= 0) {
            alert("Please enter a valid current.");
            return;
          }
          I = Iinput;
          const areaMil2 = ipcAreaFromCurrent(I, dT, layer);
          widthMil = areaMil2 / thicknessMil;
          widthMm = milToMm(widthMil);
          dTused = dT;
        } else {
          // currentFromWidth
          if (!widthInputVal || widthInputVal <= 0) {
            alert("Please enter a valid trace width.");
            return;
          }
          widthMm =
            unitSystem === "metric" ? widthInputVal : milToMm(widthInputVal);
          widthMil = mmToMil(widthMm);
          const areaMil2 = widthMil * thicknessMil;
          I = ipcCurrentFromArea(areaMil2, dT, layer);
          dTused = dT;
        }

        // Derived quantities
        const areaMm2 = widthMm * thicknessMm;
        const currentDensity = areaMm2 > 0 ? I / areaMm2 : 0;

        // Resistance per cm
        const lengthM = 0.01; // 1 cm
        const areaM2 = areaMm2 * 1e-6;
        const RperCm = areaM2 > 0 ? (RHO_CU * lengthM) / areaM2 : 0; // ohm/cm
        const RperCm_mOhm = RperCm * 1000;

        // Voltage drop per 10 cm at I
        const length10cm = 0.1;
        const R10cm = areaM2 > 0 ? (RHO_CU * length10cm) / areaM2 : 0;
        const Vdrop10cm = I * R10cm;

        // Populate UI
        const widthMain = unitSystem === "metric" ? widthMm : widthMil;
        const widthAlt = unitSystem === "metric" ? widthMil : widthMm;
        const mainUnit = unitSystem === "metric" ? "mm" : "mil";
        const altUnit = unitSystem === "metric" ? "mil" : "mm";

        document.getElementById("widthResultMain").textContent = isFinite(
          widthMain
        )
          ? widthMain.toFixed(3)
          : "–";
        document.getElementById("widthResultUnit").textContent = mainUnit;
        document.getElementById("widthResultAlt").textContent = isFinite(
          widthAlt
        )
          ? widthAlt.toFixed(1)
          : "–";
        document.getElementById("widthResultAltUnit").textContent = altUnit;
        document.getElementById("widthStandard").textContent =
          suggestStandardWidth(widthMm);

        document.getElementById("currentResult").textContent = isFinite(I)
          ? I.toFixed(3)
          : "–";
        document.getElementById("tempResult").textContent = isFinite(dTused)
          ? dTused.toFixed(1)
          : "–";
        document.getElementById("currentDensityResult").textContent = isFinite(
          currentDensity
        )
          ? currentDensity.toFixed(2)
          : "–";

        document.getElementById(
          "thicknessResult"
        ).textContent = `${thicknessMm.toFixed(3)} mm (${mmToMil(
          thicknessMm
        ).toFixed(2)} mil)`;
        document.getElementById("areaResult").textContent = isFinite(areaMm2)
          ? areaMm2.toExponential(3)
          : "–";

        document.getElementById("resPerLenResult").textContent = isFinite(
          RperCm_mOhm
        )
          ? RperCm_mOhm.toFixed(3)
          : "–";
        document.getElementById("vdropResult").textContent = isFinite(Vdrop10cm)
          ? Vdrop10cm.toFixed(4)
          : "–";

        let note = "";
        if (currentDensity > 35) {
          note =
            "Very high current density – consider wider trace, heavier copper, or multiple layers.";
        } else if (currentDensity > 20) {
          note =
            "High current density – acceptable for short runs, but consider more margin.";
        } else {
          note = "Current density is moderate for typical FR‑4 designs.";
        }
        if (layer === "internal") {
          note +=
            " Internal layer selected – thermal performance is worse than external.";
        }
        document.getElementById("designNote").textContent = note;

        document.getElementById("resultsCard").classList.remove("hidden");
      }

      function resetForm() {
        document.getElementById("unitSystem").value = "metric";
        document.getElementById("layerType").value = "external";
        document.getElementById("calcMode").value = "widthFromCurrent";
        document.getElementById("copperWeight").value = "1";
        document.getElementById("tempRise").value = "10";
        document.getElementById("current").value = "1.0";
        document.getElementById("traceWidthInput").value = "0.25";
        document.getElementById("resultsCard").classList.add("hidden");
        updateModeUI();
        updateUnitLabels();
      }

      document.addEventListener("DOMContentLoaded", function () {
        document
          .getElementById("calcMode")
          .addEventListener("change", updateModeUI);
        document
          .getElementById("unitSystem")
          .addEventListener("change", updateUnitLabels);
        document
          .getElementById("calculateBtn")
          .addEventListener("click", calculate);
        document
          .getElementById("resetBtn")
          .addEventListener("click", resetForm);
        updateModeUI();
        updateUnitLabels();
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
