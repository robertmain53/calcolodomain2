    <script>
      // Mobile menu toggle (fallback if mobile-menu.js doesn't handle it)
      document.addEventListener("DOMContentLoaded", function () {
        const toggle = document.getElementById("mobile-menu-toggle");
        const menu = document.getElementById("mobile-menu");
        if (toggle && menu) {
          toggle.addEventListener("click", () => {
            const isHidden = menu.classList.contains("hidden");
            menu.classList.toggle("hidden", !isHidden);
            toggle.setAttribute("aria-expanded", String(isHidden));
          });
        }
      });

      document.addEventListener("DOMContentLoaded", function () {
        // Mode switching
        const modeCheckBtn = document.getElementById("prime-mode-check-btn");
        const modeRangeBtn = document.getElementById("prime-mode-range-btn");
        const panelCheck = document.getElementById("prime-panel-check");
        const panelRange = document.getElementById("prime-panel-range");

        function setMode(mode) {
          const isCheck = mode === "check";
          panelCheck.classList.toggle("hidden", !isCheck);
          panelRange.classList.toggle("hidden", isCheck);
          modeCheckBtn.classList.toggle("pill-active", isCheck);
          modeCheckBtn.classList.toggle("pill-inactive", !isCheck);
          modeRangeBtn.classList.toggle("pill-active", !isCheck);
          modeRangeBtn.classList.toggle("pill-inactive", isCheck);
        }

        modeCheckBtn.addEventListener("click", function () {
          setMode("check");
        });
        modeRangeBtn.addEventListener("click", function () {
          setMode("range");
        });

        // Prime check & factorization
        const primeNInput = document.getElementById("prime-n");
        const primeCheckBtn = document.getElementById("prime-check-btn");
        const primeCheckClearBtn = document.getElementById(
          "prime-check-clear-btn"
        );
        const primeExampleBtn = document.getElementById("prime-example-btn");
        const primeCheckError = document.getElementById("prime-check-error");
        const primeCheckResults = document.getElementById(
          "prime-check-results"
        );
        const primeCheckBadge = document.getElementById("prime-check-badge");
        const primeCheckClass = document.getElementById("prime-check-class");
        const primeCheckFactorization = document.getElementById(
          "prime-check-factorization"
        );
        const primeCheckReason = document.getElementById("prime-check-reason");

        function parsePositiveInteger(str, label) {
          const t = (str || "").trim();
          if (!t) throw new Error(label + " is required.");
          if (!/^[+-]?\d+$/.test(t)) {
            throw new Error(label + " must be an integer (no decimals).");
          }
          const val = Number(t);
          if (!Number.isFinite(val)) {
            throw new Error(label + " is out of supported range.");
          }
          if (val < 2) {
            throw new Error(label + " must be ≥ 2.");
          }
          if (!Number.isSafeInteger(val)) {
            throw new Error(
              label + " must be within the safe integer range of JavaScript."
            );
          }
          return val;
        }

        function isPrimeAndFactors(n) {
          if (n === 2 || n === 3) {
            return {
              prime: true,
              factors: [{ p: n, e: 1 }],
              reason: n + " has exactly two divisors: 1 and " + n + ".",
            };
          }
          if (n % 2 === 0) {
            const half = n / 2;
            return {
              prime: false,
              factors: [{ p: 2, e: 1 }].concat(
                half === 1 ? [] : isPrimeAndFactors(half).factors
              ),
              reason:
                "The number is even and greater than 2, so it is divisible by 2 and not prime.",
            };
          }
          if (n % 3 === 0) {
            const third = n / 3;
            return {
              prime: false,
              factors: [{ p: 3, e: 1 }].concat(
                third === 1 ? [] : isPrimeAndFactors(third).factors
              ),
              reason:
                "The sum of its digits is divisible by 3 or a direct test shows n is divisible by 3.",
            };
          }
          const limit = Math.floor(Math.sqrt(n));
          let factors = [];
          let temp = n;
          // Trial division by primes of the form 6k ± 1
          for (let i = 5; i <= limit && temp > 1; i += 6) {
            [i, i + 2].forEach((candidate) => {
              if (candidate <= limit && temp % candidate === 0) {
                let e = 0;
                while (temp % candidate === 0) {
                  temp / candidate;
                  e++;
                }
                factors.push({ p: candidate, e });
              }
            });
          }
          if (factors.length === 0) {
            return {
              prime: true,
              factors: [{ p: n, e: 1 }],
              reason:
                "No divisor was found up to √n using trial division, so the number is prime.",
            };
          }
          if (temp > 1) {
            factors.push({ p: temp, e: 1 });
          }
          factors.sort((a, b) => a.p - b.p);
          return {
            prime: false,
            factors,
            reason:
              "A divisor less than or equal to √n was found, so n can be written as a product of smaller integers.",
          };
        }

        function formatFactorization(factors) {
          if (!factors || !factors.length) return "(none)";
          return factors
            .map((f) => (f.e === 1 ? f.p.toString() : f.p + "^" + f.e))
            .join(" · ");
        }

        function clearPrimeCheckOutputs() {
          primeCheckError.textContent = "";
          primeCheckError.classList.add("hidden");
          primeCheckResults.classList.add("hidden");
          primeCheckClass.textContent = "";
          primeCheckFactorization.textContent = "";
          primeCheckReason.textContent = "";
          primeCheckBadge.textContent = "–";
          primeCheckBadge.classList.remove(
            "badge-prime",
            "badge-composite",
            "badge-neutral"
          );
          primeCheckBadge.classList.add("badge-neutral");
        }

        function runPrimeCheck() {
          clearPrimeCheckOutputs();
          try {
            const n = parsePositiveInteger(
              primeNInput.value,
              "Integer to test"
            );
            if (n > 1000000000000) {
              // 1e12 safety hint
              throw new Error(
                "This demo calculator is optimized for integers up to about 10¹². For larger values, use a specialized big-integer library."
              );
            }
            const { prime, factors, reason } = isPrimeAndFactors(n);
            const factStr = formatFactorization(factors);

            if (prime) {
              primeCheckClass.textContent =
                n +
                " is a prime number (its only positive divisors are 1 and " +
                n +
                ").";
              primeCheckBadge.textContent = "Prime";
              primeCheckBadge.classList.remove(
                "badge-neutral",
                "badge-composite"
              );
              primeCheckBadge.classList.add("badge-prime");
            } else {
              primeCheckClass.textContent = n + " is a composite number.";
              primeCheckBadge.textContent = "Composite";
              primeCheckBadge.classList.remove("badge-neutral", "badge-prime");
              primeCheckBadge.classList.add("badge-composite");
            }

            primeCheckFactorization.textContent = prime
              ? n + " = " + factStr
              : n + " = " + factStr;

            primeCheckReason.textContent = reason;
            primeCheckResults.classList.remove("hidden");
          } catch (e) {
            primeCheckError.textContent =
              e.message || "An error occurred while checking primality.";
            primeCheckError.classList.remove("hidden");
          }
        }

        function clearPrimeCheck() {
          primeNInput.value = "";
          clearPrimeCheckOutputs();
        }

        function loadPrimeExample() {
          // A classic large composite: 600851475143 from well-known problems
          primeNInput.value = "600851475143";
          runPrimeCheck();
        }

        primeCheckBtn.addEventListener("click", runPrimeCheck);
        primeCheckClearBtn.addEventListener("click", clearPrimeCheck);
        primeExampleBtn.addEventListener("click", loadPrimeExample);
        primeNInput.addEventListener("keydown", function (e) {
          if (e.key === "Enter") {
            e.preventDefault();
            runPrimeCheck();
          }
        });

        // Prime list in range
        const primeStartInput = document.getElementById("prime-start");
        const primeEndInput = document.getElementById("prime-end");
        const primeRangeBtn = document.getElementById("prime-range-btn");
        const primeRangeClearBtn = document.getElementById(
          "prime-range-clear-btn"
        );
        const primeRangeError = document.getElementById("prime-range-error");
        const primeRangeResults = document.getElementById(
          "prime-range-results"
        );
        const primeRangeSummary = document.getElementById(
          "prime-range-summary"
        );
        const primeRangeList = document.getElementById("prime-range-list");
        const primeRangeNote = document.getElementById("prime-range-note");

        function parseRangeEndpoint(str, label) {
          const t = (str || "").trim();
          if (!t) throw new Error(label + " is required.");
          if (!/^[+-]?\d+$/.test(t)) {
            throw new Error(label + " must be an integer.");
          }
          const val = Number(t);
          if (!Number.isFinite(val)) {
            throw new Error(label + " is out of supported range.");
          }
          if (!Number.isSafeInteger(val)) {
            throw new Error(
              label + " must be within the safe integer range of JavaScript."
            );
          }
          return val;
        }

        function sievePrimesUpTo(n) {
          const limit = n;
          const isPrime = new Array(limit + 1).fill(true);
          isPrime[0] = false;
          if (limit >= 1) isPrime[1] = false;
          const root = Math.floor(Math.sqrt(limit));
          for (let p = 2; p <= root; p++) {
            if (isPrime[p]) {
              for (let multiple = p * p; multiple <= limit; multiple += p) {
                isPrime[multiple] = false;
              }
            }
          }
          const primes = [];
          for (let i = 2; i <= limit; i++) {
            if (isPrime[i]) primes.push(i);
          }
          return primes;
        }

        function clearPrimeRangeOutputs() {
          primeRangeError.textContent = "";
          primeRangeError.classList.add("hidden");
          primeRangeResults.classList.add("hidden");
          primeRangeSummary.textContent = "";
          primeRangeList.textContent = "";
          primeRangeNote.textContent = "";
        }

        function runPrimeRange() {
          clearPrimeRangeOutputs();
          try {
            const start = parseRangeEndpoint(
              primeStartInput.value,
              "Start of range"
            );
            const end = parseRangeEndpoint(primeEndInput.value, "End of range");
            if (start > end) {
              throw new Error("Start of range must be ≤ end of range.");
            }
            if (end < 2) {
              throw new Error(
                "The range must include at least some integers ≥ 2."
              );
            }
            const effectiveStart = Math.max(2, start);
            const length = end - start + 1;
            if (length > 100000) {
              throw new Error(
                "Range too large. Please limit the range length to 100,000 integers or fewer."
              );
            }
            if (end > 2000000) {
              throw new Error(
                "For performance reasons, the upper bound is limited to about 2,000,000 in this demo."
              );
            }

            const primesUpToEnd = sievePrimesUpTo(end);
            const primesInRange = primesUpToEnd.filter(
              (p) => p >= effectiveStart
            );
            const count = primesInRange.length;
            const totalIntegers = end - effectiveStart + 1;
            const density = totalIntegers > 0 ? count / totalIntegers : 0;

            primeRangeSummary.textContent =
              "Found " +
              count +
              " prime" +
              (count === 1 ? "" : "s") +
              " between " +
              effectiveStart +
              " and " +
              end +
              " (prime density ≈ " +
              density.toFixed(4) +
              ").";

            const displayLimit = 1000;
            if (count === 0) {
              primeRangeList.textContent = "(no primes in this range)";
              primeRangeNote.textContent = "";
            } else if (count <= displayLimit) {
              primeRangeList.textContent = primesInRange.join(", ");
              primeRangeNote.textContent =
                "All primes in the selected range are shown.";
            } else {
              const shown = primesInRange.slice(0, displayLimit);
              primeRangeList.textContent = shown.join(", ");
              primeRangeNote.textContent =
                "Showing the first " +
                displayLimit +
                " primes only. " +
                "Total primes in range: " +
                count +
                ". Copy this subset or narrow the range for full visibility.";
            }

            primeRangeResults.classList.remove("hidden");
          } catch (e) {
            primeRangeError.textContent =
              e.message || "An error occurred while generating the prime list.";
            primeRangeError.classList.remove("hidden");
          }
        }

        function clearPrimeRange() {
          primeStartInput.value = "";
          primeEndInput.value = "";
          clearPrimeRangeOutputs();
        }

        primeRangeBtn.addEventListener("click", runPrimeRange);
        primeRangeClearBtn.addEventListener("click", clearPrimeRange);
      });
    </script>
  
