<script>
  // Initialize story rows
  function buildStoryRows(n) {
    const tbody = document.getElementById('storiesBody');
    tbody.innerHTML = '';
    const defaultW = 200; // kips
    const defaultHStep = parseFloat(document.getElementById('height').value || '60') / n;
    for (let i = n; i >= 1; i--) {
      const tr = document.createElement('tr');
      tr.innerHTML = `
        <td class="border px-2 py-1 text-center">Level ${i}</td>
        <td class="border px-2 py-1">
          <input type="number" step="1" class="w-full border rounded px-1 py-0.5 story-w" value="${defaultW.toFixed(0)}">
        </td>
        <td class="border px-2 py-1">
          <input type="number" step="1" class="w-full border rounded px-1 py-0.5 story-h" value="${(defaultHStep * i).toFixed(0)}">
        </td>
      `;
      tbody.appendChild(tr);
    }
  }

  document.addEventListener('DOMContentLoaded', () => {
    buildStoryRows(parseInt(document.getElementById('numStories').value, 10));

    document.getElementById('applyStories').addEventListener('click', () => {
      const n = Math.max(1, Math.min(20, parseInt(document.getElementById('numStories').value, 10) || 1));
      document.getElementById('numStories').value = n;
      buildStoryRows(n);
    });

    document.getElementById('systemType').addEventListener('change', (e) => {
      const RInput = document.getElementById('R');
      switch (e.target.value) {
        case 'smrf': RInput.value = 8; break;
        case 'brbf': RInput.value = 8; break;
        case 'scbf': RInput.value = 6; break;
        case 'rcShear': RInput.value = 6; break;
        default: break;
      }
    });

    document.getElementById('riskCategory').addEventListener('change', (e) => {
      const IeInput = document.getElementById('Ie');
      switch (e.target.value) {
        case 'II': IeInput.value = 1.0; break;
        case 'III': IeInput.value = 1.25; break;
        case 'IV': IeInput.value = 1.5; break;
        default: IeInput.value = 1.0;
      }
    });

    document.getElementById('resetBtn').addEventListener('click', () => {
      document.getElementById('Ss').value = 1.0;
      document.getElementById('S1').value = 0.4;
      document.getElementById('siteClass').value = 'C';
      document.getElementById('riskCategory').value = 'II';
      document.getElementById('Ie').value = 1.0;
      document.getElementById('systemType').value = 'smrf';
      document.getElementById('R').value = 8;
      document.getElementById('T').value = '';
      document.getElementById('height').value = 60;
      document.getElementById('rho').value = 1.0;
      document.getElementById('numStories').value = 4;
      buildStoryRows(4);
      document.getElementById('resultsSection').classList.add('hidden');
      document.getElementById('errorMsg').textContent = '';
    });

    document.getElementById('calculateBtn').addEventListener('click', calculateSeismic);
  });

  function getFa(Ss, siteClass) {
    // Simplified Fa table (approximate, for Ss <= 1.5)
    if (siteClass === 'B') return 1.0;
    if (siteClass === 'C') {
      if (Ss <= 0.25) return 1.2;
      if (Ss <= 0.5) return 1.2;
      if (Ss <= 0.75) return 1.1;
      if (Ss <= 1.0) return 1.0;
      return 1.0;
    }
    if (siteClass === 'D') {
      if (Ss <= 0.25) return 1.6;
      if (Ss <= 0.5) return 1.4;
      if (Ss <= 0.75) return 1.2;
      if (Ss <= 1.0) return 1.1;
      return 1.0;
    }
    if (siteClass === 'E') {
      if (Ss <= 0.25) return 2.5;
      if (Ss <= 0.5) return 2.5;
      if (Ss <= 0.75) return 2.4;
      if (Ss <= 1.0) return 2.3;
      return 2.3;
    }
    return 1.0;
  }

  function getFv(S1, siteClass) {
    // Simplified Fv table (approximate, for S1 <= 0.6)
    if (siteClass === 'B') return 1.0;
    if (siteClass === 'C') {
      if (S1 <= 0.1) return 1.7;
      if (S1 <= 0.2) return 1.6;
      if (S1 <= 0.3) return 1.5;
      if (S1 <= 0.4) return 1.4;
      if (S1 <= 0.5) return 1.3;
      return 1.3;
    }
    if (siteClass === 'D') {
      if (S1 <= 0.1) return 2.4;
      if (S1 <= 0.2) return 2.0;
      if (S1 <= 0.3) return 1.8;
      if (S1 <= 0.4) return 1.6;
      if (S1 <= 0.5) return 1.5;
      return 1.4;
    }
    if (siteClass === 'E') {
      if (S1 <= 0.1) return 3.5;
      if (S1 <= 0.2) return 3.2;
      if (S1 <= 0.3) return 3.0;
      if (S1 <= 0.4) return 2.8;
      if (S1 <= 0.5) return 2.6;
      return 2.4;
    }
    return 1.0;
  }

  function calculateSeismic() {
    const errEl = document.getElementById('errorMsg');
    errEl.textContent = '';

    const Ss = parseFloat(document.getElementById('Ss').value);
    const S1 = parseFloat(document.getElementById('S1').value);
    const siteClass = document.getElementById('siteClass').value;
    const R = parseFloat(document.getElementById('R').value);
    const Ie = parseFloat(document.getElementById('Ie').value);
    const rho = parseFloat(document.getElementById('rho').value);
    const hTotal = parseFloat(document.getElementById('height').value);
    let T = parseFloat(document.getElementById('T').value);

    if (!(Ss > 0) || !(S1 > 0) || !(R > 0) || !(Ie > 0) || !(rho > 0) || !(hTotal > 0)) {
      errEl.textContent = 'Please enter valid positive values for Ss, S1, R, Ie, ρ, and total height.';
      return;
    }

    // Story data
    const wInputs = Array.from(document.querySelectorAll('.story-w'));
    const hInputs = Array.from(document.querySelectorAll('.story-h'));
    if (wInputs.length === 0) {
      errEl.textContent = 'Please define at least one level.';
      return;
    }

    const n = wInputs.length;
    const w = [];
    const h = [];
    let W = 0;
    for (let i = 0; i < n; i++) {
      const wi = parseFloat(wInputs[i].value);
      const hi = parseFloat(hInputs[i].value);
      if (!(wi > 0) || !(hi >= 0)) {
        errEl.textContent = 'All story weights must be > 0 and heights ≥ 0.';
        return;
      }
      w.push(wi);
      h.push(hi);
      W += wi;
    }

    // Estimate T if not provided
    let Tused = T;
    if (!(Tused > 0)) {
      const Ct = 0.02;
      const x = 0.75;
      Tused = Ct * Math.pow(hTotal, x);
    }

    // Exponent k
    let k;
    if (Tused <= 0.5) k = 1.0;
    else if (Tused >= 2.5) k = 2.0;
    else k = 1.0 + (Tused - 0.5) * (1.0 / 2.0); // linear between 1 and 2 over 2s

    // Site coefficients
    const Fa = getFa(Ss, siteClass);
    const Fv = getFv(S1, siteClass);

    const SMS = Fa * Ss;
    const SM1 = Fv * S1;
    const SDS = (2 / 3) * SMS;
    const SD1 = (2 / 3) * SM1;

    // Basic Cs
    let Cs = SDS / (R / Ie);

    // Upper and lower bounds (simplified)
    const TL = 8.0; // default long-period transition
    if (Tused >= TL && SD1 > 0) {
      const CsMax = SD1 / (Tused * (R / Ie));
      Cs = Math.min(Cs, CsMax);
    }

    const CsMin1 = 0.044 * SDS * Ie;
    const CsMin2 = 0.01;
    Cs = Math.max(Cs, CsMin1, CsMin2);

    const V = Cs * W * rho;

    // Vertical distribution
    let sumWhk = 0;
    const whk = [];
    for (let i = 0; i < n; i++) {
      const val = w[i] * Math.pow(h[i], k);
      whk.push(val);
      sumWhk += val;
    }

    const Fx = [];
    const Vx = new Array(n).fill(0);
    if (sumWhk > 0) {
      for (let i = 0; i < n; i++) {
        Fx[i] = V * whk[i] / sumWhk;
      }
      // Story shear from top down
      let running = 0;
      for (let i = n - 1; i >= 0; i--) {
        running += Fx[i];
        Vx[i] = running;
      }
    }

    // Output
    document.getElementById('FaOut').textContent = Fa.toFixed(2);
    document.getElementById('FvOut').textContent = Fv.toFixed(2);
    document.getElementById('SMSOut').textContent = SMS.toFixed(3);
    document.getElementById('SM1Out').textContent = SM1.toFixed(3);
    document.getElementById('SDSOut').textContent = SDS.toFixed(3);
    document.getElementById('SD1Out').textContent = SD1.toFixed(3);
    document.getElementById('TEstOut').textContent = Tused.toFixed(3);
    document.getElementById('CsOut').textContent = Cs.toFixed(4);
    document.getElementById('WOut').textContent = W.toFixed(1);
    document.getElementById('VOut').textContent = V.toFixed(1);
    document.getElementById('rhoOut').textContent = rho.toFixed(2);
    document.getElementById('IeOut').textContent = Ie.toFixed(2);

    const tbody = document.getElementById('storyResultsBody');
    tbody.innerHTML = '';
    // Levels are displayed from top to bottom
    for (let i = n - 1; i >= 0; i--) {
      const tr = document.createElement('tr');
      tr.innerHTML = `
        <td class="border px-2 py-1 text-center">Level ${i + 1}</td>
        <td class="border px-2 py-1 text-right">${w[i].toFixed(1)}</td>
        <td class="border px-2 py-1 text-right">${h[i].toFixed(1)}</td>
        <td class="border px-2 py-1 text-right">${(Fx[i] || 0).toFixed(2)}</td>
        <td class="border px-2 py-1 text-right">${(Vx[i] || 0).toFixed(2)}</td>
        <td class="border px-2 py-1 text-right">${whk[i].toFixed(2)}</td>
      `;
      tbody.appendChild(tr);
    }

    document.getElementById('resultsSection').classList.remove('hidden');
  }

  window.MathJax = {
    tex: { inlineMath: [['\\(','\\)'], ['$', '$']], displayMath: [['$','$'], ['\\[','\\]']] },
    svg: { fontCache: 'global' }
  };
</script>
