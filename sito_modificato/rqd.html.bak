    <script>
      // Tab switching
      const tabCore = document.getElementById("tab-core");
      const tabSpacing = document.getElementById("tab-spacing");
      const panelCore = document.getElementById("panel-core");
      const panelSpacing = document.getElementById("panel-spacing");

      function activateTab(tab) {
        if (tab === "core") {
          tabCore.classList.add("border-blue-600", "text-blue-600");
          tabCore.classList.remove("border-transparent", "text-gray-600");
          tabSpacing.classList.remove("border-blue-600", "text-blue-600");
          tabSpacing.classList.add("border-transparent", "text-gray-600");
          panelCore.classList.remove("hidden");
          panelSpacing.classList.add("hidden");
        } else {
          tabSpacing.classList.add("border-blue-600", "text-blue-600");
          tabSpacing.classList.remove("border-transparent", "text-gray-600");
          tabCore.classList.remove("border-blue-600", "text-blue-600");
          tabCore.classList.add("border-transparent", "text-gray-600");
          panelSpacing.classList.remove("hidden");
          panelCore.classList.add("hidden");
        }
      }

      tabCore.addEventListener("click", () => activateTab("core"));
      tabSpacing.addEventListener("click", () => activateTab("spacing"));

      // Helpers
      function parseNumberList(text) {
        if (!text) return [];
        return text
          .replace(/,/g, " ")
          .split(/[\s;]+/)
          .map((v) => v.trim())
          .filter((v) => v.length > 0)
          .map((v) => Number(v))
          .filter((v) => !isNaN(v) && v >= 0);
      }

      function classifyRQD(rqd) {
        if (rqd < 0) rqd = 0;
        if (rqd > 100) rqd = 100;
        let quality = "";
        let note = "";
        if (rqd < 25) {
          quality = "Very poor";
          note =
            "Highly fractured or crushed rock mass. Expect very low stand-up time and heavy support requirements.";
        } else if (rqd < 50) {
          quality = "Poor";
          note =
            "Very blocky rock with numerous discontinuities. Significant support is usually required.";
        } else if (rqd < 75) {
          quality = "Fair";
          note =
            "Moderately jointed rock. Moderate support is typically needed for tunnels and slopes.";
        } else if (rqd < 90) {
          quality = "Good";
          note =
            "Slightly jointed, relatively intact rock. Limited support may be sufficient.";
        } else {
          quality = "Excellent";
          note =
            "Massive, intact rock with few joints. Often self-supporting for short spans.";
        }
        return { rqd: rqd, quality, note };
      }

      function showResults(rqd, method, extraDetails) {
        const resultsCard = document.getElementById("results-card");
        const resultsContent = document.getElementById("results-content");
        const rqdValue = document.getElementById("rqd-value");
        const rqdClass = document.getElementById("rqd-class");
        const rqdMethod = document.getElementById("rqd-method");
        const rqdInterp = document.getElementById("rqd-interpretation");
        const rqdDetails = document.getElementById("rqd-details");

        const cls = classifyRQD(rqd);
        rqdValue.textContent = cls.rqd.toFixed(1) + " %";
        rqdClass.textContent = cls.quality;
        rqdMethod.textContent = method;
        rqdInterp.textContent = cls.note;
        rqdDetails.textContent = extraDetails || "";
        resultsContent.classList.remove("hidden");
      }

      // Core mode calculation
      document.getElementById("btn-calc-core").addEventListener("click", () => {
        const totalLenInput = document.getElementById("core-total-length");
        const piecesInput = document.getElementById("core-pieces");
        const errorEl = document.getElementById("core-error");

        errorEl.classList.add("hidden");
        errorEl.textContent = "";

        const Ltotal = Number(totalLenInput.value);
        if (!Ltotal || Ltotal <= 0) {
          errorEl.textContent =
            "Please enter a valid core run length greater than zero.";
          errorEl.classList.remove("hidden");
          return;
        }

        const pieces = parseNumberList(piecesInput.value);
        if (pieces.length === 0) {
          errorEl.textContent =
            "Enter at least one acceptable core piece length.";
          errorEl.classList.remove("hidden");
          return;
        }

        const sumPieces = pieces.reduce((a, b) => a + b, 0);
        if (sumPieces <= 0) {
          errorEl.textContent =
            "Sum of piece lengths must be greater than zero.";
          errorEl.classList.remove("hidden");
          return;
        }

        let rqd = (sumPieces / Ltotal) * 100;
        if (!isFinite(rqd)) {
          errorEl.textContent = "Unable to compute RQD. Check your inputs.";
          errorEl.classList.remove("hidden");
          return;
        }
        if (rqd < 0) rqd = 0;
        if (rqd > 100) rqd = 100;

        const unit = document.getElementById("core-unit").value;
        const extra = `Core-based RQD using sum of ${
          pieces.length
        } acceptable pieces (ΣL = ${sumPieces.toFixed(
          3
        )} ${unit}) over a core run of ${Ltotal.toFixed(3)} ${unit}.`;

        showResults(rqd, "Core run (Deere 1963 definition)", extra);
      });

      // Core example & clear
      document
        .getElementById("btn-example-core")
        .addEventListener("click", () => {
          document.getElementById("core-total-length").value = 1.5;
          document.getElementById("core-unit").value = "m";
          document.getElementById("core-pieces").value =
            "0.35\n0.42\n0.18\n0.27";
        });
      document
        .getElementById("btn-clear-core")
        .addEventListener("click", () => {
          document.getElementById("core-total-length").value = "";
          document.getElementById("core-pieces").value = "";
          document.getElementById("core-error").classList.add("hidden");
        });

      // Spacing mode calculation
      document
        .getElementById("btn-calc-spacing")
        .addEventListener("click", () => {
          const Linput = document.getElementById("spacing-total-length");
          const valsInput = document.getElementById("spacing-values");
          const methodSel = document.getElementById("spacing-method");
          const errorEl = document.getElementById("spacing-error");

          errorEl.classList.add("hidden");
          errorEl.textContent = "";

          const L = Number(Linput.value);
          if (!L || L <= 0) {
            errorEl.textContent =
              "Please enter a valid scanline or borehole length greater than zero.";
            errorEl.classList.remove("hidden");
            return;
          }

          const spacings = parseNumberList(valsInput.value);
          if (spacings.length === 0) {
            errorEl.textContent =
              "Enter at least one discontinuity spacing value.";
            errorEl.classList.remove("hidden");
            return;
          }

          const N = spacings.length;
          const lambda = N / L; // discontinuities per unit length
          let rqdEst;
          let methodDesc;

          if (methodSel.value === "palmstrom") {
            // Exponential approximation: RQD ≈ 100 * e^(−0.1 λ)
            rqdEst = 100 * Math.exp(-0.1 * lambda);
            methodDesc =
              "Estimated from discontinuity spacing (Palmström-type exponential approximation)";
          } else {
            // Linear approximation: RQD ≈ 115 − 3.3 λ, clipped to 0–100
            rqdEst = 115 - 3.3 * lambda;
            methodDesc =
              "Estimated from discontinuity spacing (linear approximation)";
          }

          if (!isFinite(rqdEst)) {
            errorEl.textContent = "Unable to estimate RQD. Check your inputs.";
            errorEl.classList.remove("hidden");
            return;
          }

          if (rqdEst < 0) rqdEst = 0;
          if (rqdEst > 100) rqdEst = 100;

          const unit = document.getElementById("spacing-unit").value;
          const avgSpacing = spacings.reduce((a, b) => a + b, 0) / N;
          const extra = `Estimated from ${N} discontinuities over ${L.toFixed(
            3
          )} ${unit} (λ = ${lambda.toFixed(
            2
          )} joints per ${unit}, average spacing ≈ ${avgSpacing.toFixed(
            3
          )} ${unit}).`;

          showResults(rqdEst, methodDesc, extra);
        });

      // Spacing example & clear
      document
        .getElementById("btn-example-spacing")
        .addEventListener("click", () => {
          document.getElementById("spacing-total-length").value = 10;
          document.getElementById("spacing-unit").value = "m";
          document.getElementById("spacing-values").value =
            "0.25\n0.40\n0.60\n0.35\n0.80\n0.55";
        });
      document
        .getElementById("btn-clear-spacing")
        .addEventListener("click", () => {
          document.getElementById("spacing-total-length").value = "";
          document.getElementById("spacing-values").value = "";
          document.getElementById("spacing-error").classList.add("hidden");
        });

      // Mobile menu script hook (if not already in mobile-menu.js)
    </script>

