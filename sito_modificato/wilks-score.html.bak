    <script>
      // Wilks calculator logic
      (function () {
        const bodyweightInput = document.getElementById("bodyweight");
        const totalInput = document.getElementById("total");
        const sexInputs = document.querySelectorAll('input[name="sex"]');
        const unitInputs = document.querySelectorAll('input[name="units"]');
        const bwUnitLabel = document.getElementById("bw-unit-label");
        const totalUnitLabel = document.getElementById("total-unit-label");
        const calcBtn = document.getElementById("calculate-btn");
        const resetBtn = document.getElementById("reset-btn");
        const errorMessage = document.getElementById("error-message");

        const coeffEl = document.getElementById("wilks-coefficient");
        const scoreEl = document.getElementById("wilks-score");
        const ratingEl = document.getElementById("wilks-rating");

        const presetButtons = document.querySelectorAll("button[data-preset]");

        function getSelectedSex() {
          return Array.from(sexInputs).find((r) => r.checked)?.value || "male";
        }

        function getSelectedUnits() {
          return Array.from(unitInputs).find((r) => r.checked)?.value || "kg";
        }

        function updateUnitLabels() {
          const units = getSelectedUnits();
          const label = units === "kg" ? "(kg)" : "(lb)";
          bwUnitLabel.textContent = label;
          totalUnitLabel.textContent = label;
        }

        unitInputs.forEach((r) =>
          r.addEventListener("change", updateUnitLabels)
        );

        function lbsToKg(x) {
          return x / 2.2046226218;
        }

        function getWilksCoefficients(sex) {
          if (sex === "female") {
            return {
              a: 594.31747775582,
              b: -27.23842536447,
              c: 0.82112226871,
              d: -0.00930733913,
              e: 0.00004731582,
              f: -0.00000009054,
            };
          }
          // male
          return {
            a: -216.0475144,
            b: 16.2606339,
            c: -0.002388645,
            d: -0.00113732,
            e: 0.00000701863,
            f: -0.00000001291,
          };
        }

        function computeWilksCoefficient(bodyweightKg, sex) {
          const { a, b, c, d, e, f } = getWilksCoefficients(sex);
          const W = bodyweightKg;
          const denom =
            a +
            b * W +
            c * Math.pow(W, 2) +
            d * Math.pow(W, 3) +
            e * Math.pow(W, 4) +
            f * Math.pow(W, 5);
          return 500 / denom;
        }

        function classifyWilks(score, sex) {
          if (!isFinite(score) || score <= 0) return "—";
          const s = score;
          if (sex === "female") {
            if (s < 200) return "Beginner";
            if (s < 250) return "Novice";
            if (s < 300) return "Intermediate";
            if (s < 350) return "Advanced";
            if (s < 400) return "Elite";
            return "World-class";
          } else {
            if (s < 250) return "Beginner";
            if (s < 300) return "Novice";
            if (s < 350) return "Intermediate";
            if (s < 400) return "Advanced";
            if (s < 450) return "Elite";
            return "World-class";
          }
        }

        function showError(msg) {
          errorMessage.textContent = msg;
          errorMessage.classList.remove("hidden");
        }

        function clearError() {
          errorMessage.textContent = "";
          errorMessage.classList.add("hidden");
        }

        function updateResults(coeff, score, rating) {
          coeffEl.textContent = isFinite(coeff) ? coeff.toFixed(4) : "–";
          scoreEl.textContent = isFinite(score) ? score.toFixed(2) : "–";
          ratingEl.textContent = rating || "–";
        }

        function calculate() {
          clearError();
          const sex = getSelectedSex();
          const units = getSelectedUnits();

          const bw = parseFloat(bodyweightInput.value);
          const total = parseFloat(totalInput.value);

          if (isNaN(bw) || bw <= 0) {
            showError("Please enter a valid bodyweight.");
            updateResults(NaN, NaN, "");
            return;
          }
          if (isNaN(total) || total <= 0) {
            showError("Please enter a valid powerlifting total.");
            updateResults(NaN, NaN, "");
            return;
          }

          let bwKg = bw;
          let totalKg = total;
          if (units === "lb") {
            bwKg = lbsToKg(bw);
            totalKg = lbsToKg(total);
          }

          if (bwKg < 20 || bwKg > 300) {
            showError(
              "Bodyweight should be between about 20 kg and 300 kg for realistic Wilks scores."
            );
          }

          const coeff = computeWilksCoefficient(bwKg, sex);
          const score = coeff * totalKg;
          const rating = classifyWilks(score, sex);

          updateResults(coeff, score, rating);
        }

        calcBtn.addEventListener("click", function (e) {
          e.preventDefault();
          calculate();
        });

        resetBtn.addEventListener("click", function (e) {
          e.preventDefault();
          bodyweightInput.value = "";
          totalInput.value = "";
          clearError();
          updateResults(NaN, NaN, "");
        });

        presetButtons.forEach((btn) => {
          btn.addEventListener("click", () => {
            const preset = btn.getAttribute("data-preset");
            let sex = "male";
            let bw = 90;
            let total = 700;

            switch (preset) {
              case "m90-700":
                sex = "male";
                bw = 90;
                total = 700;
                break;
              case "m74-600":
                sex = "male";
                bw = 74;
                total = 600;
                break;
              case "f63-400":
                sex = "female";
                bw = 63;
                total = 400;
                break;
              case "f57-350":
                sex = "female";
                bw = 57;
                total = 350;
                break;
            }

            // set sex
            sexInputs.forEach((r) => {
              r.checked = r.value === sex;
            });
            // force kg for presets
            unitInputs.forEach((r) => {
              r.checked = r.value === "kg";
            });
            updateUnitLabels();

            bodyweightInput.value = bw;
            totalInput.value = total;
            clearError();
            calculate();
          });
        });

        // Initialize labels
        updateUnitLabels();
      })();
    </script>

