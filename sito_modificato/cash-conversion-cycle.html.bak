    <script>
      // Mode toggle
      const modeDirectBtn = document.getElementById("mode-direct");
      const modeDerivedBtn = document.getElementById("mode-derived");
      const directInputs = document.getElementById("direct-inputs");
      const derivedInputs = document.getElementById("derived-inputs");

      function setMode(mode) {
        if (mode === "direct") {
          modeDirectBtn.classList.add(
            "bg-blue-600",
            "text-white",
            "border-blue-600"
          );
          modeDirectBtn.classList.remove(
            "bg-white",
            "text-gray-700",
            "border-gray-300"
          );
          modeDerivedBtn.classList.remove(
            "bg-blue-600",
            "text-white",
            "border-blue-600"
          );
          modeDerivedBtn.classList.add(
            "bg-white",
            "text-gray-700",
            "border-gray-300"
          );
          directInputs.classList.remove("hidden");
          derivedInputs.classList.add("hidden");
        } else {
          modeDerivedBtn.classList.add(
            "bg-blue-600",
            "text-white",
            "border-blue-600"
          );
          modeDerivedBtn.classList.remove(
            "bg-white",
            "text-gray-700",
            "border-gray-300"
          );
          modeDirectBtn.classList.remove(
            "bg-blue-600",
            "text-white",
            "border-blue-600"
          );
          modeDirectBtn.classList.add(
            "bg-white",
            "text-gray-700",
            "border-gray-300"
          );
          directInputs.classList.remove("hidden");
          derivedInputs.classList.remove("hidden");
        }
      }

      modeDirectBtn.addEventListener("click", () => setMode("direct"));
      modeDerivedBtn.addEventListener("click", () => setMode("derived"));

      // Period days selector
      const periodSelect = document.getElementById("period-days");
      const periodCustomInput = document.getElementById("period-days-custom");

      function getPeriodDays() {
        const val = periodSelect.value;
        if (val === "custom") {
          const custom = parseFloat(periodCustomInput.value);
          return isFinite(custom) && custom > 0 ? custom : null;
        }
        return parseFloat(val);
      }

      periodSelect.addEventListener("change", () => {
        if (periodSelect.value === "custom") {
          periodCustomInput.classList.remove("hidden");
          periodCustomInput.focus();
        } else {
          periodCustomInput.classList.add("hidden");
        }
      });

      // Helper: safe parse
      function parsePositive(value) {
        const n = parseFloat(value);
        return isFinite(n) && n >= 0 ? n : null;
      }

      // Calculator logic
      const calcBtn = document.getElementById("calculate-btn");
      const resetBtn = document.getElementById("reset-btn");

      const dioDirectInput = document.getElementById("dio-direct");
      const dsoDirectInput = document.getElementById("dso-direct");
      const dpoDirectInput = document.getElementById("dpo-direct");

      const invBeginInput = document.getElementById("inv-begin");
      const invEndInput = document.getElementById("inv-end");
      const cogsInput = document.getElementById("cogs");

      const arBeginInput = document.getElementById("ar-begin");
      const arEndInput = document.getElementById("ar-end");
      const creditSalesInput = document.getElementById("credit-sales");

      const apBeginInput = document.getElementById("ap-begin");
      const apEndInput = document.getElementById("ap-end");
      const purchasesInput = document.getElementById("purchases");

      const resultDio = document.getElementById("result-dio");
      const resultDso = document.getElementById("result-dso");
      const resultDpo = document.getElementById("result-dpo");
      const resultCcc = document.getElementById("result-ccc");
      const resultCccInterpretation = document.getElementById(
        "result-ccc-interpretation"
      );
      const resultsNote = document.getElementById("results-note");

      function formatDays(value) {
        if (value === null || !isFinite(value)) return "–";
        return value.toFixed(1) + " days";
      }

      function interpretCCC(ccc) {
        if (!isFinite(ccc)) return "";
        if (ccc < 0)
          return "Negative CCC: the company collects from customers before paying suppliers.";
        if (ccc < 30)
          return "Very efficient: cash is tied up for less than a month.";
        if (ccc < 90)
          return "Typical range: compare to industry peers and track trends over time.";
        return "High CCC: cash is tied up for a long time; review inventory, collections, and supplier terms.";
      }

      calcBtn.addEventListener("click", () => {
        resultsNote.textContent = "";
        let days = getPeriodDays();
        if (!days) {
          resultsNote.textContent =
            "Please enter a valid number of days for the period.";
          resultDio.textContent = "–";
          resultDso.textContent = "–";
          resultDpo.textContent = "–";
          resultCcc.textContent = "–";
          resultCccInterpretation.textContent = "";
          return;
        }

        // Start with direct inputs
        let dio = parsePositive(dioDirectInput.value);
        let dso = parsePositive(dsoDirectInput.value);
        let dpo = parsePositive(dpoDirectInput.value);

        // If derived mode is active, compute missing components where possible
        const derivedModeActive = !derivedInputs.classList.contains("hidden");

        if (derivedModeActive) {
          // DIO
          if (dio === null) {
            const invBegin = parsePositive(invBeginInput.value);
            const invEnd = parsePositive(invEndInput.value);
            const cogs = parsePositive(cogsInput.value);
            if (
              invBegin !== null &&
              invEnd !== null &&
              cogs !== null &&
              cogs > 0
            ) {
              const avgInv = (invBegin + invEnd) / 2;
              dio = (avgInv / cogs) * days;
            }
          }
          // DSO
          if (dso === null) {
            const arBegin = parsePositive(arBeginInput.value);
            const arEnd = parsePositive(arEndInput.value);
            const creditSales = parsePositive(creditSalesInput.value);
            if (
              arBegin !== null &&
              arEnd !== null &&
              creditSales !== null &&
              creditSales > 0
            ) {
              const avgAr = (arBegin + arEnd) / 2;
              dso = (avgAr / creditSales) * days;
            }
          }
          // DPO
          if (dpo === null) {
            const apBegin = parsePositive(apBeginInput.value);
            const apEnd = parsePositive(apEndInput.value);
            const purchases = parsePositive(purchasesInput.value);
            if (
              apBegin !== null &&
              apEnd !== null &&
              purchases !== null &&
              purchases > 0
            ) {
              const avgAp = (apBegin + apEnd) / 2;
              dpo = (avgAp / purchases) * days;
            }
          }
        }

        // Show results
        resultDio.textContent = dio !== null ? formatDays(dio) : "–";
        resultDso.textContent = dso !== null ? formatDays(dso) : "–";
        resultDpo.textContent = dpo !== null ? formatDays(dpo) : "–";

        if (dio === null || dso === null || dpo === null) {
          resultCcc.textContent = "–";
          resultCccInterpretation.textContent = "";
          resultsNote.textContent =
            "To compute CCC, you need values for DIO, DSO, and DPO. Enter them directly or provide enough financial data to derive them.";
          return;
        }

        const ccc = dio + dso - dpo;
        resultCcc.textContent = formatDays(ccc);
        resultCccInterpretation.textContent = interpretCCC(ccc);
        resultsNote.textContent =
          "Note: Results are approximate and depend on the quality and consistency of your input data.";
      });

      resetBtn.addEventListener("click", () => {
        // Clear inputs
        [
          dioDirectInput,
          dsoDirectInput,
          dpoDirectInput,
          invBeginInput,
          invEndInput,
          cogsInput,
          arBeginInput,
          arEndInput,
          creditSalesInput,
          apBeginInput,
          apEndInput,
          purchasesInput,
          periodCustomInput,
        ].forEach((el) => (el.value = ""));

        periodSelect.value = "365";
        periodCustomInput.classList.add("hidden");
        setMode("direct");

        // Clear results
        resultDio.textContent = "–";
        resultDso.textContent = "–";
        resultDpo.textContent = "–";
        resultCcc.textContent = "–";
        resultCccInterpretation.textContent = "";
        resultsNote.textContent = "";
      });

      // Initialize default mode
      setMode("direct");
    </script>

