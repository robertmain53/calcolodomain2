    <script>
      function safeDiv(num, den) {
        if (!isFinite(num) || !isFinite(den) || den === 0) return null;
        return num / den;
      }

      function formatPercent(value) {
        if (value === null) return "N/A";
        return (value * 100).toFixed(1) + "%";
      }

      function formatRatio(value) {
        if (value === null) return "N/A";
        if (!isFinite(value)) return "∞";
        if (value === 0) return "0";
        return value.toFixed(2);
      }

      function classifyMetric(sens, spec, lrp, lrm) {
        const parts = [];
        if (sens !== null) {
          if (sens >= 0.9) parts.push("high sensitivity");
          else if (sens >= 0.8) parts.push("moderate sensitivity");
          else parts.push("low sensitivity");
        }
        if (spec !== null) {
          if (spec >= 0.9) parts.push("high specificity");
          else if (spec >= 0.8) parts.push("moderate specificity");
          else parts.push("low specificity");
        }

        let lrComment = "";
        if (lrp !== null && isFinite(lrp)) {
          if (lrp >= 10)
            lrComment = "very strong evidence for disease when positive";
          else if (lrp >= 5)
            lrComment = "moderate evidence for disease when positive";
          else if (lrp >= 2)
            lrComment =
              "small increase in the probability of disease when positive";
          else lrComment = "little change in probability when positive";
        }
        if (lrm !== null && isFinite(lrm)) {
          if (lrm <= 0.1)
            lrComment +=
              (lrComment ? "; " : "") +
              "very strong evidence against disease when negative";
          else if (lrm <= 0.2)
            lrComment +=
              (lrComment ? "; " : "") +
              "moderate evidence against disease when negative";
          else if (lrm <= 0.5)
            lrComment +=
              (lrComment ? "; " : "") +
              "small decrease in probability when negative";
          else
            lrComment +=
              (lrComment ? "; " : "") +
              "little change in probability when negative";
        }

        return { summary: parts.join(" and "), lrComment };
      }

      function updateCalculator() {
        const tp = parseInt(document.getElementById("tp-input").value, 10) || 0;
        const fp = parseInt(document.getElementById("fp-input").value, 10) || 0;
        const fn = parseInt(document.getElementById("fn-input").value, 10) || 0;
        const tn = parseInt(document.getElementById("tn-input").value, 10) || 0;

        const diseasePos = tp + fn;
        const diseaseNeg = tn + fp;
        const total = diseasePos + diseaseNeg;

        document.getElementById("total-disease-pos").textContent = diseasePos;
        document.getElementById("total-disease-neg").textContent = diseaseNeg;
        document.getElementById("total-n").textContent = total;

        const sens = safeDiv(tp, tp + fn);
        const spec = safeDiv(tn, tn + fp);
        const ppv = safeDiv(tp, tp + fp);
        const npv = safeDiv(tn, tn + fn);
        const acc = safeDiv(tp + tn, total);
        const prev = safeDiv(diseasePos, total);

        const lrp =
          sens !== null && spec !== null ? safeDiv(sens, 1 - spec) : null;
        const lrm =
          sens !== null && spec !== null ? safeDiv(1 - sens, spec) : null;

        document.getElementById("sens-value").textContent = formatPercent(sens);
        document.getElementById("spec-value").textContent = formatPercent(spec);
        document.getElementById("ppv-value").textContent = formatPercent(ppv);
        document.getElementById("npv-value").textContent = formatPercent(npv);
        document.getElementById("acc-value").textContent = formatPercent(acc);
        document.getElementById("prev-value").textContent = formatPercent(prev);
        document.getElementById("lrp-value").textContent = formatRatio(lrp);
        document.getElementById("lrm-value").textContent = formatRatio(lrm);

        const interpBox = document.getElementById("interpretation-box");
        const { summary, lrComment } = classifyMetric(sens, spec, lrp, lrm);

        if (total === 0) {
          interpBox.textContent =
            "Enter non-zero counts for TP, FP, FN, and TN to see an interpretation of your test's performance.";
          return;
        }

        let text = "With the current numbers, this test has ";
        text += summary
          ? summary
          : "insufficient data to characterize sensitivity and specificity";
        if (lrComment) {
          text +=
            ". A positive result provides " +
            lrComment.replace(";", " and") +
            ".";
        } else {
          text +=
            ". Likelihood ratios cannot be reliably interpreted with the current data.";
        }
        interpBox.textContent = text;
      }

      ["tp-input", "fp-input", "fn-input", "tn-input"].forEach((id) => {
        document.getElementById(id).addEventListener("input", updateCalculator);
      });

      document
        .getElementById("reset-example")
        .addEventListener("click", function () {
          document.getElementById("tp-input").value = 80;
          document.getElementById("fp-input").value = 20;
          document.getElementById("fn-input").value = 10;
          document.getElementById("tn-input").value = 90;
          updateCalculator();
        });

      document.addEventListener("DOMContentLoaded", updateCalculator);
    </script>

