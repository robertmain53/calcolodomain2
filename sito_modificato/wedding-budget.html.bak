    <script>
      // Data model for categories
      const CATEGORY_CONFIG = [
        {
          id: "venue_catering",
          name: "Venue & catering",
          defaultPct: { classic: 45, minimal: 55, luxe: 40 },
          optional: false,
        },
        {
          id: "attire_beauty",
          name: "Attire & beauty",
          defaultPct: { classic: 8, minimal: 6, luxe: 8 },
          optional: false,
        },
        {
          id: "photo_video",
          name: "Photography & video",
          defaultPct: { classic: 12, minimal: 8, luxe: 15 },
          optional: false,
        },
        {
          id: "music_entertainment",
          name: "Music & entertainment",
          defaultPct: { classic: 8, minimal: 6, luxe: 10 },
          optional: false,
        },
        {
          id: "flowers_decor",
          name: "Flowers & décor",
          defaultPct: { classic: 10, minimal: 6, luxe: 12 },
          optional: false,
        },
        {
          id: "stationery",
          name: "Stationery & website",
          defaultPct: { classic: 3, minimal: 2, luxe: 3 },
          optional: true,
        },
        {
          id: "transport",
          name: "Transportation",
          defaultPct: { classic: 3, minimal: 2, luxe: 3 },
          optional: true,
        },
        {
          id: "ceremony_officiant",
          name: "Officiant & ceremony fees",
          defaultPct: { classic: 3, minimal: 3, luxe: 3 },
          optional: false,
        },
        {
          id: "favors_gifts",
          name: "Favors & gifts",
          defaultPct: { classic: 3, minimal: 2, luxe: 3 },
          optional: true,
        },
        {
          id: "contingency",
          name: "Contingency / misc.",
          defaultPct: { classic: 5, minimal: 10, luxe: 3 },
          optional: false,
        },
      ];

      let currentStyle = "classic"; // classic, minimal, luxe, custom
      let currencySymbol = "$";

      function formatMoney(value) {
        if (isNaN(value)) return "—";
        const abs = Math.abs(value);
        const sign = value < 0 ? "-" : "";
        return (
          sign +
          currencySymbol +
          abs.toLocaleString(undefined, { maximumFractionDigits: 0 })
        );
      }

      function getTotalBudget() {
        const val = parseFloat(document.getElementById("total-budget").value);
        return isNaN(val) || val < 0 ? 0 : val;
      }

      function getGuestCount() {
        const val = parseFloat(document.getElementById("guest-count").value);
        return isNaN(val) || val <= 0 ? null : val;
      }

      function buildTable() {
        const tbody = document.getElementById("budget-table-body");
        tbody.innerHTML = "";
        const showOptional = document.getElementById("toggle-optional").checked;
        const totalBudget = getTotalBudget();

        CATEGORY_CONFIG.forEach((cat) => {
          if (!showOptional && cat.optional) return;

          const tr = document.createElement("tr");
          tr.dataset.categoryId = cat.id;

          // Category name
          const tdName = document.createElement("td");
          tdName.className = "px-3 py-2 whitespace-nowrap";
          tdName.textContent = cat.name + (cat.optional ? " (optional)" : "");
          tr.appendChild(tdName);

          // Percent input
          const tdPct = document.createElement("td");
          tdPct.className = "px-3 py-2 text-right";
          const pctInput = document.createElement("input");
          pctInput.type = "number";
          pctInput.min = "0";
          pctInput.step = "0.5";
          pctInput.value =
            cat.defaultPct[currentStyle] ?? cat.defaultPct["classic"];
          pctInput.className =
            "w-20 px-2 py-1 border border-gray-300 rounded text-right text-xs";
          pctInput.dataset.role = "percent";
          if (currentStyle !== "custom") {
            pctInput.readOnly = true;
            pctInput.classList.add("bg-gray-50");
          }
          tdPct.appendChild(pctInput);
          tr.appendChild(tdPct);

          // Planned amount (read-only)
          const tdPlanned = document.createElement("td");
          tdPlanned.className = "px-3 py-2 text-right text-gray-800";
          tdPlanned.dataset.role = "planned";
          tr.appendChild(tdPlanned);

          // Quote / Actual
          const tdActual = document.createElement("td");
          tdActual.className = "px-3 py-2 text-right";
          const actualInput = document.createElement("input");
          actualInput.type = "number";
          actualInput.min = "0";
          actualInput.step = "50";
          actualInput.placeholder = "0";
          actualInput.className =
            "w-24 px-2 py-1 border border-gray-300 rounded text-right text-xs";
          actualInput.dataset.role = "actual";
          tdActual.appendChild(actualInput);
          tr.appendChild(tdActual);

          // Deposit
          const tdDeposit = document.createElement("td");
          tdDeposit.className = "px-3 py-2 text-right";
          const depInput = document.createElement("input");
          depInput.type = "number";
          depInput.min = "0";
          depInput.step = "50";
          depInput.placeholder = "0";
          depInput.className =
            "w-24 px-2 py-1 border border-gray-300 rounded text-right text-xs";
          depInput.dataset.role = "deposit";
          tdDeposit.appendChild(depInput);
          tr.appendChild(tdDeposit);

          // Remaining (read-only)
          const tdRemain = document.createElement("td");
          tdRemain.className = "px-3 py-2 text-right text-gray-800";
          tdRemain.dataset.role = "remaining";
          tr.appendChild(tdRemain);

          // Delta vs plan (read-only)
          const tdDelta = document.createElement("td");
          tdDelta.className = "px-3 py-2 text-right";
          tdDelta.dataset.role = "delta";
          tr.appendChild(tdDelta);

          tbody.appendChild(tr);
        });

        recalcAll();
        attachRowListeners();
      }

      function attachRowListeners() {
        const tbody = document.getElementById("budget-table-body");
        tbody.querySelectorAll("input").forEach((input) => {
          input.addEventListener("input", () => {
            recalcAll();
          });
        });
      }

      function recalcAll() {
        const totalBudget = getTotalBudget();
        const tbody = document.getElementById("budget-table-body");
        let totalPct = 0;
        let totalPlanned = 0;
        let totalActual = 0;
        let totalDeposit = 0;
        let totalRemaining = 0;

        tbody.querySelectorAll("tr").forEach((tr) => {
          const pctInput = tr.querySelector('input[data-role="percent"]');
          const plannedCell = tr.querySelector('[data-role="planned"]');
          const actualInput = tr.querySelector('input[data-role="actual"]');
          const depositInput = tr.querySelector('input[data-role="deposit"]');
          const remainingCell = tr.querySelector('[data-role="remaining"]');
          const deltaCell = tr.querySelector('[data-role="delta"]');

          const pct = parseFloat(pctInput.value) || 0;
          totalPct += pct;

          const planned = (totalBudget * pct) / 100;
          plannedCell.textContent = formatMoney(planned);
          totalPlanned += planned;

          const actual = parseFloat(actualInput.value) || 0;
          const deposit = parseFloat(depositInput.value) || 0;
          const remaining = Math.max(0, actual - deposit);
          const delta = actual - planned;

          totalActual += actual;
          totalDeposit += deposit;
          totalRemaining += remaining;

          remainingCell.textContent = formatMoney(remaining);
          deltaCell.textContent = formatMoney(delta);
          deltaCell.classList.remove(
            "text-green-700",
            "text-red-700",
            "text-gray-700"
          );
          if (delta > 1) {
            deltaCell.classList.add("text-red-700");
          } else if (delta < -1) {
            deltaCell.classList.add("text-green-700");
          } else {
            deltaCell.classList.add("text-gray-700");
          }
        });

        document.getElementById("total-percent").textContent = totalPct
          .toFixed(1)
          .replace(/\.0$/, "");
        document.getElementById("total-planned").textContent =
          formatMoney(totalPlanned);
        document.getElementById("total-actual").textContent =
          formatMoney(totalActual);
        document.getElementById("total-deposit").textContent =
          formatMoney(totalDeposit);
        document.getElementById("total-remaining").textContent =
          formatMoney(totalRemaining);
        document.getElementById("total-delta").textContent = formatMoney(
          totalActual - totalPlanned
        );

        const warning = document.getElementById("percent-warning");
        if (currentStyle === "custom" && Math.abs(totalPct - 100) > 0.1) {
          warning.classList.remove("hidden");
        } else {
          warning.classList.add("hidden");
        }

        updateSummary(totalBudget, totalActual);
        updateScenarios();
      }

      function updateSummary(totalBudget, totalActual) {
        document.getElementById("summary-planned-total").textContent =
          formatMoney(totalBudget);
        const guests = getGuestCount();
        const perGuestEl = document.getElementById("summary-per-guest");
        if (guests) {
          const perGuest = totalBudget / guests;
          perGuestEl.textContent = formatMoney(perGuest);
        } else {
          perGuestEl.textContent = "—";
        }

        const delta = totalActual;
        const summaryDelta = document.getElementById("summary-delta");
        if (delta <= 0) {
          summaryDelta.textContent = `Actual: ${formatMoney(
            totalActual
          )} (on or under plan)`;
        } else {
          summaryDelta.textContent = `Actual: ${formatMoney(
            totalActual
          )} (spend so far)`;
        }
      }

      function applyPreset(style) {
        currentStyle = style;
        document.querySelectorAll(".budget-style-btn").forEach((btn) => {
          if (btn.dataset.style === style) {
            btn.classList.add(
              "border-blue-600",
              "bg-blue-50",
              "text-blue-700",
              "font-medium"
            );
            btn.classList.remove("border-gray-300", "text-gray-700");
          } else {
            btn.classList.remove(
              "border-blue-600",
              "bg-blue-50",
              "text-blue-700",
              "font-medium"
            );
            btn.classList.add("border-gray-300", "text-gray-700");
          }
        });
        buildTable();
      }

      function updateCurrencySymbol() {
        const select = document.getElementById("currency-select");
        const val = select.value;
        switch (val) {
          case "EUR":
            currencySymbol = "€";
            break;
          case "GBP":
            currencySymbol = "£";
            break;
          default:
            currencySymbol = "$";
        }
        document.getElementById("currency-symbol").textContent = currencySymbol;
        recalcAll();
      }

      function updateScenarios() {
        const low =
          parseFloat(document.getElementById("scenario-low").value) || 0;
        const mid = getTotalBudget();
        const high =
          parseFloat(document.getElementById("scenario-high").value) || 0;
        document.getElementById("scenario-mid").value = mid;

        const guests = getGuestCount();
        const tbody = document.getElementById("scenario-table-body");
        tbody.innerHTML = "";

        const scenarios = [
          { label: "Lean", total: low },
          { label: "Current", total: mid },
          { label: "Luxe", total: high },
        ];

        scenarios.forEach((s) => {
          const tr = document.createElement("tr");

          const tdName = document.createElement("td");
          tdName.className = "px-3 py-2";
          tdName.textContent = s.label;
          tr.appendChild(tdName);

          const tdTotal = document.createElement("td");
          tdTotal.className = "px-3 py-2 text-right";
          tdTotal.textContent = formatMoney(s.total);
          tr.appendChild(tdTotal);

          const tdPerGuest = document.createElement("td");
          tdPerGuest.className = "px-3 py-2 text-right";
          if (guests && s.total > 0) {
            tdPerGuest.textContent = formatMoney(s.total / guests);
          } else {
            tdPerGuest.textContent = "—";
          }
          tr.appendChild(tdPerGuest);

          const tdVenue = document.createElement("td");
          tdVenue.className = "px-3 py-2 text-right";
          tdVenue.textContent = formatMoney(s.total * 0.45);
          tr.appendChild(tdVenue);

          const tdPhoto = document.createElement("td");
          tdPhoto.className = "px-3 py-2 text-right";
          tdPhoto.textContent = formatMoney(s.total * 0.12);
          tr.appendChild(tdPhoto);

          tbody.appendChild(tr);
        });
      }

      document.addEventListener("DOMContentLoaded", () => {
        // Initialize table
        buildTable();
        updateScenarios();

        document
          .getElementById("total-budget")
          .addEventListener("input", () => {
            recalcAll();
          });

        document.getElementById("guest-count").addEventListener("input", () => {
          recalcAll();
        });

        document
          .getElementById("currency-select")
          .addEventListener("change", updateCurrencySymbol);

        document
          .getElementById("toggle-optional")
          .addEventListener("change", () => {
            buildTable();
          });

        document
          .getElementById("reset-to-preset")
          .addEventListener("click", () => {
            buildTable();
          });

        document.querySelectorAll(".budget-style-btn").forEach((btn) => {
          btn.addEventListener("click", () => {
            applyPreset(btn.dataset.style);
          });
        });

        document
          .getElementById("scenario-low")
          .addEventListener("input", updateScenarios);
        document
          .getElementById("scenario-high")
          .addEventListener("input", updateScenarios);
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
