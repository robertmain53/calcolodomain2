    <script>
      // Utility functions
      function formatCurrency(value) {
        if (!isFinite(value)) return "$0";
        return value.toLocaleString(undefined, {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 0,
        });
      }
      function formatCurrency2(value) {
        if (!isFinite(value)) return "$0";
        return value.toLocaleString(undefined, {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 2,
        });
      }
      function formatPercent(value, digits = 1) {
        if (!isFinite(value)) return "0%";
        return value.toFixed(digits) + "%";
      }

      function computePayment(P, annualRate, years) {
        const n = Math.round(years * 12);
        if (n <= 0) return { payment: 0, n: 0 };
        const r = annualRate / 100 / 12;
        if (r === 0) {
          return { payment: P / n, n };
        }
        const pow = Math.pow(1 + r, n);
        const payment = (P * (r * pow)) / (pow - 1);
        return { payment, n };
      }

      function buildSchedule(P, annualRate, years, extra) {
        const { payment, n } = computePayment(P, annualRate, years);
        const r = annualRate / 100 / 12;
        let balance = P;
        let schedule = [];
        let totalInterest = 0;
        let i = 0;
        const maxIter = n + 600; // safety

        while (balance > 0.01 && i < maxIter) {
          i++;
          const interest = balance * r;
          let principal = payment - interest + extra;
          if (principal > balance + interest) {
            principal = balance;
          }
          const actualPayment = principal + interest;
          balance = balance - principal;
          if (balance < 0) balance = 0;
          totalInterest += interest;
          schedule.push({
            period: i,
            payment: actualPayment,
            principal,
            interest,
            balance,
          });
        }
        return { schedule, totalInterest, payment, periods: schedule.length };
      }

      function solveLoanAmount(targetPayment, annualRate, years) {
        const n = Math.round(years * 12);
        if (n <= 0) return 0;
        const r = annualRate / 100 / 12;
        if (r === 0) {
          return targetPayment * n;
        }
        const pow = Math.pow(1 + r, n);
        const denom = (r * pow) / (pow - 1);
        return targetPayment / denom;
      }

      function computeEffectiveApr(P, feePct, payment, years) {
        const net = P * (1 - feePct / 100);
        if (net <= 0 || payment <= 0) return NaN;
        const n = Math.round(years * 12);
        // Solve for monthly rate using Newton-Raphson
        let r = 12 / 100 / 12; // initial guess 12% APR
        for (let iter = 0; iter < 30; iter++) {
          let pow = Math.pow(1 + r, n);
          let f = (payment * (1 - pow)) / -r - net;
          let df = (payment * (pow * (n * r + 1) - 1)) / (r * r * (1 + r));
          if (!isFinite(f) || !isFinite(df) || df === 0) break;
          let rNext = r - f / df;
          if (Math.abs(rNext - r) < 1e-8) {
            r = rNext;
            break;
          }
          r = rNext;
          if (r < -0.99) {
            r = -0.99;
            break;
          }
        }
        const apr = (1 + r) * 12 - 12;
        return apr * 100;
      }

      function updateCalculator() {
        const amount =
          parseFloat(document.getElementById("loanAmount").value) || 0;
        const apr =
          parseFloat(document.getElementById("interestRate").value) || 0;
        const years =
          parseFloat(document.getElementById("termYears").value) || 0;
        const feePct =
          parseFloat(document.getElementById("originationFee").value) || 0;
        const extra =
          parseFloat(document.getElementById("extraPayment").value) || 0;

        document.getElementById("interestRateValue").textContent =
          apr.toFixed(1);
        document.getElementById("termYearsValue").textContent =
          years.toFixed(1);

        if (amount <= 0 || years <= 0) {
          document.getElementById("resultPayment").textContent = "$0";
          document.getElementById("resultInterest").textContent = "$0";
          document.getElementById("resultTotalCost").textContent = "$0";
          document.getElementById("resultNetProceeds").textContent = "$0";
          document.getElementById("resultEffectiveApr").textContent = "0.0%";
          document.getElementById("scheduleBody").innerHTML = "";
          return;
        }

        const { schedule, totalInterest, payment, periods } = buildSchedule(
          amount,
          apr,
          years,
          extra
        );
        const totalPaid = schedule.reduce((sum, row) => sum + row.payment, 0);
        const netProceeds = amount * (1 - feePct / 100);
        const effApr =
          feePct > 0
            ? computeEffectiveApr(amount, feePct, payment, years)
            : apr;

        document.getElementById("resultPayment").textContent = formatCurrency2(
          payment + extra
        );
        document.getElementById("resultInterest").textContent =
          formatCurrency2(totalInterest);
        document.getElementById("resultTotalCost").textContent =
          formatCurrency2(totalPaid);
        document.getElementById("resultNetProceeds").textContent =
          formatCurrency2(netProceeds);
        document.getElementById("resultEffectiveApr").textContent = isNaN(
          effApr
        )
          ? formatPercent(apr, 1)
          : formatPercent(effApr, 2);

        // Update comparison Offer A snapshot
        document.getElementById("offerA_apr").textContent = formatPercent(
          apr,
          1
        );
        document.getElementById("offerA_term").textContent =
          years.toFixed(1) + " years";
        document.getElementById("offerA_payment").textContent =
          formatCurrency2(payment);
        document.getElementById("offerA_interest").textContent =
          formatCurrency2(totalInterest);

        // Build schedule table
        const tbody = document.getElementById("scheduleBody");
        tbody.innerHTML = "";
        const maxRows = 360; // cap for performance
        schedule.slice(0, maxRows).forEach((row) => {
          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="px-3 py-1.5 text-gray-700">${row.period}</td>
        <td class="px-3 py-1.5 text-right text-gray-700">${formatCurrency2(
          row.payment
        )}</td>
        <td class="px-3 py-1.5 text-right text-gray-700">${formatCurrency2(
          row.principal
        )}</td>
        <td class="px-3 py-1.5 text-right text-gray-700">${formatCurrency2(
          row.interest
        )}</td>
        <td class="px-3 py-1.5 text-right text-gray-700">${formatCurrency2(
          row.balance
        )}</td>
      `;
          tbody.appendChild(tr);
        });
        if (schedule.length > maxRows) {
          const tr = document.createElement("tr");
          tr.innerHTML = `<td colspan="5" class="px-3 py-2 text-center text-xs text-gray-500">Showing first ${maxRows} payments of ${schedule.length} total.</td>`;
          tbody.appendChild(tr);
        }

        updateComparison(); // keep comparison in sync
      }

      function updateComparison() {
        const enabled = document.getElementById("enableComparison").checked;
        const panel = document.getElementById("comparisonPanel");
        if (enabled) {
          panel.classList.remove("opacity-40", "pointer-events-none");
        } else {
          panel.classList.add("opacity-40", "pointer-events-none");
        }

        const amount =
          parseFloat(document.getElementById("loanAmount").value) || 0;
        const aprA =
          parseFloat(document.getElementById("interestRate").value) || 0;
        const yearsA =
          parseFloat(document.getElementById("termYears").value) || 0;

        const aprB =
          parseFloat(document.getElementById("offerB_apr_input").value) || 0;
        const yearsB =
          parseFloat(document.getElementById("offerB_term_input").value) || 0;

        if (!enabled || amount <= 0 || yearsA <= 0 || yearsB <= 0) {
          return;
        }

        const { payment: payA, n: nA } = computePayment(amount, aprA, yearsA);
        const totalInterestA = payA * nA - amount;

        const { payment: payB, n: nB } = computePayment(amount, aprB, yearsB);
        const totalInterestB = payB * nB - amount;

        document.getElementById("offerA_payment").textContent =
          formatCurrency2(payA);
        document.getElementById("offerA_interest").textContent =
          formatCurrency2(totalInterestA);

        document.getElementById("offerB_payment").textContent =
          formatCurrency2(payB);
        document.getElementById("offerB_interest").textContent =
          formatCurrency2(totalInterestB);

        const diff = totalInterestB - totalInterestA;
        const noteEl = document.getElementById("offerComparisonNote");
        const extraEl = document.getElementById("offerB_extraCost");
        if (diff > 0) {
          extraEl.textContent = formatCurrency2(diff);
          extraEl.classList.remove("text-green-600");
          extraEl.classList.add("text-red-600");
          noteEl.innerHTML = `Offer B costs <span class="font-semibold text-red-600" id="offerB_extraCost">${formatCurrency2(
            diff
          )}</span> more in interest than Offer A.`;
        } else if (diff < 0) {
          extraEl.textContent = formatCurrency2(-diff);
          extraEl.classList.remove("text-red-600");
          extraEl.classList.add("text-green-600");
          noteEl.innerHTML = `Offer B saves <span class="font-semibold text-green-600" id="offerB_extraCost">${formatCurrency2(
            -diff
          )}</span> in interest compared with Offer A.`;
        } else {
          noteEl.textContent = "Both offers have the same total interest cost.";
        }
      }

      document.addEventListener("DOMContentLoaded", function () {
        // Bind events
        [
          "loanAmount",
          "interestRate",
          "termYears",
          "originationFee",
          "extraPayment",
        ].forEach((id) => {
          document
            .getElementById(id)
            .addEventListener("input", updateCalculator);
          document
            .getElementById(id)
            .addEventListener("change", updateCalculator);
        });

        document
          .getElementById("offerB_apr_input")
          .addEventListener("input", updateComparison);
        document
          .getElementById("offerB_term_input")
          .addEventListener("input", updateComparison);
        document
          .getElementById("enableComparison")
          .addEventListener("change", updateComparison);

        document
          .getElementById("showAdvanced")
          .addEventListener("change", function () {
            const adv = document.getElementById("advancedOptions");
            if (this.checked) {
              adv.classList.remove("hidden");
            } else {
              adv.classList.add("hidden");
            }
          });

        document
          .getElementById("resetBtn")
          .addEventListener("click", function () {
            document.getElementById("loanAmount").value = 15000;
            document.getElementById("interestRate").value = 12;
            document.getElementById("termYears").value = 5;
            document.getElementById("originationFee").value = 0;
            document.getElementById("extraPayment").value = 0;
            document.getElementById("targetPayment").value = "";
            updateCalculator();
          });

        document
          .getElementById("solveForAmountBtn")
          .addEventListener("click", function () {
            const target = parseFloat(
              document.getElementById("targetPayment").value
            );
            const apr =
              parseFloat(document.getElementById("interestRate").value) || 0;
            const years =
              parseFloat(document.getElementById("termYears").value) || 0;
            if (!target || target <= 0 || apr <= 0 || years <= 0) return;
            const amount = solveLoanAmount(target, apr, years);
            if (amount > 0) {
              document.getElementById("loanAmount").value = Math.round(amount);
              updateCalculator();
            }
          });

        document
          .getElementById("clearTargetBtn")
          .addEventListener("click", function () {
            document.getElementById("targetPayment").value = "";
          });

        document
          .getElementById("scrollToSchedule")
          .addEventListener("click", function () {
            const section = document.getElementById("schedule-heading");
            if (section && section.scrollIntoView) {
              section.scrollIntoView({ behavior: "smooth", block: "start" });
            }
          });

        // Initial calculation
        updateCalculator();
      });

      // MathJax config
      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
