    <script>
      (function () {
        const initialEl = document.getElementById("initial");
        const yearsEl = document.getElementById("years");
        const rateEl = document.getElementById("rate");
        const compoundEl = document.getElementById("compound");
        const contribEl = document.getElementById("contribution");
        const contribFreqEl = document.getElementById("contribution-frequency");
        const inflationEl = document.getElementById("inflation");
        const taxRateEl = document.getElementById("tax-rate");
        const contribTimingEl = document.getElementById("contribution-timing");
        const tableIntervalEl = document.getElementById("table-interval");

        const resultFinalEl = document.getElementById("result-final");
        const resultFinalRealEl = document.getElementById("result-final-real");
        const resultContribEl = document.getElementById("result-contrib");
        const resultInterestEl = document.getElementById("result-interest");
        const resultInterestRealEl = document.getElementById(
          "result-interest-real"
        );
        const resultEarEl = document.getElementById("result-ear");
        const resultNominalEl = document.getElementById("result-nominal");
        const errorMsgEl = document.getElementById("error-msg");
        const breakdownBodyEl = document.getElementById("breakdown-body");

        const tabSimple = document.getElementById("tab-simple");
        const tabAdvanced = document.getElementById("tab-advanced");
        const advancedOptions = document.getElementById("advanced-options");

        const calculateBtn = document.getElementById("calculate-btn");
        const resetBtn = document.getElementById("reset-btn");

        let chart;

        function formatCurrency(value) {
          return value.toLocaleString(undefined, {
            style: "currency",
            currency: "USD",
            maximumFractionDigits: 2,
          });
        }

        function formatPercent(value, digits = 2) {
          return (value * 100).toFixed(digits) + "%";
        }

        function parseInputs() {
          const P = parseFloat(initialEl.value);
          const years = parseFloat(yearsEl.value);
          const rNominal = parseFloat(rateEl.value) / 100;
          const m = parseInt(compoundEl.value, 10);
          const PMT = parseFloat(contribEl.value);
          const k = parseInt(contribFreqEl.value, 10);
          const inflation = parseFloat(inflationEl.value) / 100;
          const taxRate = parseFloat(taxRateEl.value) / 100;
          const timing = contribTimingEl.value;

          if (
            isNaN(P) ||
            isNaN(years) ||
            isNaN(rNominal) ||
            isNaN(PMT) ||
            years < 0 ||
            k <= 0 ||
            P < 0 ||
            PMT < 0
          ) {
            return null;
          }
          if (m < 0) return null;

          return { P, years, rNominal, m, PMT, k, inflation, taxRate, timing };
        }

        function computeSchedule(inputs) {
          const { P, years, rNominal, m, PMT, k, inflation, taxRate, timing } =
            inputs;
          const totalYears = years;
          const totalPeriods = Math.round(totalYears * 12); // for chart granularity (monthly-ish)
          const dt = totalYears / (totalPeriods || 1);

          let balance = P;
          let totalContrib = 0;
          let totalInterest = 0;

          const yearlyRows = [];
          const chartLabels = [];
          const chartBalance = [];
          const chartContrib = [];

          const periodsPerYear = 1 / dt;
          const rPerStep = m === 0 ? null : (rNominal / m) * (m * dt); // approximate per-step rate for discrete
          const rContPerStep = rNominal * dt; // for continuous

          let yearStartBalance = P;
          let yearContrib = 0;
          let yearInterest = 0;
          let currentYear = 0;

          for (let i = 0; i <= totalPeriods; i++) {
            const t = i * dt;
            const yearIndex = Math.floor(t + 1e-9);

            if (i > 0) {
              // contribution at beginning of step?
              if (timing === "begin") {
                const contribThisStep = PMT * (k * dt);
                balance += contribThisStep;
                totalContrib += contribThisStep;
                yearContrib += contribThisStep;
              }

              // interest for this step
              let interestThisStep;
              if (m === 0) {
                // continuous compounding approximation
                interestThisStep = balance * (Math.exp(rContPerStep) - 1);
              } else {
                interestThisStep = balance * rPerStep;
              }

              // apply tax annually on interest
              if (
                taxRate > 0 &&
                Math.abs(t - Math.round(t)) < dt / 2 &&
                t > 0
              ) {
                const taxableInterest = yearInterest;
                const tax = taxableInterest * taxRate;
                balance -= tax;
                yearInterest -= tax;
                totalInterest -= tax;
              }

              balance += interestThisStep;
              totalInterest += interestThisStep;
              yearInterest += interestThisStep;

              // contribution at end of step?
              if (timing === "end") {
                const contribThisStep = PMT * (k * dt);
                balance += contribThisStep;
                totalContrib += contribThisStep;
                yearContrib += contribThisStep;
              }
            }

            // capture chart data
            chartLabels.push(t.toFixed(1));
            chartBalance.push(balance);
            chartContrib.push(P + totalContrib);

            // end of a year or final step
            if (
              (yearIndex > currentYear && yearIndex <= totalYears) ||
              i === totalPeriods
            ) {
              yearlyRows.push({
                year: currentYear + 1,
                startBalance: yearStartBalance,
                contributions: yearContrib,
                interest: yearInterest,
                endBalance: balance,
              });
              currentYear = yearIndex;
              yearStartBalance = balance;
              yearContrib = 0;
              yearInterest = 0;
            }
          }

          return {
            finalBalance: balance,
            totalContrib,
            totalInterest,
            yearlyRows,
            chartLabels,
            chartBalance,
            chartContrib,
          };
        }

        function updateResults() {
          const inputs = parseInputs();
          if (!inputs) {
            errorMsgEl.classList.remove("hidden");
            return;
          }
          errorMsgEl.classList.add("hidden");

          const { P, years, rNominal, m, inflation } = inputs;
          const schedule = computeSchedule(inputs);

          const {
            finalBalance,
            totalContrib,
            totalInterest,
            yearlyRows,
            chartLabels,
            chartBalance,
            chartContrib,
          } = schedule;

          resultFinalEl.textContent = formatCurrency(finalBalance);
          resultContribEl.textContent = formatCurrency(P + totalContrib);
          resultInterestEl.textContent = formatCurrency(
            finalBalance - (P + totalContrib)
          );

          // Effective annual rate
          let ear;
          if (m === 0) {
            ear = Math.exp(rNominal) - 1;
          } else if (m > 0) {
            ear = Math.pow(1 + rNominal / m, m) - 1;
          } else {
            ear = rNominal;
          }
          resultEarEl.textContent = formatPercent(ear);
          resultNominalEl.textContent = `Nominal rate: ${(
            rNominal * 100
          ).toFixed(2)}%`;

          // Real (inflation-adjusted) values
          if (!isNaN(inflation) && inflation !== 0 && years > 0) {
            const inflationFactor = Math.pow(1 + inflation, years);
            const finalReal = finalBalance / inflationFactor;
            const interestReal =
              (finalBalance - (P + totalContrib)) / inflationFactor;
            resultFinalRealEl.textContent = `≈ ${formatCurrency(
              finalReal
            )} in today's dollars`;
            resultInterestRealEl.textContent = `≈ ${formatCurrency(
              interestReal
            )} real interest`;
            resultFinalRealEl.classList.remove("hidden");
            resultInterestRealEl.classList.remove("hidden");
          } else {
            resultFinalRealEl.classList.add("hidden");
            resultInterestRealEl.classList.add("hidden");
          }

          // Update table
          const interval = parseInt(tableIntervalEl.value, 10) || 1;
          breakdownBodyEl.innerHTML = "";
          yearlyRows.forEach((row) => {
            if (
              row.year % interval !== 0 &&
              row.year !== yearlyRows[yearlyRows.length - 1].year
            )
              return;
            const tr = document.createElement("tr");
            tr.innerHTML = `
        <td class="px-3 py-2 text-left text-gray-700">${row.year}</td>
        <td class="px-3 py-2 text-right text-gray-700">${formatCurrency(
          row.startBalance
        )}</td>
        <td class="px-3 py-2 text-right text-gray-700">${formatCurrency(
          row.contributions
        )}</td>
        <td class="px-3 py-2 text-right text-gray-700">${formatCurrency(
          row.interest
        )}</td>
        <td class="px-3 py-2 text-right text-gray-900 font-medium">${formatCurrency(
          row.endBalance
        )}</td>
      `;
            breakdownBodyEl.appendChild(tr);
          });

          // Update chart
          const ctx = document.getElementById("growth-chart").getContext("2d");
          if (chart) chart.destroy();
          chart = new Chart(ctx, {
            type: "line",
            data: {
              labels: chartLabels,
              datasets: [
                {
                  label: "Balance",
                  data: chartBalance,
                  borderColor: "#2563eb",
                  backgroundColor: "rgba(37, 99, 235, 0.08)",
                  tension: 0.15,
                  fill: true,
                  pointRadius: 0,
                },
                {
                  label: "Contributions",
                  data: chartContrib,
                  borderColor: "#16a34a",
                  backgroundColor: "rgba(22, 163, 74, 0.05)",
                  tension: 0.15,
                  fill: false,
                  borderDash: [4, 4],
                  pointRadius: 0,
                },
              ],
            },
            options: {
              responsive: true,
              maintainAspectRatio: false,
              interaction: { mode: "index", intersect: false },
              plugins: {
                legend: { display: false },
                tooltip: {
                  callbacks: {
                    label: function (context) {
                      const label = context.dataset.label || "";
                      return label + ": " + formatCurrency(context.parsed.y);
                    },
                  },
                },
              },
              scales: {
                x: {
                  title: { display: true, text: "Years" },
                  ticks: {
                    callback: function (value, index) {
                      const t = parseFloat(this.getLabelForValue(value));
                      return Number.isInteger(t) ? t : "";
                    },
                  },
                },
                y: {
                  title: { display: true, text: "Balance ($)" },
                  ticks: {
                    callback: function (value) {
                      return value >= 1000 ? value / 1000 + "k" : value;
                    },
                  },
                },
              },
            },
          });
        }

        // Tabs
        tabSimple.addEventListener("click", () => {
          tabSimple.classList.add("tab-active");
          tabSimple.classList.remove("tab-inactive");
          tabAdvanced.classList.add("tab-inactive");
          tabAdvanced.classList.remove("tab-active");
          advancedOptions.classList.add("hidden");
        });

        tabAdvanced.addEventListener("click", () => {
          tabAdvanced.classList.add("tab-active");
          tabAdvanced.classList.remove("tab-inactive");
          tabSimple.classList.add("tab-inactive");
          tabSimple.classList.remove("tab-active");
          advancedOptions.classList.remove("hidden");
        });

        // Events
        calculateBtn.addEventListener("click", updateResults);
        resetBtn.addEventListener("click", () => {
          initialEl.value = 10000;
          yearsEl.value = 20;
          rateEl.value = 7;
          compoundEl.value = "12";
          contribEl.value = 200;
          contribFreqEl.value = "12";
          inflationEl.value = 2;
          taxRateEl.value = 0;
          contribTimingEl.value = "end";
          tableIntervalEl.value = "1";
          updateResults();
        });

        [
          initialEl,
          yearsEl,
          rateEl,
          compoundEl,
          contribEl,
          contribFreqEl,
          inflationEl,
          taxRateEl,
          contribTimingEl,
          tableIntervalEl,
        ].forEach((el) => {
          el.addEventListener("change", updateResults);
          el.addEventListener("input", () => {
            if (
              el === rateEl ||
              el === yearsEl ||
              el === initialEl ||
              el === contribEl
            ) {
              // debounce-like quick update
              clearTimeout(el._ciTimeout);
              el._ciTimeout = setTimeout(updateResults, 400);
            }
          });
        });

        // Initial calculation
        updateResults();
      })();
    </script>
  
