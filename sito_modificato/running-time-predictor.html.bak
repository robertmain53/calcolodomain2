<script>
  // Utility functions
  function hmsToSeconds(h, m, s) {
    return h * 3600 + m * 60 + s;
  }
  function secondsToHMS(sec) {
    sec = Math.round(sec);
    const h = Math.floor(sec / 3600);
    const m = Math.floor((sec % 3600) / 60);
    const s = sec % 60;
    const pad = (n) => n.toString().padStart(2, '0');
    if (h > 0) return h + ':' + pad(m) + ':' + pad(s);
    return m + ':' + pad(s);
  }
  function paceFromTime(distanceKm, timeSec, per) {
    if (distanceKm <= 0 || timeSec <= 0) return '-';
    const dist = per === 'km' ? distanceKm : distanceKm / 1.609344;
    const paceSec = timeSec / dist;
    return secondsToHMS(paceSec);
  }
  function speedFromTime(distanceKm, timeSec) {
    if (distanceKm <= 0 || timeSec <= 0) return '-';
    const hours = timeSec / 3600;
    const kmh = distanceKm / hours;
    return kmh.toFixed(2) + ' km/h';
  }

  // Simple VO2-based model: approximate VO2 from speed, then invert
  function vo2FromSpeed(v) {
    // v in m/s, formula similar to Daniels' running formula
    return -4.6 + 0.182258 * v + 0.000104 * v * v;
  }
  function vo2CostFromSpeed(v) {
    // oxygen cost vs speed
    return 0.8 + 0.1894393 * Math.exp(-0.012778 * v) + 0.2989558 * Math.exp(-0.1932605 * v);
  }
  function predictTimeVO2(d1Km, t1Sec, d2Km) {
    const v1 = (d1Km * 1000) / t1Sec;
    const vo2max = vo2FromSpeed(v1) / vo2CostFromSpeed(v1);
    // find v2 such that vo2FromSpeed(v2)/vo2CostFromSpeed(v2) ~ vo2max
    // simple numeric search
    let vLow = 1.0, vHigh = 8.0;
    for (let i = 0; i < 40; i++) {
      const vmid = (vLow + vHigh) / 2;
      const eff = vo2FromSpeed(vmid) / vo2CostFromSpeed(vmid);
      if (eff > vo2max) vHigh = vmid; else vLow = vmid;
    }
    const v2 = (vLow + vHigh) / 2;
    const t2 = (d2Km * 1000) / v2;
    return t2;
  }

  document.addEventListener('DOMContentLoaded', function () {
    const inputDistance = document.getElementById('input-distance');
    const customDistanceWrapper = document.getElementById('custom-distance-wrapper');
    const customDistanceValue = document.getElementById('custom-distance-value');
    const customDistanceUnit = document.getElementById('custom-distance-unit');
    const timeH = document.getElementById('time-h');
    const timeM = document.getElementById('time-m');
    const timeS = document.getElementById('time-s');
    const modelTabs = document.querySelectorAll('.model-tab');
    const riegelExponentInput = document.getElementById('riegel-exponent');
    const predictBtn = document.getElementById('predict-btn');
    const inputError = document.getElementById('input-error');
    const resultsBody = document.getElementById('results-body');
    const resultsSummary = document.getElementById('results-summary');
    const targetCheckboxes = document.querySelectorAll('.target-distance');
    const addCustomTargetBtn = document.getElementById('add-custom-target');
    const customTargetDistance = document.getElementById('custom-target-distance');
    const customTargetUnit = document.getElementById('custom-target-unit');
    const customTargetList = document.getElementById('custom-target-list');

    let currentModel = 'blended';
    let customTargets = [];

    inputDistance.addEventListener('change', () => {
      if (inputDistance.value === 'custom') {
        customDistanceWrapper.classList.remove('hidden');
      } else {
        customDistanceWrapper.classList.add('hidden');
      }
    });

    modelTabs.forEach(tab => {
      tab.addEventListener('click', () => {
        modelTabs.forEach(t => t.classList.remove('tab-active'));
        tab.classList.add('tab-active');
        currentModel = tab.getAttribute('data-model');
      });
    });

    addCustomTargetBtn.addEventListener('click', () => {
      const val = parseFloat(customTargetDistance.value);
      if (!val || val <= 0) return;
      const unit = customTargetUnit.value;
      const km = unit === 'km' ? val : val * 1.609344;
      customTargets.push({ km, label: val + ' ' + unit });
      const span = document.createElement('span');
      span.className = 'inline-block mr-2 mb-1 px-2 py-0.5 bg-gray-100 rounded-full';
      span.textContent = val + ' ' + unit;
      customTargetList.appendChild(span);
      customTargetDistance.value = '';
    });

    function getInputDistanceKm() {
      let val = inputDistance.value;
      if (val === 'custom') {
        const d = parseFloat(customDistanceValue.value);
        if (!d || d <= 0) return null;
        return customDistanceUnit.value === 'km' ? d : d * 1.609344;
      }
      return parseFloat(val);
    }

    function getTargetDistances() {
      const list = [];
      targetCheckboxes.forEach(cb => {
        if (cb.checked) list.push({ km: parseFloat(cb.value), label: cb.nextElementSibling.textContent.trim() });
      });
      customTargets.forEach(ct => list.push(ct));
      return list;
    }

    predictBtn.addEventListener('click', () => {
      inputError.classList.add('hidden');
      resultsSummary.classList.add('hidden');
      const d1 = getInputDistanceKm();
      if (!d1 || d1 <= 0) {
        inputError.textContent = 'Please enter a valid race distance.';
        inputError.classList.remove('hidden');
        return;
      }
      const h = parseInt(timeH.value || '0', 10);
      const m = parseInt(timeM.value || '0', 10);
      const s = parseInt(timeS.value || '0', 10);
      const t1 = hmsToSeconds(h, m, s);
      if (!t1 || t1 <= 0) {
        inputError.textContent = 'Please enter a valid finish time.';
        inputError.classList.remove('hidden');
        return;
      }
      const exponent = parseFloat(riegelExponentInput.value || '1.06');
      const targets = getTargetDistances();
      if (targets.length === 0) {
        inputError.textContent = 'Please select at least one target distance.';
        inputError.classList.remove('hidden');
        return;
      }

      resultsBody.innerHTML = '';
      targets.forEach(target => {
        const d2 = target.km;
        const ratio = d2 / d1;
        let tRiegel = t1 * Math.pow(ratio, exponent);
        let tVO2;
        try {
          tVO2 = predictTimeVO2(d1, t1, d2);
        } catch (e) {
          tVO2 = tRiegel;
        }
        let tBlend = (tRiegel + tVO2) / 2;

        let tFinal, modelLabel;
        if (currentModel === 'riegel') {
          tFinal = tRiegel;
          modelLabel = 'Riegel';
        } else if (currentModel === 'vo2') {
          tFinal = tVO2;
          modelLabel = 'VO₂-based';
        } else {
          tFinal = tBlend;
          modelLabel = 'Blended';
        }

        const tr = document.createElement('tr');
        tr.className = 'border-t';
        tr.innerHTML = `
          <td class="px-3 py-2 whitespace-nowrap">${target.label}</td>
          <td class="px-3 py-2 whitespace-nowrap">${secondsToHMS(tFinal)}</td>
          <td class="px-3 py-2 whitespace-nowrap">${paceFromTime(d2, tFinal, 'km')}</td>
          <td class="px-3 py-2 whitespace-nowrap">${paceFromTime(d2, tFinal, 'mi')}</td>
          <td class="px-3 py-2 whitespace-nowrap">${speedFromTime(d2, tFinal)}</td>
          <td class="px-3 py-2 whitespace-nowrap text-xs text-gray-600">${modelLabel}</td>
        `;
        resultsBody.appendChild(tr);
      });

      resultsSummary.textContent = `Predictions based on ${d1.toFixed(2)} km in ${secondsToHMS(t1)} using the ${currentModel === 'blended' ? 'blended Riegel + VO₂' : currentModel} model.`;
      resultsSummary.classList.remove('hidden');
    });

    // Pacing planner
    const paceDistance = document.getElementById('pace-distance');
    const paceUnit = document.getElementById('pace-unit');
    const paceH = document.getElementById('pace-h');
    const paceM = document.getElementById('pace-m');
    const paceS = document.getElementById('pace-s');
    const paceBtn = document.getElementById('pace-btn');
    const paceError = document.getElementById('pace-error');
    const paceOutput = document.getElementById('pace-output');

    paceBtn.addEventListener('click', () => {
      paceError.classList.add('hidden');
      paceOutput.classList.add('hidden');
      const d = parseFloat(paceDistance.value);
      if (!d || d <= 0) {
        paceError.textContent = 'Please enter a valid race distance.';
        paceError.classList.remove('hidden');
        return;
      }
      const unit = paceUnit.value;
      const h = parseInt(paceH.value || '0', 10);
      const m = parseInt(paceM.value || '0', 10);
      const s = parseInt(paceS.value || '0', 10);
      const t = hmsToSeconds(h, m, s);
      if (!t || t <= 0) {
        paceError.textContent = 'Please enter a valid target time.';
        paceError.classList.remove('hidden');
        return;
      }
      const km = unit === 'km' ? d : d * 1.609344;
      const pacePerKm = paceFromTime(km, t, 'km');
      const pacePerMi = paceFromTime(km, t, 'mi');
      const speed = speedFromTime(km, t);
      paceOutput.innerHTML = `
        <p><strong>Average pace:</strong> ${pacePerKm} per km (${pacePerMi} per mile)</p>
        <p><strong>Average speed:</strong> ${speed}</p>
      `;
      paceOutput.classList.remove('hidden');
    });
  });

  window.MathJax = {
    tex: { inlineMath: [['\\(','\\)'], ['$', '$']], displayMath: [['$','$'], ['\\[','\\]']] },
    svg: { fontCache: 'global' }
  };
</script>
<script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>

