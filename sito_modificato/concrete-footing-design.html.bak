    <script>
      // Utility helpers
      function kipsToPounds(kips) {
        return kips * 1000;
      }
      function poundsToKips(lb) {
        return lb / 1000;
      }

      function formatUtilization(u) {
        if (!isFinite(u) || u <= 0) return "—";
        return (u * 100).toFixed(0) + "%";
      }

      function statusBadge(u) {
        if (!isFinite(u) || u <= 0) {
          return '<span class="badge bg-gray-100 text-gray-700 border border-gray-200">n/a</span>';
        }
        if (u <= 0.8) {
          return '<span class="badge bg-green-100 text-green-700 border border-green-200">OK</span>';
        } else if (u <= 1.0) {
          return '<span class="badge bg-yellow-100 text-yellow-700 border border-yellow-200">High</span>';
        }
        return '<span class="badge bg-red-100 text-red-700 border border-red-200">NG</span>';
      }

      function runFootingDesign() {
        const Pu = parseFloat(document.getElementById("Pu").value) || 0; // kips
        const MuX = parseFloat(document.getElementById("MuX").value) || 0; // kip-ft
        const MuY = parseFloat(document.getElementById("MuY").value) || 0; // kip-ft
        const gammaC =
          parseFloat(document.getElementById("gammaConc").value) || 150; // pcf

        const colWidth =
          parseFloat(document.getElementById("colWidth").value) || 16; // in
        const colDepth =
          parseFloat(document.getElementById("colDepth").value) || 16; // in

        const qa = parseFloat(document.getElementById("qa").value) || 4; // ksf
        const fc = parseFloat(document.getElementById("fc").value) || 4000; // psi
        const fy = parseFloat(document.getElementById("fy").value) || 60; // ksi

        const phiFlex =
          parseFloat(document.getElementById("phiFlexure").value) || 0.9;
        const phiShear =
          parseFloat(document.getElementById("phiShear").value) || 0.75;
        const phiPunch =
          parseFloat(document.getElementById("phiPunch").value) || 0.75;

        const Bx = parseFloat(document.getElementById("Bx").value) || 8; // ft
        const By = parseFloat(document.getElementById("By").value) || 8; // ft
        const h = parseFloat(document.getElementById("thickness").value) || 24; // in
        const cover = parseFloat(document.getElementById("cover").value) || 3; // in
        const barDiam =
          parseFloat(document.getElementById("barDiam").value) || 0.75; // in

        const statusEl = document.getElementById("statusMessage");
        const resultsEl = document.getElementById("resultsContainer");

        if (Pu <= 0 || qa <= 0 || Bx <= 0 || By <= 0 || h <= 0) {
          statusEl.textContent =
            "Please enter positive values for load, soil bearing, and footing dimensions.";
          statusEl.classList.remove("text-green-700");
          statusEl.classList.add("text-red-700");
          return;
        }

        statusEl.textContent = "Design calculated (simplified ACI 318 checks).";
        statusEl.classList.remove("text-red-700");
        statusEl.classList.add("text-green-700");

        // Geometry & self-weight
        const Aft = Bx * By; // ft^2
        const volFt3 = Bx * By * (h / 12); // ft^3
        const Wf_lb = volFt3 * gammaC;
        const Wf_k = poundsToKips(Wf_lb);

        // Convert Pu (factored) to service Ps using a typical factor ~1.6
        const gammaLoad = 1.6;
        const Ps = Pu / gammaLoad; // kips

        const qService = (Ps + Wf_k) / Aft; // ksf
        const bearingUtil = qService / qa;

        // Assume uniform factored pressure from Pu + Wf
        const qFactored = (Pu + 1.2 * Wf_k) / Aft; // ksf (approx)

        // Effective depth
        const d = h - cover - barDiam / 2; // in

        // Flexure in x-direction (strip width = 1 ft)
        // For interior footing with uniform pressure, max moment at column face approx:
        // M = q * Bx^2 / 8 per unit width (simplified)
        const qx_kipft2 = qFactored; // ksf
        const Mux_strip = (qx_kipft2 * Bx * Bx) / 8; // kip-ft per ft width
        const Mu_x = Mux_strip; // kip-ft

        // Required As in x-direction (per ft width)
        const jd = 0.9 * d; // in
        const Mu_x_inlb = Mu_x * 12 * 1000; // kip-ft -> in-lb
        const fy_psi = fy * 1000;
        const Asx = Mu_x_inlb / (phiFlex * fy_psi * jd); // in^2 per ft

        // Flexure in y-direction
        const qy_kipft2 = qFactored;
        const Muy_strip = (qy_kipft2 * By * By) / 8;
        const Mu_y = Muy_strip;
        const Mu_y_inlb = Mu_y * 12 * 1000;
        const Asy = Mu_y_inlb / (phiFlex * fy_psi * jd); // in^2 per ft

        // One-way shear in x-direction (critical section at d from column face)
        const d_ft = d / 12;
        const colWidth_ft = colWidth / 12;
        const shearSpanX = Bx / 2 - colWidth_ft / 2 - d_ft;
        const Vu_x = qFactored * shearSpanX * By; // kips (strip width By)
        const b_shear_x = By * 12; // in
        const Vc_x = (2 * Math.sqrt(fc) * b_shear_x * d) / 1000; // kips (2*sqrt(fc)*b*d /1000)
        const shearUtilX = Vu_x / (phiShear * Vc_x);

        // One-way shear in y-direction
        const colDepth_ft = colDepth / 12;
        const shearSpanY = By / 2 - colDepth_ft / 2 - d_ft;
        const Vu_y = qFactored * shearSpanY * Bx; // kips
        const b_shear_y = Bx * 12; // in
        const Vc_y = (2 * Math.sqrt(fc) * b_shear_y * d) / 1000; // kips
        const shearUtilY = Vu_y / (phiShear * Vc_y);

        // Punching shear (interior column, critical perimeter at d/2)
        const bx_in = colWidth;
        const by_in = colDepth;
        const bo = 2 * (bx_in + 2 * (d / 2)) + 2 * (by_in + 2 * (d / 2)); // in
        // Approximate punching shear demand as reaction within perimeter:
        const areaWithinPerim_ft2 = ((bx_in + d) / 12) * ((by_in + d) / 12);
        const Vu_punch = qFactored * areaWithinPerim_ft2 - Pu; // kips
        const vu = (Vu_punch * 1000) / (bo * d); // psi
        const vc = 4 * Math.sqrt(fc); // psi, interior column
        const punchUtil = vu / (phiPunch * vc);

        // Bar spacing suggestion (per ft width)
        function spacingFromAs(As_per_ft, barDia) {
          if (As_per_ft <= 0) return null;
          const areaBar = (Math.PI * Math.pow(barDia, 2)) / 4; // in^2
          const s_in = (areaBar * 12) / As_per_ft; // in
          return s_in;
        }

        const sx = spacingFromAs(Asx, barDiam);
        const sy = spacingFromAs(Asy, barDiam);

        const overallOK = [
          bearingUtil,
          shearUtilX,
          shearUtilY,
          punchUtil,
        ].every((u) => u > 0 && u <= 1.0);

        let html = "";

        html += '<div class="border rounded-md p-4 bg-gray-50">';
        html += '<div class="flex items-center justify-between mb-2">';
        html += '<h3 class="font-semibold">Overall status</h3>';
        html += overallOK
          ? '<span class="badge bg-green-100 text-green-700 border border-green-200">All major checks ≤ 100%</span>'
          : '<span class="badge bg-red-100 text-red-700 border border-red-200">One or more checks &gt; 100%</span>';
        html += "</div>";
        html +=
          '<p class="text-xs text-gray-600">Utilization &gt; 100% indicates the footing is overstressed for that limit state and should be resized.</p>';
        html += "</div>";

        // Bearing
        html += '<div class="mt-4 grid md:grid-cols-2 gap-4">';
        html += '<div class="border rounded-md p-4">';
        html += '<h4 class="font-semibold mb-2">Soil bearing check</h4>';
        html += '<table class="w-full text-xs table-mini">';
        html +=
          '<tr><th class="text-left">Item</th><th class="text-right">Value</th></tr>';
        html += `<tr><td>Footing area A</td><td class="text-right">${Aft.toFixed(
          2
        )} ft²</td></tr>`;
        html += `<tr><td>Footing self-weight W<sub>f</sub></td><td class="text-right">${Wf_k.toFixed(
          1
        )} kips</td></tr>`;
        html += `<tr><td>Service load P<sub>s</sub></td><td class="text-right">${Ps.toFixed(
          1
        )} kips</td></tr>`;
        html += `<tr><td>Service pressure q<sub>s</sub></td><td class="text-right">${qService.toFixed(
          2
        )} ksf</td></tr>`;
        html += `<tr><td>Allowable q<sub>a</sub></td><td class="text-right">${qa.toFixed(
          2
        )} ksf</td></tr>`;
        html += `<tr><td>Utilization</td><td class="text-right">${formatUtilization(
          bearingUtil
        )} ${statusBadge(bearingUtil)}</td></tr>`;
        html += "</table>";
        html += "</div>";

        // Flexure & steel
        html += '<div class="border rounded-md p-4">';
        html +=
          '<h4 class="font-semibold mb-2">Flexure &amp; reinforcement</h4>';
        html += '<table class="w-full text-xs table-mini">';
        html +=
          '<tr><th class="text-left">Direction</th><th class="text-right">M<sub>u</sub> (kip-ft)</th><th class="text-right">A<sub>s,req</sub> (in²/ft)</th><th class="text-right">Bar spacing (in)</th></tr>';
        html += `<tr><td>X (long)</td><td class="text-right">${Mu_x.toFixed(
          1
        )}</td><td class="text-right">${Asx.toFixed(
          3
        )}</td><td class="text-right">${sx ? sx.toFixed(1) : "—"}</td></tr>`;
        html += `<tr><td>Y (short)</td><td class="text-right">${Mu_y.toFixed(
          1
        )}</td><td class="text-right">${Asy.toFixed(
          3
        )}</td><td class="text-right">${sy ? sy.toFixed(1) : "—"}</td></tr>`;
        html += "</table>";
        html +=
          '<p class="mt-2 text-xs text-gray-600">Bar spacing is indicative only. Check minimum steel, maximum spacing, and development length per ACI 318.</p>';
        html += "</div>";
        html += "</div>";

        // Shear checks
        html += '<div class="mt-4 grid md:grid-cols-2 gap-4">';
        html += '<div class="border rounded-md p-4">';
        html += '<h4 class="font-semibold mb-2">One-way shear</h4>';
        html += '<table class="w-full text-xs table-mini">';
        html +=
          '<tr><th class="text-left">Direction</th><th class="text-right">V<sub>u</sub> (k)</th><th class="text-right">ϕV<sub>c</sub> (k)</th><th class="text-right">Utilization</th></tr>';
        html += `<tr><td>X</td><td class="text-right">${Vu_x.toFixed(
          1
        )}</td><td class="text-right">${(phiShear * Vc_x).toFixed(
          1
        )}</td><td class="text-right">${formatUtilization(
          shearUtilX
        )} ${statusBadge(shearUtilX)}</td></tr>`;
        html += `<tr><td>Y</td><td class="text-right">${Vu_y.toFixed(
          1
        )}</td><td class="text-right">${(phiShear * Vc_y).toFixed(
          1
        )}</td><td class="text-right">${formatUtilization(
          shearUtilY
        )} ${statusBadge(shearUtilY)}</td></tr>`;
        html += "</table>";
        html += "</div>";

        // Punching
        html += '<div class="border rounded-md p-4">';
        html += '<h4 class="font-semibold mb-2">Punching shear</h4>';
        html += '<table class="w-full text-xs table-mini">';
        html +=
          '<tr><th class="text-left">Item</th><th class="text-right">Value</th></tr>';
        html += `<tr><td>Critical perimeter b<sub>0</sub></td><td class="text-right">${bo.toFixed(
          1
        )} in</td></tr>`;
        html += `<tr><td>V<sub>u</sub> (punch)</td><td class="text-right">${Vu_punch.toFixed(
          1
        )} k</td></tr>`;
        html += `<tr><td>v<sub>u</sub></td><td class="text-right">${vu.toFixed(
          1
        )} psi</td></tr>`;
        html += `<tr><td>ϕv<sub>c</sub></td><td class="text-right">${(
          phiPunch * vc
        ).toFixed(1)} psi</td></tr>`;
        html += `<tr><td>Utilization</td><td class="text-right">${formatUtilization(
          punchUtil
        )} ${statusBadge(punchUtil)}</td></tr>`;
        html += "</table>";
        html +=
          '<p class="mt-2 text-xs text-gray-600">Punching shear is evaluated for an interior column with no shear reinforcement. Edge or corner columns require modified perimeters and capacities.</p>';
        html += "</div>";
        html += "</div>";

        resultsEl.innerHTML = html;
      }

      document
        .getElementById("runDesign")
        .addEventListener("click", runFootingDesign);

      document
        .getElementById("resetForm")
        .addEventListener("click", function () {
          document.getElementById("Pu").value = 400;
          document.getElementById("MuX").value = 0;
          document.getElementById("MuY").value = 0;
          document.getElementById("gammaConc").value = 150;
          document.getElementById("colWidth").value = 16;
          document.getElementById("colDepth").value = 16;
          document.getElementById("qa").value = 4;
          document.getElementById("fc").value = 4000;
          document.getElementById("fy").value = 60;
          document.getElementById("phiFlexure").value = 0.9;
          document.getElementById("phiShear").value = 0.75;
          document.getElementById("phiPunch").value = 0.75;
          document.getElementById("Bx").value = 8;
          document.getElementById("By").value = 8;
          document.getElementById("thickness").value = 24;
          document.getElementById("cover").value = 3;
          document.getElementById("barDiam").value = 0.75;
          document.getElementById("footingType").value = "square";
          document.getElementById("resultsContainer").innerHTML =
            '<p class="text-gray-600">Enter your data and click “Run Footing Design” to see bearing, shear, and reinforcement checks.</p>';
          document.getElementById("statusMessage").textContent = "";
        });

      // Keep By equal to Bx for square footing
      document
        .getElementById("footingType")
        .addEventListener("change", function () {
          if (this.value === "square") {
            const Bx = document.getElementById("Bx").value;
            document.getElementById("By").value = Bx;
          }
        });
      document.getElementById("Bx").addEventListener("input", function () {
        if (document.getElementById("footingType").value === "square") {
          document.getElementById("By").value = this.value;
        }
      });
    </script>

