    <script>
      // RRSP calculator logic
      (function () {
        const incomeEl = document.getElementById("income");
        const unusedRoomEl = document.getElementById("unusedRoom");
        const pensionAdjEl = document.getElementById("pensionAdj");
        const marginalTaxEl = document.getElementById("marginalTax");
        const contributionEl = document.getElementById("contribution");
        const yearsToRetireEl = document.getElementById("yearsToRetire");
        const returnRateEl = document.getElementById("returnRate");
        const retirementTaxEl = document.getElementById("retirementTax");

        const newRoomDisplay = document.getElementById("newRoomDisplay");
        const totalRoomDisplay = document.getElementById("totalRoomDisplay");
        const contributionDisplay = document.getElementById(
          "contributionDisplay"
        );
        const remainingRoomDisplay = document.getElementById(
          "remainingRoomDisplay"
        );
        const overcontributionWarning = document.getElementById(
          "overcontributionWarning"
        );

        const taxSavingsDisplay = document.getElementById("taxSavingsDisplay");
        const afterTaxCostDisplay = document.getElementById(
          "afterTaxCostDisplay"
        );

        const rrspFutureDisplay = document.getElementById("rrspFutureDisplay");
        const rrspAfterTaxDisplay = document.getElementById(
          "rrspAfterTaxDisplay"
        );
        const tfsaFutureDisplay = document.getElementById("tfsaFutureDisplay");
        const rrspVsTfsaNote = document.getElementById("rrspVsTfsaNote");

        const maxAllowedBtn = document.getElementById("maxAllowedBtn");
        const resetBtn = document.getElementById("resetBtn");

        // Approximate current CRA annual dollar limit (update as needed)
        const APPROX_ANNUAL_LIMIT = 31000;

        function parseNum(el) {
          const v = parseFloat(el.value.replace(",", "."));
          return isNaN(v) ? 0 : v;
        }

        function formatCurrency(value) {
          return value.toLocaleString("en-CA", {
            style: "currency",
            currency: "CAD",
            maximumFractionDigits: 0,
          });
        }

        function formatCurrency2(value) {
          return value.toLocaleString("en-CA", {
            style: "currency",
            currency: "CAD",
            maximumFractionDigits: 2,
          });
        }

        function recalc() {
          const income = parseNum(incomeEl);
          const unusedRoom = parseNum(unusedRoomEl);
          const pensionAdj = parseNum(pensionAdjEl);
          const marginalTax = parseNum(marginalTaxEl);
          const contribution = parseNum(contributionEl);
          const years = parseNum(yearsToRetireEl);
          const returnRate = parseNum(returnRateEl);
          const retirementTax = parseNum(retirementTaxEl);

          // 1) RRSP room
          let newRoom = 0;
          if (income > 0) {
            newRoom = 0.18 * income;
            if (newRoom > APPROX_ANNUAL_LIMIT) newRoom = APPROX_ANNUAL_LIMIT;
          }
          newRoom = Math.max(0, newRoom - pensionAdj);
          const totalRoom = Math.max(0, newRoom + unusedRoom);
          const remainingRoom = totalRoom - contribution;

          newRoomDisplay.textContent = formatCurrency(newRoom);
          totalRoomDisplay.textContent = formatCurrency(totalRoom);
          contributionDisplay.textContent = formatCurrency(contribution);
          remainingRoomDisplay.textContent = formatCurrency(
            Math.max(0, remainingRoom)
          );

          if (contribution > totalRoom + 2000) {
            overcontributionWarning.classList.remove("hidden");
          } else {
            overcontributionWarning.classList.add("hidden");
          }

          // 2) Tax savings
          const taxSavings = contribution * (marginalTax / 100);
          const afterTaxCost = contribution - taxSavings;

          taxSavingsDisplay.textContent = formatCurrency2(taxSavings);
          afterTaxCostDisplay.textContent = formatCurrency2(afterTaxCost);

          // 3) Future values
          let rrspFuture = contribution;
          let tfsaFuture = contribution;
          if (years > 0 && returnRate !== 0) {
            const growthFactor = Math.pow(1 + returnRate / 100, years);
            rrspFuture *= growthFactor;
            tfsaFuture *= growthFactor;
          }

          const rrspAfterTax = rrspFuture * (1 - retirementTax / 100);

          rrspFutureDisplay.textContent = formatCurrency2(rrspFuture);
          rrspAfterTaxDisplay.textContent = formatCurrency2(rrspAfterTax);
          tfsaFutureDisplay.textContent = formatCurrency2(tfsaFuture);

          // 4) RRSP vs TFSA note
          rrspVsTfsaNote.textContent = "";
          if (rrspAfterTax > 0 && tfsaFuture > 0) {
            const diff = rrspAfterTax - tfsaFuture;
            if (Math.abs(diff) < 1) {
              rrspVsTfsaNote.textContent =
                "Under these assumptions, RRSP and TFSA end up with almost the same after-tax value.";
            } else if (diff > 0) {
              rrspVsTfsaNote.textContent = `Under these assumptions, RRSP leaves you with about ${formatCurrency2(
                diff
              )} more after tax than a TFSA for the same contribution.`;
            } else {
              rrspVsTfsaNote.textContent = `Under these assumptions, a TFSA leaves you with about ${formatCurrency2(
                -diff
              )} more after tax than an RRSP for the same contribution.`;
            }
          }
        }

        // Auto-fill max allowed contribution
        maxAllowedBtn.addEventListener("click", function () {
          const income = parseNum(incomeEl);
          const unusedRoom = parseNum(unusedRoomEl);
          const pensionAdj = parseNum(pensionAdjEl);
          let newRoom = 0;
          if (income > 0) {
            newRoom = 0.18 * income;
            if (newRoom > APPROX_ANNUAL_LIMIT) newRoom = APPROX_ANNUAL_LIMIT;
          }
          newRoom = Math.max(0, newRoom - pensionAdj);
          const totalRoom = Math.max(0, newRoom + unusedRoom);
          contributionEl.value = totalRoom.toFixed(0);
          recalc();
        });

        // Reset
        resetBtn.addEventListener("click", function () {
          incomeEl.value = "";
          unusedRoomEl.value = "";
          pensionAdjEl.value = "";
          marginalTaxEl.value = "";
          contributionEl.value = "";
          yearsToRetireEl.value = "";
          returnRateEl.value = "";
          retirementTaxEl.value = "";
          recalc();
        });

        // Attach listeners
        [
          incomeEl,
          unusedRoomEl,
          pensionAdjEl,
          marginalTaxEl,
          contributionEl,
          yearsToRetireEl,
          returnRateEl,
          retirementTaxEl,
        ].forEach((el) => {
          el.addEventListener("input", recalc);
        });

        // Sensible defaults for first load
        incomeEl.value = "80000";
        unusedRoomEl.value = "15000";
        pensionAdjEl.value = "0";
        marginalTaxEl.value = "32";
        contributionEl.value = "10000";
        yearsToRetireEl.value = "25";
        returnRateEl.value = "5";
        retirementTaxEl.value = "25";
        recalc();
      })();
    </script>

