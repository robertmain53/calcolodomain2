    <script>
      // Utility
      function toNumber(id) {
        const v = parseFloat(document.getElementById(id).value);
        return isNaN(v) ? 0 : v;
      }
      function fmt(n) {
        return n.toLocaleString(undefined, {
          minimumFractionDigits: 2,
          maximumFractionDigits: 2,
        });
      }

      function updatePayTypeVisibility() {
        const type = document.getElementById("pay-type").value;
        const salaryGroup = document.getElementById("salary-group");
        const hourlyGroup = document.getElementById("hourly-group");
        if (type === "salary") {
          salaryGroup.classList.remove("hidden");
          hourlyGroup.classList.add("hidden");
        } else {
          salaryGroup.classList.remove("hidden");
          hourlyGroup.classList.remove("hidden");
        }
      }

      function calculatePaycheck() {
        const payType = document.getElementById("pay-type").value;
        const periodsPerYear =
          parseFloat(document.getElementById("pay-frequency").value) || 26;
        const filingStatus = document.getElementById("filing-status").value;

        let grossAnnual = 0;

        if (payType === "salary") {
          const annualSalary = toNumber("annual-salary");
          grossAnnual = annualSalary;
        } else {
          const rate = toNumber("hourly-rate");
          const hours = toNumber("hours-per-week");
          grossAnnual = rate * hours * 52;
        }

        const additional = toNumber("additional-taxable");
        const grossPeriodBase = grossAnnual / periodsPerYear;
        const grossPeriod = grossPeriodBase + additional;

        const pretaxPeriod = toNumber("pretax-deductions");
        const posttaxPeriod = toNumber("posttax-deductions");
        const taxablePeriod = Math.max(0, grossPeriod - pretaxPeriod);

        const taxableAnnual = taxablePeriod * periodsPerYear;

        // Simplified federal effective tax
        let federalAnnual = 0;
        if (filingStatus === "single") {
          const threshold = 50000;
          const rateLow = 0.12;
          const rateHigh = 0.22;
          if (taxableAnnual <= threshold) {
            federalAnnual = taxableAnnual * rateLow;
          } else {
            federalAnnual =
              threshold * rateLow + (taxableAnnual - threshold) * rateHigh;
          }
        } else {
          const threshold = 80000;
          const rateLow = 0.1;
          const rateHigh = 0.2;
          if (taxableAnnual <= threshold) {
            federalAnnual = taxableAnnual * rateLow;
          } else {
            federalAnnual =
              threshold * rateLow + (taxableAnnual - threshold) * rateHigh;
          }
        }

        // FICA
        const ssWageBase = 168600;
        const ssAnnual = 0.062 * Math.min(taxableAnnual, ssWageBase);
        const medicareAnnual = 0.0145 * taxableAnnual;
        const ficaAnnual = ssAnnual + medicareAnnual;

        // State/local
        const stateRate = toNumber("state-tax-rate") / 100;
        const stateAnnual = taxableAnnual * stateRate;

        // Annual deductions
        const pretaxAnnual = pretaxPeriod * periodsPerYear;
        const posttaxAnnual = posttaxPeriod * periodsPerYear;

        const netAnnual =
          grossAnnual -
          pretaxAnnual -
          federalAnnual -
          ficaAnnual -
          stateAnnual -
          posttaxAnnual;
        const netPeriod = netAnnual / periodsPerYear;

        const federalPeriod = federalAnnual / periodsPerYear;
        const ficaPeriod = ficaAnnual / periodsPerYear;
        const statePeriod = stateAnnual / periodsPerYear;
        const taxablePeriodDisplay = taxablePeriod;

        // Update per period
        document.getElementById("res-gross-period").textContent =
          "$" + fmt(grossPeriod);
        document.getElementById("res-pretax-period").textContent =
          "$" + fmt(pretaxPeriod);
        document.getElementById("res-taxable-period").textContent =
          "$" + fmt(taxablePeriodDisplay);
        document.getElementById("res-federal-period").textContent =
          "$" + fmt(federalPeriod);
        document.getElementById("res-fica-period").textContent =
          "$" + fmt(ficaPeriod);
        document.getElementById("res-state-period").textContent =
          "$" + fmt(statePeriod);
        document.getElementById("res-posttax-period").textContent =
          "$" + fmt(posttaxPeriod);
        document.getElementById("res-net-period").textContent =
          "$" + fmt(netPeriod);

        // Update annual
        document.getElementById("res-gross-annual").textContent =
          "$" + fmt(grossAnnual);
        document.getElementById("res-pretax-annual").textContent =
          "$" + fmt(pretaxAnnual);
        document.getElementById("res-taxable-annual").textContent =
          "$" + fmt(taxableAnnual);
        document.getElementById("res-federal-annual").textContent =
          "$" + fmt(federalAnnual);
        document.getElementById("res-fica-annual").textContent =
          "$" + fmt(ficaAnnual);
        document.getElementById("res-state-annual").textContent =
          "$" + fmt(stateAnnual);
        document.getElementById("res-posttax-annual").textContent =
          "$" + fmt(posttaxAnnual);
        document.getElementById("res-net-annual").textContent =
          "$" + fmt(netAnnual);

        // Breakdown bar
        const totalOut = Math.max(0, grossPeriod);
        const taxTotal = federalPeriod + ficaPeriod + statePeriod;
        const dedTotal = pretaxPeriod + posttaxPeriod;
        const net = netPeriod;

        let pctNet = 0,
          pctTax = 0,
          pctDed = 0;
        if (totalOut > 0) {
          pctNet = Math.max(0, (net / totalOut) * 100);
          pctTax = Math.max(0, (taxTotal / totalOut) * 100);
          pctDed = Math.max(0, (dedTotal / totalOut) * 100);
          const sum = pctNet + pctTax + pctDed;
          if (sum > 100) {
            const factor = 100 / sum;
            pctNet *= factor;
            pctTax *= factor;
            pctDed *= factor;
          }
        }

        document.getElementById("bar-net").style.width = pctNet + "%";
        document.getElementById("bar-tax").style.width = pctTax + "%";
        document.getElementById("bar-ded").style.width = pctDed + "%";

        document.getElementById("label-net").textContent =
          "Net " + fmt(isFinite(pctNet) ? pctNet : 0) + "%";
        document.getElementById("label-tax").textContent =
          "Taxes " + fmt(isFinite(pctTax) ? pctTax : 0) + "%";
        document.getElementById("label-ded").textContent =
          "Deductions " + fmt(isFinite(pctDed) ? pctDed : 0) + "%";
      }

      function resetForm() {
        document.getElementById("paycheck-form").reset();
        updatePayTypeVisibility();
        calculatePaycheck();
      }

      document.addEventListener("DOMContentLoaded", function () {
        updatePayTypeVisibility();
        calculatePaycheck();

        document
          .getElementById("pay-type")
          .addEventListener("change", updatePayTypeVisibility);
        document
          .getElementById("calculate-btn")
          .addEventListener("click", calculatePaycheck);
        document
          .getElementById("reset-btn")
          .addEventListener("click", resetForm);

        // Recalculate on key change for quick feedback
        [
          "annual-salary",
          "hourly-rate",
          "hours-per-week",
          "additional-taxable",
          "pretax-deductions",
          "posttax-deductions",
          "state-tax-rate",
        ].forEach((id) => {
          const el = document.getElementById(id);
          if (el)
            el.addEventListener("input", () => {
              /* debounce not needed */
            });
        });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
