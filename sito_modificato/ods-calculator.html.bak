    <script>
      // Utility functions for odds conversions
      function toNumber(val) {
        if (typeof val === "number") return val;
        if (!val) return NaN;
        return Number(String(val).replace(/,/g, "").trim());
      }

      function americanToDecimal(am) {
        am = toNumber(am);
        if (!isFinite(am) || am === 0) return NaN;
        if (am > 0) return 1 + am / 100;
        return 1 + 100 / Math.abs(am);
      }

      function decimalToAmerican(dec) {
        dec = toNumber(dec);
        if (!isFinite(dec) || dec <= 1) return NaN;
        if (dec >= 2) {
          return Math.round((dec - 1) * 100);
        } else {
          return -Math.round(100 / (dec - 1));
        }
      }

      function fractionalToDecimal(fracStr) {
        if (!fracStr) return NaN;
        const parts = String(fracStr).split("/");
        if (parts.length !== 2) return NaN;
        const num = toNumber(parts[0]);
        const den = toNumber(parts[1]);
        if (!isFinite(num) || !isFinite(den) || den === 0) return NaN;
        return 1 + num / den;
      }

      function decimalToFractional(dec, maxDen = 1000) {
        dec = toNumber(dec);
        if (!isFinite(dec) || dec <= 1) return null;
        const x = dec - 1;
        let bestNum = 1,
          bestDen = 1,
          bestError = Math.abs(x - 1);
        for (let den = 1; den <= maxDen; den++) {
          const num = Math.round(x * den);
          if (num < 0) continue;
          const approx = num / den;
          const err = Math.abs(approx - x);
          if (err < bestError - 1e-9) {
            bestError = err;
            bestNum = num;
            bestDen = den;
          }
        }
        return bestNum + "/" + bestDen;
      }

      function decimalToProb(dec) {
        dec = toNumber(dec);
        if (!isFinite(dec) || dec <= 1) return NaN;
        return 1 / dec;
      }

      function probToDecimal(p) {
        p = toNumber(p);
        if (!isFinite(p) || p <= 0 || p >= 1) return NaN;
        return 1 / p;
      }

      function formatPercent(p) {
        if (!isFinite(p)) return "–";
        return (p * 100).toFixed(2).replace(/\.00$/, "") + "%";
      }

      function formatMoney(x) {
        if (!isFinite(x)) return "–";
        return "$" + x.toFixed(2).replace(/\B(?=(\d{3})+(?!\d))/g, ",");
      }

      function formatDecimal(dec) {
        if (!isFinite(dec)) return "–";
        return dec.toFixed(3).replace(/\.?0+$/, "");
      }

      function parseOddsByFormat(value, format) {
        value = String(value || "").trim();
        if (!value) return NaN;
        switch (format) {
          case "american":
            return americanToDecimal(value);
          case "decimal":
            return toNumber(value);
          case "fractional":
            return fractionalToDecimal(value);
          case "probability":
            const p = toNumber(value) / 100;
            return probToDecimal(p);
          default:
            return NaN;
        }
      }

      // Single bet logic
      function updateSingle() {
        const format = document.getElementById("single-format").value;
        const oddsInput = document.getElementById("single-odds").value;
        const stake =
          toNumber(document.getElementById("single-stake").value) || 0;
        const marginInput = toNumber(
          document.getElementById("single-margin").value
        );
        const errorEl = document.getElementById("single-error");
        errorEl.classList.add("hidden");
        errorEl.textContent = "";

        const dec = parseOddsByFormat(oddsInput, format);
        if (!isFinite(dec) || dec <= 1) {
          clearSingleOutputs();
          if (oddsInput.trim() !== "") {
            errorEl.textContent = "Please enter valid odds or probability.";
            errorEl.classList.remove("hidden");
          }
          return;
        }

        const am = decimalToAmerican(dec);
        const frac = decimalToFractional(dec);
        const prob = decimalToProb(dec);

        // Fair probability if margin given
        let fairProb = NaN;
        if (isFinite(marginInput) && marginInput > 0) {
          // Simple approximation: assume margin is added proportionally
          const overround = 1 + marginInput / 100;
          fairProb = prob / overround;
        }

        const fairDec = isFinite(fairProb) ? probToDecimal(fairProb) : NaN;

        // Payouts
        const profit = (dec - 1) * stake;
        const ret = dec * stake;
        const breakevenProb = prob;

        // Edge vs fair if we have fairProb
        let edge = NaN;
        if (isFinite(fairProb)) {
          edge = fairProb - breakevenProb;
        }

        document.getElementById("out-american").textContent = isFinite(am)
          ? am > 0
            ? "+" + am
            : am
          : "–";
        document.getElementById("out-decimal").textContent = formatDecimal(dec);
        document.getElementById("out-fractional").textContent = frac || "–";
        document.getElementById("out-prob").textContent = formatPercent(prob);
        document.getElementById("out-fair-prob").textContent = isFinite(
          fairProb
        )
          ? formatPercent(fairProb)
          : "–";
        document.getElementById("out-fair-decimal").textContent = isFinite(
          fairDec
        )
          ? formatDecimal(fairDec)
          : "–";
        document.getElementById("out-profit").textContent =
          stake > 0 ? formatMoney(profit) : "–";
        document.getElementById("out-return").textContent =
          stake > 0 ? formatMoney(ret) : "–";
        document.getElementById("out-breakeven").textContent =
          formatPercent(breakevenProb);
        document.getElementById("out-edge").textContent = isFinite(edge)
          ? (edge >= 0 ? "+" : "") + formatPercent(edge)
          : "–";
      }

      function clearSingleOutputs() {
        const ids = [
          "out-american",
          "out-decimal",
          "out-fractional",
          "out-prob",
          "out-fair-prob",
          "out-fair-decimal",
          "out-profit",
          "out-return",
          "out-breakeven",
          "out-edge",
        ];
        ids.forEach((id) => (document.getElementById(id).textContent = "–"));
      }

      // No-vig logic
      function calcNoVig() {
        const fmtA = document.getElementById("novig-format-a").value;
        const fmtB = document.getElementById("novig-format-b").value;
        const oddsA = document.getElementById("novig-odds-a").value;
        const oddsB = document.getElementById("novig-odds-b").value;
        const stakeA =
          toNumber(document.getElementById("novig-stake-a").value) || 0;
        const stakeB =
          toNumber(document.getElementById("novig-stake-b").value) || 0;
        const errorEl = document.getElementById("novig-error");
        errorEl.classList.add("hidden");
        errorEl.textContent = "";

        const decA = parseOddsByFormat(oddsA, fmtA);
        const decB = parseOddsByFormat(oddsB, fmtB);

        if (!isFinite(decA) || decA <= 1 || !isFinite(decB) || decB <= 1) {
          clearNoVigOutputs();
          errorEl.textContent = "Enter valid odds for both sides (A and B).";
          errorEl.classList.remove("hidden");
          return;
        }

        const pA = decimalToProb(decA);
        const pB = decimalToProb(decB);
        const sum = pA + pB;
        if (!isFinite(sum) || sum <= 0) {
          clearNoVigOutputs();
          errorEl.textContent =
            "Could not compute implied probabilities from the given odds.";
          errorEl.classList.remove("hidden");
          return;
        }

        const margin = sum - 1;
        const fairProbA = pA / sum;
        const fairProbB = pB / sum;
        const fairDecA = probToDecimal(fairProbA);
        const fairDecB = probToDecimal(fairProbB);

        document.getElementById("novig-prob-a").textContent = formatPercent(pA);
        document.getElementById("novig-prob-b").textContent = formatPercent(pB);
        document.getElementById("novig-sum").textContent = formatPercent(sum);
        document.getElementById("novig-margin").textContent =
          formatPercent(margin);

        document.getElementById("novig-fair-prob-a").textContent =
          formatPercent(fairProbA);
        document.getElementById("novig-fair-dec-a").textContent =
          formatDecimal(fairDecA);
        document.getElementById("novig-fair-prob-b").textContent =
          formatPercent(fairProbB);
        document.getElementById("novig-fair-dec-b").textContent =
          formatDecimal(fairDecB);
      }

      function clearNoVigOutputs() {
        const ids = [
          "novig-prob-a",
          "novig-prob-b",
          "novig-sum",
          "novig-margin",
          "novig-fair-prob-a",
          "novig-fair-dec-a",
          "novig-fair-prob-b",
          "novig-fair-dec-b",
        ];
        ids.forEach((id) => (document.getElementById(id).textContent = "–"));
      }

      // Tabs
      function setTab(active) {
        const tabSingle = document.getElementById("tab-single");
        const tabNovig = document.getElementById("tab-novig");
        const panelSingle = document.getElementById("panel-single");
        const panelNovig = document.getElementById("panel-novig");

        if (active === "single") {
          tabSingle.classList.add("tab-active");
          tabNovig.classList.remove("tab-active");
          panelSingle.classList.remove("hidden");
          panelNovig.classList.add("hidden");
        } else {
          tabNovig.classList.add("tab-active");
          tabSingle.classList.remove("tab-active");
          panelNovig.classList.remove("hidden");
          panelSingle.classList.add("hidden");
        }
      }

      document.addEventListener("DOMContentLoaded", function () {
        // Single bet listeners
        [
          "single-format",
          "single-odds",
          "single-stake",
          "single-margin",
        ].forEach((id) => {
          const el = document.getElementById(id);
          el.addEventListener("input", updateSingle);
          el.addEventListener("change", updateSingle);
        });
        document
          .getElementById("single-reset")
          .addEventListener("click", function () {
            document.getElementById("single-format").value = "american";
            document.getElementById("single-odds").value = "";
            document.getElementById("single-stake").value = 100;
            document.getElementById("single-margin").value = "";
            document.getElementById("single-error").classList.add("hidden");
            clearSingleOutputs();
          });

        // No-vig listeners
        document
          .getElementById("novig-calc")
          .addEventListener("click", calcNoVig);
        document
          .getElementById("novig-reset")
          .addEventListener("click", function () {
            document.getElementById("novig-format-a").value = "american";
            document.getElementById("novig-format-b").value = "american";
            document.getElementById("novig-odds-a").value = "";
            document.getElementById("novig-odds-b").value = "";
            document.getElementById("novig-stake-a").value = "";
            document.getElementById("novig-stake-b").value = "";
            document.getElementById("novig-error").classList.add("hidden");
            clearNoVigOutputs();
          });

        // Tabs
        document
          .getElementById("tab-single")
          .addEventListener("click", function () {
            setTab("single");
          });
        document
          .getElementById("tab-novig")
          .addEventListener("click", function () {
            setTab("novig");
          });

        // Initial
        clearSingleOutputs();
        clearNoVigOutputs();
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
