    <script>
      // Mobile menu toggle
      document
        .getElementById("mobile-menu-toggle")
        ?.addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          if (!menu) return;
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          this.setAttribute("aria-expanded", String(isHidden));
        });

      function parseNumber(id) {
        const el = document.getElementById(id);
        if (!el) return NaN;
        const v = parseFloat(el.value);
        return isNaN(v) ? NaN : v;
      }

      function roundTo(value, step) {
        if (!step || step <= 0) return value;
        return Math.round(value / step) * step;
      }

      function convertLbToKg(lb) {
        return lb * 0.45359237;
      }

      document.getElementById("route")?.addEventListener("change", function () {
        const route = this.value;
        const Finput = document.getElementById("bioavailability");
        if (!Finput) return;
        if (route === "iv") {
          Finput.value = "1";
        } else if (!Finput.value) {
          Finput.placeholder = "e.g. 0.7 for 70%";
        }
      });

      document
        .getElementById("calc-btn")
        ?.addEventListener("click", function () {
          const vd = parseNumber("vd");
          const vdUnit =
            document.getElementById("vd-unit")?.value || "L_per_kg";
          const weight = parseNumber("weight");
          const weightUnit =
            document.getElementById("weight-unit")?.value || "kg";
          const Ctarget = parseNumber("target-conc");
          const concUnit =
            document.getElementById("conc-unit")?.value || "mg_per_L";
          let F = parseNumber("bioavailability");
          let S = parseNumber("salt-factor");
          const roundStep = parseNumber("round-to");
          const maxDose = parseNumber("max-dose");

          const resultDoseEl = document.getElementById("result-dose");
          const resultRawEl = document.getElementById("result-raw");
          const resultSplitEl = document.getElementById("result-split");

          function showError(msg) {
            if (resultDoseEl) resultDoseEl.textContent = "—";
            if (resultRawEl) resultRawEl.textContent = msg;
            if (resultSplitEl) resultSplitEl.textContent = "";
          }

          if (isNaN(vd) || vd <= 0)
            return showError("Enter a valid volume of distribution.");
          if (vdUnit === "L_per_kg" && (isNaN(weight) || weight <= 0)) {
            return showError("Enter a valid patient weight for Vd in L/kg.");
          }
          if (isNaN(Ctarget) || Ctarget <= 0)
            return showError("Enter a valid target concentration.");
          if (isNaN(F) || F <= 0 || F > 1.5)
            return showError(
              "Enter a realistic bioavailability F (e.g. 1 for IV, 0.3–1 for oral)."
            );
          if (isNaN(S) || S <= 0) S = 1;

          // Convert weight to kg if needed
          let weightKg = weight;
          if (!isNaN(weight) && weightUnit === "lb") {
            weightKg = convertLbToKg(weight);
          }

          // Total Vd in L
          let VdTotalL = vd;
          if (vdUnit === "L_per_kg") {
            VdTotalL = vd * weightKg;
          }

          // Concentration units: mg/L and mcg/mL are equivalent numerically
          let CmgPerL = Ctarget;
          if (concUnit === "mcg_per_mL") {
            CmgPerL = Ctarget; // 1 mcg/mL = 1 mg/L
          }

          // Loading dose in mg
          const doseMgRaw = (CmgPerL * VdTotalL) / (F * S);
          if (!isFinite(doseMgRaw) || doseMgRaw <= 0)
            return showError(
              "Unable to compute loading dose with the given inputs."
            );

          const doseRounded = roundTo(doseMgRaw, roundStep || 1);

          if (resultDoseEl)
            resultDoseEl.textContent = doseRounded.toFixed(0) + " mg";
          if (resultRawEl) {
            resultRawEl.innerHTML =
              "Unrounded dose: " +
              doseMgRaw.toFixed(1) +
              " mg" +
              "<br>Total Vd: " +
              VdTotalL.toFixed(1) +
              " L" +
              "<br>Assuming C<sub>target</sub> = " +
              CmgPerL.toFixed(2) +
              " mg/L, F = " +
              F.toFixed(2) +
              ", S = " +
              S.toFixed(2) +
              ".";
          }

          if (resultSplitEl) {
            resultSplitEl.textContent = "";
            if (!isNaN(maxDose) && maxDose > 0 && doseRounded > maxDose) {
              const nDoses = Math.ceil(doseRounded / maxDose);
              const perDose = doseRounded / nDoses;
              resultSplitEl.innerHTML =
                "Suggested split loading: " +
                nDoses +
                " doses of approximately " +
                perDose.toFixed(0) +
                " mg (max " +
                maxDose.toFixed(0) +
                " mg per dose).";
            }
          }
        });
    </script>

