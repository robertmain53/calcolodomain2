    <script>
      // Simple histogram drawing without external libraries
      function drawHistogram(canvas, data) {
        if (!canvas || !data || data.length === 0) return;
        const ctx = canvas.getContext("2d");
        const width = (canvas.width = canvas.clientWidth);
        const height = (canvas.height = canvas.clientHeight);

        const nBins = 30;
        const min = Math.min(...data);
        const max = Math.max(...data);
        if (!isFinite(min) || !isFinite(max) || min === max) {
          ctx.clearRect(0, 0, width, height);
          ctx.fillStyle = "#9CA3AF";
          ctx.font = "12px Inter, sans-serif";
          ctx.fillText("Not enough variation to plot a histogram.", 10, 20);
          return;
        }
        const binWidth = (max - min) / nBins;
        const bins = new Array(nBins).fill(0);
        data.forEach((v) => {
          let idx = Math.floor((v - min) / binWidth);
          if (idx < 0) idx = 0;
          if (idx >= nBins) idx = nBins - 1;
          bins[idx]++;
        });
        const maxCount = Math.max(...bins);

        ctx.clearRect(0, 0, width, height);
        ctx.fillStyle = "#DBEAFE";
        ctx.strokeStyle = "#2563EB";

        const barWidth = width / nBins;
        bins.forEach((count, i) => {
          const barHeight = (count / maxCount) * (height - 20);
          const x = i * barWidth;
          const y = height - barHeight;
          ctx.fillRect(x, y, barWidth - 1, barHeight);
        });
      }

      function percentile(arr, p) {
        if (!arr.length) return NaN;
        const sorted = [...arr].sort((a, b) => a - b);
        const idx = (p / 100) * (sorted.length - 1);
        const lower = Math.floor(idx);
        const upper = Math.ceil(idx);
        if (lower === upper) return sorted[lower];
        const weight = idx - lower;
        return sorted[lower] * (1 - weight) + sorted[upper] * weight;
      }

      function formatNumber(n) {
        if (!isFinite(n)) return "–";
        const abs = Math.abs(n);
        if (abs >= 1e9) return (n / 1e9).toFixed(2) + "B";
        if (abs >= 1e6) return (n / 1e6).toFixed(2) + "M";
        if (abs >= 1e3) return (n / 1e3).toFixed(2) + "k";
        if (abs >= 1) return n.toFixed(2);
        if (abs === 0) return "0";
        return n.toExponential(2);
      }

      function createFactorRow(index) {
        const tr = document.createElement("tr");
        tr.className = index % 2 === 0 ? "bg-white" : "bg-gray-50";
        tr.innerHTML = `
      <td class="px-2 py-2 align-top text-gray-500">${index + 1}</td>
      <td class="px-2 py-2 align-top">
        <input type="text" class="w-40 md:w-48 border border-gray-300 rounded-md px-2 py-1 text-xs" placeholder="e.g. Population of the city">
      </td>
      <td class="px-2 py-2 align-top">
        <input type="number" step="any" class="w-20 border border-gray-300 rounded-md px-2 py-1 text-xs" placeholder="Low">
      </td>
      <td class="px-2 py-2 align-top">
        <input type="number" step="any" class="w-20 border border-gray-300 rounded-md px-2 py-1 text-xs" placeholder="Best">
      </td>
      <td class="px-2 py-2 align-top">
        <input type="number" step="any" class="w-20 border border-gray-300 rounded-md px-2 py-1 text-xs" placeholder="High">
      </td>
      <td class="px-2 py-2 align-top">
        <select class="border border-gray-300 rounded-md px-1 py-1 text-xs">
          <option value="*">× (multiply)</option>
          <option value="/">÷ (divide)</option>
          <option value="+">+ (add)</option>
          <option value="-">− (subtract)</option>
        </select>
      </td>
      <td class="px-2 py-2 align-top">
        <input type="text" class="w-28 md:w-32 border border-gray-300 rounded-md px-2 py-1 text-xs" placeholder="Unit / note">
      </td>
      <td class="px-2 py-2 align-top text-right">
        <button type="button" class="text-xs text-red-600 hover:underline remove-factor-btn">Remove</button>
      </td>
    `;
        return tr;
      }

      function getFactors() {
        const rows = Array.from(document.querySelectorAll("#factors-body tr"));
        return rows.map((row) => {
          const [
            descInput,
            lowInput,
            bestInput,
            highInput,
            opSelect,
            unitInput,
          ] = row.querySelectorAll("input, select");
          return {
            description: descInput.value.trim(),
            low: parseFloat(lowInput.value),
            best: parseFloat(bestInput.value),
            high: parseFloat(highInput.value),
            op: opSelect.value,
            unit: unitInput.value.trim(),
          };
        });
      }

      function validateFactors(factors) {
        if (!factors.length) return "Add at least one factor.";
        for (let i = 0; i < factors.length; i++) {
          const f = factors[i];
          if (!isFinite(f.low) || !isFinite(f.best) || !isFinite(f.high)) {
            return `Factor ${
              i + 1
            }: please fill low, best and high with numbers.`;
          }
          if (f.low > f.high) {
            return `Factor ${i + 1}: low cannot be greater than high.`;
          }
          if (f.best < f.low || f.best > f.high) {
            return `Factor ${i + 1}: best should be between low and high.`;
          }
        }
        return null;
      }

      function sampleFromFactor(f, distribution) {
        const a = f.low;
        const m = f.best;
        const b = f.high;
        if (distribution === "uniform") {
          return a + Math.random() * (b - a);
        }
        // triangular
        const u = Math.random();
        const c = (m - a) / (b - a || 1);
        if (u < c) {
          return a + Math.sqrt(u * (b - a) * (m - a));
        } else {
          return b - Math.sqrt((1 - u) * (b - a) * (b - m));
        }
      }

      function runSimulation() {
        const errorEl = document.getElementById("error-message");
        errorEl.classList.add("hidden");
        errorEl.textContent = "";

        const factors = getFactors();
        const validationError = validateFactors(factors);
        if (validationError) {
          errorEl.textContent = validationError;
          errorEl.classList.remove("hidden");
          return;
        }

        const iterationsInput = document.getElementById("iterations-input");
        let n = parseInt(iterationsInput.value, 10);
        if (!isFinite(n) || n < 100) n = 100;
        if (n > 50000) n = 50000;
        iterationsInput.value = n;

        const distribution = document.getElementById(
          "distribution-select"
        ).value;
        const results = [];
        const scenarios = [];

        for (let i = 0; i < n; i++) {
          let value = null;
          const scenarioFactors = [];
          for (let j = 0; j < factors.length; j++) {
            const f = factors[j];
            const sample = sampleFromFactor(f, distribution);
            scenarioFactors.push({
              description: f.description || `Factor ${j + 1}`,
              value: sample,
              op: f.op,
            });
            if (j === 0) {
              value = sample;
            } else {
              switch (f.op) {
                case "*":
                  value *= sample;
                  break;
                case "/":
                  value / sample;
                  break;
                case "+":
                  value += sample;
                  break;
                case "-":
                  value -= sample;
                  break;
              }
            }
          }
          results.push(value);
          if (i < 5) {
            scenarios.push({ value, factors: scenarioFactors });
          }
        }

        if (!results.length) return;

        const mean = results.reduce((a, b) => a + b, 0) / results.length;
        const variance =
          results.reduce((a, b) => a + Math.pow(b - mean, 2), 0) /
          results.length;
        const std = Math.sqrt(variance);
        const p10 = percentile(results, 10);
        const p50 = percentile(results, 50);
        const p90 = percentile(results, 90);

        document.getElementById("results-empty").classList.add("hidden");
        document.getElementById("results-container").classList.remove("hidden");

        const unit = factors[factors.length - 1].unit || "";
        const unitSuffix = unit ? " " + unit : "";

        document.getElementById("result-median").textContent =
          formatNumber(p50) + unitSuffix;
        document.getElementById("result-range").textContent =
          formatNumber(p10) +
          unitSuffix +
          " – " +
          formatNumber(p90) +
          unitSuffix;
        document.getElementById("result-mean-std").textContent =
          "Mean " +
          formatNumber(mean) +
          unitSuffix +
          ", SD " +
          formatNumber(std) +
          unitSuffix;

        const canvas = document.getElementById("histogram-canvas");
        drawHistogram(canvas, results);
        document.getElementById(
          "histogram-note"
        ).textContent = `Histogram based on ${results.length.toLocaleString()} simulation runs.`;

        const scenariosContainer = document.getElementById(
          "scenarios-container"
        );
        scenariosContainer.innerHTML = "";
        scenarios.forEach((s, idx) => {
          const div = document.createElement("div");
          div.className = "border border-gray-200 rounded-md p-2 bg-gray-50";
          const lines = s.factors.map((f, i) => {
            const op = i === 0 ? "" : f.op + " ";
            return `${op}${
              f.description || "Factor " + (i + 1)
            } ≈ ${formatNumber(f.value)}`;
          });
          div.innerHTML = `
        <div class="font-semibold mb-1">Scenario ${idx + 1}: ${formatNumber(
            s.value
          )}${unitSuffix}</div>
        <div class="whitespace-pre-line text-gray-700">${lines.join("\n")}</div>
      `;
          scenariosContainer.appendChild(div);
        });
      }

      function loadExample() {
        const tbody = document.getElementById("factors-body");
        tbody.innerHTML = "";
        const exampleProblem =
          "How many cups of coffee are sold in a city per day?";
        document.getElementById("problem-description").value = exampleProblem;

        const exampleFactors = [
          {
            description: "City population",
            low: 2_000_000,
            best: 3_000_000,
            high: 4_000_000,
            op: "*",
            unit: "people",
          },
          {
            description: "Share of people who drink coffee",
            low: 0.4,
            best: 0.6,
            high: 0.8,
            op: "*",
            unit: "fraction",
          },
          {
            description: "Average cups per coffee drinker per day",
            low: 1,
            best: 2,
            high: 3,
            op: "*",
            unit: "cups/day",
          },
        ];

        exampleFactors.forEach((f, idx) => {
          const row = createFactorRow(idx);
          const inputs = row.querySelectorAll("input, select");
          inputs[0].value = f.description;
          inputs[1].value = f.low;
          inputs[2].value = f.best;
          inputs[3].value = f.high;
          inputs[4].value = f.op;
          inputs[5].value = f.unit;
          tbody.appendChild(row);
        });

        document.getElementById("iterations-input").value = 5000;
        document.getElementById("distribution-select").value = "triangular";
      }

      document.addEventListener("DOMContentLoaded", () => {
        const tbody = document.getElementById("factors-body");
        const addBtn = document.getElementById("add-factor-btn");
        const runBtn = document.getElementById("run-simulation-btn");
        const loadExampleBtn = document.getElementById("load-example-btn");

        // Start with two blank factors
        for (let i = 0; i < 2; i++) {
          tbody.appendChild(createFactorRow(i));
        }

        addBtn.addEventListener("click", () => {
          const index = tbody.querySelectorAll("tr").length;
          tbody.appendChild(createFactorRow(index));
        });

        tbody.addEventListener("click", (e) => {
          if (e.target.classList.contains("remove-factor-btn")) {
            const row = e.target.closest("tr");
            row.remove();
            const rows = Array.from(tbody.querySelectorAll("tr"));
            rows.forEach((r, idx) => {
              r.querySelector("td").textContent = idx + 1;
              r.className = idx % 2 === 0 ? "bg-white" : "bg-gray-50";
            });
          }
        });

        runBtn.addEventListener("click", runSimulation);
        loadExampleBtn.addEventListener("click", loadExample);
      });
    </script>

