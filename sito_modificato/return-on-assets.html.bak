    <script>
      // Helper: format number as currency-like with commas
      function formatNumber(num) {
        if (num === null || num === undefined || isNaN(num)) return "–";
        return num.toLocaleString(undefined, { maximumFractionDigits: 0 });
      }

      function formatPercent(num) {
        if (num === null || num === undefined || isNaN(num)) return "–";
        return (
          num.toLocaleString(undefined, { maximumFractionDigits: 2 }) + " %"
        );
      }

      function getAssetsAverage(basis, beginVal, endVal, endOnlyVal) {
        if (basis === "average") {
          if (isNaN(beginVal) || isNaN(endVal)) return null;
          return (beginVal + endVal) / 2;
        } else {
          if (isNaN(endOnlyVal)) return null;
          return endOnlyVal;
        }
      }

      function calculateScenario(prefix) {
        const profitType = document.getElementById("profitType" + prefix).value;
        const profitInput = parseFloat(
          document.getElementById("profit" + prefix).value
        );

        const assetBasis = document.getElementById("assetBasis" + prefix).value;
        const begin = parseFloat(
          document.getElementById("assetsBegin" + prefix)?.value
        );
        const end = parseFloat(
          document.getElementById("assetsEnd" + prefix)?.value
        );
        const endOnly = parseFloat(
          document.getElementById("assetsEndOnly" + prefix)?.value
        );

        const avgAssets = getAssetsAverage(assetBasis, begin, end, endOnly);

        const resultEl = document.getElementById("roaResult" + prefix);
        const avgEl = document.getElementById("avgAssets" + prefix);

        if (isNaN(profitInput) || avgAssets === null || avgAssets === 0) {
          resultEl.textContent = "–";
          avgEl.textContent = "–";
          return { roa: null, avgAssets: null, profitType: profitType };
        }

        const roa = (profitInput / avgAssets) * 100;
        resultEl.textContent = formatPercent(roa);
        avgEl.textContent = "$" + formatNumber(avgAssets);

        return { roa: roa, avgAssets: avgAssets, profitType: profitType };
      }

      function resetScenario(prefix) {
        [
          "profit" + prefix,
          "assetsBegin" + prefix,
          "assetsEnd" + prefix,
          "assetsEndOnly" + prefix,
        ].forEach((id) => {
          const el = document.getElementById(id);
          if (el) el.value = "";
        });
        document.getElementById("roaResult" + prefix).textContent = "–";
        document.getElementById("avgAssets" + prefix).textContent = "–";
      }

      function updateAssetGroups(prefix) {
        const basis = document.getElementById("assetBasis" + prefix).value;
        const avgGroup = document.getElementById("averageAssetsGroup" + prefix);
        const endGroup = document.getElementById("endingAssetsGroup" + prefix);
        if (basis === "average") {
          avgGroup.classList.remove("hidden");
          endGroup.classList.add("hidden");
        } else {
          avgGroup.classList.add("hidden");
          endGroup.classList.remove("hidden");
        }
      }

      function updateComparison() {
        const summaryEl = document.getElementById("comparisonSummary");
        const roaA = window._roaA;
        const roaB = window._roaB;

        if (!roaA && !roaB) {
          summaryEl.textContent =
            "Enter values for Scenario A (and optionally B) to see a comparison here.";
          return;
        }

        if (roaA && !roaB) {
          summaryEl.textContent = `Scenario A ROA is ${formatPercent(
            roaA.roa
          )} based on ${
            roaA.profitType === "net" ? "net income" : "EBIT"
          }. Add Scenario B to compare against another company, year, or target.`;
          return;
        }

        if (!roaA && roaB) {
          summaryEl.textContent = `Scenario B ROA is ${formatPercent(
            roaB.roa
          )} based on ${
            roaB.profitType === "net" ? "net income" : "EBIT"
          }. Add Scenario A to compare.`;
          return;
        }

        const diff = roaA.roa - roaB.roa;
        const better =
          diff > 0 ? "Scenario A" : diff < 0 ? "Scenario B" : "Both scenarios";
        const absDiff = Math.abs(diff);

        if (absDiff < 0.01) {
          summaryEl.textContent = `Both scenarios have almost identical ROA (${formatPercent(
            roaA.roa
          )} vs ${formatPercent(
            roaB.roa
          )}). Small differences may be due to rounding.`;
        } else {
          summaryEl.textContent = `${better} has the higher ROA. Scenario A: ${formatPercent(
            roaA.roa
          )}; Scenario B: ${formatPercent(
            roaB.roa
          )}. The difference is about ${formatPercent(absDiff)} points.`;
        }
      }

      document.addEventListener("DOMContentLoaded", function () {
        // Toggle asset basis groups
        ["A", "B"].forEach((prefix) => {
          document
            .getElementById("assetBasis" + prefix)
            .addEventListener("change", function () {
              updateAssetGroups(prefix);
            });
          updateAssetGroups(prefix);
        });

        // Calculate buttons
        document.getElementById("calcA").addEventListener("click", function () {
          window._roaA = calculateScenario("A");
          updateComparison();
        });
        document.getElementById("calcB").addEventListener("click", function () {
          window._roaB = calculateScenario("B");
          updateComparison();
        });

        // Reset buttons
        document
          .getElementById("resetA")
          .addEventListener("click", function () {
            resetScenario("A");
            window._roaA = null;
            updateComparison();
          });
        document
          .getElementById("resetB")
          .addEventListener("click", function () {
            resetScenario("B");
            window._roaB = null;
            updateComparison();
          });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
