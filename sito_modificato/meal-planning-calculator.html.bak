    <script>
      // Core state
      const caloriesInput = document.getElementById("calories");
      const proteinPctInput = document.getElementById("proteinPct");
      const carbPctInput = document.getElementById("carbPct");
      const fatPctInput = document.getElementById("fatPct");
      const macroPctWarning = document.getElementById("macroPctWarning");

      const proteinGramsEl = document.getElementById("proteinGrams");
      const carbGramsEl = document.getElementById("carbGrams");
      const fatGramsEl = document.getElementById("fatGrams");

      const summaryCaloriesEl = document.getElementById("summaryCalories");
      const summaryProteinEl = document.getElementById("summaryProtein");
      const summaryCarbsEl = document.getElementById("summaryCarbs");
      const summaryFatEl = document.getElementById("summaryFat");
      const macroPctLabelEl = document.getElementById("macroPctLabel");

      const barProtein = document.getElementById("barProtein");
      const barCarb = document.getElementById("barCarb");
      const barFat = document.getElementById("barFat");

      const mealsPerDaySelect = document.getElementById("mealsPerDay");
      const daysSelect = document.getElementById("days");
      const generatePlanBtn = document.getElementById("generatePlanBtn");
      const duplicateDayBtn = document.getElementById("duplicateDayBtn");
      const exportCsvBtn = document.getElementById("exportCsvBtn");
      const mealPlanBody = document.getElementById("mealPlanBody");

      const dailyTotalCaloriesEl =
        document.getElementById("dailyTotalCalories");
      const dailyTotalProteinEl = document.getElementById("dailyTotalProtein");
      const dailyTotalCarbsEl = document.getElementById("dailyTotalCarbs");
      const dailyTotalFatEl = document.getElementById("dailyTotalFat");

      function getMacroPercents() {
        const p = Number(proteinPctInput.value) || 0;
        const c = Number(carbPctInput.value) || 0;
        const f = Number(fatPctInput.value) || 0;
        return { p, c, f, sum: p + c + f };
      }

      function updateMacroSummary() {
        const K = Math.max(0, Number(caloriesInput.value) || 0);
        const { p, c, f, sum } = getMacroPercents();

        if (sum !== 100) {
          macroPctWarning.classList.remove("hidden");
        } else {
          macroPctWarning.classList.add("hidden");
        }

        const proteinKcal = K * (p / 100);
        const carbKcal = K * (c / 100);
        const fatKcal = K * (f / 100);

        const proteinG = proteinKcal / 4;
        const carbG = carbKcal / 4;
        const fatG = fatKcal / 9;

        proteinGramsEl.textContent = isFinite(proteinG)
          ? proteinG.toFixed(0) + " g"
          : "0 g";
        carbGramsEl.textContent = isFinite(carbG)
          ? carbG.toFixed(0) + " g"
          : "0 g";
        fatGramsEl.textContent = isFinite(fatG)
          ? fatG.toFixed(0) + " g"
          : "0 g";

        summaryCaloriesEl.textContent = K.toFixed(0) + " kcal";
        summaryProteinEl.textContent = isFinite(proteinG)
          ? proteinG.toFixed(0) + " g"
          : "0 g";
        summaryCarbsEl.textContent = isFinite(carbG)
          ? carbG.toFixed(0) + " g"
          : "0 g";
        summaryFatEl.textContent = isFinite(fatG)
          ? fatG.toFixed(0) + " g"
          : "0 g";

        macroPctLabelEl.textContent = `P ${p}% · C ${c}% · F ${f}%`;

        barProtein.style.width = (p || 0) + "%";
        barCarb.style.width = (c || 0) + "%";
        barFat.style.width = (f || 0) + "%";
      }

      ["input", "change"].forEach((evt) => {
        caloriesInput.addEventListener(evt, updateMacroSummary);
        proteinPctInput.addEventListener(evt, updateMacroSummary);
        carbPctInput.addEventListener(evt, updateMacroSummary);
        fatPctInput.addEventListener(evt, updateMacroSummary);
      });

      // Meal pattern weights
      function getMealWeights(mealsPerDay, pattern) {
        const base = new Array(mealsPerDay).fill(1);
        if (pattern === "balanced") return base;

        if (pattern === "frontloaded") {
          // more weight earlier
          for (let i = 0; i < mealsPerDay; i++) {
            base[i] = 1 + (mealsPerDay - i - 1) * 0.15;
          }
        } else if (pattern === "backloaded") {
          // more weight later
          for (let i = 0; i < mealsPerDay; i++) {
            base[i] = 1 + i * 0.15;
          }
        }
        return base;
      }

      function getMealLabels(mealsPerDay) {
        if (mealsPerDay === 2) return ["Meal 1", "Meal 2"];
        if (mealsPerDay === 3) return ["Breakfast", "Lunch", "Dinner"];
        if (mealsPerDay === 4) return ["Breakfast", "Lunch", "Snack", "Dinner"];
        if (mealsPerDay === 5)
          return ["Breakfast", "Snack 1", "Lunch", "Snack 2", "Dinner"];
        return Array.from({ length: mealsPerDay }, (_, i) => `Meal ${i + 1}`);
      }

      function generatePlan() {
        const K = Math.max(0, Number(caloriesInput.value) || 0);
        const { p, c, f, sum } = getMacroPercents();
        if (sum !== 100 || K <= 0) {
          alert(
            "Please enter a positive calorie target and ensure macros add up to 100%."
          );
          return;
        }

        const days = Number(daysSelect.value) || 7;
        const mealsPerDay = Number(mealsPerDaySelect.value) || 3;
        const pattern =
          document.querySelector('input[name="mealPattern"]:checked')?.value ||
          "balanced";

        const weights = getMealWeights(mealsPerDay, pattern);
        const totalWeight = weights.reduce((a, b) => a + b, 0);
        const labels = getMealLabels(mealsPerDay);

        mealPlanBody.innerHTML = "";

        for (let d = 1; d <= days; d++) {
          for (let m = 0; m < mealsPerDay; m++) {
            const mealCalories = K * (weights[m] / totalWeight);
            const proteinKcal = mealCalories * (p / 100);
            const carbKcal = mealCalories * (c / 100);
            const fatKcal = mealCalories * (f / 100);

            const proteinG = proteinKcal / 4;
            const carbG = carbKcal / 4;
            const fatG = fatKcal / 9;

            const tr = document.createElement("tr");
            tr.dataset.day = d.toString();
            tr.dataset.mealIndex = m.toString();

            tr.innerHTML = `
          <td class="px-2 py-1 border-b text-gray-700">Day ${d}</td>
          <td class="px-2 py-1 border-b text-gray-700">${labels[m]}</td>
          <td class="px-2 py-1 border-b">
            <input type="text" class="w-full border border-gray-200 rounded px-1 py-0.5 text-xs" value="">
          </td>
          <td class="px-2 py-1 border-b text-right">
            <input type="number" class="meal-cal w-20 border border-gray-200 rounded px-1 py-0.5 text-right text-xs" value="${mealCalories.toFixed(
              0
            )}" min="0">
          </td>
          <td class="px-2 py-1 border-b text-right">
            <input type="number" class="meal-protein w-16 border border-gray-200 rounded px-1 py-0.5 text-right text-xs" value="${proteinG.toFixed(
              0
            )}" min="0">
          </td>
          <td class="px-2 py-1 border-b text-right">
            <input type="number" class="meal-carbs w-16 border border-gray-200 rounded px-1 py-0.5 text-right text-xs" value="${carbG.toFixed(
              0
            )}" min="0">
          </td>
          <td class="px-2 py-1 border-b text-right">
            <input type="number" class="meal-fat w-16 border border-gray-200 rounded px-1 py-0.5 text-right text-xs" value="${fatG.toFixed(
              0
            )}" min="0">
          </td>
        `;
            mealPlanBody.appendChild(tr);
          }
        }
        updateDailyTotals();
      }

      function updateDailyTotals() {
        const rows = Array.from(mealPlanBody.querySelectorAll("tr"));
        if (!rows.length) {
          dailyTotalCaloriesEl.textContent = "0";
          dailyTotalProteinEl.textContent = "0";
          dailyTotalCarbsEl.textContent = "0";
          dailyTotalFatEl.textContent = "0";
          return;
        }
        // Compute average per day (assuming similar structure each day)
        const days = Number(daysSelect.value) || 7;
        let totalCalories = 0,
          totalProtein = 0,
          totalCarbs = 0,
          totalFat = 0;

        rows.forEach((row) => {
          const cal = Number(row.querySelector(".meal-cal")?.value) || 0;
          const pro = Number(row.querySelector(".meal-protein")?.value) || 0;
          const carb = Number(row.querySelector(".meal-carbs")?.value) || 0;
          const fat = Number(row.querySelector(".meal-fat")?.value) || 0;
          totalCalories += cal;
          totalProtein += pro;
          totalCarbs += carb;
          totalFat += fat;
        });

        const perDayCalories = totalCalories / days;
        const perDayProtein = totalProtein / days;
        const perDayCarbs = totalCarbs / days;
        const perDayFat = totalFat / days;

        dailyTotalCaloriesEl.textContent = perDayCalories.toFixed(0);
        dailyTotalProteinEl.textContent = perDayProtein.toFixed(0);
        dailyTotalCarbsEl.textContent = perDayCarbs.toFixed(0);
        dailyTotalFatEl.textContent = perDayFat.toFixed(0);
      }

      generatePlanBtn.addEventListener("click", generatePlan);

      mealPlanBody.addEventListener("input", function (e) {
        if (
          e.target.matches(".meal-cal, .meal-protein, .meal-carbs, .meal-fat")
        ) {
          updateDailyTotals();
        }
      });

      duplicateDayBtn.addEventListener("click", function () {
        const days = Number(daysSelect.value) || 7;
        const rows = Array.from(mealPlanBody.querySelectorAll("tr"));
        if (!rows.length) return;

        const mealsPerDay = Number(mealsPerDaySelect.value) || 3;
        const day1Rows = rows.filter((r) => r.dataset.day === "1");

        for (let d = 2; d <= days; d++) {
          for (let m = 0; m < mealsPerDay; m++) {
            const source = day1Rows[m];
            const target = rows.find(
              (r) =>
                r.dataset.day === d.toString() &&
                r.dataset.mealIndex === m.toString()
            );
            if (!source || !target) continue;

            const srcInputs = source.querySelectorAll("input");
            const tgtInputs = target.querySelectorAll("input");
            srcInputs.forEach((inp, idx) => {
              tgtInputs[idx].value = inp.value;
            });
          }
        }
        updateDailyTotals();
      });

      exportCsvBtn.addEventListener("click", function () {
        const rows = Array.from(mealPlanBody.querySelectorAll("tr"));
        if (!rows.length) {
          alert("Generate a plan first.");
          return;
        }
        const header = [
          "Day",
          "Meal",
          "Label",
          "Calories",
          "Protein (g)",
          "Carbs (g)",
          "Fat (g)",
        ];
        const lines = [header.join(",")];

        rows.forEach((row) => {
          const cells = row.querySelectorAll("td");
          const day = cells[0].textContent.trim();
          const meal = cells[1].textContent.trim();
          const label =
            cells[2].querySelector("input")?.value.replace(/"/g, '""') || "";
          const cal = cells[3].querySelector("input")?.value || "";
          const pro = cells[4].querySelector("input")?.value || "";
          const carb = cells[5].querySelector("input")?.value || "";
          const fat = cells[6].querySelector("input")?.value || "";
          const rowArr = [day, meal, `"${label}"`, cal, pro, carb, fat];
          lines.push(rowArr.join(","));
        });

        const blob = new Blob([lines.join("\n")], {
          type: "text/csv;charset=utf-8;",
        });
        const url = URL.createObjectURL(blob);
        const a = document.createElement("a");
        a.href = url;
        a.download = "meal-plan.csv";
        document.body.appendChild(a);
        a.click();
        document.body.removeChild(a);
        URL.revokeObjectURL(url);
      });

      // Initialize
      updateMacroSummary();
      generatePlan();

      document.querySelectorAll(".faq-question").forEach((btn) => {
        btn.addEventListener("click", () => {
          const answer = btn.nextElementSibling;
          if (!answer) return;
          answer.classList.toggle("hidden");
        });
      });
    </script>

