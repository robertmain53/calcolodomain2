    <script>
      // Utility formatting
      function fmtMoney(v) {
        if (!isFinite(v)) return '–';
        const sign = v < 0 ? '-' : '';
        const abs = Math.abs(v);
        return sign + '$' + abs.toLocaleString(undefined, {maximumFractionDigits: 0});
      }
      function fmtMoney1(v) {
        if (!isFinite(v)) return '–';
        const sign = v < 0 ? '-' : '';
        const abs = Math.abs(v);
        return sign + '$' + abs.toLocaleString(undefined, {minimumFractionDigits: 1, maximumFractionDigits: 1});
      }
      function fmtPct(v) {
        if (!isFinite(v)) return '–';
        return v.toFixed(1) + '%';
      }

      function getActiveMode() {
        return document.getElementById('airbnb-form-advanced').classList.contains('hidden') ? 'simple' : 'advanced';
      }

      function collectInputs(mode, overrideOcc, overrideRate) {
        if (mode === 'simple') {
          const nightlyRate = overrideRate ?? parseFloat(document.getElementById('nightlyRateSimple').value) || 0;
          const occupancy = overrideOcc ?? (parseFloat(document.getElementById('occupancySimple').value) || 0);
          const nightsPerMonth = parseFloat(document.getElementById('nightsPerMonthSimple').value) || 30;
          const platformFeePct = parseFloat(document.getElementById('platformFeeSimple').value) || 0;
          const cleaningFee = parseFloat(document.getElementById('cleaningFeeSimple').value) || 0;
          const cleaningPayer = document.querySelector('input[name="cleaningPayerSimple"]:checked')?.value || 'guest';
          const fixedCosts = parseFloat(document.getElementById('fixedCostsSimple').value) || 0;
          const variablePerBooking = parseFloat(document.getElementById('variablePerBookingSimple').value) || 0;
          const avgStay = 3; // implicit in simple mode

          return {
            nightlyRate,
            occupancy,
            nightsPerMonth,
            platformFeePct,
            taxPct: 0,
            cleaningFee,
            cleaningPayer,
            extraGuestFees: 0,
            fixedCosts,
            variablePerBooking,
            avgStay
          };
        } else {
          const nightlyRate = overrideRate ?? parseFloat(document.getElementById('nightlyRateAdv').value) || 0;
          const occupancy = overrideOcc ?? (parseFloat(document.getElementById('occupancyAdv').value) || 0);
          const nightsPerMonth = parseFloat(document.getElementById('nightsPerMonthAdv').value) || 30;
          const avgStay = Math.max(parseFloat(document.getElementById('avgStayLengthAdv').value) || 1, 0.1);
          const platformFeePct = parseFloat(document.getElementById('platformFeeAdv').value) || 0;
          const taxPct = parseFloat(document.getElementById('taxRateAdv').value) || 0;
          const cleaningFee = parseFloat(document.getElementById('cleaningFeeAdv').value) || 0;
          const cleaningPayer = document.querySelector('input[name="cleaningPayerAdv"]:checked')?.value || 'guest';
          const extraGuestFees = parseFloat(document.getElementById('extraGuestFeesAdv').value) || 0;

          const mortgage = parseFloat(document.getElementById('mortgageAdv').value) || 0;
          const utilities = parseFloat(document.getElementById('utilitiesAdv').value) || 0;
          const internet = parseFloat(document.getElementById('internetAdv').value) || 0;
          const insurance = parseFloat(document.getElementById('insuranceAdv').value) || 0;
          const hoa = parseFloat(document.getElementById('hoaAdv').value) || 0;
          const fixedCosts = mortgage + utilities + internet + insurance + hoa;

          const cleaningLabor = parseFloat(document.getElementById('cleaningLaborAdv').value) || 0;
          const laundry = parseFloat(document.getElementById('laundryAdv').value) || 0;
          const supplies = parseFloat(document.getElementById('suppliesAdv').value) || 0;
          const otherVar = parseFloat(document.getElementById('otherVarAdv').value) || 0;
          const variablePerBooking = cleaningLabor + laundry + supplies + otherVar;

          return {
            nightlyRate,
            occupancy,
            nightsPerMonth,
            platformFeePct,
            taxPct,
            cleaningFee,
            cleaningPayer,
            extraGuestFees,
            fixedCosts,
            variablePerBooking,
            avgStay
          };
        }
      }

      function computeScenario(params) {
        const {
          nightlyRate,
          occupancy,
          nightsPerMonth,
          platformFeePct,
          taxPct,
          cleaningFee,
          cleaningPayer,
          extraGuestFees,
          fixedCosts,
          variablePerBooking,
          avgStay
        } = params;

        const occFrac = Math.min(Math.max(occupancy / 100, 0), 1);
        const bookedNights = nightsPerMonth * occFrac;
        const bookings = bookedNights / Math.max(avgStay, 0.1);

        let gross = bookedNights * nightlyRate;
        if (cleaningPayer === 'guest') {
          gross += bookings * cleaningFee;
        }
        gross += bookings * extraGuestFees;

        const feeRate = (platformFeePct + taxPct) / 100;
        const platformAndTax = gross * feeRate;

        let variableCosts = bookings * variablePerBooking;
        if (cleaningPayer === 'host') {
          variableCosts += bookings * cleaningFee;
        }

        const totalCosts = platformAndTax + variableCosts + fixedCosts;
        const net = gross - totalCosts;
        const margin = gross > 0 ? (net / gross) * 100 : NaN;

        const perNightNet = bookedNights > 0 ? net / bookedNights : NaN;
        const perBookingNet = bookings > 0 ? net / bookings : NaN;
        const perAvailableNightNet = nightsPerMonth > 0 ? net / nightsPerMonth : NaN;

        // Break-even occupancy: solve net(occ) = 0
        // net = occ * nightsPerMonth * nightlyRate * (1 - feeRate) + bookingsTerm - fixedCosts
        // bookingsTerm = (occ * nightsPerMonth / avgStay) * (guestCleaningOrExtra - hostCleaning - variablePerBooking)
        const guestCleaningTerm = cleaningPayer === 'guest' ? cleaningFee : 0;
        const hostCleaningTerm = cleaningPayer === 'host' ? cleaningFee : 0;
        const perBookingRevenueExtra = guestCleaningTerm + extraGuestFees;
        const perBookingCostExtra = hostCleaningTerm + variablePerBooking;

        const A = nightsPerMonth * nightlyRate * (1 - feeRate);
        const B = (nightsPerMonth / Math.max(avgStay, 0.1)) * (perBookingRevenueExtra - perBookingCostExtra);
        const denom = A + B;
        let breakevenOcc = NaN;
        if (denom > 0) {
          const occFracBE = fixedCosts / denom;
          breakevenOcc = occFracBE * 100;
        }

        return {
          bookedNights,
          bookings,
          gross,
          platformAndTax,
          variableCosts,
          fixedCosts,
          net,
          margin,
          perNightNet,
          perBookingNet,
          perAvailableNightNet,
          breakevenOcc
        };
      }

      function updateMainResults() {
        const mode = getActiveMode();
        const inputs = collectInputs(mode);
        const res = computeScenario(inputs);

        document.getElementById('monthlyProfit').textContent = fmtMoney(res.net);
        document.getElementById('monthlyMargin').textContent = isFinite(res.margin) ? ('Margin: ' + fmtPct(res.margin)) : 'Margin: –';

        const yearlyProfit = res.net * 12;
        const yearlyRevenue = res.gross * 12;
        document.getElementById('yearlyProfit').textContent = fmtMoney(yearlyProfit);
        document.getElementById('yearlyRevenue').textContent = isFinite(yearlyRevenue) ? ('Revenue: ' + fmtMoney(yearlyRevenue)) : 'Revenue: –';

        document.getElementById('breakevenOcc').textContent = isFinite(res.breakevenOcc) ? fmtPct(res.breakevenOcc) : '–';

        document.getElementById('bookedNights').textContent = isFinite(res.bookedNights) ? res.bookedNights.toFixed(1) + ' nights' : '–';
        document.getElementById('bookings').textContent = isFinite(res.bookings) ? res.bookings.toFixed(1) + ' bookings' : '–';
        document.getElementById('grossRevenue').textContent = fmtMoney(res.gross);
        document.getElementById('platformFees').textContent = fmtMoney(res.platformAndTax);
        document.getElementById('variableCosts').textContent = fmtMoney(res.variableCosts);
        document.getElementById('fixedCosts').textContent = fmtMoney(res.fixedCosts);

        const nightlyRate = inputs.nightlyRate;
        document.getElementById('perNightRate').textContent = fmtMoney1(nightlyRate);
        document.getElementById('perNightNet').textContent = fmtMoney1(res.perNightNet);
        document.getElementById('perBookingNet').textContent = fmtMoney1(res.perBookingNet);
        document.getElementById('perAvailableNightNet').textContent = fmtMoney1(res.perAvailableNightNet);

        // Sync scenario sliders defaults with current inputs if they are not manually changed
        if (!updateMainResults.scenarioTouched) {
          document.getElementById('scenarioOcc').value = inputs.occupancy;
          document.getElementById('scenarioRate').value = nightlyRate;
          document.getElementById('scenarioOccVal').textContent = inputs.occupancy.toFixed(0) + '%';
          document.getElementById('scenarioRateVal').textContent = fmtMoney1(nightlyRate);
        }

        updateScenarioResults();
      }

      function updateScenarioResults() {
        const mode = getActiveMode();
        const occ = parseFloat(document.getElementById('scenarioOcc').value) || 0;
        const rate = parseFloat(document.getElementById('scenarioRate').value) || 0;
        document.getElementById('scenarioOccVal').textContent = occ.toFixed(0) + '%';
        document.getElementById('scenarioRateVal').textContent = fmtMoney1(rate);

        const inputs = collectInputs(mode, occ, rate);
        const res = computeScenario(inputs);

        document.getElementById('scenarioProfit').textContent = fmtMoney(res.net);
        document.getElementById('scenarioMargin').textContent = isFinite(res.margin) ? ('Margin: ' + fmtPct(res.margin)) : 'Margin: –';
        document.getElementById('scenarioRevenue').textContent = fmtMoney(res.gross);
        document.getElementById('scenarioBookings').textContent = isFinite(res.bookings) ? res.bookings.toFixed(1) + ' bookings / month' : '–';
        document.getElementById('scenarioBreakeven').textContent = isFinite(res.breakevenOcc) ? fmtPct(res.breakevenOcc) : '–';
      }

      document.addEventListener('DOMContentLoaded', function () {
        const tabSimple = document.getElementById('tab-simple');
        const tabAdvanced = document.getElementById('tab-advanced');
        const formSimple = document.getElementById('airbnb-form-simple');
        const formAdvanced = document.getElementById('airbnb-form-advanced');

        tabSimple.addEventListener('click', () => {
          tabSimple.classList.add('bg-blue-600', 'text-white');
          tabSimple.classList.remove('bg-gray-100', 'text-gray-700');
          tabAdvanced.classList.remove('bg-blue-600', 'text-white');
          tabAdvanced.classList.add('bg-gray-100', 'text-gray-700');
          formSimple.classList.remove('hidden');
          formAdvanced.classList.add('hidden');
          updateMainResults.scenarioTouched = false;
          updateMainResults();
        });

        tabAdvanced.addEventListener('click', () => {
          tabAdvanced.classList.add('bg-blue-600', 'text-white');
          tabAdvanced.classList.remove('bg-gray-100', 'text-gray-700');
          tabSimple.classList.remove('bg-blue-600', 'text-white');
          tabSimple.classList.add('bg-gray-100', 'text-gray-700');
          formAdvanced.classList.remove('hidden');
          formSimple.classList.add('hidden');
          updateMainResults.scenarioTouched = false;
          updateMainResults();
        });

        // Attach input listeners
        formSimple.querySelectorAll('input').forEach(el => {
          el.addEventListener('input', () => {
            updateMainResults.scenarioTouched = true;
            updateMainResults();
          });
          el.addEventListener('change', () => {
            updateMainResults.scenarioTouched = true;
            updateMainResults();
          });
        });
        formAdvanced.querySelectorAll('input').forEach(el => {
          el.addEventListener('input', () => {
            updateMainResults.scenarioTouched = true;
            updateMainResults();
          });
          el.addEventListener('change', () => {
            updateMainResults.scenarioTouched = true;
            updateMainResults();
          });
        });

        document.getElementById('scenarioOcc').addEventListener('input', () => {
          updateMainResults.scenarioTouched = true;
          updateScenarioResults();
        });
        document.getElementById('scenarioRate').addEventListener('input', () => {
          updateMainResults.scenarioTouched = true;
          updateScenarioResults();
        });

        updateMainResults();
      });

      // Mobile menu script (from template)
      document.addEventListener('DOMContentLoaded', function () {
        const toggle = document.getElementById('mobile-menu-toggle');
        const menu = document.getElementById('mobile-menu');
        if (toggle && menu) {
          toggle.addEventListener('click', () => {
            const isHidden = menu.classList.contains('hidden');
            menu.classList.toggle('hidden', !isHidden);
            toggle.setAttribute('aria-expanded', isHidden ? 'true' : 'false');
          });
        }
      });

      window.MathJax = {
        tex: { inlineMath: [['\\(','\\)'], ['$', '$']], displayMath: [['$','$'], ['\\[','\\]']] },
        svg: { fontCache: 'global' }
      };
    </script>
