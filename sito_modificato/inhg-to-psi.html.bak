  <script>
    (function() {
      const inputValue = document.getElementById('inputValue');
      const inputUnit = document.getElementById('inputUnit');
      const refTemp = document.getElementById('refTemp');
      const outPsi = document.getElementById('outPsi');
      const outKpa = document.getElementById('outKpa');
      const outBar = document.getElementById('outBar');
      const tableBody = document.getElementById('tableBody');

      // constants
      const PA_PER_PSI = 6894.757293168; // 1 psi in Pa
      const PA_PER_BAR = 100000;

      function getPaPerInHg() {
        return refTemp.value === '32' ? 3386.389 : 3376.85;
      }

      function format(val) {
        if (!isFinite(val)) return '—';
        if (Math.abs(val) < 1e-3 || Math.abs(val) > 1e6) {
          return val.toExponential(6);
        }
        return Number(val.toFixed(6)).toString();
      }

      function updateOutputs() {
        const val = parseFloat(inputValue.value);
        if (isNaN(val)) {
          outPsi.textContent = '—';
          outKpa.textContent = '—';
          outBar.textContent = '—';
          return;
        }
        const paPerInHg = getPaPerInHg();
        let pa;
        if (inputUnit.value === 'inhg') {
          pa = val * paPerInHg;
        } else {
          // psi to inHg
          pa = val * PA_PER_PSI;
        }

        // now outputs should be based on inputUnit?
        // we always show psi/kpa/bar for given pressure
        const psi = pa / PA_PER_PSI;
        const kpa = pa / 1000;
        const bar = pa / PA_PER_BAR;

        outPsi.textContent = format(psi);
        outKpa.textContent = format(kpa);
        outBar.textContent = format(bar);
      }

      inputValue.addEventListener('input', updateOutputs);
      inputUnit.addEventListener('change', updateOutputs);
      refTemp.addEventListener('change', updateOutputs);

      // fill table for 0-30 inHg at 32°F
      const paPerInHg32 = 3386.389;
      for (let i = 0; i <= 30; i += 5) {
        const pa = i * paPerInHg32;
        const psi = pa / PA_PER_PSI;
        const kpa = pa / 1000;
        const tr = document.createElement('tr');
        tr.innerHTML = `
          <td class="text-right">${i}</td>
          <td class="text-right">${psi.toFixed(4)}</td>
          <td class="text-right">${kpa.toFixed(2)}</td>
        `;
        tableBody.appendChild(tr);
      }

      // init
      inputValue.value = 29.92;
      updateOutputs();
    })();
  </script>

