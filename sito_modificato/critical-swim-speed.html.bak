    <script>
      // Utility functions
      function timeToSeconds(min, sec, ms) {
        min = Number(min) || 0;
        sec = Number(sec) || 0;
        ms = Number(ms) || 0;
        return min * 60 + sec + ms / 100;
      }

      function secondsToPace(sec) {
        if (!isFinite(sec) || sec <= 0) return "–";
        const total = Math.round(sec * 100) / 100;
        const m = Math.floor(total / 60);
        const s = Math.floor(total % 60);
        const hs = Math.round((total - Math.floor(total)) * 100);
        const pad = (n) => n.toString().padStart(2, "0");
        if (hs === 0) {
          return m + ":" + pad(s);
        }
        return m + ":" + pad(s) + "." + pad(hs);
      }

      function formatSpeed(value, unit) {
        if (!isFinite(value) || value <= 0) return "–";
        return value.toFixed(3) + " " + unit;
      }

      function roundToNearestStep(sec, step) {
        return Math.round(sec / step) * step;
      }

      function buildZones(cssPer100, unitLabel) {
        const zones = [
          {
            name: "Z1 – Easy / Recovery",
            purpose: "Technique, warm-up, cool-down",
            offset: +10,
          },
          {
            name: "Z2 – Endurance",
            purpose: "Aerobic base, steady swimming",
            offset: +6,
          },
          {
            name: "Z3 – CSS / Threshold",
            purpose: "Lactate threshold, race-pace 1500 m",
            offset: 0,
          },
          {
            name: "Z4 – Tempo / VO₂",
            purpose: "Hard efforts, 200–400 m repeats",
            offset: -6,
          },
          {
            name: "Z5 – Sprint / Speed",
            purpose: "Short sprints, neuromuscular speed",
            offset: -15,
          },
        ];
        const tbody = document.getElementById("zones-body");
        tbody.innerHTML = "";
        zones.forEach((z) => {
          const paceSec = cssPer100 + z.offset;
          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="px-3 py-2 border-b align-top">${z.name}</td>
        <td class="px-3 py-2 border-b align-top text-gray-700">${z.purpose}</td>
        <td class="px-3 py-2 border-b align-top text-gray-700">${
          z.offset === 0
            ? "≈ CSS"
            : z.offset > 0
            ? "Easier than CSS"
            : "Faster than CSS"
        }</td>
        <td class="px-3 py-2 border-b align-top font-semibold">${secondsToPace(
          paceSec
        )} /100 ${unitLabel}</td>
      `;
          tbody.appendChild(tr);
        });
      }

      function buildExampleSet(cssPer100, unitLabel) {
        const list = document.getElementById("example-set");
        const css = cssPer100;
        const easy = css + 8;
        const tempo = css - 4;

        list.innerHTML = `
      <li><strong>Warm-up:</strong> 400 ${unitLabel} easy @ ~${secondsToPace(
          easy
        )} /100 ${unitLabel}, plus drills.</li>
      <li><strong>Main set:</strong> 10 × 100 ${unitLabel} @ CSS (${secondsToPace(
          css
        )} /100 ${unitLabel}), 15–20 s rest.</li>
      <li><strong>Tempo set:</strong> 8 × 50 ${unitLabel} @ ~${secondsToPace(
          tempo
        )} /100 ${unitLabel}, 20–30 s rest.</li>
      <li><strong>Cool-down:</strong> 200–300 ${unitLabel} very easy, focus on relaxed technique.</li>
    `;
      }

      function calculateCSS() {
        const errorEl = document.getElementById("error-msg");
        errorEl.textContent = "";

        const d1 = Number(document.getElementById("dist1").value);
        const d2 = Number(document.getElementById("dist2").value);
        const u1 = document.getElementById("dist1-unit").value;
        const u2 = document.getElementById("dist2-unit").value;

        const t1 = timeToSeconds(
          document.getElementById("t1-min").value,
          document.getElementById("t1-sec").value,
          document.getElementById("t1-ms").value
        );
        const t2 = timeToSeconds(
          document.getElementById("t2-min").value,
          document.getElementById("t2-sec").value,
          document.getElementById("t2-ms").value
        );

        if (
          !d1 ||
          !d2 ||
          !isFinite(t1) ||
          !isFinite(t2) ||
          t1 <= 0 ||
          t2 <= 0
        ) {
          errorEl.textContent =
            "Please enter valid distances and times for both tests.";
          return;
        }
        if (d1 === d2) {
          errorEl.textContent =
            "Distances must be different for the CSS calculation.";
          return;
        }
        if (t1 === t2) {
          errorEl.textContent = "Test times must not be identical.";
          return;
        }

        // Convert distances to a common unit (meters) for internal calculation
        const ydToM = 0.9144;
        const d1m = u1 === "yd" ? d1 * ydToM : d1;
        const d2m = u2 === "yd" ? d2 * ydToM : d2;

        const deltaD = d2m - d1m;
        const deltaT = t2 - t1;

        if (deltaD <= 0 || deltaT <= 0) {
          errorEl.textContent =
            "Longer distance must have a longer time than the shorter distance.";
          return;
        }

        const css_ms = deltaD / deltaT; // m/s

        // Determine primary unit for display based on distance-system or test units
        const system = document.getElementById("distance-system").value;
        let primaryUnit = "m";
        if (system === "imperial") {
          primaryUnit = "yd";
        } else if (system === "auto") {
          // If both tests are yards, use yards; otherwise meters
          primaryUnit = u1 === "yd" && u2 === "yd" ? "yd" : "m";
        }

        const mPer100 = 100;
        const ydPer100 = 91.44; // 100 yd in meters

        const css_per100_m = mPer100 / css_ms; // seconds per 100 m
        const css_per100_yd = ydPer100 / css_ms; // seconds per 100 yd

        // Main pace display based on pace-unit selector
        const paceUnitSel = document.getElementById("pace-unit").value;
        const poolLen =
          Number(document.getElementById("pool-length").value) || 25;
        const poolUnit = document.getElementById("pool-unit").value;
        const poolLenM = poolUnit === "yd" ? poolLen * ydToM : poolLen;

        let mainPaceSec = css_per100_m;
        let mainLabel = "per 100 m";
        let altText = "";

        if (paceUnitSel === "100m") {
          mainPaceSec = css_per100_m;
          mainLabel = "per 100 m";
          altText = "≈ " + secondsToPace(css_per100_yd) + " per 100 yd";
        } else if (paceUnitSel === "100yd") {
          mainPaceSec = css_per100_yd;
          mainLabel = "per 100 yd";
          altText = "≈ " + secondsToPace(css_per100_m) + " per 100 m";
        } else if (paceUnitSel === "pool") {
          const secPerMeter = 1 / css_ms;
          const poolSec = poolLenM * secPerMeter;
          mainPaceSec = poolSec;
          mainLabel = "per " + poolLen + " " + poolUnit;
          altText = "≈ " + secondsToPace(css_per100_m) + " per 100 m";
        }

        document.getElementById("css-main-pace").textContent =
          secondsToPace(mainPaceSec);
        document.getElementById("pace-label-badge").textContent = mainLabel;
        document.getElementById("css-main-pace-alt").textContent = altText;

        // Speed display
        const speed_m_s = css_ms;
        const speed_m_min = css_ms * 60;
        const speed_yd_s = css_ms / ydToM;
        const speed_yd_min = speed_yd_s * 60;

        document.getElementById("css-speed").textContent =
          primaryUnit === "m"
            ? formatSpeed(speed_m_s, "m/s") +
              " (" +
              formatSpeed(speed_m_min, "m/min") +
              ")"
            : formatSpeed(speed_yd_s, "yd/s") +
              " (" +
              formatSpeed(speed_yd_min, "yd/min") +
              ")";

        document.getElementById("css-speed-alt").textContent =
          primaryUnit === "m"
            ? "≈ " +
              formatSpeed(speed_yd_s, "yd/s") +
              " (" +
              formatSpeed(speed_yd_min, "yd/min") +
              ")"
            : "≈ " +
              formatSpeed(speed_m_s, "m/s") +
              " (" +
              formatSpeed(speed_m_min, "m/min") +
              ")";

        // Split paces (always show both metric-based distances)
        const secPerMeter = 1 / css_ms;
        const p25 = 25 * secPerMeter;
        const p50 = 50 * secPerMeter;
        const p100 = 100 * secPerMeter;

        document.getElementById("pace-25").textContent = secondsToPace(p25);
        document.getElementById("pace-50").textContent = secondsToPace(p50);
        document.getElementById("pace-100").textContent = secondsToPace(p100);

        // Build zones and example set based on primary unit
        const cssPer100Primary =
          primaryUnit === "m" ? css_per100_m : css_per100_yd;
        const unitLabel = primaryUnit === "m" ? "m" : "yd";
        buildZones(cssPer100Primary, unitLabel);
        buildExampleSet(cssPer100Primary, unitLabel);

        document.getElementById("results-section").classList.remove("hidden");
      }

      document
        .getElementById("calculate-btn")
        .addEventListener("click", calculateCSS);

      document
        .getElementById("reset-btn")
        .addEventListener("click", function () {
          document.getElementById("dist1").value = 200;
          document.getElementById("dist2").value = 400;
          document.getElementById("dist1-unit").value = "m";
          document.getElementById("dist2-unit").value = "m";
          document.getElementById("t1-min").value = 3;
          document.getElementById("t1-sec").value = 0;
          document.getElementById("t1-ms").value = 0;
          document.getElementById("t2-min").value = 6;
          document.getElementById("t2-sec").value = 20;
          document.getElementById("t2-ms").value = 0;
          document.getElementById("pool-length").value = 25;
          document.getElementById("pool-unit").value = "m";
          document.getElementById("pace-unit").value = "100m";
          document.getElementById("distance-system").value = "auto";
          document.getElementById("error-msg").textContent = "";
          document.getElementById("results-section").classList.add("hidden");
        });

      // Recalculate when pace unit changes, if results already visible
      document
        .getElementById("pace-unit")
        .addEventListener("change", function () {
          if (
            !document
              .getElementById("results-section")
              .classList.contains("hidden")
          ) {
            calculateCSS();
          }
        });

      // Simple FAQ toggle (in addition to global script_faq if present)
      document
        .querySelectorAll("#faq-container .faq-question")
        .forEach((btn) => {
          btn.addEventListener("click", () => {
            const answer = btn.parentElement.querySelector(".faq-answer");
            const icon = btn.querySelector("span:last-child");
            const isHidden = answer.classList.contains("hidden");
            document
              .querySelectorAll("#faq-container .faq-answer")
              .forEach((a) => a.classList.add("hidden"));
            document
              .querySelectorAll("#faq-container .faq-question span:last-child")
              .forEach((i) => (i.textContent = "+"));
            if (isHidden) {
              answer.classList.remove("hidden");
              icon.textContent = "−";
            }
          });
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
