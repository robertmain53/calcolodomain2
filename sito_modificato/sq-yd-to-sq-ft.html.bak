    <script>
      // area conversion factors to square feet as base
      const AREA_TO_SQFT = {
        sq_yd: 9,
        sq_ft: 1,
        sq_m: 10.7639104167,
        sq_in: 1/144,
        acre: 43560,
        sq_cm: 0.00107639104167,
        gaj: 9 // treat gaj ≈ sq yd
      };

      const areaValue = document.getElementById('area-value');
      const areaUnit = document.getElementById('area-unit');

      const resSqYd = document.getElementById('res-sq-yd');
      const resSqFt = document.getElementById('res-sq-ft');
      const resSqM = document.getElementById('res-sq-m');
      const resSqIn = document.getElementById('res-sq-in');
      const resAcre = document.getElementById('res-acre');
      const resSqCm = document.getElementById('res-sq-cm');

      function updateArea() {
        const val = parseFloat(areaValue.value || 0);
        const unit = areaUnit.value;
        const inSqFt = val * AREA_TO_SQFT[unit];

        // convert back to other units
        const sqYd = inSqFt / AREA_TO_SQFT['sq_yd'];
        const sqM = inSqFt / AREA_TO_SQFT['sq_m'];
        const sqIn = inSqFt / AREA_TO_SQFT['sq_in'];
        const acre = inSqFt / AREA_TO_SQFT['acre'];
        const sqCm = inSqFt / AREA_TO_SQFT['sq_cm'];

        resSqFt.textContent = inSqFt.toFixed(6).replace(/\.?0+$/,"");
        resSqYd.textContent = sqYd.toFixed(6).replace(/\.?0+$/,"");
        resSqM.textContent = sqM.toFixed(6).replace(/\.?0+$/,"");
        resSqIn.textContent = sqIn.toFixed(6).replace(/\.?0+$/,"");
        resAcre.textContent = acre.toFixed(10).replace(/\.?0+$/,"");
        resSqCm.textContent = sqCm.toFixed(2).replace(/\.?0+$/,"");
      }

      areaValue.addEventListener('input', updateArea);
      areaUnit.addEventListener('input', updateArea);
      updateArea();
    </script>

