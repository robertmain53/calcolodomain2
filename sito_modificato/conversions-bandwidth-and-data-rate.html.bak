      const fileSizeUnit = document.getElementById('fileSizeUnit');
      const bandwidthInput = document.getElementById('bandwidthInput');
      const transferTime = document.getElementById('transferTime');
      const rateTableBody = document.getElementById('rateTableBody');

      // base unit: bit per second
      const RATE_FACTORS = {
        bps: 1,
        kbps: 1e3,
        Mbps: 1e6,
        Gbps: 1e9,
        Tbps: 1e12,
        Bps: 8,        // 1 B/s = 8 bps
        KBps: 8e3,
        MBps: 8e6,
        GBps: 8e9
      };

      function format(val) {
        if (!isFinite(val)) return '—';
        if (val === 0) return '0';
        if (val >= 1e9 || val <= 1e-3) {
          return val.toExponential(4);
        }
        return parseFloat(val.toFixed(6)).toString();
      }

      function updateRateOutputs() {
        const val = parseFloat(rateInput.value);
        const unit = unitSelect.value;
        if (isNaN(val)) {
          outMbps.textContent = '—';
          outMBps.textContent = '—';
          outGbps.textContent = '—';
          return;
        }
        const bps = val * (RATE_FACTORS[unit] || 1);
        const mbps = bps / 1e6;
        const mbpsBytes = mbps / 8; // MB/s
        const gbps = bps / 1e9;

        outMbps.textContent = format(mbps);
        outMBps.textContent = format(mbpsBytes);
        outGbps.textContent = format(gbps);
      }

      function formatDuration(seconds) {
        if (!isFinite(seconds) || seconds <= 0) return '—';
        const hrs = Math.floor(seconds / 3600);
        const mins = Math.floor((seconds % 3600) / 60);
        const secs = Math.floor(seconds % 60);
        const parts = [];
        if (hrs) parts.push(hrs + 'h');
        if (mins) parts.push(mins + 'm');
        parts.push(secs + 's');
        return parts.join(' ');
      }

      function updateTransferTime() {
        const size = parseFloat(fileSizeInput.value);
        const sizeUnit = fileSizeUnit.value;
        const bwMbps = parseFloat(bandwidthInput.value);
        if (isNaN(size) || isNaN(bwMbps) || bwMbps <= 0) {
          transferTime.textContent = '—';
          return;
        }
        // convert file size to bits
        let sizeInBits;
        if (sizeUnit === 'MB') {
          sizeInBits = size * 8 * 1e6; // 1 MB = 8,000,000 bits
        } else if (sizeUnit === 'GB') {
          sizeInBits = size * 8 * 1e9;
        } else {
          // TB
          sizeInBits = size * 8 * 1e12;
        }
        // bandwidth in bps
        const bps = bwMbps * 1e6;
        const seconds = sizeInBits / bps;
        transferTime.textContent = formatDuration(seconds);
      }

      rateInput.addEventListener('input', function() {
        updateRateOutputs();
        updateTransferTime();
      });
      unitSelect.addEventListener('change', function() {
        updateRateOutputs();
      });

      fileSizeInput.addEventListener('input', updateTransferTime);
      fileSizeUnit.addEventListener('change', updateTransferTime);
      bandwidthInput.addEventListener('input', updateTransferTime);

      // populate table
      const sample = [
        { label: '10 Mbps', val: 10, unit: 'Mbps' },
        { label: '25 Mbps', val: 25, unit: 'Mbps' },
        { label: '100 Mbps', val: 100, unit: 'Mbps' },
        { label: '1 Gbps', val: 1, unit: 'Gbps' },
        { label: '500 MB/s', val: 500, unit: 'MBps' }
      ];
      sample.forEach(item => {
        const bps = item.val * (RATE_FACTORS[item.unit] || 1);
        const mbps = bps / 1e6;
        const mbpsBytes = mbps / 8;
        const gbps = bps / 1e9;
        const tr = document.createElement('tr');
        tr.innerHTML = `
          <td>${item.label}</td>
          <td>${format(mbps)}</td>
          <td>${format(mbpsBytes)}</td>
          <td>${format(gbps)}</td>
        `;
        rateTableBody.appendChild(tr);
      });

      // initial
      rateInput.value = 100;
      updateRateOutputs();
      updateTransferTime();
    })();
  </script>

