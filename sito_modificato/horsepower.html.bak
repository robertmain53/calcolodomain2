    <script>
      // Tab switching
      document.querySelectorAll(".hp-tab-btn").forEach((btn) => {
        btn.addEventListener("click", () => {
          const target = btn.getAttribute("data-tab");
          document.querySelectorAll(".hp-tab-btn").forEach((b) => {
            b.classList.remove("border-blue-600", "text-blue-600");
            b.classList.add("border-transparent", "text-gray-600");
          });
          btn.classList.add("border-blue-600", "text-blue-600");
          btn.classList.remove("text-gray-600");

          document.querySelectorAll(".hp-tab-panel").forEach((panel) => {
            panel.classList.add("hidden");
          });
          document.getElementById("tab-" + target).classList.remove("hidden");
        });
      });

      // Utility: parse number safely
      function parseVal(id) {
        const v = parseFloat(document.getElementById(id).value);
        return isNaN(v) ? null : v;
      }

      // HP ↔ Power conversions
      function hpToWatts(hp, unit) {
        if (unit === "hp") return hp * 745.7; // mechanical
        if (unit === "ps") return hp * 735.5; // metric
        if (unit === "hp_electric") return hp * 746; // electric hp
        return hp * 745.7;
      }

      function wattsToHp(watts, unit) {
        if (unit === "hp") return watts / 745.7;
        if (unit === "ps") return watts / 735.5;
        if (unit === "hp_electric") return watts / 746;
        return watts / 745.7;
      }

      document.getElementById("btnHpToPower").addEventListener("click", () => {
        const hp = parseVal("hpValue");
        const hpUnit = document.getElementById("hpUnit").value;
        const powerUnit = document.getElementById("powerUnit").value;
        const out = document.getElementById("hpPowerResult");

        if (hp === null) {
          out.textContent = "Enter a horsepower value first.";
          out.classList.add("text-red-600");
          return;
        }
        out.classList.remove("text-red-600");

        let watts = hpToWatts(hp, hpUnit);
        let result;
        if (powerUnit === "kW") {
          result = watts / 1000;
        } else {
          result = watts;
        }

        document.getElementById("powerKw").value = result
          .toFixed(4)
          .replace(/\.?0+$/, "");
        const unitLabel = powerUnit === "kW" ? "kW" : "W";
        out.textContent = `${hp} ${
          hpUnit === "ps"
            ? "PS"
            : hpUnit === "hp_electric"
            ? "hp (electric)"
            : "hp"
        } ≈ ${result.toFixed(3)} ${unitLabel}`;
      });

      document.getElementById("btnPowerToHp").addEventListener("click", () => {
        const power = parseVal("powerKw");
        const powerUnit = document.getElementById("powerUnit").value;
        const hpUnit = document.getElementById("hpUnit").value;
        const out = document.getElementById("hpPowerResult");

        if (power === null) {
          out.textContent = "Enter a power value first.";
          out.classList.add("text-red-600");
          return;
        }
        out.classList.remove("text-red-600");

        let watts = powerUnit === "kW" ? power * 1000 : power;
        const hp = wattsToHp(watts, hpUnit);

        document.getElementById("hpValue").value = hp
          .toFixed(4)
          .replace(/\.?0+$/, "");
        const unitLabel =
          hpUnit === "ps"
            ? "PS"
            : hpUnit === "hp_electric"
            ? "hp (electric)"
            : "hp";
        out.textContent = `${power} ${powerUnit} ≈ ${hp.toFixed(
          3
        )} ${unitLabel}`;
      });

      document
        .getElementById("btnHpPowerClear")
        .addEventListener("click", () => {
          ["hpValue", "powerKw"].forEach(
            (id) => (document.getElementById(id).value = "")
          );
          document.getElementById("hpPowerResult").textContent = "";
        });

      // Torque & RPM ↔ HP
      document
        .getElementById("btnCalcTorqueHp")
        .addEventListener("click", () => {
          const torque = parseVal("torqueValue");
          const torqueUnit = document.getElementById("torqueUnit").value;
          const rpm = parseVal("rpmValue");
          const hpVal = parseVal("hpFromTorque");
          const hpUnit = document.getElementById("hpFromTorqueUnit").value;
          const solveFor = document.getElementById("solveFor").value;
          const out = document.getElementById("hpTorqueResult");

          out.classList.remove("text-red-600");

          // Convert torque to lb·ft or N·m as needed
          let torqueLbft = null;
          let torqueNm = null;
          if (torque !== null) {
            if (torqueUnit === "lbft") {
              torqueLbft = torque;
              torqueNm = torque * 1.355817948; // 1 lb·ft = 1.3558 N·m
            } else {
              torqueNm = torque;
              torqueLbft = torque / 1.355817948;
            }
          }

          if (solveFor === "hp") {
            if (torqueLbft === null || rpm === null) {
              out.textContent =
                "Enter both torque and RPM to calculate horsepower.";
              out.classList.add("text-red-600");
              return;
            }
            const hp = (torqueLbft * rpm) / 5252;
            const kW = hp * 0.7457;
            document.getElementById("hpFromTorque").value = (
              hpUnit === "kW" ? kW : hp
            )
              .toFixed(4)
              .replace(/\.?0+$/, "");
            out.textContent = `Power ≈ ${hp.toFixed(2)} hp (${kW.toFixed(
              2
            )} kW).`;
          } else if (solveFor === "torque") {
            if (hpVal === null || rpm === null) {
              out.textContent =
                "Enter horsepower (or kW) and RPM to calculate torque.";
              out.classList.add("text-red-600");
              return;
            }
            let hp;
            if (hpUnit === "kW") {
              hp = hpVal * 1.34102209;
            } else {
              hp = hpVal;
            }
            const torqueLb = (hp * 5252) / rpm;
            const torqueNmOut = torqueLb * 1.355817948;
            const displayTorque =
              torqueUnit === "lbft" ? torqueLb : torqueNmOut;
            document.getElementById("torqueValue").value = displayTorque
              .toFixed(4)
              .replace(/\.?0+$/, "");
            const unitLabel = torqueUnit === "lbft" ? "lb·ft" : "N·m";
            out.textContent = `Torque ≈ ${displayTorque.toFixed(
              2
            )} ${unitLabel}.`;
          } else if (solveFor === "rpm") {
            if (hpVal === null || torqueLbft === null) {
              out.textContent =
                "Enter horsepower (or kW) and torque to calculate RPM.";
              out.classList.add("text-red-600");
              return;
            }
            let hp;
            if (hpUnit === "kW") {
              hp = hpVal * 1.34102209;
            } else {
              hp = hpVal;
            }
            const rpmOut = (hp * 5252) / torqueLbft;
            document.getElementById("rpmValue").value = rpmOut.toFixed(1);
            out.textContent = `Engine speed ≈ ${rpmOut.toFixed(0)} RPM.`;
          }
        });

      document
        .getElementById("btnHpTorqueClear")
        .addEventListener("click", () => {
          ["torqueValue", "rpmValue", "hpFromTorque"].forEach(
            (id) => (document.getElementById(id).value = "")
          );
          document.getElementById("hpTorqueResult").textContent = "";
        });

      // Mobile menu script (simple)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", () => {
          const menu = document.getElementById("mobile-menu");
          const expanded = menu.classList.contains("hidden") ? "true" : "false";
          menu.classList.toggle("hidden");
          document
            .getElementById("mobile-menu-toggle")
            .setAttribute("aria-expanded", expanded);
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
