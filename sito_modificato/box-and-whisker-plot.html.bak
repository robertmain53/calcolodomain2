      tabSummary.addEventListener("click", () => activateTab("summary"));

      // Sample data
      document
        .getElementById("btn-sample-data")
        .addEventListener("click", () => {
          const sample = [
            "Class A: 55, 61, 63, 65, 68, 70, 72, 75, 78, 80",
            "Class B: 50, 52, 54, 57, 60, 62, 64, 66, 68, 71",
            "Class C: 58, 60, 62, 64, 67, 69, 71, 73, 76, 79",
          ].join("\n");
          document.getElementById("raw-data").value = sample;
        });

      document.getElementById("btn-clear-raw").addEventListener("click", () => {
        document.getElementById("raw-data").value = "";
      });

      // Parsing helpers
      function detectDelimiter(text) {
        const counts = {
          ",": (text.match(/,/g) || []).length,
          ";": (text.match(/;/g) || []).length,
          " ": (text.match(/ /g) || []).length,
          "\t": (text.match(/\t/g) || []).length,
        };
        let best = ",";
        let max = -1;
        for (const [delim, count] of Object.entries(counts)) {
          if (count > max) {
            max = count;
            best = delim;
          }
        }
        return best;
      }

      function parseLineToNumbers(line, delimiterMode) {
        if (!line.trim()) return [];
        let label = null;
        let dataPart = line;
        const colonIndex = line.indexOf(":");
        if (colonIndex !== -1) {
          label = line.slice(0, colonIndex).trim();
          dataPart = line.slice(colonIndex + 1);
        }
        let delim = delimiterMode;
        if (delimiterMode === "auto") {
          delim = detectDelimiter(dataPart);
        } else if (delimiterMode === "space") {
          delim = " ";
        } else if (delimiterMode === "tab") {
          delim = "\t";
        } else if (delimiterMode === "newline") {
          // already one per line
          delim = null;
        }
        let tokens;
        if (delim === null) {
          tokens = [dataPart];
        } else if (delim === " ") {
          tokens = dataPart.trim().split(/[\s,;]+/);
        } else {
          tokens = dataPart.split(delim);
        }
        const nums = [];
        for (const t of tokens) {
          const v = parseFloat(t);
          if (!isNaN(v)) nums.push(v);
        }
        return { label, values: nums };
      }

      function computeQuartiles(sorted) {
        const n = sorted.length;
        if (n === 0) return null;
        const median = (arr) => {
          const m = arr.length;
          if (m === 0) return NaN;
          const mid = Math.floor(m / 2);
          return m % 2 === 0 ? (arr[mid - 1] + arr[mid]) / 2 : arr[mid];
        };
        const Q2 = median(sorted);
        let lower, upper;
        if (n % 2 === 0) {
          lower = sorted.slice(0, n / 2);
          upper = sorted.slice(n / 2);
        } else {
          lower = sorted.slice(0, Math.floor(n / 2));
          upper = sorted.slice(Math.floor(n / 2) + 1);
        }
        const Q1 = median(lower);
        const Q3 = median(upper);
        return { Q1, Q2, Q3 };
      }

      function computeStats(values, outlierRule) {
        if (!values || values.length === 0) return null;
        const sorted = [...values].sort((a, b) => a - b);
        const n = sorted.length;
        const min = sorted[0];
        const max = sorted[n - 1];
        const { Q1, Q2, Q3 } = computeQuartiles(sorted);
        const IQR = Q3 - Q1;
        const mean = sorted.reduce((a, b) => a + b, 0) / n;

        let whiskerMin = min;
        let whiskerMax = max;
        const outliers = [];

        if (outlierRule !== "none" && IQR >= 0) {
          const k = parseFloat(outlierRule);
          const lowerFence = Q1 - k * IQR;
          const upperFence = Q3 + k * IQR;
          let lowIndex = 0;
          while (lowIndex < n && sorted[lowIndex] < lowerFence) {
            outliers.push(sorted[lowIndex]);
            lowIndex++;
          }
          let highIndex = n - 1;
          while (highIndex >= 0 && sorted[highIndex] > upperFence) {
            outliers.push(sorted[highIndex]);
            highIndex--;
          }
          whiskerMin = sorted[lowIndex] ?? min;
          whiskerMax = sorted[highIndex] ?? max;
        }

        return {
          min,
          max,
          Q1,
          median: Q2,
          Q3,
          IQR,
          mean,
          whiskerMin,
          whiskerMax,
          outliers,
          n,
        };
      }

      let chartInstance = null;

      function renderBoxplotFromRaw(datasets, outlierRule, showMean) {
        const labels = [];
        const statsList = [];
        const outlierPoints = [];
        const colors = [
          "#2563eb",
          "#16a34a",
          "#f97316",
          "#7c3aed",
          "#dc2626",
          "#0d9488",
        ];

        datasets.forEach((ds, idx) => {
          const label = ds.label || `Series ${idx + 1}`;
          const stats = computeStats(ds.values, outlierRule);
          if (!stats) return;
          labels.push(label);
          statsList.push({ label, stats });

          // Collect outlier points
          stats.outliers.forEach((v) => {
            outlierPoints.push({ x: label, y: v });
          });
        });

        if (labels.length === 0) {
          updateStatus(
            "No valid numeric data found. Please check your input.",
            true
          );
          return;
        }

        const ctx = document.getElementById("boxplot-canvas").getContext("2d");
        if (chartInstance) chartInstance.destroy();

        const datasetsChart = [];

        // Box & whisker as custom bar-like dataset
        statsList.forEach((item, idx) => {
          const color = colors[idx % colors.length];
          datasetsChart.push({
            type: "bar",
            label: item.label,
            data: [
              {
                x: item.label,
                y: [
                  item.stats.whiskerMin,
                  item.stats.Q1,
                  item.stats.median,
                  item.stats.Q3,
                  item.stats.whiskerMax,
                ],
              },
            ],
            backgroundColor: color + "33",
            borderColor: color,
            borderWidth: 1.5,
            barPercentage: 0.6,
            categoryPercentage: 0.8,
          });
        });

        // Outliers as scatter
        if (outlierPoints.length > 0) {
          datasetsChart.push({
            type: "scatter",
            label: "Outliers",
            data: outlierPoints,
            backgroundColor: "#ef4444",
            pointRadius: 3,
            pointHoverRadius: 4,
            showLine: false,
          });
        }

        // Mean markers
        if (showMean) {
          const meanPoints = statsList.map((item) => ({
            x: item.label,
            y: item.stats.mean,
          }));
          datasetsChart.push({
            type: "scatter",
            label: "Mean",
            data: meanPoints,
            backgroundColor: "#111827",
            pointStyle: "crossRot",
            pointRadius: 4,
            pointHoverRadius: 5,
            showLine: false,
          });
        }

        chartInstance = new Chart(ctx, {
          data: { datasets: datasetsChart },
          options: {
            responsive: true,
            maintainAspectRatio: false,
            parsing: false,
            plugins: {
              legend: { display: true },
              tooltip: {
                callbacks: {
                  label: function (context) {
                    const ds = context.dataset;
                    const v =
                      context.raw && context.raw.y !== undefined
                        ? context.raw.y
                        : context.parsed.y;
                    if (ds.type === "scatter" && ds.label === "Outliers") {
                      return `Outlier: ${v}`;
                    }
                    if (ds.type === "scatter" && ds.label === "Mean") {
                      return `Mean: ${v.toFixed(3)}`;
                    }
                    if (Array.isArray(context.raw.y)) {
                      const [wMin, Q1, med, Q3, wMax] = context.raw.y;
                      return [
                        `Min (whisker): ${wMin}`,
                        `Q1: ${Q1}`,
                        `Median: ${med}`,
                        `Q3: ${Q3}`,
                        `Max (whisker): ${wMax}`,
                      ];
                    }
                    return `${ds.label}: ${v}`;
                  },
                },
              },
            },
            scales: {
              x: {
                type: "category",
                title: { display: true, text: "Dataset" },
              },
              y: {
                type: "linear",
                title: { display: true, text: "Value" },
                ticks: { precision: 2 },
              },
            },
          },
        });

        renderStatsPanel(statsList, true);
        updateStatus(
          `Generated ${labels.length} boxplot${
            labels.length > 1 ? "s" : ""
          } from raw data.`,
          false
        );
      }

      function renderBoxplotFromSummary(seriesList) {
        const labels = [];
        const datasetsChart = [];
        const colors = [
          "#2563eb",
          "#16a34a",
          "#f97316",
          "#7c3aed",
          "#dc2626",
          "#0d9488",
        ];

        seriesList.forEach((s, idx) => {
          labels.push(s.label);
          const color = colors[idx % colors.length];
          datasetsChart.push({
            type: "bar",
            label: s.label,
            data: [
              {
                x: s.label,
                y: [s.min, s.Q1, s.median, s.Q3, s.max],
              },
            ],
            backgroundColor: color + "33",
            borderColor: color,
            borderWidth: 1.5,
            barPercentage: 0.6,
            categoryPercentage: 0.8,
          });
        });

        const ctx = document.getElementById("boxplot-canvas").getContext("2d");
        if (chartInstance) chartInstance.destroy();

        chartInstance = new Chart(ctx, {
          data: { datasets: datasetsChart },
          options: {
            responsive: true,
            maintainAspectRatio: false,
            parsing: false,
            plugins: {
              legend: { display: true },
              tooltip: {
                callbacks: {
                  label: function (context) {
                    if (Array.isArray(context.raw.y)) {
                      const [min, Q1, med, Q3, max] = context.raw.y;
                      return [
                        `Min: ${min}`,
                        `Q1: ${Q1}`,
                        `Median: ${med}`,
                        `Q3: ${Q3}`,
                        `Max: ${max}`,
                      ];
                    }
                    return `${context.dataset.label}: ${context.parsed.y}`;
                  },
                },
              },
            },
            scales: {
              x: {
                type: "category",
                title: { display: true, text: "Dataset" },
              },
              y: {
                type: "linear",
                title: { display: true, text: "Value" },
                ticks: { precision: 2 },
              },
            },
          },
        });

        const statsList = seriesList.map((s) => ({
          label: s.label,
          stats: {
            min: s.min,
            max: s.max,
            Q1: s.Q1,
            median: s.median,
            Q3: s.Q3,
            IQR: s.Q3 - s.Q1,
            n: null,
          },
        }));
        renderStatsPanel(statsList, false);
        updateStatus(
          `Generated ${labels.length} boxplot${
            labels.length > 1 ? "s" : ""
          } from summary statistics.`,
          false
        );
      }

      function renderStatsPanel(statsList, includeN) {
        const panel = document.getElementById("stats-panel");
        const content = document.getElementById("stats-content");
        content.innerHTML = "";
        statsList.forEach((item) => {
          const s = item.stats;
          const div = document.createElement("div");
          div.innerHTML = `
        <div class="font-semibold">${item.label}</div>
        <div class="grid grid-cols-2 md:grid-cols-3 gap-x-4 text-gray-700">
          <div>Min: <span class="font-mono">${
            s.min.toFixed ? s.min.toFixed(3) : s.min
          }</span></div>
          <div>Q1: <span class="font-mono">${
            s.Q1.toFixed ? s.Q1.toFixed(3) : s.Q1
          }</span></div>
          <div>Median: <span class="font-mono">${
            s.median.toFixed ? s.median.toFixed(3) : s.median
          }</span></div>
          <div>Q3: <span class="font-mono">${
            s.Q3.toFixed ? s.Q3.toFixed(3) : s.Q3
          }</span></div>
          <div>Max: <span class="font-mono">${
            s.max.toFixed ? s.max.toFixed(3) : s.max
          }</span></div>
          <div>IQR: <span class="font-mono">${
            s.IQR.toFixed ? s.IQR.toFixed(3) : s.IQR
          }</span></div>
          ${
            includeN && s.n != null
              ? `<div>n: <span class="font-mono">${s.n}</span></div>`
              : ""
          }
        </div>
      `;
          content.appendChild(div);
        });
        panel.classList.remove("hidden");
      }

      function updateStatus(msg, isError) {
        const el = document.getElementById("status-message");
        el.textContent = msg;
        el.classList.toggle("text-red-600", !!isError);
        el.classList.toggle("text-gray-500", !isError);
      }

      // Generate from raw data
      document
        .getElementById("btn-generate-raw")
        .addEventListener("click", () => {
          const text = document.getElementById("raw-data").value;
          const delimiterMode = document.getElementById("delimiter").value;
          const outlierRule = document.getElementById("outlier-rule").value;
          const showMean = document.getElementById("show-mean").checked;

          const lines = text.split(/\r?\n/);
          const datasets = [];
          let nonNumericCount = 0;

          lines.forEach((line) => {
            if (!line.trim()) return;
            const parsed = parseLineToNumbers(line, delimiterMode);
            if (parsed.values.length === 0) {
              nonNumericCount++;
              return;
            }
            datasets.push(parsed);
          });

          if (datasets.length === 0) {
            updateStatus(
              "No valid numeric data found. Please enter at least one dataset.",
              true
            );
            return;
          }
          if (nonNumericCount > 0) {
            updateStatus(
              `Ignored ${nonNumericCount} line(s) with no numeric values.`,
              true
            );
          } else {
            updateStatus("", false);
          }

          renderBoxplotFromRaw(datasets, outlierRule, showMean);
        });

      // Summary statistics mode: add/remove rows
      document
        .getElementById("btn-add-series")
        .addEventListener("click", () => {
          const tbody = document.getElementById("summary-tbody");
          const idx = tbody.children.length + 1;
          const tr = document.createElement("tr");
          tr.innerHTML = `
      <td class="px-2 py-1 border-b"><input type="text" class="w-full border border-gray-200 rounded px-1 py-0.5" value="Series ${idx}"></td>
      <td class="px-2 py-1 border-b text-right"><input type="number" step="any" class="w-20 border border-gray-200 rounded px-1 py-0.5 text-right"></td>
      <td class="px-2 py-1 border-b text-right"><input type="number" step="any" class="w-20 border border-gray-200 rounded px-1 py-0.5 text-right"></td>
      <td class="px-2 py-1 border-b text-right"><input type="number" step="any" class="w-20 border border-gray-200 rounded px-1 py-0.5 text-right"></td>
      <td class="px-2 py-1 border-b text-right"><input type="number" step="any" class="w-20 border border-gray-200 rounded px-1 py-0.5 text-right"></td>
      <td class="px-2 py-1 border-b text-right"><input type="number" step="any" class="w-20 border border-gray-200 rounded px-1 py-0.5 text-right"></td>
      <td class="px-2 py-1 border-b text-center">
        <button type="button" class="text-red-500 text-xs btn-remove-row">✕</button>
      </td>
    `;
          tbody.appendChild(tr);
        });

      document
        .getElementById("summary-tbody")
        .addEventListener("click", (e) => {
          if (e.target.classList.contains("btn-remove-row")) {
            const tbody = document.getElementById("summary-tbody");
            if (tbody.children.length > 1) {
              e.target.closest("tr").remove();
            }
          }
        });

      document
        .getElementById("btn-generate-summary")
        .addEventListener("click", () => {
          const tbody = document.getElementById("summary-tbody");
          const rows = Array.from(tbody.querySelectorAll("tr"));
          const seriesList = [];
          for (const row of rows) {
            const inputs = row.querySelectorAll("input");
            const label =
              inputs[0].value.trim() || `Series ${seriesList.length + 1}`;
            const nums = Array.from(inputs)
              .slice(1)
              .map((i) => parseFloat(i.value));
            if (nums.some((v) => isNaN(v))) {
              updateStatus(
                "Please fill in all numeric fields (min, Q1, median, Q3, max) for each series.",
                true
              );
              return;
            }
            const [min, Q1, median, Q3, max] = nums;
            if (!(min <= Q1 && Q1 <= median && median <= Q3 && Q3 <= max)) {
              updateStatus(
                `Invalid order in series "${label}". Ensure min ≤ Q1 ≤ median ≤ Q3 ≤ max.`,
                true
              );
              return;
            }
            seriesList.push({ label, min, Q1, median, Q3, max });
          }
          if (seriesList.length === 0) {
            updateStatus("Please add at least one series.", true);
            return;
          }
          updateStatus("", false);
          renderBoxplotFromSummary(seriesList);
        });

      // Download PNG
      document.getElementById("btn-download").addEventListener("click", () => {
        if (!chartInstance) {
          updateStatus("Generate a boxplot before downloading.", true);
          return;
        }
        const link = document.createElement("a");
        link.href = chartInstance.toBase64Image("image/png", 1);
        link.download = "box-and-whisker-plot.png";
        link.click();
      });
    </script>

