    <script>
      const inputEl = document.getElementById('energy-value');
      const fromEl = document.getElementById('energy-from');
      const btn = document.getElementById('convert-btn');

      const resKwh = document.getElementById('res-kwh');
      const resWh = document.getElementById('res-wh');
      const resJ = document.getElementById('res-j');
      const resKj = document.getElementById('res-kj');
      const resMj = document.getElementById('res-mj');
      const resSteps = document.getElementById('res-steps');

      const KWH_TO_J = 3600000; // 1 kWh = 3,600,000 J

      function convertEnergy() {
        const val = parseFloat(inputEl.value);
        const from = fromEl.value;

        if (isNaN(val)) {
          resKwh.textContent = '—';
          resWh.textContent = '—';
          resJ.textContent = '—';
          resKj.textContent = '—';
          resMj.textContent = '—';
          resSteps.textContent = 'Please enter a valid number.';
          return;
        }

        let joules = 0;
        let steps = '';

        if (from === 'kWh') {
          joules = val * KWH_TO_J;
          steps += `Given: ${val} kWh\n`;
          steps += `Convert kWh → J: J = kWh × 3,600,000 = ${val} × 3,600,000 = ${joules} J\n`;
        } else if (from === 'Wh') {
          // 1 Wh = 3600 J
          joules = val * 3600;
          steps += `Given: ${val} Wh\n`;
          steps += `Convert Wh → J: J = Wh × 3600 = ${val} × 3600 = ${joules} J\n`;
        } else if (from === 'J') {
          joules = val;
          steps += `Given: ${val} J\n`;
        } else if (from === 'kJ') {
          joules = val * 1000;
          steps += `Given: ${val} kJ\n`;
          steps += `Convert kJ → J: J = kJ × 1000 = ${val} × 1000 = ${joules} J\n`;
        } else if (from === 'MJ') {
          joules = val * 1000000;
          steps += `Given: ${val} MJ\n`;
          steps += `Convert MJ → J: J = MJ × 1,000,000 = ${val} × 1,000,000 = ${joules} J\n`;
        }

        const kwh = joules / KWH_TO_J;
        const wh = joules / 3600;
        const kj = joules / 1000;
        const mj = joules / 1000000;

        resKwh.textContent = kwh.toFixed(6).replace(/\.?0+$/,'');
        resWh.textContent = wh.toFixed(6).replace(/\.?0+$/,'');
        resJ.textContent = joules.toFixed(6).replace(/\.?0+$/,'');
        resKj.textContent = kj.toFixed(6).replace(/\.?0+$/,'');
        resMj.textContent = mj.toFixed(6).replace(/\.?0+$/,'');

        steps += `\nNow convert J → other units:\n`;
        steps += `Wh = J ÷ 3600 = ${joules} ÷ 3600 = ${wh}\n`;
        steps += `kWh = J ÷ 3,600,000 = ${joules} ÷ 3,600,000 = ${kwh}\n`;
        steps += `kJ = J ÷ 1000 = ${joules} ÷ 1000 = ${kj}\n`;
        steps += `MJ = J ÷ 1,000,000 = ${joules} ÷ 1,000,000 = ${mj}\n`;

        resSteps.textContent = steps;
      }

      btn.addEventListener('click', convertEnergy);
      convertEnergy();
    </script>

