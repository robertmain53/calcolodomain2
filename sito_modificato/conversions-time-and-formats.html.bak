<script>
(function() {
  // Tab handling
  const tabs = {
    'tab-duration': 'panel-duration',
    'tab-datetime': 'panel-datetime',
    'tab-unix': 'panel-unix'
  };
  Object.keys(tabs).forEach(tabId => {
    const tab = document.getElementById(tabId);
    const panelId = tabs[tabId];
    tab.addEventListener('click', () => {
      Object.keys(tabs).forEach(id => {
        const t = document.getElementById(id);
        const p = document.getElementById(tabs[id]);
        if (id === tabId) {
          t.classList.add('tab-active');
          p.classList.remove('hidden');
        } else {
          t.classList.remove('tab-active');
          p.classList.add('hidden');
        }
      });
    });
  });

  // Helper: clamp to finite number
  function toNumber(val) {
    const n = Number(val);
    return Number.isFinite(n) ? n : 0;
  }

  // --- Duration logic ---
  let updatingDuration = false;
  function setDurationFromSeconds(totalSeconds) {
    if (updatingDuration) return;
    updatingDuration = true;
    if (!Number.isFinite(totalSeconds)) totalSeconds = 0;
    const sign = totalSeconds < 0 ? -1 : 1;
    totalSeconds = Math.abs(totalSeconds);

    const hours = Math.floor(totalSeconds / 3600);
    const minutes = Math.floor((totalSeconds - hours * 3600) / 60);
    const seconds = Math.round(totalSeconds - hours * 3600 - minutes * 60);

    document.getElementById('dur-total-seconds').value = sign * (hours * 3600 + minutes * 60 + seconds);
    document.getElementById('dur-hours').value = sign * hours;
    document.getElementById('dur-minutes').value = minutes;
    document.getElementById('dur-seconds').value = seconds;

    const hms = `${sign < 0 ? '-' : ''}${hours}:${String(minutes).padStart(2,'0')}:${String(seconds).padStart(2,'0')}`;
    document.getElementById('dur-hms').value = hms;

    const totalMinutes = (sign * (hours * 3600 + minutes * 60 + seconds)) / 60;
    const totalHours = totalMinutes / 60;
    const totalDays = totalHours / 24;
    document.getElementById('dur-minutes-total').value = totalMinutes.toFixed(4).replace(/\.?0+$/,'');
    document.getElementById('dur-hours-total').value = totalHours.toFixed(4).replace(/\.?0+$/,'');
    document.getElementById('dur-days-total').value = totalDays.toFixed(6).replace(/\.?0+$/,'');

    const decimalHours = totalSeconds / 3600 * sign;
    document.getElementById('dur-decimal-hours').value = decimalHours.toFixed(4).replace(/\.?0+$/,'');

    updatingDuration = false;
  }

  function durationFromHMSString(str) {
    str = String(str).trim();
    if (!str) return 0;
    let sign = 1;
    if (str.startsWith('-')) {
      sign = -1;
      str = str.slice(1);
    }
    const parts = str.split(':').map(p => p.trim());
    if (parts.length === 1) {
      return sign * toNumber(parts[0]);
    }
    if (parts.length === 2) {
      const h = toNumber(parts[0]);
      const m = toNumber(parts[1]);
      return sign * (h * 3600 + m * 60);
    }
    const h = toNumber(parts[0]);
    const m = toNumber(parts[1]);
    const s = toNumber(parts[2]);
    return sign * (h * 3600 + m * 60 + s);
  }

  // Duration event bindings
  document.getElementById('dur-total-seconds').addEventListener('input', e => {
    setDurationFromSeconds(toNumber(e.target.value));
  });
  document.getElementById('dur-hours').addEventListener('input', () => {
    const h = toNumber(document.getElementById('dur-hours').value);
    const m = toNumber(document.getElementById('dur-minutes').value);
    const s = toNumber(document.getElementById('dur-seconds').value);
    setDurationFromSeconds(h * 3600 + m * 60 + s);
  });
  document.getElementById('dur-minutes').addEventListener('input', () => {
    const h = toNumber(document.getElementById('dur-hours').value);
    const m = toNumber(document.getElementById('dur-minutes').value);
    const s = toNumber(document.getElementById('dur-seconds').value);
    setDurationFromSeconds(h * 3600 + m * 60 + s);
  });
  document.getElementById('dur-seconds').addEventListener('input', () => {
    const h = toNumber(document.getElementById('dur-hours').value);
    const m = toNumber(document.getElementById('dur-minutes').value);
    const s = toNumber(document.getElementById('dur-seconds').value);
    setDurationFromSeconds(h * 3600 + m * 60 + s);
  });
  document.getElementById('dur-hms').addEventListener('input', e => {
    setDurationFromSeconds(durationFromHMSString(e.target.value));
  });
  document.getElementById('dur-decimal-hours').addEventListener('input', e => {
    const decH = toNumber(e.target.value);
    setDurationFromSeconds(decH * 3600);
  });
  document.getElementById('dur-minutes-total').addEventListener('input', e => {
    const totalMinutes = toNumber(e.target.value);
    setDurationFromSeconds(totalMinutes * 60);
  });
  document.getElementById('dur-hours-total').addEventListener('input', e => {
    const totalHours = toNumber(e.target.value);
    setDurationFromSeconds(totalHours * 3600);
  });
  document.getElementById('dur-days-total').addEventListener('input', e => {
    const totalDays = toNumber(e.target.value);
    setDurationFromSeconds(totalDays * 24 * 3600);
  });

  // Initialize with 3661 seconds example
  setDurationFromSeconds(3661);

  // --- DateTime logic ---
  let updatingDateTime = false;
  function getCurrentDate() {
    const d = new Date();
    d.setSeconds(0,0);
    return d;
  }
  let currentDate = getCurrentDate();

  function pad2(n) { return String(n).padStart(2,'0'); }

  function updateAllFromDate(date) {
    if (updatingDateTime) return;
    updatingDateTime = true;
    currentDate = date;

    const localYear = date.getFullYear();
    const localMonth = date.getMonth() + 1;
    const localDay = date.getDate();
    const localHours = date.getHours();
    const localMinutes = date.getMinutes();
    const localSeconds = date.getSeconds();

    document.getElementById('dt-local-date').value = `${localYear}-${pad2(localMonth)}-${pad2(localDay)}`;
    document.getElementById('dt-local-time').value = `${pad2(localHours)}:${pad2(localMinutes)}:${pad2(localSeconds)}`;

    const offsetMinutes = -date.getTimezoneOffset();
    const offsetSign = offsetMinutes >= 0 ? '+' : '-';
    const absOffset = Math.abs(offsetMinutes);
    const offsetH = pad2(Math.floor(absOffset / 60));
    const offsetM = pad2(absOffset % 60);
    const offsetStr = `${offsetSign}${offsetH}:${offsetM}`;

    const isoLocal = `${localYear}-${pad2(localMonth)}-${pad2(localDay)}T${pad2(localHours)}:${pad2(localMinutes)}:${pad2(localSeconds)}${offsetStr}`;
    document.getElementById('dt-iso-local').value = isoLocal;

    const isoUTC = date.toISOString().replace(/\.\d{3}Z$/, 'Z');
    document.getElementById('dt-iso-utc').value = isoUTC;

    const unixSeconds = Math.floor(date.getTime() / 1000);
    const unixMillis = date.getTime();
    document.getElementById('dt-unix-seconds').value = unixSeconds;
    document.getElementById('dt-unix-millis').value = unixMillis;

    document.getElementById('dt-rfc2822').value = date.toUTCString().replace('GMT', '+0000');

    updateCustomPatternPreview();

    // Sync Unix/ISO tab
    document.getElementById('ux-unix-seconds').value = unixSeconds;
    document.getElementById('ux-iso-utc').value = isoUTC;
    document.getElementById('ux-iso-local').value = isoLocal;
    document.getElementById('ux-local-readable').value = date.toString();

    updatingDateTime = false;
  }

  function parseLocalDateTime(dateStr, timeStr) {
    if (!dateStr) return null;
    const [y,m,d] = dateStr.split('-').map(Number);
    if (!timeStr) {
      return new Date(y, (m||1)-1, d||1);
    }
    const parts = timeStr.split(':').map(Number);
    const hh = parts[0] || 0;
    const mm = parts[1] || 0;
    const ss = parts[2] || 0;
    return new Date(y, (m||1)-1, d||1, hh, mm, ss);
  }

  function updateCustomPatternPreview() {
    const pattern = document.getElementById('dt-pattern').value || '';
    const d = currentDate;
    const map = {
      'YYYY': d.getFullYear(),
      'MM': pad2(d.getMonth()+1),
      'DD': pad2(d.getDate()),
      'HH': pad2(d.getHours()),
      'mm': pad2(d.getMinutes()),
      'ss': pad2(d.getSeconds())
    };
    let out = pattern;
    Object.keys(map).forEach(k => {
      out = out.split(k).join(map[k]);
    });
    document.getElementById('dt-custom').value = out;
  }

  // DateTime events
  document.getElementById('dt-local-date').addEventListener('input', () => {
    const d = parseLocalDateTime(
      document.getElementById('dt-local-date').value,
      document.getElementById('dt-local-time').value
    );
    if (d) updateAllFromDate(d);
  });
  document.getElementById('dt-local-time').addEventListener('input', () => {
    const d = parseLocalDateTime(
      document.getElementById('dt-local-date').value,
      document.getElementById('dt-local-time').value
    );
    if (d) updateAllFromDate(d);
  });

  document.getElementById('dt-iso-local').addEventListener('input', e => {
    const val = e.target.value.trim();
    const parsed = new Date(val);
    if (!isNaN(parsed.getTime())) {
      updateAllFromDate(parsed);
    }
  });
  document.getElementById('dt-iso-utc').addEventListener('input', e => {
    const val = e.target.value.trim();
    const parsed = new Date(val);
    if (!isNaN(parsed.getTime())) {
      updateAllFromDate(parsed);
    }
  });

  document.getElementById('dt-unix-seconds').addEventListener('input', e => {
    const unix = toNumber(e.target.value);
    const d = new Date(unix * 1000);
    if (!isNaN(d.getTime())) updateAllFromDate(d);
  });
  document.getElementById('dt-unix-millis').addEventListener('input', e => {
    const ms = toNumber(e.target.value);
    const d = new Date(ms);
    if (!isNaN(d.getTime())) updateAllFromDate(d);
  });

  document.getElementById('dt-rfc2822').addEventListener('input', e => {
    const val = e.target.value.trim();
    const parsed = new Date(val);
    if (!isNaN(parsed.getTime())) {
      updateAllFromDate(parsed);
    }
  });

  document.getElementById('dt-pattern').addEventListener('input', updateCustomPatternPreview);

  // --- Unix / ISO tab events ---
  document.getElementById('ux-unix-seconds').addEventListener('input', e => {
    const unix = toNumber(e.target.value);
    const d = new Date(unix * 1000);
    if (!isNaN(d.getTime())) updateAllFromDate(d);
  });
  document.getElementById('ux-iso-utc').addEventListener('input', e => {
    const val = e.target.value.trim();
    const parsed = new Date(val);
    if (!isNaN(parsed.getTime())) updateAllFromDate(parsed);
  });
  document.getElementById('ux-iso-local').addEventListener('input', e => {
    const val = e.target.value.trim();
    const parsed = new Date(val);
    if (!isNaN(parsed.getTime())) updateAllFromDate(parsed);
  });

  // Initialize datetime with now
  updateAllFromDate(getCurrentDate());
})();
</script>

