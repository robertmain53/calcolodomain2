    <script>
      // Mobile menu toggle
      document
        .getElementById("mobile-menu-toggle")
        ?.addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          if (!menu) return;
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          this.setAttribute("aria-expanded", String(isHidden));
        });

      // 5 Whys tool logic
      (function () {
        const whyContainer = document.getElementById("why-steps-container");
        const addWhyBtn = document.getElementById("add-why-btn");
        const addBranchBtn = document.getElementById("add-branch-btn");
        const whyCountLabel = document.getElementById("why-count-label");
        const actionsContainer = document.getElementById("actions-container");
        const addActionBtn = document.getElementById("add-action-btn");
        const generateSummaryBtn = document.getElementById(
          "generate-summary-btn"
        );
        const copySummaryBtn = document.getElementById("copy-summary-btn");
        const summaryOutput = document.getElementById("summary-output");
        const copyFeedback = document.getElementById("copy-feedback");

        let whyChains = [{ id: "A", label: "Main chain", whys: [] }];

        function updateWhyCount() {
          const total = whyChains.reduce(
            (sum, chain) => sum + chain.whys.length,
            0
          );
          whyCountLabel.textContent =
            total === 1 ? "1 Why captured" : total + " Whys captured";
        }

        function renderWhys() {
          whyContainer.innerHTML = "";
          whyChains.forEach((chain, chainIndex) => {
            const chainWrapper = document.createElement("div");
            chainWrapper.className =
              "border border-gray-200 rounded-md p-3 space-y-2";
            if (chainIndex === 0) {
              chainWrapper.classList.add("step-active");
            }

            const header = document.createElement("div");
            header.className = "flex items-center justify-between mb-1";
            const title = document.createElement("div");
            title.className = "text-sm font-semibold text-gray-800";
            title.textContent =
              chainIndex === 0 ? "Main Why chain" : `Branch ${chain.id}`;
            header.appendChild(title);

            if (chainIndex > 0) {
              const removeBtn = document.createElement("button");
              removeBtn.type = "button";
              removeBtn.className = "text-xs text-red-600 hover:underline";
              removeBtn.textContent = "Remove branch";
              removeBtn.addEventListener("click", () => {
                whyChains.splice(chainIndex, 1);
                renderWhys();
                updateWhyCount();
              });
              header.appendChild(removeBtn);
            }

            chainWrapper.appendChild(header);

            if (chain.whys.length === 0) {
              const empty = document.createElement("p");
              empty.className = "text-xs text-gray-500";
              empty.textContent =
                "No Whys yet. Click “Add another Why” to start this chain.";
              chainWrapper.appendChild(empty);
            } else {
              chain.whys.forEach((why, idx) => {
                const step = document.createElement("div");
                step.className =
                  "border border-gray-200 rounded-md px-3 py-2 bg-white flex flex-col gap-1";

                const topRow = document.createElement("div");
                topRow.className = "flex items-center justify-between";

                const label = document.createElement("span");
                label.className = "text-xs font-medium text-gray-700";
                label.textContent = `Why ${idx + 1}`;
                topRow.appendChild(label);

                const controls = document.createElement("div");
                controls.className = "flex items-center gap-2";

                const moveUpBtn = document.createElement("button");
                moveUpBtn.type = "button";
                moveUpBtn.className =
                  "text-[11px] text-gray-400 hover:text-gray-600";
                moveUpBtn.textContent = "↑";
                moveUpBtn.title = "Move up";
                moveUpBtn.disabled = idx === 0;
                if (!moveUpBtn.disabled) {
                  moveUpBtn.addEventListener("click", () => {
                    const temp = chain.whys[idx - 1];
                    chain.whys[idx - 1] = chain.whys[idx];
                    chain.whys[idx] = temp;
                    renderWhys();
                  });
                }
                controls.appendChild(moveUpBtn);

                const moveDownBtn = document.createElement("button");
                moveDownBtn.type = "button";
                moveDownBtn.className =
                  "text-[11px] text-gray-400 hover:text-gray-600";
                moveDownBtn.textContent = "↓";
                moveDownBtn.title = "Move down";
                moveDownBtn.disabled = idx === chain.whys.length - 1;
                if (!moveDownBtn.disabled) {
                  moveDownBtn.addEventListener("click", () => {
                    const temp = chain.whys[idx + 1];
                    chain.whys[idx + 1] = chain.whys[idx];
                    chain.whys[idx] = temp;
                    renderWhys();
                  });
                }
                controls.appendChild(moveDownBtn);

                const removeBtn = document.createElement("button");
                removeBtn.type = "button";
                removeBtn.className =
                  "text-[11px] text-red-500 hover:text-red-700";
                removeBtn.textContent = "✕";
                removeBtn.title = "Remove this Why";
                removeBtn.addEventListener("click", () => {
                  chain.whys.splice(idx, 1);
                  renderWhys();
                  updateWhyCount();
                });
                controls.appendChild(removeBtn);

                topRow.appendChild(controls);
                step.appendChild(topRow);

                const textarea = document.createElement("textarea");
                textarea.className =
                  "w-full border border-gray-200 rounded-md px-2 py-1 text-xs textarea-small focus:ring-1 focus:ring-blue-500 focus:border-transparent";
                textarea.placeholder = "Answer: Why did this happen?";
                textarea.value = why.text;
                textarea.addEventListener("input", (e) => {
                  why.text = e.target.value;
                });
                step.appendChild(textarea);

                chainWrapper.appendChild(step);
              });
            }

            whyContainer.appendChild(chainWrapper);
          });
        }

        function addWhyToMainChain() {
          const mainChain = whyChains[0];
          mainChain.whys.push({ text: "" });
          renderWhys();
          updateWhyCount();
        }

        function addBranchChain() {
          const idCharCode = 65 + whyChains.length; // A, B, C...
          const id = String.fromCharCode(idCharCode);
          whyChains.push({ id, label: `Branch ${id}`, whys: [] });
          renderWhys();
          updateWhyCount();
        }

        function renderActions() {
          actionsContainer.innerHTML = "";
          if (!actionsContainer.dataset.initialized) {
            // Start with two default rows: corrective & preventive
            actionsContainer.dataset.initialized = "true";
            actionsContainer.dataset.count = "0";
            addActionRow("Corrective");
            addActionRow("Preventive");
            return;
          }
          const count = parseInt(actionsContainer.dataset.count || "0", 10);
          for (let i = 0; i < count; i++) {
            const rowData = JSON.parse(
              localStorage.getItem("5whys_action_" + i) || "{}"
            );
            createActionRowElement(
              i,
              rowData.type || "",
              rowData.description || "",
              rowData.owner || "",
              rowData.due || ""
            );
          }
        }

        function addActionRow(defaultType) {
          const currentCount = parseInt(
            actionsContainer.dataset.count || "0",
            10
          );
          const index = currentCount;
          actionsContainer.dataset.count = String(currentCount + 1);
          createActionRowElement(index, defaultType || "", "", "", "");
        }

        function createActionRowElement(index, type, description, owner, due) {
          const row = document.createElement("div");
          row.className =
            "border border-gray-200 rounded-md p-3 bg-white space-y-2";

          const topRow = document.createElement("div");
          topRow.className = "flex items-center justify-between gap-2";

          const typeSelect = document.createElement("select");
          typeSelect.className =
            "border border-gray-300 rounded-md px-2 py-1 text-xs focus:ring-1 focus:ring-blue-500 focus:border-transparent";
          ["Type...", "Corrective", "Preventive", "Monitoring"].forEach(
            (opt) => {
              const o = document.createElement("option");
              o.textContent = opt;
              if (opt === "Type...") o.value = "";
              if (opt === type) o.selected = true;
              typeSelect.appendChild(o);
            }
          );

          const removeBtn = document.createElement("button");
          removeBtn.type = "button";
          removeBtn.className = "text-xs text-red-500 hover:text-red-700";
          removeBtn.textContent = "Remove";
          removeBtn.addEventListener("click", () => {
            row.remove();
          });

          topRow.appendChild(typeSelect);
          topRow.appendChild(removeBtn);
          row.appendChild(topRow);

          const descInput = document.createElement("input");
          descInput.type = "text";
          descInput.className =
            "w-full border border-gray-300 rounded-md px-2 py-1 text-xs focus:ring-1 focus:ring-blue-500 focus:border-transparent";
          descInput.placeholder = "Action description (what will be done?)";
          descInput.value = description;
          row.appendChild(descInput);

          const bottomRow = document.createElement("div");
          bottomRow.className = "grid grid-cols-2 gap-2 mt-1";

          const ownerInput = document.createElement("input");
          ownerInput.type = "text";
          ownerInput.className =
            "w-full border border-gray-300 rounded-md px-2 py-1 text-xs focus:ring-1 focus:ring-blue-500 focus:border-transparent";
          ownerInput.placeholder = "Owner";
          ownerInput.value = owner;

          const dueInput = document.createElement("input");
          dueInput.type = "date";
          dueInput.className =
            "w-full border border-gray-300 rounded-md px-2 py-1 text-xs focus:ring-1 focus:ring-blue-500 focus:border-transparent";
          dueInput.value = due;

          bottomRow.appendChild(ownerInput);
          bottomRow.appendChild(dueInput);
          row.appendChild(bottomRow);

          actionsContainer.appendChild(row);
        }

        function generateSummary() {
          const title = document.getElementById("session-title").value.trim();
          const date = document.getElementById("session-date").value.trim();
          const facilitator = document
            .getElementById("session-facilitator")
            .value.trim();
          const participants = document
            .getElementById("session-participants")
            .value.trim();
          const problem = document
            .getElementById("problem-statement")
            .value.trim();
          const rootCause = document.getElementById("root-cause").value.trim();
          const confidence = document.getElementById("confidence").value.trim();
          const evidence = document.getElementById("evidence").value.trim();

          const lines = [];

          lines.push("5 Whys Root Cause Analysis");
          lines.push("==========================");
          if (title) lines.push("Title: " + title);
          if (date) lines.push("Date: " + date);
          if (facilitator) lines.push("Facilitator: " + facilitator);
          if (participants) lines.push("Participants: " + participants);
          lines.push("");

          lines.push("Problem statement");
          lines.push("-----------------");
          lines.push(problem || "(not specified)");
          lines.push("");

          whyChains.forEach((chain, chainIndex) => {
            const chainTitle =
              chainIndex === 0 ? "Main Why chain" : `Branch ${chain.id}`;
            lines.push(chainTitle);
            lines.push("-".repeat(chainTitle.length));
            if (chain.whys.length === 0) {
              lines.push("(no Whys captured)");
            } else {
              chain.whys.forEach((why, idx) => {
                const text = (why.text || "").trim();
                lines.push(`Why ${idx + 1}: ${text || "(empty)"}`);
              });
            }
            lines.push("");
          });

          lines.push("Root cause");
          lines.push("----------");
          lines.push(rootCause || "(not specified)");
          if (confidence) lines.push("Confidence: " + confidence);
          if (evidence) lines.push("Evidence / data sources: " + evidence);
          lines.push("");

          lines.push("Actions");
          lines.push("-------");
          const actionRows = Array.from(
            actionsContainer.querySelectorAll("div.border")
          );
          if (actionRows.length === 0) {
            lines.push("(no actions defined)");
          } else {
            actionRows.forEach((row, idx) => {
              const type = row.querySelector("select")?.value || "";
              const desc = row.querySelector('input[type="text"]')?.value || "";
              const owner =
                row.querySelectorAll('input[type="text"]')[1]?.value ||
                row.querySelectorAll('input[type="text"]')[0]?.value ||
                "";
              const due = row.querySelector('input[type="date"]')?.value || "";
              lines.push(`Action ${idx + 1}:`);
              if (type) lines.push("  Type: " + type);
              if (desc) lines.push("  Description: " + desc);
              if (owner) lines.push("  Owner: " + owner);
              if (due) lines.push("  Due date: " + due);
            });
          }

          summaryOutput.value = lines.join("\n");
          copySummaryBtn.disabled = false;
          copyFeedback.classList.add("hidden");
        }

        function copySummary() {
          if (!summaryOutput.value) return;
          summaryOutput.select();
          summaryOutput.setSelectionRange(0, summaryOutput.value.length);
          try {
            const ok = document.execCommand("copy");
            if (ok) {
              copyFeedback.classList.remove("hidden");
              setTimeout(() => copyFeedback.classList.add("hidden"), 2000);
            }
          } catch (e) {
            console.error("Copy failed", e);
          }
          window.getSelection().removeAllRanges();
        }

        // Event bindings
        addWhyBtn?.addEventListener("click", addWhyToMainChain);
        addBranchBtn?.addEventListener("click", addBranchChain);
        addActionBtn?.addEventListener("click", () => addActionRow(""));
        generateSummaryBtn?.addEventListener("click", generateSummary);
        copySummaryBtn?.addEventListener("click", copySummary);

        // Initialize
        renderWhys();
        updateWhyCount();
        renderActions();
      })();
    </script>

