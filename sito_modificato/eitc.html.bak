<script>
  // Simplified EITC parameter sets (approximate, not official IRS tables)
  const EITC_PARAMS = {
    "2023": {
      maxCredit: {0: 600, 1: 3995, 2: 6604, 3: 7430},
      phaseInRate: {0: 0.0765, 1: 0.34, 2: 0.4, 3: 0.45},
      phaseOutRate: {0: 0.0765, 1: 0.1598, 2: 0.2106, 3: 0.2106},
      phaseInEnd: {0: 7840, 1: 11750, 2: 16510, 3: 16510},
      phaseOutStartSingle: {0: 9800, 1: 21560, 2: 21560, 3: 21560},
      phaseOutStartMFJ: {0: 16370, 1: 28120, 2: 28120, 3: 28120},
      maxIncomeSingle: {0: 17640, 1: 46560, 2: 52918, 3: 56838},
      maxIncomeMFJ: {0: 24210, 1: 53120, 2: 59478, 3: 62398}
    },
    "2024": {
      maxCredit: {0: 632, 1: 4213, 2: 6980, 3: 7410},
      phaseInRate: {0: 0.0765, 1: 0.34, 2: 0.4, 3: 0.45},
      phaseOutRate: {0: 0.0765, 1: 0.1598, 2: 0.2106, 3: 0.2106},
      phaseInEnd: {0: 8260, 1: 12390, 2: 17400, 3: 17400},
      phaseOutStartSingle: {0: 9920, 1: 22800, 2: 22800, 3: 22800},
      phaseOutStartMFJ: {0: 16580, 1: 29460, 2: 29460, 3: 29460},
      maxIncomeSingle: {0: 18490, 1: 49200, 2: 55300, 3: 59100},
      maxIncomeMFJ: {0: 25150, 1: 55860, 2: 61960, 3: 65760}
    }
  };

  function formatCurrency(value) {
    return new Intl.NumberFormat('en-US', {style: 'currency', currency: 'USD', maximumFractionDigits: 0}).format(value);
  }

  function calculateEITC() {
    const year = document.getElementById('tax-year').value;
    const filingStatus = document.getElementById('filing-status').value;
    const numChildren = parseInt(document.getElementById('num-children').value, 10);
    const earnedIncome = parseFloat(document.getElementById('earned-income').value) || 0;
    const agi = parseFloat(document.getElementById('agi').value) || 0;
    const investmentOk = document.getElementById('investment-income-check').checked;

    const params = EITC_PARAMS[year];
    const childrenKey = numChildren >= 3 ? 3 : numChildren;

    const resultEl = document.getElementById('eitc-amount');
    const summaryEl = document.getElementById('eitc-summary');
    const badgeEl = document.getElementById('eligibility-badge');
    const phaseInRangeEl = document.getElementById('phase-in-range');
    const phaseOutRangeEl = document.getElementById('phase-out-range');
    const maxEitcEl = document.getElementById('max-eitc');
    const posBar = document.getElementById('eitc-position');
    const posLabel = document.getElementById('eitc-position-label');

    // Basic disqualifiers
    if (!investmentOk) {
      resultEl.textContent = '$0';
      badgeEl.textContent = 'Not eligible (investment income)';
      badgeEl.className = 'badge bg-red-100 text-red-800 border border-red-300';
      summaryEl.textContent = 'Your investment income appears to exceed the IRS limit, so you are not eligible for the Earned Income Tax Credit.';
      phaseInRangeEl.textContent = '–';
      phaseOutRangeEl.textContent = '–';
      maxEitcEl.textContent = '–';
      posBar.style.width = '0%';
      posLabel.textContent = 'Not eligible';
      return;
    }

    if (filingStatus === 'mfs') {
      resultEl.textContent = '$0';
      badgeEl.textContent = 'Generally not eligible';
      badgeEl.className = 'badge bg-red-100 text-red-800 border border-red-300';
      summaryEl.textContent = 'Married filing separately taxpayers are generally not eligible for the EITC under IRS rules.';
      phaseInRangeEl.textContent = '–';
      phaseOutRangeEl.textContent = '–';
      maxEitcEl.textContent = '–';
      posBar.style.width = '0%';
      posLabel.textContent = 'Not eligible';
      return;
    }

    const income = Math.min(earnedIncome, agi);
    if (income <= 0) {
      resultEl.textContent = '$0';
      badgeEl.textContent = 'Estimate';
      badgeEl.className = 'badge bg-yellow-100 text-yellow-800 border border-yellow-300';
      summaryEl.textContent = 'Enter a positive amount of earned income to estimate your EITC.';
      phaseInRangeEl.textContent = '0 – ' + formatCurrency(params.phaseInEnd[childrenKey]);
      const maxIncome = (filingStatus === 'mfj') ? params.maxIncomeMFJ[childrenKey] : params.maxIncomeSingle[childrenKey];
      phaseOutRangeEl.textContent = formatCurrency(params.phaseOutStartSingle[childrenKey]) + ' – ' + formatCurrency(maxIncome);
      maxEitcEl.textContent = formatCurrency(params.maxCredit[childrenKey]);
      posBar.style.width = '0%';
      posLabel.textContent = 'No earned income';
      return;
    }

    const maxCredit = params.maxCredit[childrenKey];
    const phaseInRate = params.phaseInRate[childrenKey];
    const phaseOutRate = params.phaseOutRate[childrenKey];
    const phaseInEnd = params.phaseInEnd[childrenKey];
    const phaseOutStart = (filingStatus === 'mfj') ? params.phaseOutStartMFJ[childrenKey] : params.phaseOutStartSingle[childrenKey];
    const maxIncome = (filingStatus === 'mfj') ? params.maxIncomeMFJ[childrenKey] : params.maxIncomeSingle[childrenKey];

    let credit = 0;
    let positionText = '';

    if (income <= phaseInEnd) {
      credit = phaseInRate * income;
      if (credit > maxCredit) credit = maxCredit;
      positionText = 'Phase‑in region: your credit increases as your income rises.';
    } else if (income > phaseInEnd && income < phaseOutStart) {
      credit = maxCredit;
      positionText = 'Plateau region: you receive the maximum EITC for your situation.';
    } else if (income >= phaseOutStart && income < maxIncome) {
      const excess = income - phaseOutStart;
      credit = maxCredit - phaseOutRate * excess;
      if (credit < 0) credit = 0;
      positionText = 'Phase‑out region: your credit decreases as your income rises.';
    } else {
      credit = 0;
      positionText = 'Above the income limit: you are not eligible for EITC.';
    }

    credit = Math.max(0, Math.round(credit));

    resultEl.textContent = formatCurrency(credit);
    badgeEl.textContent = credit > 0 ? 'Potentially eligible' : 'Not eligible at this income';
    badgeEl.className = credit > 0
      ? 'badge bg-green-100 text-green-800 border border-green-300'
      : 'badge bg-red-100 text-red-800 border border-red-300';

    if (credit > 0) {
      summaryEl.textContent = `Based on your inputs, you may qualify for an estimated Earned Income Tax Credit of about ${formatCurrency(credit)}. Your actual credit will be determined on your filed tax return.`;
    } else {
      summaryEl.textContent = 'At this income level and filing status, you are above the approximate EITC income limit for your situation.';
    }

    phaseInRangeEl.textContent = '0 – ' + formatCurrency(phaseInEnd);
    phaseOutRangeEl.textContent = formatCurrency(phaseOutStart) + ' – ' + formatCurrency(maxIncome);
    maxEitcEl.textContent = formatCurrency(maxCredit);

    // Position on curve (0–100%)
    let pos = 0;
    if (income <= phaseInEnd) {
      pos = (income / phaseInEnd) * 33;
    } else if (income > phaseInEnd && income < phaseOutStart) {
      pos = 33 + ((income - phaseInEnd) / Math.max(1, (phaseOutStart - phaseInEnd))) * 34;
    } else if (income >= phaseOutStart && income < maxIncome) {
      pos = 67 + ((income - phaseOutStart) / Math.max(1, (maxIncome - phaseOutStart))) * 33;
    } else if (income >= maxIncome) {
      pos = 100;
    }
    pos = Math.max(0, Math.min(100, pos));
    posBar.style.width = pos.toFixed(0) + '%';
    posLabel.textContent = positionText;
  }

  document.getElementById('calculate-btn').addEventListener('click', calculateEITC);

  ['tax-year','filing-status','num-children','earned-income','agi'].forEach(id => {
    document.getElementById(id).addEventListener('change', () => {
      // Do not auto-calc on every keystroke, but recalc on change for selects and blur for numbers
      if (id === 'earned-income' || id === 'agi') return;
      calculateEITC();
    });
    if (id === 'earned-income' || id === 'agi') {
      document.getElementById(id).addEventListener('blur', calculateEITC);
    }
  });

  document.getElementById('investment-income-check').addEventListener('change', calculateEITC);

  // Initial calculation
  calculateEITC();
</script>

