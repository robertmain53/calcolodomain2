<script>
    function calculateTurnover() {
        const cogs = parseFloat(document.getElementById('costOfGoodsSold').value);
        const avgInventory = parseFloat(document.getElementById('averageInventory').value);
        const turnoverElement = document.getElementById('turnoverResult');
        let errorMessage = '';
        
        if (isNaN(cogs) || cogs <= 0) {
            errorMessage = 'Please enter a valid number greater than zero for COGS.';
            document.getElementById('cogsError').textContent = errorMessage;
        } else {
            document.getElementById('cogsError').textContent = '';
        }

        if (isNaN(avgInventory) || avgInventory <= 0) {
            errorMessage = 'Please enter a valid number greater than zero for Average Inventory.';
            document.getElementById('averageInventoryError').textContent = errorMessage;
        } else {
            document.getElementById('averageInventoryError').textContent = '';
        }

        if (!errorMessage) {
            const turnover = (cogs / avgInventory).toFixed(2);
            turnoverElement.textContent = turnover;
        } else {
            turnoverElement.textContent = '-';
        }
    }
</script>



