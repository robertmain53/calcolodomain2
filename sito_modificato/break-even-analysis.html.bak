    <script>
      // Simple chart without external libraries
      function createBeChart(ctx, data) {
        const { maxUnits, beUnits, fixedCosts, price, variable, currency } =
          data;
        const width = ctx.canvas.width;
        const height = ctx.canvas.height;
        const padding = 40;

        const maxRevenue = price * maxUnits;
        const maxCost = fixedCosts + variable * maxUnits;
        const maxY = Math.max(maxRevenue, maxCost) * 1.1;

        const xScale = (units) =>
          padding + (units / maxUnits) * (width - 2 * padding);
        const yScale = (value) =>
          height - padding - (value / maxY) * (height - 2 * padding);

        ctx.clearRect(0, 0, width, height);

        // Axes
        ctx.strokeStyle = "#4b5563";
        ctx.lineWidth = 1;
        ctx.beginPath();
        ctx.moveTo(padding, padding);
        ctx.lineTo(padding, height - padding);
        ctx.lineTo(width - padding, height - padding);
        ctx.stroke();

        // Grid lines and labels
        ctx.fillStyle = "#6b7280";
        ctx.font = "10px Inter, sans-serif";
        ctx.textAlign = "right";
        ctx.textBaseline = "middle";
        const ySteps = 4;
        for (let i = 0; i <= ySteps; i++) {
          const value = (maxY / ySteps) * i;
          const y = yScale(value);
          ctx.strokeStyle = "#e5e7eb";
          ctx.beginPath();
          ctx.moveTo(padding, y);
          ctx.lineTo(width - padding, y);
          ctx.stroke();
          ctx.fillText(currency + value.toFixed(0), padding - 4, y);
        }

        ctx.textAlign = "center";
        ctx.textBaseline = "top";
        const xSteps = 4;
        for (let i = 0; i <= xSteps; i++) {
          const units = (maxUnits / xSteps) * i;
          const x = xScale(units);
          ctx.fillText(units.toFixed(0), x, height - padding + 4);
        }

        // Fixed cost line
        ctx.strokeStyle = "#9ca3af";
        ctx.lineWidth = 1.5;
        ctx.beginPath();
        ctx.moveTo(padding, yScale(fixedCosts));
        ctx.lineTo(width - padding, yScale(fixedCosts));
        ctx.stroke();

        // Total cost line
        ctx.strokeStyle = "#ef4444";
        ctx.lineWidth = 2;
        ctx.beginPath();
        for (let u = 0; u <= maxUnits; u += maxUnits / 50) {
          const cost = fixedCosts + variable * u;
          const x = xScale(u);
          const y = yScale(cost);
          if (u === 0) ctx.moveTo(x, y);
          else ctx.lineTo(x, y);
        }
        ctx.stroke();

        // Revenue line
        ctx.strokeStyle = "#22c55e";
        ctx.lineWidth = 2;
        ctx.beginPath();
        for (let u = 0; u <= maxUnits; u += maxUnits / 50) {
          const rev = price * u;
          const x = xScale(u);
          const y = yScale(rev);
          if (u === 0) ctx.moveTo(x, y);
          else ctx.lineTo(x, y);
        }
        ctx.stroke();

        // Break-even point
        if (isFinite(beUnits) && beUnits >= 0 && beUnits <= maxUnits) {
          const beX = xScale(beUnits);
          const beY = yScale(price * beUnits);
          ctx.fillStyle = "#1d4ed8";
          ctx.beginPath();
          ctx.arc(beX, beY, 4, 0, 2 * Math.PI);
          ctx.fill();

          ctx.fillStyle = "#111827";
          ctx.font = "11px Inter, sans-serif";
          ctx.textAlign = "left";
          ctx.textBaseline = "bottom";
          ctx.fillText(
            `Break-even ≈ ${beUnits.toFixed(1)} units`,
            beX + 6,
            beY - 4
          );
        }

        // Legend
        const legendY = padding - 20;
        let legendX = padding;
        const legendItems = [
          { color: "#22c55e", label: "Revenue" },
          { color: "#ef4444", label: "Total cost" },
          { color: "#9ca3af", label: "Fixed cost" },
        ];
        ctx.textAlign = "left";
        ctx.textBaseline = "middle";
        ctx.font = "11px Inter, sans-serif";
        legendItems.forEach((item) => {
          ctx.fillStyle = item.color;
          ctx.fillRect(legendX, legendY - 5, 10, 10);
          ctx.fillStyle = "#374151";
          ctx.fillText(item.label, legendX + 14, legendY);
          legendX += ctx.measureText(item.label).width + 40;
        });
      }

      function formatCurrency(value, currency) {
        if (!isFinite(value)) return "–";
        const abs = Math.abs(value);
        let formatted = value.toFixed(abs >= 1000 ? 0 : 2);
        return (value < 0 ? "-" : "") + currency + Math.abs(formatted);
      }

      function formatPercent(value) {
        if (!isFinite(value)) return "–";
        return value.toFixed(1) + "%";
      }

      document.addEventListener("DOMContentLoaded", () => {
        const currencyEl = document.getElementById("currency");
        const periodEl = document.getElementById("period");
        const fixedCostsEl = document.getElementById("fixedCosts");
        const priceEl = document.getElementById("pricePerUnit");
        const variableEl = document.getElementById("variableCostPerUnit");
        const expectedUnitsEl = document.getElementById("expectedSalesUnits");
        const targetProfitEl = document.getElementById("targetProfit");
        const calcBtn = document.getElementById("calculateBtn");

        const cmPerUnitEl = document.getElementById("cmPerUnit");
        const cmRatioEl = document.getElementById("cmRatio");
        const beUnitsEl = document.getElementById("beUnits");
        const beSalesEl = document.getElementById("beSales");
        const beUtilizationEl = document.getElementById("beUtilization");
        const mosUnitsEl = document.getElementById("mosUnits");
        const mosPercentEl = document.getElementById("mosPercent");
        const profitAtExpectedEl = document.getElementById("profitAtExpected");
        const unitsForTargetProfitEl = document.getElementById(
          "unitsForTargetProfit"
        );

        const scenarioUnitsEl = document.getElementById("scenarioUnits");
        const scenarioUnitsLabel =
          document.getElementById("scenarioUnitsLabel");
        const scenarioProfitLabel = document.getElementById(
          "scenarioProfitLabel"
        );

        const currencySpans = [
          "fixedCosts-currency",
          "pricePerUnit-currency",
          "variableCostPerUnit-currency",
          "targetProfit-currency",
        ].map((id) => document.getElementById(id));

        const canvas = document.getElementById("beChart");
        const ctx = canvas.getContext("2d");

        function updateCurrencySymbol() {
          const symbol = currencyEl.value;
          currencySpans.forEach((span) => (span.textContent = symbol));
        }

        function calculate() {
          const currency = currencyEl.value;
          const fixedCosts = parseFloat(fixedCostsEl.value) || 0;
          const price = parseFloat(priceEl.value) || 0;
          const variable = parseFloat(variableEl.value) || 0;
          const expectedUnits = parseFloat(expectedUnitsEl.value) || 0;
          const targetProfit = parseFloat(targetProfitEl.value) || 0;

          const cmPerUnit = price - variable;
          const cmRatio = price > 0 ? cmPerUnit / price : NaN;

          cmPerUnitEl.textContent = formatCurrency(cmPerUnit, currency);
          cmRatioEl.textContent = isFinite(cmRatio)
            ? formatPercent(cmRatio * 100)
            : "–";

          let beUnits = NaN;
          let beSales = NaN;
          let beUtilization = NaN;
          let mosUnits = NaN;
          let mosPercent = NaN;
          let profitAtExpected = NaN;
          let unitsForTargetProfit = NaN;

          if (cmPerUnit > 0) {
            beUnits = fixedCosts / cmPerUnit;
            beSales = beUnits * price;
            beUtilization =
              expectedUnits > 0 ? (beUnits / expectedUnits) * 100 : NaN;
            mosUnits = expectedUnits - beUnits;
            mosPercent =
              expectedUnits > 0 ? (mosUnits / expectedUnits) * 100 : NaN;
            profitAtExpected = (price - variable) * expectedUnits - fixedCosts;
            unitsForTargetProfit = (fixedCosts + targetProfit) / cmPerUnit;
          }

          beUnitsEl.textContent = isFinite(beUnits)
            ? beUnits.toFixed(1) + " units"
            : "–";
          beSalesEl.textContent = formatCurrency(beSales, currency);
          beUtilizationEl.textContent = isFinite(beUtilization)
            ? formatPercent(beUtilization)
            : "–";
          mosUnitsEl.textContent = isFinite(mosUnits)
            ? mosUnits.toFixed(1) + " units"
            : "–";
          mosPercentEl.textContent = isFinite(mosPercent)
            ? formatPercent(mosPercent)
            : "–";
          profitAtExpectedEl.textContent = formatCurrency(
            profitAtExpected,
            currency
          );
          unitsForTargetProfitEl.textContent = isFinite(unitsForTargetProfit)
            ? unitsForTargetProfit.toFixed(1) + " units"
            : "–";

          // Scenario slider
          const maxUnits = Math.max(
            expectedUnits * 1.5 || 1000,
            beUnits * 1.5 || 1000
          );
          scenarioUnitsEl.max = Math.max(10, Math.round(maxUnits));
          if (parseFloat(scenarioUnitsEl.value) > scenarioUnitsEl.max) {
            scenarioUnitsEl.value = scenarioUnitsEl.max;
          }
          updateScenario();

          // Chart
          canvas.width = canvas.clientWidth * (window.devicePixelRatio || 1);
          canvas.height = 320 * (window.devicePixelRatio || 1);
          createBeChart(ctx, {
            maxUnits: scenarioUnitsEl.max,
            beUnits,
            fixedCosts,
            price,
            variable,
            currency,
          });
        }

        function updateScenario() {
          const currency = currencyEl.value;
          const units = parseFloat(scenarioUnitsEl.value) || 0;
          const price = parseFloat(priceEl.value) || 0;
          const variable = parseFloat(variableEl.value) || 0;
          const fixedCosts = parseFloat(fixedCostsEl.value) || 0;

          const revenue = price * units;
          const variableCost = variable * units;
          const totalCost = fixedCosts + variableCost;
          const profit = revenue - totalCost;

          scenarioUnitsLabel.textContent = `Units: ${units.toFixed(0)}`;
          scenarioProfitLabel.textContent = `Profit: ${formatCurrency(
            profit,
            currency
          )}`;
        }

        currencyEl.addEventListener("change", () => {
          updateCurrencySymbol();
          calculate();
        });

        [
          fixedCostsEl,
          priceEl,
          variableEl,
          expectedUnitsEl,
          targetProfitEl,
        ].forEach((el) => {
          el.addEventListener("input", () => {
            calculate();
          });
        });

        scenarioUnitsEl.addEventListener("input", () => {
          updateScenario();
        });

        calcBtn.addEventListener("click", (e) => {
          e.preventDefault();
          calculate();
        });

        updateCurrencySymbol();
        calculate();

        window.addEventListener("resize", () => {
          calculate();
        });
      });
    </script>

