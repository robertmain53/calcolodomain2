    <script>
      // Utility: format currency
      function formatCurrency(value) {
        if (!isFinite(value)) return "$0";
        const abs = Math.abs(value);
        const sign = value < 0 ? "-" : "";
        return (
          sign +
          abs.toLocaleString(undefined, {
            style: "currency",
            currency: "USD",
            maximumFractionDigits: 0,
          })
        );
      }

      function updateSavingsLabel() {
        const savingsRate =
          Number(document.getElementById("savingsRate").value) || 0;
        document.getElementById("savingsRateLabel").textContent =
          savingsRate.toFixed(0) + "%";
      }

      function updateWeightLabelsAndTotal() {
        const categories = [
          "housing",
          "food",
          "transport",
          "health",
          "tax",
          "utilities",
          "other",
        ];
        let total = 0;
        categories.forEach((cat) => {
          const slider = document.getElementById(cat + "Weight");
          const label = document.getElementById(cat + "WeightLabel");
          const val = Number(slider.value) || 0;
          total += val;
          label.textContent = val.toFixed(0) + "%";
        });
        const totalLabel = document.getElementById("weightsTotalLabel");
        totalLabel.textContent = total.toFixed(0) + "%";
        const errorEl = document.getElementById("weightsError");
        if (total !== 100) {
          errorEl.classList.remove("hidden");
        } else {
          errorEl.classList.add("hidden");
        }
        return total;
      }

      function resetWeights() {
        const defaults = {
          housingWeight: 30,
          foodWeight: 15,
          transportWeight: 10,
          healthWeight: 10,
          taxWeight: 15,
          utilitiesWeight: 8,
          otherWeight: 12,
        };
        Object.keys(defaults).forEach((id) => {
          const el = document.getElementById(id);
          if (el) el.value = defaults[id];
        });
        updateWeightLabelsAndTotal();
      }

      function calculateCostOfLiving() {
        const currentIncome = Number(
          document.getElementById("currentIncome").value
        );
        const currentIndex =
          Number(document.getElementById("currentIndex").value) || 100;
        const destIndex =
          Number(document.getElementById("destIndex").value) || 100;
        const savingsRate =
          Number(document.getElementById("savingsRate").value) / 100 || 0;

        const currentCity = (
          document.getElementById("currentCity").value || "your current city"
        ).trim();
        const destCity = (
          document.getElementById("destCity").value || "the destination city"
        ).trim();

        const weightsTotal = updateWeightLabelsAndTotal();
        if (!currentIncome || currentIncome <= 0 || weightsTotal !== 100) {
          document.getElementById("resultsSection").classList.add("hidden");
          return;
        }

        const takeHome = currentIncome * (1 - savingsRate);
        const monthlyTakeHome = takeHome / 12;

        const categories = [
          { key: "housing", label: "Housing" },
          { key: "food", label: "Food & groceries" },
          { key: "transport", label: "Transportation" },
          { key: "health", label: "Healthcare" },
          { key: "tax", label: "Taxes & insurance" },
          { key: "utilities", label: "Utilities" },
          { key: "other", label: "Entertainment & other" },
        ];

        const tbody = document.getElementById("resultsTableBody");
        tbody.innerHTML = "";

        let totalCurrent = 0;
        let totalDest = 0;

        categories.forEach((cat) => {
          const weight =
            Number(document.getElementById(cat.key + "Weight").value) / 100 ||
            0;
          const indexVal =
            Number(document.getElementById(cat.key + "Index").value) || 100;

          const currentMonthly = monthlyTakeHome * weight;
          const destMonthly = currentMonthly * (indexVal / 100);
          const diff = destMonthly - currentMonthly;

          totalCurrent += currentMonthly;
          totalDest += destMonthly;

          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="px-3 py-2 text-gray-800">${cat.label}</td>
        <td class="px-3 py-2 text-right text-gray-800">${formatCurrency(
          currentMonthly
        )}</td>
        <td class="px-3 py-2 text-right text-gray-800">${formatCurrency(
          destMonthly
        )}</td>
        <td class="px-3 py-2 text-right ${
          diff >= 0 ? "text-red-600" : "text-green-600"
        }">${diff === 0 ? "$0" : formatCurrency(diff)}</td>
      `;
          tbody.appendChild(tr);
        });

        document.getElementById("totalCurrentSpend").textContent =
          formatCurrency(totalCurrent);
        document.getElementById("totalDestSpend").textContent =
          formatCurrency(totalDest);
        const totalDiff = totalDest - totalCurrent;
        document.getElementById("totalDiffSpend").textContent =
          totalDiff === 0 ? "$0" : formatCurrency(totalDiff);

        const incomeNeeded = currentIncome * (destIndex / currentIndex);
        document.getElementById("destIncomeNeeded").textContent =
          formatCurrency(incomeNeeded);

        const overallDiffPercent = (destIndex / currentIndex - 1) * 100;
        const diffLabel =
          (overallDiffPercent >= 0 ? "+" : "") +
          overallDiffPercent.toFixed(1) +
          "%";
        document.getElementById("overallDiffPercent").textContent = diffLabel;

        const summary = document.getElementById("summaryText");
        const moreOrLess = overallDiffPercent >= 0 ? "more" : "less";
        summary.textContent =
          `Based on your current income of ${formatCurrency(
            currentIncome
          )} in ${currentCity} and the indexes you entered, ` +
          `${destCity} is approximately ${diffLabel} ${moreOrLess} expensive overall. ` +
          `To maintain a similar lifestyle, you would need about ${formatCurrency(
            incomeNeeded
          )} per year in ${destCity}.`;

        document.getElementById("resultsSection").classList.remove("hidden");
      }

      document.addEventListener("DOMContentLoaded", function () {
        updateSavingsLabel();
        resetWeights();

        document
          .getElementById("savingsRate")
          .addEventListener("input", updateSavingsLabel);

        document.querySelectorAll(".weight-slider").forEach((slider) => {
          slider.addEventListener("input", updateWeightLabelsAndTotal);
        });

        document
          .getElementById("resetWeights")
          .addEventListener("click", resetWeights);
        document
          .getElementById("calculateBtn")
          .addEventListener("click", calculateCostOfLiving);
      });

      // Mobile menu script (from template)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          this.setAttribute("aria-expanded", isHidden ? "true" : "false");
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
