    <script>
      // Conversion factors in liters per fluid ounce
      const OZ_FACTORS = {
        us: 0.0295735295625,
        uk: 0.0284130625,
        metric: 0.1,
      };

      let currentOzType = "us";
      let direction = "oz-to-l"; // or 'l-to-oz'

      function updateFactorText() {
        const factor = OZ_FACTORS[currentOzType];
        const labelMap = {
          us: "US",
          uk: "UK (imperial)",
          metric: "metric",
        };
        const el = document.getElementById("current-factor");
        if (!el) return;
        if (direction === "oz-to-l") {
          el.textContent = `1 ${
            labelMap[currentOzType]
          } fl oz = ${factor.toFixed(6)} L`;
        } else {
          el.textContent = `1 L = ${(1 / factor).toFixed(4)} ${
            labelMap[currentOzType]
          } fl oz`;
        }
      }

      function convertOzToL() {
        const input = document.getElementById("input-oz");
        const output = document.getElementById("output-l");
        if (!input || !output) return;
        const val = parseFloat(input.value);
        if (isNaN(val)) {
          output.value = "";
          return;
        }
        const factor = OZ_FACTORS[currentOzType];
        const result = val * factor;
        output.value = result.toFixed(6).replace(/\.?0+$/, "");
      }

      function convertLToOz() {
        const input = document.getElementById("input-l");
        const output = document.getElementById("output-oz");
        if (!input || !output) return;
        const val = parseFloat(input.value);
        if (isNaN(val)) {
          output.value = "";
          return;
        }
        const factor = OZ_FACTORS[currentOzType];
        const result = val / factor;
        output.value = result.toFixed(4).replace(/\.?0+$/, "");
      }

      function attachEvents() {
        const inputOz = document.getElementById("input-oz");
        const inputL = document.getElementById("input-l");

        if (inputOz) {
          inputOz.addEventListener("input", convertOzToL);
        }
        if (inputL) {
          inputL.addEventListener("input", convertLToOz);
        }

        // Direction toggle
        const btnOzToL = document.getElementById("dir-oz-to-l");
        const btnLToOz = document.getElementById("dir-l-to-oz");
        const rowOzToL = document.getElementById("row-oz-to-l");
        const rowLToOz = document.getElementById("row-l-to-oz");

        if (btnOzToL && btnLToOz && rowOzToL && rowLToOz) {
          btnOzToL.addEventListener("click", () => {
            direction = "oz-to-l";
            btnOzToL.classList.add("toggle-btn-active");
            btnLToOz.classList.remove("toggle-btn-active");
            rowOzToL.classList.remove("hidden");
            rowLToOz.classList.add("hidden");
            updateFactorText();
            convertOzToL();
          });

          btnLToOz.addEventListener("click", () => {
            direction = "l-to-oz";
            btnLToOz.classList.add("toggle-btn-active");
            btnOzToL.classList.remove("toggle-btn-active");
            rowLToOz.classList.remove("hidden");
            rowOzToL.classList.add("hidden");
            updateFactorText();
            convertLToOz();
          });
        }

        // Oz type buttons
        const ozButtons = document.querySelectorAll(".oz-type-btn");
        ozButtons.forEach((btn) => {
          btn.addEventListener("click", () => {
            ozButtons.forEach((b) => b.classList.remove("toggle-btn-active"));
            btn.classList.add("toggle-btn-active");
            currentOzType = btn.getAttribute("data-oz-type") || "us";
            updateFactorText();
            if (direction === "oz-to-l") {
              convertOzToL();
            } else {
              convertLToOz();
            }
          });
        });

        // Swap button
        const btnSwap = document.getElementById("btn-swap");
        if (btnSwap) {
          btnSwap.addEventListener("click", () => {
            if (direction === "oz-to-l") {
              const val = document.getElementById("output-l").value;
              document.getElementById("input-l").value = val;
              direction = "l-to-oz";
              document.getElementById("dir-l-to-oz").click();
            } else {
              const val = document.getElementById("output-oz").value;
              document.getElementById("input-oz").value = val;
              direction = "oz-to-l";
              document.getElementById("dir-oz-to-l").click();
            }
          });
        }

        // Clear button
        const btnClear = document.getElementById("btn-clear");
        if (btnClear) {
          btnClear.addEventListener("click", () => {
            if (inputOz) inputOz.value = "";
            if (inputL) inputL.value = "";
            const outL = document.getElementById("output-l");
            const outOz = document.getElementById("output-oz");
            if (outL) outL.value = "";
            if (outOz) outOz.value = "";
          });
        }

        updateFactorText();
      }

      document.addEventListener("DOMContentLoaded", attachEvents);
    </script>

