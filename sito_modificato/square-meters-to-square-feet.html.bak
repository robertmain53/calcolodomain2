            <script>
              document.addEventListener('DOMContentLoaded', () => {
                // --- FATTORI DI CONVERSIONE ---
                // Tutto basato su 1 Metro Quadro (m²)
                const M2_TO_FT2 = 10.7639104;
                const M2_TO_YD2 = 1.19599005;
                const M2_TO_ACRE = 0.000247105;
                
                const factors = {
                    'm2': 1,
                    'ft2': M2_TO_FT2,
                    'yd2': M2_TO_YD2,
                    'acre': M2_TO_ACRE
                };

                // --- LOGICA STRUMENTO 1: DIMENSIONI ---
                const lengthInput = document.getElementById('length-input');
                const widthInput = document.getElementById('width-input');
                const areaM2Output = document.getElementById('area-m2-output');
                const areaFt2Output = document.getElementById('area-ft2-output');

                function calculateDimensions() {
                    const length = parseFloat(lengthInput.value) || 0;
                    const width = parseFloat(widthInput.value) || 0;
                    
                    if (length > 0 && width > 0) {
                        const areaM2 = length * width;
                        const areaFt2 = areaM2 * M2_TO_FT2;
                        
                        areaM2Output.value = formatNum(areaM2, 2);
                        areaFt2Output.value = formatNum(areaFt2, 2);
                    } else {
                        areaM2Output.value = '';
                        areaFt2Output.value = '';
                    }
                }
                
                lengthInput.addEventListener('input', calculateDimensions);
                widthInput.addEventListener('input', calculateDimensions);

                // --- LOGICA STRUMENTO 2: MULTI-CONVERTITORE ---
                const m2Input = document.getElementById('m2-input');
                const ft2Input = document.getElementById('ft2-input');
                const yd2Input = document.getElementById('yd2-input');
                const acreInput = document.getElementById('acre-input');
                
                const areaInputs = [m2Input, ft2Input, yd2Input, acreInput];

                function formatNum(num, precision = 6) {
                    if (isNaN(num) || num === 0) return '';
                    return parseFloat(num.toPrecision(precision)).toString();
                }

                function update(sourceUnit, value) {
                    if (isNaN(value) || value === 0) {
                        areaInputs.forEach(input => input.value = '');
                        return;
                    }

                    let m2 = 0; // Base unit

                    // 1. Converti l'input sorgente in Metri Quadri (m²)
                    m2 = value / factors[sourceUnit];
                    
                    // 2. Aggiorna tutti gli altri campi
                    areaInputs.forEach(input => {
                        const unit = input.dataset.unit;
                        if (unit !== sourceUnit) {
                            input.value = formatNum(m2 * factors[unit]);
                        }
                    });
                }

                areaInputs.forEach(input => {
                  input.addEventListener('input', (e) => {
                    const sourceUnit = e.target.dataset.unit;
                    const value = parseFloat(e.target.value);
                    update(sourceUnit, value);
                  });
                });
              });
            </script>
            
