    <script>
      (function () {
        const amountInput = document.getElementById("amount");
        const gstRateInput = document.getElementById("gst-rate");
        const qtyInput = document.getElementById("quantity");

        const modeAddBtn = document.getElementById("mode-add");
        const modeRemoveBtn = document.getElementById("mode-remove");

        const amountLabel = document.getElementById("amount-label");
        const gstAmountEl = document.getElementById("gst-amount");
        const netAmountEl = document.getElementById("net-amount");
        const grossAmountEl = document.getElementById("gross-amount");
        const noteEl = document.getElementById("calculation-note");
        const resetBtn = document.getElementById("reset-btn");

        let mode = "add"; // 'add' or 'remove'

        function parseNumber(el) {
          const v = parseFloat(el.value.replace(",", "."));
          return isNaN(v) ? 0 : v;
        }

        function formatMoney(value) {
          return "$" + value.toFixed(2);
        }

        function recalc() {
          const baseAmount = parseNumber(amountInput);
          const ratePercent = parseNumber(gstRateInput);
          const qty = Math.max(parseNumber(qtyInput) || 1, 1);

          const r = ratePercent / 100;
          if (r < 0) return;

          let net = 0,
            gross = 0,
            gst = 0;

          if (mode === "add") {
            net = baseAmount * qty;
            gst = net * r;
            gross = net + gst;
          } else {
            gross = baseAmount * qty;
            const divisor = 1 + r;
            net = divisor === 0 ? 0 : gross / divisor;
            gst = gross - net;
          }

          gstAmountEl.textContent = formatMoney(gst);
          netAmountEl.textContent = formatMoney(net);
          grossAmountEl.textContent = formatMoney(gross);
        }

        function setMode(newMode) {
          mode = newMode;
          if (mode === "add") {
            modeAddBtn.classList.add("toggle-btn-active");
            modeAddBtn.classList.remove("toggle-btn-inactive");
            modeRemoveBtn.classList.add("toggle-btn-inactive");
            modeRemoveBtn.classList.remove("toggle-btn-active");
            amountLabel.textContent = "GST‑exclusive";
            noteEl.innerHTML =
              'Mode: <span class="font-semibold">Add GST</span> – input is treated as GST‑exclusive. Change mode above to remove GST from a total price.';
          } else {
            modeRemoveBtn.classList.add("toggle-btn-active");
            modeRemoveBtn.classList.remove("toggle-btn-inactive");
            modeAddBtn.classList.add("toggle-btn-inactive");
            modeAddBtn.classList.remove("toggle-btn-active");
            amountLabel.textContent = "GST‑inclusive";
            noteEl.innerHTML =
              'Mode: <span class="font-semibold">Remove GST</span> – input is treated as GST‑inclusive. Change mode above to add GST to a net price.';
          }
          recalc();
        }

        modeAddBtn.addEventListener("click", () => setMode("add"));
        modeRemoveBtn.addEventListener("click", () => setMode("remove"));

        [amountInput, gstRateInput, qtyInput].forEach((el) => {
          el.addEventListener("input", recalc);
        });

        resetBtn.addEventListener("click", () => {
          amountInput.value = "";
          gstRateInput.value = "10";
          qtyInput.value = "1";
          setMode("add");
          recalc();
        });

        // initial calculation
        recalc();
      })();
    </script>

