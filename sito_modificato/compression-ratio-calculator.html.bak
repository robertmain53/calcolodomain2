    <script>
      // Unit conversion helpers
      const PI = Math.PI;
      const CI_PER_CC = 0.0610237441; // cubic inches per cc
      const CC_PER_CI = 1 / CI_PER_CC;
      const IN_PER_MM = 0.03937007874;

      function toInches(value, unitSystem) {
        if (!isFinite(value)) return NaN;
        return unitSystem === "metric" ? value * IN_PER_MM : value;
      }

      function toCcFromCi(ci) {
        return ci * CC_PER_CI;
      }

      function toCiFromCc(cc) {
        return cc * CI_PER_CC;
      }

      function formatRatio(x) {
        if (!isFinite(x) || x <= 0) return "–";
        return x.toFixed(2) + " : 1";
      }

      function formatVolumeCc(cc) {
        if (!isFinite(cc) || cc <= 0) return "–";
        return cc.toFixed(1) + " cc";
      }

      function formatVolumeCi(ci) {
        if (!isFinite(ci) || ci <= 0) return "–";
        return ci.toFixed(3) + " ci";
      }

      function formatLengthIn(inches) {
        if (!isFinite(inches) || inches <= 0) return "–";
        return inches.toFixed(3) + " in";
      }

      function formatPressurePsi(psi) {
        if (!isFinite(psi) || psi <= 0) return "–";
        return psi.toFixed(0) + " psi";
      }

      // Mode switching
      const modeTabs = document.querySelectorAll(".cr-mode-tab");
      const dynamicSection = document.getElementById("dynamic-section");

      modeTabs.forEach((tab) => {
        tab.addEventListener("click", () => {
          const mode = tab.getAttribute("data-mode");
          modeTabs.forEach((t) => {
            t.classList.remove("bg-white", "border-gray-200", "text-blue-600");
            t.classList.add("border-transparent", "text-gray-600");
          });
          tab.classList.add("bg-white", "border-gray-200", "text-blue-600");
          tab.classList.remove("border-transparent", "text-gray-600");

          if (mode === "dynamic") {
            dynamicSection.classList.remove("hidden");
          } else {
            dynamicSection.classList.add("hidden");
          }
        });
      });

      // Unit labels
      const unitSystemSelect = document.getElementById("unit-system");
      const volumeUnitSelect = document.getElementById("volume-unit");

      function updateUnitLabels() {
        const unitSystem = unitSystemSelect.value;
        const volUnit = volumeUnitSelect.value;

        document.getElementById("bore-unit-label").textContent =
          unitSystem === "metric" ? "(mm)" : "(in)";
        document.getElementById("stroke-unit-label").textContent =
          unitSystem === "metric" ? "(mm)" : "(in)";
        document.getElementById("rod-unit-label").textContent =
          unitSystem === "metric" ? "(mm)" : "(in)";
        document.getElementById("gasket-bore-unit-label").textContent =
          unitSystem === "metric" ? "(mm)" : "(in)";
        document.getElementById("gasket-thickness-unit-label").textContent =
          unitSystem === "metric" ? "(mm)" : "(in)";
        document.getElementById("deck-unit-label").textContent =
          unitSystem === "metric" ? "(mm)" : "(in)";

        document.getElementById("chamber-unit-label").textContent =
          volUnit === "cc" ? "(cc)" : "(ci)";
        document.getElementById("piston-unit-label").textContent =
          volUnit === "cc" ? "(cc, +dish / -dome)" : "(ci, +dish / -dome)";
      }

      unitSystemSelect.addEventListener("change", updateUnitLabels);
      volumeUnitSelect.addEventListener("change", updateUnitLabels);
      updateUnitLabels();

      // Main calculation
      document.getElementById("calculate-btn").addEventListener("click", () => {
        const errorEl = document.getElementById("cr-error");
        errorEl.classList.add("hidden");

        const unitSystem = unitSystemSelect.value;
        const volUnit = volumeUnitSelect.value;

        const boreInput = parseFloat(document.getElementById("bore").value);
        const strokeInput = parseFloat(document.getElementById("stroke").value);
        const cylinders = parseInt(
          document.getElementById("cylinders").value,
          10
        );
        const rodInput = parseFloat(
          document.getElementById("rod-length").value
        );

        const chamberVolInput = parseFloat(
          document.getElementById("chamber-volume").value
        );
        const gasketBoreInput = parseFloat(
          document.getElementById("gasket-bore").value
        );
        const gasketThickInput = parseFloat(
          document.getElementById("gasket-thickness").value
        );
        const pistonVolInput = parseFloat(
          document.getElementById("piston-volume").value
        );
        const deckInput = parseFloat(
          document.getElementById("deck-height").value
        );

        const intakeClosing = parseFloat(
          document.getElementById("intake-closing").value
        );
        const boostPressure = parseFloat(
          document.getElementById("boost-pressure").value
        );

        // Basic validation
        if (
          !isFinite(boreInput) ||
          !isFinite(strokeInput) ||
          !isFinite(cylinders) ||
          !isFinite(chamberVolInput) ||
          !isFinite(gasketBoreInput) ||
          !isFinite(gasketThickInput)
        ) {
          errorEl.textContent =
            "Please enter valid numeric values for all required fields.";
          errorEl.classList.remove("hidden");
          return;
        }

        if (
          boreInput <= 0 ||
          strokeInput <= 0 ||
          cylinders <= 0 ||
          chamberVolInput < 0 ||
          gasketBoreInput <= 0 ||
          gasketThickInput < 0
        ) {
          errorEl.textContent =
            "Dimensions and volumes must be positive (or zero where allowed).";
          errorEl.classList.remove("hidden");
          return;
        }

        // Convert lengths to inches
        const boreIn = toInches(boreInput, unitSystem);
        const strokeIn = toInches(strokeInput, unitSystem);
        const rodIn = isFinite(rodInput) ? toInches(rodInput, unitSystem) : NaN;
        const gasketBoreIn = toInches(gasketBoreInput, unitSystem);
        const gasketThickIn = toInches(gasketThickInput, unitSystem);
        const deckIn = isFinite(deckInput)
          ? toInches(deckInput, unitSystem)
          : 0;

        // Volumes to cubic inches
        const chamberCi =
          volUnit === "cc" ? toCiFromCc(chamberVolInput) : chamberVolInput;
        const pistonCi = isFinite(pistonVolInput)
          ? volUnit === "cc"
            ? toCiFromCc(pistonVolInput)
            : pistonVolInput
          : 0;

        // Swept volume per cylinder
        const sweptCi = PI * Math.pow(boreIn / 2, 2) * strokeIn;

        // Gasket volume
        const gasketCi = PI * Math.pow(gasketBoreIn / 2, 2) * gasketThickIn;

        // Deck volume (can be negative if piston above deck)
        const deckCi = PI * Math.pow(boreIn / 2, 2) * deckIn;

        // Clearance volume
        const clearanceCi = chamberCi + gasketCi + deckCi + pistonCi;

        if (clearanceCi <= 0) {
          errorEl.textContent =
            "Clearance volume is zero or negative. Check chamber, gasket, piston, and deck values.";
          errorEl.classList.remove("hidden");
          return;
        }

        // Static compression ratio
        const staticCR = (sweptCi + clearanceCi) / clearanceCi;

        // Total displacement
        const totalDisplacementCi = sweptCi * cylinders;
        const totalDisplacementCc = toCcFromCi(totalDisplacementCi);

        // Dynamic compression
        let dynamicCR = NaN;
        let effectiveStrokeIn = NaN;
        let crankingPsi = NaN;

        const activeMode =
          Array.from(modeTabs)
            .find((t) => t.classList.contains("text-blue-600"))
            ?.getAttribute("data-mode") || "static";

        if (
          activeMode === "dynamic" &&
          isFinite(intakeClosing) &&
          isFinite(rodIn) &&
          rodIn > 0
        ) {
          const thetaDeg = intakeClosing;
          const thetaRad = thetaDeg * (PI / 180);

          const S = strokeIn;
          const R = rodIn;

          const halfS = S / 2;
          const sinTheta = Math.sin(thetaRad);
          const cosTheta = Math.cos(thetaRad);
          const underRoot = R * R - Math.pow(halfS * sinTheta, 2);

          if (underRoot > 0) {
            const h = halfS * (1 - cosTheta) + R - Math.sqrt(underRoot);
            effectiveStrokeIn = S - h;

            if (effectiveStrokeIn > 0 && effectiveStrokeIn < S * 1.2) {
              const effSweptCi =
                PI * Math.pow(boreIn / 2, 2) * effectiveStrokeIn;
              dynamicCR = (effSweptCi + clearanceCi) / clearanceCi;

              // Rough cranking pressure estimate (very simplified)
              const atm = 14.7;
              crankingPsi = atm * Math.pow(dynamicCR, 1.2);
            }
          }
        }

        // Boost-adjusted effective CR
        let boostCR = NaN;
        if (isFinite(boostPressure) && boostPressure > 0) {
          const atm = 14.7;
          boostCR = staticCR * ((atm + boostPressure) / atm);
        }

        // Update UI
        document.getElementById("static-cr-display").textContent =
          formatRatio(staticCR);

        const sweptCc = toCcFromCi(sweptCi);
        const clearanceCc = toCcFromCi(clearanceCi);

        document.getElementById("swept-volume-display").textContent =
          formatVolumeCc(sweptCc);
        document.getElementById("clearance-volume-display").textContent =
          formatVolumeCc(clearanceCc);
        document.getElementById("displacement-display").textContent =
          totalDisplacementCc > 0
            ? totalDisplacementCc.toFixed(1) +
              " cc (" +
              totalDisplacementCi.toFixed(1) +
              " ci)"
            : "–";

        document.getElementById("dynamic-cr-display").textContent = isFinite(
          dynamicCR
        )
          ? formatRatio(dynamicCR)
          : "–";
        document.getElementById("effective-stroke-display").textContent =
          isFinite(effectiveStrokeIn) ? formatLengthIn(effectiveStrokeIn) : "–";
        document.getElementById("cranking-pressure-display").textContent =
          isFinite(crankingPsi) ? formatPressurePsi(crankingPsi) : "–";
        document.getElementById("boost-cr-display").textContent = isFinite(
          boostCR
        )
          ? formatRatio(boostCR)
          : "–";
      });

      document.getElementById("reset-btn").addEventListener("click", () => {
        document.getElementById("bore").value = 4.0;
        document.getElementById("stroke").value = 3.48;
        document.getElementById("cylinders").value = 8;
        document.getElementById("rod-length").value = 6.0;
        document.getElementById("chamber-volume").value = 64;
        document.getElementById("gasket-bore").value = 4.1;
        document.getElementById("gasket-thickness").value = 0.04;
        document.getElementById("piston-volume").value = -5;
        document.getElementById("deck-height").value = 0.01;
        document.getElementById("intake-closing").value = 60;
        document.getElementById("boost-pressure").value = 0;
        document.getElementById("unit-system").value = "imperial";
        document.getElementById("volume-unit").value = "cc";
