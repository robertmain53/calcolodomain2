    <script>
      // Inverse kinematics logic for 2R manipulator
      function deg(rad) {
        return (rad * 180) / Math.PI;
      }
      function rad(degVal) {
        return (degVal * Math.PI) / 180;
      }

      function computeIK(L1, L2, x, y) {
        const d = Math.hypot(x, y);
        const minReach = Math.abs(L1 - L2);
        const maxReach = L1 + L2;

        if (d < minReach - 1e-9 || d > maxReach + 1e-9) {
          return { reachable: false };
        }

        let c2 = (x * x + y * y - L1 * L1 - L2 * L2) / (2 * L1 * L2);
        c2 = Math.min(1, Math.max(-1, c2)); // clamp for numerical safety
        const s2Pos = Math.sqrt(Math.max(0, 1 - c2 * c2));
        const s2Neg = -s2Pos;

        const theta2Down = Math.atan2(s2Pos, c2);
        const theta2Up = Math.atan2(s2Neg, c2);

        function theta1For(theta2) {
          const k1 = L1 + L2 * Math.cos(theta2);
          const k2 = L2 * Math.sin(theta2);
          return Math.atan2(y, x) - Math.atan2(k2, k1);
        }

        const theta1Down = theta1For(theta2Down);
        const theta1Up = theta1For(theta2Up);

        function fk(theta1, theta2) {
          const xFK = L1 * Math.cos(theta1) + L2 * Math.cos(theta1 + theta2);
          const yFK = L1 * Math.sin(theta1) + L2 * Math.sin(theta1 + theta2);
          return { x: xFK, y: yFK };
        }

        const fkDown = fk(theta1Down, theta2Down);
        const fkUp = fk(theta1Up, theta2Up);

        return {
          reachable: true,
          down: { theta1: theta1Down, theta2: theta2Down, fk: fkDown },
          up: { theta1: theta1Up, theta2: theta2Up, fk: fkUp },
        };
      }

      function drawArm(ctx, L1, L2, solution, target, options) {
        const { width, height } = ctx.canvas;
        ctx.clearRect(0, 0, width, height);

        const scale = 80; // pixels per unit (approx)
        const originX = width / 2;
        const originY = height / 2;

        function toCanvas(pt) {
          return {
            x: originX + pt.x * scale,
            y: originY - pt.y * scale,
          };
        }

        // Workspace circles
        if (options.showWorkspace) {
          ctx.save();
          ctx.strokeStyle = "#e5e7eb";
          ctx.lineWidth = 1;
          ctx.setLineDash([4, 4]);
          const outerR = (L1 + L2) * scale;
          const innerR = Math.abs(L1 - L2) * scale;
          ctx.beginPath();
          ctx.arc(originX, originY, outerR, 0, 2 * Math.PI);
          ctx.stroke();
          if (innerR > 0) {
            ctx.beginPath();
            ctx.arc(originX, originY, innerR, 0, 2 * Math.PI);
            ctx.stroke();
          }
          ctx.restore();
        }

        // Target point
        if (target) {
          const t = toCanvas(target);
          ctx.save();
          ctx.fillStyle = "#dc2626";
          ctx.beginPath();
          ctx.arc(t.x, t.y, 4, 0, 2 * Math.PI);
          ctx.fill();
          ctx.restore();
        }

        if (!solution) return;

        function drawConfig(sol, color, alpha) {
          const { theta1, theta2 } = sol;
          const joint1 = { x: 0, y: 0 };
          const joint2 = {
            x: L1 * Math.cos(theta1),
            y: L1 * Math.sin(theta1),
          };
          const end = {
            x: joint2.x + L2 * Math.cos(theta1 + theta2),
            y: joint2.y + L2 * Math.sin(theta1 + theta2),
          };

          const p0 = toCanvas(joint1);
          const p1 = toCanvas(joint2);
          const p2 = toCanvas(end);

          ctx.save();
          ctx.globalAlpha = alpha;
          ctx.lineWidth = 4;
          ctx.strokeStyle = color;
          ctx.lineCap = "round";

          ctx.beginPath();
          ctx.moveTo(p0.x, p0.y);
          ctx.lineTo(p1.x, p1.y);
          ctx.lineTo(p2.x, p2.y);
          ctx.stroke();

          ctx.fillStyle = "#111827";
          ctx.beginPath();
          ctx.arc(p0.x, p0.y, 4, 0, 2 * Math.PI);
          ctx.fill();

          ctx.fillStyle = "#4b5563";
          ctx.beginPath();
          ctx.arc(p1.x, p1.y, 4, 0, 2 * Math.PI);
          ctx.fill();

          ctx.fillStyle = color;
          ctx.beginPath();
          ctx.arc(p2.x, p2.y, 4, 0, 2 * Math.PI);
          ctx.fill();

          ctx.restore();
        }

        if (options.branch === "elbow-down" && solution.down) {
          drawConfig(solution.down, "#2563eb", 1.0);
        } else if (options.branch === "elbow-up" && solution.up) {
          drawConfig(solution.up, "#16a34a", 1.0);
        } else if (options.branch === "both") {
          if (solution.down) drawConfig(solution.down, "#2563eb", 0.9);
          if (solution.up) drawConfig(solution.up, "#16a34a", 0.7);
        }
      }

      document.addEventListener("DOMContentLoaded", function () {
        const link1El = document.getElementById("link1");
        const link2El = document.getElementById("link2");
        const xEl = document.getElementById("targetX");
        const yEl = document.getElementById("targetY");
        const msgEl = document.getElementById("ikMessage");
        const canvas = document.getElementById("ikCanvas");
        const ctx = canvas.getContext("2d");
        const showWorkspaceEl = document.getElementById("showWorkspace");

        const theta1DownEl = document.getElementById("theta1Down");
        const theta2DownEl = document.getElementById("theta2Down");
        const fkXDownEl = document.getElementById("fkXDown");
        const fkYDownEl = document.getElementById("fkYDown");

        const theta1UpEl = document.getElementById("theta1Up");
        const theta2UpEl = document.getElementById("theta2Up");
        const fkXUpEl = document.getElementById("fkXUp");
        const fkYUpEl = document.getElementById("fkYUp");

        function getBranch() {
          const radios = document.querySelectorAll('input[name="branch"]');
          for (const r of radios) {
            if (r.checked) return r.value;
          }
          return "elbow-down";
        }

        function format(val) {
          if (!isFinite(val)) return "";
          return Math.round(val * 1000) / 1000;
        }

        function update() {
          const L1 = parseFloat(link1El.value);
          const L2 = parseFloat(link2El.value);
          const x = parseFloat(xEl.value);
          const y = parseFloat(yEl.value);

          if (!(L1 > 0) || !(L2 > 0)) {
            msgEl.className = "mb-4 text-sm text-red-600";
            msgEl.textContent = "Link lengths L₁ and L₂ must be positive.";
            drawArm(
              ctx,
              L1 || 1,
              L2 || 1,
              null,
              { x, y },
              { branch: getBranch(), showWorkspace: showWorkspaceEl.checked }
            );
            return;
          }

          const result = computeIK(L1, L2, x, y);

          if (!result.reachable) {
            msgEl.className = "mb-4 text-sm text-red-600";
            msgEl.textContent =
              "Target is unreachable for the given link lengths. Adjust L₁, L₂, or (x, y).";

            theta1DownEl.value =
              theta2DownEl.value =
              fkXDownEl.value =
              fkYDownEl.value =
                "";
            theta1UpEl.value =
              theta2UpEl.value =
              fkXUpEl.value =
              fkYUpEl.value =
                "";

            drawArm(
              ctx,
              L1,
              L2,
              null,
              { x, y },
              { branch: getBranch(), showWorkspace: showWorkspaceEl.checked }
            );
            return;
          }

          msgEl.className = "mb-4 text-sm text-green-700";
          msgEl.textContent =
            "Solution found. Forward kinematics values show numerical agreement with the target.";

          if (result.down) {
            theta1DownEl.value = format(deg(result.down.theta1));
            theta2DownEl.value = format(deg(result.down.theta2));
            fkXDownEl.value = format(result.down.fk.x);
            fkYDownEl.value = format(result.down.fk.y);
          }

          if (result.up) {
            theta1UpEl.value = format(deg(result.up.theta1));
            theta2UpEl.value = format(deg(result.up.theta2));
            fkXUpEl.value = format(result.up.fk.x);
            fkYUpEl.value = format(result.up.fk.y);
          }

          drawArm(
            ctx,
            L1,
            L2,
            result,
            { x, y },
            { branch: getBranch(), showWorkspace: showWorkspaceEl.checked }
          );
        }

        document.getElementById("computeBtn").addEventListener("click", update);
        document
          .getElementById("resetBtn")
          .addEventListener("click", function () {
            link1El.value = 2;
            link2El.value = 1.5;
            xEl.value = 2.5;
            yEl.value = 0.5;
            document.querySelector(
              'input[name="branch"][value="elbow-down"]'
            ).checked = true;
            showWorkspaceEl.checked = true;
            update();
          });

        document.querySelectorAll('input[name="branch"]').forEach((r) => {
          r.addEventListener("change", update);
        });
        showWorkspaceEl.addEventListener("change", update);

        // Click-to-set target
        canvas.addEventListener("click", function (e) {
          const rect = canvas.getBoundingClientRect();
          const cx = e.clientX - rect.left;
          const cy = e.clientY - rect.top;
          const originX = canvas.width / 2;
          const originY = canvas.height / 2;
          const scale = 80;

          const x = (cx - originX) / scale;
          const y = (originY - cy) / scale;

          xEl.value = format(x);
          yEl.value = format(y);
          update();
        });

        // Initial draw
        update();
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
