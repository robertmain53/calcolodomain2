  function formatPct(v) {
    return (v * 100).toFixed(2) + '%';
  }
  function formatMoney(v) {
    return '$' + v.toFixed(2).replace(/\B(?=(\d{3})+(?!\d))/g, ',');
  }

  function populateCountySelects() {
    const resSel = document.getElementById('county-res');
    const workSel = document.getElementById('county-work');
    countyRates.forEach(c => {
      const opt1 = document.createElement('option');
      opt1.value = c.code;
      opt1.textContent = `${c.name} (${c.code})`;
      resSel.appendChild(opt1);

      const opt2 = document.createElement('option');
      opt2.value = c.code;
      opt2.textContent = `${c.name} (${c.code})`;
      workSel.appendChild(opt2);
    });
    // Default to Marion for both
    resSel.value = '49';
    workSel.value = '49';
  }

  function populateRatesTable() {
    const tbody = document.getElementById('rates-tbody');
    countyRates.forEach(c => {
      const tr = document.createElement('tr');
      tr.innerHTML = `
        <td class="sticky-col px-3 py-1 whitespace-nowrap font-medium text-gray-700">${c.name}</td>
        <td class="px-3 py-1 text-right">${formatPct(c.r2025)}</td>
        <td class="px-3 py-1 text-right">${formatPct(c.nr2025)}</td>
        <td class="px-3 py-1 text-right">${formatPct(c.r2024)}</td>
        <td class="px-3 py-1 text-right">${formatPct(c.nr2024)}</td>
      `;
      tbody.appendChild(tr);
    });
  }

  function getCountyRate(code, year, type) {
    const c = countyRates.find(c => c.code === code);
    if (!c) return 0;
    if (year === '2025') {
      return type === 'res' ? c.r2025 : c.nr2025;
    } else {
      return type === 'res' ? c.r2024 : c.nr2024;
    }
  }

  function calculateTaxes() {
    const income = parseFloat(document.getElementById('annual-income').value) || 0;
    const year = document.getElementById('tax-year').value;
    const resCode = document.getElementById('county-res').value;
    const workCode = document.getElementById('county-work').value;
    const showPP = document.getElementById('show-paycheck').checked;

    if (income <= 0) {
      document.getElementById('results-summary').textContent = 'Enter a positive Indiana taxable income amount.';
      return;
    }

    const stateTax = income * IN_STATE_RATE;
    const resRate = getCountyRate(resCode, year, 'res');
    const workRate = workCode && workCode !== resCode ? getCountyRate(workCode, year, 'nr') : 0;

    const resTax = income * resRate;
    const workTax = income * workRate;
    const totalTax = stateTax + resTax + workTax;

    const resCounty = countyRates.find(c => c.code === resCode);
    const workCounty = countyRates.find(c => c.code === workCode);

    document.getElementById('state-tax').textContent = formatMoney(stateTax);
    document.getElementById('county-res-tax').textContent = formatMoney(resTax);
    document.getElementById('county-work-tax').textContent = formatMoney(workTax);
    document.getElementById('total-tax').textContent = formatMoney(totalTax);

    const summaryParts = [];
    summaryParts.push(`Using a flat Indiana state rate of ${(IN_STATE_RATE*100).toFixed(2)}%.`);
    if (resCounty) {
      summaryParts.push(`Resident county: ${resCounty.name} at ${formatPct(resRate)}.`);
    }
    if (workCounty && workRate > 0) {
      summaryParts.push(`Employment county: ${workCounty.name} nonresident rate ${formatPct(workRate)}.`);
    }
    document.getElementById('results-summary').textContent = summaryParts.join(' ');

    const ppDiv = document.getElementById('per-paycheck');
    if (showPP) {
      const periods = 26;
      document.getElementById('state-tax-pp').textContent = formatMoney(stateTax / periods);
      document.getElementById('county-tax-pp').textContent = formatMoney((resTax + workTax) / periods);
      document.getElementById('total-tax-pp').textContent = formatMoney(totalTax / periods);
      ppDiv.classList.remove('hidden');
    } else {
      ppDiv.classList.add('hidden');
    }
  }

  document.addEventListener('DOMContentLoaded', () => {
    populateCountySelects();
    populateRatesTable();
    document.getElementById('calc-btn').addEventListener('click', calculateTaxes);
  });

  window.MathJax = {
    tex: { inlineMath: [['\\(','\\)'], ['$', '$']], displayMath: [['$','$'], ['\\[','\\]']] },
    svg: { fontCache: 'global' }
  };
</script>
