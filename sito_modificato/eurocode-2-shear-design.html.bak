    <script>
      // Utility functions
      function round(value, decimals) {
        if (!isFinite(value)) return value;
        const factor = Math.pow(10, decimals);
        return Math.round(value * factor) / factor;
      }

      function setText(id, value) {
        const el = document.getElementById(id);
        if (el) el.textContent = value;
      }

      function calcShear() {
        const fck = parseFloat(document.getElementById("fck").value) || 0;
        const bw = parseFloat(document.getElementById("bw").value) || 0;
        const d = parseFloat(document.getElementById("d").value) || 0;
        const Asl = parseFloat(document.getElementById("Asl").value) || 0;
        const Ved = parseFloat(document.getElementById("Ved").value) || 0; // kN
        const Med = parseFloat(document.getElementById("Med").value) || 0; // kNm (not used directly)
        const fyk = parseFloat(document.getElementById("fyk").value) || 0;
        const gammaC =
          parseFloat(document.getElementById("gammaC").value) || 1.5;
        const gammaS =
          parseFloat(document.getElementById("gammaS").value) || 1.15;
        const phiW = parseFloat(document.getElementById("phiW").value) || 0;
        const nLegs = parseFloat(document.getElementById("nLegs").value) || 0;
        const s = parseFloat(document.getElementById("s").value) || 0;
        const densityType = document.getElementById("densityType").value;
        const thetaOption = document.getElementById("thetaOption").value;

        const resultsSection = document.getElementById("resultsSection");
        const overallStatus = document.getElementById("overallStatus");
        if (!fck || !bw || !d || !Ved || !fyk || !phiW || !nLegs || !s) {
          alert(
            "Please fill in all key inputs (fck, bw, d, Asl, Ved, fyk, link data)."
          );
          return;
        }

        // 1. Concrete shear resistance VRd,c
        const k = Math.min(2.0, 1 + Math.sqrt(200 / d));
        let rhol = Asl / (bw * d);
        if (!isFinite(rhol) || rhol < 0) rhol = 0;
        rhol = Math.min(rhol, 0.02);
        const Crdc = 0.18 / gammaC;
        const k1 = 0.15; // default EC2
        const vmin = 0.035 * Math.pow(k, 1.5) * Math.sqrt(fck); // MPa
        let vRdc = Crdc * k * Math.pow(100 * rhol * fck, 1 / 3); // MPa
        if (!isFinite(vRdc)) vRdc = 0;
        if (vRdc < vmin) vRdc = vmin;

        const vEd = (Ved * 1000) / (bw * d); // MPa
        const Vrdc = (vRdc * bw * d) / 1000; // kN

        // 2. Shear reinforcement VRd,s
        const fyd = fyk / gammaS;
        let cotTheta = 2.0;
        if (thetaOption === "cot15") cotTheta = 1.5;
        if (thetaOption === "cot25") cotTheta = 2.5;
        const z = 0.9 * d; // mm
        const AswLeg = (Math.PI * Math.pow(phiW, 2)) / 4; // mm² per leg
        const AswProv = AswLeg * nLegs; // mm² per link
        const AswProvPerS = (AswProv / s) * 1000; // mm²/m
        const VrdS = ((AswProv / s) * z * fyd * cotTheta) / 1000; // kN

        // 3. Required shear reinforcement
        let AswReqPerS = 0;
        if (Ved > Vrdc) {
          AswReqPerS = ((Ved - Vrdc) * 1000 * 1000) / (z * fyd * cotTheta); // mm²/m
        }

        // 4. Minimum shear reinforcement (simplified expression)
        const AswMinPerS = ((0.08 * Math.sqrt(fck)) / fyk) * bw * 1000; // mm²/m

        // 5. Spacing limits (indicative)
        const sMax1 = 0.75 * d;
        const sMax2 = 600;
        const sMax = Math.min(sMax1, sMax2);

        // Status messages
        const concreteOK = Ved <= Vrdc;
        const shearReinfOK = Ved <= Vrdc + VrdS;
        const provVsReqOK = AswProvPerS + 1e-6 >= AswReqPerS; // tolerance
        const minReinfOK = AswProvPerS + 1e-6 >= AswMinPerS;
        const spacingOK = s <= sMax;

        // Overall status
        let statusText = "";
        let statusClass = "";
        if (shearReinfOK && minReinfOK && spacingOK) {
          statusText =
            "Design OK: provided shear reinforcement satisfies Eurocode 2 checks.";
          statusClass = "result-badge-ok";
        } else if (shearReinfOK) {
          statusText =
            "Design conditionally OK: shear capacity is adequate, but some detailing checks fail.";
          statusClass = "result-badge-warn";
        } else {
          statusText = "Design NOT OK: shear capacity is insufficient for VEd.";
          statusClass = "result-badge-fail";
        }
        overallStatus.textContent = statusText;
        overallStatus.className =
          "mb-4 text-sm font-semibold inline-flex items-center px-3 py-1 rounded-full " +
          statusClass;

        // Fill numeric outputs
        setText("vRdc", round(Vrdc, 1));
        setText("vEd", round(vEd, 3));
        setText("vRdcStress", round(vRdc, 3));
        setText("vRds", round(VrdS, 1));
        setText("aswReqPerS", round(AswReqPerS, 1));
        setText("aswProvPerS", round(AswProvPerS, 1));
        setText("aswMinPerS", round(AswMinPerS, 1));
        setText("sMax", round(sMax, 0));

        // Debug / intermediate
        setText("dbg_d", round(d, 0));
        setText("dbg_bw", round(bw, 0));
        setText("dbg_rhol", round(rhol, 4));
        setText("dbg_k", round(k, 3));
        setText("dbg_Crdc", round(Crdc, 3));
        setText("dbg_k1", round(k1, 3));
        setText("dbg_vmin", round(vmin, 3));
        setText("dbg_fyd", round(fyd, 1));
        setText("dbg_cotTheta", round(cotTheta, 2));
        setText("dbg_AswLeg", round(AswLeg, 1));
        setText("dbg_AswProvPerS", round(AswProvPerS, 1));

        // Textual checks
        const concreteShearCheck =
          document.getElementById("concreteShearCheck");
        if (concreteOK) {
          concreteShearCheck.textContent =
            "Concrete alone is sufficient for the applied shear (VEd ≤ VRd,c). Shear links may still be required for minimum reinforcement and crack control.";
        } else {
          concreteShearCheck.textContent =
            "Concrete alone is NOT sufficient (VEd > VRd,c). Shear reinforcement is required.";
        }

        const shearReinfCheck = document.getElementById("shearReinfCheck");
        if (shearReinfOK) {
          shearReinfCheck.textContent =
            "Provided links give VRd,s such that VEd ≤ VRd,c + VRd,s.";
        } else {
          shearReinfCheck.textContent =
            "Provided links are insufficient: increase bar diameter, number of legs, or reduce spacing.";
        }

        const minReinfCheck = document.getElementById("minReinfCheck");
        if (minReinfOK && spacingOK) {
          minReinfCheck.textContent =
            "Provided shear reinforcement satisfies minimum area and spacing limits.";
        } else if (!minReinfOK && spacingOK) {
          minReinfCheck.textContent =
            "Provided shear reinforcement is below minimum Asw,min/s. Increase link area or reduce spacing.";
        } else if (minReinfOK && !spacingOK) {
          minReinfCheck.textContent =
            "Shear capacity is OK but spacing exceeds recommended maximum. Reduce s to satisfy detailing rules.";
        } else {
          minReinfCheck.textContent =
            "Both minimum area and spacing requirements are not satisfied. Revise shear reinforcement layout.";
        }

        // Design summary bullet list
        const summary = document.getElementById("designSummary");
        summary.innerHTML = "";
        const items = [];

        items.push(`Design shear force VEd = ${round(Ved, 1)} kN.`);
        items.push(`Concrete shear resistance VRd,c = ${round(Vrdc, 1)} kN.`);
        items.push(`Shear resistance of links VRd,s = ${round(VrdS, 1)} kN.`);
        items.push(
          `Required shear steel Asw,req/s = ${round(AswReqPerS, 1)} mm²/m.`
        );
        items.push(
          `Provided shear steel Asw/s = ${round(AswProvPerS, 1)} mm²/m.`
        );
        items.push(
          `Minimum shear steel Asw,min/s = ${round(AswMinPerS, 1)} mm²/m.`
        );
        items.push(
          `Link spacing s = ${round(s, 0)} mm (limit ≈ ${round(sMax, 0)} mm).`
        );

        items.forEach((text) => {
          const li = document.createElement("li");
          li.textContent = text;
          summary.appendChild(li);
        });

        resultsSection.classList.remove("hidden");
      }

      document
        .getElementById("btnCalculate")
        .addEventListener("click", calcShear);

      document.getElementById("btnReset").addEventListener("click", () => {
        document.getElementById("shearForm").reset();
        document.getElementById("fck").value = 30;
        document.getElementById("bw").value = 300;
        document.getElementById("d").value = 500;
        document.getElementById("Asl").value = 2010;
        document.getElementById("Ved").value = 250;
        document.getElementById("Med").value = 300;
        document.getElementById("fyk").value = 500;
        document.getElementById("gammaC").value = 1.5;
        document.getElementById("gammaS").value = 1.15;
        document.getElementById("phiW").value = 10;
        document.getElementById("nLegs").value = 2;
        document.getElementById("s").value = 200;
        document.getElementById("densityType").value = "normal";
        document.getElementById("thetaOption").value = "cot2";
        document.getElementById("resultsSection").classList.add("hidden");
      });

      // Basic keyboard accessibility: Enter key triggers calculation
      document.getElementById("shearForm").addEventListener("keydown", (e) => {
        if (e.key === "Enter") {
          e.preventDefault();
          calcShear();
        }
      });

      // Mobile menu script (simple)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", () => {
