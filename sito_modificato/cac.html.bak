    <script>
      // CAC calculator logic
      (function () {
        const channelRowsBody = document.getElementById("channel-rows");
        const addChannelBtn = document.getElementById("add-channel");
        const resetBtn = document.getElementById("reset-channels");
        const currencySelect = document.getElementById("currency");
        const periodSelect = document.getElementById("period");
        const avgMonthlyGpInput = document.getElementById("avg-monthly-gp");
        const currencyPrefixAvgGp = document.getElementById(
          "currency-prefix-avg-gp"
        );

        const totalMarketingEl = document.getElementById("total-marketing");
        const totalSalesEl = document.getElementById("total-sales");
        const totalCustomersEl = document.getElementById("total-customers");
        const blendedCacEl = document.getElementById("blended-cac");

        const summaryBlendedCacEl = document.getElementById(
          "summary-blended-cac"
        );
        const summaryPaybackEl = document.getElementById("summary-payback");
        const summaryTotalSpendEl = document.getElementById(
          "summary-total-spend"
        );
        const channelComparisonText = document.getElementById(
          "channel-comparison-text"
        );
        const bestChannelBadge = document.getElementById("best-channel-badge");

        let channelIdCounter = 0;

        function formatCurrency(value, currencySymbol) {
          if (isNaN(value)) return "–";
          return (
            currencySymbol +
            value.toLocaleString(undefined, { maximumFractionDigits: 2 })
          );
        }

        function formatNumber(value) {
          if (isNaN(value)) return "–";
          return value.toLocaleString(undefined, { maximumFractionDigits: 2 });
        }

        function createChannelRow(
          name = "",
          marketing = "",
          sales = "",
          customers = "",
          note = ""
        ) {
          const id = channelIdCounter++;
          const tr = document.createElement("tr");
          tr.dataset.id = id;

          tr.innerHTML = `
        <td class="px-3 py-2">
          <input type="text" class="table-input" value="${name}" placeholder="e.g. Paid Search">
        </td>
        <td class="px-3 py-2 text-right">
          <input type="number" min="0" step="0.01" class="table-input text-right" value="${marketing}">
        </td>
        <td class="px-3 py-2 text-right">
          <input type="number" min="0" step="0.01" class="table-input text-right" value="${sales}">
        </td>
        <td class="px-3 py-2 text-right">
          <input type="number" min="0" step="1" class="table-input text-right" value="${customers}">
        </td>
        <td class="px-3 py-2 text-right">
          <span class="inline-block text-xs font-semibold" data-channel-cac>–</span>
        </td>
        <td class="px-3 py-2 text-right">
          <input type="text" class="table-input" value="${note}" placeholder="Optional note">
        </td>
        <td class="px-3 py-2 text-right">
          <button type="button" class="text-xs text-red-500 hover:text-red-700" data-remove-channel>&times;</button>
        </td>
      `;

          channelRowsBody.appendChild(tr);
        }

        function initDefaultRows() {
          channelRowsBody.innerHTML = "";
          createChannelRow("Paid Search", "15000", "8000", "200", "");
          createChannelRow("Organic / SEO", "5000", "4000", "180", "");
          createChannelRow("Outbound Sales", "2000", "12000", "90", "");
          updateCalculations();
        }

        function getChannelData() {
          const rows = Array.from(channelRowsBody.querySelectorAll("tr"));
          return rows.map((row) => {
            const inputs = row.querySelectorAll("input");
            const name = inputs[0].value.trim() || "Channel";
            const marketing = parseFloat(inputs[1].value) || 0;
            const sales = parseFloat(inputs[2].value) || 0;
            const customers = parseFloat(inputs[3].value) || 0;
            const note = inputs[4].value.trim();
            const cacSpan = row.querySelector("[data-channel-cac]");
            return { row, name, marketing, sales, customers, note, cacSpan };
          });
        }

        function updateCalculations() {
          const currencySymbol = currencySelect.value || "$";
          currencyPrefixAvgGp.textContent = currencySymbol;

          const channels = getChannelData();
          let totalMarketing = 0;
          let totalSales = 0;
          let totalCustomers = 0;

          channels.forEach((ch) => {
            totalMarketing += ch.marketing;
            totalSales += ch.sales;
            totalCustomers += ch.customers;

            const totalCost = ch.marketing + ch.sales;
            let cac = NaN;
            if (ch.customers > 0) {
              cac = totalCost / ch.customers;
            }
            ch.cacSpan.textContent = isNaN(cac)
              ? "–"
              : formatCurrency(cac, currencySymbol + " ");
          });

          const totalSpend = totalMarketing + totalSales;
          const blendedCac =
            totalCustomers > 0 ? totalSpend / totalCustomers : NaN;

          totalMarketingEl.textContent = formatCurrency(
            totalMarketing,
            currencySymbol + " "
          );
          totalSalesEl.textContent = formatCurrency(
            totalSales,
            currencySymbol + " "
          );
          totalCustomersEl.textContent = isNaN(totalCustomers)
            ? "–"
            : totalCustomers.toLocaleString();

          blendedCacEl.textContent = formatCurrency(
            blendedCac,
            currencySymbol + " "
          );

          summaryTotalSpendEl.textContent = formatCurrency(
            totalSpend,
            currencySymbol + " "
          );
          summaryBlendedCacEl.textContent = formatCurrency(
            blendedCac,
            currencySymbol + " "
          );

          const avgMonthlyGp = parseFloat(avgMonthlyGpInput.value) || 0;
          if (!isNaN(blendedCac) && blendedCac > 0 && avgMonthlyGp > 0) {
            const paybackMonths = blendedCac / avgMonthlyGp;
            summaryPaybackEl.textContent =
              formatNumber(paybackMonths) + " months";
          } else {
            summaryPaybackEl.textContent = "–";
          }

          // Channel comparison
          const channelsWithCustomers = channels.filter(
            (ch) => ch.customers > 0
          );
          if (channelsWithCustomers.length >= 2) {
            const withCac = channelsWithCustomers
              .map((ch) => {
                const totalCost = ch.marketing + ch.sales;
                const cac = totalCost / ch.customers;
                return { name: ch.name, cac };
              })
              .filter((x) => isFinite(x.cac) && x.cac >= 0);

            if (withCac.length >= 2) {
              withCac.sort((a, b) => a.cac - b.cac);
              const best = withCac[0];
              const worst = withCac[withCac.length - 1];
              const improvement =
                worst.cac > 0 ? ((worst.cac - best.cac) / worst.cac) * 100 : 0;

              channelComparisonText.textContent =
                `${best.name} currently has the lowest CAC at ${formatCurrency(
                  best.cac,
                  currencySymbol + " "
                )}. ` +
                `${worst.name} is highest at ${formatCurrency(
                  worst.cac,
                  currencySymbol + " "
                )} ` +
                (improvement > 0
                  ? `— a ${improvement.toFixed(
                      1
                    )}% improvement opportunity if you can shift budget efficiently.`
                  : "");
              bestChannelBadge.classList.remove("hidden");
            } else {
              channelComparisonText.textContent =
                "Add more data to compare CAC across channels.";
              bestChannelBadge.classList.add("hidden");
            }
          } else {
            channelComparisonText.textContent =
              "Add at least two channels to compare CAC efficiency.";
            bestChannelBadge.classList.add("hidden");
          }
        }

        // Event listeners
        addChannelBtn.addEventListener("click", () => {
          createChannelRow("", "", "", "", "");
          updateCalculations();
        });

        resetBtn.addEventListener("click", () => {
          initDefaultRows();
        });

        channelRowsBody.addEventListener("input", (e) => {
          if (e.target.matches("input")) {
            updateCalculations();
          }
        });

        channelRowsBody.addEventListener("click", (e) => {
          if (e.target.matches("[data-remove-channel]")) {
            const row = e.target.closest("tr");
            if (row) {
              row.remove();
              updateCalculations();
            }
          }
        });

        currencySelect.addEventListener("change", updateCalculations);
        periodSelect.addEventListener("change", updateCalculations);
        avgMonthlyGpInput.addEventListener("input", updateCalculations);

        // Initialize
        initDefaultRows();
      })();
    </script>

