    <script src="https://cdn.jsdelivr.net/npm/chart.js@4.4.1/dist/chart.umd.min.js"></script>
    <script>
      // Utility: parse sieve size to numeric (mm or inch)
      function parseSieveSize(value) {
        const v = parseFloat(value);
        return isNaN(v) ? null : v;
      }

      const sieveTbody = document.getElementById("sieve-tbody");
      const totalMassInput = document.getElementById("total-mass");
      const massUnitSelect = document.getElementById("mass-unit");
      const sieveUnitSelect = document.getElementById("sieve-unit");
      const addRowBtn = document.getElementById("add-row");
      const resetRowsBtn = document.getElementById("reset-rows");
      const calculateBtn = document.getElementById("calculate-btn");
      const templateSelect = document.getElementById("template-select");
      const targetSelect = document.getElementById("target-select");
      const sumMassRetainedEl = document.getElementById("sum-mass-retained");
      const sumMassUnitEl = document.getElementById("sum-mass-unit");
      const sumPercentRetainedEl = document.getElementById(
        "sum-percent-retained"
      );
      const massBalanceMsg = document.getElementById("mass-balance-msg");
      const outOfBandSummary = document.getElementById("out-of-band-summary");

      let gradationChart = null;

      // Example target bands (illustrative only)
      const targetBands = {
        "astm-34": {
          unit: "inch",
          bands: [
            { size: 0.75, min: 90, max: 100 },
            { size: 0.5, min: 25, max: 60 },
            { size: 0.375, min: 10, max: 30 },
            { size: 0.187, min: 0, max: 10 },
            { size: 0.0937, min: 0, max: 5 },
          ],
        },
        "astm-fine": {
          unit: "mm",
          bands: [
            { size: 9.5, min: 100, max: 100 }, // 3/8"
            { size: 4.75, min: 95, max: 100 }, // No.4
            { size: 2.36, min: 80, max: 100 }, // No.8
            { size: 1.18, min: 50, max: 85 }, // No.16
            { size: 0.6, min: 25, max: 60 }, // No.30
            { size: 0.3, min: 10, max: 30 }, // No.50
            { size: 0.15, min: 2, max: 10 }, // No.100
          ],
        },
      };

      function createRow(sieveLabel = "", opening = "", massRetained = "") {
        const tr = document.createElement("tr");
        tr.innerHTML = `
      <td class="px-2 py-1 text-left">
        <input type="text" class="w-20 border-gray-300 rounded-md px-2 py-1 text-sm" placeholder="e.g. #4" value="${sieveLabel}">
      </td>
      <td class="px-2 py-1 text-left">
        <div class="flex items-center gap-1">
          <input type="number" step="0.0001" class="w-24 border-gray-300 rounded-md px-2 py-1 text-sm sieve-opening" value="${opening}">
          <span class="text-xs text-gray-500 sieve-unit-label">${sieveUnitSelect.value}</span>
        </div>
      </td>
      <td class="px-2 py-1">
        <input type="number" step="0.01" class="table-input w-24 border-gray-300 rounded-md px-2 py-1 text-sm mass-retained" value="${massRetained}">
      </td>
      <td class="px-2 py-1 text-gray-700"><span class="percent-retained">–</span></td>
      <td class="px-2 py-1 text-gray-700"><span class="percent-cum-retained">–</span></td>
      <td class="px-2 py-1 text-gray-700">
        <span class="percent-passing">–</span>
        <span class="ml-1 text-[10px] px-1 py-0.5 rounded hidden status-badge"></span>
      </td>
      <td class="px-2 py-1 text-xs text-gray-500">
        <span class="target-range">–</span>
      </td>
      <td class="px-2 py-1">
        <button type="button" class="text-xs text-red-600 hover:underline remove-row">Remove</button>
      </td>
    `;
        sieveTbody.appendChild(tr);
      }

      function resetTable() {
        sieveTbody.innerHTML = "";
        // Default basic sieve set
        const defaults = [
          { label: '3/4"', openingInch: 0.75 },
          { label: '1/2"', openingInch: 0.5 },
          { label: '3/8"', openingInch: 0.375 },
          { label: "No. 4", openingInch: 0.187 },
          { label: "No. 8", openingInch: 0.0937 },
          { label: "Pan", openingInch: 0 },
        ];
        const unit = sieveUnitSelect.value;
        defaults.forEach((d) => {
          let opening = d.openingInch;
          if (unit === "mm") opening = d.openingInch * 25.4;
          createRow(d.label, opening ? opening.toFixed(3) : "");
        });
        updateSieveUnitLabels();
        clearResults();
      }

      function applyTemplate(template) {
        sieveTbody.innerHTML = "";
        const unit = sieveUnitSelect.value;
        if (template === "astm-34") {
          const sieves = [
            { label: '1"', sizeInch: 1.0 },
            { label: '3/4"', sizeInch: 0.75 },
            { label: '1/2"', sizeInch: 0.5 },
            { label: '3/8"', sizeInch: 0.375 },
            { label: "No. 4", sizeInch: 0.187 },
            { label: "No. 8", sizeInch: 0.0937 },
            { label: "Pan", sizeInch: 0 },
          ];
          sieves.forEach((s) => {
            let opening = s.sizeInch;
            if (unit === "mm") opening = s.sizeInch * 25.4;
            createRow(s.label, opening ? opening.toFixed(3) : "");
          });
        } else if (template === "astm-fine") {
          const sieves = [
            { label: '3/8"', sizeMm: 9.5 },
            { label: "No. 4", sizeMm: 4.75 },
            { label: "No. 8", sizeMm: 2.36 },
            { label: "No. 16", sizeMm: 1.18 },
            { label: "No. 30", sizeMm: 0.6 },
            { label: "No. 50", sizeMm: 0.3 },
            { label: "No. 100", sizeMm: 0.15 },
            { label: "Pan", sizeMm: 0 },
          ];
          sieves.forEach((s) => {
            let opening = s.sizeMm;
            if (unit === "inch") opening = s.sizeMm / 25.4;
            createRow(s.label, opening ? opening.toFixed(3) : "");
          });
        } else {
          resetTable();
        }
        updateSieveUnitLabels();
        clearResults();
      }

      function updateSieveUnitLabels() {
        document.querySelectorAll(".sieve-unit-label").forEach((el) => {
          el.textContent = sieveUnitSelect.value;
        });
      }

      function clearResults() {
        sumMassRetainedEl.textContent = "0.00";
        sumPercentRetainedEl.textContent = "0.00";
        massBalanceMsg.textContent = "Mass balance not checked yet.";
        massBalanceMsg.className = "text-xs text-gray-600";
        outOfBandSummary.textContent =
          "No calculation yet. Out-of-band sieves will be summarized here.";
        outOfBandSummary.className =
          "text-xs text-gray-700 bg-gray-50 border border-dashed border-gray-300 rounded-md p-2 w-full";
        Array.from(sieveTbody.querySelectorAll("tr")).forEach((tr) => {
          tr.querySelector(".percent-retained").textContent = "–";
          tr.querySelector(".percent-cum-retained").textContent = "–";
          tr.querySelector(".percent-passing").textContent = "–";
          tr.querySelector(".target-range").textContent = "–";
          const badge = tr.querySelector(".status-badge");
          badge.textContent = "";
          badge.classList.add("hidden");
        });
        if (gradationChart) {
          gradationChart.data.labels = [];
          gradationChart.data.datasets[0].data = [];
          gradationChart.update();
        }
      }

      function calculate() {
        const totalMass = parseFloat(totalMassInput.value);
        if (isNaN(totalMass) || totalMass <= 0) {
          alert("Please enter a valid total sample mass.");
          return;
        }
        const rows = Array.from(sieveTbody.querySelectorAll("tr"));
        if (!rows.length) {
          alert("Please add at least one sieve row.");
          return;
        }

        // Gather data
        let data = [];
        rows.forEach((tr) => {
          const label = tr.cells[0].querySelector("input").value.trim();
          const openingVal = tr.querySelector(".sieve-opening").value;
          const opening = parseSieveSize(openingVal);
          const massVal = tr.querySelector(".mass-retained").value;
          const mass = parseFloat(massVal);
          if (opening !== null && !isNaN(mass) && mass >= 0) {
            data.push({ tr, label, opening, mass });
          }
        });

        if (!data.length) {
          alert("Please enter sieve openings and masses retained.");
          return;
        }

        // Sort by opening descending (largest first)
        data.sort((a, b) => b.opening - a.opening);

        // Compute sums and percentages
        let sumMass = 0;
        data.forEach((d) => (sumMass += d.mass));
        sumMassRetainedEl.textContent = sumMass.toFixed(2);
        sumMassUnitEl.textContent = massUnitSelect.value;

        const percentDiff = ((sumMass - totalMass) / totalMass) * 100;
        sumPercentRetainedEl.textContent = (
          (sumMass / totalMass) *
          100
        ).toFixed(2);

        if (Math.abs(percentDiff) <= 0.5) {
          massBalanceMsg.textContent = `Mass balance OK: difference ${percentDiff.toFixed(
            2
          )}%`;
          massBalanceMsg.className = "text-xs text-emerald-700";
        } else {
          massBalanceMsg.textContent = `Warning: mass balance difference is ${percentDiff.toFixed(
            2
          )}% (check weights and pan).`;
          massBalanceMsg.className = "text-xs text-red-700";
        }

        // Percent retained and passing
        let cumPercent = 0;
        const labels = [];
        const percentPassingArr = [];
        const targetKey = targetSelect.value;
        const bandDef = targetBands[targetKey] || null;
        const outOfBand = [];

        data.forEach((d) => {
          const percentRetained = (d.mass / totalMass) * 100;
          cumPercent += percentRetained;
          const percentPassing = 100 - cumPercent;

          d.tr.querySelector(".percent-retained").textContent =
            percentRetained.toFixed(2);
          d.tr.querySelector(".percent-cum-retained").textContent =
            cumPercent.toFixed(2);
          d.tr.querySelector(".percent-passing").textContent =
            percentPassing.toFixed(2);

          labels.push(d.opening);
          percentPassingArr.push(percentPassing);

          const targetRangeEl = d.tr.querySelector(".target-range");
          const badge = d.tr.querySelector(".status-badge");
          badge.classList.add("hidden");
          badge.textContent = "";

          if (bandDef) {
            // Convert opening to band unit if needed
            let openingForBand = d.opening;
            if (sieveUnitSelect.value !== bandDef.unit) {
              if (sieveUnitSelect.value === "mm" && bandDef.unit === "inch") {
                openingForBand = d.opening / 25.4;
              } else if (
                sieveUnitSelect.value === "inch" &&
                bandDef.unit === "mm"
              ) {
                openingForBand = d.opening * 25.4;
              }
            }
            // Find closest band by size
            let closest = null;
            let minDiff = Infinity;
            bandDef.bands.forEach((b) => {
              const diff = Math.abs(openingForBand - b.size);
              if (diff < minDiff) {
                minDiff = diff;
                closest = b;
              }
            });
            if (closest) {
              targetRangeEl.textContent = `${closest.min.toFixed(
                0
              )}–${closest.max.toFixed(0)}%`;
              if (
                percentPassing < closest.min - 0.01 ||
                percentPassing > closest.max + 0.01
              ) {
                badge.textContent = "Out of band";
                badge.classList.remove("hidden");
                badge.classList.remove("badge-ok");
                badge.classList.add("badge-out");
                outOfBand.push({
                  label: d.label || d.opening.toString(),
                  size: d.opening,
                  percentPassing,
                  min: closest.min,
                  max: closest.max,
                });
              } else {
                badge.textContent = "OK";
                badge.classList.remove("hidden");
                badge.classList.remove("badge-out");
                badge.classList.add("badge-ok");
              }
            } else {
              targetRangeEl.textContent = "–";
            }
          } else {
            targetRangeEl.textContent = "–";
          }
        });

        // Update out-of-band summary
        if (bandDef) {
          if (outOfBand.length === 0) {
            outOfBandSummary.textContent =
              "All sieves are within the selected target gradation band.";
            outOfBandSummary.className =
              "text-xs text-emerald-700 bg-emerald-50 border border-emerald-200 rounded-md p-2 w-full";
          } else {
            const lines = outOfBand.map(
              (o) =>
                `${o.label || o.size}: ${o.percentPassing.toFixed(
                  1
                )}% passing (target ${o.min.toFixed(0)}–${o.max.toFixed(0)}%)`
            );
            outOfBandSummary.innerHTML =
              "<strong>Out-of-band sieves:</strong><br>" + lines.join("<br>");
            outOfBandSummary.className =
              "text-xs text-red-700 bg-red-50 border border-red-200 rounded-md p-2 w-full";
          }
        } else {
          outOfBandSummary.textContent =
            "No target band selected. Choose a band to highlight out-of-spec sieves.";
          outOfBandSummary.className =
            "text-xs text-gray-700 bg-gray-50 border border-dashed border-gray-300 rounded-md p-2 w-full";
        }

        // Update chart
        if (!gradationChart) {
          const ctx = document
            .getElementById("gradationChart")
            .getContext("2d");
          gradationChart = new Chart(ctx, {
            type: "line",
            data: {
              labels: labels,
              datasets: [
                {
                  label: "% Passing",
                  data: percentPassingArr,
                  borderColor: "#2563eb",
                  backgroundColor: "rgba(37,99,235,0.1)",
                  tension: 0.15,
                  pointRadius: 3,
                  pointBackgroundColor: "#2563eb",
                },
              ],
            },
            options: {
              responsive: true,
              maintainAspectRatio: false,
              parsing: false,
              scales: {
                x: {
                  type: "logarithmic",
                  title: {
                    display: true,
                    text: `Sieve opening (${sieveUnitSelect.value})`,
                  },
                  ticks: {
                    callback: function (value, index, ticks) {
                      const v = this.getLabelForValue(value);
                      return v;
                    },
                  },
                },
                y: {
                  title: { display: true, text: "% Passing" },
                  min: 0,
                  max: 100,
                },
              },
              plugins: {
                legend: { display: false },
                tooltip: {
                  callbacks: {
                    title: function (context) {
                      const size = context[0].label;
                      return `Sieve: ${size} ${sieveUnitSelect.value}`;
                    },
                    label: function (context) {
                      return `% Passing: ${context.parsed.y.toFixed(2)}%`;
                    },
                  },
                },
              },
            },
          });
        } else {
          gradationChart.data.labels = labels;
          gradationChart.data.datasets[0].data = percentPassingArr;
          gradationChart.options.scales.x.title.text = `Sieve opening (${sieveUnitSelect.value})`;
          gradationChart.update();
        }
      }

      // Event listeners
      addRowBtn.addEventListener("click", () => {
        createRow();
      });

      resetRowsBtn.addEventListener("click", () => {
        templateSelect.value = "";
        resetTable();
      });

      templateSelect.addEventListener("change", (e) => {
        applyTemplate(e.target.value);
      });

      sieveUnitSelect.addEventListener("change", () => {
        updateSieveUnitLabels();
      });

      sieveTbody.addEventListener("click", (e) => {
        if (e.target.classList.contains("remove-row")) {
          const tr = e.target.closest("tr");
          tr.remove();
          clearResults();
        }
      });

      calculateBtn.addEventListener("click", calculate);

      // Initialize
      resetTable();
    </script>

