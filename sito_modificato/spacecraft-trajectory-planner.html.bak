    <script>
      // Physical constants
      const AU = 1.495978707e11; // meters
      const MU_SUN = 1.32712440018e20; // m^3/s^2

      // Simple planetary data (circular orbits)
      const PLANETS = {
        mercury: { name: "Mercury", a_au: 0.387, color: "#f97316" },
        venus: { name: "Venus", a_au: 0.723, color: "#a855f7" },
        earth: { name: "Earth", a_au: 1.0, color: "#22c55e" },
        mars: { name: "Mars", a_au: 1.524, color: "#ef4444" },
        jupiter: { name: "Jupiter", a_au: 5.204, color: "#0ea5e9" },
        saturn: { name: "Saturn", a_au: 9.582, color: "#eab308" },
      };

      function getOrbitRadiusAU(id, advanced) {
        if (advanced) {
          const r1 = parseFloat(document.getElementById("r1-au").value);
          const r2 = parseFloat(document.getElementById("r2-au").value);
          if (id === "r1") return isFinite(r1) && r1 > 0 ? r1 : null;
          if (id === "r2") return isFinite(r2) && r2 > 0 ? r2 : null;
        }
        const dep = document.getElementById("departure-body").value;
        const arr = document.getElementById("arrival-body").value;
        return id === "r1" ? PLANETS[dep].a_au : PLANETS[arr].a_au;
      }

      function hohmannTransfer(r1_m, r2_m) {
        const at = 0.5 * (r1_m + r2_m);
        const tof = Math.PI * Math.sqrt(Math.pow(at, 3) / MU_SUN); // seconds

        const v1 = Math.sqrt(MU_SUN / r1_m);
        const v2 = Math.sqrt(MU_SUN / r2_m);
        const vt1 = Math.sqrt(MU_SUN * (2 / r1_m - 1 / at));
        const vt2 = Math.sqrt(MU_SUN * (2 / r2_m - 1 / at));

        const dv1 = Math.abs(vt1 - v1);
        const dv2 = Math.abs(v2 - vt2);

        return { at, tof, v1, v2, vt1, vt2, dv1, dv2, dvTotal: dv1 + dv2 };
      }

      function meanMotion(r_m) {
        return Math.sqrt(MU_SUN / Math.pow(r_m, 3)); // rad/s
      }

      function idealPhaseAngle(r1_m, r2_m, tof_s) {
        const n1 = meanMotion(r1_m);
        const n2 = meanMotion(r2_m);
        if (r2_m > r1_m) {
          // outbound
          return Math.PI - n2 * tof_s;
        } else {
          // inbound
          return Math.PI - n1 * tof_s;
        }
      }

      function normalizeAngle(angle) {
        let a = angle % (2 * Math.PI);
        if (a > Math.PI) a -= 2 * Math.PI;
        if (a < -Math.PI) a += 2 * Math.PI;
        return a;
      }

      function phaseAngleAtDate(r1_m, r2_m, launchDate) {
        // Assume both planets aligned at t0 = now
        const t0 = Date.now() / 1000;
        const tLaunch = launchDate.getTime() / 1000;
        const dt = tLaunch - t0;

        const n1 = meanMotion(r1_m);
        const n2 = meanMotion(r2_m);

        const theta1 = n1 * dt;
        const theta2 = n2 * dt;

        return normalizeAngle(theta2 - theta1);
      }

      function launchWindowScore(phaseErrorRad) {
        const deg = Math.abs((phaseErrorRad * 180) / Math.PI);
        const score = Math.max(0, 100 - (deg / 180) * 100 * 2); // penalize quickly
        return Math.round(score);
      }

      function formatDaysHours(seconds) {
        const days = seconds / 86400;
        if (days < 2) {
          const hours = seconds / 3600;
          return hours.toFixed(1) + " h";
        }
        return days.toFixed(1) + " days";
      }

      function formatDv(ms) {
        if (ms < 1000) return ms.toFixed(0) + " m/s";
        return (ms / 1000).toFixed(2) + " km/s";
      }

      // Orbit visualization
      function drawOrbits(r1_au, r2_au, transfer, launchDate) {
        const canvas = document.getElementById("orbit-canvas");
        if (!canvas || !canvas.getContext) return;
        const ctx = canvas.getContext("2d");
        const w = canvas.width;
        const h = canvas.height;
        ctx.clearRect(0, 0, w, h);

        const maxR = Math.max(r1_au, r2_au);
        const margin = 20;
        const scale = (Math.min(w, h) / 2 - margin) / maxR;
        const cx = w / 2;
        const cy = h / 2;

        // Sun
        ctx.fillStyle = "#facc15";
        ctx.beginPath();
        ctx.arc(cx, cy, 6, 0, 2 * Math.PI);
        ctx.fill();

        // Orbits
        ctx.strokeStyle = "#9ca3af";
        ctx.lineWidth = 1;
        [r1_au, r2_au].forEach((r, idx) => {
          ctx.beginPath();
          ctx.arc(cx, cy, r * scale, 0, 2 * Math.PI);
          ctx.stroke();
        });

        // Planet positions at launch
        const r1_m = r1_au * AU;
        const r2_m = r2_au * AU;
        const phase = phaseAngleAtDate(r1_m, r2_m, launchDate);
        const t0 = Date.now() / 1000;
        const tLaunch = launchDate.getTime() / 1000;
        const dt = tLaunch - t0;
        const n1 = meanMotion(r1_m);
        const theta1 = n1 * dt;
        const theta2 = theta1 + phase;

        const x1 = cx + r1_au * scale * Math.cos(theta1);
        const y1 = cy + r1_au * scale * Math.sin(theta1);
        const x2 = cx + r2_au * scale * Math.cos(theta2);
        const y2 = cy + r2_au * scale * Math.sin(theta2);

        ctx.fillStyle = "#22c55e";
        ctx.beginPath();
        ctx.arc(x1, y1, 4, 0, 2 * Math.PI);
        ctx.fill();

        ctx.fillStyle = "#ef4444";
        ctx.beginPath();
        ctx.arc(x2, y2, 4, 0, 2 * Math.PI);
        ctx.fill();

        // Transfer ellipse (approximate, aligned with departure position)
        ctx.strokeStyle = "#2563eb";
        ctx.lineWidth = 1.5;
        const at_au = transfer.at / AU;
        const c_au = Math.abs(r2_au - r1_au) / 2;
        const e = c_au / at_au;
        const aPix = at_au * scale;
        const bPix = aPix * Math.sqrt(1 - e * e);

        ctx.save();
        ctx.translate(cx, cy);
        ctx.rotate(theta1);
        ctx.beginPath();
        for (let ang = 0; ang <= 2 * Math.PI + 0.01; ang += 0.02) {
          const x = aPix * Math.cos(ang) - c_au * scale;
          const y = bPix * Math.sin(ang);
          if (ang === 0) ctx.moveTo(x, y);
          else ctx.lineTo(x, y);
        }
        ctx.stroke();
        ctx.restore();
      }

      // Heatmap
      function generateHeatmap(r1_au, r2_au) {
        const canvas = document.getElementById("heatmap-canvas");
        if (!canvas || !canvas.getContext) return;
        const ctx = canvas.getContext("2d");
        const w = canvas.width;
        const h = canvas.height;
        ctx.clearRect(0, 0, w, h);

        const cols = 60; // departure date offsets
        const rows = 40; // TOF samples

        const maxOffsetDays = 720; // ~2 years
        const minTofDays = 50;
        const maxTofDays = 600;

        const r1_m = r1_au * AU;
        const r2_m = r2_au * AU;

        const now = new Date();

        // Precompute dv range for color scaling
        let dvMin = Infinity;
        let dvMax = 0;
        const dvGrid = [];

        for (let i = 0; i < cols; i++) {
          dvGrid[i] = [];
          const offsetDays = (i / (cols - 1)) * maxOffsetDays;
          const launchDate = new Date(
            now.getTime() + offsetDays * 86400 * 1000
          );

          for (let j = 0; j < rows; j++) {
            const tofDays =
              minTofDays + (j / (rows - 1)) * (maxTofDays - minTofDays);
            const tof_s = tofDays * 86400;
            const transfer = hohmannTransfer(r1_m, r2_m);
            // Scale transfer time to requested TOF by simple factor
            const scale = tof_s / transfer.tof;
            const dvTotal =
              transfer.dvTotal * Math.max(0.5, Math.min(2.0, scale));

            const phiIdeal = idealPhaseAngle(r1_m, r2_m, tof_s);
            const phiActual = phaseAngleAtDate(r1_m, r2_m, launchDate);
            const phaseErr = normalizeAngle(phiActual - phiIdeal);
            const score = launchWindowScore(phaseErr);
            const effectiveDv = dvTotal * (1 + (100 - score) / 200); // penalize bad phase

            dvGrid[i][j] = effectiveDv;
            if (effectiveDv < dvMin) dvMin = effectiveDv;
            if (effectiveDv > dvMax) dvMax = effectiveDv;
          }
        }

        const cellW = w / cols;
        const cellH = h / rows;

        for (let i = 0; i < cols; i++) {
          for (let j = 0; j < rows; j++) {
            const dv = dvGrid[i][j];
            const t = (dv - dvMin) / (dvMax - dvMin + 1e-9); // 0..1
            // Color scale: dark blue (good) to light gray (bad)
            const r = 15 + Math.round(200 * t);
            const g = 23 + Math.round(180 * t);
            const b = 42 + Math.round(150 * t);
            ctx.fillStyle = `rgb(${r},${g},${b})`;
            ctx.fillRect(i * cellW, h - (j + 1) * cellH, cellW + 1, cellH + 1);
          }
        }
      }

      // Event handlers
      document.addEventListener("DOMContentLoaded", () => {
        const advancedCheckbox = document.getElementById("advanced-mode");
        const advancedInputs = document.getElementById("advanced-inputs");
        const computeBtn = document.getElementById("compute-btn");
        const generateHeatmapBtn = document.getElementById("generate-heatmap");
        const launchDateInput = document.getElementById("launch-date");

        // Default launch date = today
        const today = new Date();
        launchDateInput.valueAsDate = today;

        advancedCheckbox.addEventListener("change", () => {
          advancedInputs.classList.toggle("hidden", !advancedCheckbox.checked);
        });

        computeBtn.addEventListener("click", () => {
          const advanced = advancedCheckbox.checked;
          const r1_au = getOrbitRadiusAU("r1", advanced);
          const r2_au = getOrbitRadiusAU("r2", advanced);

          if (!r1_au || !r2_au) {
            alert("Please provide valid orbital radii in AU.");
            return;
          }

          const r1_m = r1_au * AU;
          const r2_m = r2_au * AU;

          let transfer = hohmannTransfer(r1_m, r2_m);

          const tofInput = document.getElementById("tof-days");
          let tofOverride = parseFloat(tofInput.value);
          let tof_s = transfer.tof;
          if (isFinite(tofOverride) && tofOverride > 0) {
            tof_s = tofOverride * 86400;
          } else {
            tofOverride = transfer.tof / 86400;
          }

          const launchDate = launchDateInput.valueAsDate || new Date();

          const phiIdeal = idealPhaseAngle(r1_m, r2_m, tof_s);
          const phiActual = phaseAngleAtDate(r1_m, r2_m, launchDate);
          const phaseErr = normalizeAngle(phiActual - phiIdeal);
          const score = launchWindowScore(phaseErr);

          // Update results
          document.getElementById("results-section").classList.remove("hidden");
          document.getElementById("a-transfer").textContent = (
            transfer.at / AU
          ).toFixed(3);
          document.getElementById("tof-result").textContent =
            formatDaysHours(tof_s) + ` (${tofOverride.toFixed(1)} days)`;
          document.getElementById("phase-angle").textContent =
            ((phiIdeal * 180) / Math.PI).toFixed(1) + "°";
          document.getElementById("window-score").textContent =
            score + " / 100";

          document.getElementById("dv1").textContent = formatDv(transfer.dv1);
          document.getElementById("dv2").textContent = formatDv(transfer.dv2);
          document.getElementById("dv-total").textContent = formatDv(
            transfer.dvTotal
          );

          drawOrbits(r1_au, r2_au, transfer, launchDate);
        });

        generateHeatmapBtn.addEventListener("click", () => {
          const advanced = advancedCheckbox.checked;
          const r1_au = getOrbitRadiusAU("r1", advanced);
          const r2_au = getOrbitRadiusAU("r2", advanced);
          if (!r1_au || !r2_au) {
            alert("Please provide valid orbital radii in AU.");
            return;
          }
          generateHeatmap(r1_au, r2_au);
        });

        // Initial quick draw
        const r1_au_init = PLANETS["earth"].a_au;
        const r2_au_init = PLANETS["mars"].a_au;
        const transferInit = hohmannTransfer(r1_au_init * AU, r2_au_init * AU);
        drawOrbits(r1_au_init, r2_au_init, transferInit, today);
      });
    </script>
  
