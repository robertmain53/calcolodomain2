    <script>
      // Utility: unit handling
      const state = {
        unit: "ft", // 'ft' or 'm'
        rooms: [],
      };

      function createRoom(index) {
        const roomId = `room-${index}`;
        const roomDiv = document.createElement("div");
        roomDiv.className = "border border-gray-200 rounded-md p-3";
        roomDiv.dataset.roomIndex = index;

        roomDiv.innerHTML = `
      <div class="flex justify-between items-center mb-2">
        <h3 class="font-semibold text-sm">Room ${index + 1}</h3>
        <button type="button" class="text-xs text-red-600 hover:underline remove-room">Remove</button>
      </div>
      <div class="grid grid-cols-2 md:grid-cols-4 gap-3 mb-3">
        <div>
          <label class="input-label">Length</label>
          <div class="flex items-center gap-1">
            <input type="number" min="0" step="0.01" value="12" class="input-field text-sm room-length">
            <span class="input-suffix room-unit-label">ft</span>
          </div>
        </div>
        <div>
          <label class="input-label">Width</label>
          <div class="flex items-center gap-1">
            <input type="number" min="0" step="0.01" value="12" class="input-field text-sm room-width">
            <span class="input-suffix room-unit-label">ft</span>
          </div>
        </div>
        <div>
          <label class="input-label">Wall height</label>
          <div class="flex items-center gap-1">
            <input type="number" min="0" step="0.01" value="8" class="input-field text-sm room-height">
            <span class="input-suffix room-unit-label">ft</span>
          </div>
        </div>
        <div>
          <label class="input-label">Include ceiling</label>
          <select class="input-field text-sm room-ceiling">
            <option value="auto">Follow global setting</option>
            <option value="yes">Yes (force include)</option>
            <option value="no">No (walls only)</option>
          </select>
        </div>
      </div>
      <div class="border-t border-gray-100 pt-2 mt-1">
        <div class="flex justify-between items-center mb-2">
          <span class="text-xs font-semibold text-gray-600 uppercase tracking-wide">Openings (doors & windows)</span>
          <button type="button" class="text-xs text-blue-600 hover:underline add-opening">+ Add opening</button>
        </div>
        <div class="space-y-2 room-openings"></div>
      </div>
    `;
        return roomDiv;
      }

      function addRoom() {
        const container = document.getElementById("rooms-container");
        const index = container.children.length;
        const room = createRoom(index);
        container.appendChild(room);
        attachRoomEvents(room);
      }

      function attachRoomEvents(roomDiv) {
        roomDiv.querySelector(".remove-room").addEventListener("click", () => {
          roomDiv.remove();
          // Re-label rooms
          document
            .querySelectorAll("#rooms-container > div")
            .forEach((el, idx) => {
              el.dataset.roomIndex = idx;
              const title = el.querySelector("h3");
              if (title) title.textContent = `Room ${idx + 1}`;
            });
        });

        roomDiv.querySelector(".add-opening").addEventListener("click", () => {
          const openingsContainer = roomDiv.querySelector(".room-openings");
          const openingDiv = document.createElement("div");
          openingDiv.className =
            "grid grid-cols-2 md:grid-cols-5 gap-2 items-end";
          openingDiv.innerHTML = `
        <div>
          <label class="input-label">Width</label>
          <div class="flex items-center gap-1">
            <input type="number" min="0" step="0.01" value="3" class="input-field text-xs opening-width">
            <span class="input-suffix opening-unit-label">ft</span>
          </div>
        </div>
        <div>
          <label class="input-label">Height</label>
          <div class="flex items-center gap-1">
            <input type="number" min="0" step="0.01" value="7" class="input-field text-xs opening-height">
            <span class="input-suffix opening-unit-label">ft</span>
          </div>
        </div>
        <div>
          <label class="input-label">Type</label>
          <select class="input-field text-xs opening-type">
            <option value="door">Door</option>
            <option value="window">Window</option>
            <option value="other">Other opening</option>
          </select>
        </div>
        <div>
          <label class="input-label">Quantity</label>
          <input type="number" min="1" step="1" value="1" class="input-field text-xs opening-qty">
        </div>
        <div class="flex justify-end">
          <button type="button" class="text-xs text-red-600 hover:underline remove-opening">Remove</button>
        </div>
      `;
          openingsContainer.appendChild(openingDiv);
          openingDiv
            .querySelector(".remove-opening")
            .addEventListener("click", () => {
              openingDiv.remove();
            });
          updateUnitLabels();
        });
      }

      function updateUnitLabels() {
        const unitLabel = state.unit === "ft" ? "ft" : "m";
        document
          .querySelectorAll(".room-unit-label")
          .forEach((el) => (el.textContent = unitLabel));
        document
          .querySelectorAll(".opening-unit-label")
          .forEach((el) => (el.textContent = unitLabel));
        document.getElementById("board-width-unit").textContent = state.unit;
        document.getElementById("board-length-unit").textContent = state.unit;
      }

      function setUnit(unit) {
        if (unit === state.unit) return;
        const factor = unit === "ft" ? 3.28084 : 0.3048; // m->ft or ft->m
        // Convert existing numeric inputs
        document
          .querySelectorAll('#rooms-container input[type="number"]')
          .forEach((input) => {
            const val = parseFloat(input.value);
            if (!isNaN(val) && val > 0) {
              input.value = (val * factor).toFixed(2);
            }
          });
        const bw = document.getElementById("board-width");
        const bl = document.getElementById("board-length");
        [bw, bl].forEach((input) => {
          const val = parseFloat(input.value);
          if (!isNaN(val) && val > 0) {
            input.value = (val * factor).toFixed(2);
          }
        });
        state.unit = unit;
        updateUnitLabels();
      }

      function getBoardDimensions() {
        const size = document.getElementById("board-size").value;
        let width, length;
        if (size === "custom") {
          width = parseFloat(document.getElementById("board-width").value) || 0;
          length =
            parseFloat(document.getElementById("board-length").value) || 0;
        } else {
          // base in feet
          if (size === "4x8") {
            width = 4;
            length = 8;
          } else if (size === "4x10") {
            width = 4;
            length = 10;
          } else if (size === "4x12") {
            width = 4;
            length = 12;
          }
          if (state.unit === "m") {
            width *= 0.3048;
            length *= 0.3048;
          }
        }
        return { width, length, area: width * length };
      }

      function collectRooms() {
        const rooms = [];
        document
          .querySelectorAll("#rooms-container > div")
          .forEach((roomDiv) => {
            const length =
              parseFloat(roomDiv.querySelector(".room-length").value) || 0;
            const width =
              parseFloat(roomDiv.querySelector(".room-width").value) || 0;
            const height =
              parseFloat(roomDiv.querySelector(".room-height").value) || 0;
            const ceilingMode = roomDiv.querySelector(".room-ceiling").value;

            const openings = [];
            roomDiv
              .querySelectorAll(".room-openings > div")
              .forEach((openDiv) => {
                const w =
                  parseFloat(openDiv.querySelector(".opening-width").value) ||
                  0;
                const h =
                  parseFloat(openDiv.querySelector(".opening-height").value) ||
                  0;
                const qty =
                  parseInt(openDiv.querySelector(".opening-qty").value, 10) ||
                  1;
                openings.push({ width: w, height: h, qty });
              });

            rooms.push({ length, width, height, ceilingMode, openings });
          });
        return rooms;
      }

      function calculateDrywall() {
        const rooms = collectRooms();
        if (!rooms.length) {
          alert("Please add at least one room.");
          return;
        }

        const coverageMode = document.getElementById("coverage-mode").value;
        const wastePercent =
          parseFloat(document.getElementById("waste-percent").value) || 0;
        const board = getBoardDimensions();
        if (!board.area || board.area <= 0) {
          alert("Please provide a valid board size.");
          return;
        }

        let wallAreaTotal = 0;
        let ceilingAreaTotal = 0;
        let openingsAreaTotal = 0;

        const perRoom = [];

        rooms.forEach((room, idx) => {
          const { length, width, height, ceilingMode, openings } = room;
          if (length <= 0 || width <= 0 || height < 0) return;

          const wallArea = 2 * (length + width) * height;
          let ceilingArea = 0;
          const includeCeilingGlobal = coverageMode === "walls-ceiling";
          const includeCeiling =
            ceilingMode === "yes"
              ? true
              : ceilingMode === "no"
              ? false
              : includeCeilingGlobal;

          if (includeCeiling) {
            ceilingArea = length * width;
          }

          let openingsArea = 0;
          openings.forEach((o) => {
            if (o.width > 0 && o.height > 0 && o.qty > 0) {
              openingsArea += o.width * o.height * o.qty;
            }
          });

          const netArea = Math.max(0, wallArea + ceilingArea - openingsArea);

          wallAreaTotal += wallArea;
          ceilingAreaTotal += ceilingArea;
          openingsAreaTotal += openingsArea;

          perRoom.push({
            index: idx + 1,
            wallArea,
            ceilingArea,
            openingsArea,
            netArea,
          });
        });

        const netAreaTotal = Math.max(
          0,
          wallAreaTotal + ceilingAreaTotal - openingsAreaTotal
        );
        const areaWithWaste = netAreaTotal * (1 + wastePercent / 100);
        const sheets = Math.ceil(areaWithWaste / board.area);

        // Screws estimation: approximate screws per sheet based on spacing
        const studSpacingIn =
          parseFloat(document.getElementById("stud-spacing").value) || 16;
        const screwSpacingVal = document.getElementById("screw-spacing").value;
        let edgeSpacingIn = 8,
          fieldSpacingIn = 12;
        if (screwSpacingVal === "7x12") {
          edgeSpacingIn = 7;
          fieldSpacingIn = 12;
        }
        if (screwSpacingVal === "8x16") {
          edgeSpacingIn = 8;
          fieldSpacingIn = 16;
        }

        // Convert board dimensions to inches for screw layout
        const boardWidthIn =
          (state.unit === "ft" ? board.width : board.width * 3.28084) * 12;
        const boardLengthIn =
          (state.unit === "ft" ? board.length : board.length * 3.28084) * 12;

        // Assume studs run along board length; spacing across width
        const studsAcross = Math.floor(boardWidthIn / studSpacingIn) + 1;
        const rowsAlong = Math.floor(boardLengthIn / fieldSpacingIn) + 1;
        const edgeRows = Math.floor(boardLengthIn / edgeSpacingIn) + 1;

        // Very rough: screws on two long edges + interior rows
        const screwsPerSheet = Math.round(
          2 * edgeRows + (studsAcross - 2) * rowsAlong
        );
        const totalScrews = sheets * screwsPerSheet;

        // Tape estimation: approximate seam length per sheet
        // For horizontal hanging, assume ~ linear feet of seams per sheet:
        const seamPerSheetFt = (board.width + board.length) * 0.75; // heuristic
        const totalTapeFt = seamPerSheetFt * sheets;
        const totalTapeM = totalTapeFt * 0.3048;

        // Joint compound estimation
        const mudPerSqFtGal = 0.06; // mid-range
        const mudGallons = netAreaTotal * mudPerSqFtGal;
        const mudBuckets = mudGallons / 4.5;

        // Corner bead estimation: assume 4 outside corners per room
        let cornerBeadLen = 0;
        rooms.forEach((room) => {
          const h = room.height;
          if (h > 0) cornerBeadLen += 4 * h;
        });

        // Display results
        const areaUnit = state.unit === "ft" ? "ft²" : "m²";
        const lengthUnit = state.unit === "ft" ? "ft" : "m";

        document.getElementById(
          "result-wall-area"
        ).textContent = `${wallAreaTotal.toFixed(1)} ${areaUnit}`;
        document.getElementById(
          "result-ceiling-area"
        ).textContent = `${ceilingAreaTotal.toFixed(1)} ${areaUnit}`;
        document.getElementById(
          "result-openings-area"
        ).textContent = `${openingsAreaTotal.toFixed(1)} ${areaUnit}`;
        document.getElementById(
          "result-net-area"
        ).textContent = `${netAreaTotal.toFixed(1)} ${areaUnit}`;
        document.getElementById(
          "result-area-with-waste"
        ).textContent = `${areaWithWaste.toFixed(
          1
        )} ${areaUnit} (incl. ${wastePercent.toFixed(0)}% waste)`;
        document.getElementById(
          "result-sheets"
        ).textContent = `${sheets} sheets (${screwsPerSheet} screws/sheet est.)`;

        document.getElementById(
          "result-screws"
        ).textContent = `${totalScrews.toLocaleString()} screws (approx.)`;
        document.getElementById(
          "result-tape"
        ).textContent = `${totalTapeFt.toFixed(0)} ft (${totalTapeM.toFixed(
          0
        )} m) of tape (approx.)`;
        document.getElementById(
          "result-mud"
        ).textContent = `${mudGallons.toFixed(1)} gal (${mudBuckets.toFixed(
          1
        )} × 4.5 gal buckets, approx.)`;
        document.getElementById(
          "result-corner-bead"
        ).textContent = `${cornerBeadLen.toFixed(
          1
        )} ${lengthUnit} of corner bead (outside corners, est.)`;

        const resultsSection = document.getElementById("results-section");
        resultsSection.classList.remove("hidden");

        // Per-room breakdown table
        const breakdownContainer =
          document.getElementById("per-room-breakdown");
        const tableHolder = document.getElementById("per-room-table");
        if (perRoom.length > 1) {
          let html =
            '<table class="min-w-full border border-gray-200 text-xs"><thead><tr class="bg-gray-50">';
          html += '<th class="px-2 py-1 border-b text-left">Room</th>';
          html += `<th class="px-2 py-1 border-b text-right">Walls (${areaUnit})</th>`;
          html += `<th class="px-2 py-1 border-b text-right">Ceiling (${areaUnit})</th>`;
          html += `<th class="px-2 py-1 border-b text-right">Openings (${areaUnit})</th>`;
          html += `<th class="px-2 py-1 border-b text-right">Net (${areaUnit})</th>`;
          html += "</tr></thead><tbody>";
          perRoom.forEach((r) => {
            html += "<tr>";
            html += `<td class="px-2 py-1 border-b">Room ${r.index}</td>`;
            html += `<td class="px-2 py-1 border-b text-right">${r.wallArea.toFixed(
              1
            )}</td>`;
            html += `<td class="px-2 py-1 border-b text-right">${r.ceilingArea.toFixed(
              1
            )}</td>`;
            html += `<td class="px-2 py-1 border-b text-right">${r.openingsArea.toFixed(
              1
            )}</td>`;
            html += `<td class="px-2 py-1 border-b text-right">${r.netArea.toFixed(
              1
            )}</td>`;
            html += "</tr>";
          });
          html += "</tbody></table>";
          tableHolder.innerHTML = html;
          breakdownContainer.classList.remove("hidden");
        } else {
          breakdownContainer.classList.add("hidden");
          tableHolder.innerHTML = "";
        }
      }

      document.addEventListener("DOMContentLoaded", () => {
        // Initialize with one room
        addRoom();
        updateUnitLabels();

        document.getElementById("add-room").addEventListener("click", addRoom);

        document.getElementById("unit-ft").addEventListener("click", () => {
          document
            .getElementById("unit-ft")
            .classList.add("bg-blue-600", "text-white");
          document
            .getElementById("unit-ft")
            .classList.remove("bg-white", "text-gray-700");
          document
            .getElementById("unit-m")
            .classList.remove("bg-blue-600", "text-white");
          document
            .getElementById("unit-m")
            .classList.add("bg-white", "text-gray-700");
          setUnit("ft");
        });

        document.getElementById("unit-m").addEventListener("click", () => {
          document
            .getElementById("unit-m")
            .classList.add("bg-blue-600", "text-white");
          document
            .getElementById("unit-m")
            .classList.remove("bg-white", "text-gray-700");
          document
            .getElementById("unit-ft")
            .classList.remove("bg-blue-600", "text-white");
          document
            .getElementById("unit-ft")
            .classList.add("bg-white", "text-gray-700");
          setUnit("m");
        });

        document
          .getElementById("board-size")
          .addEventListener("change", (e) => {
            const customWrapper = document.getElementById(
              "custom-board-wrapper"
            );
            if (e.target.value === "custom") {
              customWrapper.classList.remove("hidden");
            } else {
              customWrapper.classList.add("hidden");
            }
          });

        document
          .getElementById("calculate-btn")
          .addEventListener("click", calculateDrywall);
      });
    </script>

