    <script>
      // Core calculation logic for AISC steel column design (simplified)
      function calcColumn() {
        const designMethod = document.getElementById("designMethod").value;
        const Fy = parseFloat(document.getElementById("fy").value || 0); // ksi
        const E = parseFloat(document.getElementById("E").value || 0); // ksi

        const Ag = parseFloat(document.getElementById("Ag").value || 0); // in^2
        const rx = parseFloat(document.getElementById("rx").value || 0); // in
        const ry = parseFloat(document.getElementById("ry").value || 0); // in
        const Zx = parseFloat(document.getElementById("Zx").value || 0); // in^3
        const Zy = parseFloat(document.getElementById("Zy").value || 0); // in^3

        const Lx_ft = parseFloat(document.getElementById("Lx").value || 0); // ft
        const Ly_ft = parseFloat(document.getElementById("Ly").value || 0); // ft
        const Kx = parseFloat(document.getElementById("Kx").value || 0);
        const Ky = parseFloat(document.getElementById("Ky").value || 0);

        const Pu = parseFloat(document.getElementById("Pu").value || 0); // kips
        const Mux = parseFloat(document.getElementById("Mux").value || 0); // kip-ft
        const Muy = parseFloat(document.getElementById("Muy").value || 0); // kip-ft

        // Basic validation
        if (
          !Fy ||
          !E ||
          !Ag ||
          !rx ||
          !ry ||
          !Zx ||
          !Zy ||
          !Lx_ft ||
          !Ly_ft ||
          !Kx ||
          !Ky
        ) {
          alert(
            "Please fill in all section, material, and length fields with non-zero values."
          );
          return;
        }

        // Convert lengths to inches
        const Lx = Lx_ft * 12.0;
        const Ly = Ly_ft * 12.0;

        // Slenderness
        const klrX = (Kx * Lx) / rx;
        const klrY = (Ky * Ly) / ry;
        const klrMax = Math.max(klrX, klrY);

        // Column curve per AISC-style approximation
        const pi2 = Math.PI * Math.PI;
        const Fe = (pi2 * E) / (klrMax * klrMax); // Euler elastic buckling stress (ksi)
        const lambda = Math.sqrt(Fy / Fe); // non-dimensional slenderness

        let Fcr;
        if (lambda <= 1.5) {
          Fcr = Math.pow(0.658, lambda * lambda) * Fy;
        } else {
          Fcr = 0.877 * Fe;
        }

        // Nominal axial strength
        const Pn = Fcr * Ag; // kips (since Fcr in ksi, Ag in in^2)

        // Flexural nominal strengths (very simplified: Fy * Z)
        const Mnx = (Fy * Zx) / 12.0; // kip-ft (Fy ksi * Z in^3 / 12 in/ft)
        const Mny = (Fy * Zy) / 12.0; // kip-ft

        // LRFD / ASD factors
        let phi_c = 0.9,
          phi_b = 0.9,
          omega_c = 1.67,
          omega_b = 1.67;
        let Pcap, MxCap, MyCap, interaction;

        if (designMethod === "LRFD") {
          Pcap = phi_c * Pn;
          MxCap = phi_b * Mnx;
          MyCap = phi_b * Mny;
          interaction =
            (Pcap > 0 ? Pu / Pcap : 0) +
            (MxCap > 0 ? Mux / MxCap : 0) +
            (MyCap > 0 ? Muy / MyCap : 0);
        } else {
          // ASD: convert nominal to allowable by dividing by Ω
          Pcap = Pn / omega_c;
          MxCap = Mnx / omega_b;
          MyCap = Mny / omega_b;
          interaction =
            (Pcap > 0 ? Pu / Pcap : 0) +
            (MxCap > 0 ? Mux / MxCap : 0) +
            (MyCap > 0 ? Muy / MyCap : 0);
        }

        // Update UI
        document.getElementById("klrX").textContent = klrX.toFixed(1);
        document.getElementById("klrY").textContent = klrY.toFixed(1);
        document.getElementById("PnCap").textContent = Pcap.toFixed(1);
        document.getElementById("MnxCap").textContent = MxCap.toFixed(1);
        document.getElementById("MnyCap").textContent = MyCap.toFixed(1);
        document.getElementById("interaction").textContent =
          interaction.toFixed(2);

        const statusBadge = document.getElementById("statusBadge");
        const statusText = document.getElementById("statusText");
        statusBadge.classList.remove("hidden");

        if (interaction <= 1.0) {
          statusText.textContent =
            "OK – Section passes simplified AISC interaction check";
          statusText.className =
            "badge bg-green-100 text-green-800 border border-green-300";
        } else {
          statusText.textContent =
            "NG – Section fails simplified AISC interaction check";
          statusText.className =
            "badge bg-red-100 text-red-800 border border-red-300";
        }
      }

      document.getElementById("runCalc").addEventListener("click", calcColumn);
    </script>

