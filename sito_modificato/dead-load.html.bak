    <script>
      // Dead load calculator logic
      (function () {
        const state = {
          system: "us", // 'us' or 'metric'
          layers: [],
        };

        const els = {
          btnUs: document.getElementById("btn-us"),
          btnMetric: document.getElementById("btn-metric"),
          layerType: document.getElementById("layer-type"),
          layerDesc: document.getElementById("layer-desc"),
          layerThickness: document.getElementById("layer-thickness"),
          layerUnitweight: document.getElementById("layer-unitweight"),
          thicknessLabel: document.getElementById("thickness-label"),
          unitweightLabel: document.getElementById("unitweight-label"),
          addLayer: document.getElementById("add-layer"),
          layersBody: document.getElementById("layers-body"),
          noLayersMsg: document.getElementById("no-layers-msg"),
          areaLength: document.getElementById("area-length"),
          areaWidth: document.getElementById("area-width"),
          lengthLabel: document.getElementById("length-label"),
          widthLabel: document.getElementById("width-label"),
          resultUniform: document.getElementById("result-uniform"),
          resultTotal: document.getElementById("result-total"),
          resultUniformMetric: document.getElementById("result-uniform-metric"),
          resultTotalMetric: document.getElementById("result-total-metric"),
          resetAll: document.getElementById("reset-all"),
        };

        function setSystem(system) {
          state.system = system;
          if (system === "us") {
            els.btnUs.classList.add(
              "bg-white",
              "shadow-sm",
              "border",
              "border-gray-300",
              "text-gray-900"
            );
            els.btnMetric.classList.remove(
              "bg-white",
              "shadow-sm",
              "border",
              "border-gray-300",
              "text-gray-900"
            );
            els.thicknessLabel.textContent = "Thickness (in)";
            els.unitweightLabel.textContent = "Unit weight (pcf)";
            els.lengthLabel.textContent = "Length (ft)";
            els.widthLabel.textContent = "Width (ft)";
          } else {
            els.btnMetric.classList.add(
              "bg-white",
              "shadow-sm",
              "border",
              "border-gray-300",
              "text-gray-900"
            );
            els.btnUs.classList.remove(
              "bg-white",
              "shadow-sm",
              "border",
              "border-gray-300",
              "text-gray-900"
            );
            els.thicknessLabel.textContent = "Thickness (mm)";
            els.unitweightLabel.textContent = "Unit weight (kN/m³)";
            els.lengthLabel.textContent = "Length (m)";
            els.widthLabel.textContent = "Width (m)";
          }
          recalc();
        }

        function defaultValuesForType() {
          const type = els.layerType.value;
          if (state.system === "us") {
            if (type === "slab") {
              els.layerDesc.value = "Concrete slab";
              els.layerThickness.value = 6;
              els.layerUnitweight.value = 150;
            } else if (type === "deck") {
              els.layerDesc.value = "Composite deck + slab";
              els.layerThickness.value = 4;
              els.layerUnitweight.value = 145;
            } else if (type === "wood") {
              els.layerDesc.value = "Wood sheathing";
              els.layerThickness.value = 0.75;
              els.layerUnitweight.value = 35;
            } else if (type === "roof") {
              els.layerDesc.value = "Roofing & insulation";
              els.layerThickness.value = 3;
              els.layerUnitweight.value = 5;
            } else if (type === "ceiling") {
              els.layerDesc.value = "Suspended ceiling";
              els.layerThickness.value = 1;
              els.layerUnitweight.value = 2;
            } else if (type === "partition") {
              els.layerDesc.value = "Gypsum partition (equivalent)";
              els.layerThickness.value = 1;
              els.layerUnitweight.value = 10;
            } else if (type === "finish") {
              els.layerDesc.value = "Floor finish";
              els.layerThickness.value = 1;
              els.layerUnitweight.value = 5;
            } else {
              els.layerDesc.value = "";
            }
          } else {
            if (type === "slab") {
              els.layerDesc.value = "Concrete slab";
              els.layerThickness.value = 150;
              els.layerUnitweight.value = 24;
            } else if (type === "deck") {
              els.layerDesc.value = "Composite deck + slab";
              els.layerThickness.value = 100;
              els.layerUnitweight.value = 23;
            } else if (type === "wood") {
              els.layerDesc.value = "Timber sheathing";
              els.layerThickness.value = 18;
              els.layerUnitweight.value = 5.5;
            } else if (type === "roof") {
              els.layerDesc.value = "Roofing & insulation";
              els.layerThickness.value = 80;
              els.layerUnitweight.value = 0.8;
            } else if (type === "ceiling") {
              els.layerDesc.value = "Suspended ceiling";
              els.layerThickness.value = 25;
              els.layerUnitweight.value = 0.3;
            } else if (type === "partition") {
              els.layerDesc.value = "Partition (equivalent)";
              els.layerThickness.value = 25;
              els.layerUnitweight.value = 1.0;
            } else if (type === "finish") {
              els.layerDesc.value = "Floor finish";
              els.layerThickness.value = 20;
              els.layerUnitweight.value = 1.0;
            } else {
              els.layerDesc.value = "";
            }
          }
        }

        function addLayer() {
          const desc =
            els.layerDesc.value.trim() ||
            els.layerType.options[els.layerType.selectedIndex].text;
          const t = parseFloat(els.layerThickness.value);
          const gamma = parseFloat(els.layerUnitweight.value);
          if (!(t > 0) || !(gamma > 0)) return;

          const layer = {
            id: Date.now() + Math.random(),
            desc,
            t,
            gamma,
          };
          state.layers.push(layer);
          renderLayers();
          recalc();
        }

        function removeLayer(id) {
          state.layers = state.layers.filter((l) => l.id !== id);
          renderLayers();
          recalc();
        }

        function renderLayers() {
          els.layersBody.innerHTML = "";
          if (state.layers.length === 0) {
            els.noLayersMsg.classList.remove("hidden");
            return;
          }
          els.noLayersMsg.classList.add("hidden");

          state.layers.forEach((layer) => {
            const tr = document.createElement("tr");
            tr.className = "border-t border-gray-100";
            const w = layerLoad(layer); // psf or kN/m² depending on system
            const thicknessDisplay =
              state.system === "us"
                ? `${layer.t.toFixed(2)} in`
                : `${layer.t.toFixed(1)} mm`;
            const gammaDisplay =
              state.system === "us"
                ? `${layer.gamma.toFixed(1)} pcf`
                : `${layer.gamma.toFixed(2)} kN/m³`;
            const loadDisplay =
              state.system === "us"
                ? `${w.toFixed(2)} psf`
                : `${w.toFixed(3)} kN/m²`;

            tr.innerHTML = `
          <td class="px-2 py-1 align-top">${layer.desc}</td>
          <td class="px-2 py-1 text-right align-top whitespace-nowrap">${thicknessDisplay}</td>
          <td class="px-2 py-1 text-right align-top whitespace-nowrap">${gammaDisplay}</td>
          <td class="px-2 py-1 text-right align-top whitespace-nowrap">${loadDisplay}</td>
          <td class="px-2 py-1 text-right align-top">
            <button type="button" class="text-red-500 hover:text-red-700 text-xs" data-id="${layer.id}">✕</button>
          </td>
        `;
            els.layersBody.appendChild(tr);
          });

          // Attach delete handlers
          els.layersBody.querySelectorAll("button[data-id]").forEach((btn) => {
            btn.addEventListener("click", () => {
              const id = parseFloat(btn.getAttribute("data-id"));
              removeLayer(id);
            });
          });
        }

        function layerLoad(layer) {
          if (state.system === "us") {
            const t_ft = layer.t / 12.0;
            return layer.gamma * t_ft; // psf
          } else {
            const t_m = layer.t / 1000.0;
            return layer.gamma * t_m; // kN/m²
          }
        }

        function recalc() {
          let wUniformUs = 0; // psf
          let wUniformMetric = 0; // kN/m²

          if (state.system === "us") {
            state.layers.forEach((l) => {
              wUniformUs += layerLoad(l);
            });
            // convert psf to kN/m²: 1 psf ≈ 0.04788 kN/m²
            wUniformMetric = wUniformUs * 0.047880258;
          } else {
            state.layers.forEach((l) => {
              wUniformMetric += layerLoad(l);
            });
            // convert kN/m² to psf: 1 kN/m² ≈ 20.885 psf
            wUniformUs = wUniformMetric * 20.885434;
          }

          const L = parseFloat(els.areaLength.value) || 0;
          const W = parseFloat(els.areaWidth.value) || 0;
          let areaFt2, areaM2;
          if (state.system === "us") {
            areaFt2 = L * W;
            areaM2 = areaFt2 * 0.09290304;
          } else {
            areaM2 = L * W;
            areaFt2 = areaM2 * 10.7639104;
          }

          const totalDeadUs = wUniformUs * areaFt2; // lb
          const totalDeadMetric = wUniformMetric * areaM2; // kN

          els.resultUniform.textContent = `${wUniformUs.toFixed(2)} psf`;
          els.resultTotal.textContent = `${totalDeadUs.toFixed(0)} lb`;
          els.resultUniformMetric.textContent = `${wUniformMetric.toFixed(
            3
          )} kN/m²`;
          els.resultTotalMetric.textContent = `${totalDeadMetric.toFixed(
            2
          )} kN`;
          renderLayers();
        }

        function resetAll() {
          state.layers = [];
          els.layerDesc.value = "";
          els.layerThickness.value = "";
          els.layerUnitweight.value = "";
          els.areaLength.value = state.system === "us" ? 20 : 6;
          els.areaWidth.value = state.system === "us" ? 30 : 9;
          renderLayers();
          recalc();
        }

        // Event bindings
        els.btnUs.addEventListener("click", () => setSystem("us"));
        els.btnMetric.addEventListener("click", () => setSystem("metric"));
        els.layerType.addEventListener("change", defaultValuesForType);
        els.addLayer.addEventListener("click", addLayer);
        els.areaLength.addEventListener("input", recalc);
        els.areaWidth.addEventListener("input", recalc);
        els.resetAll.addEventListener("click", resetAll);

        // Initialize
        defaultValuesForType();
        setSystem("us");
      })();
    </script>

