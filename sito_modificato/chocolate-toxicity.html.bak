    <script>
      // Chocolate toxicity calculator logic
      (function () {
        const dogWeightInput = document.getElementById("dog-weight");
        const weightUnitSelect = document.getElementById("weight-unit");
        const chocTypeSelect = document.getElementById("choc-type");
        const customTheoWrapper = document.getElementById(
          "custom-theobromine-wrapper"
        );
        const customTheoInput = document.getElementById("custom-theobromine");
        const chocAmountInput = document.getElementById("choc-amount");
        const amountUnitSelect = document.getElementById("amount-unit");
        const calcBtn = document.getElementById("calc-btn");

        const resWeightKg = document.getElementById("res-weight-kg");
        const resAmountG = document.getElementById("res-amount-g");
        const resMgPerG = document.getElementById("res-mg-per-g");
        const resTotalMg = document.getElementById("res-total-mg");
        const resMgPerKg = document.getElementById("res-mg-per-kg");

        const riskLabel = document.getElementById("risk-label");
        const riskSummary = document.getElementById("risk-summary");
        const riskDetail = document.getElementById("risk-detail");
        const riskAction = document.getElementById("risk-action");

        const theoMap = {
          white: 0.1,
          milk: 2.0,
          dark: 5.5,
          semisweet: 6.0,
          baking: 15.0,
          cocoa: 20.0,
        };

        chocTypeSelect.addEventListener("change", () => {
          if (chocTypeSelect.value === "custom") {
            customTheoWrapper.classList.remove("hidden");
          } else {
            customTheoWrapper.classList.add("hidden");
          }
        });

        function toNumber(el) {
          const v = parseFloat(el.value);
          return isNaN(v) ? null : v;
        }

        function setRiskBadge(colorClasses, text) {
          riskLabel.className = "badge " + colorClasses;
          riskLabel.textContent = text;
        }

        function formatNumber(val, decimals) {
          if (val === null || isNaN(val)) return "–";
          return val.toFixed(decimals);
        }

        calcBtn.addEventListener("click", () => {
          const w = toNumber(dogWeightInput);
          const a = toNumber(chocAmountInput);
          if (w === null || w <= 0 || a === null || a <= 0) {
            alert("Please enter a positive dog weight and chocolate amount.");
            return;
          }

          // Convert weight to kg
          const weightKg = weightUnitSelect.value === "kg" ? w : w / 2.20462;
          // Convert amount to grams
          const amountG = amountUnitSelect.value === "g" ? a : a * 28.35;

          // Determine mg/g
          let mgPerG;
          const type = chocTypeSelect.value;
          if (type === "custom") {
            const custom = toNumber(customTheoInput);
            if (custom === null || custom <= 0) {
              alert(
                "Please enter a positive custom theobromine content (mg per gram)."
              );
              return;
            }
            mgPerG = custom;
          } else {
            mgPerG = theoMap[type] || 0;
          }

          const totalMg = amountG * mgPerG;
          const mgPerKg = totalMg / weightKg;

          // Update numeric results
          resWeightKg.textContent = formatNumber(weightKg, 2);
          resAmountG.textContent = formatNumber(amountG, 1);
          resMgPerG.textContent = formatNumber(mgPerG, 2);
          resTotalMg.textContent = formatNumber(totalMg, 1);
          resMgPerKg.textContent = formatNumber(mgPerKg, 1);

          // Risk interpretation
          if (!isFinite(mgPerKg) || mgPerKg <= 0) {
            setRiskBadge("bg-gray-200 text-gray-800", "No data");
            riskSummary.textContent = "";
            riskDetail.textContent =
              "Unable to estimate risk. Please check your inputs.";
            riskAction.textContent = "";
            riskAction.className = "mt-2 text-sm font-semibold";
            return;
          }

          let summary, detail, action, badgeClass;

          if (mgPerKg < 20) {
            badgeClass = "bg-green-100 text-green-800";
            summary = "Likely low systemic toxicity risk.";
            detail =
              "At this estimated dose, most healthy dogs experience at most mild gastrointestinal upset. However, individual sensitivity varies, and small dogs or those with underlying disease may still be at risk.";
            action =
              "Monitor your dog closely for vomiting, diarrhea, or restlessness. If any concerning signs develop, or if you are unsure of the amount eaten, contact your veterinarian.";
          } else if (mgPerKg < 40) {
            badgeClass = "bg-amber-100 text-amber-800";
            summary = "Mild to moderate toxicity possible.";
            detail =
              "This dose is within a range where restlessness, vomiting, increased heart rate, and agitation are more likely. Early veterinary advice is recommended.";
            action =
              "Call your veterinarian or an animal poison hotline as soon as possible. They may recommend inducing vomiting and monitoring, especially if ingestion was recent.";
          } else if (mgPerKg < 60) {
            badgeClass = "bg-orange-100 text-orange-800";
            summary = "Moderate to severe toxicity risk.";
            detail =
              "At this dose, neurologic signs (tremors), significant tachycardia, and arrhythmias are possible. Prompt treatment can be life‑saving.";
            action =
              "Seek urgent veterinary care immediately, even if your dog currently appears normal. Bring chocolate packaging if available.";
          } else {
            badgeClass = "bg-red-100 text-red-800";
            summary = "High risk of severe / life‑threatening toxicity.";
            detail =
              "This estimated dose is associated with seizures, dangerous heart rhythm disturbances, and potentially fatal outcomes without aggressive treatment.";
            action =
              "This is an emergency. Contact an emergency veterinary clinic or poison hotline immediately and follow their instructions. Do not wait for symptoms to appear.";
          }

          setRiskBadge(badgeClass, mgPerKg.toFixed(1) + " mg/kg");
          riskSummary.textContent = summary;
          riskDetail.textContent = detail;
          riskAction.textContent = action;
          riskAction.className = "mt-2 text-sm font-semibold text-red-700";
        });
      })();
    </script>

