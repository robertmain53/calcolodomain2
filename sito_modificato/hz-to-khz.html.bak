    <script>
      // Hz <-> kHz converter logic
      (function () {
        const inputEl = document.getElementById("input-value");
        const outputEl = document.getElementById("output-value");
        const inputUnitEl = document.getElementById("input-unit");
        const outputUnitEl = document.getElementById("output-unit");
        const modeHzToKHzBtn = document.getElementById("mode-hz-to-khz");
        const modeKHzToHzBtn = document.getElementById("mode-khz-to-hz");
        const decimalsEl = document.getElementById("decimal-places");
        const swapBtn = document.getElementById("swap-values");
        const noteEl = document.getElementById("conversion-note");
        const presetButtons = document.querySelectorAll("button[data-preset]");

        let mode = "hz-to-khz"; // or 'khz-to-hz'

        function setMode(newMode) {
          mode = newMode;
          const hzToKHz = mode === "hz-to-khz";
          inputUnitEl.textContent = hzToKHz ? "Hz" : "kHz";
          outputUnitEl.textContent = hzToKHz ? "kHz" : "Hz";

          modeHzToKHzBtn.classList.toggle("toggle-btn-active", hzToKHz);
          modeHzToKHzBtn.classList.toggle("toggle-btn-inactive", !hzToKHz);
          modeKHzToHzBtn.classList.toggle("toggle-btn-active", !hzToKHz);
          modeKHzToHzBtn.classList.toggle("toggle-btn-inactive", hzToKHz);

          update();
        }

        function formatNumber(value, decimals) {
          if (!isFinite(value)) return "";
          const factor = Math.pow(10, decimals);
          const rounded = Math.round(value * factor) / factor;
          return rounded.toString();
        }

        function update() {
          const raw = inputEl.value.trim();
          if (raw === "") {
            outputEl.value = "";
            noteEl.textContent = "";
            return;
          }
          const x = Number(raw);
          if (!isFinite(x)) {
            outputEl.value = "";
            noteEl.textContent = "Please enter a valid number.";
            return;
          }

          const decimals = parseInt(decimalsEl.value, 10) || 0;
          let result, explanation;

          if (mode === "hz-to-khz") {
            result = x / 1000;
            explanation = x + " Hz ÷ 1000 = " + result + " kHz";
          } else {
            result = x * 1000;
            explanation = x + " kHz × 1000 = " + result + " Hz";
          }

          outputEl.value = formatNumber(result, decimals);
          noteEl.textContent = explanation;
        }

        // Event listeners
        inputEl.addEventListener("input", update);
        decimalsEl.addEventListener("change", update);
        modeHzToKHzBtn.addEventListener("click", () => setMode("hz-to-khz"));
        modeKHzToHzBtn.addEventListener("click", () => setMode("khz-to-hz"));

        swapBtn.addEventListener("click", () => {
          const currentOutput = outputEl.value.trim();
          if (currentOutput === "") return;
          inputEl.value = currentOutput;
          setMode(mode === "hz-to-khz" ? "khz-to-hz" : "hz-to-khz");
          update();
        });

        presetButtons.forEach((btn) => {
          btn.addEventListener("click", () => {
            const value = btn.getAttribute("data-preset");
            const unit = btn.getAttribute("data-unit"); // 'Hz' or 'kHz'
            if (unit === "Hz") {
              setMode("hz-to-khz");
            } else {
              setMode("khz-to-hz");
            }
            inputEl.value = value;
            update();
            inputEl.focus();
          });
        });

        // Initialize
        setMode("hz-to-khz");
      })();
    </script>

