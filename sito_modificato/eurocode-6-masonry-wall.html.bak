    <script>
      // Mobile menu toggle
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const expanded = this.getAttribute("aria-expanded") === "true";
          this.setAttribute("aria-expanded", String(!expanded));
          menu.classList.toggle("hidden");
        });

      function roundTo(value, decimals) {
        const factor = Math.pow(10, decimals);
        return Math.round(value * factor) / factor;
      }

      function calculateEurocode6() {
        const statusText = document.getElementById("status-text");
        statusText.textContent = "";

        const fk = parseFloat(document.getElementById("fk").value);
        const gammaM = parseFloat(document.getElementById("gamma-m").value);
        const t = parseFloat(document.getElementById("t").value); // mm
        const length = parseFloat(document.getElementById("length").value); // mm
        const h = parseFloat(document.getElementById("height").value); // mm
        const support = document.getElementById("support-condition").value;

        const ned_kN = parseFloat(document.getElementById("ned").value); // kN
        const eccTop = parseFloat(document.getElementById("ecc-top").value); // mm
        const eccBottom = parseFloat(
          document.getElementById("ecc-bottom").value
        ); // mm
        const emin = parseFloat(document.getElementById("emin").value); // mm

        if (
          [fk, gammaM, t, length, h, ned_kN, eccTop, eccBottom, emin].some(
            (v) => isNaN(v) || v <= 0
          )
        ) {
          statusText.textContent =
            "Please enter positive numeric values for all inputs.";
          statusText.classList.remove("text-gray-500");
          statusText.classList.add("text-red-600");
          return;
        }

        // 1. Design compressive strength
        const fd = fk / gammaM; // N/mm²

        // 2. Effective height
        let heff;
        if (support === "fixed-fixed") {
          heff = 0.75 * h;
        } else if (support === "cantilever") {
          heff = 2.0 * h;
        } else {
          heff = h;
        }

        const teff = t;
        const slenderness = heff / teff;

        // 3. Eccentricity
        const em = Math.max((eccTop + eccBottom) / 2.0, emin);

        // 4. Reduction factor (simplified, conservative)
        const lambdaLim = 27.0;
        const alpha = 0.7;
        const beta = 0.6;

        let phiSlender = 1.0 - alpha * Math.pow(slenderness / lambdaLim, 2);
        phiSlender = Math.min(1.0, Math.max(0.1, phiSlender)); // keep within [0.1,1]

        const emRatio = em / (t / 2.0);
        let phiEcc = 1.0 - beta * emRatio;
        phiEcc = Math.min(1.0, Math.max(0.1, phiEcc));

        const phi = phiSlender * phiEcc;

        // 5. Area and resistance
        const area = t * length; // mm²
        const nrd_N = phi * fd * area; // N
        const nrd_kN = nrd_N / 1000.0;

        const utilisation = ned_kN / nrd_kN;

        // Update UI
        document.getElementById("fdm").textContent = roundTo(fd, 3).toString();
        document.getElementById("heff").textContent = roundTo(
          heff,
          0
        ).toString();
        document.getElementById("slenderness").textContent = roundTo(
          slenderness,
          2
        ).toString();
        document.getElementById("nrd").textContent = roundTo(
          nrd_kN,
          1
        ).toString();
        document.getElementById("utilisation").textContent = roundTo(
          utilisation,
          2
        ).toString();

        const badge = document.getElementById("utilisation-badge");
        badge.classList.remove(
          "bg-green-100",
          "text-green-800",
          "bg-yellow-100",
          "text-yellow-800",
          "bg-red-100",
          "text-red-800",
          "bg-gray-200",
          "text-gray-800"
        );

        if (utilisation <= 0.8) {
          badge.textContent = "OK (reserve)";
          badge.classList.add("bg-green-100", "text-green-800");
        } else if (utilisation <= 1.0) {
          badge.textContent = "OK (governing)";
          badge.classList.add("bg-yellow-100", "text-yellow-800");
        } else {
          badge.textContent = "Not OK";
          badge.classList.add("bg-red-100", "text-red-800");
        }

        const resultsSection = document.getElementById("results-section");
        resultsSection.classList.remove("hidden");

        statusText.textContent =
          "Calculation completed (simplified Eurocode 6 check).";
        statusText.classList.remove("text-red-600");
        statusText.classList.add("text-gray-500");
      }

      document
        .getElementById("calculate-btn")
        .addEventListener("click", calculateEurocode6);

      document
        .getElementById("reset-btn")
        .addEventListener("click", function () {
          document.getElementById("fk").value = 5;
          document.getElementById("gamma-m").value = 2.0;
          document.getElementById("t").value = 240;
          document.getElementById("length").value = 3000;
          document.getElementById("height").value = 2700;
          document.getElementById("support-condition").value = "pinned-pinned";
          document.getElementById("ned").value = 300;
          document.getElementById("ecc-top").value = 20;
          document.getElementById("ecc-bottom").value = 10;
          document.getElementById("emin").value = 20;
          document.getElementById("status-text").textContent = "";
          document.getElementById("results-section").classList.add("hidden");
        });
    </script>

