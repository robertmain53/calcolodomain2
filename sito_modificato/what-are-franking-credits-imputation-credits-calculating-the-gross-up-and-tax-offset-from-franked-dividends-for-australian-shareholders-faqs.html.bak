    <script>
      // Franking credits calculator logic
      function formatCurrency(value) {
        if (!isFinite(value)) return "$0.00";
        const sign = value < 0 ? "-" : "";
        const abs = Math.abs(value);
        return (
          sign +
          abs
            .toLocaleString("en-AU", { style: "currency", currency: "AUD" })
            .replace("A$", "$")
        );
      }

      function calculateFranking() {
        const cashDividend =
          parseFloat(document.getElementById("cashDividend").value) || 0;
        let frankingPercent = parseFloat(
          document.getElementById("frankingPercent").value
        );
        let companyTaxRate = parseFloat(
          document.getElementById("companyTaxRate").value
        );
        let marginalTaxRate = parseFloat(
          document.getElementById("marginalTaxRate").value
        );

        if (frankingPercent < 0) frankingPercent = 0;
        if (frankingPercent > 100) frankingPercent = 100;
        if (companyTaxRate < 0) companyTaxRate = 0;
        if (companyTaxRate > 60) companyTaxRate = 60;
        if (marginalTaxRate < 0) marginalTaxRate = 0;
        if (marginalTaxRate > 60) marginalTaxRate = 60;

        const f = frankingPercent / 100;
        const c = companyTaxRate / 100;
        const m = marginalTaxRate / 100;

        const frankedCash = cashDividend * f;
        const unfrankedCash = cashDividend - frankedCash;

        let frankingCredit = 0;
        if (c > 0 && c < 1) {
          frankingCredit = frankedCash * (c / (1 - c));
        }

        const grossedUpDividend = frankedCash + frankingCredit;
        const assessableIncome = cashDividend + frankingCredit;

        const taxOnGrossed = grossedUpDividend * m;
        const netTax = taxOnGrossed - frankingCredit;

        const refundable = Math.max(frankingCredit - taxOnGrossed, 0);
        const extraTax = Math.max(netTax, 0);

        const afterTaxCash = cashDividend - extraTax;

        document.getElementById("resCashDividend").textContent =
          formatCurrency(cashDividend);
        document.getElementById("resFrankingCredit").textContent =
          formatCurrency(frankingCredit);
        document.getElementById("resGrossedUp").textContent =
          formatCurrency(grossedUpDividend);
        document.getElementById("resAssessable").textContent =
          formatCurrency(assessableIncome);
        document.getElementById("resTaxOnGrossed").textContent =
          formatCurrency(taxOnGrossed);
        document.getElementById("resTaxOffset").textContent =
          formatCurrency(frankingCredit);
        document.getElementById("resNetTax").textContent =
          formatCurrency(netTax);
        document.getElementById("resAfterTaxCash").textContent =
          formatCurrency(afterTaxCash);
        document.getElementById("resRefund").textContent =
          formatCurrency(refundable);

        let summary;
        if (refundable > 0.0001) {
          summary = `With a marginal tax rate of ${marginalTaxRate.toFixed(
            1
          )}%, your franking credits exceed the tax on this dividend. You would receive approximately ${formatCurrency(
            refundable
          )} as a refund, keeping ${formatCurrency(
            afterTaxCash + refundable
          )} in total after tax.`;
        } else if (netTax > 0.0001) {
          summary = `With a marginal tax rate of ${marginalTaxRate.toFixed(
            1
          )}%, you pay ${formatCurrency(
            extraTax
          )} of extra tax on this dividend and keep ${formatCurrency(
            afterTaxCash
          )} after tax.`;
        } else {
          summary = `With a marginal tax rate of ${marginalTaxRate.toFixed(
            1
          )}%, your franking credits fully cover the tax on this dividend. You keep the full cash dividend of ${formatCurrency(
            cashDividend
          )} after tax.`;
        }
        document.getElementById("summaryText").textContent = summary;
      }

      document
        .getElementById("calculateBtn")
        .addEventListener("click", calculateFranking);

      document.getElementById("resetBtn").addEventListener("click", () => {
        document.getElementById("cashDividend").value = 700;
        document.getElementById("frankingPercent").value = 100;
        document.getElementById("companyTaxRate").value = 30;
        document.getElementById("marginalTaxRate").value = 34.5;
        calculateFranking();
      });

      [
        "cashDividend",
        "frankingPercent",
        "companyTaxRate",
        "marginalTaxRate",
      ].forEach((id) => {
        document.getElementById(id).addEventListener("input", () => {
          // live update but debounced by simple timeout
          clearTimeout(window._frankingTimeout);
          window._frankingTimeout = setTimeout(calculateFranking, 200);
        });
      });

      // Initial calculation
      calculateFranking();

      // Mobile menu script (simple)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const isHidden = menu.classList.contains("hidden");
          if (isHidden) {
            menu.classList.remove("hidden");
            this.setAttribute("aria-expanded", "true");
          } else {
            menu.classList.add("hidden");
            this.setAttribute("aria-expanded", "false");
          }
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
