    <script>
      // FTE Calculator Logic
      (function () {
        const periodSelect = document.getElementById("period-select");
        const fulltimeHoursInput = document.getElementById("fulltime-hours");
        const acaModeSelect = document.getElementById("aca-mode");
        const fulltimeCountInput = document.getElementById("fulltime-count");
        const parttimeTotalHoursInput = document.getElementById(
          "parttime-total-hours"
        );
        const employeeCountApproxInput = document.getElementById(
          "employee-count-approx"
        );

        const totalFteEl = document.getElementById("total-fte");
        const fulltimeFteEl = document.getElementById("fulltime-fte");
        const parttimeFteEl = document.getElementById("parttime-fte");
        const parttimeHoursAdjustedEl = document.getElementById(
          "parttime-hours-adjusted"
        );
        const fulltimeHoursDisplayEl = document.getElementById(
          "fulltime-hours-display"
        );
        const acaNoteEl = document.getElementById("aca-note");
        const errorEl = document.getElementById("calc-error");

        const calcBtn = document.getElementById("calculate-btn");
        const resetBtn = document.getElementById("reset-btn");

        // Part-time employee table
        const pteBody = document.getElementById("pte-rows");
        const addRowBtn = document.getElementById("add-pte-row");

        function addPteRow(name = "", hours = "") {
          const rowIndex = pteBody.children.length + 1;
          const tr = document.createElement("tr");
          tr.className = rowIndex % 2 === 0 ? "bg-gray-50" : "bg-white";
          tr.innerHTML = `
        <td class="px-3 py-2 text-xs text-gray-500">${rowIndex}</td>
        <td class="px-3 py-2">
          <input type="text" class="w-full border-gray-300 rounded-md text-xs px-2 py-1" placeholder="Employee ${rowIndex}" value="${name}">
        </td>
        <td class="px-3 py-2">
          <input type="number" min="0" step="0.1" class="pte-hours w-full border-gray-300 rounded-md text-xs px-2 py-1" value="${hours}">
        </td>
        <td class="px-3 py-2 text-center">
          <button type="button" class="remove-pte-row text-xs text-red-600 hover:underline">Remove</button>
        </td>
      `;
          pteBody.appendChild(tr);
          renumberPteRows();
        }

        function renumberPteRows() {
          Array.from(pteBody.children).forEach((tr, idx) => {
            const indexCell = tr.querySelector("td:first-child");
            if (indexCell) indexCell.textContent = idx + 1;
            tr.className = (idx + 1) % 2 === 0 ? "bg-gray-50" : "bg-white";
          });
        }

        addRowBtn.addEventListener("click", () => addPteRow());

        pteBody.addEventListener("click", (e) => {
          if (e.target.classList.contains("remove-pte-row")) {
            const row = e.target.closest("tr");
            if (row) {
              row.remove();
              renumberPteRows();
            }
          }
        });

        function getPteHoursSum() {
          const rows = pteBody.querySelectorAll(".pte-hours");
          let sum = 0;
          rows.forEach((input) => {
            const v = parseFloat(input.value);
            if (!isNaN(v) && v >= 0) sum += v;
          });
          return sum;
        }

        function getAcaCapPerEmployee(period, fulltimeHours) {
          // ACA uses 30 hours/week as full-time threshold.
          const weeklyCap = 30;
          switch (period) {
            case "week":
              return weeklyCap;
            case "month":
              return weeklyCap * 4; // approx
            case "quarter":
              return weeklyCap * 13; // approx 13 weeks
            case "year":
              return weeklyCap * 52;
            default:
              return weeklyCap;
          }
        }

        function calculate() {
          errorEl.classList.add("hidden");

          const period = periodSelect.value;
          let fulltimeHours = parseFloat(fulltimeHoursInput.value);
          let fulltimeCount = parseFloat(fulltimeCountInput.value);
          let parttimeHours = parseFloat(parttimeTotalHoursInput.value);
          const approxHeadcount = parseFloat(employeeCountApproxInput.value);
          const acaMode = acaModeSelect.value;

          if (
            isNaN(fulltimeHours) ||
            fulltimeHours <= 0 ||
            isNaN(fulltimeCount) ||
            fulltimeCount < 0
          ) {
            errorEl.classList.remove("hidden");
            return;
          }

          // If per-employee table has data, override total part-time hours
          const pteSum = getPteHoursSum();
          if (pteBody.children.length > 0) {
            parttimeHours = pteSum;
            parttimeTotalHoursInput.value = pteSum.toFixed(2);
          }

          if (isNaN(parttimeHours) || parttimeHours < 0) {
            errorEl.classList.remove("hidden");
            return;
          }

          let adjustedParttimeHours = parttimeHours;

          if (acaMode === "aca") {
            const capPerEmployee = getAcaCapPerEmployee(period, fulltimeHours);
            let effectiveHeadcount = approxHeadcount;

            if (pteBody.children.length > 0) {
              // If we have individual rows, cap each row separately
              adjustedParttimeHours = 0;
              const rows = pteBody.querySelectorAll(".pte-hours");
              rows.forEach((input) => {
                const v = parseFloat(input.value);
                if (!isNaN(v) && v > 0) {
                  adjustedParttimeHours += Math.min(v, capPerEmployee);
                }
              });
              effectiveHeadcount = rows.length;
            } else if (!isNaN(approxHeadcount) && approxHeadcount > 0) {
              // Approximate capping using average hours per employee
              const avgHours = parttimeHours / approxHeadcount;
              const cappedAvg = Math.min(avgHours, capPerEmployee);
              adjustedParttimeHours = cappedAvg * approxHeadcount;
            }
          }

          const fulltimeFte = fulltimeCount;
          const parttimeFte = adjustedParttimeHours / fulltimeHours;
          const totalFte = fulltimeFte + parttimeFte;

          fulltimeFteEl.textContent = fulltimeFte
            .toFixed(2)
            .replace(/\.00$/, "");
          parttimeFteEl.textContent = parttimeFte.toFixed(2);
          totalFteEl.textContent = totalFte.toFixed(2).replace(/\.00$/, "");
          parttimeHoursAdjustedEl.textContent =
            adjustedParttimeHours.toFixed(2);
          fulltimeHoursDisplayEl.textContent = fulltimeHours
            .toFixed(2)
            .replace(/\.00$/, "");

          if (acaMode === "aca") {
            acaNoteEl.classList.remove("hidden");
          } else {
            acaNoteEl.classList.add("hidden");
          }
        }

        function resetCalculator() {
          periodSelect.value = "week";
          fulltimeHoursInput.value = 40;
          acaModeSelect.value = "standard";
          fulltimeCountInput.value = 8;
          parttimeTotalHoursInput.value = 60;
          employeeCountApproxInput.value = 3;
          pteBody.innerHTML = "";
          totalFteEl.textContent = "9.5";
          fulltimeFteEl.textContent = "8.0";
          parttimeFteEl.textContent = "1.5";
          parttimeHoursAdjustedEl.textContent = "60.0";
          fulltimeHoursDisplayEl.textContent = "40";
          acaNoteEl.classList.add("hidden");
          errorEl.classList.add("hidden");
        }

        calcBtn.addEventListener("click", calculate);
        resetBtn.addEventListener("click", resetCalculator);

        // Auto-calc on input change (debounced)
        let debounceTimer;
        ["input", "change"].forEach((evt) => {
          document.addEventListener(evt, (e) => {
            if (!e.target.closest("main")) return;
            clearTimeout(debounceTimer);
            debounceTimer = setTimeout(() => {
              calculate();
            }, 300);
          });
        });

        // Initialize with a couple of part-time rows as example
        addPteRow("PT-1", "20");
        addPteRow("PT-2", "15");
        addPteRow("PT-3", "25");
        calculate();
      })();

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
