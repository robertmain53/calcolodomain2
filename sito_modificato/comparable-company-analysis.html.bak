<script>
  // Utility: format numbers as compact currency-like values
  function formatNumber(val) {
    if (val === null || val === undefined || isNaN(val)) return '–';
    const abs = Math.abs(val);
    let suffix = '';
    let num = val;
    if (abs >= 1e9) { num = val / 1e9; suffix = 'B'; }
    else if (abs >= 1e6) { num = val / 1e6; suffix = 'M'; }
    else if (abs >= 1e3) { num = val / 1e3; suffix = 'K'; }
    return num.toFixed(2) + suffix;
  }

  function formatCurrency(val) {
    if (val === null || val === undefined || isNaN(val)) return '–';
    return '$' + formatNumber(val);
  }

  function formatMultiple(val) {
    if (val === null || val === undefined || isNaN(val)) return '–';
    return val.toFixed(2) + 'x';
  }

  // Peer row management
  const peersTbody = document.getElementById('peers-tbody');
  const peerMultiplesTbody = document.getElementById('peer-multiples-tbody');

  function createPeerRow(defaults = {}) {
    const tr = document.createElement('tr');
    tr.innerHTML = `
      <td class="px-2 py-1">
        <input type="text" class="w-32 md:w-40 border border-gray-200 rounded px-1 py-0.5 text-xs" value="${defaults.name || ''}" placeholder="Ticker / name">
      </td>
      <td class="px-2 py-1 text-right">
        <input type="number" step="0.0001" class="w-24 border border-gray-200 rounded px-1 py-0.5 text-xs table-input" value="${defaults.price || ''}">
      </td>
      <td class="px-2 py-1 text-right">
        <input type="number" step="1" class="w-28 border border-gray-200 rounded px-1 py-0.5 text-xs table-input" value="${defaults.shares || ''}">
      </td>
      <td class="px-2 py-1 text-right">
        <input type="number" step="0.01" class="w-28 border border-gray-200 rounded px-1 py-0.5 text-xs table-input" value="${defaults.mktcap || ''}">
      </td>
      <td class="px-2 py-1 text-right">
        <input type="number" step="0.01" class="w-24 border border-gray-200 rounded px-1 py-0.5 text-xs table-input" value="${defaults.netdebt || ''}">
      </td>
      <td class="px-2 py-1 text-right">
        <input type="number" step="0.01" class="w-28 border border-gray-200 rounded px-1 py-0.5 text-xs table-input" value="${defaults.ev || ''}">
      </td>
      <td class="px-2 py-1 text-right">
        <input type="number" step="0.01" class="w-28 border border-gray-200 rounded px-1 py-0.5 text-xs table-input" value="${defaults.revenue || ''}">
      </td>
      <td class="px-2 py-1 text-right">
        <input type="number" step="0.01" class="w-24 border border-gray-200 rounded px-1 py-0.5 text-xs table-input" value="${defaults.ebitda || ''}">
      </td>
      <td class="px-2 py-1 text-right">
        <input type="number" step="0.01" class="w-28 border border-gray-200 rounded px-1 py-0.5 text-xs table-input" value="${defaults.netincome || ''}">
      </td>
      <td class="px-2 py-1 text-center">
        <button type="button" class="text-red-600 hover:text-red-800 text-xs remove-row-btn">✕</button>
      </td>
    `;
    peersTbody.appendChild(tr);
  }

  function seedDefaultPeers() {
    peersTbody.innerHTML = '';
    // Simple illustrative defaults
    createPeerRow({ name: 'Peer A', price: 50, shares: 100000000, revenue: 2000000000, ebitda: 400000000, netincome: 250000000, netdebt: 300000000 });
    createPeerRow({ name: 'Peer B', price: 35, shares: 80000000, revenue: 1500000000, ebitda: 300000000, netincome: 180000000, netdebt: 150000000 });
    createPeerRow({ name: 'Peer C', price: 60, shares: 120000000, revenue: 2600000000, ebitda: 520000000, netincome: 310000000, netdebt: 500000000 });
  }

  document.getElementById('add-row-btn').addEventListener('click', () => createPeerRow());
  document.getElementById('reset-table-btn').addEventListener('click', () => {
    seedDefaultPeers();
    peerMultiplesTbody.innerHTML = '';
  });

  peersTbody.addEventListener('click', (e) => {
    if (e.target.classList.contains('remove-row-btn')) {
      const row = e.target.closest('tr');
      row.parentNode.removeChild(row);
    }
  });

  function getNumeric(input) {
    const v = parseFloat(input);
    return isNaN(v) ? null : v;
  }

  function percentile(sortedArr, p) {
    if (!sortedArr.length) return null;
    const idx = (sortedArr.length - 1) * p;
    const lower = Math.floor(idx);
    const upper = Math.ceil(idx);
    if (lower === upper) return sortedArr[lower];
    const weight = idx - lower;
    return sortedArr[lower] * (1 - weight) + sortedArr[upper] * weight;
  }

  document.getElementById('run-analysis-btn').addEventListener('click', () => {
    const primary = document.getElementById('primary-multiple').value;
    const range = document.getElementById('percentile-range').value;

    // Collect peers and compute multiples
    const rows = Array.from(peersTbody.querySelectorAll('tr'));
    const peers = [];
    rows.forEach(row => {
      const cells = row.querySelectorAll('input');
      const [nameEl, priceEl, sharesEl, mktcapEl, netdebtEl, evEl, revEl, ebitdaEl, niEl] = cells;
      const name = nameEl.value || '';
      const price = getNumeric(priceEl.value);
      const shares = getNumeric(sharesEl.value);
      let mktcap = getNumeric(mktcapEl.value);
      const netdebt = getNumeric(netdebtEl.value);
      let ev = getNumeric(evEl.value);
      const revenue = getNumeric(revEl.value);
      const ebitda = getNumeric(ebitdaEl.value);
      const netincome = getNumeric(niEl.value);

      if (!mktcap && price != null && shares != null) {
        mktcap = price * shares;
      }
      if (!ev && mktcap != null && netdebt != null) {
        ev = mktcap + netdebt;
      }

      const evRevenue = (ev != null && revenue && revenue !== 0) ? ev / revenue : null;
      const evEbitda = (ev != null && ebitda && ebitda !== 0) ? ev / ebitda : null;
      const pe = (mktcap != null && netincome && netincome !== 0) ? mktcap / netincome : null;

      peers.push({ name, evRevenue, evEbitda, pe });
    });

    // Build multiples table
    peerMultiplesTbody.innerHTML = '';
    peers.forEach(p => {
      const tr = document.createElement('tr');
      tr.innerHTML = `
        <td class="px-2 py-1">${p.name || '-'}</td>
        <td class="px-2 py-1 text-right">${formatMultiple(p.evRevenue)}</td>
        <td class="px-2 py-1 text-right">${formatMultiple(p.evEbitda)}</td>
        <td class="px-2 py-1 text-right">${formatMultiple(p.pe)}</td>
      `;
      peerMultiplesTbody.appendChild(tr);
    });

    // Select relevant multiple array
    let arr = [];
    if (primary === 'ev_ebitda') {
      arr = peers.map(p => p.evEbitda).filter(v => v != null && v > 0);
    } else if (primary === 'ev_revenue') {
      arr = peers.map(p => p.evRevenue).filter(v => v != null && v > 0);
    } else {
      arr = peers.map(p => p.pe).filter(v => v != null && v > 0);
    }

    arr.sort((a, b) => a - b);
    const count = arr.length;

    const outCount = document.getElementById('out-count');
    const outMin = document.getElementById('out-min');
    const outP25 = document.getElementById('out-p25');
    const outMedian = document.getElementById('out-median');
    const outP75 = document.getElementById('out-p75');
    const outMax = document.getElementById('out-max');

    if (!count) {
      outCount.textContent = '0';
      outMin.textContent = outP25.textContent = outMedian.textContent = outP75.textContent = outMax.textContent = '–';
      document.getElementById('out-multiple').textContent = '–';
      document.getElementById('out-ev').textContent = '–';
      document.getElementById('out-net-debt').textContent = '–';
      document.getElementById('out-equity').textContent = '–';
      document.getElementById('out-price').textContent = '–';
      return;
    }

    outCount.textContent = count.toString();
    outMin.textContent = formatMultiple(arr[0]);
    outMax.textContent = formatMultiple(arr[count - 1]);
    outP25.textContent = formatMultiple(percentile(arr, 0.25));
    outMedian.textContent = formatMultiple(percentile(arr, 0.5));
    outP75.textContent = formatMultiple(percentile(arr, 0.75));

    // Apply range filter
    let filtered = arr.slice();
    if (range === 'middle') {
      const p25 = percentile(arr, 0.25);
      const p75 = percentile(arr, 0.75);
      filtered = arr.filter(v => v >= p25 && v <= p75);
    } else if (range === 'trimmed') {
      const p10 = percentile(arr, 0.10);
      const p90 = percentile(arr, 0.90);
      filtered = arr.filter(v => v >= p10 && v <= p90);
    }

    const selectedMultiple = percentile(filtered, 0.5); // median of filtered
    document.getElementById('out-multiple').textContent = formatMultiple(selectedMultiple);

    // Target valuation
    const rev = getNumeric(document.getElementById('target-revenue').value);
    const ebitda = getNumeric(document.getElementById('target-ebitda').value);
    const netIncome = getNumeric(document.getElementById('target-net-income').value);
    const cash = getNumeric(document.getElementById('target-cash').value) || 0;
    const debt = getNumeric(document.getElementById('target-debt').value) || 0;
    const otherAdj = getNumeric(document.getElementById('target-nci').value) || 0;
    const shares = getNumeric(document.getElementById('target-shares').value);

    let impliedEV = null;
    if (primary === 'ev_ebitda' && ebitda != null) {
      impliedEV = selectedMultiple * ebitda;
    } else if (primary === 'ev_revenue' && rev != null) {
      impliedEV = selectedMultiple * rev;
    } else if (primary === 'pe' && netIncome != null) {
      const equityVal = selectedMultiple * netIncome;
      const netDebtAdj = debt - cash + otherAdj;
      impliedEV = equityVal + netDebtAdj;
    }

    const netDebtAdj = debt - cash + otherAdj;
    const impliedEquity = (impliedEV != null) ? impliedEV - netDebtAdj : null;
    const impliedPrice = (impliedEquity != null && shares) ? impliedEquity / shares : null;

    document.getElementById('out-ev').textContent = formatCurrency(impliedEV);
    document.getElementById('out-net-debt').textContent = formatCurrency(netDebtAdj);
    document.getElementById('out-equity').textContent = formatCurrency(impliedEquity);
    document.getElementById('out-price').textContent = impliedPrice != null && !isNaN(impliedPrice) ? '$' + impliedPrice.toFixed(2) : '–';
  });

  // Initialize defaults
  seedDefaultPeers();

  window.MathJax = {
    tex: { inlineMath: [['\\(','\\)'], ['$', '$']], displayMath: [['$','$'], ['\\[','\\]']] },
    svg: { fontCache: 'global' }
  };
</script>
