    <script>
      function clampLabValues(bilirubin, albumin, inr) {
        let b = Math.max(bilirubin, 1.0);
        let a = Math.min(Math.max(albumin, 1.0), 5.4);
        let i = Math.max(inr, 1.0);
        return { b, a, i };
      }

      function computeRawPeld(
        bilirubin,
        albumin,
        inr,
        ageYears,
        ageMonths,
        hasGrowthFailure
      ) {
        const { b, a, i } = clampLabValues(bilirubin, albumin, inr);
        const lnB = Math.log(b);
        const lnA = Math.log(a);
        const lnI = Math.log(i);

        const isUnderOne = ageYears < 1 || (ageYears === 1 && ageMonths < 0); // ageYears<1 is enough; months just for UI
        const ageTerm = isUnderOne ? 0.436 : 0;
        const growthTerm = hasGrowthFailure ? 0.667 : 0;

        const raw =
          0.48 * lnB + 1.857 * lnI - 0.687 * lnA + ageTerm + growthTerm;
        return { raw, isUnderOne, b, a, i };
      }

      function convertToAllocationScore(raw) {
        if (!isFinite(raw)) return null;
        let score = Math.round(raw * 10);
        if (score < -10) score = -10;
        if (score > 99) score = 99;
        return score;
      }

      function estimateMortality(peldScore) {
        if (peldScore === null || isNaN(peldScore))
          return { text: "–", range: null };
        if (peldScore < 10)
          return { text: "< 2% (low short-term risk)", range: "<2%" };
        if (peldScore < 20)
          return { text: "≈ 2–6% (mild–moderate risk)", range: "2–6%" };
        if (peldScore < 30)
          return { text: "≈ 6–15% (moderate risk)", range: "6–15%" };
        if (peldScore < 40)
          return { text: "≈ 15–30% (high risk)", range: "15–30%" };
        return { text: "> 30% (very high risk)", range: ">30%" };
      }

      function describePriority(peldScore) {
        if (peldScore === null || isNaN(peldScore)) return "";
        if (peldScore < 10)
          return "Relatively low calculated urgency; clinical context still essential.";
        if (peldScore < 20)
          return "Mild–moderate disease severity; monitor closely and follow transplant center guidance.";
        if (peldScore < 30)
          return "Moderate–high severity; typically higher transplant priority.";
        if (peldScore < 40)
          return "High severity; substantial short‑term mortality risk.";
        return "Very high severity; extremely elevated short‑term mortality risk.";
      }

      document.addEventListener("DOMContentLoaded", function () {
        const form = document.getElementById("peld-form");
        const calcBtn = document.getElementById("calculate-btn");
        const resetBtn = document.getElementById("reset-btn");

        const ageYearsEl = document.getElementById("age-years");
        const ageMonthsEl = document.getElementById("age-months");
        const biliEl = document.getElementById("bilirubin");
        const albEl = document.getElementById("albumin");
        const inrEl = document.getElementById("inr");

        const scoreDisplay = document.getElementById("peld-score-display");
        const mortalityDisplay = document.getElementById(
          "peld-mortality-display"
        );
        const priorityLabel = document.getElementById("peld-priority-label");
        const assumptionsList = document.getElementById("peld-assumptions");

        function showError(inputEl, message) {
          inputEl.classList.add(
            "border-red-500",
            "focus:border-red-500",
            "focus:ring-red-500"
          );
          inputEl.setAttribute("aria-invalid", "true");
          if (message) {
            inputEl.title = message;
          }
        }

        function clearError(inputEl) {
          inputEl.classList.remove(
            "border-red-500",
            "focus:border-red-500",
            "focus:ring-red-500"
          );
          inputEl.removeAttribute("aria-invalid");
          inputEl.title = "";
        }

        function resetOutputs() {
          scoreDisplay.textContent = "–";
          mortalityDisplay.textContent = "–";
          priorityLabel.textContent = "";
          assumptionsList.innerHTML = `
        <li>Lab values are recent and stable (chronic liver disease).</li>
        <li>Age &lt;1 year and growth failure are correctly classified.</li>
        <li>Score is capped between −10 and 99, as in allocation systems.</li>
      `;
        }

        calcBtn.addEventListener("click", function () {
          [ageYearsEl, ageMonthsEl, biliEl, albEl, inrEl].forEach(clearError);

          const ageYears = parseFloat(ageYearsEl.value || "0");
          const ageMonths = parseFloat(ageMonthsEl.value || "0");
          const bilirubin = parseFloat(biliEl.value);
          const albumin = parseFloat(albEl.value);
          const inr = parseFloat(inrEl.value);
          const growthFailure =
            (form.querySelector('input[name="growth-failure"]:checked') || {})
              .value === "yes";

          let hasError = false;

          if (isNaN(ageYears) || ageYears < 0 || ageYears > 11) {
            showError(ageYearsEl, "Enter age in years between 0 and 11.");
            hasError = true;
          }
          if (!isNaN(ageMonths) && (ageMonths < 0 || ageMonths > 11)) {
            showError(ageMonthsEl, "Enter age in months between 0 and 11.");
            hasError = true;
          }
          if (isNaN(bilirubin) || bilirubin <= 0) {
            showError(biliEl, "Enter a positive bilirubin value.");
            hasError = true;
          }
          if (isNaN(albumin) || albumin <= 0) {
            showError(albEl, "Enter a positive albumin value.");
            hasError = true;
          }
          if (isNaN(inr) || inr <= 0) {
            showError(inrEl, "Enter a positive INR value.");
            hasError = true;
          }

          if (hasError) {
            resetOutputs();
            return;
          }

          const { raw, isUnderOne, b, a, i } = computeRawPeld(
            bilirubin,
            albumin,
            inr,
            ageYears,
            ageMonths,
            growthFailure
          );
          const score = convertToAllocationScore(raw);
          const mortality = estimateMortality(score);
          const priority = describePriority(score);

          scoreDisplay.textContent =
            score !== null && isFinite(score) ? score.toString() : "–";
          mortalityDisplay.textContent = mortality.text;
          priorityLabel.textContent = priority;

          const assumptions = [];
          assumptions.push(
            `Capped lab values used in formula: bilirubin = ${b.toFixed(
              2
            )} mg/dL, albumin = ${a.toFixed(2)} g/dL, INR = ${i.toFixed(2)}.`
          );
          assumptions.push(
            isUnderOne
              ? "Age &lt;1 year term applied (+0.436)."
              : "Age ≥1 year term not applied."
          );
          assumptions.push(
            growthFailure
              ? "Growth failure term applied (+0.667)."
              : "No growth failure term applied."
          );
          assumptions.push(
            "Final PELD score rounded and capped between −10 and 99."
          );

          assumptionsList.innerHTML = assumptions
            .map((t) => `<li>${t}</li>`)
            .join("");
        });

        resetBtn.addEventListener("click", function () {
          form.reset();
          [ageYearsEl, ageMonthsEl, biliEl, albEl, inrEl].forEach(clearError);
          resetOutputs();
        });

        resetOutputs();
      });
    </script>

