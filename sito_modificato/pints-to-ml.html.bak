    <script>
      (function () {
        const factors = {
          us_liquid: 473.176473,
          us_dry: 550.610471,
          uk_imperial: 568.26125,
        };

        let mode = "pints_to_ml"; // or 'ml_to_pints'

        const inputValueEl = document.getElementById("input-value");
        const outputValueEl = document.getElementById("output-value");
        const pintTypeEl = document.getElementById("pint-type");
        const explanationEl = document.getElementById("explanation-text");
        const modePintsToMlBtn = document.getElementById("mode-pints-to-ml");
        const modeMlToPintsBtn = document.getElementById("mode-ml-to-pints");
        const inputLabelEl = document.getElementById("input-label");
        const outputLabelEl = document.getElementById("output-label");
        const swapBtn = document.getElementById("swap-btn");
        const clearBtn = document.getElementById("clear-btn");

        function formatNumber(value, decimals) {
          if (!isFinite(value)) return "";
          const abs = Math.abs(value);
          let d = decimals;
          if (abs !== 0 && (abs < 0.001 || abs > 1e6)) {
            return value.toExponential(4);
          }
          if (typeof d !== "number") d = 3;
          return Number(value.toFixed(d)).toString();
        }

        function getFactor() {
          return factors[pintTypeEl.value] || factors.us_liquid;
        }

        function getPintLabel() {
          switch (pintTypeEl.value) {
            case "us_liquid":
              return "US liquid pint";
            case "us_dry":
              return "US dry pint";
            case "uk_imperial":
              return "UK / Imperial pint";
            default:
              return "pint";
          }
        }

        function updateExplanation(input, output) {
          const factor = getFactor();
          const pintLabel = getPintLabel();

          if (input === "" || isNaN(input)) {
            explanationEl.textContent = "";
            return;
          }

          if (mode === "pints_to_ml") {
            explanationEl.textContent = `${input} ${pintLabel}${
              Math.abs(input) === 1 ? "" : "s"
            } × ${factor} = ${output} mL`;
          } else {
            explanationEl.textContent = `${input} mL ÷ ${factor} = ${output} ${pintLabel}${
              Math.abs(output) === 1 ? "" : "s"
            }`;
          }
        }

        function calculate() {
          const raw = inputValueEl.value.trim();
          if (raw === "") {
            outputValueEl.value = "";
            explanationEl.textContent = "";
            return;
          }
          const val = parseFloat(raw);
          if (isNaN(val)) {
            outputValueEl.value = "";
            explanationEl.textContent = "";
            return;
          }

          const factor = getFactor();
          let result;
          if (mode === "pints_to_ml") {
            result = val * factor;
            outputValueEl.value = formatNumber(result, 3);
          } else {
            result = val / factor;
            outputValueEl.value = formatNumber(result, 4);
          }
          updateExplanation(val, result);
        }

        function setMode(newMode) {
          mode = newMode;
          if (mode === "pints_to_ml") {
            modePintsToMlBtn.classList.add("toggle-btn-active");
            modePintsToMlBtn.classList.remove("bg-white", "text-blue-600");
            modeMlToPintsBtn.classList.remove("toggle-btn-active");
            modeMlToPintsBtn.classList.add("bg-white", "text-blue-600");
            inputLabelEl.textContent = "Pints";
            outputLabelEl.textContent = "Milliliters (mL)";
          } else {
            modeMlToPintsBtn.classList.add("toggle-btn-active");
            modeMlToPintsBtn.classList.remove("bg-white", "text-blue-600");
            modePintsToMlBtn.classList.remove("toggle-btn-active");
            modePintsToMlBtn.classList.add("bg-white", "text-blue-600");
            inputLabelEl.textContent = "Milliliters (mL)";
            outputLabelEl.textContent = "Pints";
          }
          calculate();
        }

        modePintsToMlBtn.addEventListener("click", () =>
          setMode("pints_to_ml")
        );
        modeMlToPintsBtn.addEventListener("click", () =>
          setMode("ml_to_pints")
        );

        pintTypeEl.addEventListener("change", calculate);
        inputValueEl.addEventListener("input", calculate);

        swapBtn.addEventListener("click", () => {
          const currentInput = inputValueEl.value;
          const currentOutput = outputValueEl.value;
          if (currentOutput !== "") {
            inputValueEl.value = currentOutput;
          }
          setMode(mode === "pints_to_ml" ? "ml_to_pints" : "pints_to_ml");
          calculate();
        });

        clearBtn.addEventListener("click", () => {
          inputValueEl.value = "";
          outputValueEl.value = "";
          explanationEl.textContent = "";
          inputValueEl.focus();
        });

        // Populate quick reference table
        const quickBody = document.getElementById("quick-table-body");
        const rows = [0.25, 0.5, 1, 2, 3, 4, 5];
        rows.forEach((p) => {
          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="px-3 py-2 whitespace-nowrap">${p}</td>
        <td class="px-3 py-2 whitespace-nowrap">${formatNumber(
          p * factors.us_liquid,
          1
        )}</td>
        <td class="px-3 py-2 whitespace-nowrap">${formatNumber(
          p * factors.us_dry,
          1
        )}</td>
        <td class="px-3 py-2 whitespace-nowrap">${formatNumber(
          p * factors.uk_imperial,
          1
        )}</td>
      `;
          quickBody.appendChild(tr);
        });

        // Initial explanation state
        explanationEl.textContent = "";
      })();
    </script>

    <script>
      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
