        document.getElementById('convertButton').addEventListener('click', function() {
            const torrInput = document.getElementById('torrInput');
            const torrValue = parseFloat(torrInput.value);
            const paResult = document.getElementById('paResult');
            const torrError = document.getElementById('torrError');

            if (isNaN(torrValue) || torrValue < 0) {
                torrError.textContent = 'Please enter a valid number greater than or equal to 0.';
                paResult.textContent = '0 Pa';
            } else {
                torrError.textContent = '';
                const result = torrValue * 133.322;
                paResult.textContent = result.toFixed(2) + ' Pa';
            }
        });
    </script>


