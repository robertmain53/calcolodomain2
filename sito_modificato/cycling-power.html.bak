    <script>
      // Utility functions
      function toNumber(id) {
        const v = parseFloat(document.getElementById(id).value);
        return isNaN(v) ? 0 : v;
      }

      function clamp(val, min, max) {
        return Math.min(max, Math.max(min, val));
      }

      function kgFrom(value, unit) {
        return unit === "lb" ? value * 0.45359237 : value;
      }

      function mpsFromSpeed(value, unit) {
        if (unit === "kph") return value / 3.6;
        if (unit === "mph") return value * 0.44704;
        return value; // m/s
      }

      function speedFromMps(v, unit) {
        if (unit === "kph") return v * 3.6;
        if (unit === "mph") return v / 0.44704;
        return v;
      }

      function formatNumber(x, decimals) {
        if (!isFinite(x)) return "–";
        return x.toFixed(decimals);
      }

      function showError(msg) {
        const el = document.getElementById("error-message");
        el.textContent = msg;
        el.classList.remove("hidden");
      }

      function clearError() {
        const el = document.getElementById("error-message");
        el.textContent = "";
        el.classList.add("hidden");
      }

      // Mode handling
      const modes = {
        POWER_FROM_SPEED: "power-from-speed",
        SPEED_FROM_POWER: "speed-from-power",
        REQUIRED_POWER: "required-power",
      };
      let currentMode = modes.POWER_FROM_SPEED;

      function setMode(mode) {
        currentMode = mode;
        const powerGroup = document.getElementById("power-input-group");
        const speedGroup = document.getElementById("speed-input-group");

        document.querySelectorAll(".mode-btn").forEach((btn) => {
          btn.classList.remove(
            "border-blue-600",
            "text-blue-600",
            "bg-blue-50",
            "font-medium"
          );
          btn.classList.add("border-gray-300", "text-gray-700");
        });

        if (mode === modes.POWER_FROM_SPEED) {
          document
            .getElementById("mode-power-from-speed")
            .classList.add(
              "border-blue-600",
              "text-blue-600",
              "bg-blue-50",
              "font-medium"
            );
          powerGroup.classList.add("hidden");
          speedGroup.classList.remove("hidden");
        } else if (mode === modes.SPEED_FROM_POWER) {
          document
            .getElementById("mode-speed-from-power")
            .classList.add(
              "border-blue-600",
              "text-blue-600",
              "bg-blue-50",
              "font-medium"
            );
          powerGroup.classList.remove("hidden");
          speedGroup.classList.add("hidden");
        } else {
          document
            .getElementById("mode-required-power")
            .classList.add(
              "border-blue-600",
              "text-blue-600",
              "bg-blue-50",
              "font-medium"
            );
          powerGroup.classList.add("hidden");
          speedGroup.classList.remove("hidden");
        }
      }

      document
        .getElementById("mode-power-from-speed")
        .addEventListener("click", () => setMode(modes.POWER_FROM_SPEED));
      document
        .getElementById("mode-speed-from-power")
        .addEventListener("click", () => setMode(modes.SPEED_FROM_POWER));
      document
        .getElementById("mode-required-power")
        .addEventListener("click", () => setMode(modes.REQUIRED_POWER));

      // Physics model: given speed (m/s) -> power (W)
      function powerFromSpeed(v, params) {
        const {
          massTotal,
          gradient,
          rho,
          cda,
          crr,
          drivetrainLoss,
          windSpeed,
          windDir,
        } = params;
        if (v <= 0) return 0;

        const theta = Math.atan(gradient / 100);
        let vRel = v;
        if (windDir === "head") vRel = v + windSpeed;
        else if (windDir === "tail") vRel = Math.max(0, v - windSpeed);

        const g = 9.80665;
        const pAero = 0.5 * rho * cda * Math.pow(vRel, 3);
        const pRoll = crr * massTotal * g * v * Math.cos(theta);
        const pGrav = massTotal * g * v * Math.sin(theta);
        const pWheel = pAero + pRoll + pGrav;
        const pRider = pWheel / (1 - drivetrainLoss);

        return {
          pRider,
          pAero,
          pRoll,
          pGrav,
          pWheel,
        };
      }

      // Inverse: given power (W) -> speed (m/s) via binary search
      function speedFromPower(targetPower, params) {
        if (targetPower <= 0) return 0;
        let low = 0;
        let high = 50; // 50 m/s ~ 180 km/h
        for (let i = 0; i < 40; i++) {
          const mid = (low + high) / 2;
          const { pRider } = powerFromSpeed(mid, params);
          if (pRider > targetPower) high = mid;
          else low = mid;
        }
        return (low + high) / 2;
      }

      function updateFtpZones(ftp) {
        const zones = [
          { id: "zone1-range", min: 0, max: 0.55 },
          { id: "zone2-range", min: 0.56, max: 0.75 },
          { id: "zone3-range", min: 0.76, max: 0.9 },
          { id: "zone4-range", min: 0.91, max: 1.05 },
          { id: "zone5-range", min: 1.06, max: 1.2 },
          { id: "zone6-range", min: 1.21, max: 1.5 },
        ];
        zones.forEach((z) => {
          const minW = Math.round(ftp * z.min);
          const maxW = Math.round(ftp * z.max);
          document.getElementById(z.id).textContent = `${minW}–${maxW} W`;
        });
      }

      function classifyWkg(wkg) {
        if (!isFinite(wkg) || wkg <= 0) return "–";
        if (wkg < 2) return "Recreational";
        if (wkg < 3) return "Enthusiast / club rider";
        if (wkg < 4) return "Strong club / amateur";
        if (wkg < 5) return "Competitive amateur";
        if (wkg < 6) return "Elite / semi-pro";
        return "World-class";
      }

      document.getElementById("calculate-btn").addEventListener("click", () => {
        clearError();

        const riderWeight = kgFrom(
          toNumber("rider-weight"),
          document.getElementById("rider-weight-unit").value
        );
        const bikeWeight = kgFrom(
          toNumber("bike-weight"),
          document.getElementById("bike-weight-unit").value
        );
        const massTotal = riderWeight + bikeWeight;

        if (massTotal <= 0) {
          showError("Please enter a positive rider and bike weight.");
          return;
        }

        const gradient = toNumber("gradient");
        const rho = Math.max(
          0.8,
          Math.min(1.4, toNumber("air-density") || 1.226)
        );
        const cda = Math.max(0.15, Math.min(0.6, toNumber("cda") || 0.32));
        const crr = Math.max(0.001, Math.min(0.02, toNumber("crr") || 0.004));
        const drivetrainLoss = clamp(
          (toNumber("drivetrain-loss") || 3) / 100,
          0,
          0.15
        );

        const windSpeed = mpsFromSpeed(
          toNumber("wind-speed"),
          document.getElementById("wind-speed-unit").value
        );
        const windDir = document.getElementById("wind-direction").value;

        const params = {
          massTotal,
          gradient,
          rho,
          cda,
          crr,
          drivetrainLoss,
          windSpeed,
          windDir,
        };

        let resultPowerW = 0;
        let resultSpeedMps = 0;
        let breakdown = { pAero: 0, pRoll: 0, pGrav: 0, pWheel: 0 };

        if (
          currentMode === modes.POWER_FROM_SPEED ||
          currentMode === modes.REQUIRED_POWER
        ) {
          const speedVal = toNumber("speed");
          if (speedVal <= 0) {
            showError("Please enter a positive speed.");
            return;
          }
          const v = mpsFromSpeed(
            speedVal,
            document.getElementById("speed-unit").value
          );
          const res = powerFromSpeed(v, params);
          resultPowerW = res.pRider;
          resultSpeedMps = v;
          breakdown = res;
        } else if (currentMode === modes.SPEED_FROM_POWER) {
          const inputPower = toNumber("input-power");
          if (inputPower <= 0) {
            showError("Please enter a positive power.");
            return;
          }
          const v = speedFromPower(inputPower, params);
          const res = powerFromSpeed(v, params);
          resultPowerW = inputPower;
          resultSpeedMps = v;
          breakdown = res;
        }

        // Duration & energy
        const durationVal = toNumber("effort-duration");
        const durationUnit = document.getElementById(
          "effort-duration-unit"
        ).value;
        let durationSec = 0;
        if (durationVal > 0) {
          if (durationUnit === "sec") durationSec = durationVal;
          else if (durationUnit === "min") durationSec = durationVal * 60;
          else durationSec = durationVal * 3600;
        }
        const energyKJ =
          durationSec > 0 ? (resultPowerW * durationSec) / 1000 : 0;

        // FTP
        const ftpVal = toNumber("ftp");
        const hasFtp = ftpVal > 0;
        const ftpPct = hasFtp ? (resultPowerW / ftpVal) * 100 : NaN;

        // W/kg
        const wkg = riderWeight > 0 ? resultPowerW / riderWeight : NaN;

        // Update UI
        document.getElementById("results").classList.remove("hidden");

        document.getElementById("result-power").textContent = isFinite(
          resultPowerW
        )
          ? `${Math.round(resultPowerW)} W`
          : "– W";
        document.getElementById("result-power-ftp").textContent =
          hasFtp && isFinite(ftpPct)
            ? `${ftpPct.toFixed(0)}% of FTP`
            : "Enter FTP to see % of threshold";

        document.getElementById("result-wkg").textContent = isFinite(wkg)
          ? `${wkg.toFixed(2)} W/kg`
          : "– W/kg";
        document.getElementById("result-wkg-label").textContent =
          classifyWkg(wkg);

        const speedUnit = document.getElementById("speed-unit").value;
        const speedOut = speedFromMps(resultSpeedMps, speedUnit);
        const speedLabel =
          speedUnit === "kph" ? "km/h" : speedUnit === "mph" ? "mph" : "m/s";
        document.getElementById("result-speed").textContent = isFinite(speedOut)
          ? `${speedOut.toFixed(1)} ${speedLabel}`
          : "–";

        document.getElementById("result-energy").textContent =
          durationSec > 0 && isFinite(energyKJ)
            ? `Approx. ${energyKJ.toFixed(
                0
              )} kJ over ${durationVal} ${durationUnit}`
            : "Enter duration to estimate total energy (kJ).";

        // Breakdown
        const pTotal = breakdown.pWheel / (1 - drivetrainLoss) || 0;
        const aeroPct = pTotal > 0 ? (breakdown.pAero / pTotal) * 100 : 0;
        const rollPct = pTotal > 0 ? (breakdown.pRoll / pTotal) * 100 : 0;
        const gravPct = pTotal > 0 ? (breakdown.pGrav / pTotal) * 100 : 0;

        document.getElementById("result-p-aero").textContent = `${Math.round(
          breakdown.pAero / (1 - drivetrainLoss) || 0
        )} W`;
        document.getElementById("result-p-roll").textContent = `${Math.round(
          breakdown.pRoll / (1 - drivetrainLoss) || 0
        )} W`;
        document.getElementById("result-p-grav").textContent = `${Math.round(
          breakdown.pGrav / (1 - drivetrainLoss) || 0
        )} W`;

        document.getElementById("result-p-aero-pct").textContent =
          pTotal > 0 ? `${aeroPct.toFixed(0)}% of total` : "–";
        document.getElementById("result-p-roll-pct").textContent =
          pTotal > 0 ? `${rollPct.toFixed(0)}% of total` : "–";
        document.getElementById("result-p-grav-pct").textContent =
          pTotal > 0 ? `${gravPct.toFixed(0)}% of total` : "–";

        // FTP zones
        const zonesContainer = document.getElementById("ftp-zones-container");
        if (hasFtp) {
          zonesContainer.classList.remove("hidden");
          updateFtpZones(ftpVal);
        } else {
          zonesContainer.classList.add("hidden");
        }
      });

      // Initialize default mode
      setMode(modes.POWER_FROM_SPEED);

      // Mobile menu script (simple)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", () => {
          const menu = document.getElementById("mobile-menu");
          const expanded = menu.classList.contains("hidden");
          menu.classList.toggle("hidden");
          document
            .getElementById("mobile-menu-toggle")
            .setAttribute("aria-expanded", expanded ? "true" : "false");
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
