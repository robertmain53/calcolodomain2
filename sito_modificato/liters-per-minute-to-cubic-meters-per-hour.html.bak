    <script>
      // Core conversion factors
      const FACTOR_LMIN_TO_M3H = 0.06; // exact
      const FACTOR_M3H_TO_LMIN = 1 / 0.06; // 16.666666...

      const fromValueEl = document.getElementById("fromValue");
      const fromUnitEl = document.getElementById("fromUnit");
      const toValueEl = document.getElementById("toValue");
      const toUnitEl = document.getElementById("toUnit");
      const convertBtn = document.getElementById("convertBtn");
      const clearBtn = document.getElementById("clearBtn");
      const swapBtn = document.getElementById("swapUnits");
      const summaryEl = document.getElementById("conversionSummary");
      const formulaTextEl = document.getElementById("conversionFormulaText");

      function roundSmart(value) {
        if (!isFinite(value)) return "";
        const abs = Math.abs(value);
        if (abs === 0) return "0";
        if (abs >= 1000) return value.toFixed(2);
        if (abs >= 1)
          return value.toFixed(4).replace(/0+$/, "").replace(/\.$/, "");
        if (abs >= 0.01)
          return value.toFixed(6).replace(/0+$/, "").replace(/\.$/, "");
        return value.toExponential(4);
      }

      function convertFlow(value, fromUnit, toUnit) {
        if (fromUnit === toUnit) return value;

        let result;
        if (fromUnit === "Lmin" && toUnit === "m3h") {
          result = value * FACTOR_LMIN_TO_M3H;
        } else if (fromUnit === "m3h" && toUnit === "Lmin") {
          result = value * FACTOR_M3H_TO_LMIN;
        } else {
          return NaN;
        }
        return result;
      }

      function updateFormulaText(fromUnit, toUnit) {
        let text = "";
        if (fromUnit === "Lmin" && toUnit === "m3h") {
          text = "Formula: m³/h = L/min × 0.06";
        } else if (fromUnit === "m3h" && toUnit === "Lmin") {
          text = "Formula: L/min = m³/h × 16.6667";
        } else {
          text = "";
        }
        formulaTextEl.textContent = text;
      }

      function performConversion() {
        const raw = fromValueEl.value.trim();
        if (raw === "") {
          toValueEl.value = "";
          summaryEl.classList.add("hidden");
          formulaTextEl.textContent = "";
          return;
        }

        const value = parseFloat(raw);
        if (isNaN(value)) {
          toValueEl.value = "";
          summaryEl.textContent = "Please enter a valid numeric value.";
          summaryEl.classList.remove("hidden");
          formulaTextEl.textContent = "";
          return;
        }

        const fromUnit = fromUnitEl.value;
        const toUnit = toUnitEl.value;
        const result = convertFlow(value, fromUnit, toUnit);

        if (!isFinite(result)) {
          toValueEl.value = "";
          summaryEl.textContent = "Conversion error. Please check your input.";
          summaryEl.classList.remove("hidden");
          formulaTextEl.textContent = "";
          return;
        }

        toValueEl.value = roundSmart(result);

        const fromLabel = fromUnit === "Lmin" ? "L/min" : "m³/h";
        const toLabel = toUnit === "Lmin" ? "L/min" : "m³/h";
        summaryEl.textContent = `${value} ${fromLabel} = ${roundSmart(
          result
        )} ${toLabel}`;
        summaryEl.classList.remove("hidden");

        updateFormulaText(fromUnit, toUnit);
      }

      convertBtn.addEventListener("click", performConversion);
      fromValueEl.addEventListener("input", () => {
        // live update but not too aggressive; simple call
        performConversion();
      });
      fromUnitEl.addEventListener("change", () => {
        // auto-adjust target unit to the opposite if same
        if (fromUnitEl.value === toUnitEl.value) {
          toUnitEl.value = fromUnitEl.value === "Lmin" ? "m3h" : "Lmin";
        }
        performConversion();
      });
      toUnitEl.addEventListener("change", () => {
        performConversion();
      });

      swapBtn.addEventListener("click", () => {
        const tmpUnit = fromUnitEl.value;
        fromUnitEl.value = toUnitEl.value;
        toUnitEl.value = tmpUnit;

        // Move value to keep numerical meaning
        const raw = toValueEl.value.trim();
        if (raw !== "" && !isNaN(parseFloat(raw))) {
          fromValueEl.value = raw;
        }

        performConversion();
      });

      clearBtn.addEventListener("click", () => {
        fromValueEl.value = "";
        toValueEl.value = "";
        summaryEl.classList.add("hidden");
        summaryEl.textContent = "";
        formulaTextEl.textContent = "";
        fromUnitEl.value = "Lmin";
        toUnitEl.value = "m3h";
      });

      // Quick reference table generation
      const quickTableBody = document.getElementById("quickTableBody");
      (function buildQuickTable() {
        const baseValues = [5, 10, 20, 30, 40, 50, 75, 100, 150, 200];
        let html = "";
        baseValues.forEach((v) => {
          const converted = v * FACTOR_LMIN_TO_M3H;
          html += `
        <tr class="odd:bg-white even:bg-gray-50">
          <td class="px-3 py-2 border-b text-gray-800">${v}</td>
          <td class="px-3 py-2 border-b text-gray-800">${roundSmart(
            converted
          )}</td>
        </tr>`;
        });
        quickTableBody.innerHTML = html;
      })();
    </script>

