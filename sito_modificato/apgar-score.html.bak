    <script>
      // Apgar calculator logic
      (function () {
        const criteria = [
          {
            key: "appearance",
            label: "Appearance (color)",
            options: [
              { score: 0, label: "Blue or pale all over" },
              { score: 1, label: "Pink body, blue extremities" },
              { score: 2, label: "Completely pink" },
            ],
          },
          {
            key: "pulse",
            label: "Pulse (heart rate)",
            options: [
              { score: 0, label: "Absent" },
              { score: 1, label: "< 100 beats/min" },
              { score: 2, label: "≥ 100 beats/min" },
            ],
          },
          {
            key: "grimace",
            label: "Grimace (reflex irritability)",
            options: [
              { score: 0, label: "No response to stimulation" },
              { score: 1, label: "Grimace or weak response" },
              { score: 2, label: "Cough, sneeze, or vigorous cry" },
            ],
          },
          {
            key: "activity",
            label: "Activity (muscle tone)",
            options: [
              { score: 0, label: "Limp, no movement" },
              { score: 1, label: "Some flexion of extremities" },
              { score: 2, label: "Active motion, well flexed" },
            ],
          },
          {
            key: "respiration",
            label: "Respiration (breathing)",
            options: [
              { score: 0, label: "Absent" },
              { score: 1, label: "Slow, irregular, or weak cry" },
              { score: 2, label: "Good, strong cry" },
            ],
          },
        ];

        const times = [1, 5, 10];
        const state = {
          currentTime: 1,
          // per time: { criterionKey: score }
          scores: {
            1: {},
            5: {},
            10: {},
          },
        };

        const criteriaContainer = document.getElementById("criteria-container");
        const timeButtons = document.querySelectorAll(".time-btn");
        const scoreEl = document.getElementById("apgar-score");
        const interpEl = document.getElementById("apgar-interpretation");
        const categoryEl = document.getElementById("apgar-category");
        const selectedTimeLabel = document.getElementById(
          "selected-time-label"
        );
        const summaryBody = document.getElementById("summary-table-body");
        const resetBtn = document.getElementById("reset-btn");
        const exportBtn = document.getElementById("export-btn");
        const exportStatus = document.getElementById("export-status");

        function renderCriteria() {
          criteriaContainer.innerHTML = "";
          criteria.forEach((crit) => {
            const card = document.createElement("div");
            card.className = "border border-gray-200 rounded-lg p-3";

            const title = document.createElement("h3");
            title.className = "text-sm font-semibold mb-2";
            title.textContent = crit.label;
            card.appendChild(title);

            const btnGroup = document.createElement("div");
            btnGroup.className = "flex flex-col gap-1";
            crit.options.forEach((opt) => {
              const btn = document.createElement("button");
              btn.type = "button";
              btn.className =
                "criterion-btn text-left text-xs px-2 py-1 rounded border border-gray-200 hover:bg-gray-50";
              btn.dataset.criterion = crit.key;
              btn.dataset.score = opt.score;
              btn.textContent = opt.score + " – " + opt.label;
              btnGroup.appendChild(btn);
            });

            card.appendChild(btnGroup);
            criteriaContainer.appendChild(card);
          });

          // Attach listeners
          criteriaContainer
            .querySelectorAll(".criterion-btn")
            .forEach((btn) => {
              btn.addEventListener("click", () => {
                const critKey = btn.dataset.criterion;
                const score = parseInt(btn.dataset.score, 10);
                state.scores[state.currentTime][critKey] = score;
                updateCriterionButtons(critKey);
                updateScore();
                updateSummaryTable();
              });
            });

          // Initialize selection styles
          updateAllCriterionButtons();
        }

        function updateCriterionButtons(criterionKey) {
          const currentScores = state.scores[state.currentTime];
          const selectedScore = currentScores[criterionKey];

          criteriaContainer
            .querySelectorAll(
              `.criterion-btn[data-criterion="${criterionKey}"]`
            )
            .forEach((btn) => {
              const btnScore = parseInt(btn.dataset.score, 10);
              if (btnScore === selectedScore) {
                btn.classList.remove("border-gray-200", "bg-white");
                btn.classList.add("border-blue-500", "bg-blue-50");
              } else {
                btn.classList.remove("border-blue-500", "bg-blue-50");
                btn.classList.add("border-gray-200", "bg-white");
              }
            });
        }

        function updateAllCriterionButtons() {
          criteria.forEach((c) => updateCriterionButtons(c.key));
        }

        function interpretScore(total) {
          if (isNaN(total))
            return {
              label: "Score not complete",
              category: "Select all five criteria to see interpretation.",
              color: "bg-gray-100 text-gray-800",
            };
          if (total >= 7)
            return {
              label: "Normal (7–10)",
              category: "Newborn generally adapting well.",
              color: "bg-green-100 text-green-800",
            };
          if (total >= 4)
            return {
              label: "Moderately low (4–6)",
              category: "May need medical support and close monitoring.",
              color: "bg-yellow-100 text-yellow-800",
            };
          return {
            label: "Critically low (0–3)",
            category: "Usually indicates need for immediate resuscitation.",
            color: "bg-red-100 text-red-800",
          };
        }

        function updateScore() {
          const currentScores = state.scores[state.currentTime];
          // Ensure all 5 criteria selected
          if (
            criteria.some((c) => typeof currentScores[c.key] === "undefined")
          ) {
            scoreEl.textContent = "0";
            const interp = interpretScore(NaN);
            interpEl.textContent = interp.label;
            interpEl.className = "score-badge " + interp.color;
            categoryEl.textContent = interp.category;
            return;
          }
          const total = criteria.reduce(
            (sum, c) => sum + (currentScores[c.key] || 0),
            0
          );
          scoreEl.textContent = total.toString();
          const interp = interpretScore(total);
          interpEl.textContent = interp.label;
          interpEl.className = "score-badge " + interp.color;
          categoryEl.textContent = interp.category;
        }

        function updateSummaryTable() {
          const rows = [];
          times.forEach((t) => {
            const scores = state.scores[t];
            let total = null;
            if (criteria.every((c) => typeof scores[c.key] !== "undefined")) {
              total = criteria.reduce(
                (sum, c) => sum + (scores[c.key] || 0),
                0
              );
            }
            const interp = interpretScore(total ?? NaN);
            rows.push(`
          <tr>
            <td class="px-2 py-1 border-b">${t} min</td>
            <td class="px-2 py-1 border-b ${
              total === null ? "text-gray-400" : ""
            }">${total === null ? "–" : total}</td>
            <td class="px-2 py-1 border-b ${
              total === null ? "text-gray-400" : ""
            }">${total === null ? "No data" : interp.label}</td>
          </tr>
        `);
          });
          summaryBody.innerHTML = rows.join("");
        }

        function setCurrentTime(t) {
          state.currentTime = t;
          timeButtons.forEach((btn) => {
            const btnTime = parseInt(btn.dataset.time, 10);
            if (btnTime === t) {
              btn.classList.remove("bg-gray-100", "text-gray-700");
              btn.classList.add("bg-blue-600", "text-white");
            } else {
              btn.classList.remove("bg-blue-600", "text-white");
              btn.classList.add("bg-gray-100", "text-gray-700");
            }
          });
          selectedTimeLabel.textContent = t + " minute" + (t > 1 ? "s" : "");
          updateAllCriterionButtons();
          updateScore();
        }

        function resetAll() {
          state.scores = { 1: {}, 5: {}, 10: {} };
          setCurrentTime(1);
          updateAllCriterionButtons();
          updateScore();
          updateSummaryTable();
          exportStatus.classList.add("hidden");
        }

        function exportSummary() {
          const lines = [];
          lines.push("Apgar score summary");
          times.forEach((t) => {
            const scores = state.scores[t];
            if (criteria.every((c) => typeof scores[c.key] !== "undefined")) {
              const total = criteria.reduce(
                (sum, c) => sum + (scores[c.key] || 0),
                0
              );
              const interp = interpretScore(total);
              lines.push(`${t} min: ${total}/10 – ${interp.label}`);
            } else {
              lines.push(`${t} min: not fully documented`);
            }
          });
          lines.push("");
          lines.push("Components:");
          criteria.forEach((c) => {
            const labels = times
              .map((t) => {
                const val = state.scores[t][c.key];
                return typeof val === "number" ? `${t}min=${val}` : `${t}min=–`;
              })
              .join(", ");
            lines.push(`- ${c.label}: ${labels}`);
          });

          const text = lines.join("\n");
          if (navigator.clipboard && navigator.clipboard.writeText) {
            navigator.clipboard
              .writeText(text)
              .then(() => {
                exportStatus.textContent = "Summary copied to clipboard.";
                exportStatus.classList.remove("hidden");
              })
              .catch(() => {
                exportStatus.textContent =
                  "Unable to copy automatically. You can select and copy manually.";
                exportStatus.classList.remove("hidden");
              });
          } else {
            exportStatus.textContent =
              "Clipboard not available. You can select and copy the page content manually.";
            exportStatus.classList.remove("hidden");
          }
        }

        // Event bindings
        timeButtons.forEach((btn) => {
          btn.addEventListener("click", () => {
            const t = parseInt(btn.dataset.time, 10);
            setCurrentTime(t);
          });
        });

        resetBtn.addEventListener("click", resetAll);
        exportBtn.addEventListener("click", exportSummary);

        // Initial render
        renderCriteria();
        updateScore();
        updateSummaryTable();
      })();
    </script>

