<script>
        function calculateEfficiency() {
            const numPiles = parseFloat(document.getElementById('numberOfPiles').value);
            const indCapacity = parseFloat(document.getElementById('individualPileCapacity').value);
            const groupCapacity = parseFloat(document.getElementById('groupCapacity').value);

            if (isNaN(numPiles) || isNaN(indCapacity) || isNaN(groupCapacity) || numPiles <= 0 || indCapacity <= 0 || groupCapacity <= 0) {
                alert('Please enter valid numbers for all fields.');
                return;
            }

            const efficiency = (groupCapacity / (numPiles * indCapacity)) * 100;
            document.getElementById('efficiencyResult').textContent = efficiency.toFixed(2) + '%';
        }
    </script>


