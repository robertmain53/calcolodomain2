    <script>
      function classifyAbi(value) {
        if (isNaN(value))
          return {
            label: "N/A",
            description: "No value calculated yet.",
            color: "bg-gray-200 text-gray-800",
          };
        if (value > 1.4)
          return {
            label: "Non-compressible",
            description:
              "ABI suggests non-compressible, calcified arteries. ABI may underestimate disease; consider toe-brachial index or imaging.",
            color: "bg-yellow-100 text-yellow-800",
          };
        if (value >= 1.0 && value <= 1.4)
          return {
            label: "Normal",
            description:
              "ABI is in the normal range. Significant peripheral artery disease is unlikely in this limb.",
            color: "bg-green-100 text-green-800",
          };
        if (value >= 0.91 && value <= 0.99)
          return {
            label: "Borderline",
            description:
              "Borderline ABI. Consider risk factors, symptoms, and follow-up testing if indicated.",
            color: "bg-amber-100 text-amber-800",
          };
        if (value >= 0.41 && value <= 0.9)
          return {
            label: "Abnormal (PAD)",
            description:
              "ABI indicates mild to moderate peripheral artery disease. Further vascular evaluation is usually recommended.",
            color: "bg-orange-100 text-orange-800",
          };
        if (value <= 0.4)
          return {
            label: "Severe PAD",
            description:
              "ABI suggests severe peripheral artery disease and possible critical limb ischemia. Urgent specialist assessment is often required.",
            color: "bg-red-100 text-red-800",
          };
        return {
          label: "N/A",
          description: "",
          color: "bg-gray-200 text-gray-800",
        };
      }

      function updateAbiDisplay(side, abi) {
        const valueEl = document.getElementById(side + "-abi-value");
        const interpEl = document.getElementById(side + "-abi-interpretation");
        const badgeEl = document.getElementById(side + "-abi-badge");

        if (!abi || isNaN(abi)) {
          valueEl.textContent = "–";
          interpEl.textContent = "No value calculated.";
          badgeEl.className = "badge bg-gray-200 text-gray-800 hidden";
          badgeEl.textContent = "";
          return;
        }

        const rounded = Math.round(abi * 100) / 100;
        valueEl.textContent = rounded.toFixed(2);

        const cls = classifyAbi(rounded);
        interpEl.textContent = cls.description;
        badgeEl.textContent = cls.label;
        badgeEl.className = "badge " + cls.color;
      }

      document
        .getElementById("calculate-btn")
        .addEventListener("click", function () {
          const brachial = parseFloat(
            document.getElementById("brachial-systolic").value
          );
          const rightAnkle = parseFloat(
            document.getElementById("right-ankle").value
          );
          const leftAnkle = parseFloat(
            document.getElementById("left-ankle").value
          );
          const errorEl = document.getElementById("input-error");

          errorEl.classList.add("hidden");
          errorEl.textContent = "";

          if (!brachial || brachial <= 0) {
            errorEl.textContent =
              "Please enter a valid highest arm systolic blood pressure.";
            errorEl.classList.remove("hidden");
            return;
          }

          let rightAbi = null;
          let leftAbi = null;

          if (rightAnkle && rightAnkle > 0) {
            rightAbi = rightAnkle / brachial;
          }
          if (leftAnkle && leftAnkle > 0) {
            leftAbi = leftAnkle / brachial;
          }

          if (!rightAbi && !leftAbi) {
            errorEl.textContent =
              "Please enter at least one ankle systolic blood pressure value.";
            errorEl.classList.remove("hidden");
            return;
          }

          updateAbiDisplay("right", rightAbi);
          updateAbiDisplay("left", leftAbi);

          // Overall (lowest) ABI
          const abiValues = [];
          if (rightAbi) abiValues.push(rightAbi);
          if (leftAbi) abiValues.push(leftAbi);

          if (abiValues.length > 0) {
            const minAbi = Math.min.apply(null, abiValues);
            const rounded = Math.round(minAbi * 100) / 100;
            const overallValueEl = document.getElementById("overall-abi-value");
            const overallInterpEl = document.getElementById(
              "overall-abi-interpretation"
            );
            const cls = classifyAbi(rounded);

            overallValueEl.textContent = rounded.toFixed(2);
            overallInterpEl.textContent =
              "Lowest ABI (overall): " + cls.label + ". " + cls.description;
          }
        });

      document
        .getElementById("reset-btn")
        .addEventListener("click", function () {
          document.getElementById("brachial-systolic").value = "";
          document.getElementById("right-ankle").value = "";
          document.getElementById("left-ankle").value = "";

          updateAbiDisplay("right", null);
          updateAbiDisplay("left", null);

          document.getElementById("overall-abi-value").textContent = "–";
          document.getElementById("overall-abi-interpretation").textContent =
            "";
          const errorEl = document.getElementById("input-error");
          errorEl.classList.add("hidden");
          errorEl.textContent = "";
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
