    <script>
      // Utility functions
      function boltArea(db) {
        const d = db; // inches
        return (Math.PI * d * d) / 4.0;
      }

      function formatKip(value) {
        return value.toFixed(2) + " kip";
      }

      function formatKipIn(value) {
        return value.toFixed(1) + " kip-in";
      }

      function formatIn(value) {
        return value.toFixed(2) + " in";
      }

      function formatPsi(value) {
        return value.toFixed(0) + " psi";
      }

      function utilizationBadge(u) {
        const pct = (u * 100).toFixed(0) + "%";
        if (!isFinite(u))
          return '<span class="result-badge bg-gray-100 text-gray-700">n/a</span>';
        if (u <= 0.8)
          return (
            '<span class="result-badge bg-green-100 text-green-800">OK ' +
            pct +
            "</span>"
          );
        if (u <= 1.0)
          return (
            '<span class="result-badge bg-yellow-100 text-yellow-800">High ' +
            pct +
            "</span>"
          );
        return (
          '<span class="result-badge bg-red-100 text-red-800">NG ' +
          pct +
          "</span>"
        );
      }

      function calculateBasePlate() {
        const Pu = parseFloat(document.getElementById("Pu").value || "0"); // kips
        const Mux = parseFloat(document.getElementById("Mux").value || "0"); // kip-ft
        const Vu = parseFloat(document.getElementById("Vu").value || "0"); // kips

        const B = parseFloat(document.getElementById("B").value || "0"); // in
        const N = parseFloat(document.getElementById("N").value || "0"); // in
        const bc = parseFloat(document.getElementById("bc").value || "0"); // in
        const nc = parseFloat(document.getElementById("nc").value || "0"); // in
        const tg = parseFloat(document.getElementById("tg").value || "0"); // in
        const tp = parseFloat(document.getElementById("tp").value || "0"); // in

        const gx = parseFloat(document.getElementById("gx").value || "0"); // in
        const gy = parseFloat(document.getElementById("gy").value || "0"); // in
        const db = parseFloat(document.getElementById("db").value || "0"); // in
        const fu_anchor = parseFloat(
          document.getElementById("fu_anchor").value || "0"
        ); // ksi

        const Fy = parseFloat(document.getElementById("Fy").value || "0"); // ksi
        const fc = parseFloat(document.getElementById("fc").value || "0"); // ksi

        const results = [];

        if (
          Pu <= 0 ||
          B <= 0 ||
          N <= 0 ||
          bc <= 0 ||
          nc <= 0 ||
          Fy <= 0 ||
          fc <= 0
        ) {
          document.getElementById("results-container").innerHTML =
            '<p class="text-red-600">Please provide positive values for loads, plate size, column footprint, and material strengths.</p>';
          document.getElementById("status-badge").classList.add("hidden");
          return;
        }

        // 1. Bearing pressures
        const Aplate = B * N; // in^2
        const Pu_lbs = Pu * 1000.0;
        const qavg = Pu_lbs / Aplate; // psi

        const e_ft = Mux / Pu; // ft
        const e_in = e_ft * 12.0; // in

        const qmax = qavg * (1 + (6 * e_in) / B);
        const qmin = qavg * (1 - (6 * e_in) / B);

        const uplift = qmin < 0;

        results.push({
          title: "Bearing pressure distribution",
          html:
            "<p>Average bearing pressure q<sub>avg</sub> = " +
            formatPsi(qavg) +
            "</p>" +
            "<p>Eccentricity e = " +
            formatIn(e_in) +
            "</p>" +
            "<p>q<sub>max</sub> = " +
            formatPsi(qmax) +
            ", q<sub>min</sub> = " +
            formatPsi(qmin) +
            (uplift ? " (uplift occurs)" : "") +
            "</p>",
        });

        // 2. Concrete bearing strength (simplified ACI)
        const A1 = bc * nc; // in^2
        const A2 = Aplate;
        const phi_b = 0.65;
        const phiPb =
          phi_b * 0.85 * fc * 1000 * A1 * Math.min(Math.sqrt(A2 / A1), 2.0); // lbs, limit A2/A1^0.5 to 2.0
        const phiPb_kip = phiPb / 1000.0;
        const uBearing = Pu / phiPb_kip;

        results.push({
          title: "Concrete bearing check",
          html:
            "<p>Loaded area A<sub>1</sub> = " +
            A1.toFixed(0) +
            " in², supporting area A<sub>2</sub> = " +
            A2.toFixed(0) +
            " in²</p>" +
            "<p>ϕP<sub>b</sub> ≈ " +
            phiPb_kip.toFixed(1) +
            " kip</p>" +
            "<p>Utilization P<sub>u</sub> / ϕP<sub>b</sub> = " +
            uBearing.toFixed(2) +
            " " +
            utilizationBadge(uBearing) +
            "</p>",
        });

        // 3. Plate bending and required thickness (cantilever strip)
        const phi_flex = 0.9;
        const m = (B - bc) / 2.0; // in
        const wu = (qmax * N) / 1000.0; // kip/in
        const Mu_in = (wu * m * m) / 2.0; // kip-in per strip
        const Zreq = Mu_in / (phi_flex * Fy); // in^3 per in width
        const tReq = Math.sqrt(4 * Zreq); // in

        const uThickness = tReq / tp;

        results.push({
          title: "Plate bending & thickness",
          html:
            "<p>Cantilever projection m = " +
            formatIn(m) +
            "</p>" +
            "<p>Factored moment M<sub>u</sub> ≈ " +
            formatKipIn(Mu_in) +
            " per unit width</p>" +
            "<p>Required thickness t<sub>req</sub> ≈ " +
            formatIn(tReq) +
            "</p>" +
            "<p>Provided thickness t<sub>p</sub> = " +
            formatIn(tp) +
            " ⇒ utilization t<sub>req</sub>/t<sub>p</sub> = " +
            uThickness.toFixed(2) +
            " " +
            utilizationBadge(uThickness) +
            "</p>",
        });

        // 4. Anchor tension (very simplified)
        let uTension = 0;
        let Tanchor = 0;
        if (uplift) {
          // Assume compression block length a where q ranges from 0 to qmax
          const a = B / 3.0; // crude estimate
          const C = (qmax * a * N) / 1000.0; // kip
          const Ttot = Math.max(0, Pu - C); // kip
          Tanchor = Ttot / 2.0; // two tension anchors
          const Ab = boltArea(db); // in^2
          const phi_t = 0.75;
          const phiTn = phi_t * 0.79 * Ab * fu_anchor; // kips (ksi*in^2)
          uTension = Tanchor / phiTn;

          results.push({
            title: "Anchor tension (approximate)",
            html:
              "<p>Total tension T<sub>tot</sub> ≈ " +
              formatKip(Ttot) +
              "</p>" +
              "<p>Tension per anchor (2 anchors) T<sub>a</sub> ≈ " +
              formatKip(Tanchor) +
              "</p>" +
              "<p>ϕT<sub>n</sub> ≈ " +
              phiTn.toFixed(1) +
              " kip ⇒ utilization T<sub>a</sub>/ϕT<sub>n</sub> = " +
              uTension.toFixed(2) +
              " " +
              utilizationBadge(uTension) +
              "</p>",
          });
        } else {
          results.push({
            title: "Anchor tension",
            html: "<p>No uplift region (q<sub>min</sub> ≥ 0). Anchor tension from overturning is negligible in this simplified model.</p>",
          });
        }

        // 5. Anchor shear
        let uShear = 0;
        if (Vu > 0 && db > 0 && fu_anchor > 0) {
          const Va = Vu / 4.0; // kip per anchor
          const Ab = boltArea(db);
          const phi_v = 0.75;
          const phiVn = phi_v * 0.6 * Ab * fu_anchor; // kips
          uShear = Va / phiVn;

          results.push({
            title: "Anchor shear",
            html:
              "<p>Shear per anchor V<sub>a</sub> ≈ " +
              formatKip(Va) +
              "</p>" +
              "<p>ϕV<sub>n</sub> ≈ " +
              phiVn.toFixed(1) +
              " kip ⇒ utilization V<sub>a</sub>/ϕV<sub>n</sub> = " +
              uShear.toFixed(2) +
              " " +
              utilizationBadge(uShear) +
              "</p>",
          });
        } else {
          results.push({
            title: "Anchor shear",
            html: "<p>No shear specified or anchor data missing. Shear check skipped.</p>",
          });
        }

        // Overall status
        const utilizations = [uBearing, uThickness];
        if (uplift) utilizations.push(uTension);
        if (Vu > 0) utilizations.push(uShear);
        const uMax = Math.max.apply(
          null,
          utilizations.filter((v) => isFinite(v) && v > 0)
        );
        const statusEl = document.getElementById("status-badge");
        if (isFinite(uMax)) {
          statusEl.classList.remove("hidden");
          statusEl.innerHTML =
            uMax <= 1
              ? "Design OK (max util " + (uMax * 100).toFixed(0) + "%)"
              : "Design NG (max util " + (uMax * 100).toFixed(0) + "%)";
          statusEl.className =
            "result-badge " +
            (uMax <= 1
              ? "bg-green-100 text-green-800"
              : "bg-red-100 text-red-800");
        } else {
          statusEl.classList.add("hidden");
        }

        // Render results
        const container = document.getElementById("results-container");
        container.innerHTML = results
          .map(
            (block) =>
              '<div class="border border-gray-200 rounded-lg p-3">' +
              '<h3 class="font-semibold mb-1 text-gray-800">' +
              block.title +
              "</h3>" +
              block.html +
              "</div>"
          )
          .join("");
      }

      document
        .getElementById("calculate-btn")
        .addEventListener("click", calculateBasePlate);
      document.getElementById("reset-btn").addEventListener("click", () => {
        document.getElementById("baseplate-form").reset();
        document.getElementById("results-container").innerHTML =
          '<p class="text-gray-500">Enter your data and click “Calculate Design” to see bearing, plate thickness, and anchor checks.</p>';
        document.getElementById("status-badge").classList.add("hidden");
      });
    </script>

