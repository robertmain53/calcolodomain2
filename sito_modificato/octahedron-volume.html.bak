<script="">
        document.getElementById('calculateButton').addEventListener('click', function() {
            const sideLengthInput = document.getElementById('sideLength');
            const sideLength = parseFloat(sideLengthInput.value);
            const errorElement = document.getElementById('sideLengthError');
            const volumeResultElement = document.getElementById('volumeResult');

            if (isNaN(sideLength) || sideLength <= 0) {
                errorElement.textContent = 'Please enter a valid positive number for the side length.';
                sideLengthInput.setAttribute('aria-invalid', 'true');
                volumeResultElement.textContent = 'N/A';
            } else {
                errorElement.textContent = '';
                sideLengthInput.removeAttribute('aria-invalid');

                const volume = (Math.sqrt(2) / 3) * Math.pow(sideLength, 3);
                volumeResultElement.textContent = volume.toFixed(2) + ' cubic units';
            }
        });
    </script>


