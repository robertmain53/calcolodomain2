    <script>
      // Core EAN / GTIN check digit logic
      function computeGtinCheckDigit(digits) {
        // digits: string without check digit
        const len = digits.length;
        let sum = 0;
        // For GTIN (8, 12, 13, 14), weights are applied from right to left:
        // starting with 3 on the rightmost, then 1, 3, 1, ...
        // This matches GS1 for EAN-13, UPC-A, GTIN-14 and EAN-8.
        for (let i = 0; i < len; i++) {
          const d = parseInt(digits[len - 1 - i], 10);
          const weight = i % 2 === 0 ? 3 : 1;
          sum += d * weight;
        }
        const mod = sum % 10;
        const check = (10 - mod) % 10;
        return { checkDigit: check, sum, mod };
      }

      function detectTypeFromLength(len) {
        if (len === 8) return "EAN-8";
        if (len === 12) return "UPC-A";
        if (len === 13) return "EAN-13";
        if (len === 14) return "GTIN-14";
        return null;
      }

      function getPrefixInfo(code, type) {
        if (!code || code.length < 3) return null;
        const prefix3 = code.slice(0, 3);
        const prefix2 = code.slice(0, 2);

        // Very small, non-authoritative mapping for user guidance only
        if (type === "EAN-13" || type === "GTIN-14" || type === "UPC-A") {
          if (prefix3 >= "000" && prefix3 <= "019") {
            return (
              "Prefix " +
              prefix3 +
              ": GS1 US (USA & Canada). Often used for UPC‑A codes."
            );
          }
          if (prefix3 >= "300" && prefix3 <= "379") {
            return "Prefix " + prefix3 + ": GS1 France.";
          }
          if (prefix3 >= "380" && prefix3 <= "380") {
            return "Prefix 380: GS1 Bulgaria.";
          }
          if (prefix3 >= "400" && prefix3 <= "440") {
            return "Prefix " + prefix3 + ": GS1 Germany.";
          }
          if (prefix3 >= "450" && prefix3 <= "459") {
            return "Prefix " + prefix3 + ": GS1 Japan.";
          }
          if (prefix3 >= "460" && prefix3 <= "469") {
            return "Prefix " + prefix3 + ": GS1 Russia.";
          }
          if (
            prefix3 === "690" ||
            prefix3 === "691" ||
            prefix3 === "692" ||
            prefix3 === "693" ||
            prefix3 === "694" ||
            prefix3 === "695"
          ) {
            return "Prefix " + prefix3 + ": GS1 China.";
          }
          if (prefix2 === "50") {
            return "Prefix " + prefix2 + "‑: GS1 UK.";
          }
          if (prefix2 === "54") {
            return "Prefix " + prefix2 + "‑: GS1 Belgium & Luxembourg.";
          }
          if (prefix2 === "57") {
            return "Prefix " + prefix2 + "‑: GS1 Denmark.";
          }
          if (prefix2 === "64") {
            return "Prefix " + prefix2 + "‑: GS1 Finland.";
          }
          if (prefix2 === "76") {
            return "Prefix " + prefix2 + "‑: GS1 Switzerland & Liechtenstein.";
          }
          if (prefix2 === "80") {
            return "Prefix " + prefix2 + "‑: GS1 Italy.";
          }
        }
        return (
          "Prefix " +
          prefix3 +
          ": valid GS1 prefix range not guaranteed. Use GS1 resources for authoritative information."
        );
      }

      function buildBreakdownText(code, type, calc) {
        const body = code.slice(0, -1);
        const checkDigit = code.slice(-1);
        const len = body.length;
        const digits = body.split("").map((d) => parseInt(d, 10));
        const weights = [];
        const products = [];

        for (let i = 0; i < len; i++) {
          const d = digits[len - 1 - i];
          const weight = i % 2 === 0 ? 3 : 1;
          weights.unshift(weight);
          products.unshift(d * weight);
        }

        return [
          `Type: ${type}`,
          `Code: ${code}`,
          "",
          `Body digits:      ${body.split("").join(" ")}`,
          `Weights (L→R):    ${weights.join(" ")}`,
          `Products:         ${products.join(" ")}`,
          "",
          `Sum of products = ${calc.sum}`,
          `Sum mod 10 = ${calc.mod}`,
          `Computed check digit = (10 − ${calc.mod}) mod 10 = ${calc.checkDigit}`,
          `Provided check digit = ${checkDigit}`,
        ].join("\n");
      }

      function validateSingleCode(raw, forcedType) {
        const original = raw;
        const normalized = (raw || "").replace(/\D+/g, "");
        if (!normalized) {
          return {
            original,
            normalized,
            type: null,
            valid: false,
            status: "Empty or no digits",
            error: "No digits found in input.",
          };
        }

        const len = normalized.length;
        const detectedType =
          forcedType === "auto" ? detectTypeFromLength(len) : forcedType;

        if (!detectedType) {
          return {
            original,
            normalized,
            type: null,
            valid: false,
            status: "Invalid length",
            error: `Unsupported length ${len}. Expected 8, 12, 13 or 14 digits.`,
          };
        }

        const body = normalized.slice(0, -1);
        const providedCheck = parseInt(normalized.slice(-1), 10);
        const calc = computeGtinCheckDigit(body);
        const valid = calc.checkDigit === providedCheck;

        const typeLabel = detectedType;
        const status = valid ? "Valid" : "Invalid check digit";

        const breakdown = buildBreakdownText(normalized, typeLabel, calc);
        const prefixInfo = getPrefixInfo(normalized, typeLabel);

        return {
          original,
          normalized,
          type: typeLabel,
          valid,
          status,
          expectedCheckDigit: calc.checkDigit,
          providedCheckDigit: providedCheck,
          breakdown,
          prefixInfo,
        };
      }

      function parseInputToCodes(text) {
        if (!text) return [];
        // Split on commas, semicolons, whitespace and newlines
        return text
          .split(/[\s,;]+/)
          .map((s) => s.trim())
          .filter((s) => s.length > 0);
      }

      document.addEventListener("DOMContentLoaded", function () {
        const inputEl = document.getElementById("ean-input");
        const typeEl = document.getElementById("ean-type");
        const validateBtn = document.getElementById("validate-btn");
        const clearBtn = document.getElementById("clear-btn");
        const summaryEl = document.getElementById("ean-summary");
        const summaryBadge = document.getElementById("ean-summary-badge");
        const resultsWrapper = document.getElementById("ean-results-wrapper");
        const resultsBody = document.getElementById("ean-results-body");
        const breakdownEl = document.getElementById("ean-breakdown");
        const breakdownText = document.getElementById("ean-breakdown-text");
        const prefixEl = document.getElementById("ean-prefix");
        const prefixText = document.getElementById("ean-prefix-text");

        function renderResults(results) {
          resultsBody.innerHTML = "";
          breakdownEl.classList.add("hidden");
          prefixEl.classList.add("hidden");

          if (!results.length) {
            summaryEl.classList.add("hidden");
            resultsWrapper.classList.add("hidden");
            return;
          }

          const validCount = results.filter((r) => r.valid).length;
          const invalidCount = results.length - validCount;

          summaryBadge.textContent = `${validCount} valid, ${invalidCount} invalid`;
          summaryBadge.className =
            "inline-flex items-center px-3 py-1 rounded-full text-xs font-medium " +
            (invalidCount === 0
              ? "bg-green-100 text-green-800"
              : "bg-yellow-100 text-yellow-800");
          summaryEl.classList.remove("hidden");

          results.forEach((r, idx) => {
            const tr = document.createElement("tr");
            tr.className = "hover:bg-gray-50";

            const statusColor = r.valid
              ? "text-green-700 bg-green-50 border-green-200"
              : "text-red-700 bg-red-50 border-red-200";

            tr.innerHTML = `
          <td class="px-3 py-2 mono break-all">${
            r.original || "<em class='text-gray-400'>n/a</em>"
          }</td>
          <td class="px-3 py-2 mono break-all">${
            r.normalized || "<span class='text-gray-400'>–</span>"
          }</td>
          <td class="px-3 py-2">${
            r.type || "<span class='text-gray-400'>Unknown</span>"
          }</td>
          <td class="px-3 py-2">
            <span class="inline-flex items-center px-2 py-0.5 rounded-full text-xs font-medium border ${statusColor}">
              ${r.status}
            </span>
          </td>
          <td class="px-3 py-2">${
            typeof r.expectedCheckDigit === "number"
              ? r.expectedCheckDigit
              : "<span class='text-gray-400'>–</span>"
          }</td>
          <td class="px-3 py-2">
            ${
              r.error
                ? `<span class="text-xs text-red-600">${r.error}</span>`
                : `
              <button type="button" data-idx="${idx}" class="text-xs text-blue-600 hover:underline ean-show-details">
                Show breakdown
              </button>
            `
            }
          </td>
        `;
            resultsBody.appendChild(tr);
          });

          resultsWrapper.classList.remove("hidden");

          // Attach click handlers for breakdown buttons
          resultsBody.querySelectorAll(".ean-show-details").forEach((btn) => {
            btn.addEventListener("click", () => {
              const idx = parseInt(btn.getAttribute("data-idx"), 10);
              const r = results[idx];
              if (!r) return;
              breakdownText.textContent = r.breakdown || "";
              breakdownEl.classList.remove("hidden");

              if (r.prefixInfo) {
                prefixText.textContent = r.prefixInfo;
                prefixEl.classList.remove("hidden");
              } else {
                prefixEl.classList.add("hidden");
              }

              // Scroll into view on small screens
              breakdownEl.scrollIntoView({
                behavior: "smooth",
                block: "start",
              });
            });
          });
        }

        function handleValidate() {
          const text = inputEl.value;
          const forcedType = typeEl.value || "auto";
          const codes = parseInputToCodes(text);
          const results = codes.map((code) =>
            validateSingleCode(code, forcedType)
          );
          renderResults(results);
        }

        validateBtn.addEventListener("click", handleValidate);

        clearBtn.addEventListener("click", () => {
          inputEl.value = "";
          resultsBody.innerHTML = "";
          summaryEl.classList.add("hidden");
          resultsWrapper.classList.add("hidden");
          breakdownEl.classList.add("hidden");
          prefixEl.classList.add("hidden");
        });

        inputEl.addEventListener("keydown", (e) => {
          if (e.key === "Enter" && (e.metaKey || e.ctrlKey)) {
            e.preventDefault();
            handleValidate();
          }
        });
      });
    </script>

