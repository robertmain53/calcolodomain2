  <script>
    // mobile menu toggle
    const mobileToggle = document.getElementById('mobile-menu-toggle');
    const mobileMenu = document.getElementById('mobile-menu');
    if (mobileToggle && mobileMenu) {
      mobileToggle.addEventListener('click', () => {
        const isHidden = mobileMenu.classList.contains('hidden');
        mobileMenu.classList.toggle('hidden', !isHidden);
        mobileToggle.setAttribute('aria-expanded', String(isHidden));
      });
    }

    // Checksum logic
    const dataInput = document.getElementById('data-input');
    const inputType = document.getElementById('input-type');
    const algorithm = document.getElementById('algorithm');
    const endianSel = document.getElementById('endian');
    const calcBtn = document.getElementById('calc-btn');
    const resultHex = document.getElementById('result-hex');
    const resultDec = document.getElementById('result-dec');
    const resultAlgo = document.getElementById('result-algo');
    const inputError = document.getElementById('input-error');
    const cryptoWarning = document.getElementById('crypto-warning');

    // parse input to byte array
    function parseInputToBytes(str, type) {
      if (type === 'text') {
        const bytes = [];
        for (let i = 0; i < str.length; i++) {
          bytes.push(str.charCodeAt(i) & 0xFF);
        }
        return bytes;
      } else {
        // hex
        const cleaned = str.replace(/[^0-9a-fA-F]/g, '');
        if (cleaned.length % 2 !== 0) {
          // try to fix odd length
          return null;
        }
        const bytes = [];
        for (let i = 0; i < cleaned.length; i += 2) {
          bytes.push(parseInt(cleaned.substr(i, 2), 16));
        }
        return bytes;
      }
    }

    // CRC32
    let CRC32_TABLE = null;
    function makeCrc32Table() {
      const table = new Uint32Array(256);
      for (let i = 0; i < 256; i++) {
        let c = i;
        for (let j = 0; j < 8; j++) {
          if (c & 1) {
            c = 0xEDB88320 ^ (c >>> 1);
          } else {
            c = c >>> 1;
          }
        }
        table[i] = c >>> 0;
      }
      return table;
    }
    function crc32(bytes) {
      if (!CRC32_TABLE) CRC32_TABLE = makeCrc32Table();
      let crc = 0xffffffff;
      for (let i = 0; i < bytes.length; i++) {
        const byte = bytes[i];
        const idx = (crc ^ byte) & 0xff;
        crc = (crc >>> 8) ^ CRC32_TABLE[idx];
      }
      return (crc ^ 0xffffffff) >>> 0;
    }

    // CRC16 / IBM (Modbus style poly 0xA001)
    function crc16(bytes) {
      let crc = 0xFFFF;
      for (let i = 0; i < bytes.length; i++) {
        crc ^= bytes[i];
        for (let j = 0; j < 8; j++) {
          if (crc & 1) {
            crc = (crc >>> 1) ^ 0xA001;
          } else {
            crc = crc >>> 1;
          }
        }
      }
      return crc & 0xFFFF;
    }

    // Adler-32
    function adler32(bytes) {
      let MOD_ADLER = 65521;
      let a = 1, b = 0;
      for (let i = 0; i < bytes.length; i++) {
        a = (a + bytes[i]) % MOD_ADLER;
        b = (b + a) % MOD_ADLER;
      }
      return ((b << 16) | a) >>> 0;
    }

    function sum8(bytes) {
      let s = 0;
      for (const b of bytes) s = (s + b) & 0xFF;
      return s;
    }

    function sum16(bytes) {
      let s = 0;
      for (const b of bytes) s = (s + b) & 0xFFFF;
      return s;
    }

    function xor8(bytes) {
      let x = 0;
      for (const b of bytes) x ^= b;
      return x & 0xFF;
    }

    function toHex(value, bytes, endian) {
      const arr = [];
      for (let i = 0; i < bytes; i++) {
        const shift = endian === 'little' ? i*8 : (bytes-1-i)*8;
        const b = (value >>> shift) & 0xFF;
        arr.push(b.toString(16).toUpperCase().padStart(2,'0'));
      }
      return arr.join(' ');
    }

    async function computeDigest(algoName, bytes) {
      if (!window.crypto || !window.crypto.subtle) {
        cryptoWarning.style.display = 'inline-block';
        return null;
      }
      cryptoWarning.style.display = 'none';
      const data = new Uint8Array(bytes);
      // map select to subtle names
      const algoMap = {
        md5: 'MD5',
        sha1: 'SHA-1',
        sha256: 'SHA-256'
      };
      const subtleName = algoMap[algoName];
      if (!subtleName) return null;
      try {
        const digest = await window.crypto.subtle.digest(subtleName, data);
        return new Uint8Array(digest);
      } catch (e) {
        console.error(e);
        return null;
      }
    }

    function bytesToHex(arr) {
      return Array.from(arr).map(b => b.toString(16).toUpperCase().padStart(2,'0')).join('');
    }

    calcBtn.addEventListener('click', async () => {
      inputError.textContent = "";
      resultHex.textContent = "—";
      resultDec.textContent = "—";
      const raw = dataInput.value.trim();
      if (!raw) {
        inputError.textContent = "Please enter some data.";
        return;
      }
      const bytes = parseInputToBytes(raw, inputType.value);
      if (!bytes) {
        inputError.textContent = "Invalid hex input. Use even number of hex digits.";
        return;
      }
      const algo = algorithm.value;
      const endian = endianSel.value;
      resultAlgo.textContent = algorithm.options[algorithm.selectedIndex].text;

      if (algo === 'crc32') {
        const val = crc32(bytes);
        resultHex.textContent = toHex(val, 4, endian);
        resultDec.textContent = val.toString(10);
        return;
      }
      if (algo === 'crc16') {
        const val = crc16(bytes);
        resultHex.textContent = toHex(val, 2, endian);
        resultDec.textContent = val.toString(10);
        return;
      }
      if (algo === 'adler32') {
        const val = adler32(bytes);
        resultHex.textContent = toHex(val, 4, endian);
        resultDec.textContent = val.toString(10);
        return;
      }
      if (algo === 'sum8') {
        const val = sum8(bytes);
        resultHex.textContent = toHex(val, 1, endian);
        resultDec.textContent = val.toString(10);
        return;
      }
      if (algo === 'sum16') {
        const val = sum16(bytes);
        resultHex.textContent = toHex(val, 2, endian);
        resultDec.textContent = val.toString(10);
        return;
      }
      if (algo === 'xor8') {
        const val = xor8(bytes);
        resultHex.textContent = toHex(val, 1, endian);
        resultDec.textContent = val.toString(10);
        return;
      }
      // crypto digests
      if (algo === 'md5' || algo === 'sha1' || algo === 'sha256') {
        const dig = await computeDigest(algo, bytes);
        if (!dig) {
          inputError.textContent = "WebCrypto not available in this browser. Try CRC/Adler/sum or a modern browser.";
          return;
        }
        const hexString = bytesToHex(dig);
        resultHex.textContent = hexString.match(/.{1,2}/g).join(' ');
        resultDec.textContent = "— (digest)";
        return;
      }
    });

    // initial compute
    calcBtn.click();
  </script>

