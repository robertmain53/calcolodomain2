    <script>
      // Tab switching
      const tabBeer = document.getElementById("tab-beer");
      const tabSourdough = document.getElementById("tab-sourdough");
      const panelBeer = document.getElementById("panel-beer");
      const panelSourdough = document.getElementById("panel-sourdough");

      function activateTab(tab) {
        if (tab === "beer") {
          tabBeer.classList.add("tab-active");
          tabBeer.setAttribute("aria-selected", "true");
          tabSourdough.classList.remove("tab-active");
          tabSourdough.setAttribute("aria-selected", "false");
          panelBeer.classList.remove("hidden");
          panelSourdough.classList.add("hidden");
        } else {
          tabSourdough.classList.add("tab-active");
          tabSourdough.setAttribute("aria-selected", "true");
          tabBeer.classList.remove("tab-active");
          tabBeer.setAttribute("aria-selected", "false");
          panelSourdough.classList.remove("hidden");
          panelBeer.classList.add("hidden");
        }
      }

      tabBeer.addEventListener("click", () => activateTab("beer"));
      tabSourdough.addEventListener("click", () => activateTab("sourdough"));

      // Utility functions
      function roundTo(value, decimals) {
        const factor = Math.pow(10, decimals);
        return Math.round(value * factor) / factor;
      }

      function formatCells(billions) {
        if (billions >= 1000) {
          return roundTo(billions / 1000, 2) + " trillion";
        }
        return roundTo(billions, 2) + " billion";
      }

      // Beer starter calculation
      document.getElementById("beer-calc-btn").addEventListener("click", () => {
        const vol =
          parseFloat(document.getElementById("beer-volume").value) || 0;
        const volUnit = document.getElementById("beer-volume-unit").value;
        const og = parseFloat(document.getElementById("beer-og").value) || 1.0;
        const yeastType = document.getElementById("yeast-type").value;
        const yeastForm = document.getElementById("yeast-form").value;
        const ageDays =
          parseFloat(document.getElementById("yeast-age").value) || 0;
        const stirMethod = document.getElementById("stir-method").value;
        const steps =
          parseInt(document.getElementById("starter-steps").value, 10) || 1;
        const maxVolInput =
          parseFloat(document.getElementById("starter-max-volume").value) || 1;
        const maxVolUnit = document.getElementById(
          "starter-max-volume-unit"
        ).value;
        const starterSG =
          parseFloat(document.getElementById("starter-sg").value) || 1.037;

        if (vol <= 0 || og < 1.0) {
          alert("Please enter a valid batch volume and OG.");
          return;
        }

        // Convert batch volume to liters
        const batchVolL = volUnit === "gal" ? vol * 3.78541 : vol;
        const batchVolmL = batchVolL * 1000;

        // OG to Plato (approx)
        const degP = -463.37 + 668.72 * og - 205.35 * og * og;

        // Pitch rate (million cells/mL/°P)
        let pitchRate;
        if (yeastType === "lager") pitchRate = 1.5;
        else if (yeastType === "hybrid") pitchRate = 1.0;
        else pitchRate = 0.75;

        const requiredCellsMillions = pitchRate * batchVolmL * degP;
        const requiredCellsBillions = requiredCellsMillions / 1000;

        // Yeast pack assumptions
        let freshCellsBillions;
        let decayPerDay;
        if (yeastForm === "dry") {
          freshCellsBillions = 200; // typical 11 g pack
          decayPerDay = 0.002;
        } else if (yeastForm === "slurry") {
          freshCellsBillions = 150; // rough assumption
          decayPerDay = 0.007;
        } else {
          freshCellsBillions = 100; // liquid pack
          decayPerDay = 0.007;
        }

        const viability = Math.max(0, 1 - decayPerDay * ageDays);
        const viableCellsBillions = freshCellsBillions * viability;

        // Growth factor by method (approximate)
        let maxGrowth;
        if (stirMethod === "stirplate") maxGrowth = 4.0;
        else if (stirMethod === "intermittent") maxGrowth = 2.5;
        else maxGrowth = 1.5;

        // Max starter volume in liters (80% of flask)
        let maxFlaskL = maxVolUnit === "mL" ? maxVolInput / 1000 : maxVolInput;
        const maxStarterL = maxFlaskL * 0.8;

        // Simple model: growth factor scales with volume up to maxStarterL
        function growthFactor(volumeL) {
          const rel = Math.min(1, volumeL / maxStarterL);
          const minGrowth = 1.1;
          return minGrowth + (maxGrowth - minGrowth) * rel;
        }

        // Step 1 volume: try to get close to required cells in one step
        let step1VolL = Math.min(maxStarterL, Math.max(0.5, batchVolL * 0.08)); // 8% of batch volume, capped
        let step1Growth = growthFactor(step1VolL);
        let step1FinalCells = viableCellsBillions * step1Growth;

        let step2Needed = false;
        let step2VolL = 0;
        let step2Growth = 1;
        let step2FinalCells = step1FinalCells;

        if (step1FinalCells < requiredCellsBillions && steps === 2) {
          step2Needed = true;
          // Second step: use maxStarterL
          step2VolL = maxStarterL;
          step2Growth = growthFactor(step2VolL);
          step2FinalCells = step1FinalCells * step2Growth;
        }

        // If still short, just report that it's under-pitched
        const beerResults = document.getElementById("beer-results");
        beerResults.classList.remove("hidden");

        document.getElementById("beer-req-cells").textContent = formatCells(
          requiredCellsBillions
        );
        document.getElementById("beer-degP").textContent = roundTo(degP, 1);
        document.getElementById("beer-volume-display").textContent =
          roundTo(batchVolL, 2) + " L";

        document.getElementById("beer-viable-cells").textContent =
          formatCells(viableCellsBillions);
        document.getElementById("beer-viability").textContent = Math.round(
          viability * 100
        );

        // Starter gravity display (e.g. 1.037 -> "037")
        const sgSuffix = String(Math.round((starterSG - 1) * 1000)).padStart(
          3,
          "0"
        );

        // DME calculation
        function dmeForVolume(volumeL) {
          // 100 g/L at 1.037; scale linearly with gravity
          const baseSG = 1.037;
          const factor = (starterSG - 1) / (baseSG - 1);
          return 100 * volumeL * factor;
        }

        // Step 1 display
        document.getElementById("beer-step1-vol").textContent =
          roundTo(step1VolL, 2) + " L";
        document.getElementById("beer-step1-sg").textContent = sgSuffix;
        document.getElementById("beer-step1-dme").textContent =
          roundTo(dmeForVolume(step1VolL), 1) + " g DME";
        document.getElementById("beer-step1-growth").textContent = roundTo(
          step1Growth,
          2
        );
        document.getElementById("beer-step1-final-cells").textContent =
          formatCells(step1FinalCells);

        const step2Block = document.getElementById("beer-step2");
        if (step2Needed) {
          step2Block.classList.remove("hidden");
          document.getElementById("beer-step2-vol").textContent =
            roundTo(step2VolL, 2) + " L";
          document.getElementById("beer-step2-sg").textContent = sgSuffix;
          document.getElementById("beer-step2-dme").textContent =
            roundTo(dmeForVolume(step2VolL), 1) + " g DME";
          document.getElementById("beer-step2-growth").textContent = roundTo(
            step2Growth,
            2
          );
          document.getElementById("beer-step2-final-cells").textContent =
            formatCells(step2FinalCells);
        } else {
          step2Block.classList.add("hidden");
        }
      });

      // Sourdough calculation
      const sdRatioSelect = document.getElementById("sd-ratio");
      const sdCustomRatioWrap = document.getElementById("sd-custom-ratio-wrap");
      const sdHydrationSelect = document.getElementById("sd-hydration");
      const sdCustomHydrationWrap = document.getElementById(
        "sd-custom-hydration-wrap"
      );

      sdRatioSelect.addEventListener("change", () => {
        if (sdRatioSelect.value === "custom") {
          sdCustomRatioWrap.classList.remove("hidden");
        } else {
          sdCustomRatioWrap.classList.add("hidden");
        }
      });

      sdHydrationSelect.addEventListener("change", () => {
        if (sdHydrationSelect.value === "custom") {
          sdCustomHydrationWrap.classList.remove("hidden");
        } else {
          sdCustomHydrationWrap.classList.add("hidden");
        }
      });

      document.getElementById("sd-calc-btn").addEventListener("click", () => {
        const currentStarter =
          parseFloat(document.getElementById("sd-current-weight").value) || 0;
        const targetTotal =
          parseFloat(document.getElementById("sd-target-weight").value) || 0;
        if (currentStarter <= 0 || targetTotal <= 0) {
          alert("Please enter valid starter and target weights.");
          return;
        }

        let S, F, W;
        const ratioVal = sdRatioSelect.value;
        if (ratioVal === "custom") {
          S =
            parseFloat(document.getElementById("sd-ratio-starter").value) || 1;
          F = parseFloat(document.getElementById("sd-ratio-flour").value) || 1;
          W = parseFloat(document.getElementById("sd-ratio-water").value) || 1;
        } else {
          const parts = ratioVal.split(":").map(parseFloat);
          S = parts[0];
          F = parts[1];
          W = parts[2];
        }

        const totalParts = S + F + W;
        let keepStarter = targetTotal * (S / totalParts);
        let addFlour = targetTotal * (F / totalParts);
        let addWater = targetTotal * (W / totalParts);

        // Adjust if we don't have enough starter: cap by currentStarter
        if (keepStarter > currentStarter) {
          keepStarter = currentStarter;
          const scale = keepStarter / (targetTotal * (S / totalParts));
          addFlour *= scale;
          addWater *= scale;
        }

        // Hydration
        let hydration;
        if (sdHydrationSelect.value === "custom") {
          hydration =
            parseFloat(document.getElementById("sd-hydration-custom").value) ||
            100;
        } else {
          hydration = parseFloat(sdHydrationSelect.value);
        }

        // For non-100% hydration, adjust water amount to match hydration
        // Assume starter being kept is already at approx hydration; we only adjust the added water
        const flourFromStarter = keepStarter / (1 + hydration / 100); // rough approximation
        const waterFromStarter = keepStarter - flourFromStarter;
        const totalFlour = flourFromStarter + addFlour;
        const desiredWaterTotal = totalFlour * (hydration / 100);
        addWater = Math.max(0, desiredWaterTotal - waterFromStarter);

        const totalWeight = keepStarter + addFlour + addWater;
        const finalHydration =
          ((waterFromStarter + addWater) / totalFlour) * 100;

        document.getElementById("sd-keep-starter").textContent =
          roundTo(keepStarter, 1) + " g";
        document.getElementById("sd-add-flour").textContent =
          roundTo(addFlour, 1) + " g";
        document.getElementById("sd-add-water").textContent =
          roundTo(addWater, 1) + " g";
        document.getElementById("sd-total-weight").textContent =
          roundTo(totalWeight, 1) + " g";
        document.getElementById("sd-result-hydration").textContent = roundTo(
          finalHydration,
          1
        );
        document.getElementById("sd-result-ratio").textContent =
          S + " : " + F + " : " + W;

        document.getElementById("sd-results").classList.remove("hidden");
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
