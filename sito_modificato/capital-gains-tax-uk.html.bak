    <script>
      // Core CGT calculator logic
      function getConfigForYear(year) {
        if (year === "2023-24") {
          return {
            allowanceDefault: 6000,
            basicRateLimit: 37700,
          };
        }
        // 2024-25 default (update when thresholds change)
        return {
          allowanceDefault: 3000,
          basicRateLimit: 37700,
        };
      }

      function getRates(assetType) {
        // 2024/25 CGT rates – adjust if HMRC changes
        if (assetType === "residential") {
          return { basic: 0.18, higher: 0.24 };
        }
        // Other property uses standard rates
        return { basic: 0.1, higher: 0.2 };
      }

      function formatGBP(value) {
        if (!isFinite(value)) return "£0";
        return (
          "£" +
          value.toLocaleString("en-GB", {
            minimumFractionDigits: 2,
            maximumFractionDigits: 2,
          })
        );
      }

      function calculateCGT() {
        const taxYear = document.getElementById("taxYear").value;
        const assetType = document.getElementById("assetType").value;
        const saleProceeds =
          parseFloat(document.getElementById("saleProceeds").value) || 0;
        const allowableCosts =
          parseFloat(document.getElementById("allowableCosts").value) || 0;
        const otherGains =
          parseFloat(document.getElementById("otherGains").value) || 0;
        const taxableIncome =
          parseFloat(document.getElementById("taxableIncome").value) || 0;
        const useDefaultAllowance =
          document.getElementById("useDefaultAllowance").value === "yes";
        const joint = document.getElementById("jointCalculation").checked;

        const cfg = getConfigForYear(taxYear);
        const allowance = useDefaultAllowance
          ? cfg.allowanceDefault
          : parseFloat(document.getElementById("annualAllowance").value) || 0;
        const rates = getRates(assetType);

        // Step 1: gain on this disposal
        let gain = saleProceeds - allowableCosts;
        if (gain < 0) gain = 0;

        // Step 2: total gains
        let totalGains = gain + Math.max(0, otherGains);

        // Step 3: allowance (single or joint)
        const totalAllowance = joint ? allowance * 2 : allowance;
        const allowanceUsed = Math.min(totalAllowance, totalGains);
        const taxableGain = Math.max(0, totalGains - totalAllowance);

        // Step 4: split between basic and higher rate
        const basicLimit = cfg.basicRateLimit * (joint ? 2 : 1);
        const income = Math.max(0, taxableIncome * (joint ? 2 : 1));
        const unusedBasicBand = Math.max(0, basicLimit - income);

        const basicRateGain = Math.min(taxableGain, unusedBasicBand);
        const higherRateGain = Math.max(0, taxableGain - basicRateGain);

        // Step 5: tax
        const basicTax = basicRateGain * rates.basic;
        const higherTax = higherRateGain * rates.higher;
        const totalTax = basicTax + higherTax;

        // Update UI
        document.getElementById("gainDisplay").textContent = formatGBP(gain);
        document.getElementById("totalGainsDisplay").textContent =
          formatGBP(totalGains);
        document.getElementById("allowanceUsedDisplay").textContent =
          formatGBP(allowanceUsed);
        document.getElementById("taxableGainDisplay").textContent =
          formatGBP(taxableGain);
        document.getElementById("unusedBasicBandDisplay").textContent =
          formatGBP(unusedBasicBand);
        document.getElementById("basicRateGainDisplay").textContent =
          formatGBP(basicRateGain);
        document.getElementById("higherRateGainDisplay").textContent =
          formatGBP(higherRateGain);
        document.getElementById("basicTaxDisplay").textContent =
          formatGBP(basicTax);
        document.getElementById("higherTaxDisplay").textContent =
          formatGBP(higherTax);
        document.getElementById("totalTaxDisplay").textContent =
          formatGBP(totalTax);
      }

      document.addEventListener("DOMContentLoaded", function () {
        const taxYearSelect = document.getElementById("taxYear");
        const useDefaultAllowanceSelect = document.getElementById(
          "useDefaultAllowance"
        );
        const customAllowanceWrapper = document.getElementById(
          "customAllowanceWrapper"
        );
        const taxYearNote = document.getElementById("taxYearNote");
        const calculateBtn = document.getElementById("calculateBtn");

        function updateYearNote() {
          const year = taxYearSelect.value;
          const cfg = getConfigForYear(year);
          taxYearNote.textContent = `Standard CGT allowance for ${year} is £${cfg.allowanceDefault.toLocaleString(
            "en-GB"
          )}. Basic-rate band limit used: £${cfg.basicRateLimit.toLocaleString(
            "en-GB"
          )} per person.`;
          if (useDefaultAllowanceSelect.value === "yes") {
            document.getElementById("annualAllowance").value =
              cfg.allowanceDefault;
          }
        }

        taxYearSelect.addEventListener("change", function () {
          const cfg = getConfigForYear(this.value);
          if (useDefaultAllowanceSelect.value === "yes") {
            document.getElementById("annualAllowance").value =
              cfg.allowanceDefault;
          }
          updateYearNote();
          calculateCGT();
        });

        useDefaultAllowanceSelect.addEventListener("change", function () {
          if (this.value === "yes") {
            customAllowanceWrapper.classList.add("hidden");
            const cfg = getConfigForYear(taxYearSelect.value);
            document.getElementById("annualAllowance").value =
              cfg.allowanceDefault;
          } else {
            customAllowanceWrapper.classList.remove("hidden");
          }
          calculateCGT();
        });

        calculateBtn.addEventListener("click", function (e) {
          e.preventDefault();
          calculateCGT();
        });

        // Auto-calc on input change for better UX
        [
          "saleProceeds",
          "allowableCosts",
          "otherGains",
          "taxableIncome",
          "assetType",
          "jointCalculation",
          "annualAllowance",
        ].forEach((id) => {
          const el = document.getElementById(id);
          if (el) {
            el.addEventListener("input", calculateCGT);
            el.addEventListener("change", calculateCGT);
          }
        });

        updateYearNote();
        calculateCGT();

        document
          .querySelectorAll("#faq-container .faq-question")
          .forEach((btn) => {
            btn.addEventListener("click", () => {
              const answer = btn.parentElement.querySelector(".faq-answer");
              const isHidden = answer.classList.contains("hidden");
              document
                .querySelectorAll("#faq-container .faq-answer")
                .forEach((a) => a.classList.add("hidden"));
              document
                .querySelectorAll(
                  "#faq-container .faq-question span:last-child"
                )
                .forEach((icon) => (icon.textContent = "+"));
              if (isHidden) {
                answer.classList.remove("hidden");
                btn.querySelector("span:last-child").textContent = "−";
              }
            });
          });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
