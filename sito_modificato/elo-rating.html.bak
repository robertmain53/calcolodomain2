    <script>
      // Utility: clamp and parse number
      function parseNum(value, fallback) {
        const n = parseFloat(value);
        return Number.isFinite(n) ? n : fallback;
      }

      function computeExpected(rA, rB) {
        const exponent = (rB - rA) / 400;
        return 1 / (1 + Math.pow(10, exponent));
      }

      function updateElo() {
        const rA = parseNum(document.getElementById("ratingA").value, 1500);
        const rB = parseNum(document.getElementById("ratingB").value, 1500);
        const kA = Math.max(
          1,
          parseNum(document.getElementById("kFactorA").value, 20)
        );
        const kB = Math.max(
          1,
          parseNum(document.getElementById("kFactorB").value, 20)
        );

        const mode = document.getElementById("scoreMode").value;
        let sA, sB;

        if (mode === "result") {
          const activeBtn = document.querySelector(".result-btn.bg-blue-600");
          const result = activeBtn ? activeBtn.dataset.result : "winA";
          if (result === "winA") {
            sA = 1;
            sB = 0;
          } else if (result === "winB") {
            sA = 0;
            sB = 1;
          } else {
            sA = 0.5;
            sB = 0.5;
          }
        } else {
          sA = parseNum(document.getElementById("scoreA").value, 1);
          sB = parseNum(document.getElementById("scoreB").value, 0);
        }

        const eA = computeExpected(rA, rB);
        const eB = 1 - eA;

        const dA = kA * (sA - eA);
        const dB = kB * (sB - eB);

        const newRA = rA + dA;
        const newRB = rB + dB;

        document.getElementById("expectedA").textContent = eA.toFixed(3);
        document.getElementById("expectedB").textContent = eB.toFixed(3);
        document.getElementById("deltaA").textContent =
          (dA >= 0 ? "+" : "") + dA.toFixed(1);
        document.getElementById("deltaB").textContent =
          (dB >= 0 ? "+" : "") + dB.toFixed(1);
        document.getElementById("newRatingA").textContent = newRA.toFixed(1);
        document.getElementById("newRatingB").textContent = newRB.toFixed(1);
      }

      document.addEventListener("DOMContentLoaded", function () {
        // Preset buttons
        document.querySelectorAll(".preset-btn").forEach((btn) => {
          btn.addEventListener("click", () => {
            const k = parseNum(btn.dataset.k, 20);
            document.getElementById("kFactorA").value = k;
            document.getElementById("kFactorB").value = k;
            updateElo();
          });
        });

        // Result buttons
        document.querySelectorAll(".result-btn").forEach((btn) => {
          btn.addEventListener("click", () => {
            document.querySelectorAll(".result-btn").forEach((b) => {
              b.classList.remove("bg-blue-600", "text-white");
              b.classList.add("bg-white", "text-gray-700");
            });
            btn.classList.remove("bg-white", "text-gray-700");
            btn.classList.add("bg-blue-600", "text-white");
            updateElo();
          });
        });

        // Score mode switch
        document.getElementById("scoreMode").addEventListener("change", (e) => {
          const mode = e.target.value;
          const resultRow = document.getElementById("resultModeRow");
          const customRow = document.getElementById("customModeRow");
          if (mode === "result") {
            resultRow.classList.remove("hidden");
            customRow.classList.add("hidden");
          } else {
            resultRow.classList.add("hidden");
            customRow.classList.remove("hidden");
          }
          updateElo();
        });

        // Inputs trigger live update
        [
          "ratingA",
          "ratingB",
          "kFactorA",
          "kFactorB",
          "scoreA",
          "scoreB",
        ].forEach((id) => {
          const el = document.getElementById(id);
          if (el) {
            el.addEventListener("input", () => {
              updateElo();
            });
          }
        });

        document
          .getElementById("computeElo")
          .addEventListener("click", updateElo);

        document.getElementById("resetElo").addEventListener("click", () => {
          document.getElementById("ratingA").value = 1500;
          document.getElementById("ratingB").value = 1500;
          document.getElementById("kFactorA").value = 20;
          document.getElementById("kFactorB").value = 20;
          document.getElementById("scoreMode").value = "result";
          document.getElementById("resultModeRow").classList.remove("hidden");
          document.getElementById("customModeRow").classList.add("hidden");
          document.querySelectorAll(".result-btn").forEach((b, idx) => {
            if (idx === 0) {
              b.classList.add("bg-blue-600", "text-white");
              b.classList.remove("bg-white", "text-gray-700");
            } else {
              b.classList.remove("bg-blue-600", "text-white");
              b.classList.add("bg-white", "text-gray-700");
            }
          });
          updateElo();
        });

        // Batch processing
        document.getElementById("runBatch").addEventListener("click", () => {
          const startRating = parseNum(
            document.getElementById("batchStartRating").value,
            1500
          );
          const k = Math.max(
            1,
            parseNum(document.getElementById("batchK").value, 20)
          );
          const lines = document
            .getElementById("batchLines")
            .value.split(/\r?\n/)
            .map((l) => l.trim())
            .filter((l) => l.length > 0);

          const errorEl = document.getElementById("batchError");
          const resultBox = document.getElementById("batchResult");
          errorEl.classList.add("hidden");
          resultBox.classList.add("hidden");

          if (lines.length === 0) {
            errorEl.textContent = "Please enter at least one game line.";
            errorEl.classList.remove("hidden");
            return;
          }

          let rating = startRating;
          let totalDelta = 0;
          let games = 0;

          for (let i = 0; i < lines.length; i++) {
            const parts = lines[i].split(",");
            if (parts.length !== 2) {
              errorEl.textContent = `Invalid format on line ${i + 1}: "${
                lines[i]
              }". Use "opponentRating,result".`;
              errorEl.classList.remove("hidden");
              return;
            }
            const opp = parseFloat(parts[0]);
            const s = parseFloat(parts[1]);
            if (!Number.isFinite(opp) || !Number.isFinite(s)) {
              errorEl.textContent = `Invalid numbers on line ${i + 1}: "${
                lines[i]
              }".`;
              errorEl.classList.remove("hidden");
              return;
            }
            const e = computeExpected(rating, opp);
            const d = k * (s - e);
            rating += d;
            totalDelta += d;
            games++;
          }

          document.getElementById("batchGames").textContent = games.toString();
          document.getElementById("batchFinalRating").textContent =
            rating.toFixed(1);
          document.getElementById("batchTotalDelta").textContent =
            (totalDelta >= 0 ? "+" : "") + totalDelta.toFixed(1);
          resultBox.classList.remove("hidden");
        });

        document.getElementById("clearBatch").addEventListener("click", () => {
          document.getElementById("batchLines").value = "";
          document.getElementById("batchResult").classList.add("hidden");
          document.getElementById("batchError").classList.add("hidden");
        });

        // Initial compute
        updateElo();
      });
    </script>

