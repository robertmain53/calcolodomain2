    <script>
      // --- Tax configuration (approximate 2025 values; update when official tables change) ---

      const FEDERAL_BASIC_PERSONAL = 15805; // illustrative
      const NU_BASIC_PERSONAL = 17000; // illustrative Nunavut basic personal amount

      // Federal brackets: [upperLimit, rate]
      const FEDERAL_BRACKETS = [
        { limit: 55867, rate: 0.15 },
        { limit: 111733, rate: 0.205 },
        { limit: 173205, rate: 0.26 },
        { limit: 246752, rate: 0.29 },
        { limit: Infinity, rate: 0.33 },
      ];

      // Nunavut territorial brackets (illustrative; align with latest NU rate sheet)
      const NU_BRACKETS = [
        { limit: 50000, rate: 0.04 },
        { limit: 100000, rate: 0.07 },
        { limit: 150000, rate: 0.09 },
        { limit: Infinity, rate: 0.115 },
      ];

      // CPP & EI (approximate 2025)
      const CPP_RATE = 0.0595;
      const CPP_BASIC_EXEMPTION = 3500;
      const CPP_MAX_PENSIONABLE = 73500;

      const EI_RATE = 0.0166;
      const EI_MAX_INSURABLE = 64000;

      function applyBrackets(taxable, brackets, basicPersonal) {
        const effectiveTaxable = Math.max(0, taxable - basicPersonal);
        let remaining = effectiveTaxable;
        let lastLimit = 0;
        let tax = 0;
        const details = [];

        for (const bracket of brackets) {
          if (remaining <= 0) break;
          const span = Math.min(bracket.limit - lastLimit, remaining);
          if (span > 0) {
            const taxForBracket = span * bracket.rate;
            tax += taxForBracket;
            details.push({
              from: lastLimit,
              to: bracket.limit,
              amount: span,
              rate: bracket.rate,
              tax: taxForBracket,
            });
            remaining -= span;
          }
          lastLimit = bracket.limit;
        }

        const marginalRate = details.length
          ? details[details.length - 1].rate
          : 0;
        return { tax, details, marginalRate };
      }

      function formatCurrency(value) {
        return value.toLocaleString("en-CA", {
          style: "currency",
          currency: "CAD",
          maximumFractionDigits: 0,
        });
      }

      function formatCurrency2(value) {
        return value.toLocaleString("en-CA", {
          style: "currency",
          currency: "CAD",
          maximumFractionDigits: 2,
        });
      }

      function formatPercent(value) {
        return (value * 100).toFixed(1) + "%";
      }

      function getPayPeriods(freq) {
        switch (freq) {
          case "monthly":
            return 12;
          case "biweekly":
            return 26;
          case "weekly":
            return 52;
          default:
            return 1;
        }
      }

      function updateIncomeModeUI() {
        const mode = document.getElementById("income-mode").value;
        const annualWrapper = document.getElementById("annual-income-wrapper");
        const hourlyWrapper = document.getElementById("hourly-rate-wrapper");
        const hoursWrapper = document.getElementById("hours-per-week-wrapper");

        if (mode === "annual") {
          annualWrapper.classList.remove("hidden");
          hourlyWrapper.classList.add("hidden");
          hoursWrapper.classList.add("hidden");
        } else {
          annualWrapper.classList.add("hidden");
          hourlyWrapper.classList.remove("hidden");
          hoursWrapper.classList.remove("hidden");
        }
      }

      document
        .getElementById("income-mode")
        .addEventListener("change", updateIncomeModeUI);

      document.getElementById("calculate-btn").addEventListener("click", () => {
        const incomeType = document.getElementById("income-type").value;
        const mode = document.getElementById("income-mode").value;
        const rrsp = Number(document.getElementById("rrsp").value) || 0;
        const payFreq = document.getElementById("pay-frequency").value;

        let grossAnnual = 0;
        if (mode === "annual") {
          grossAnnual =
            Number(document.getElementById("annual-income").value) || 0;
        } else {
          const hourly =
            Number(document.getElementById("hourly-rate").value) || 0;
          const hours =
            Number(document.getElementById("hours-per-week").value) || 0;
          grossAnnual = hourly * hours * 52;
        }

        if (grossAnnual < 0) grossAnnual = 0;

        const taxableIncome = Math.max(0, grossAnnual - rrsp);

        // Federal & Nunavut tax
        const federalResult = applyBrackets(
          taxableIncome,
          FEDERAL_BRACKETS,
          FEDERAL_BASIC_PERSONAL
        );
        const nuResult = applyBrackets(
          taxableIncome,
          NU_BRACKETS,
          NU_BASIC_PERSONAL
        );

        let cpp = 0;
        let ei = 0;
        if (incomeType === "employment") {
          const cppBase = Math.max(
            0,
            Math.min(grossAnnual, CPP_MAX_PENSIONABLE) - CPP_BASIC_EXEMPTION
          );
          cpp = cppBase * CPP_RATE;

          const eiBase = Math.min(grossAnnual, EI_MAX_INSURABLE);
          ei = eiBase * EI_RATE;
        }

        const totalTax = federalResult.tax + nuResult.tax + cpp + ei;
        const netIncome = grossAnnual - totalTax;

        const periods = getPayPeriods(payFreq);
        const periodLabel =
          payFreq === "annual"
            ? "Per year only"
            : `Per ${
                payFreq === "monthly"
                  ? "month"
                  : payFreq === "biweekly"
                  ? "2 weeks"
                  : "week"
              } (${periods} per year)`;

        // Update summary cards
        document.getElementById("results-section").classList.remove("hidden");
        document.getElementById("net-income-annual").textContent =
          formatCurrency(netIncome);
        document.getElementById("net-income-period").textContent =
          periods === 1
            ? ""
            : `${formatCurrency2(netIncome / periods)} ${
                payFreq === "monthly"
                  ? "per month"
                  : payFreq === "biweekly"
                  ? "every 2 weeks"
                  : "per week"
              }`;

        document.getElementById("total-tax-annual").textContent =
          formatCurrency(totalTax);
        document.getElementById("federal-tax-annual").textContent =
          formatCurrency(federalResult.tax);
        document.getElementById("nu-tax-annual").textContent = formatCurrency(
          nuResult.tax
        );
        document.getElementById("cpp-annual").textContent = formatCurrency(cpp);
        document.getElementById("ei-annual").textContent = formatCurrency(ei);

        // Table
        document.getElementById("period-col-header").textContent =
          periods === 1 ? "Per period" : periodLabel;

        const per = (v) => v / periods;

        document.getElementById("gross-annual-row").textContent =
          formatCurrency(grossAnnual);
        document.getElementById("gross-period-row").textContent =
          formatCurrency2(per(grossAnnual));

        document.getElementById("taxable-annual-row").textContent =
          formatCurrency(taxableIncome);
        document.getElementById("taxable-period-row").textContent =
          formatCurrency2(per(taxableIncome));

        document.getElementById("federal-annual-row").textContent =
          formatCurrency(federalResult.tax);
        document.getElementById("federal-period-row").textContent =
          formatCurrency2(per(federalResult.tax));

        document.getElementById("nu-annual-row").textContent = formatCurrency(
          nuResult.tax
        );
        document.getElementById("nu-period-row").textContent = formatCurrency2(
          per(nuResult.tax)
        );

        document.getElementById("cpp-annual-row").textContent =
          formatCurrency(cpp);
        document.getElementById("cpp-period-row").textContent = formatCurrency2(
          per(cpp)
        );

        document.getElementById("ei-annual-row").textContent =
          formatCurrency(ei);
        document.getElementById("ei-period-row").textContent = formatCurrency2(
          per(ei)
        );

        document.getElementById("net-annual-row").textContent =
          formatCurrency(netIncome);
        document.getElementById("net-period-row").textContent = formatCurrency2(
          per(netIncome)
        );

        const avgRate = grossAnnual > 0 ? totalTax / grossAnnual : 0;
        const marginalCombined =
          federalResult.marginalRate + nuResult.marginalRate;
        document.getElementById("avg-tax-rate-row").textContent =
          formatPercent(avgRate);
        document.getElementById("marginal-tax-rate-row").textContent =
          "Marginal: " + formatPercent(marginalCombined);

        // Bracket breakdown
        const breakdownEl = document.getElementById("bracket-breakdown");
        breakdownEl.innerHTML = "";

        function addSection(title, result, basicPersonal) {
          const section = document.createElement("div");
          section.className = "mb-3";
          const heading = document.createElement("div");
          heading.className = "font-semibold mb-1";
          heading.textContent = title;
          section.appendChild(heading);

          const ul = document.createElement("ul");
          ul.className = "list-disc ml-4 space-y-1";

          const liBasic = document.createElement("li");
          liBasic.textContent = `Basic personal amount applied: ${formatCurrency(
            basicPersonal
          )}`;
          ul.appendChild(liBasic);

          if (!result.details.length) {
            const li = document.createElement("li");
            li.textContent = "No tax payable after basic personal amount.";
            ul.appendChild(li);
          } else {
            result.details.forEach((d) => {
              const li = document.createElement("li");
              li.textContent = `${formatCurrency2(d.amount)} taxed at ${(
                d.rate * 100
              ).toFixed(1)}% = ${formatCurrency2(d.tax)}`;
              ul.appendChild(li);
            });
          }

          const totalLi = document.createElement("li");
          totalLi.className = "font-semibold";
          totalLi.textContent = `Total ${title.toLowerCase()} = ${formatCurrency2(
            result.tax
          )}`;
          ul.appendChild(totalLi);

          section.appendChild(ul);
          breakdownEl.appendChild(section);
        }

        addSection("Federal tax", federalResult, FEDERAL_BASIC_PERSONAL);
        addSection("Nunavut territorial tax", nuResult, NU_BASIC_PERSONAL);
      });

      // Initialize UI
      updateIncomeModeUI();
    </script>

