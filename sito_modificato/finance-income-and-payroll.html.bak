    <script>
      // Income & Cost of Living Explorer logic
      function formatCurrency(value) {
        if (!isFinite(value)) return "–";
        return (
          "$" + value.toLocaleString(undefined, { maximumFractionDigits: 0 })
        );
      }

      function calculateIncomeExplorer() {
        const annualIncome = parseFloat(
          document.getElementById("annual-income").value
        );
        const payFreq = parseFloat(
          document.getElementById("pay-frequency").value
        );
        const taxRate = parseFloat(document.getElementById("tax-rate").value);
        const monthlyCol = parseFloat(
          document.getElementById("monthly-col").value
        );

        const grossValid = annualIncome > 0 && isFinite(annualIncome);
        const freqValid = payFreq > 0 && isFinite(payFreq);
        const taxValid = taxRate >= 0 && taxRate <= 60 && isFinite(taxRate);

        if (!grossValid || !freqValid || !taxValid) {
          document.getElementById("res-gross-check").textContent = "–";
          document.getElementById("res-net-check").textContent = "–";
          document.getElementById("res-net-month").textContent = "–";
          document.getElementById("res-net-year").textContent = "–";
          document.getElementById("res-col-balance").textContent =
            "Please enter valid income, frequency, and tax rate.";
          document.getElementById("res-col-balance").className =
            "font-semibold text-red-600";
          document.getElementById("res-col-note").textContent = "";
          return;
        }

        const grossPerCheck = annualIncome / payFreq;
        const netAnnual = annualIncome * (1 - taxRate / 100);
        const netPerCheck = netAnnual / payFreq;
        const netPerMonth = netAnnual / 12;

        document.getElementById("res-gross-check").textContent =
          formatCurrency(grossPerCheck);
        document.getElementById("res-net-check").textContent =
          formatCurrency(netPerCheck);
        document.getElementById("res-net-month").textContent =
          formatCurrency(netPerMonth);
        document.getElementById("res-net-year").textContent =
          formatCurrency(netAnnual);

        if (!monthlyCol || !isFinite(monthlyCol) || monthlyCol <= 0) {
          document.getElementById("res-col-balance").textContent =
            "Add an estimated monthly cost of living to see your budget balance.";
          document.getElementById("res-col-balance").className =
            "font-semibold text-gray-700";
          document.getElementById("res-col-note").textContent = "";
          return;
        }

        const balance = netPerMonth - monthlyCol;
        const pct = (balance / monthlyCol) * 100;

        const balanceEl = document.getElementById("res-col-balance");
        const noteEl = document.getElementById("res-col-note");

        balanceEl.textContent =
          (balance >= 0 ? "Surplus: " : "Shortfall: ") +
          formatCurrency(Math.abs(balance)) +
          " per month";
        if (balance > 0) {
          balanceEl.className = "font-semibold text-green-600";
          noteEl.textContent =
            "Your estimated net income covers your cost of living with room for savings, debt payoff, or discretionary spending.";
        } else if (balance < 0) {
          balanceEl.className = "font-semibold text-red-600";
          noteEl.textContent =
            "Your estimated net income does not fully cover your cost of living. Consider adjusting expenses, increasing income, or re-evaluating location choices.";
        } else {
          balanceEl.className = "font-semibold text-gray-700";
          noteEl.textContent =
            "Your estimated net income roughly matches your cost of living. There may be little room for savings or unexpected expenses.";
        }
      }

      document.addEventListener("DOMContentLoaded", function () {
        const btn = document.getElementById("income-explorer-btn");
        if (btn) {
          btn.addEventListener("click", calculateIncomeExplorer);
          // Run once with defaults
          calculateIncomeExplorer();
        }
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
