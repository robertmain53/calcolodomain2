    <script>
      // Utility helpers
      function roundTo(value, decimals) {
        const f = Math.pow(10, decimals);
        return Math.round(value * f) / f;
      }
      function suggestStandardVA(va) {
        const standards = [
          5, 10, 15, 20, 25, 30, 40, 50, 75, 100, 150, 200, 250, 300,
        ];
        for (let s of standards) if (s >= va) return s;
        return Math.ceil(va / 50) * 50;
      }
      function suggestStandardCap(uF) {
        const e6 = [
          10, 15, 22, 33, 47, 68, 100, 150, 220, 330, 470, 680, 1000, 1500,
          2200, 3300, 4700, 6800, 10000,
        ];
        for (let v of e6) if (v >= uF) return v;
        return Math.ceil(uF / 1000) * 1000;
      }
      function suggestVoltageRating(v) {
        const ratings = [10, 16, 25, 35, 50, 63, 80, 100];
        for (let r of ratings) if (r >= v) return r;
        return Math.ceil(v / 50) * 50;
      }

      document.getElementById("resetBtn").addEventListener("click", () => {
        document.getElementById("psu-form").reset();
        document.getElementById("errorMsg").textContent = "";
        // trigger default mains freq select reset
        document.getElementById("mainsFreq").value = "50";
        document.getElementById("regType").value = "linear";
        // clear results
        const ids = [
          "xfmrVrated",
          "xfmrVmin",
          "xfmrIsec",
          "xfmrVA",
          "xfmrSuggestion",
          "dioIavg",
          "dioIpeak",
          "dioPIV",
          "dioPdiss",
          "dioSuggestion",
          "capC",
          "capV",
          "capFreq",
          "capRipple",
          "capSuggestion",
          "vCapMin",
          "regPdiss",
          "pin",
          "pout",
          "eff",
        ];
        ids.forEach((id) => (document.getElementById(id).textContent = "–"));
      });

      document.getElementById("calcBtn").addEventListener("click", () => {
        const vout = parseFloat(document.getElementById("vout").value);
        const iout = parseFloat(document.getElementById("iout").value);
        const margin =
          parseFloat(document.getElementById("safetyMargin").value) / 100;
        const mainsV = parseFloat(
          document.getElementById("mainsVoltage").value
        );
        const mainsF = parseFloat(document.getElementById("mainsFreq").value);
        const topology = document.getElementById("topology").value;
        const ripple = parseFloat(document.getElementById("ripple").value);
        const headroom = parseFloat(document.getElementById("headroom").value);
        const mainsTol =
          parseFloat(document.getElementById("mainsTol").value) / 100;
        const diodeDrop = parseFloat(
          document.getElementById("diodeDrop").value
        );
        const xfmrReg =
          parseFloat(document.getElementById("xfmrReg").value) / 100;
        const regType = document.getElementById("regType").value;

        const errEl = document.getElementById("errorMsg");
        errEl.textContent = "";

        if (
          [vout, iout, mainsV, mainsF, ripple, headroom].some(
            (v) => !isFinite(v) || v <= 0
          )
        ) {
          errEl.textContent =
            "Please enter positive numeric values for all main inputs.";
          return;
        }

        const Iload = iout * (1 + (isFinite(margin) ? margin : 0));
        const fripple = 2 * mainsF;

        // Required minimum capacitor voltage
        const vHead = regType === "linear" ? headroom : 0;
        const vCminReq = vout + vHead;

        // Back-calc transformer secondary RMS at lowest mains
        let Vsec_rms_min;
        if (topology === "bridge") {
          Vsec_rms_min =
            (vCminReq + ripple / 2 + 2 * diodeDrop) /
            (Math.SQRT2 * (1 - mainsTol));
        } else {
          // center tap
          Vsec_rms_min =
            (2 * (vCminReq + ripple / 2 + diodeDrop)) /
            (Math.SQRT2 * (1 - mainsTol));
        }

        // Rated secondary at nominal mains including regulation
        const Vsec_rated = Vsec_rms_min * (1 + xfmrReg);

        // Secondary current and VA
        // Approx: Isec ≈ 1.8 * Iload for bridge, 1.2 for center-tap (rough heuristic)
        const Isec = topology === "bridge" ? Iload * 1.8 : Iload * 1.2;
        const VA = Vsec_rated * Isec;
        const VAstd = suggestStandardVA(VA);

        // Peak secondary voltage at nominal mains
        const Vsec_pk_nom = Vsec_rated * Math.SQRT2;

        // Capacitor voltage extremes
        let Vc_pk, Vc_min;
        if (topology === "bridge") {
          Vc_pk = Vsec_pk_nom - 2 * diodeDrop;
        } else {
          Vc_pk = Vsec_pk_nom / 2 - diodeDrop;
        }
        Vc_min = Vc_pk - ripple;

        // Capacitor sizing
        const C = Iload / (ripple * fripple); // Farads
        const C_uF = C * 1e6;
        const Cstd = suggestStandardCap(C_uF);
        const VcapRating = suggestVoltageRating(Vc_pk * 1.3); // 30% margin

        // Rectifier currents and PIV
        const dioIavg = Iload; // conservative
        const dioIpeak = Iload * 5; // rough pulse factor
        let dioPIV;
        if (topology === "bridge") {
          dioPIV = Vsec_pk_nom * 2; // conservative
        } else {
          dioPIV = Vsec_pk_nom; // each diode sees full half winding
        }
        const dioPdiss = (topology === "bridge" ? 2 : 1) * diodeDrop * Iload;

        // Regulator and efficiency
        const Vc_avg = (Vc_pk + Vc_min) / 2;
        const Pout = vout * iout;
        let Preg = 0;
        if (regType === "linear") {
          Preg = Math.max(Vc_avg - vout, 0) * iout;
        }
        const Psec = Vsec_rated * Isec; // approximate transformer secondary power
        const Pin = Psec + dioPdiss + Preg;
        const eff = Pin > 0 ? (Pout / Pin) * 100 : 0;

        // Populate results
        document.getElementById("xfmrVrated").textContent = isFinite(Vsec_rated)
          ? `${roundTo(Vsec_rated, 2)} Vrms`
          : "–";
        document.getElementById("xfmrVmin").textContent = isFinite(Vsec_rms_min)
          ? `${roundTo(Vsec_rms_min, 2)} Vrms`
          : "–";
        document.getElementById("xfmrIsec").textContent = isFinite(Isec)
          ? `${roundTo(Isec, 2)} A`
          : "–";
        document.getElementById("xfmrVA").textContent = isFinite(VA)
          ? `${roundTo(VA, 1)} VA`
          : "–";
        document.getElementById("xfmrSuggestion").textContent = isFinite(VAstd)
          ? `${VAstd} VA transformer (min)`
          : "–";

        document.getElementById("dioIavg").textContent = isFinite(dioIavg)
          ? `${roundTo(dioIavg, 2)} A`
          : "–";
        document.getElementById("dioIpeak").textContent = isFinite(dioIpeak)
          ? `${roundTo(dioIpeak, 1)} A (approx)`
          : "–";
        document.getElementById("dioPIV").textContent = isFinite(dioPIV)
          ? `${roundTo(dioPIV, 0)} V`
          : "–";
        document.getElementById("dioPdiss").textContent = isFinite(dioPdiss)
          ? `${roundTo(dioPdiss, 2)} W`
          : "–";
        document.getElementById("dioSuggestion").textContent =
          isFinite(dioIavg) && isFinite(dioPIV)
            ? `Use diodes ≥ ${roundTo(dioIavg * 2, 1)} A, ≥ ${roundTo(
                dioPIV * 2,
                0
              )} V`
            : "–";

        document.getElementById("capC").textContent = isFinite(C_uF)
          ? `${roundTo(C_uF, 0)} µF (min)`
          : "–";
        document.getElementById("capV").textContent = isFinite(VcapRating)
          ? `${VcapRating} V`
          : "–";
        document.getElementById("capFreq").textContent = `${fripple} Hz`;
        document.getElementById("capRipple").textContent = isFinite(ripple)
          ? `${roundTo(ripple, 2)} Vpp`
          : "–";
        document.getElementById("capSuggestion").textContent =
          isFinite(Cstd) && isFinite(VcapRating)
            ? `${Cstd} µF, ${VcapRating} V electrolytic`
            : "–";

        document.getElementById("vCapMin").textContent = isFinite(Vc_min)
          ? `${roundTo(Vc_min, 2)} V`
          : "–";
        document.getElementById("regPdiss").textContent = isFinite(Preg)
          ? `${roundTo(Preg, 2)} W`
          : "–";
        document.getElementById("pin").textContent = isFinite(Pin)
          ? `${roundTo(Pin, 2)} W`
          : "–";
        document.getElementById("pout").textContent = isFinite(Pout)
          ? `${roundTo(Pout, 2)} W`
          : "–";
        document.getElementById("eff").textContent = isFinite(eff)
          ? `${roundTo(eff, 1)} %`
          : "–";

        if (Vc_min < vCminReq) {
          errEl.textContent =
            "Warning: Calculated minimum capacitor voltage may be too low to maintain regulation. Increase transformer voltage, reduce ripple, or reduce headroom.";
        }
      });
    </script>

