    <script>
      // Utility: format currency
      function fmtCurrency(value) {
        if (!isFinite(value)) return "$0";
        return value.toLocaleString("en-US", {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 0,
        });
      }

      function fmtCurrency2(value) {
        if (!isFinite(value)) return "$0";
        return value.toLocaleString("en-US", {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 2,
        });
      }

      function fmtPercent(value) {
        if (!isFinite(value)) return "–";
        return value.toFixed(1) + "%";
      }

      function calculateAffordability() {
        const income =
          parseFloat(document.getElementById("grossIncome").value) || 0;
        const otherDebts =
          parseFloat(document.getElementById("otherDebts").value) || 0;
        const frontEndDTI =
          (parseFloat(document.getElementById("frontEndDTI").value) || 0) / 100;
        const backEndDTI =
          (parseFloat(document.getElementById("backEndDTI").value) || 0) / 100;
        const rateAnnual =
          parseFloat(document.getElementById("interestRate").value) || 0;
        const termYears =
          parseInt(document.getElementById("loanTerm").value, 10) || 30;
        const taxRate =
          (parseFloat(document.getElementById("taxRate").value) || 0) / 100;
        const insRate =
          (parseFloat(document.getElementById("insuranceRate").value) || 0) /
          100;
        const hoaMonthly =
          parseFloat(document.getElementById("hoaMonthly").value) || 0;
        const downPct =
          (parseFloat(document.getElementById("downPaymentPercent").value) ||
            0) / 100;
        const downFixed =
          parseFloat(document.getElementById("downPaymentAmount").value) || 0;

        const maxHousingFront = income * frontEndDTI;
        const maxHousingBack = income * backEndDTI - otherDebts;
        const maxHousing = Math.max(
          0,
          Math.min(maxHousingFront, maxHousingBack)
        );

        const taxMonthlyPerDollar = taxRate / 12;
        const insMonthlyPerDollar = insRate / 12;

        const r = rateAnnual / 12 / 100;
        const n = termYears * 12;

        let maxHomePrice = 0;
        let maxLoanAmount = 0;
        let piPayment = 0;
        let taxPayment = 0;
        let insPayment = 0;
        let hoaPayment = hoaMonthly;

        if (income > 0 && maxHousing > 0 && r > 0 && n > 0) {
          const maxPI = maxHousing - hoaMonthly;
          if (maxPI > 0) {
            const factor = (r * Math.pow(1 + r, n)) / (Math.pow(1 + r, n) - 1);
            const loanFromPI = maxPI / factor;

            const nonPIperDollar = taxMonthlyPerDollar + insMonthlyPerDollar;
            let homePriceGuess = loanFromPI;
            if (nonPIperDollar > 0) {
              homePriceGuess =
                loanFromPI /
                (1 -
                  (nonPIperDollar * 12) /
                    (rateAnnual / 100 + nonPIperDollar * 12));
            }

            const maxIterations = 30;
            let low = loanFromPI * 0.5;
            let high = loanFromPI * 2.5;
            let bestPrice = homePriceGuess;

            for (let i = 0; i < maxIterations; i++) {
              const mid = (low + high) / 2;
              const down = downFixed > 0 ? downFixed : mid * downPct;
              const loan = Math.max(0, mid - down);
              const pi = loan * factor;
              const tax = mid * taxMonthlyPerDollar;
              const ins = mid * insMonthlyPerDollar;
              const totalHousing = pi + tax + ins + hoaMonthly;

              if (totalHousing > maxHousing) {
                high = mid;
              } else {
                low = mid;
                bestPrice = mid;
              }
            }

            maxHomePrice = Math.max(0, bestPrice);
            const downFinal =
              downFixed > 0 ? downFixed : maxHomePrice * downPct;
            maxLoanAmount = Math.max(0, maxHomePrice - downFinal);

            const factorFinal =
              (r * Math.pow(1 + r, n)) / (Math.pow(1 + r, n) - 1);
            piPayment = maxLoanAmount * factorFinal;
            taxPayment = maxHomePrice * taxMonthlyPerDollar;
            insPayment = maxHomePrice * insMonthlyPerDollar;
          }
        }

        const totalHousingPayment =
          piPayment + taxPayment + insPayment + hoaPayment;

        document.getElementById("maxHomePrice").textContent =
          fmtCurrency(maxHomePrice);
        document.getElementById("maxHousingPayment").textContent =
          fmtCurrency2(totalHousingPayment);
        document.getElementById("maxLoanAmount").textContent =
          fmtCurrency(maxLoanAmount);
        document.getElementById("piPayment").textContent =
          fmtCurrency2(piPayment);
        document.getElementById("taxPayment").textContent =
          fmtCurrency2(taxPayment);
        document.getElementById("insPayment").textContent =
          fmtCurrency2(insPayment);
        document.getElementById("hoaPayment").textContent =
          fmtCurrency2(hoaPayment);

        let downUsed = 0;
        if (maxHomePrice > 0) {
          downUsed = Math.max(0, maxHomePrice - maxLoanAmount);
        }
        const downPctUsed =
          maxHomePrice > 0 ? (downUsed / maxHomePrice) * 100 : 0;
        document.getElementById("downPaymentSummary").textContent =
          downUsed > 0
            ? `Down payment: ${fmtCurrency(downUsed)} (${downPctUsed.toFixed(
                1
              )}% of price)`
            : "Down payment: $0";

        const frontEnd =
          income > 0 ? (totalHousingPayment / income) * 100 : NaN;
        const backEnd =
          income > 0
            ? ((totalHousingPayment + otherDebts) / income) * 100
            : NaN;
        document.getElementById("frontEndResult").textContent =
          fmtPercent(frontEnd);
        document.getElementById("backEndResult").textContent =
          fmtPercent(backEnd);

        const dtiWarning = document.getElementById("dtiWarning");
        dtiWarning.classList.add("hidden");
        dtiWarning.textContent = "";

        if (isFinite(frontEnd) && isFinite(backEnd)) {
          if (
            frontEnd > frontEndDTI * 100 + 0.1 ||
            backEnd > backEndDTI * 100 + 0.1
          ) {
            dtiWarning.textContent =
              "Warning: At this price, your DTI ratios exceed the limits you set. Try a lower price, larger down payment, or lower debts.";
            dtiWarning.classList.remove("hidden");
          }
        }

        const note = document.getElementById("affordabilityNote");
        if (
          !income ||
          !maxHousing ||
          !isFinite(maxHomePrice) ||
          maxHomePrice <= 0
        ) {
          note.textContent =
            "Enter your income, debts, and loan details to see how much house you can afford.";
        } else {
          note.textContent =
            "Based on your inputs and DTI limits, this is an estimated maximum. Choosing a lower price can provide more budget flexibility.";
        }
      }

      document
        .getElementById("calcBtn")
        .addEventListener("click", calculateAffordability);

      document.getElementById("resetBtn").addEventListener("click", () => {
        document.getElementById("grossIncome").value = 8000;
        document.getElementById("otherDebts").value = 600;
        document.getElementById("frontEndDTI").value = 28;
        document.getElementById("backEndDTI").value = 36;
        document.getElementById("interestRate").value = 6.75;
        document.getElementById("loanTerm").value = 30;
        document.getElementById("taxRate").value = 1.2;
        document.getElementById("insuranceRate").value = 0.4;
        document.getElementById("hoaMonthly").value = 0;
        document.getElementById("downPaymentPercent").value = 20;
        document.getElementById("downPaymentAmount").value = 0;
        calculateAffordability();
      });

      [
        "grossIncome",
        "otherDebts",
        "frontEndDTI",
        "backEndDTI",
        "interestRate",
        "loanTerm",
        "taxRate",
        "insuranceRate",
        "hoaMonthly",
        "downPaymentPercent",
        "downPaymentAmount",
      ].forEach((id) => {
        const el = document.getElementById(id);
        if (el) {
          el.addEventListener("change", calculateAffordability);
          el.addEventListener("keyup", (e) => {
            if (["Enter", "NumpadEnter"].includes(e.key))
              calculateAffordability();
          });
        }
      });

      document.addEventListener("DOMContentLoaded", calculateAffordability);
    </script>

