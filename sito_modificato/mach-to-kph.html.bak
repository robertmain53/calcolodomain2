    <script>
      // Constants
      const GAMMA = 1.4; // specific heat ratio for air
      const R_AIR = 287.05; // J/(kg*K)

      const machInput = document.getElementById("machInput");
      const kphInput = document.getElementById("kphInput");
      const atmRadios = document.querySelectorAll('input[name="atmMode"]');
      const customTempRow = document.getElementById("customTempRow");
      const tempValueInput = document.getElementById("tempValue");
      const tempUnitSelect = document.getElementById("tempUnit");
      const speedOfSoundDisplay = document.getElementById(
        "speedOfSoundDisplay"
      );
      const resultSummary = document.getElementById("resultSummary");

      function toKelvin(value, unit) {
        const v = parseFloat(value);
        if (isNaN(v)) return NaN;
        if (unit === "C") return v + 273.15;
        if (unit === "F") return ((v - 32) * 5) / 9 + 273.15;
        return v; // already K
      }

      function getSpeedOfSound() {
        const mode = document.querySelector(
          'input[name="atmMode"]:checked'
        ).value;
        let T_K;
        if (mode === "sea") {
          T_K = 288.15; // 15°C
        } else if (mode === "cruise") {
          T_K = 216.65; // -56.5°C
        } else {
          T_K = toKelvin(tempValueInput.value, tempUnitSelect.value);
        }
        if (!isFinite(T_K) || T_K <= 0) return { a_ms: NaN, a_kph: NaN };
        const a_ms = Math.sqrt(GAMMA * R_AIR * T_K);
        const a_kph = a_ms * 3.6;
        return { a_ms, a_kph };
      }

      function updateSpeedOfSoundDisplay() {
        const { a_ms, a_kph } = getSpeedOfSound();
        if (!isFinite(a_ms)) {
          speedOfSoundDisplay.textContent = "—";
          return;
        }
        speedOfSoundDisplay.textContent = `${a_kph.toFixed(
          1
        )} km/h (${a_ms.toFixed(1)} m/s)`;
      }

      function showResult(text) {
        resultSummary.textContent = text;
        resultSummary.classList.remove("hidden");
      }

      function convertMachToKph() {
        const mach = parseFloat(machInput.value);
        if (isNaN(mach)) {
          showResult("Enter a valid Mach value to convert.");
          return;
        }
        const { a_ms, a_kph } = getSpeedOfSound();
        if (!isFinite(a_ms)) {
          showResult("Please enter a valid temperature for the custom mode.");
          return;
        }
        const v_kph = mach * a_kph;
        kphInput.value = v_kph.toFixed(2);
        showResult(
          `Mach ${mach.toFixed(4)} corresponds to approximately ${v_kph.toFixed(
            2
          )} km/h for the selected atmosphere model.`
        );
      }

      function convertKphToMach() {
        const v_kph = parseFloat(kphInput.value);
        if (isNaN(v_kph)) {
          showResult("Enter a valid speed in km/h to convert.");
          return;
        }
        const { a_ms, a_kph } = getSpeedOfSound();
        if (!isFinite(a_ms)) {
          showResult("Please enter a valid temperature for the custom mode.");
          return;
        }
        const mach = v_kph / a_kph;
        machInput.value = mach.toFixed(4);
        showResult(
          `${v_kph.toFixed(
            2
          )} km/h corresponds to approximately Mach ${mach.toFixed(
            4
          )} for the selected atmosphere model.`
        );
      }

      document
        .getElementById("btnMachToKph")
        .addEventListener("click", convertMachToKph);
      document
        .getElementById("btnKphToMach")
        .addEventListener("click", convertKphToMach);

      document.getElementById("btnSwap").addEventListener("click", () => {
        machInput.value = "";
        kphInput.value = "";
        resultSummary.classList.add("hidden");
      });

      atmRadios.forEach((r) => {
        r.addEventListener("change", () => {
          const mode = document.querySelector(
            'input[name="atmMode"]:checked'
          ).value;
          if (mode === "custom") {
            customTempRow.classList.remove("hidden");
          } else {
            customTempRow.classList.add("hidden");
          }
          updateSpeedOfSoundDisplay();
          resultSummary.classList.add("hidden");
        });
      });

      tempValueInput.addEventListener("input", () => {
        if (
          document.querySelector('input[name="atmMode"]:checked').value ===
          "custom"
        ) {
          updateSpeedOfSoundDisplay();
        }
      });
      tempUnitSelect.addEventListener("change", () => {
        if (
          document.querySelector('input[name="atmMode"]:checked').value ===
          "custom"
        ) {
          updateSpeedOfSoundDisplay();
        }
      });

      document.querySelectorAll(".preset-btn").forEach((btn) => {
        btn.addEventListener("click", () => {
          const mach = parseFloat(btn.getAttribute("data-preset-mach"));
          machInput.value = mach.toFixed(2);
          convertMachToKph();
        });
      });

      // Initialize display
      updateSpeedOfSoundDisplay();

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
