    <script>
      // Utility: trim and normalize input
      function normalizeInput(str) {
        return (str || "").trim();
      }

      function setBadge(el, text, colorClasses) {
        el.textContent = text;
        el.className = "badge " + colorClasses;
      }

      function escapeHtml(str) {
        return str.replace(/[&<>"']/g, function (m) {
          return {
            "&": "&amp;",
            "<": "&lt;",
            ">": "&gt;",
            '"': "&quot;",
            "'": "&#39;",
          }[m];
        });
      }

      function validateSyntax(raw) {
        const input = normalizeInput(raw);
        if (!input) {
          return { valid: false, error: "Empty input." };
        }
        try {
          const url = new URL(input);
          if (!url.protocol || !url.hostname) {
            return { valid: false, error: "Missing protocol or hostname." };
          }
          return { valid: true, url };
        } catch (e) {
          return { valid: false, error: "Invalid URL syntax." };
        }
      }

      function analyzeSafety(urlObj) {
        const issues = [];
        const scheme = urlObj.protocol.toLowerCase();
        const host = urlObj.hostname;
        const full = urlObj.href;

        if (
          (scheme === "http:" || scheme === "https:") &&
          scheme !== "https:"
        ) {
          issues.push("Uses HTTP instead of HTTPS.");
        }
        if (/\s/.test(full)) {
          issues.push("Contains whitespace characters.");
        }
        if (/[\u0000-\u001F\u007F]/.test(full)) {
          issues.push("Contains control (non-printable) characters.");
        }
        if (full.length > 2048) {
          issues.push("URL is very long (over 2048 characters).");
        }
        if ((urlObj.search || "").length > 512) {
          issues.push("Query string is unusually long.");
        }
        if ((urlObj.hash || "").length > 256) {
          issues.push("Fragment identifier is unusually long.");
        }
        if ((full.match(/@/g) || []).length > 1) {
          issues.push(
            'Contains multiple "@" characters, which can be used to obfuscate links.'
          );
        }

        return {
          safe: issues.length === 0,
          issues,
        };
      }

      async function fetchStatusChain(urlStr, maxRedirects = 5) {
        const chain = [];
        let currentUrl = urlStr;
        let redirects = 0;

        try {
          const response = await fetch(currentUrl, {
            method: "GET",
            mode: "cors",
            redirect: "follow",
          });

          // We only see the final response in fetch, but we can still show it
          chain.push({
            url: response.url || currentUrl,
            status: response.status,
            statusText: response.statusText || "",
            type: response.type,
          });

          return { ok: true, chain };
        } catch (err) {
          return { ok: false, error: err.message || String(err) };
        }
      }

      document.addEventListener("DOMContentLoaded", function () {
        const urlInput = document.getElementById("url-input");
        const validateBtn = document.getElementById("validate-btn");
        const clearBtn = document.getElementById("clear-btn");
        const checkSafe = document.getElementById("check-safe-patterns");
        const checkHttp = document.getElementById("check-http-status");

        const resultContainer = document.getElementById("url-result-container");
        const syntaxBadge = document.getElementById("syntax-badge");
        const safetyBadge = document.getElementById("safety-badge");
        const statusBadge = document.getElementById("status-badge");
        const messagesEl = document.getElementById("url-messages");

        const protoEl = document.getElementById("url-protocol");
        const hostEl = document.getElementById("url-host");
        const portEl = document.getElementById("url-port");
        const pathEl = document.getElementById("url-path");
        const queryEl = document.getElementById("url-query");
        const hashEl = document.getElementById("url-hash");

        const redirectDetails = document.getElementById("redirect-details");
        const redirectChainEl = document.getElementById("redirect-chain");

        const regexInput = document.getElementById("regex-input");
        const regexFlagI = document.getElementById("regex-flag-i");
        const regexFlagM = document.getElementById("regex-flag-m");
        const regexBtn = document.getElementById("regex-test-btn");
        const regexResult = document.getElementById("regex-result");

        function resetSingleResults() {
          resultContainer.classList.add("hidden");
          safetyBadge.classList.add("hidden");
          statusBadge.classList.add("hidden");
          messagesEl.innerHTML = "";
          protoEl.textContent = "–";
          hostEl.textContent = "–";
          portEl.textContent = "–";
          pathEl.textContent = "–";
          queryEl.textContent = "–";
          hashEl.textContent = "–";
          redirectDetails.classList.add("hidden");
          redirectChainEl.innerHTML = "";
          regexResult.textContent = "";
        }

        async function handleValidate() {
          resetSingleResults();
          const raw = urlInput.value;
          const syntax = validateSyntax(raw);

          resultContainer.classList.remove("hidden");

          if (!syntax.valid) {
            setBadge(
              syntaxBadge,
              "Invalid URL",
              "bg-red-100 text-red-800 border border-red-300"
            );
            messagesEl.innerHTML =
              '<p class="text-sm text-red-700">' +
              escapeHtml(syntax.error) +
              "</p>";
            return;
          }

          const urlObj = syntax.url;
          setBadge(
            syntaxBadge,
            "Valid URL",
            "bg-emerald-100 text-emerald-800 border border-emerald-300"
          );

          // URL breakdown
          protoEl.textContent = urlObj.protocol || "–";
          hostEl.textContent = urlObj.hostname || "–";
          portEl.textContent = urlObj.port || "default";
          pathEl.textContent = urlObj.pathname || "/";
          queryEl.textContent = urlObj.search || "–";
          hashEl.textContent = urlObj.hash || "–";

          const notes = [];

          // Safety analysis
          if (checkSafe.checked) {
            const safety = analyzeSafety(urlObj);
            if (safety.safe) {
              setBadge(
                safetyBadge,
                "Safe-looking",
                "bg-blue-100 text-blue-800 border border-blue-300"
              );
              safetyBadge.classList.remove("hidden");
              notes.push(
                "<li>No obvious red flags detected in the URL structure.</li>"
              );
            } else {
              setBadge(
                safetyBadge,
                "Potential issues",
                "bg-amber-100 text-amber-800 border border-amber-300"
              );
              safetyBadge.classList.remove("hidden");
              notes.push(
                '<li><strong>Potential issues:</strong><ul class="list-disc ml-5">' +
                  safety.issues
                    .map((i) => "<li>" + escapeHtml(i) + "</li>")
                    .join("") +
                  "</ul></li>"
              );
            }
          }

          // HTTP status / redirect
          if (
            checkHttp.checked &&
            (urlObj.protocol === "http:" || urlObj.protocol === "https:")
          ) {
            setBadge(
              statusBadge,
              "Checking…",
              "bg-gray-100 text-gray-700 border border-gray-300"
            );
            statusBadge.classList.remove("hidden");

            const statusInfo = await fetchStatusChain(urlObj.href);
            if (!statusInfo.ok) {
              setBadge(
                statusBadge,
                "Network / CORS error",
                "bg-amber-100 text-amber-800 border border-amber-300"
              );
              notes.push(
                "<li><strong>HTTP check:</strong> " +
                  escapeHtml(statusInfo.error) +
                  " (many sites block cross-origin requests; this does not always mean the URL is down).</li>"
              );
            } else {
              const final = statusInfo.chain[statusInfo.chain.length - 1];
              const label =
                final.status + (final.statusText ? " " + final.statusText : "");
              const ok = final.status >= 200 && final.status < 400;
              setBadge(
                statusBadge,
                "HTTP " + label,
                ok
                  ? "bg-emerald-100 text-emerald-800 border border-emerald-300"
                  : "bg-red-100 text-red-800 border border-red-300"
              );

              redirectDetails.classList.remove("hidden");
              redirectChainEl.innerHTML = statusInfo.chain
                .map((step, idx) => {
                  return (
                    '<div class="flex flex-col sm:flex-row sm:items-center sm:justify-between gap-1">' +
                    '<div class="font-mono break-all text-gray-800">' +
                    escapeHtml(step.url) +
                    "</div>" +
                    '<div class="text-xs text-gray-700 sm:text-right">' +
                    (idx === 0 ? "Initial request" : "Redirect " + idx) +
                    " &middot; HTTP " +
                    escapeHtml(String(step.status)) +
                    (step.statusText ? " " + escapeHtml(step.statusText) : "") +
                    (step.type ? " &middot; " + escapeHtml(step.type) : "") +
                    "</div>" +
                    "</div>"
                  );
                })
                .join('<hr class="my-1 border-gray-100">');

              notes.push(
                "<li><strong>HTTP check:</strong> final response " +
                  escapeHtml(label) +
                  ".</li>"
              );
            }
          } else if (checkHttp.checked) {
            notes.push(
              "<li>HTTP status check is only available for http:// and https:// URLs.</li>"
            );
          }

          if (notes.length === 0) {
            notes.push("<li>No additional notes.</li>");
          }

          messagesEl.innerHTML =
            '<ul class="list-disc ml-5 text-sm text-gray-700">' +
            notes.join("") +
            "</ul>";
        }

        validateBtn.addEventListener("click", handleValidate);
        urlInput.addEventListener("keydown", function (e) {
          if (e.key === "Enter") {
            e.preventDefault();
            handleValidate();
          }
        });
        clearBtn.addEventListener("click", function () {
          urlInput.value = "";
          resetSingleResults();
          urlInput.focus();
        });

        // Regex tester
        regexBtn.addEventListener("click", function () {
          regexResult.textContent = "";
          const pattern = regexInput.value;
          const testUrl = normalizeInput(urlInput.value);

          if (!pattern) {
            regexResult.textContent = "Enter a regex pattern first.";
            regexResult.className = "text-xs text-red-700";
            return;
          }
          if (!testUrl) {
            regexResult.textContent = "Enter a URL above to test against.";
            regexResult.className = "text-xs text-red-700";
            return;
          }

          let flags = "";
          if (regexFlagI.checked) flags += "i";
          if (regexFlagM.checked) flags += "m";

          try {
            const re = new RegExp(pattern, flags);
            const match = re.test(testUrl);
            if (match) {
              regexResult.textContent = "Match: the regex matches this URL.";
              regexResult.className = "text-xs text-emerald-700";
            } else {
              regexResult.textContent =
                "No match: the regex does not match this URL.";
              regexResult.className = "text-xs text-gray-700";
            }
          } catch (err) {
            regexResult.textContent =
              "Invalid regex: " + (err.message || String(err));
            regexResult.className = "text-xs text-red-700";
          }
        });

        // Bulk validator
        const bulkInput = document.getElementById("bulk-input");
        const bulkBtn = document.getElementById("bulk-validate-btn");
        const bulkClearBtn = document.getElementById("bulk-clear-btn");
        const bulkSummary = document.getElementById("bulk-summary");
        const bulkResults = document.getElementById("bulk-results");
        const bulkTbody = document.getElementById("bulk-tbody");

        bulkBtn.addEventListener("click", function () {
          const lines = bulkInput.value.split(/\r?\n/);
          bulkTbody.innerHTML = "";
          bulkResults.classList.add("hidden");
          bulkSummary.textContent = "";

          let total = 0;
          let validCount = 0;
          let invalidCount = 0;

          lines.forEach((line, idx) => {
            const trimmed = normalizeInput(line);
            if (!trimmed) return; // skip empty lines
            total++;
            const res = validateSyntax(trimmed);
            const tr = document.createElement("tr");
            tr.className = res.valid ? "" : "bg-red-50";

            const tdIndex = document.createElement("td");
            tdIndex.className = "px-2 py-1 align-top text-gray-500";
            tdIndex.textContent = String(idx + 1);

            const tdUrl = document.createElement("td");
            tdUrl.className = "px-2 py-1 align-top font-mono break-all";
            tdUrl.textContent = trimmed;

            const tdSyntax = document.createElement("td");
            tdSyntax.className = "px-2 py-1 align-top";
            tdSyntax.textContent = res.valid ? "Valid" : "Invalid";

            const tdNotes = document.createElement("td");
            tdNotes.className = "px-2 py-1 align-top text-gray-700 text-xs";
            tdNotes.textContent = res.valid ? "" : res.error || "Invalid URL";

            tr.appendChild(tdIndex);
            tr.appendChild(tdUrl);
            tr.appendChild(tdSyntax);
            tr.appendChild(tdNotes);
            bulkTbody.appendChild(tr);

            if (res.valid) validCount++;
            else invalidCount++;
          });

          if (total > 0) {
            bulkResults.classList.remove("hidden");
            bulkSummary.textContent =
              "Validated " +
              total +
              " URL" +
              (total !== 1 ? "s" : "") +
              " · " +
              validCount +
              " valid, " +
              invalidCount +
              " invalid.";
          } else {
            bulkSummary.textContent = "No non-empty lines to validate.";
          }
        });

        bulkClearBtn.addEventListener("click", function () {
          bulkInput.value = "";
          bulkTbody.innerHTML = "";
          bulkResults.classList.add("hidden");
          bulkSummary.textContent = "";
          bulkInput.focus();
        });
      });

      // MathJax config (global)
      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
