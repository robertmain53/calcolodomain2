    <script>
      // Default pKa data
      const defaultPkaData = [
        {
          id: "n_term",
          label: "N-terminus",
          type: "base",
          residue: "N-term",
          pka: 9.6,
        },
        {
          id: "c_term",
          label: "C-terminus",
          type: "acid",
          residue: "C-term",
          pka: 2.4,
        },
        { id: "D", label: "Asp (D)", type: "acid", residue: "D", pka: 3.9 },
        { id: "E", label: "Glu (E)", type: "acid", residue: "E", pka: 4.1 },
        { id: "H", label: "His (H)", type: "base", residue: "H", pka: 6.5 },
        { id: "C", label: "Cys (C)", type: "acid", residue: "C", pka: 8.3 },
        { id: "Y", label: "Tyr (Y)", type: "acid", residue: "Y", pka: 10.1 },
        { id: "K", label: "Lys (K)", type: "base", residue: "K", pka: 10.5 },
        { id: "R", label: "Arg (R)", type: "base", residue: "R", pka: 12.5 },
      ];

      function buildPkaTable() {
        const tbody = document.getElementById("pka-table-body");
        tbody.innerHTML = "";
        defaultPkaData.forEach((group) => {
          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="px-2 py-1">${group.label}</td>
        <td class="px-2 py-1">${group.residue}</td>
        <td class="px-2 py-1 capitalize">${group.type}</td>
        <td class="px-2 py-1">${group.pka.toFixed(2)}</td>
        <td class="px-2 py-1">
          <input type="number" step="0.01" class="w-20 border border-gray-300 rounded px-1 py-0.5 text-xs custom-pka-input" data-group-id="${
            group.id
          }">
        </td>
      `;
          tbody.appendChild(tr);
        });
      }

      function parseSequence(raw) {
        if (!raw) return "";
        return raw.toUpperCase().replace(/[^ACDEFGHIKLMNPQRSTVWY]/g, "");
      }

      function countIonizableGroups(seq, includeTermini) {
        const counts = {
          n_term: includeTermini && seq.length > 0 ? 1 : 0,
          c_term: includeTermini && seq.length > 0 ? 1 : 0,
          D: 0,
          E: 0,
          H: 0,
          C: 0,
          Y: 0,
          K: 0,
          R: 0,
        };
        for (const aa of seq) {
          if (counts.hasOwnProperty(aa)) counts[aa]++;
        }
        return counts;
      }

      function getPkaMap() {
        const useCustom =
          document.querySelector('input[name="pka-set"]:checked')?.value ===
          "custom";
        const map = {};
        defaultPkaData.forEach((group) => {
          let value = group.pka;
          if (useCustom) {
            const input = document.querySelector(
              `.custom-pka-input[data-group-id="${group.id}"]`
            );
            if (input && input.value !== "") {
              const v = parseFloat(input.value);
              if (!isNaN(v) && v > -2 && v < 20) value = v;
            }
          }
          map[group.id] = { pka: value, type: group.type };
        });
        return map;
      }

      function fractionalCharge(pH, pka, type) {
        const pow = Math.pow(10, pH - pka);
        if (type === "acid") {
          // fraction deprotonated (negative)
          const frac = 1 / (1 + Math.pow(10, pka - pH));
          return -frac;
        } else {
          // base: fraction protonated (positive)
          const frac = 1 / (1 + pow);
          return frac;
        }
      }

      function netChargeAtPH(pH, counts, pkaMap) {
        let q = 0;
        for (const key in counts) {
          const n = counts[key];
          if (!n) continue;
          const group = pkaMap[key];
          if (!group) continue;
          q += n * fractionalCharge(pH, group.pka, group.type);
        }
        return q;
      }

      function findPI(counts, pkaMap) {
        let low = 0.0;
        let high = 14.0;
        let qLow = netChargeAtPH(low, counts, pkaMap);
        let qHigh = netChargeAtPH(high, counts, pkaMap);

        // If both have same sign, expand a bit or return midpoint heuristic
        if (qLow * qHigh > 0) {
          return 7.0;
        }

        for (let i = 0; i < 80; i++) {
          const mid = (low + high) / 2;
          const qMid = netChargeAtPH(mid, counts, pkaMap);
          if (Math.abs(qMid) < 1e-4 || high - low < 1e-4) {
            return mid;
          }
          if (qLow * qMid <= 0) {
            high = mid;
            qHigh = qMid;
          } else {
            low = mid;
            qLow = qMid;
          }
        }
        return (low + high) / 2;
      }

      function updateChargeBreakdown(pH, counts, pkaMap) {
        const container = document.getElementById("charge-breakdown");
        container.innerHTML = "";
        const rows = [];

        function addRow(label, key) {
          const n = counts[key];
          if (!n) return;
          const group = pkaMap[key];
          const frac = fractionalCharge(pH, group.pka, group.type);
          const contrib = n * frac;
          rows.push({ label, n, pka: group.pka, frac, contrib });
        }

        addRow("N-terminus", "n_term");
        addRow("C-terminus", "c_term");
        addRow("Asp (D)", "D");
        addRow("Glu (E)", "E");
        addRow("His (H)", "H");
        addRow("Cys (C)", "C");
        addRow("Tyr (Y)", "Y");
        addRow("Lys (K)", "K");
        addRow("Arg (R)", "R");

        rows.forEach((r) => {
          const div = document.createElement("div");
          div.textContent = `${r.label}: count=${r.n}, pKa=${r.pka.toFixed(
            2
          )}, charge contribution=${r.contrib.toFixed(3)}`;
          container.appendChild(div);
        });
      }

      document.addEventListener("DOMContentLoaded", () => {
        buildPkaTable();

        const calcBtn = document.getElementById("calculate-btn");
        const errorEl = document.getElementById("error-message");
        const resultsContent = document.getElementById("results-content");
        const piValueEl = document.getElementById("pi-value");
        const chargeAt7El = document.getElementById("charge-at-7");
        const seqLengthEl = document.getElementById("seq-length");
        const probeBtn = document.getElementById("probe-btn");
        const probePhInput = document.getElementById("probe-ph");
        const probeResultEl = document.getElementById("probe-result");

        let lastCounts = null;
        let lastPkaMap = null;
        let lastPI = null;

        calcBtn.addEventListener("click", () => {
          errorEl.classList.add("hidden");
          errorEl.textContent = "";

          const rawSeq = document.getElementById("sequence-input").value;
          const seq = parseSequence(rawSeq);

          if (!seq) {
            errorEl.textContent =
              "Please enter at least one valid amino acid (A, R, N, D, C, E, Q, G, H, I, L, K, M, F, P, S, T, W, Y, V).";
            errorEl.classList.remove("hidden");
            resultsContent.classList.add("hidden");
            return;
          }

          const terminiMode =
            document.querySelector('input[name="termini-mode"]:checked')
              ?.value || "include";
          const includeTermini = terminiMode === "include";

          const counts = countIonizableGroups(seq, includeTermini);
          const pkaMap = getPkaMap();

          const pi = findPI(counts, pkaMap);
          const chargeAt7 = netChargeAtPH(7.0, counts, pkaMap);

          lastCounts = counts;
          lastPkaMap = pkaMap;
          lastPI = pi;

          piValueEl.textContent = pi.toFixed(3);
          chargeAt7El.textContent = chargeAt7.toFixed(3);
          seqLengthEl.textContent = `${seq.length} aa`;

          resultsContent.classList.remove("hidden");
          probeResultEl.textContent = "";
          updateChargeBreakdown(pi, counts, pkaMap);
        });

        probeBtn.addEventListener("click", () => {
          if (!lastCounts || !lastPkaMap) {
            probeResultEl.textContent = "Run a pI calculation first.";
            return;
          }
          const pH = parseFloat(probePhInput.value);
          if (isNaN(pH) || pH < 0 || pH > 14) {
            probeResultEl.textContent = "Enter a pH between 0 and 14.";
            return;
          }
          const q = netChargeAtPH(pH, lastCounts, lastPkaMap);
          probeResultEl.textContent = `Net charge at pH ${pH.toFixed(
            2
          )}: ${q.toFixed(3)}`;
          updateChargeBreakdown(pH, lastCounts, lastPkaMap);
        });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
