    <script>
      // Simple line chart without external libraries
      class SimpleLineChart {
        constructor(canvas) {
          this.canvas = canvas;
          this.ctx = canvas.getContext('2d');
          this.best = [];
          this.avg = [];
        }
        setData(best, avg) {
          this.best = best.slice();
          this.avg = avg.slice();
          this.draw();
        }
        clear() {
          const { ctx, canvas } = this;
          ctx.clearRect(0, 0, canvas.width, canvas.height);
        }
        draw() {
          const { ctx, canvas } = this;
          const w = canvas.width;
          const h = canvas.height;
          ctx.clearRect(0, 0, w, h);
          if (!this.best.length) return;

          const n = this.best.length;
          const all = this.best.concat(this.avg);
          const minVal = Math.min.apply(null, all);
          const maxVal = Math.max.apply(null, all);
          const pad = 10;
          const xStep = (w - 2 * pad) / Math.max(1, n - 1);
          const scaleY = (val) => {
            if (maxVal === minVal) return h / 2;
            return h - pad - (val - minVal) * (h - 2 * pad) / (maxVal - minVal);
          };

          // Axes
          ctx.strokeStyle = '#e5e7eb';
          ctx.lineWidth = 1;
          ctx.beginPath();
          ctx.moveTo(pad, pad);
          ctx.lineTo(pad, h - pad);
          ctx.lineTo(w - pad, h - pad);
          ctx.stroke();

          const drawSeries = (data, color) => {
            ctx.strokeStyle = color;
            ctx.lineWidth = 1.5;
            ctx.beginPath();
            data.forEach((v, i) => {
              const x = pad + i * xStep;
              const y = scaleY(v);
              if (i === 0) ctx.moveTo(x, y);
              else ctx.lineTo(x, y);
            });
            ctx.stroke();
          };

          drawSeries(this.avg, '#9ca3af');   // gray
          drawSeries(this.best, '#2563eb');  // blue
        }
      }

      // Genetic Algorithm implementation
      const GA = (() => {
        let population = [];
        let fitnesses = [];
        let generation = 0;
        let bestEver = null;
        let bestEverFitness = -Infinity;
        let bestEverGen = 0;
        let running = false;
        let chart;

        function log(msg) {
          const logArea = document.getElementById('logArea');
          logArea.textContent += '\\n' + msg;
          logArea.scrollTop = logArea.scrollHeight;
        }

        function randomReal(min, max) {
          return min + Math.random() * (max - min);
        }

        function gaussianNoise(std) {
          // Box-Muller
          let u = 0, v = 0;
          while (u === 0) u = Math.random();
          while (v === 0) v = Math.random();
          return std * Math.sqrt(-2.0 * Math.log(u)) * Math.cos(2.0 * Math.PI * v);
        }

        function initPopulation(config) {
          const {
            chromosomeType, populationSize, dimension,
            bitLength, lowerBound, upperBound
          } = config;

          population = [];
          fitnesses = [];
          generation = 0;
          bestEver = null;
          bestEverFitness = -Infinity;
          bestEverGen = 0;

          for (let i = 0; i < populationSize; i++) {
            if (chromosomeType === 'real') {
              const indiv = [];
              for (let d = 0; d < dimension; d++) {
                indiv.push(randomReal(lowerBound, upperBound));
              }
              population.push(indiv);
            } else {
              let bits = '';
              for (let b = 0; b < bitLength; b++) {
                bits += Math.random() < 0.5 ? '0' : '1';
              }
              population.push(bits);
            }
          }
        }

        function evaluatePopulation(fitnessFn) {
          fitnesses = population.map(ind => {
            try {
              const val = fitnessFn(ind);
              return (typeof val === 'number' && isFinite(val)) ? val : -Infinity;
            } catch (e) {
              console.error('Fitness error', e);
              return -Infinity;
            }
          });
        }

        function selectParent(config) {
          const { selectionMethod } = config;
          if (selectionMethod === 'tournament') {
            const k = 3;
            let bestIdx = null;
            for (let i = 0; i < k; i++) {
              const idx = Math.floor(Math.random() * population.length);
              if (bestIdx === null || fitnesses[idx] > fitnesses[bestIdx]) {
                bestIdx = idx;
              }
            }
            return JSON.parse(JSON.stringify(population[bestIdx]));
          } else {
            // Roulette wheel
            const minFit = Math.min.apply(null, fitnesses);
            const offset = minFit < 0 ? -minFit + 1e-9 : 0;
            const adjusted = fitnesses.map(f => f + offset);
            const sum = adjusted.reduce((a, b) => a + b, 0);
            if (sum <= 0) {
              // fallback: random
              return JSON.parse(JSON.stringify(population[Math.floor(Math.random() * population.length)]));
            }
            let r = Math.random() * sum;
            for (let i = 0; i < adjusted.length; i++) {
              r -= adjusted[i];
              if (r <= 0) {
                return JSON.parse(JSON.stringify(population[i]));
              }
            }
            return JSON.parse(JSON.stringify(population[population.length - 1]));
          }
        }

        function crossover(a, b, config) {
          const { chromosomeType, crossoverRate } = config;
          if (Math.random() > crossoverRate) {
            return [JSON.parse(JSON.stringify(a)), JSON.parse(JSON.stringify(b))];
          }
          if (chromosomeType === 'real') {
            const alpha = Math.random();
            const child1 = a.map((v, i) => alpha * v + (1 - alpha) * b[i]);
            const child2 = a.map((v, i) => (1 - alpha) * v + alpha * b[i]);
            return [child1, child2];
          } else {
            const len = a.length;
            if (len < 2) return [a, b];
            const point = 1 + Math.floor(Math.random() * (len - 1));
            const c1 = a.slice(0, point) + b.slice(point);
            const c2 = b.slice(0, point) + a.slice(point);
            return [c1, c2];
          }
        }

        function mutate(ind, config) {
          const { chromosomeType, mutationRate, lowerBound, upperBound } = config;
          if (chromosomeType === 'real') {
            const std = (upperBound - lowerBound) * 0.05; // 5% of range
            for (let i = 0; i < ind.length; i++) {
              if (Math.random() < mutationRate) {
                ind[i] += gaussianNoise(std);
                if (ind[i] < lowerBound) ind[i] = lowerBound;
                if (ind[i] > upperBound) ind[i] = upperBound;
              }
            }
          } else {
            let bits = ind.split('');
            for (let i = 0; i < bits.length; i++) {
              if (Math.random() < mutationRate) {
                bits[i] = bits[i] === '0' ? '1' : '0';
              }
            }
            ind = bits.join('');
          }
          return ind;
        }

        function step(config, fitnessFn, history) {
          if (!population.length) {
            initPopulation(config);
          }
          evaluatePopulation(fitnessFn);

          const popSize = population.length;
          const paired = population.map((ind, i) => ({ ind, fit: fitnesses[i] }))
            .sort((a, b) => b.fit - a.fit);

          const best = paired[0];
          const avg = fitnesses.reduce((a, b) => a + b, 0) / popSize;

          if (best.fit > bestEverFitness) {
            bestEverFitness = best.fit;
            bestEver = JSON.parse(JSON.stringify(best.ind));
            bestEverGen = generation;
          }

          history.best.push(best.fit);
          history.avg.push(avg);

          // Update UI
          document.getElementById('bestGeneration').textContent = bestEverGen;
          document.getElementById('bestFitness').textContent = bestEverFitness.toFixed(6);
          document.getElementById('bestChromosome').textContent = JSON.stringify(bestEver, null, 2);

          // Population table
          const tbody = document.getElementById('populationTableBody');
          tbody.innerHTML = '';
          paired.slice(0, 10).forEach((p, idx) => {
            const tr = document.createElement('tr');
            tr.innerHTML = `
              <td class="px-2 py-1 text-gray-700">${idx + 1}</td>
              <td class="px-2 py-1 text-gray-700">${Number.isFinite(p.fit) ? p.fit.toFixed(6) : 'NaN'}</td>
              <td class="px-2 py-1 text-gray-600 whitespace-pre-wrap">${JSON.stringify(p.ind)}</td>
            `;
            tbody.appendChild(tr);
          });

          if (chart) {
            chart.setData(history.best, history.avg);
          }

          log(\`Generation \${generation}: best = \${best.fit.toFixed(6)}, avg = \${avg.toFixed(6)}\`);

          // Create next generation
          const newPop = [];
          const elitismCount = Math.max(0, Math.min(config.elitismCount, popSize));
          for (let i = 0; i < elitismCount; i++) {
            newPop.push(JSON.parse(JSON.stringify(paired[i].ind)));
          }
          while (newPop.length < popSize) {
            const parent1 = selectParent(config);
            const parent2 = selectParent(config);
            let [child1, child2] = crossover(parent1, parent2, config);
            child1 = mutate(child1, config);
            child2 = mutate(child2, config);
            newPop.push(child1);
            if (newPop.length < popSize) newPop.push(child2);
          }

          population = newPop;
          generation += 1;
        }

        function getConfigFromUI() {
          const chromosomeType = document.getElementById('chromosomeType').value;
          const populationSize = parseInt(document.getElementById('populationSize').value, 10);
          const generations = parseInt(document.getElementById('generations').value, 10);
          const crossoverRate = parseFloat(document.getElementById('crossoverRate').value);
          const mutationRate = parseFloat(document.getElementById('mutationRate').value);
          const selectionMethod = document.getElementById('selectionMethod').value;
          const elitismCount = parseInt(document.getElementById('elitismCount').value, 10);
          const dimension = parseInt(document.getElementById('dimension').value, 10);
          const bitLength = parseInt(document.getElementById('bitLength').value, 10);
          const lowerBound = parseFloat(document.getElementById('lowerBound').value);
          const upperBound = parseFloat(document.getElementById('upperBound').value);

          return {
            chromosomeType,
            populationSize,
            generations,
            crossoverRate,
            mutationRate,
            selectionMethod,
            elitismCount,
            dimension,
            bitLength,
            lowerBound,
            upperBound
          };
        }

        function getFitnessFnFromUI() {
          const code = document.getElementById('fitnessFunction').value;
          let fitnessFn;
          try {
            // Wrap in IIFE to avoid leaking variables
            const wrapper = new Function(code + '; return fitness;');
            fitnessFn = wrapper();
            if (typeof fitnessFn !== 'function') {
              throw new Error('fitness is not a function');
            }
          } catch (e) {
            alert('Error in fitness function: ' + e.message);
            throw e;
          }
          return fitnessFn;
        }

        function resetUI() {
          document.getElementById('bestGeneration').textContent = '–';
          document.getElementById('bestFitness').textContent = '–';
          document.getElementById('bestChromosome').textContent = '–';
          document.getElementById('populationTableBody').innerHTML =
            '<tr><td colspan="3" class="px-2 py-2 text-center text-gray-400">Run the GA to see population details.</td></tr>';
          document.getElementById('gaStatus').textContent = '';
          if (chart) chart.setData([], []);
        }

        function init() {
          const canvas = document.getElementById('fitnessChart');
          // Ensure canvas has explicit size for crisp drawing
          canvas.width = canvas.clientWidth || 320;
          canvas.height = canvas.clientHeight || 160;
          chart = new SimpleLineChart(canvas);

          document.getElementById('chromosomeType').addEventListener('change', (e) => {
            const type = e.target.value;
            document.getElementById('real-dimension-wrapper').classList.toggle('hidden', type !== 'real');
            document.getElementById('real-bounds-wrapper').classList.toggle('hidden', type !== 'real');
            document.getElementById('binary-length-wrapper').classList.toggle('hidden', type !== 'binary');
          });

          document.getElementById('runGA').addEventListener('click', async () => {
            if (running) return;
            const config = getConfigFromUI();
            let fitnessFn;
            try {
              fitnessFn = getFitnessFnFromUI();
            } catch {
              return;
            }

            resetUI();
            document.getElementById('logArea').textContent = 'Starting genetic algorithm run...';
            const history = { best: [], avg: [] };
            initPopulation(config);
            running = true;
            document.getElementById('gaStatus').textContent = 'Running...';

            for (let g = 0; g < config.generations; g++) {
              step(config, fitnessFn, history);
              await new Promise(r => setTimeout(r, 10)); // small delay for UI responsiveness
            }

            running = false;
            document.getElementById('gaStatus').textContent = 'Finished.';
            log('Run complete.');
          });

          document.getElementById('stepGA').addEventListener('click', () => {
            if (running) return;
            const config = getConfigFromUI();
            let fitnessFn;
            try {
              fitnessFn = getFitnessFnFromUI();
            } catch {
              return;
            }
            if (!population.length) {
              resetUI();
              document.getElementById('logArea').textContent = 'Initialized population. Stepping one generation at a time.';
              const history = { best: [], avg: [] };
              initPopulation(config);
              step(config, fitnessFn, history);
              // store history on window for subsequent steps
              window.__gaHistory = history;
            } else {
              const history = window.__gaHistory || { best: [], avg: [] };
              step(config, fitnessFn, history);
              window.__gaHistory = history;
            }
            document.getElementById('gaStatus').textContent = 'Stepped 1 generation.';
          });

          document.getElementById('resetGA').addEventListener('click', () => {
            population = [];
            fitnesses = [];
            generation = 0;
            bestEver = null;
            bestEverFitness = -Infinity;
            bestEverGen = 0;
            window.__gaHistory = { best: [], avg: [] };
            resetUI();
            document.getElementById('logArea').textContent = 'Ready. Configure the GA and click “Run genetic algorithm”.';
          });

          document.getElementById('clearLog').addEventListener('click', () => {
            document.getElementById('logArea').textContent = '';
          });

          window.addEventListener('resize', () => {
            const canvas = document.getElementById('fitnessChart');
            canvas.width = canvas.clientWidth || 320;
            canvas.height = canvas.clientHeight || 160;
            if (chart) chart.draw();
          });
        }

        return { init };
      })();

      document.addEventListener('DOMContentLoaded', () => {
        GA.init();
      });

      window.MathJax = {
        tex: { inlineMath: [['\\(','\\)'], ['$', '$']], displayMath: [['$','$'], ['\\[','\\]']] },
        svg: { fontCache: 'global' }
      };
    </script>
