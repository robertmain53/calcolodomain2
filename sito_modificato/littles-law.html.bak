    <script>
      // Little's Law calculator logic
      (function () {
        const solveForEl = document.getElementById("solveFor");
        const presetEl = document.getElementById("contextPreset");
        const wipInput = document.getElementById("wipInput");
        const wipUnit = document.getElementById("wipUnit");
        const throughputInput = document.getElementById("throughputInput");
        const throughputTimeUnit =
          document.getElementById("throughputTimeUnit");
        const cycleTimeInput = document.getElementById("cycleTimeInput");
        const cycleTimeUnit = document.getElementById("cycleTimeUnit");
        const calcBtn = document.getElementById("calculateBtn");
        const clearBtn = document.getElementById("clearBtn");
        const resultCard = document.getElementById("resultCard");
        const resultMain = document.getElementById("resultMain");
        const resultDetail = document.getElementById("resultDetail");
        const resultHint = document.getElementById("resultHint");
        const errorBox = document.getElementById("errorBox");

        function showError(msg) {
          errorBox.textContent = msg;
          errorBox.classList.remove("hidden");
          resultCard.classList.add("hidden");
        }

        function clearError() {
          errorBox.classList.add("hidden");
          errorBox.textContent = "";
        }

        function applyPreset() {
          const p = presetEl.value;
          if (p === "kanban") {
            wipUnit.value = "stories";
            throughputTimeUnit.value = "week";
            cycleTimeUnit.value = "week";
          } else if (p === "callcenter") {
            wipUnit.value = "callers";
            throughputTimeUnit.value = "hour";
            cycleTimeUnit.value = "hour";
          } else if (p === "manufacturing") {
            wipUnit.value = "parts";
            throughputTimeUnit.value = "day";
            cycleTimeUnit.value = "day";
          } else if (p === "hospital") {
            wipUnit.value = "patients";
            throughputTimeUnit.value = "day";
            cycleTimeUnit.value = "day";
          }
        }

        presetEl.addEventListener("change", applyPreset);

        function calculate() {
          clearError();
          const solveFor = solveForEl.value;

          const wip = parseFloat(wipInput.value);
          const thr = parseFloat(throughputInput.value);
          const ct = parseFloat(cycleTimeInput.value);

          if (throughputTimeUnit.value !== cycleTimeUnit.value) {
            showError(
              "Time units for throughput and cycle time must match (e.g., both per day or both per hour)."
            );
            return;
          }

          if (solveFor === "wip") {
            if (isNaN(thr) || thr <= 0 || isNaN(ct) || ct < 0) {
              showError(
                "To compute WIP, enter a positive throughput and a non-negative cycle time."
              );
              return;
            }
            const wipVal = thr * ct;
            wipInput.value = wipVal.toFixed(3).replace(/\.?0+$/, "");
            resultMain.textContent = `WIP ≈ ${wipInput.value} ${
              wipUnit.value || "items"
            }`;
            resultDetail.textContent = `With a throughput of ${thr} ${
              wipUnit.value || "items"
            } per ${throughputTimeUnit.value} and a cycle time of ${ct} ${
              cycleTimeUnit.value
            }, the average number of items in the system is about ${
              wipInput.value
            }.`;
            resultHint.textContent =
              "If this WIP feels too high, consider reducing WIP limits or improving process efficiency to shorten cycle time.";
          } else if (solveFor === "cycleTime") {
            if (isNaN(wip) || wip < 0 || isNaN(thr) || thr <= 0) {
              showError(
                "To compute cycle time, enter a non-negative WIP and a positive throughput."
              );
              return;
            }
            const ctVal = wip / thr;
            cycleTimeInput.value = ctVal.toFixed(3).replace(/\.?0+$/, "");
            resultMain.textContent = `Cycle time ≈ ${cycleTimeInput.value} ${cycleTimeUnit.value}`;
            resultDetail.textContent = `With an average WIP of ${wip} ${
              wipUnit.value || "items"
            } and throughput of ${thr} per ${
              throughputTimeUnit.value
            }, items spend about ${cycleTimeInput.value} ${
              cycleTimeUnit.value
            } in the system.`;
            resultHint.textContent =
              "To reduce cycle time, you can either increase throughput or reduce WIP (for example by limiting work in progress).";
          } else if (solveFor === "throughput") {
            if (isNaN(wip) || wip < 0 || isNaN(ct) || ct <= 0) {
              showError(
                "To compute throughput, enter a non-negative WIP and a positive cycle time."
              );
              return;
            }
            const thrVal = wip / ct;
            throughputInput.value = thrVal.toFixed(3).replace(/\.?0+$/, "");
            resultMain.textContent = `Throughput ≈ ${throughputInput.value} ${
              wipUnit.value || "items"
            } per ${throughputTimeUnit.value}`;
            resultDetail.textContent = `With an average WIP of ${wip} ${
              wipUnit.value || "items"
            } and cycle time of ${ct} ${
              cycleTimeUnit.value
            }, the system completes about ${throughputInput.value} ${
              wipUnit.value || "items"
            } per ${throughputTimeUnit.value}.`;
            resultHint.textContent =
              "To increase throughput, you can shorten cycle time (e.g., remove bottlenecks) or, in some systems, modestly increase WIP without causing congestion.";
          }

          resultCard.classList.remove("hidden");
        }

        function clearAll() {
          wipInput.value = "";
          throughputInput.value = "";
          cycleTimeInput.value = "";
          resultCard.classList.add("hidden");
          clearError();
        }

        calcBtn.addEventListener("click", calculate);
        clearBtn.addEventListener("click", clearAll);
      })();

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
