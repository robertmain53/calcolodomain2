    <script>
      // Utility: format currency based on selected code (simple symbol mapping)
      const currencySymbols = {
        USD: "$",
        EUR: "€",
        GBP: "£",
        AUD: "$",
        CAD: "$",
        INR: "₹",
      };

      function formatCurrency(value, currency) {
        const symbol = currencySymbols[currency] || "$";
        if (isNaN(value) || !isFinite(value)) return symbol + "0";
        const absVal = Math.abs(value);
        let formatted;
        if (absVal >= 1000000) {
          formatted = (value / 1000000).toFixed(2) + "M";
        } else if (absVal >= 1000) {
          formatted = (value / 1000).toFixed(1) + "k";
        } else {
          formatted = value.toFixed(0);
        }
        return symbol + formatted;
      }

      function formatMonths(m) {
        if (!isFinite(m) || m < 0) return "–";
        if (m < 1) {
          const days = Math.round(m * 30);
          return days + " days";
        }
        return m.toFixed(1) + " months";
      }

      document.addEventListener("DOMContentLoaded", () => {
        const currencyEl = document.getElementById("currency");
        const currencySymbolEls = document.querySelectorAll(
          '#currency-symbol, [for="monthly-expenses"] + div span.inline-flex, [for="current-savings"] + div span.inline-flex, [for="monthly-contribution"] + div span.inline-flex'
        );

        const incomeEl = document.getElementById("monthly-income");
        const expensesEl = document.getElementById("monthly-expenses");
        const jobStabilityEl = document.getElementById("job-stability");
        const dependentsEl = document.getElementById("dependents");
        const singleIncomeEl = document.getElementById("single-income");
        const healthIssuesEl = document.getElementById("health-issues");
        const variableIncomeEl = document.getElementById("variable-income");

        const currentSavingsEl = document.getElementById("current-savings");
        const monthlyContributionEl = document.getElementById(
          "monthly-contribution"
        );

        const monthsRangeLabelEl =
          document.getElementById("months-range-label");
        const targetMinAmountEl = document.getElementById("target-min-amount");
        const targetMaxAmountEl = document.getElementById("target-max-amount");
        const riskSummaryEl = document.getElementById("risk-summary");

        const currentSavingsDisplayEl = document.getElementById(
          "current-savings-display"
        );
        const gapMinDisplayEl = document.getElementById("gap-min-display");
        const gapMaxDisplayEl = document.getElementById("gap-max-display");
        const currentCoverageDisplayEl = document.getElementById(
          "current-coverage-display"
        );

        const timeToMinDisplayEl = document.getElementById(
          "time-to-min-display"
        );
        const timeToMaxDisplayEl = document.getElementById(
          "time-to-max-display"
        );

        const coverageSliderEl = document.getElementById("coverage-slider");
        const coverageMonthsDisplayEl = document.getElementById(
          "coverage-months-display"
        );
        const coverageAmountDisplayEl = document.getElementById(
          "coverage-amount-display"
        );

        const resetBtn = document.getElementById("reset-btn");

        function updateCurrencySymbols() {
          const cur = currencyEl.value;
          const symbol = currencySymbols[cur] || "$";
          currencySymbolEls.forEach((el) => {
            el.textContent = symbol;
          });
        }

        function calculateRecommendation() {
          const expenses = Math.max(0, Number(expensesEl.value) || 0);
          const jobRisk = Number(jobStabilityEl.value) || 0;
          const dependents = Math.max(0, Number(dependentsEl.value) || 0);

          let riskScore = 0;
          riskScore += jobRisk;
          riskScore += Math.min(dependents * 0.25, 2); // cap dependent effect
          if (singleIncomeEl.checked) riskScore += 0.5;
          if (healthIssuesEl.checked) riskScore += 0.5;
          if (variableIncomeEl.checked) riskScore += 0.5;

          // Base range 3–6 months; adjust upper bound with riskScore, lower bound slightly
          let minMonths = 3;
          let maxMonths = 6 + riskScore;

          // If riskScore is very low, allow 2–4 months
          if (riskScore <= 0.25) {
            minMonths = 2;
            maxMonths = 4;
          }

          // If riskScore is high, push towards 6–12 months
          if (riskScore >= 2.5) {
            minMonths = 6;
            maxMonths = Math.min(12, 8 + (riskScore - 2.5) * 2);
          }

          // Clamp
          minMonths = Math.max(1, Math.min(minMonths, 12));
          maxMonths = Math.max(minMonths, Math.min(maxMonths, 12));

          const currency = currencyEl.value;

          const minTarget = expenses * minMonths;
          const maxTarget = expenses * maxMonths;

          monthsRangeLabelEl.textContent = `${minMonths.toFixed(
            0
          )}–${maxMonths.toFixed(0)} months`;
          targetMinAmountEl.textContent = formatCurrency(minTarget, currency);
          targetMaxAmountEl.textContent = formatCurrency(maxTarget, currency);

          // Risk summary text
          let summary;
          if (riskScore <= 0.25) {
            summary =
              "Low overall risk. A 2–4 month buffer is often sufficient, but more is always safer.";
          } else if (riskScore <= 1.5) {
            summary =
              "Typical risk profile. A 3–6 month buffer is usually recommended.";
          } else if (riskScore <= 3) {
            summary =
              "Elevated risk (dependents, income uncertainty, or health factors). Aim for at least 6 months of expenses.";
          } else {
            summary =
              "High risk profile. Consider building 9–12 months of expenses for stronger protection.";
          }
          riskSummaryEl.textContent = summary;

          return { minMonths, maxMonths, minTarget, maxTarget };
        }

        function updateOutputs() {
          updateCurrencySymbols();
          const { minMonths, maxMonths, minTarget, maxTarget } =
            calculateRecommendation();

          const expenses = Math.max(0, Number(expensesEl.value) || 0);
          const currentSavings = Math.max(
            0,
            Number(currentSavingsEl.value) || 0
          );
          const monthlyContribution = Math.max(
            0,
            Number(monthlyContributionEl.value) || 0
          );
          const currency = currencyEl.value;

          currentSavingsDisplayEl.textContent = formatCurrency(
            currentSavings,
            currency
          );

          const gapMin = Math.max(0, minTarget - currentSavings);
          const gapMax = Math.max(0, maxTarget - currentSavings);

          gapMinDisplayEl.textContent = formatCurrency(gapMin, currency);
          gapMaxDisplayEl.textContent = formatCurrency(gapMax, currency);

          // Current coverage
          let coverageMonths = expenses > 0 ? currentSavings / expenses : 0;
          currentCoverageDisplayEl.textContent = `${formatMonths(
            coverageMonths
          )} of expenses`;

          // Timeline
          if (monthlyContribution > 0) {
            const monthsToMin = gapMin / monthlyContribution;
            const monthsToMax = gapMax / monthlyContribution;
            timeToMinDisplayEl.textContent =
              monthsToMin <= 0 ? "Already there" : formatMonths(monthsToMin);
            timeToMaxDisplayEl.textContent =
              monthsToMax <= 0 ? "Already there" : formatMonths(monthsToMax);
          } else {
            timeToMinDisplayEl.textContent =
              gapMin <= 0
                ? "Already there"
                : "Not reachable (monthly contribution is 0)";
            timeToMaxDisplayEl.textContent =
              gapMax <= 0
                ? "Already there"
                : "Not reachable (monthly contribution is 0)";
          }

          // Coverage slider
          const sliderMonths = Number(coverageSliderEl.value) || 3;
          coverageMonthsDisplayEl.textContent =
            sliderMonths + (sliderMonths === 1 ? " month" : " months");
          const coverageAmount = expenses * sliderMonths;
          coverageAmountDisplayEl.textContent = formatCurrency(
            coverageAmount,
            currency
          );
        }

        // Event listeners
        [
          currencyEl,
          incomeEl,
          expensesEl,
          jobStabilityEl,
          dependentsEl,
          singleIncomeEl,
          healthIssuesEl,
          variableIncomeEl,
          currentSavingsEl,
          monthlyContributionEl,
          coverageSliderEl,
        ].forEach((el) => {
          el.addEventListener("input", updateOutputs);
          el.addEventListener("change", updateOutputs);
        });

        resetBtn.addEventListener("click", () => {
          currencyEl.value = "USD";
          incomeEl.value = 4000;
          expensesEl.value = 2500;
          jobStabilityEl.value = "0.5";
          dependentsEl.value = 0;
          singleIncomeEl.checked = false;
          healthIssuesEl.checked = false;
          variableIncomeEl.checked = false;
          currentSavingsEl.value = 2000;
          monthlyContributionEl.value = 500;
          coverageSliderEl.value = 3;
          updateOutputs();
        });

        // Initial render
        updateOutputs();
      });

      // Mobile menu script (from template)
      document.addEventListener("DOMContentLoaded", () => {
        const toggle = document.getElementById("mobile-menu-toggle");
        const menu = document.getElementById("mobile-menu");
        if (toggle && menu) {
          toggle.addEventListener("click", () => {
            const isHidden = menu.classList.contains("hidden");
            menu.classList.toggle("hidden", !isHidden);
            toggle.setAttribute("aria-expanded", isHidden ? "true" : "false");
          });
        }
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
