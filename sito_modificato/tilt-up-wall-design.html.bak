    <script>
      // Core tilt-up wall design calculations (simplified ACI-style)
      function runTiltUpCalc() {
        const h_ft = parseFloat(document.getElementById("h_ft").value) || 0;
        const b_ft = parseFloat(document.getElementById("b_ft").value) || 0;
        const t_in = parseFloat(document.getElementById("t_in").value) || 0;
        const fc_psi = parseFloat(document.getElementById("fc_psi").value) || 0;
        const fy_ksi = parseFloat(document.getElementById("fy_ksi").value) || 0;
        const rho_v = parseFloat(document.getElementById("rho_v").value) || 0;
        const cover_in =
          parseFloat(document.getElementById("cover_in").value) || 0;
        const Pu_k = parseFloat(document.getElementById("Pu_k").value) || 0;
        const qu_psf = parseFloat(document.getElementById("qu_psf").value) || 0;
        const endCond = document.getElementById("end_condition").value;
        const k_factor =
          parseFloat(document.getElementById("k_factor").value) || 1.0;
        const phi_flex =
          parseFloat(document.getElementById("phi_flex").value) || 0.9;
        const phi_axial =
          parseFloat(document.getElementById("phi_axial").value) || 0.65;

        const overallStatus = document.getElementById("overallStatus");
        const geomResults = document.getElementById("geomResults");
        const loadResults = document.getElementById("loadResults");
        const flexureResults = document.getElementById("flexureResults");
        const axialResults = document.getElementById("axialResults");

        // Clear lists
        [geomResults, loadResults, flexureResults, axialResults].forEach(
          (ul) => (ul.innerHTML = "")
        );
        overallStatus.textContent = "";

        if (h_ft <= 0 || t_in <= 0 || fc_psi <= 0 || fy_ksi <= 0) {
          overallStatus.textContent =
            "Please enter valid positive values for geometry and material properties.";
          overallStatus.className = "text-sm font-semibold text-red-600";
          return;
        }

        // Geometry & section (per 1 ft strip)
        const t_ft = t_in / 12.0;
        const b_strip_ft = 1.0;
        const Ag_ft2 = b_strip_ft * t_ft; // ft^2
        const Ag_in2 = Ag_ft2 * 144.0; // in^2
        const Ig_ft4 = (b_strip_ft * Math.pow(t_ft, 3)) / 12.0;
        const Ig_in4 = Ig_ft4 * Math.pow(12, 4);
        const r_ft = Math.sqrt(Ig_ft4 / Ag_ft2);
        const slenderness = (k_factor * h_ft) / r_ft;

        // Loads & moments
        const wu_plf = qu_psf * b_strip_ft; // plf
        let Mu_ftlb;
        if (endCond === "pf") {
          Mu_ftlb = (wu_plf * Math.pow(h_ft, 2)) / 2.0;
        } else if (endCond === "pp") {
          Mu_ftlb = (wu_plf * Math.pow(h_ft, 2)) / 8.0;
        } else {
          Mu_ftlb = (wu_plf * Math.pow(h_ft, 2)) / 12.0;
        }
        const Mu_kft = Mu_ftlb / 1000.0;

        // Material properties
        const Ec_psi = 57000 * Math.sqrt(fc_psi); // approximate ACI
        const Ec_ksf = Ec_psi / 144.0;

        // Euler critical load for slenderness (per 1 ft strip)
        const h_in = h_ft * 12.0;
        const kL_in = k_factor * h_in;
        const Pcr_lb = (Math.PI * Math.PI * Ec_psi * Ig_in4) / (kL_in * kL_in);
        const Pcr_k = Pcr_lb / 1000.0;

        let delta = 1.0;
        if (Pcr_k > 0 && Pu_k > 0 && Pu_k < 0.75 * Pcr_k) {
          delta = 1.0 / (1.0 - Pu_k / Pcr_k);
        } else if (Pu_k >= 0.75 * Pcr_k && Pu_k < Pcr_k) {
          delta = 1.0 / (1.0 - Pu_k / Pcr_k);
        }
        const Mu2_kft = Mu_kft * delta;

        // Flexural capacity
        const fy_psi = fy_ksi * 1000.0;
        const As_in2 = rho_v * b_strip_ft * t_in; // b in ft, t in in -> ft*in, but rho is dimensionless; treat as in^2 approx
        const d_in = Math.max(t_in - cover_in, 0.5 * t_in);
        const a_in = (As_in2 * fy_psi) / (0.85 * fc_psi * 12.0); // b=12 in for 1 ft strip
        const Mn_inlb = As_in2 * fy_psi * (d_in - a_in / 2.0);
        const Mn_kft = Mn_inlb / 12.0 / 1000.0;
        const phiMn_kft = phi_flex * Mn_kft;

        const flexRatio = phiMn_kft > 0 ? Mu2_kft / phiMn_kft : Infinity;

        // Axial capacity
        const Pn_lb = 0.85 * fc_psi * Ag_in2 + As_in2 * fy_psi;
        const Pn_k = Pn_lb / 1000.0;
        const phiPn_k = phi_axial * Pn_k;
        const axialRatio = phiPn_k > 0 ? Pu_k / phiPn_k : Infinity;

        // Interaction (linear)
        const interaction =
          phiPn_k > 0 && phiMn_kft > 0
            ? Pu_k / phiPn_k + Mu2_kft / phiMn_kft
            : Infinity;

        // Overall status
        let statusText = "";
        let statusClass = "text-sm font-semibold ";
        if (
          !isFinite(flexRatio) ||
          !isFinite(axialRatio) ||
          !isFinite(interaction)
        ) {
          statusText = "Calculation error – please check inputs.";
          statusClass += "text-red-600";
        } else if (
          interaction <= 0.8 &&
          flexRatio <= 0.8 &&
          axialRatio <= 0.8
        ) {
          statusText =
            "OK for preliminary design – all utilization ratios are below 0.8. Refine with full code checks.";
          statusClass += "text-green-600";
        } else if (interaction <= 1.0) {
          statusText =
            "Borderline – interaction ratio is between 0.8 and 1.0. Consider increasing thickness or reinforcement.";
          statusClass += "text-amber-600";
        } else {
          statusText =
            "Not adequate – interaction ratio exceeds 1.0. Increase panel thickness, reinforcement, or reduce loads.";
          statusClass += "text-red-600";
        }
        if (Pu_k >= Pcr_k && Pcr_k > 0) {
          statusText +=
            " Warning: Pu exceeds or approaches Euler critical load; panel is too slender.";
        }
        overallStatus.textContent = statusText;
        overallStatus.className = statusClass;

        // Populate geometry results
        const addItem = (ul, label, value, unit) => {
          const li = document.createElement("li");
          li.textContent = `${label}: ${value} ${unit || ""}`.trim();
          ul.appendChild(li);
        };

        addItem(geomResults, "Panel height h", h_ft.toFixed(2), "ft");
        addItem(geomResults, "Panel width b", b_ft.toFixed(2), "ft");
        addItem(geomResults, "Thickness t", t_in.toFixed(2), "in");
        addItem(
          geomResults,
          "Gross area A_g (per ft width)",
          Ag_ft2.toFixed(3),
          "ft²"
        );
        addItem(
          geomResults,
          "Moment of inertia I_g (per ft width)",
          Ig_ft4.toFixed(4),
          "ft⁴"
        );
        addItem(geomResults, "Radius of gyration r", r_ft.toFixed(3), "ft");
        addItem(geomResults, "Slenderness k·h/r", slenderness.toFixed(1), "");

        // Loads & slenderness
        addItem(
          loadResults,
          "Factored axial load P_u",
          Pu_k.toFixed(1),
          "kips"
        );
        addItem(loadResults, "Factored pressure q_u", qu_psf.toFixed(1), "psf");
        addItem(loadResults, "Line load w_u", wu_plf.toFixed(1), "plf");
        addItem(
          loadResults,
          "End condition",
          endCond === "pf"
            ? "Pinned–fixed (cantilever)"
            : endCond === "pp"
            ? "Pinned–pinned"
            : "Fixed–fixed",
          ""
        );
        addItem(
          loadResults,
          "Unmagnified moment M_u",
          Mu_kft.toFixed(2),
          "kip-ft"
        );
        addItem(
          loadResults,
          "Euler critical load P_cr",
          isFinite(Pcr_k) ? Pcr_k.toFixed(1) : "—",
          "kips"
        );
        addItem(
          loadResults,
          "Moment magnifier δ",
          isFinite(delta) ? delta.toFixed(2) : "—",
          ""
        );
        addItem(
          loadResults,
          "Magnified moment M_u,2",
          Mu2_kft.toFixed(2),
          "kip-ft"
        );

        // Flexure
        addItem(
          flexureResults,
          "Steel area A_s (per ft width)",
          As_in2.toFixed(3),
          "in²"
        );
        addItem(flexureResults, "Effective depth d", d_in.toFixed(2), "in");
        addItem(
          flexureResults,
          "Nominal moment M_n",
          Mn_kft.toFixed(2),
          "kip-ft"
        );
        addItem(
          flexureResults,
          "Design strength φM_n",
          phiMn_kft.toFixed(2),
          "kip-ft"
        );
        addItem(
          flexureResults,
          "Flexure utilization M_u,2 / φM_n",
          flexRatio.toFixed(2),
          ""
        );

        // Axial & interaction
        addItem(
          axialResults,
          "Nominal axial strength P_n",
          Pn_k.toFixed(1),
          "kips"
        );
        addItem(
          axialResults,
          "Design strength φP_n",
          phiPn_k.toFixed(1),
          "kips"
        );
        addItem(
          axialResults,
          "Axial utilization P_u / φP_n",
          axialRatio.toFixed(2),
          ""
        );
        addItem(
          axialResults,
          "Interaction ratio P/φP_n + M/φM_n",
          interaction.toFixed(2),
          ""
        );
      }

      document
        .getElementById("runCalc")
        .addEventListener("click", runTiltUpCalc);
