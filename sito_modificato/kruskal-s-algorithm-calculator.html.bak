    <script>
      // --- Graph data structures ---
      const vertices = new Map(); // id -> {id, x, y}
      const edges = []; // {id, u, v, w}
      let edgeIdCounter = 1;
      let selectedVertexForEdge = null;

      // Kruskal state
      let kruskalSteps = [];
      let currentStepIndex = -1;
      let mstEdgeIds = new Set();
      let rejectedEdgeIds = new Set();

      // DOM elements
      const canvas = document.getElementById("graph-canvas");
      const edgeTableBody = document.getElementById("edge-table-body");
      const summaryVertices = document.getElementById("summary-vertices");
      const summaryEdges = document.getElementById("summary-edges");
      const summaryMstEdges = document.getElementById("summary-mst-edges");
      const summaryMstWeight = document.getElementById("summary-mst-weight");
      const summaryNote = document.getElementById("summary-note");
      const stepLog = document.getElementById("step-log");
      const stepIndicator = document.getElementById("step-indicator");
      const stepPrevBtn = document.getElementById("step-prev-btn");
      const stepNextBtn = document.getElementById("step-next-btn");
      const resetStepsBtn = document.getElementById("reset-steps-btn");

      // Utility
      function sanitizeLabel(label) {
        return label.trim();
      }

      function addVertex(id, x = null, y = null) {
        id = sanitizeLabel(id);
        if (!id || vertices.has(id)) return false;
        if (x === null || y === null) {
          // place roughly in a circle
          const n = vertices.size;
          const angle = (2 * Math.PI * n) / Math.max(1, n + 3);
          const cx = 300,
            cy = 180,
            r = 120;
          x = cx + r * Math.cos(angle);
          y = cy + r * Math.sin(angle);
        }
        vertices.set(id, { id, x, y });
        renderGraph();
        updateSummary();
        return true;
      }

      function addEdge(u, v, w) {
        u = sanitizeLabel(u);
        v = sanitizeLabel(v);
        if (!u || !v || u === v || isNaN(w)) return false;
        if (!vertices.has(u)) addVertex(u);
        if (!vertices.has(v)) addVertex(v);
        edges.push({ id: edgeIdCounter++, u, v, w: Number(w) });
        renderGraph();
        updateEdgeTable();
        updateSummary();
        return true;
      }

      function removeEdge(id) {
        const idx = edges.findIndex((e) => e.id === id);
        if (idx >= 0) {
          edges.splice(idx, 1);
          renderGraph();
          updateEdgeTable();
          updateSummary();
        }
      }

      // --- Rendering ---
      function renderGraph() {
        while (canvas.firstChild) canvas.removeChild(canvas.firstChild);

        // Draw edges
        edges.forEach((edge) => {
          const u = vertices.get(edge.u);
          const v = vertices.get(edge.v);
          if (!u || !v) return;
          const line = document.createElementNS(
            "http://www.w3.org/2000/svg",
            "line"
          );
          line.setAttribute("x1", u.x);
          line.setAttribute("y1", u.y);
          line.setAttribute("x2", v.x);
          line.setAttribute("y2", v.y);
          line.setAttribute("stroke-width", mstEdgeIds.has(edge.id) ? 3 : 2);
          let color = "#94a3b8"; // default gray
          if (mstEdgeIds.has(edge.id)) color = "#059669"; // green
          else if (rejectedEdgeIds.has(edge.id)) color = "#dc2626"; // red
          line.setAttribute("stroke", color);
          line.classList.add("edge-line");
          canvas.appendChild(line);

          // weight label
          const label = document.createElementNS(
            "http://www.w3.org/2000/svg",
            "text"
          );
          label.setAttribute("x", (u.x + v.x) / 2);
          label.setAttribute("y", (u.y + v.y) / 2 - 4);
          label.setAttribute("text-anchor", "middle");
          label.setAttribute("fill", "#1f2933");
          label.classList.add("edge-weight-label");
          label.textContent = edge.w;
          canvas.appendChild(label);
        });

        // Draw vertices
        vertices.forEach((v) => {
          const g = document.createElementNS("http://www.w3.org/2000/svg", "g");
          g.classList.add("node");
          g.setAttribute("data-id", v.id);
          g.setAttribute("transform", `translate(${v.x},${v.y})`);

          const circle = document.createElementNS(
            "http://www.w3.org/2000/svg",
            "circle"
          );
          circle.setAttribute("r", 14);
          circle.setAttribute("fill", "#2563eb");
          circle.setAttribute("stroke", "#1d4ed8");
          circle.setAttribute("stroke-width", 2);
          g.appendChild(circle);

          const text = document.createElementNS(
            "http://www.w3.org/2000/svg",
            "text"
          );
          text.setAttribute("text-anchor", "middle");
          text.setAttribute("dy", "0.35em");
          text.setAttribute("fill", "#ffffff");
          text.textContent = v.id;
          g.appendChild(text);

          canvas.appendChild(g);
        });

        attachNodeEvents();
      }

      function updateEdgeTable(sorted = false) {
        edgeTableBody.innerHTML = "";
        const list = sorted ? [...edges].sort((a, b) => a.w - b.w) : edges;
        list.forEach((edge, idx) => {
          const tr = document.createElement("tr");
          tr.className = "border-t";

          const statusClass = mstEdgeIds.has(edge.id)
            ? "text-emerald-700 font-semibold"
            : rejectedEdgeIds.has(edge.id)
            ? "text-red-600"
            : "text-gray-700";

          tr.innerHTML = `
        <td class="px-2 py-1">${idx + 1}</td>
        <td class="px-2 py-1">${edge.u}</td>
        <td class="px-2 py-1">${edge.v}</td>
        <td class="px-2 py-1 ${statusClass}">${edge.w}</td>
        <td class="px-2 py-1 text-right">
          <button data-edge-id="${
            edge.id
          }" class="text-xs text-red-600 hover:underline">Delete</button>
        </td>
      `;
          edgeTableBody.appendChild(tr);
        });

        edgeTableBody
          .querySelectorAll("button[data-edge-id]")
          .forEach((btn) => {
            btn.addEventListener("click", () => {
              const id = Number(btn.getAttribute("data-edge-id"));
              removeEdge(id);
              resetKruskalState();
            });
          });
      }

      function updateSummary() {
        summaryVertices.textContent = vertices.size;
        summaryEdges.textContent = edges.length;
        summaryMstEdges.textContent = mstEdgeIds.size;
        const totalWeight = edges
          .filter((e) => mstEdgeIds.has(e.id))
          .reduce((sum, e) => sum + e.w, 0);
        summaryMstWeight.textContent = mstEdgeIds.size ? totalWeight : "–";

        // connectivity note (simple heuristic)
        if (
          mstEdgeIds.size &&
          mstEdgeIds.size < Math.max(0, vertices.size - 1)
        ) {
          summaryNote.textContent =
            "Graph is disconnected: showing a minimum spanning forest (one MST per component).";
          summaryNote.classList.remove("hidden");
        } else {
          summaryNote.classList.add("hidden");
        }
      }

      // --- Canvas interactions (click to add vertex / edge, drag vertices) ---
      let draggingNodeId = null;
      let dragOffset = { x: 0, y: 0 };

      function svgPoint(evt) {
        const pt = canvas.createSVGPoint();
        pt.x = evt.clientX;
        pt.y = evt.clientY;
        const svgP = pt.matrixTransform(canvas.getScreenCTM().inverse());
        return { x: svgP.x, y: svgP.y };
      }

      canvas.addEventListener("dblclick", (evt) => {
        const { x, y } = svgPoint(evt);
        const label = prompt("Vertex label (e.g. A):");
        if (!label) return;
        if (!addVertex(label, x, y)) {
          alert("Invalid or duplicate vertex label.");
        } else {
          resetKruskalState();
        }
      });

      function attachNodeEvents() {
        canvas.querySelectorAll(".node").forEach((nodeEl) => {
          const id = nodeEl.getAttribute("data-id");

          nodeEl.addEventListener("mousedown", (evt) => {
            if (evt.button !== 0) return;
            draggingNodeId = id;
            const { x, y } = svgPoint(evt);
            const v = vertices.get(id);
            dragOffset.x = v.x - x;
            dragOffset.y = v.y - y;
          });

          nodeEl.addEventListener("click", (evt) => {
            evt.stopPropagation();
            if (draggingNodeId) return;
            handleNodeClickForEdge(id);
          });
        });
      }

      window.addEventListener("mousemove", (evt) => {
        if (!draggingNodeId) return;
        const { x, y } = svgPoint(evt);
        const v = vertices.get(draggingNodeId);
        v.x = x + dragOffset.x;
        v.y = y + dragOffset.y;
        renderGraph();
      });

      window.addEventListener("mouseup", () => {
        draggingNodeId = null;
      });

      function handleNodeClickForEdge(id) {
        if (!selectedVertexForEdge) {
          selectedVertexForEdge = id;
        } else if (selectedVertexForEdge === id) {
          selectedVertexForEdge = null;
        } else {
          const w = prompt(
            `Weight for edge ${selectedVertexForEdge} – ${id}:`,
            "1"
          );
          if (w !== null && !isNaN(w) && w !== "") {
            addEdge(selectedVertexForEdge, id, Number(w));
            resetKruskalState();
          }
          selectedVertexForEdge = null;
        }
      }

      // --- Kruskal algorithm implementation ---
      function runKruskal() {
        resetKruskalState();
        if (vertices.size === 0 || edges.length === 0) {
          alert("Please add at least one edge.");
          return;
        }

        const sortedEdges = [...edges].sort((a, b) => a.w - b.w);
        const parent = {};
        const rank = {};
        vertices.forEach((_, id) => {
          parent[id] = id;
          rank[id] = 0;
        });

        function find(x) {
          if (parent[x] !== x) parent[x] = find(parent[x]);
          return parent[x];
        }

        function union(x, y) {
          const rx = find(x),
            ry = find(y);
          if (rx === ry) return false;
          if (rank[rx] < rank[ry]) parent[rx] = ry;
          else if (rank[rx] > rank[ry]) parent[ry] = rx;
          else {
            parent[ry] = rx;
            rank[rx]++;
          }
          return true;
        }

        const steps = [];
        const mstIds = new Set();
        const rejectedIds = new Set();

        sortedEdges.forEach((e, idx) => {
          const uRoot = find(e.u);
          const vRoot = find(e.v);
          const willFormCycle = uRoot === vRoot;
          const accepted = !willFormCycle;
          steps.push({
            index: idx,
            edgeId: e.id,
            u: e.u,
            v: e.v,
            w: e.w,
            willFormCycle,
            accepted,
            mstIdsSnapshot: new Set(mstIds),
            rejectedIdsSnapshot: new Set(rejectedIds),
          });
          if (accepted) {
            mstIds.add(e.id);
            union(e.u, e.v);
          } else {
            rejectedIds.add(e.id);
          }
        });

        // final snapshot
        steps.push({
          final: true,
          mstIdsSnapshot: mstIds,
          rejectedIdsSnapshot: rejectedIds,
        });

        kruskalSteps = steps;
        currentStepIndex = -1;
        mstEdgeIds = new Set();
        rejectedEdgeIds = new Set();
        renderGraph();
        updateEdgeTable();
        updateSummary();
        updateStepControls();
        renderStepLogInitial(sortedEdges);
      }

      function applyStep(index) {
        if (index < 0 || index >= kruskalSteps.length) return;
        const step = kruskalSteps[index];
        mstEdgeIds = new Set(step.mstIdsSnapshot || []);
        rejectedEdgeIds = new Set(step.rejectedIdsSnapshot || []);
        renderGraph();
        updateEdgeTable();
        updateSummary();
        renderStepLogAt(index);
        stepIndicator.textContent = step.final
          ? `Completed – ${mstEdgeIds.size} MST edges`
          : `Step ${index + 1} of ${kruskalSteps.length - 1}`;
      }

      function resetKruskalState() {
        kruskalSteps = [];
        currentStepIndex = -1;
        mstEdgeIds = new Set();
        rejectedEdgeIds = new Set();
        renderGraph();
        updateEdgeTable();
        updateSummary();
        stepLog.innerHTML =
          '<p class="text-gray-500 text-xs">Build a graph and click “Run Kruskal” to see the algorithm steps here.</p>';
        stepIndicator.textContent = "No run yet";
        updateStepControls();
      }

      function updateStepControls() {
        const hasSteps = kruskalSteps.length > 0;
        stepPrevBtn.disabled = !hasSteps || currentStepIndex <= 0;
        stepNextBtn.disabled =
          !hasSteps || currentStepIndex >= kruskalSteps.length - 1;
        resetStepsBtn.disabled = !hasSteps;
      }

      function renderStepLogInitial(sortedEdges) {
        stepLog.innerHTML = "";
        const intro = document.createElement("p");
        intro.className = "text-xs text-gray-700 mb-1";
        intro.textContent = `Edges sorted by non-decreasing weight: ${sortedEdges
          .map((e) => `${e.u}${e.v}(${e.w})`)
          .join(", ")}`;
        stepLog.appendChild(intro);
      }

      function renderStepLogAt(index) {
        const step = kruskalSteps[index];
        if (!step || step.final) return;
        const p = document.createElement("p");
        p.className = "text-xs";
        const edgeLabel = `${step.u}–${step.v} (w=${step.w})`;
        if (step.willFormCycle) {
          p.innerHTML = `<span class="font-semibold text-red-600">Reject</span> edge ${edgeLabel} – it would create a cycle.`;
        } else {
          p.innerHTML = `<span class="font-semibold text-emerald-700">Accept</span> edge ${edgeLabel} – it connects two different components.`;
        }
        stepLog.appendChild(p);
        stepLog.scrollTop = stepLog.scrollHeight;
      }

      // --- Event listeners for controls ---
      document
        .getElementById("add-vertex-btn")
        .addEventListener("click", () => {
          const input = document.getElementById("vertex-name");
          const label = input.value;
          if (!label) return;
          if (!addVertex(label)) {
            alert("Invalid or duplicate vertex label.");
          } else {
            resetKruskalState();
            input.value = "";
          }
        });

      document.getElementById("add-edge-btn").addEventListener("click", () => {
        const u = document.getElementById("edge-u").value;
        const v = document.getElementById("edge-v").value;
        const w = parseFloat(document.getElementById("edge-w").value || "1");
        if (!u || !v || isNaN(w)) {
          alert("Please enter valid endpoints and weight.");
          return;
        }
        addEdge(u, v, w);
        resetKruskalState();
      });

      document
        .getElementById("run-kruskal-btn")
        .addEventListener("click", () => {
          runKruskal();
          currentStepIndex = 0;
          applyStep(currentStepIndex);
          updateStepControls();
        });

      stepNextBtn.addEventListener("click", () => {
        if (currentStepIndex < kruskalSteps.length - 1) {
          currentStepIndex++;
          applyStep(currentStepIndex);
          updateStepControls();
        }
      });

      stepPrevBtn.addEventListener("click", () => {
        if (currentStepIndex > 0) {
          currentStepIndex--;
          applyStep(currentStepIndex);
          updateStepControls();
        }
      });

      resetStepsBtn.addEventListener("click", () => {
        if (!kruskalSteps.length) return;
        currentStepIndex = 0;
        applyStep(currentStepIndex);
        stepLog.innerHTML = "";
        updateStepControls();
      });

      document
        .getElementById("clear-graph-btn")
        .addEventListener("click", () => {
          if (!confirm("Clear all vertices and edges?")) return;
          vertices.clear();
          edges.splice(0, edges.length);
          resetKruskalState();
        });

      document
        .getElementById("sort-edges-btn")
        .addEventListener("click", () => {
          updateEdgeTable(true);
        });

      document
        .getElementById("example-graph-btn")
        .addEventListener("click", () => {
          vertices.clear();
          edges.splice(0, edges.length);
          edgeIdCounter = 1;
          // Simple example graph
          ["A", "B", "C", "D", "E"].forEach((v, i) => {
            const angle = (2 * Math.PI * i) / 5;
            const cx = 300,
              cy = 180,
              r = 120;
            addVertex(v, cx + r * Math.cos(angle), cy + r * Math.sin(angle));
          });
          addEdge("A", "B", 4);
          addEdge("A", "C", 2);
          addEdge("B", "C", 5);
          addEdge("B", "D", 10);
          addEdge("C", "D", 3);
          addEdge("C", "E", 4);
          addEdge("D", "E", 1);
          resetKruskalState();
        });

      document
        .getElementById("import-edge-list-btn")
        .addEventListener("click", () => {
          const text = document
            .getElementById("edge-list-textarea")
            .value.trim();
          if (!text) return;
          vertices.clear();
          edges.splice(0, edges.length);
          edgeIdCounter = 1;
          const lines = text.split(/\r?\n/);
          for (const line of lines) {
            const parts = line.trim().split(/\s+/);
            if (parts.length < 3) continue;
            const [u, v, wStr] = parts;
            const w = parseFloat(wStr);
            if (isNaN(w)) continue;
            addEdge(u, v, w);
          }
          resetKruskalState();
        });

      document
        .getElementById("export-edge-list-btn")
        .addEventListener("click", () => {
          const lines = edges.map((e) => `${e.u} ${e.v} ${e.w}`);
          document.getElementById("edge-list-textarea").value =
            lines.join("\n");
        });

      // Initial render
      renderGraph();
      updateEdgeTable();
      updateSummary();

      // Mobile menu script (simple)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", () => {
          const menu = document.getElementById("mobile-menu");
          const expanded = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !expanded);
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
