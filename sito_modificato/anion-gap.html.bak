<script>
  // Toggle albumin input visibility
  const useAlbuminCheckbox = document.getElementById('use-albumin');
  const albuminRow = document.getElementById('albumin-row');
  useAlbuminCheckbox.addEventListener('change', () => {
    if (useAlbuminCheckbox.checked) {
      albuminRow.classList.remove('hidden');
    } else {
      albuminRow.classList.add('hidden');
    }
  });

  function parseValue(id) {
    const el = document.getElementById(id);
    if (!el) return null;
    const v = el.value.trim();
    if (v === '') return null;
    const num = Number(v);
    return isNaN(num) ? null : num;
  }

  function classifyAG(ag) {
    if (ag == null || isNaN(ag)) return { label: '–', color: 'bg-gray-100 text-gray-700' };
    if (ag < 8) return { label: 'Low', color: 'bg-blue-100 text-blue-800' };
    if (ag <= 12) return { label: 'Normal', color: 'bg-green-100 text-green-800' };
    return { label: 'High', color: 'bg-red-100 text-red-800' };
  }

  function buildInterpretation(ag, agcorr, hco3) {
    if (ag == null || isNaN(ag) || hco3 == null || isNaN(hco3)) {
      return 'Enter valid values for Na⁺, Cl⁻, and HCO₃⁻ to see an interpretation.';
    }

    const agUsed = (agcorr != null && !isNaN(agcorr)) ? agcorr : ag;
    const agLabel = agUsed > 12 ? 'high' : (agUsed < 8 ? 'low' : 'normal');
    let text = `Using the ${agcorr != null && !isNaN(agcorr) ? 'albumin-corrected' : 'uncorrected'} anion gap, the value is approximately ${agUsed.toFixed(1)} mEq/L, which is ${agLabel} relative to a typical 8–12 mEq/L reference range. `;

    if (hco3 < 22) {
      if (agUsed > 12) {
        text += 'This pattern is compatible with a high anion gap metabolic acidosis (e.g. lactic acidosis, ketoacidosis, renal failure, or certain toxin ingestions).';
      } else if (agUsed >= 8 && agUsed <= 12) {
        text += 'With low bicarbonate and a normal anion gap, consider a normal anion gap (hyperchloremic) metabolic acidosis (e.g. diarrhea, renal tubular acidosis), but always confirm with full acid–base analysis.';
      } else {
        text += 'Low bicarbonate with a low anion gap is unusual and may reflect measurement error, hypoalbuminemia, or mixed acid–base disorders.';
      }
    } else if (hco3 > 28) {
      text += 'Bicarbonate is elevated, which may indicate a metabolic alkalosis or compensation for chronic respiratory acidosis; the anion gap is less central in this setting.';
    } else {
      text += 'Bicarbonate is within a typical reference range; correlate with pH, PaCO₂, and clinical context to determine whether any acid–base disorder is present.';
    }

    text += ' Always interpret these results together with arterial or venous blood gases, clinical findings, and your laboratory’s reference ranges.';
    return text;
  }

  document.getElementById('calculate-btn').addEventListener('click', () => {
    const na = parseValue('na');
    const k = parseValue('k');
    const cl = parseValue('cl');
    const hco3 = parseValue('hco3');

    if (na == null || cl == null || hco3 == null) {
      alert('Please enter valid values for Na⁺, Cl⁻, and HCO₃⁻.');
      return;
    }

    const ag = na - (cl + hco3);
    const agk = (na + (k != null ? k : 0)) - (cl + hco3);

    let agcorr = null;
    let albuminUsed = null;
    if (useAlbuminCheckbox.checked) {
      let alb = parseValue('albumin');
      if (alb == null) alb = 4.0;
      albuminUsed = alb;
      agcorr = ag + 2.5 * (4.0 - alb);
    }

    // Update numeric outputs
    const agEl = document.getElementById('ag-value');
    const agkEl = document.getElementById('agk-value');
    const agStatusEl = document.getElementById('ag-status');
    const agkStatusEl = document.getElementById('agk-status');
    const agcorrContainer = document.getElementById('corrected-container');
    const agcorrEl = document.getElementById('agcorr-value');
    const agcorrStatusEl = document.getElementById('agcorr-status');
    const albuminUsedEl = document.getElementById('albumin-used');
    const resultsSection = document.getElementById('results-section');
    const interpEl = document.getElementById('interpretation-text');

    agEl.textContent = ag.toFixed(1) + ' mEq/L';
    const agClass = classifyAG(ag);
    agStatusEl.textContent = agClass.label;
    agStatusEl.className = 'badge ' + agClass.color;

    agkEl.textContent = agk.toFixed(1) + ' mEq/L';
    const agkClass = classifyAG(agk);
    agkStatusEl.textContent = agkClass.label;
    agkStatusEl.className = 'badge ' + agkClass.color;

    if (agcorr != null && albuminUsed != null) {
      agcorrContainer.classList.remove('hidden');
      agcorrEl.textContent = agcorr.toFixed(1) + ' mEq/L';
      albuminUsedEl.textContent = albuminUsed.toFixed(1);
      const agcorrClass = classifyAG(agcorr);
      agcorrStatusEl.textContent = agcorrClass.label;
      agcorrStatusEl.className = 'badge ' + agcorrClass.color;
    } else {
      agcorrContainer.classList.add('hidden');
    }

    interpEl.textContent = buildInterpretation(ag, agcorr, hco3);
    resultsSection.classList.remove('hidden');
    resultsSection.scrollIntoView({ behavior: 'smooth', block: 'start' });
  });

  document.getElementById('reset-btn').addEventListener('click', () => {
    document.getElementById('anion-gap-form').reset();
    document.getElementById('results-section').classList.add('hidden');
    document.getElementById('corrected-container').classList.add('hidden');
    albuminRow.classList.add('hidden');

    // Reset badges
    ['ag-status', 'agk-status', 'agcorr-status'].forEach(id => {
      const el = document.getElementById(id);
      if (el) {
        el.textContent = '–';
        el.className = 'badge bg-gray-100 text-gray-700';
      }
    });

    document.getElementById('ag-value').textContent = '–';
    document.getElementById('agk-value').textContent = '–';
    document.getElementById('agcorr-value').textContent = '–';
    document.getElementById('interpretation-text').textContent = '';
  });
</script>

