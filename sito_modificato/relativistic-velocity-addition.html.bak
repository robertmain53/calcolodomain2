    <script>
      // Constants
      const C = 299792458; // speed of light in m/s

      function toFractionOfC(value, unit) {
        if (!isFinite(value)) return NaN;
        switch (unit) {
          case "fraction":
            return value; // already v/c
          case "ms":
            return value / C;
          case "kmh":
            return (value * 1000) / 3600 / C;
          default:
            return NaN;
        }
      }

      function fromFractionOfC(beta, unit) {
        const v = beta * C;
        switch (unit) {
          case "fraction":
            return beta;
          case "ms":
            return v;
          case "kmh":
            return v * 3.6;
          default:
            return NaN;
        }
      }

      function gamma(beta) {
        const b2 = beta * beta;
        if (b2 >= 1) return Infinity;
        return 1 / Math.sqrt(1 - b2);
      }

      function rapidity(beta) {
        const b = Math.max(Math.min(beta, 0.9999999999), -0.9999999999);
        return 0.5 * Math.log((1 + b) / (1 - b)); // artanh
      }

      function formatNumber(x, digits = 4) {
        if (!isFinite(x)) return "—";
        const ax = Math.abs(x);
        if (ax !== 0 && (ax < 1e-3 || ax >= 1e5)) {
          return x.toExponential(digits - 1);
        }
        return x.toFixed(digits);
      }

      function showError(msg) {
        const el = document.getElementById("error-msg");
        el.textContent = msg;
        el.classList.remove("hidden");
      }

      function clearError() {
        const el = document.getElementById("error-msg");
        el.textContent = "";
        el.classList.add("hidden");
      }

      function updateUnitLabels() {
        const unit = document.getElementById("unit-select").value;
        const label =
          unit === "fraction" ? "c" : unit === "ms" ? "m/s" : "km/h";
        ["u", "v", "ux", "uy", "v2d"].forEach((prefix) => {
          const span = document.getElementById(prefix + "-unit-label");
          if (span) span.textContent = label;
        });
      }

      function swapVelocities() {
        const mode = document.getElementById("mode-select").value;
        if (mode === "1d") {
          const uEl = document.getElementById("u-input");
          const vEl = document.getElementById("v-input");
          const tmp = uEl.value;
          uEl.value = vEl.value;
          vEl.value = tmp;
        } else {
          // swap frame and x-component in 2D as a simple helper
          const uxEl = document.getElementById("ux-input");
          const vEl = document.getElementById("v2d-input");
          const tmp = uxEl.value;
          uxEl.value = vEl.value;
          vEl.value = tmp;
        }
      }

      function resetAll() {
        ["u-input", "v-input", "ux-input", "uy-input", "v2d-input"].forEach(
          (id) => {
            const el = document.getElementById(id);
            if (el) el.value = "";
          }
        );
        clearError();
        document.getElementById("results-container").classList.add("hidden");
      }

      function compute1D() {
        const unit = document.getElementById("unit-select").value;
        const uVal = parseFloat(document.getElementById("u-input").value);
        const vVal = parseFloat(document.getElementById("v-input").value);

        if (isNaN(uVal) || isNaN(vVal)) {
          showError("Please enter both u and v.");
          return;
        }

        const betaU = toFractionOfC(uVal, unit);
        const betaV = toFractionOfC(vVal, unit);

        if (!isFinite(betaU) || !isFinite(betaV)) {
          showError("Invalid input values.");
          return;
        }

        if (Math.abs(betaU) >= 1 || Math.abs(betaV) >= 1) {
          showError(
            "Each speed must be strictly less than the speed of light (|v| < c)."
          );
          return;
        }

        const denom = 1 + betaU * betaV;
        if (Math.abs(denom) < 1e-12) {
          showError(
            "Combination is too close to the speed of light; denominator approaches zero."
          );
          return;
        }

        const betaW = (betaU + betaV) / denom;
        const betaClassical = betaU + betaV;

        // Fill results
        const unitLabel =
          unit === "fraction" ? "c" : unit === "ms" ? "m/s" : "km/h";

        const wRelVal = fromFractionOfC(betaW, unit);
        const wClassVal = fromFractionOfC(betaClassical, unit);

        document.getElementById("w-rel-label").textContent =
          unit === "fraction"
            ? `${formatNumber(betaW, 5)} c`
            : `${formatNumber(wRelVal, 4)} ${unitLabel}`;

        document.getElementById("w-rel-alt").textContent =
          unit === "fraction"
            ? `${formatNumber(fromFractionOfC(betaW, "ms"), 4)} m/s`
            : `${formatNumber(betaW, 5)} c`;

        document.getElementById("w-classical-label").textContent =
          unit === "fraction"
            ? `${formatNumber(betaClassical, 5)} c`
            : `${formatNumber(wClassVal, 4)} ${unitLabel}`;

        const exceeds = Math.abs(betaClassical) > 1;
        document.getElementById("w-classical-note").textContent = exceeds
          ? "Classical sum exceeds c and is not physically allowed in relativity."
          : "At these speeds, the classical sum is still below c but slightly inaccurate.";

        // Gammas
        document.getElementById("gamma-u").textContent = formatNumber(
          gamma(betaU),
          5
        );
        document.getElementById("gamma-v").textContent = formatNumber(
          gamma(betaV),
          5
        );
        document.getElementById("gamma-w").textContent = formatNumber(
          gamma(betaW),
          5
        );

        // Rapidities
        const etaU = rapidity(betaU);
        const etaV = rapidity(betaV);
        const etaW = rapidity(betaW);

        document.getElementById("rapidity-u").textContent = formatNumber(
          etaU,
          5
        );
        document.getElementById("rapidity-v").textContent = formatNumber(
          etaV,
          5
        );
        document.getElementById("rapidity-w").textContent = formatNumber(
          etaW,
          5
        );

        document.getElementById("results-1d").classList.remove("hidden");
        document.getElementById("results-2d").classList.add("hidden");
        document.getElementById("results-container").classList.remove("hidden");
      }

      function compute2D() {
        const unit = document.getElementById("unit-select").value;
        const uxVal = parseFloat(document.getElementById("ux-input").value);
        const uyVal = parseFloat(document.getElementById("uy-input").value);
        const vVal = parseFloat(document.getElementById("v2d-input").value);

        if (isNaN(uxVal) || isNaN(uyVal) || isNaN(vVal)) {
          showError("Please enter uₓ, uᵧ, and v.");
          return;
        }

        const betaUx = toFractionOfC(uxVal, unit);
        const betaUy = toFractionOfC(uyVal, unit);
        const betaV = toFractionOfC(vVal, unit);

        if (!isFinite(betaUx) || !isFinite(betaUy) || !isFinite(betaV)) {
          showError("Invalid input values.");
          return;
        }

        const speed2 = betaUx * betaUx + betaUy * betaUy;
        if (speed2 >= 1) {
          showError(
            "The object speed in S must be less than c: √(uₓ² + uᵧ²) < c."
          );
          return;
        }
        if (Math.abs(betaV) >= 1) {
          showError("The frame speed v must satisfy |v| < c.");
          return;
        }

        const denom = 1 - betaUx * betaV;
        if (Math.abs(denom) < 1e-12) {
          showError(
            "Combination is too close to the speed of light; denominator approaches zero."
          );
          return;
        }

        const gammaV = gamma(betaV);

        const betaUxPrime = (betaUx - betaV) / denom;
        const betaUyPrime = betaUy / (gammaV * denom);

        const betaPrimeMag = Math.sqrt(
          betaUxPrime * betaUxPrime + betaUyPrime * betaUyPrime
        );

        // Classical (Galilean) transform for comparison
        const betaUxClass = betaUx - betaV;
        const betaUyClass = betaUy;
        const betaClassMag = Math.sqrt(
          betaUxClass * betaUxClass + betaUyClass * betaUyClass
        );

        const unitLabel =
          unit === "fraction" ? "c" : unit === "ms" ? "m/s" : "km/h";

        // Relativistic components
        const uxPrimeDisp =
          unit === "fraction"
            ? `${formatNumber(betaUxPrime, 5)} c`
            : `${formatNumber(
                fromFractionOfC(betaUxPrime, unit),
                4
              )} ${unitLabel}`;
        const uyPrimeDisp =
          unit === "fraction"
            ? `${formatNumber(betaUyPrime, 5)} c`
            : `${formatNumber(
                fromFractionOfC(betaUyPrime, unit),
                4
              )} ${unitLabel}`;

        document.getElementById(
          "uprime-components"
        ).innerHTML = `u'<sub>x</sub> = ${uxPrimeDisp}, u'<sub>y</sub> = ${uyPrimeDisp}`;

        const magDisp =
          unit === "fraction"
            ? `${formatNumber(betaPrimeMag, 5)} c`
            : `${formatNumber(
                fromFractionOfC(betaPrimeMag, unit),
                4
              )} ${unitLabel}`;

        document.getElementById(
          "uprime-mag"
        ).textContent = `|u'| = ${magDisp} (always < c in relativity)`;

        // Classical components
        const uxClassDisp =
          unit === "fraction"
            ? `${formatNumber(betaUxClass, 5)} c`
            : `${formatNumber(
                fromFractionOfC(betaUxClass, unit),
                4
              )} ${unitLabel}`;
        const uyClassDisp =
          unit === "fraction"
            ? `${formatNumber(betaUyClass, 5)} c`
            : `${formatNumber(
                fromFractionOfC(betaUyClass, unit),
                4
              )} ${unitLabel}`;

        document.getElementById(
          "uprime-classical-components"
        ).innerHTML = `u'<sub>x,classical</sub> = ${uxClassDisp}, u'<sub>y,classical</sub> = ${uyClassDisp}`;

        const magClassDisp =
          unit === "fraction"
            ? `${formatNumber(betaClassMag, 5)} c`
            : `${formatNumber(
                fromFractionOfC(betaClassMag, unit),
                4
              )} ${unitLabel}`;

        const exceeds = betaClassMag > 1;
        document.getElementById("uprime-classical-mag").textContent = exceeds
          ? `|u'| ≈ ${magClassDisp} (classical result can exceed c and is unphysical).`
          : `|u'| ≈ ${magClassDisp} (classical result differs slightly from relativity).`;

        // Direction angle in S'
        const angleRad = Math.atan2(betaUyPrime, betaUxPrime);
        const angleDeg = (angleRad * 180) / Math.PI;
        document.getElementById(
          "angle-info"
        ).textContent = `Direction in S': θ' = ${formatNumber(
          angleDeg,
          4
        )}° measured from the +x' axis.`;

        document.getElementById("results-1d").classList.add("hidden");
        document.getElementById("results-2d").classList.remove("hidden");
        document.getElementById("results-container").classList.remove("hidden");
      }

      document.addEventListener("DOMContentLoaded", () => {
        updateUnitLabels();

        document
          .getElementById("unit-select")
          .addEventListener("change", () => {
            updateUnitLabels();
          });

        document
          .getElementById("mode-select")
          .addEventListener("change", (e) => {
            const mode = e.target.value;
            if (mode === "1d") {
              document.getElementById("inputs-1d").classList.remove("hidden");
              document.getElementById("inputs-2d").classList.add("hidden");
            } else {
              document.getElementById("inputs-1d").classList.add("hidden");
              document.getElementById("inputs-2d").classList.remove("hidden");
            }
            clearError();
            document
              .getElementById("results-container")
              .classList.add("hidden");
          });

        document.getElementById("calc-btn").addEventListener("click", () => {
          clearError();
          const mode = document.getElementById("mode-select").value;
          if (mode === "1d") {
            compute1D();
          } else {
            compute2D();
          }
        });

        document.getElementById("swap-btn").addEventListener("click", () => {
          swapVelocities();
        });

        document.getElementById("reset-btn").addEventListener("click", (e) => {
          e.preventDefault();
          resetAll();
        });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
