            <script>
              document.addEventListener('DOMContentLoaded', () => {
                const inputs = {
                    l: document.getElementById('l-input'),
                    ml: document.getElementById('ml-input'),
                    us_gal: document.getElementById('us-gal-input'),
                    us_qt: document.getElementById('us-qt-input'),
                    us_pt: document.getElementById('us-pt-input'),
                    us_cup: document.getElementById('us-cup-input'),
                    us_floz: document.getElementById('us-floz-input'),
                    uk_gal: document.getElementById('uk-gal-input')
                };

                // Conversion factors TO 1 LITER (our base unit)
                const FACTORS_TO_LITER = {
                    l: 1,
                    ml: 1000,
                    us_gal: 0.264172052,
                    us_qt: 1.05668821,
                    us_pt: 2.11337642,
                    us_cup: 4.22675284, // US Customary Cup
                    us_floz: 33.8140227,
                    uk_gal: 0.219969248
                };
                
                function formatNum(num) {
                    if (isNaN(num) || num === 0) return '';
                    return parseFloat(num.toPrecision(7)).toString();
                }

                function update(sourceUnit, sourceValue) {
                    if (isNaN(sourceValue) || sourceValue === 0) {
                        Object.values(inputs).forEach(input => input.value = '');
                        return;
                    }

                    // 1. Converti l'input sorgente in Litri (base unit)
                    const baseValueLiters = sourceValue / FACTORS_TO_LITER[sourceUnit];

                    // 2. Aggiorna tutti gli altri campi
                    for (const [unit, input] of Object.entries(inputs)) {
                        if (unit !== sourceUnit) {
                            const convertedValue = baseValueLiters * FACTORS_TO_LITER[unit];
                            input.value = formatNum(convertedValue);
                        }
                    }
                }

                Object.values(inputs).forEach(input => {
                  input.addEventListener('input', (e) => {
                    const sourceUnit = e.target.dataset.unit;
                    const value = parseFloat(e.target.value);
                    update(sourceUnit, value);
                  });
                });
              });
            </script>
            
