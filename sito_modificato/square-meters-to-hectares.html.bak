    <script>
      (function () {
        const input = document.getElementById("input-value");
        const output = document.getElementById("output-value");
        const outputRounded = document.getElementById("output-rounded");
        const decimalsSelect = document.getElementById("decimals");
        const btnM2ToHa = document.getElementById("btn-m2-to-ha");
        const btnHaToM2 = document.getElementById("btn-ha-to-m2");
        const swapBtn = document.getElementById("swap-btn");
        const resetBtn = document.getElementById("reset-btn");
        const inputLabel = document.getElementById("input-label");
        const quickTableBody = document.getElementById("quick-table-body");

        let direction = "m2-to-ha"; // or 'ha-to-m2'

        function formatNumber(value, decimals) {
          if (!isFinite(value)) return "";
          const factor = Math.pow(10, decimals);
          const rounded = Math.round(value * factor) / factor;
          return rounded.toLocaleString(undefined, {
            minimumFractionDigits: 0,
            maximumFractionDigits: decimals,
          });
        }

        function convert(value, dir) {
          if (!isFinite(value)) return NaN;
          if (dir === "m2-to-ha") {
            return value / 10000;
          } else {
            return value * 10000;
          }
        }

        function updateLabels() {
          if (direction === "m2-to-ha") {
            inputLabel.textContent = "Square meters (m²)";
            input.placeholder = "Enter area in square meters";
          } else {
            inputLabel.textContent = "Hectares (ha)";
            input.placeholder = "Enter area in hectares";
          }
        }

        function updateToggleButtons() {
          if (direction === "m2-to-ha") {
            btnM2ToHa.classList.add("toggle-btn-active");
            btnHaToM2.classList.remove("toggle-btn-active");
            btnM2ToHa.setAttribute("aria-selected", "true");
            btnHaToM2.setAttribute("aria-selected", "false");
          } else {
            btnHaToM2.classList.add("toggle-btn-active");
            btnM2ToHa.classList.remove("toggle-btn-active");
            btnHaToM2.setAttribute("aria-selected", "true");
            btnM2ToHa.setAttribute("aria-selected", "false");
          }
        }

        function handleConversion() {
          const raw = input.value.trim();
          if (raw === "") {
            output.textContent = "";
            outputRounded.textContent = "";
            return;
          }
          const value = parseFloat(raw);
          if (!isFinite(value)) {
            output.textContent = "Invalid number";
            outputRounded.textContent = "";
            return;
          }
          const decimals = parseInt(decimalsSelect.value, 10) || 2;
          const result = convert(value, direction);
          output.textContent = result.toLocaleString(undefined, {
            maximumFractionDigits: 10,
          });
          outputRounded.textContent = formatNumber(result, decimals);
        }

        function populateQuickTable() {
          const rows = [
            100, 250, 500, 1000, 2000, 5000, 10000, 20000, 50000, 100000,
          ];
          quickTableBody.innerHTML = "";
          rows.forEach((m2) => {
            const ha = m2 / 10000;
            const tr = document.createElement("tr");
            tr.innerHTML = `
          <td class="px-3 py-1 border-b">${m2.toLocaleString()} m²</td>
          <td class="px-3 py-1 border-b">${ha.toLocaleString(undefined, {
            maximumFractionDigits: 4,
          })} ha</td>
        `;
            quickTableBody.appendChild(tr);
          });
        }

        btnM2ToHa.addEventListener("click", () => {
          direction = "m2-to-ha";
          updateLabels();
          updateToggleButtons();
          handleConversion();
        });

        btnHaToM2.addEventListener("click", () => {
          direction = "ha-to-m2";
          updateLabels();
          updateToggleButtons();
          handleConversion();
        });

        swapBtn.addEventListener("click", () => {
          direction = direction === "m2-to-ha" ? "ha-to-m2" : "m2-to-ha";
          updateLabels();
          updateToggleButtons();
          handleConversion();
        });

        [input, decimalsSelect].forEach((el) => {
          el.addEventListener("input", handleConversion);
          el.addEventListener("change", handleConversion);
        });

        resetBtn.addEventListener("click", () => {
          setTimeout(() => {
            output.textContent = "";
            outputRounded.textContent = "";
          }, 0);
        });

        populateQuickTable();
        updateLabels();
        updateToggleButtons();
      })();
    </script>

