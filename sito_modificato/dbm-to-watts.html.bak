<script>
  // Utility: format number in engineering notation with unit
  function formatEngineering(value, unit, decimals = 3) {
    if (!isFinite(value)) return '–';
    if (value === 0) return '0 ' + unit;
    const prefixes = [
      { p: -12, s: 'p' },
      { p: -9,  s: 'n' },
      { p: -6,  s: 'µ' },
      { p: -3,  s: 'm' },
      { p: 0,   s: ''  },
      { p: 3,   s: 'k' },
      { p: 6,   s: 'M' },
      { p: 9,   s: 'G' }
    ];
    const exp = Math.floor(Math.log10(Math.abs(value)));
    let engExp = Math.floor(exp / 3) * 3;
    if (engExp < -12) engExp = -12;
    if (engExp > 9) engExp = 9;
    const prefix = prefixes.find(p => p.p === engExp) || { p: 0, s: '' };
    const scaled = value / Math.pow(10, prefix.p);
    return scaled.toFixed(decimals).replace(/\.?0+$/,'') + ' ' + prefix.s + unit;
  }

  function formatFixed(value, decimals = 3) {
    if (!isFinite(value)) return '–';
    return value.toFixed(decimals).replace(/\.?0+$/,'');
  }

  function computeFromDbm() {
    const dbm = parseFloat(document.getElementById('dbmInput').value);
    if (isNaN(dbm)) {
      document.getElementById('resultNote').textContent = 'Enter a valid dBm value.';
      return;
    }
    const R = parseFloat(document.getElementById('impedanceInput').value);
    if (!isFinite(R) || R <= 0) {
      document.getElementById('resultNote').textContent = 'Impedance must be a positive number.';
      return;
    }
    const P_w = Math.pow(10, (dbm - 30) / 10); // watts
    updateResults(P_w, dbm, R, 'fromDbm');
  }

  function computeFromWatts() {
    const watts = parseFloat(document.getElementById('wattsInput').value);
    if (!isFinite(watts) || watts <= 0) {
      document.getElementById('resultNote').textContent = 'Power in watts must be greater than 0.';
      return;
    }
    const R = parseFloat(document.getElementById('impedanceInput').value);
    if (!isFinite(R) || R <= 0) {
      document.getElementById('resultNote').textContent = 'Impedance must be a positive number.';
      return;
    }
    const dbm = 10 * Math.log10(watts / 0.001);
    updateResults(watts, dbm, R, 'fromWatts');
  }

  function updateResults(P_w, dbm, R, source) {
    const dbw = 10 * Math.log10(P_w); // dBW
    const Vrms = Math.sqrt(P_w * R);
    const Vpeak = Vrms * Math.SQRT2;
    const Irms = Math.sqrt(P_w / R);

    const powerUnit = document.getElementById('powerUnitSelect').value;
    const voltageUnit = document.getElementById('voltageUnitSelect').value;

    let powerDisplay;
    if (powerUnit === 'W') {
      powerDisplay = formatEngineering(P_w, 'W');
    } else if (powerUnit === 'mW') {
      powerDisplay = formatEngineering(P_w * 1e3, 'W'); // show as mW but keep W symbol with prefix
    } else {
      powerDisplay = formatEngineering(P_w * 1e6, 'W');
    }

    let VrmsDisplay, VpeakDisplay;
    if (voltageUnit === 'V') {
      VrmsDisplay = formatEngineering(Vrms, 'V');
      VpeakDisplay = formatEngineering(Vpeak, 'V');
    } else {
      VrmsDisplay = formatEngineering(Vrms * 1e3, 'V');
      VpeakDisplay = formatEngineering(Vpeak * 1e3, 'V');
    }

    document.getElementById('resultPower').textContent = powerDisplay;
    document.getElementById('resultDbm').textContent = formatFixed(dbm, 3) + ' dBm';
    document.getElementById('resultDbw').textContent = formatFixed(dbw, 3) + ' dBW';
    document.getElementById('resultVrms').textContent = VrmsDisplay;
    document.getElementById('resultVpeak').textContent = VpeakDisplay;
    document.getElementById('resultIrms').textContent = formatEngineering(Irms, 'A');

    if (source === 'fromDbm') {
      document.getElementById('wattsInput').value = formatFixed(P_w, 6);
      document.getElementById('resultNote').textContent = 'Converted from dBm to watts using P(W) = 10^((dBm − 30)/10).';
    } else if (source === 'fromWatts') {
      document.getElementById('dbmInput').value = formatFixed(dbm, 3);
      document.getElementById('resultNote').textContent = 'Converted from watts to dBm using dBm = 10·log10(P/1 mW).';
    } else {
      document.getElementById('resultNote').textContent = 'Results updated.';
    }
  }

  function populateReferenceTable() {
    const tbody = document.getElementById('referenceTableBody');
    if (!tbody) return;
    const rows = [];
    const values = [-30,-20,-10,0,3,10,13,20,23,30,33,40,43];
    values.forEach(dbm => {
      const P_w = Math.pow(10, (dbm - 30) / 10);
      const P_mw = P_w * 1000;
      rows.push(
        '<tr>' +
          '<td class="px-3 py-1 mono">' + dbm + '</td>' +
          '<td class="px-3 py-1 mono">' + formatEngineering(P_w, 'W', 4) + '</td>' +
          '<td class="px-3 py-1 mono">' + formatFixed(P_mw, 4) + ' mW</td>' +
        '</tr>'
      );
    });
    tbody.innerHTML = rows.join('');
  }

  document.addEventListener('DOMContentLoaded', function () {
    document.getElementById('btnFromDbm').addEventListener('click', computeFromDbm);
    document.getElementById('btnFromWatts').addEventListener('click', computeFromWatts);

    document.getElementById('powerUnitSelect').addEventListener('change', function () {
      const dbmVal = parseFloat(document.getElementById('dbmInput').value);
      const wattsVal = parseFloat(document.getElementById('wattsInput').value);
      const R = parseFloat(document.getElementById('impedanceInput').value);
      if (isFinite(wattsVal) && wattsVal > 0 && isFinite(R) && R > 0) {
        updateResults(wattsVal, isFinite(dbmVal) ? dbmVal : 10 * Math.log10(wattsVal / 0.001), R, null);
      }
    });

    document.getElementById('voltageUnitSelect').addEventListener('change', function () {
      const dbmVal = parseFloat(document.getElementById('dbmInput').value);
      const wattsVal = parseFloat(document.getElementById('wattsInput').value);
      const R = parseFloat(document.getElementById('impedanceInput').value);
      if (isFinite(wattsVal) && wattsVal > 0 && isFinite(R) && R > 0) {
        updateResults(wattsVal, isFinite(dbmVal) ? dbmVal : 10 * Math.log10(wattsVal / 0.001), R, null);
      }
    });

    // Quick presets
    document.querySelectorAll('[data-preset-dbm]').forEach(btn => {
      btn.addEventListener('click', () => {
        const val = parseFloat(btn.getAttribute('data-preset-dbm'));
        document.getElementById('dbmInput').value = val;
        computeFromDbm();
      });
    });

    populateReferenceTable();
  });

  // Mobile menu script (simple toggle)
  document.addEventListener('DOMContentLoaded', function () {
    const toggle = document.getElementById('mobile-menu-toggle');
    const menu = document.getElementById('mobile-menu');
    if (toggle && menu) {
      toggle.addEventListener('click', () => {
        const isHidden = menu.classList.contains('hidden');
        menu.classList.toggle('hidden', !isHidden);
        toggle.setAttribute('aria-expanded', isHidden ? 'true' : 'false');
      });
    }
  });

  window.MathJax = {
    tex: { inlineMath: [['\\(','\\)'], ['$', '$']], displayMath: [['$','$'], ['\\[','\\]']] },
    svg: { fontCache: 'global' }
  };
</script>
