    <script>
      // Pivot Point Calculator Logic
      (function () {
        const highEl = document.getElementById("high");
        const lowEl = document.getElementById("low");
        const closeEl = document.getElementById("close");
        const decimalsEl = document.getElementById("decimals");
        const errorEl = document.getElementById("error-msg");
        const methodTabs = document.querySelectorAll(".pivot-tab");
        const methodLabel = document.getElementById("method-label");
        const singleBody = document.getElementById("single-method-body");
        const allBody = document.getElementById("all-methods-body");
        const singleResults = document.getElementById("single-method-results");
        const allResults = document.getElementById("all-methods-results");
        const allMethodsBtn = document.getElementById("all-methods-btn");

        let currentMethod = "classic";

        function getInputs() {
          const H = parseFloat(highEl.value);
          const L = parseFloat(lowEl.value);
          const C = parseFloat(closeEl.value);
          const d = parseInt(decimalsEl.value, 10);
          return {
            H,
            L,
            C,
            decimals: isNaN(d) ? 4 : Math.min(Math.max(d, 0), 8),
          };
        }

        function validateInputs({ H, L, C }) {
          if (isNaN(H) || isNaN(L) || isNaN(C)) {
            return "Please enter numeric values for High, Low, and Close.";
          }
          if (L > H) {
            return "Low cannot be greater than High. Use the swap button if needed.";
          }
          if (H === L) {
            return "High and Low cannot be equal. Range must be greater than zero.";
          }
          return "";
        }

        function fmt(v, decimals) {
          return Number(v).toFixed(decimals);
        }

        function calcClassic(H, L, C) {
          const P = (H + L + C) / 3;
          const R = H - L;
          return {
            P,
            R1: 2 * P - L,
            S1: 2 * P - H,
            R2: P + R,
            S2: P - R,
            R3: H + 2 * (P - L),
            S3: L - 2 * (H - P),
          };
        }

        function calcFibonacci(H, L, C) {
          const P = (H + L + C) / 3;
          const R = H - L;
          return {
            P,
            R1: P + 0.382 * R,
            S1: P - 0.382 * R,
            R2: P + 0.618 * R,
            S2: P - 0.618 * R,
            R3: P + 1.0 * R,
            S3: P - 1.0 * R,
          };
        }

        function calcWoodie(H, L, C) {
          const P = (H + L + 2 * C) / 4;
          const R = H - L;
          return {
            P,
            R1: 2 * P - L,
            S1: 2 * P - H,
            R2: P + R,
            S2: P - R,
          };
        }

        function calcCamarilla(H, L, C) {
          const R = H - L;
          return {
            P: (H + L + C) / 3,
            R1: C + 0.0833 * R,
            S1: C - 0.0833 * R,
            R2: C + 0.1666 * R,
            S2: C - 0.1666 * R,
            R3: C + 0.25 * R,
            S3: C - 0.25 * R,
            R4: C + 0.5 * R,
            S4: C - 0.5 * R,
          };
        }

        function calcDeMark(H, L, C, O) {
          let X;
          if (C > O) X = H + 2 * C + L;
          else if (C < O) X = 2 * H + C + L;
          else X = H + C + L;
          const P = X / 4;
          return {
            P,
            R1: X / 2 - L,
            S1: X / 2 - H,
          };
        }

        function renderSingle(method, vals, decimals) {
          methodLabel.textContent =
            method.charAt(0).toUpperCase() + method.slice(1);
          singleBody.innerHTML = "";
          const order = ["R4", "R3", "R2", "R1", "P", "S1", "S2", "S3", "S4"];
          order.forEach((level) => {
            if (vals[level] !== undefined) {
              const row = document.createElement("tr");
              row.innerHTML = `
            <td class="px-3 py-1.5 border-b text-gray-700">${level}</td>
            <td class="px-3 py-1.5 border-b text-right text-gray-900">${fmt(
              vals[level],
              decimals
            )}</td>
          `;
              singleBody.appendChild(row);
            }
          });
          singleResults.classList.remove("hidden");
        }

        function renderAll(allVals, decimals) {
          allBody.innerHTML = "";
          const levels = ["R4", "R3", "R2", "R1", "P", "S1", "S2", "S3", "S4"];
          levels.forEach((level) => {
            const row = document.createElement("tr");
            row.innerHTML = `
          <td class="px-2 py-1.5 border-b text-gray-700">${level}</td>
          <td class="px-2 py-1.5 border-b text-right">${
            allVals.classic[level] !== undefined
              ? fmt(allVals.classic[level], decimals)
              : ""
          }</td>
          <td class="px-2 py-1.5 border-b text-right">${
            allVals.fibonacci[level] !== undefined
              ? fmt(allVals.fibonacci[level], decimals)
              : ""
          }</td>
          <td class="px-2 py-1.5 border-b text-right">${
            allVals.woodie[level] !== undefined
              ? fmt(allVals.woodie[level], decimals)
              : ""
          }</td>
          <td class="px-2 py-1.5 border-b text-right">${
            allVals.camarilla[level] !== undefined
              ? fmt(allVals.camarilla[level], decimals)
              : ""
          }</td>
          <td class="px-2 py-1.5 border-b text-right">${
            allVals.demark[level] !== undefined
              ? fmt(allVals.demark[level], decimals)
              : ""
          }</td>
        `;
            allBody.appendChild(row);
          });
          allResults.classList.remove("hidden");
        }

        document
          .getElementById("calculate-btn")
          .addEventListener("click", () => {
            errorEl.classList.add("hidden");
            singleResults.classList.add("hidden");
            allResults.classList.add("hidden");

            const { H, L, C, decimals } = getInputs();
            const err = validateInputs({ H, L, C });
            if (err) {
              errorEl.textContent = err;
              errorEl.classList.remove("hidden");
              return;
            }

            const classic = calcClassic(H, L, C);
            const fib = calcFibonacci(H, L, C);
            const woodie = calcWoodie(H, L, C);
            const camarilla = calcCamarilla(H, L, C);
            const demark = calcDeMark(H, L, C, C); // assume O = C if not provided

            const allVals = {
              classic,
              fibonacci: fib,
              woodie,
              camarilla,
              demark,
            };

            if (allResults.classList.contains("hidden")) {
              renderSingle(currentMethod, allVals[currentMethod], decimals);
            } else {
              renderAll(allVals, decimals);
            }
          });

        methodTabs.forEach((tab) => {
          tab.addEventListener("click", () => {
            methodTabs.forEach((t) => t.classList.remove("tab-active"));
            tab.classList.add("tab-active");
            currentMethod = tab.dataset.method;
            allResults.classList.add("hidden");
            if (!singleResults.classList.contains("hidden")) {
              document.getElementById("calculate-btn").click();
            }
          });
        });

        allMethodsBtn.addEventListener("click", () => {
          allResults.classList.remove("hidden");
          singleResults.classList.add("hidden");
          document.getElementById("calculate-btn").click();
        });

        document.getElementById("swap-hlc").addEventListener("click", () => {
          const H = highEl.value;
          highEl.value = lowEl.value;
          lowEl.value = H;
        });

        document.getElementById("reset-btn").addEventListener("click", () => {
          ["symbol", "high", "low", "close"].forEach((id) => {
            const el = document.getElementById(id);
            if (el) el.value = "";
          });
          decimalsEl.value = 4;
          errorEl.classList.add("hidden");
          singleResults.classList.add("hidden");
          allResults.classList.add("hidden");
        });
      })();

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
