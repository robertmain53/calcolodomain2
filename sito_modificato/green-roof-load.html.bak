    <script>
      // Utility conversions
      function kNm2ToPsf(kNm2) {
        // 1 kN/m² ≈ 20.885434233 psf
        return kNm2 * 20.885434233;
      }
      function psfToKNm2(psf) {
        return psf / 20.885434233;
      }
      function kNToKg(kN) {
        // 1 kN ≈ 101.971621 kg
        return kN * 101.971621;
      }
      function lbToKip(lb) {
        return lb / 1000;
      }

      const els = {
        unitSystem: document.getElementById("unit-system"),
        roofArea: document.getElementById("roof-area"),
        roofAreaUnit: document.getElementById("roof-area-unit"),
        roofType: document.getElementById("roof-type"),
        roofTypeHint: document.getElementById("roof-type-hint"),
        mediaDepth: document.getElementById("media-depth"),
        mediaDepthUnit: document.getElementById("media-depth-unit"),
        uwMembrane: document.getElementById("uw-membrane"),
        uwMembraneUnit: document.getElementById("uw-membrane-unit"),
        uwDrainage: document.getElementById("uw-drainage"),
        uwDrainageUnit: document.getElementById("uw-drainage-unit"),
        uwMedia: document.getElementById("uw-media"),
        uwMediaUnit: document.getElementById("uw-media-unit"),
        uwVegetation: document.getElementById("uw-vegetation"),
        uwVegetationUnit: document.getElementById("uw-vegetation-unit"),
        uwInsulation: document.getElementById("uw-insulation"),
        uwInsulationUnit: document.getElementById("uw-insulation-unit"),
        uwAdditional: document.getElementById("uw-additional"),
        uwAdditionalUnit: document.getElementById("uw-additional-unit"),
        liveLoad: document.getElementById("live-load"),
