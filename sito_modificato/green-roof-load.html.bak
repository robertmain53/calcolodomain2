        calcStatus: document.getElementById("calc-status"),
        resMediaLoad: document.getElementById("res-media-load"),
        resDeadLoad: document.getElementById("res-dead-load"),
        resTotalDead: document.getElementById("res-total-dead"),
        resLiveLoad: document.getElementById("res-live-load"),
        resTotalDesign: document.getElementById("res-total-design"),
        resUnitNote: document.getElementById("res-unit-note"),
        resTotalDeadWeight: document.getElementById("res-total-dead-weight"),
        resTotalDesignWeight: document.getElementById(
          "res-total-design-weight"
        ),
        resCapacity: document.getElementById("res-capacity"),
        resUtilization: document.getElementById("res-utilization"),
        resStatusMsg: document.getElementById("res-status-msg"),
      };

      function updateRoofTypeHint() {
        const type = els.roofType.value;
        let hint = "";
        switch (type) {
          case "extensive":
            hint = "Typical saturated load: 0.8–1.5 kN/m² (17–31 psf).";
            if (els.unitSystem.value === "imperial") {
              hint = "Typical saturated load: 17–31 psf (0.8–1.5 kN/m²).";
            }
            break;
          case "semi":
            hint = "Typical saturated load: 1.5–3.0 kN/m² (31–63 psf).";
            if (els.unitSystem.value === "imperial") {
              hint = "Typical saturated load: 31–63 psf (1.5–3.0 kN/m²).";
            }
            break;
          case "intensive":
            hint = "Typical saturated load: >3.0 kN/m² (>63 psf).";
            if (els.unitSystem.value === "imperial") {
              hint = "Typical saturated load: >63 psf (>3.0 kN/m²).";
            }
            break;
          default:
            hint =
              "Enter project‑specific unit weights from your green roof supplier.";
        }
        els.roofTypeHint.textContent = hint;

        // Adjust default media density based on type (metric kN/m³)
        let gamma = 1.4;
        if (type === "extensive") gamma = 1.3;
        else if (type === "semi") gamma = 1.6;
        else if (type === "intensive") gamma = 1.8;
        els.uwMedia.value = gamma.toFixed(1);
      }

      function updateUnits() {
        const metric = els.unitSystem.value === "metric";

        els.roofAreaUnit.textContent = metric ? "m²" : "ft²";
        els.mediaDepthUnit.textContent = metric ? "mm" : "in";

        els.uwMembraneUnit.textContent = metric ? "kN/m²" : "psf";
        els.uwDrainageUnit.textContent = metric ? "kN/m²" : "psf";
        els.uwVegetationUnit.textContent = metric ? "kN/m²" : "psf";
        els.uwInsulationUnit.textContent = metric ? "kN/m²" : "psf";
        els.uwAdditionalUnit.textContent = metric ? "kN/m²" : "psf";
        els.uwMediaUnit.textContent = metric ? "kN/m³" : "pcf";

        els.liveLoadUnit.textContent = metric ? "kN/m²" : "psf";
        els.capacityUnit.textContent = metric ? "kN/m²" : "psf";

        // Convert existing numeric values approximately when switching
        try {
          if (metric) {
            // psf -> kN/m²
            [
              "uwMembrane",
              "uwDrainage",
              "uwVegetation",
              "uwInsulation",
              "uwAdditional",
              "liveLoad",
              "capacity",
            ].forEach((id) => {
              const el = els[id];
              const v = parseFloat(el.value);
              if (!isNaN(v)) el.value = psfToKNm2(v).toFixed(2);
            });
            // pcf -> kN/m³ (1 pcf ≈ 0.157087 kN/m³)
            const mediaVal = parseFloat(els.uwMedia.value);
            if (!isNaN(mediaVal))
              els.uwMedia.value = (mediaVal * 0.157087).toFixed(2);
            // in -> mm
            const depthVal = parseFloat(els.mediaDepth.value);
            if (!isNaN(depthVal))
              els.mediaDepth.value = (depthVal * 25.4).toFixed(0);
          } else {
            // kN/m² -> psf
            [
              "uwMembrane",
              "uwDrainage",
              "uwVegetation",
              "uwInsulation",
              "uwAdditional",
              "liveLoad",
              "capacity",
            ].forEach((id) => {
              const el = els[id];
              const v = parseFloat(el.value);
              if (!isNaN(v)) el.value = kNm2ToPsf(v).toFixed(1);
            });
            // kN/m³ -> pcf (1 kN/m³ ≈ 6.3661977 pcf)
            const mediaVal = parseFloat(els.uwMedia.value);
            if (!isNaN(mediaVal))
              els.uwMedia.value = (mediaVal * 6.3661977).toFixed(1);
            // mm -> in
            const depthVal = parseFloat(els.mediaDepth.value);
            if (!isNaN(depthVal))
              els.mediaDepth.value = (depthVal / 25.4).toFixed(2);
          }
        } catch (e) {
          // ignore conversion errors
        }

        updateRoofTypeHint();
      }

      function resetDefaults() {
        els.unitSystem.value = "metric";
        els.roofArea.value = 100;
        els.roofType.value = "extensive";
        els.mediaDepth.value = 100;
        els.uwMembrane.value = 0.2;
        els.uwDrainage.value = 0.3;
        els.uwMedia.value = 1.4;
        els.uwVegetation.value = 0.1;
        els.uwInsulation.value = 0.0;
        els.uwAdditional.value = 0.0;
        els.liveLoad.value = 0.75;
        els.capacity.value = 4.0;
        els.safetyFactor.value = 1.3;
        updateUnits();
        clearResults();
      }

      function clearResults() {
        [
          els.resMediaLoad,
          els.resDeadLoad,
          els.resTotalDead,
          els.resLiveLoad,
          els.resTotalDesign,
          els.resTotalDeadWeight,
          els.resTotalDesignWeight,
          els.resCapacity,
          els.resUtilization,
        ].forEach((el) => (el.textContent = "–"));
        els.resUnitNote.textContent = "";
        els.resStatusMsg.textContent = "";
        els.resStatusMsg.className = "mt-2 text-xs";
        els.calcStatus.textContent = "";
      }

      function calculate() {
        clearResults();
        const metric = els.unitSystem.value === "metric";

        const A = parseFloat(els.roofArea.value);
        const depthInput = parseFloat(els.mediaDepth.value);
        const gammaMediaInput = parseFloat(els.uwMedia.value);
        const qMembrane = parseFloat(els.uwMembrane.value) || 0;
        const qDrainage = parseFloat(els.uwDrainage.value) || 0;
        const qVegetation = parseFloat(els.uwVegetation.value) || 0;
        const qInsulation = parseFloat(els.uwInsulation.value) || 0;
        const qAdditional = parseFloat(els.uwAdditional.value) || 0;
        const qLive = parseFloat(els.liveLoad.value) || 0;
        const qCap = parseFloat(els.capacity.value) || 0;
        const gammaSF = parseFloat(els.safetyFactor.value) || 1;

        if (!(A > 0) || !(depthInput >= 0)) {
          els.calcStatus.textContent =
            "Please enter a positive roof area and non‑negative media depth.";
          return;
        }

        // Convert depth to meters or feet
        let hMedia; // m or ft
        if (metric) {
          hMedia = depthInput / 1000; // mm -> m
        } else {
          hMedia = depthInput / 12; // in -> ft
        }

        // Media load
        let qMedia;
        if (metric) {
          // gammaMediaInput is kN/m³
          qMedia = gammaMediaInput * hMedia; // kN/m²
        } else {
          // gammaMediaInput is pcf (lb/ft³)
          // qMedia in psf = pcf * ft
          qMedia = gammaMediaInput * hMedia; // psf
        }

        // Dead load (surface)
        const qDead =
          qMedia +
          qMembrane +
          qDrainage +
          qVegetation +
          qInsulation +
          qAdditional;
        const qTotal = qDead + qLive;

        // Total weights
        let WDead, WTotal, WCapEff;
        if (metric) {
          // kN/m² * m² = kN
          WDead = qDead * A;
          WTotal = qTotal * A;
          const qCapEff = qCap / gammaSF;
          WCapEff = qCapEff * A;

          els.resMediaLoad.textContent = qMedia.toFixed(2) + " kN/m²";
          els.resDeadLoad.textContent = (qDead - qMedia).toFixed(2) + " kN/m²";
          els.resTotalDead.textContent = qDead.toFixed(2) + " kN/m²";
          els.resLiveLoad.textContent = qLive.toFixed(2) + " kN/m²";
          els.resTotalDesign.textContent = qTotal.toFixed(2) + " kN/m²";
          els.resUnitNote.textContent =
            "Surface loads in kN/m². Total weights in kN and approximate kg.";

          els.resTotalDeadWeight.textContent =
            WDead.toFixed(1) + " kN (" + kNToKg(WDead).toFixed(0) + " kg)";
          els.resTotalDesignWeight.textContent =
            WTotal.toFixed(1) + " kN (" + kNToKg(WTotal).toFixed(0) + " kg)";
          els.resCapacity.textContent =
            WCapEff.toFixed(1) + " kN (effective, with safety factor)";
        } else {
          // psf * ft² = lb
          WDead = qDead * A;
          WTotal = qTotal * A;
          const qCapEff = qCap / gammaSF;
          WCapEff = qCapEff * A;

          els.resMediaLoad.textContent = qMedia.toFixed(1) + " psf";
          els.resDeadLoad.textContent = (qDead - qMedia).toFixed(1) + " psf";
          els.resTotalDead.textContent = qDead.toFixed(1) + " psf";
          els.resLiveLoad.textContent = qLive.toFixed(1) + " psf";
          els.resTotalDesign.textContent = qTotal.toFixed(1) + " psf";
          els.resUnitNote.textContent =
            "Surface loads in psf. Total weights in lb and kips (1 kip = 1000 lb).";

          els.resTotalDeadWeight.textContent =
            WDead.toFixed(0) + " lb (" + lbToKip(WDead).toFixed(2) + " kips)";
          els.resTotalDesignWeight.textContent =
            WTotal.toFixed(0) + " lb (" + lbToKip(WTotal).toFixed(2) + " kips)";
          els.resCapacity.textContent =
            WCapEff.toFixed(0) + " lb (effective, with safety factor)";
        }

        // Utilization
        if (qCap > 0) {
          const qCapEff = qCap / gammaSF;
          const U = qCapEff > 0 ? qTotal / qCapEff : NaN;
          if (!isNaN(U) && isFinite(U)) {
            els.resUtilization.textContent = (U * 100).toFixed(1) + " %";
            if (U <= 1) {
              els.resStatusMsg.textContent =
                "Estimated total load is within the target limit (with the chosen safety factor). Always confirm with a structural engineer.";
              els.resStatusMsg.className = "mt-2 text-xs text-green-700";
            } else {
              els.resStatusMsg.textContent =
                "Warning: estimated total load exceeds the target limit with the chosen safety factor. Consult a structural engineer and consider reducing weight or increasing capacity.";
              els.resStatusMsg.className = "mt-2 text-xs text-red-700";
            }
          }
        } else {
          els.resUtilization.textContent = "–";
          els.resStatusMsg.textContent =
            "Enter a roof capacity to see a utilization check.";
          els.resStatusMsg.className = "mt-2 text-xs text-gray-600";
        }

        els.calcStatus.textContent = "Calculation updated.";
      }

      els.unitSystem.addEventListener("change", updateUnits);
      els.roofType.addEventListener("change", updateRoofTypeHint);
      els.calcBtn.addEventListener("click", calculate);
      els.resetBtn.addEventListener("click", resetDefaults);

      // Initialize
      updateUnits();
      clearResults();
    </script>

