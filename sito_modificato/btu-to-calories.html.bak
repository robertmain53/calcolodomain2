  <script>
    // Tab logic
    const tabButtons = document.querySelectorAll('.tab-button');
    const tabPanels = document.querySelectorAll('.tab-panel');
    tabButtons.forEach(btn => {
      btn.addEventListener('click', () => {
        const target = btn.getAttribute('data-tab');
        tabButtons.forEach(b => b.classList.remove('tab-button-active', 'bg-blue-50', 'text-blue-900', 'border-blue-100'));
        btn.classList.add('tab-button-active', 'bg-blue-50', 'text-blue-900', 'border-blue-100');
        tabPanels.forEach(panel => panel.classList.toggle('hidden', panel.id !== 'tab-' + target));
      });
    });

    // Constants
    const BTU_J = {
      btu_it: 1055.05585262,
      btu_th: 1054.35026444,
      btu_mean: 1055.87
    };

    const CAL_J = {
      cal_th: 4.184,
      cal_it: 4.1868,
      cal_15: 4.1855,
      // kcal is 1000 × thermochemical calorie
      kcal: 4184
    };

    // Elements BTU -> cal
    const btuValueEl = document.getElementById('btu-value');
    const btuTypeEl = document.getElementById('btu-type');
    const calTypeEl = document.getElementById('cal-type');
    const btuCalResultEl = document.getElementById('btu-cal-result');
    const btuJoulesResultEl = document.getElementById('btu-joules-result');
    const factorLabelEl = document.getElementById('factor-label');

    // Elements cal -> BTU
    const calValueEl = document.getElementById('cal-value');
    const calType2El = document.getElementById('cal-type-2');
    const btuType2El = document.getElementById('btu-type-2');
    const calBtuResultEl = document.getElementById('cal-btu-result');
    const calJoulesResultEl = document.getElementById('cal-joules-result');

    // Table elements
    const tbl1kcal = document.getElementById('tbl-btu-1-kcal');
    const tbl1cal = document.getElementById('tbl-btu-1-cal');
    const tbl2kcal = document.getElementById('tbl-btu-2-kcal');
    const tbl2cal = document.getElementById('tbl-btu-2-cal');
    const tbl5kcal = document.getElementById('tbl-btu-5-kcal');
    const tbl5cal = document.getElementById('tbl-btu-5-cal');
    const tbl10kcal = document.getElementById('tbl-btu-10-kcal');
    const tbl10cal = document.getElementById('tbl-btu-10-cal');

    function updateBtuToCal() {
      const btuVal = parseFloat(btuValueEl.value || 0);
      const btuType = btuTypeEl.value;
      const calType = calTypeEl.value;
      const btuJ = BTU_J[btuType];
      const calJ = CAL_J[calType];
      const joules = btuVal * btuJ;
      const calories = joules / calJ;
      btuJoulesResultEl.textContent = joules.toFixed(4).replace(/\.?0+$/,'');
      btuCalResultEl.textContent = calories.toFixed(6).replace(/\.?0+$/,'');
      let btuLabel = 'BTU (IT)';
      if (btuType === 'btu_th') btuLabel = 'BTU (thermochemical)';
      if (btuType === 'btu_mean') btuLabel = 'BTU (mean)';
      let calLabel = 'cal (th)';
      if (calType === 'cal_it') calLabel = 'cal (IT)';
      if (calType === 'cal_15') calLabel = 'cal (15°C)';
      if (calType === 'kcal') calLabel = 'kcal (nutritional)';
      factorLabelEl.textContent = 'Using 1 ' + btuLabel + ' = ' + btuJ.toFixed(6) + ' J and 1 ' + calLabel + ' = ' + calJ.toFixed(6) + ' J';
    }

    function updateCalToBtu() {
      const calVal = parseFloat(calValueEl.value || 0);
      const calType = calType2El.value;
      const btuType = btuType2El.value;
      const calJ = CAL_J[calType];
      const btuJ = BTU_J[btuType];
      const joules = calVal * calJ;
      const btu = joules / btuJ;
      calJoulesResultEl.textContent = joules.toFixed(4).replace(/\.?0+$/,'');
      calBtuResultEl.textContent = btu.toFixed(6).replace(/\.?0+$/,'');
    }

    function updateTable() {
      const btuJ = BTU_J['btu_it']; // table uses IT
      const calJ = CAL_J['cal_th'];
      const kcalJ = CAL_J['kcal'];

      function btuToCal(btu) { return (btu * btuJ) / calJ; }
      function btuToKcal(btu) { return (btu * btuJ) / kcalJ; }

      tbl1kcal.textContent = btuToKcal(1).toFixed(6).replace(/\.?0+$/,'');
      tbl1cal.textContent = btuToCal(1).toFixed(3).replace(/\.?0+$/,'');
      tbl2kcal.textContent = btuToKcal(2).toFixed(6).replace(/\.?0+$/,'');
      tbl2cal.textContent = btuToCal(2).toFixed(3).replace(/\.?0+$/,'');
      tbl5kcal.textContent = btuToKcal(5).toFixed(6).replace(/\.?0+$/,'');
      tbl5cal.textContent = btuToCal(5).toFixed(3).replace(/\.?0+$/,'');
      tbl10kcal.textContent = btuToKcal(10).toFixed(6).replace(/\.?0+$/,'');
      tbl10cal.textContent = btuToCal(10).toFixed(3).replace(/\.?0+$/,'');
    }

    btuValueEl.addEventListener('input', updateBtuToCal);
    btuTypeEl.addEventListener('input', () => { updateBtuToCal(); updateTable(); });
    calTypeEl.addEventListener('input', updateBtuToCal);

    calValueEl.addEventListener('input', updateCalToBtu);
    calType2El.addEventListener('input', updateCalToBtu);
    btuType2El.addEventListener('input', updateCalToBtu);

    updateBtuToCal();
    updateCalToBtu();
    updateTable();
  </script>

