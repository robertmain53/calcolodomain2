<script>
  // Utility functions
  function ipToInt(ip) {
    const parts = ip.split('.');
    if (parts.length !== 4) return null;
    let n = 0;
    for (let i = 0; i < 4; i++) {
      const p = Number(parts[i]);
      if (!Number.isInteger(p) || p < 0 || p > 255) return null;
      n = (n << 8) + p;
    }
    return n >>> 0;
  }

  function intToIp(n) {
    return [
      (n >>> 24) & 255,
      (n >>> 16) & 255,
      (n >>> 8) & 255,
      n & 255
    ].join('.');
  }

  function prefixToMask(prefix) {
    prefix = Number(prefix);
    if (!Number.isInteger(prefix) || prefix < 0 || prefix > 32) return null;
    if (prefix === 0) return 0;
    return (0xFFFFFFFF << (32 - prefix)) >>> 0;
  }

  function maskToPrefix(maskInt) {
    let bits = maskInt.toString(2);
    bits = bits.padStart(32, '0');
    if (!/^1*0*$/.test(bits)) return null; // non-contiguous mask
    return bits.indexOf('0') === -1 ? 32 : bits.indexOf('0');
  }

  function maskIntToDotted(maskInt) {
    return intToIp(maskInt >>> 0);
  }

  function dottedMaskToInt(mask) {
    const n = ipToInt(mask);
    if (n === null) return null;
    const p = maskToPrefix(n);
    return p === null ? null : n;
  }

  function toBinaryOctets(n) {
    const parts = [
      (n >>> 24) & 255,
      (n >>> 16) & 255,
      (n >>> 8) & 255,
      n & 255
    ];
    return parts.map(p => p.toString(2).padStart(8, '0')).join(' ');
  }

  function formatNumber(n) {
    return n.toLocaleString('en-US');
  }

  function buildNearbyTable(networkInt, basePrefix) {
    const tbody = document.getElementById('nearbyTableBody');
    tbody.innerHTML = '';
    const start = Math.max(0, basePrefix - 4);
    const end = Math.min(32, basePrefix + 4);
    for (let p = start; p <= end; p++) {
      const mask = prefixToMask(p);
      const net = networkInt & mask;
      const wildcard = (~mask) >>> 0;
      const broadcast = net | wildcard;
      const total = 2 ** (32 - p);
      const usable = p >= 31 ? 0 : Math.max(0, total - 2);
      const tr = document.createElement('tr');
      tr.innerHTML = `
        <td class="px-2 py-1 border-b">/${p}</td>
        <td class="px-2 py-1 border-b">${maskIntToDotted(mask)}</td>
        <td class="px-2 py-1 border-b text-right">${formatNumber(total)}</td>
        <td class="px-2 py-1 border-b text-right">${formatNumber(usable)}</td>
        <td class="px-2 py-1 border-b">${intToIp(net)}</td>
        <td class="px-2 py-1 border-b">${intToIp(broadcast)}</td>
      `;
      tbody.appendChild(tr);
    }
  }

  function copyNearbyAsCSV() {
    const tbody = document.getElementById('nearbyTableBody');
    if (!tbody || !tbody.children.length) return;
    const rows = [['CIDR','Mask','Total','Usable','Network','Broadcast']];
    Array.from(tbody.children).forEach(tr => {
      const cells = Array.from(tr.children).map(td => td.textContent.trim());
      rows.push(cells);
    });
    const csv = rows.map(r => r.map(v => `"${v.replace(/"/g,'""')}"`).join(',')).join('\n');
    navigator.clipboard.writeText(csv).catch(() => {});
  }

  function parseCidrNotation(str) {
    const parts = str.split('/');
    if (parts.length !== 2) return null;
    const ipInt = ipToInt(parts[0].trim());
    const prefix = Number(parts[1]);
    if (ipInt === null || !Number.isInteger(prefix) || prefix < 0 || prefix > 32) return null;
    return { ipInt, prefix };
  }

  function nextNetwork(baseNet, prefix) {
    const total = 2 ** (32 - prefix);
    return (baseNet + total) >>> 0;
  }

  function requiredPrefixForHosts(hosts) {
    // total >= hosts + 2 (network + broadcast) for traditional subnets
    for (let p = 32; p >= 0; p--) {
      const total = 2 ** (32 - p);
      const usable = p >= 31 ? 0 : Math.max(0, total - 2);
      if (usable >= hosts) return p;
    }
    return null;
  }

  // Initialize mask dropdown
  (function initMaskSelect() {
    const select = document.getElementById('maskSelect');
    const common = [8,16,24,25,26,27,28,29,30];
    for (let p = 0; p <= 32; p++) {
      const opt = document.createElement('option');
      opt.value = p;
      const mask = prefixToMask(p);
      opt.textContent = `/${p}  –  ${maskIntToDotted(mask)}${common.includes(p) ? ' (common)' : ''}`;
      if (p === 24) opt.selected = true;
      select.appendChild(opt);
    }
  })();

  // Sync CIDR and mask dropdown
  document.getElementById('maskSelect').addEventListener('change', e => {
    const p = Number(e.target.value);
    document.getElementById('cidrInput').value = p;
  });

  document.getElementById('cidrInput').addEventListener('input', e => {
    const p = Number(e.target.value);
    if (Number.isInteger(p) && p >= 0 && p <= 32) {
      document.getElementById('maskSelect').value = String(p);
    }
  });

  // Main calculation
  document.getElementById('calcBtn').addEventListener('click', () => {
    const ipStr = document.getElementById('ipInput').value.trim();
    const cidrStr = document.getElementById('cidrInput').value.trim();
    const errorBox = document.getElementById('errorBox');
    errorBox.classList.add('hidden');
    errorBox.textContent = '';

    const ipInt = ipToInt(ipStr);
    if (ipInt === null) {
      errorBox.textContent = 'Please enter a valid IPv4 address (e.g. 192.168.1.10).';
      errorBox.classList.remove('hidden');
      document.getElementById('resultsSection').classList.add('hidden');
      return;
    }

    let prefix;
    if (cidrStr !== '') {
      prefix = Number(cidrStr);
      if (!Number.isInteger(prefix) || prefix < 0 || prefix > 32) {
        errorBox.textContent = 'CIDR prefix must be an integer between 0 and 32.';
        errorBox.classList.remove('hidden');
        document.getElementById('resultsSection').classList.add('hidden');
        return;
      }
    } else {
      prefix = Number(document.getElementById('maskSelect').value);
    }

    const maskInt = prefixToMask(prefix);
    const wildcardInt = (~maskInt) >>> 0;
    const networkInt = ipInt & maskInt;
    const broadcastInt = networkInt | wildcardInt;
    const total = 2 ** (32 - prefix);
    const usable = prefix >= 31 ? 0 : Math.max(0, total - 2);
    const firstHost = prefix >= 31 ? null : (networkInt + 1) >>> 0;
    const lastHost = prefix >= 31 ? null : (broadcastInt - 1) >>> 0;

    // Fill summary
    document.getElementById('resIp').textContent = intToIp(ipInt);
    document.getElementById('resNetwork').textContent = intToIp(networkInt);
    document.getElementById('resBroadcast').textContent = intToIp(broadcastInt);
    document.getElementById('resCidr').textContent = `/${prefix}`;
    document.getElementById('resMask').textContent = maskIntToDotted(maskInt);
    document.getElementById('resWildcard').textContent = maskIntToDotted(wildcardInt);
    document.getElementById('resTotal').textContent = formatNumber(total);
    document.getElementById('resUsable').textContent = formatNumber(usable);
    document.getElementById('resRange').textContent = usable === 0 ? 'n/a' : `${intToIp(firstHost)} – ${intToIp(lastHost)}`;

    // Binary view
    document.getElementById('binIp').textContent = toBinaryOctets(ipInt);
    document.getElementById('binMask').textContent = toBinaryOctets(maskInt);
    document.getElementById('binNetwork').textContent = toBinaryOctets(networkInt);
    document.getElementById('binBroadcast').textContent = toBinaryOctets(broadcastInt);

    // Nearby table
    buildNearbyTable(networkInt, prefix);

    document.getElementById('resultsSection').classList.remove('hidden');
  });

  document.getElementById('resetBtn').addEventListener('click', () => {
    document.getElementById('ipInput').value = '';
    document.getElementById('cidrInput').value = '';
    document.getElementById('maskSelect').value = '24';
    document.getElementById('resultsSection').classList.add('hidden');
    const errorBox = document.getElementById('errorBox');
    errorBox.classList.add('hidden');
    errorBox.textContent = '';
  });

  document.getElementById('exportBtn').addEventListener('click', copyNearbyAsCSV);

  // VLSM planner
  document.getElementById('vlsmBtn').addEventListener('click', () => {
    const baseStr = document.getElementById('vlsmBase').value.trim();
    const hostsStr = document.getElementById('vlsmHosts').value.trim();
    const err = document.getElementById('vlsmError');
    const resDiv = document.getElementById('vlsmResult');
    const tbody = document.getElementById('vlsmTableBody');
    err.classList.add('hidden');
    err.textContent = '';
    resDiv.classList.add('hidden');
    tbody.innerHTML = '';

    const parsed = parseCidrNotation(baseStr);
    if (!parsed) {
      err.textContent = 'Enter a valid base network in CIDR notation, e.g. 192.168.0.0/24.';
      err.classList.remove('hidden');
      return;
    }
    const { ipInt: baseIp, prefix: basePrefix } = parsed;
    const baseMask = prefixToMask(basePrefix);
    const baseNet = baseIp & baseMask;
    const baseTotal = 2 ** (32 - basePrefix);

    if (!hostsStr) {
      err.textContent = 'Enter at least one required host count (one per line).';
      err.classList.remove('hidden');
      return;
    }

    const hostLines = hostsStr.split('\n')
      .map(s => s.trim())
      .filter(s => s !== '');
    const hostReqs = [];
    for (const line of hostLines) {
      const n = Number(line);
      if (!Number.isInteger(n) || n <= 0) {
        err.textContent = `Invalid host count: "${line}". Use positive integers only.`;
        err.classList.remove('hidden');
        return;
      }
      hostReqs.push(n);
    }

    // Sort descending for VLSM
    const indexed = hostReqs.map((h, idx) => ({ h, idx }));
    indexed.sort((a, b) => b.h - a.h);

    let currentNet = baseNet;
    let usedTotal = 0;
    const allocations = [];

    for (const item of indexed) {
      const p = requiredPrefixForHosts(item.h);
      if (p === null) {
        err.textContent = `Cannot allocate subnet for ${item.h} hosts.`;
        err.classList.remove('hidden');
        return;
      }
      const total = 2 ** (32 - p);
      usedTotal += total;
      if (usedTotal > baseTotal) {
        err.textContent = 'Requested subnets do not fit inside the base network. Choose a larger base block or reduce host counts.';
        err.classList.remove('hidden');
        return;
      }
      const mask = prefixToMask(p);
      const net = currentNet & mask;
      const wildcard = (~mask) >>> 0;
      const broadcast = net | wildcard;
      const usable = p >= 31 ? 0 : Math.max(0, total - 2);
      const firstHost = p >= 31 ? null : (net + 1) >>> 0;
      const lastHost = p >= 31 ? null : (broadcast - 1) >>> 0;

      allocations.push({
        originalIndex: item.idx,
        hosts: item.h,
        prefix: p,
        network: net,
        firstHost,
        lastHost
      });

      currentNet = nextNetwork(net, p);
    }

    // Restore original order for display
    allocations.sort((a, b) => a.originalIndex - b.originalIndex);

    allocations.forEach((a, i) => {
      const tr = document.createElement('tr');
      tr.innerHTML = `
        <td class="px-2 py-1 border-b">${i + 1}</td>
        <td class="px-2 py-1 border-b text-right">${formatNumber(a.hosts)}</td>
        <td class="px-2 py-1 border-b">/${a.prefix}</td>
        <td class="px-2 py-1 border-b">${intToIp(a.network)}</td>
        <td class="px-2 py-1 border-b">${a.firstHost === null ? 'n/a' : intToIp(a.firstHost) + ' – ' + intToIp(a.lastHost)}</td>
      `;
      tbody.appendChild(tr);
    });

    resDiv.classList.remove('hidden');
  });

  // Simple keyboard usability: Enter triggers calculation from main inputs
  document.getElementById('ipInput').addEventListener('keydown', e => {
    if (e.key === 'Enter') document.getElementById('calcBtn').click();
  });
  document.getElementById('cidrInput').addEventListener('keydown', e => {
    if (e.key === 'Enter') document.getElementById('calcBtn').click();
  });

  // Mobile menu script (from template)
  document.getElementById('mobile-menu-toggle').addEventListener('click', function () {
    const menu = document.getElementById('mobile-menu');
    const expanded = this.getAttribute('aria-expanded') === 'true';
    this.setAttribute('aria-expanded', String(!expanded));
    menu.classList.toggle('hidden');
  });

  window.MathJax = {
    tex: { inlineMath: [['\\(','\\)'], ['$', '$']], displayMath: [['$','$'], ['\\[','\\]']] },
    svg: { fontCache: 'global' }
  };
</script>
