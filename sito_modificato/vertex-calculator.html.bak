            <script>
              document.addEventListener('DOMContentLoaded', function() {
                const tabs = document.querySelectorAll('.tab-button');
                const panels = document.querySelectorAll('.tab-panel');
                const calculateBtn = document.getElementById('calculate-btn');
                const resultsContainer = document.getElementById('results-container');
                const errorMessage = document.getElementById('error-message');
                const resultVertex = document.getElementById('result-vertex');
                const resultAOS = document.getElementById('result-aos');
                const stepByStep = document.getElementById('step-by-step');
                
                let activeTab = 'standard-form';

                // Tab switching logic
                tabs.forEach(tab => {
                  tab.addEventListener('click', () => {
                    // Deactivate all
                    tabs.forEach(t => t.classList.remove('active'));
                    panels.forEach(p => p.classList.remove('active'));
                    
                    // Activate clicked
                    tab.classList.add('active');
                    activeTab = tab.getAttribute('data-tab');
                    document.getElementById(activeTab).classList.add('active');
                    
                    // Clear results
                    resultsContainer.classList.add('hidden');
                    errorMessage.textContent = '';
                  });
                });

                // --- Helper: Parse fractions and decimals ---
                function parseValue(val) {
                  val = String(val).trim();
                  if (val.includes('/')) {
                    const parts = val.split('/');
                    if (parts.length === 2) {
                      const num = parseFloat(parts[0]);
                      const den = parseFloat(parts[1]);
                      if (den !== 0 && !isNaN(num) && !isNaN(den)) {
                        return num / den;
                      }
                    }
                  }
                  const num = parseFloat(val);
                  return isNaN(num) ? NaN : num;
                }
                
                function formatNumber(num) {
                    if (Math.abs(num) < 1e-10) return '0';
                    return parseFloat(num.toFixed(10)).toString();
                }
                
                function showError(message) {
                    errorMessage.textContent = `Error: ${message}`;
                    resultsContainer.classList.add('hidden');
                }
                
                // --- Function to calculate k = f(h) for Standard Form ---
                function calculateK(a, b, c, h) {
                    return a * h * h + b * h + c;
                }
                
                // --- Function to calculate k = f(h) for Intercept Form ---
                function calculateKIntercept(a, p, q, h) {
                    return a * (h - p) * (h - q);
                }

                // --- Main Calculation Logic ---
                calculateBtn.addEventListener('click', () => {
                  let h, k;
                  let a, b, c, p, q, h_val, k_val;
                  let steps = '';
                  
                  try {
                    errorMessage.textContent = '';
                    
                    if (activeTab === 'standard-form') {
                      a = parseValue(document.getElementById('std-a').value);
                      b = parseValue(document.getElementById('std-b').value);
                      c = parseValue(document.getElementById('std-c').value);
                      
                      if (isNaN(a) || isNaN(b) || isNaN(c)) throw new Error('Please enter valid coefficients for a, b, and c.');
                      if (Math.abs(a) < 1e-10) throw new Error("'a' cannot be zero (not a parabola).");
                      
                      // 1. Calculate h
                      h = -b / (2 * a);
                      
                      // 2. Calculate k
                      k = calculateK(a, b, c, h);
                      
                      const fA = formatNumber(a), fB = formatNumber(b), fC = formatNumber(c);
                      const fH = formatNumber(h);
                      const fK = formatNumber(k);
                      
                      steps = `
                        <h4 class="font-semibold text-lg">1. Find x-coordinate (h), the Axis of Symmetry:</h4>
                        <p>Formula: $h = \\frac{-b}{2a}$</p>
                        <p>Substitution: $h = \\frac{-(${fB})}{2(${fA})} = ${fH}$</p>
                        
                        <h4 class="font-semibold text-lg mt-4">2. Find y-coordinate (k):</h4>
                        <p>Formula: $k = f(h) = a(h)^2 + b(h) + c$</p>
                        <p>Substitution: $k = ${fA}(${fH})^2 + ${fB}(${fH}) + ${fC}$</p>
                        <p>Result: $k = ${fK}$</p>
                        
                        <h4 class="font-semibold text-lg mt-4">3. State the Vertex:</h4>
                        <p>The vertex is $(h, k)$: $(${fH}, ${fK})$</p>
                      `;
                      
                    } else if (activeTab === 'vertex-form') {
                      h_val = parseValue(document.getElementById('vtx-h').value);
                      k_val = parseValue(document.getElementById('vtx-k').value);
                      a = parseValue(document.getElementById('vtx-a').value);
                      
                      if (isNaN(h_val) || isNaN(k_val)) throw new Error("Please enter valid numbers for 'h' and 'k'.");
                      if (isNaN(a) || Math.abs(a) < 1e-10) throw new Error("Please enter a non-zero number for 'a'.");
                      
                      h = h_val;
                      k = k_val;
                      
                      const fH = formatNumber(h);
                      const fK = formatNumber(k);
                      
                      steps = `
                        <h4 class="font-semibold text-lg">1. Use Vertex Form: $y = a(x - h)^2 + k$</h4>
                        <p>By definition, the vertex coordinates $(h, k)$ are read directly from the equation.</p>
                        <p>From your input, $h = ${fH}$ and $k = ${fK}$.</p>
                        
                        <h4 class="font-semibold text-lg mt-4">2. State the Vertex:</h4>
                        <p>The vertex is $(h, k)$: $(${fH}, ${fK})$</p>
                      `;

                    } else if (activeTab === 'intercept-form') {
                      p = parseValue(document.getElementById('int-p').value);
                      q = parseValue(document.getElementById('int-q').value);
                      a = parseValue(document.getElementById('int-a').value);
                      
                      if (isNaN(p) || isNaN(q)) throw new Error("Please enter valid numbers for the intercepts 'p' and 'q'.");
                      if (isNaN(a) || Math.abs(a) < 1e-10) throw new Error("Please enter a non-zero number for 'a'.");
                      
                      // 1. Calculate h (midpoint)
                      h = (p + q) / 2;
                      
                      // 2. Calculate k
                      k = calculateKIntercept(a, p, q, h);
                      
                      const fA = formatNumber(a), fP = formatNumber(p), fQ = formatNumber(q);
                      const fH = formatNumber(h);
                      const fK = formatNumber(k);
                      
                      steps = `
                        <h4 class="font-semibold text-lg">1. Find x-coordinate (h), the Midpoint of Intercepts:</h4>
                        <p>Formula: $h = \\frac{p + q}{2}$</p>
                        <p>Substitution: $h = \\frac{${fP} + ${fQ}}{2} = ${fH}$</p>
                        
                        <h4 class="font-semibold text-lg mt-4">2. Find y-coordinate (k):</h4>
                        <p>Formula: $k = f(h) = a(h - p)(h - q)$</p>
                        <p>Substitution: $k = ${fA}(${fH} - ${fP})(${fH} - ${fQ})$</p>
                        <p>Result: $k = ${fK}$</p>
                        
                        <h4 class="font-semibold text-lg mt-4">3. State the Vertex:</h4>
                        <p>The vertex is $(h, k)$: $(${fH}, ${fK})$</p>
                      `;
                    }
                    
                    // Display results
                    const fH = formatNumber(h);
                    const fK = formatNumber(k);
                    resultVertex.textContent = `(${fH}, ${fK})`;
                    resultAOS.textContent = `x = ${fH}`;
                    stepByStep.innerHTML = steps;
                    resultsContainer.classList.remove('hidden');
                    
                    // Re-render MathJax
                    if (window.MathJax) {
                        window.MathJax.typesetPromise([stepByStep]);
                    }
                    
                  } catch (e) {
                    showError(e.message);
                  }
                });
                
                // Initial calculation (for example values)
                calculateBtn.click();
              });
            </script>

