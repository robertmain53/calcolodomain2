    <script>
      // Utility: unit conversion to base units
      function toBaseVoltage(value, unit) {
        if (value == null || value === "") return null;
        const v = parseFloat(value);
        if (isNaN(v)) return null;
        switch (unit) {
          case "mV":
            return v / 1000;
          case "kV":
            return v * 1000;
          default:
            return v;
        }
      }

      function toBaseCurrent(value, unit) {
        if (value == null || value === "") return null;
        const v = parseFloat(value);
        if (isNaN(v)) return null;
        switch (unit) {
          case "mA":
            return v / 1000;
          default:
            return v;
        }
      }

      function toBaseResistance(value, unit) {
        if (value == null || value === "") return null;
        const v = parseFloat(value);
        if (isNaN(v)) return null;
        switch (unit) {
          case "kohm":
            return v * 1e3;
          case "Mohm":
            return v * 1e6;
          default:
            return v;
        }
      }

      function toBasePower(value, unit) {
        if (value == null || value === "") return null;
        const v = parseFloat(value);
        if (isNaN(v)) return null;
        switch (unit) {
          case "mW":
            return v / 1000;
          case "kW":
            return v * 1000;
          default:
            return v;
        }
      }

      // Format with engineering prefixes
      function formatEngineering(value, unitType) {
        if (value == null || !isFinite(value)) return "—";
        const abs = Math.abs(value);
        let scaled = value;
        let suffix = "";

        if (unitType === "current") {
          if (abs < 1) {
            scaled = value * 1000;
            suffix = " mA";
          } else {
            suffix = " A";
          }
        } else if (unitType === "resistance") {
          if (abs >= 1e6) {
            scaled = value / 1e6;
            suffix = " MΩ";
          } else if (abs >= 1e3) {
            scaled = value / 1e3;
            suffix = " kΩ";
          } else {
            suffix = " Ω";
          }
        } else if (unitType === "power") {
          if (abs >= 1e3) {
            scaled = value / 1e3;
            suffix = " kW";
          } else if (abs < 1) {
            scaled = value * 1000;
            suffix = " mW";
          } else {
            suffix = " W";
          }
        } else if (unitType === "voltage") {
          if (abs >= 1e3) {
            scaled = value / 1e3;
            suffix = " kV";
          } else if (abs < 1) {
            scaled = value * 1000;
            suffix = " mV";
          } else {
            suffix = " V";
          }
        }

        return scaled.toFixed(3).replace(/\.?0+$/, "") + suffix;
      }

      function countDefined(values) {
        return values.filter((v) => v !== null && !isNaN(v)).length;
      }

      function solveOhmsLaw() {
        const vIn = document.getElementById("voltageValue").value;
        const vUnit = document.getElementById("voltageUnit").value;
        const iIn = document.getElementById("currentValue").value;
        const iUnit = document.getElementById("currentUnit").value;
        const rIn = document.getElementById("resistanceValue").value;
        const rUnit = document.getElementById("resistanceUnit").value;
        const pIn = document.getElementById("powerValue").value;
        const pUnit = document.getElementById("powerUnit").value;

        let V = toBaseVoltage(vIn, vUnit);
        let I = toBaseCurrent(iIn, iUnit);
        let R = toBaseResistance(rIn, rUnit);
        let P = toBasePower(pIn, pUnit);

        const knownCount = countDefined([V, I, R, P]);
        const statusEl = document.getElementById("statusMessage");

        if (knownCount < 2) {
          statusEl.textContent =
            "Enter at least two values to perform a calculation.";
          statusEl.className = "text-sm font-medium text-red-600";
          updateResults(null, null, null, null, null);
          return;
        }

        // Copy originals for consistency check
        const orig = { V, I, R, P };

        // Solve using combinations
        // Priority: use Ohm's law first, then power relations
        // Case 1: V & I known
        if (V != null && I != null) {
          R = V / I;
          P = V * I;
        }
        // Case 2: V & R known
        else if (V != null && R != null) {
          I = V / R;
          P = V * I;
        }
        // Case 3: I & R known
        else if (I != null && R != null) {
          V = I * R;
          P = V * I;
        }
        // Case 4: V & P known
        else if (V != null && P != null) {
          I = P / V;
          R = V / I;
        }
        // Case 5: I & P known
        else if (I != null && P != null) {
          V = P / I;
          R = V / I;
        }
        // Case 6: R & P known
        else if (R != null && P != null) {
          I = Math.sqrt(P / R);
          V = I * R;
        }

        // Consistency check if user provided more than 2 values
        let consistent = true;
        let mildlyInconsistent = false;
        const tol = 1e-6;

        function checkApproxEqual(a, b) {
          if (a == null || b == null) return true;
          if (!isFinite(a) || !isFinite(b)) return false;
          const diff = Math.abs(a - b);
          const scale = Math.max(1, Math.abs(a), Math.abs(b));
          return diff / scale < 1e-3; // 0.1% relative tolerance
        }

        if (knownCount > 2) {
          if (orig.V != null && !checkApproxEqual(orig.V, V))
            consistent = false;
          if (orig.I != null && !checkApproxEqual(orig.I, I))
            consistent = false;
          if (orig.R != null && !checkApproxEqual(orig.R, R))
            consistent = false;
          if (orig.P != null && !checkApproxEqual(orig.P, P))
            consistent = false;

          // If not strictly consistent, check if they are close enough to be mild
          if (!consistent) {
            mildlyInconsistent = true;
          }
        }

        if (!isFinite(V) || !isFinite(I) || !isFinite(R) || !isFinite(P)) {
          statusEl.textContent =
            "The given values lead to an invalid result. Check for zeros or impossible combinations.";
          statusEl.className = "text-sm font-medium text-red-600";
          updateResults(null, null, null, null, null);
          return;
        }

        if (consistent || knownCount === 2) {
          statusEl.textContent =
            "Calculation successful. All values are consistent with Ohm’s law and power equations.";
          statusEl.className = "text-sm font-medium text-green-700";
        } else if (mildlyInconsistent) {
          statusEl.textContent =
            "Results computed, but your inputs are slightly inconsistent. Likely due to rounding or unit mix-ups.";
          statusEl.className = "text-sm font-medium text-yellow-700";
        }

        updateResults(V, I, R, P, true);
      }

      function updateResults(V, I, R, P, showEngineering) {
        const vEl = document.getElementById("resultVoltage");
        const iEl = document.getElementById("resultCurrent");
        const rEl = document.getElementById("resultResistance");
        const pEl = document.getElementById("resultPower");
        const engEl = document.getElementById("resultEngineering");

        if (V == null || I == null || R == null || P == null) {
          vEl.textContent = "—";
          iEl.textContent = "—";
          rEl.textContent = "—";
          pEl.textContent = "—";
          if (engEl) {
            engEl.textContent =
              "Enter at least two values to see results here in convenient units (mA, kΩ, kW, etc.).";
          }
          return;
        }

        vEl.textContent = V.toFixed(6).replace(/\.?0+$/, "") + " V";
        iEl.textContent = I.toFixed(6).replace(/\.?0+$/, "") + " A";
        rEl.textContent = R.toFixed(6).replace(/\.?0+$/, "") + " Ω";
        pEl.textContent = P.toFixed(6).replace(/\.?0+$/, "") + " W";

        if (showEngineering && engEl) {
          engEl.innerHTML =
            "<strong>Voltage:</strong> " +
            formatEngineering(V, "voltage") +
            "<br>" +
            "<strong>Current:</strong> " +
            formatEngineering(I, "current") +
            "<br>" +
            "<strong>Resistance:</strong> " +
            formatEngineering(R, "resistance") +
            "<br>" +
            "<strong>Power:</strong> " +
            formatEngineering(P, "power");
        }
      }

      document
        .getElementById("solveButton")
        .addEventListener("click", solveOhmsLaw);

      document
        .getElementById("clearButton")
        .addEventListener("click", function () {
          [
            "voltageValue",
            "currentValue",
            "resistanceValue",
            "powerValue",
          ].forEach((id) => {
            document.getElementById(id).value = "";
          });
          document.getElementById("statusMessage").textContent = "";
          document.getElementById("statusMessage").className =
            "text-sm font-medium text-gray-700";
          updateResults(null, null, null, null, null);
        });

      // Example buttons
      document.querySelectorAll(".example-btn").forEach((btn) => {
        btn.addEventListener("click", () => {
          document.getElementById("voltageValue").value = btn.dataset.v || "";
          document.getElementById("voltageUnit").value = btn.dataset.vu || "V";
          document.getElementById("currentValue").value = btn.dataset.i || "";
          document.getElementById("currentUnit").value = btn.dataset.iu || "A";
          document.getElementById("resistanceValue").value =
            btn.dataset.r || "";
          document.getElementById("resistanceUnit").value =
            btn.dataset.ru || "ohm";
          document.getElementById("powerValue").value = btn.dataset.p || "";
          document.getElementById("powerUnit").value = btn.dataset.pu || "W";
          solveOhmsLaw();
          window.scrollTo({ top: 0, behavior: "smooth" });
        });
      });

      // Mobile menu toggle (from template)
      document
        .getElementById("mobile-menu-toggle")
        ?.addEventListener("click", () => {
          const menu = document.getElementById("mobile-menu");
          if (!menu) return;
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          document
            .getElementById("mobile-menu-toggle")
            .setAttribute("aria-expanded", isHidden ? "true" : "false");
        });
    </script>

