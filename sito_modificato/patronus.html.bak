      const state = {
        answers: {}, // questionId -> optionIndex
        scores: {},
      };

      function initScores() {
        state.scores = {};
        Object.keys(patronusArchetypes).forEach((key) => {
          state.scores[key] = 0;
        });
      }

      function renderQuiz() {
        const container = document.getElementById("quiz-container");
        container.innerHTML = "";
        quizQuestions.forEach((q, qIndex) => {
          const card = document.createElement("div");
          card.className =
            "border border-gray-200 rounded-lg p-4 bg-white card-hover";
          card.dataset.questionId = q.id;

          const header = document.createElement("div");
          header.className = "flex items-start justify-between mb-3";
          const title = document.createElement("h3");
          title.className = "font-semibold";
          title.textContent = `Question ${qIndex + 1}: ${q.text}`;
          const badge = document.createElement("span");
          badge.className =
            "text-xs px-2 py-0.5 rounded-full bg-gray-100 text-gray-600";
          badge.textContent = "Unanswered";
          badge.id = `badge-q-${q.id}`;
          header.appendChild(title);
          header.appendChild(badge);

          const optionsWrapper = document.createElement("div");
          optionsWrapper.className = "mt-2 space-y-2";
          q.options.forEach((opt, optIndex) => {
            const btn = document.createElement("button");
            btn.type = "button";
            btn.className =
              "w-full text-left border border-gray-200 rounded-md px-3 py-2 text-sm hover:border-blue-400 hover:bg-blue-50 focus:outline-none focus:ring-2 focus:ring-blue-400";
            btn.textContent = opt.text;
            btn.dataset.questionId = q.id;
            btn.dataset.optionIndex = optIndex;
            btn.addEventListener("click", () => handleAnswer(q.id, optIndex));
            optionsWrapper.appendChild(btn);
          });

          card.appendChild(header);
          card.appendChild(optionsWrapper);
          container.appendChild(card);
        });
      }

      function handleAnswer(questionId, optionIndex) {
        state.answers[questionId] = optionIndex;
        updateUISelection(questionId, optionIndex);
        updateProgress();
        if (Object.keys(state.answers).length === quizQuestions.length) {
          calculateResult();
        }
      }

      function updateUISelection(questionId, optionIndex) {
        const card = document.querySelector(
          `[data-question-id="${questionId}"]`
        );
        if (!card) return;
        const buttons = card.querySelectorAll("button[data-question-id]");
        buttons.forEach((btn) => {
          if (parseInt(btn.dataset.optionIndex, 10) === optionIndex) {
            btn.classList.add("option-selected");
          } else {
            btn.classList.remove("option-selected");
          }
        });
        const badge = document.getElementById(`badge-q-${questionId}`);
        if (badge) {
          badge.textContent = "Answered";
          badge.className =
            "text-xs px-2 py-0.5 rounded-full bg-emerald-100 text-emerald-700";
        }
      }

      function updateProgress() {
        const answered = Object.keys(state.answers).length;
        const total = quizQuestions.length;
        const progressText = document.getElementById("quiz-progress-text");
        if (progressText) {
          progressText.textContent = `${answered} / ${total} questions answered`;
        }
      }

      function calculateResult() {
        initScores();
        quizQuestions.forEach((q) => {
          const optIndex = state.answers[q.id];
          if (typeof optIndex === "number") {
            const opt = q.options[optIndex];
            Object.entries(opt.scores).forEach(([key, val]) => {
              state.scores[key] += val;
            });
          }
        });

        // Find highest score
        let bestKey = null;
        let bestScore = -Infinity;
        Object.entries(state.scores).forEach(([key, val]) => {
          if (val > bestScore) {
            bestScore = val;
            bestKey = key;
          }
        });

        if (!bestKey) return;

        const archetype = patronusArchetypes[bestKey];
        const resultSection = document.getElementById("result-section");
        const summaryEl = document.getElementById("result-summary");
        const meaningEl = document.getElementById("result-meaning");
        const traitsEl = document.getElementById("result-traits");
        const badgeEl = document.getElementById("result-badge");
        const explanationEl = document.getElementById("result-explanation");

        resultSection.classList.remove("hidden");
        summaryEl.textContent = `${archetype.summary}`;
        meaningEl.textContent = archetype.meaning;

        const traitsHtml = `
      <p class="font-semibold mb-1">Key traits of your Patronus:</p>
      <ul class="list-disc ml-5">
        ${archetype.traits.map((t) => `<li>${t}</li>`).join("")}
      </ul>
    `;
        traitsEl.innerHTML = traitsHtml;

        badgeEl.classList.remove("hidden");
        badgeEl.textContent = archetype.name;

        const sortedScores = Object.entries(state.scores)
          .sort((a, b) => b[1] - a[1])
          .map(([key, val]) => `${patronusArchetypes[key].name}: ${val}`)
          .join(" · ");

        explanationEl.textContent = `Each of your answers added points to different Patronus archetypes. The highest total was for ${archetype.name}, which matched your choices most strongly. For transparency, here are your relative scores: ${sortedScores}.`;
      }

      function resetQuiz() {
        state.answers = {};
        initScores();
        renderQuiz();
        updateProgress();
        const resultSection = document.getElementById("result-section");
        if (resultSection) {
          resultSection.classList.add("hidden");
        }
      }

      document.addEventListener("DOMContentLoaded", () => {
        initScores();
        renderQuiz();
        updateProgress();

        const startBtn = document.getElementById("start-quiz-btn");
        const resetBtn = document.getElementById("reset-quiz-btn");

        if (startBtn) {
          startBtn.addEventListener("click", () => {
            const firstCard = document.querySelector("#quiz-container > div");
            if (firstCard) {
              firstCard.scrollIntoView({ behavior: "smooth", block: "start" });
              firstCard.classList.add("question-active");
              setTimeout(
                () => firstCard.classList.remove("question-active"),
                800
              );
            }
          });
        }

        if (resetBtn) {
          resetBtn.addEventListener("click", () => {
            resetQuiz();
            const top = document.getElementById("patronus-calculator-heading");
            if (top) {
              top.scrollIntoView({ behavior: "smooth", block: "start" });
            }
          });
        }
      });
    </script>

