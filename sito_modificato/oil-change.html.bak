    <script>
      // Utility: parse date safely
      function parseDateInput(value) {
        if (!value) return null;
        const d = new Date(value + "T00:00:00");
        return isNaN(d.getTime()) ? null : d;
      }

      function formatDateShort(d) {
        if (!(d instanceof Date) || isNaN(d.getTime())) return "—";
        const y = d.getFullYear();
        const m = String(d.getMonth() + 1).padStart(2, "0");
        const day = String(d.getDate()).padStart(2, "0");
        return `${y}-${m}-${day}`;
      }

      function daysBetween(a, b) {
        const ms = b.getTime() - a.getTime();
        return ms / (1000 * 60 * 60 * 24);
      }

      function addMonths(date, months) {
        const d = new Date(date.getTime());
        const day = d.getDate();
        d.setMonth(d.getMonth() + months);
        if (d.getDate() < day) {
          d.setDate(0); // last day of previous month
        }
        return d;
      }

      function clamp(val, min, max) {
        return Math.min(max, Math.max(min, val));
      }

      document.addEventListener("DOMContentLoaded", function () {
        const unitSystemEl = document.getElementById("unitSystem");
        const avgPerMonthUnitEl = document.getElementById("avgPerMonthUnit");
        const manualIntervalUnitEl =
          document.getElementById("manualIntervalUnit");
        const calculateBtn = document.getElementById("calculateBtn");
        const resetBtn = document.getElementById("resetBtn");
        const errorMsg = document.getElementById("errorMsg");

        const resultsSection = document.getElementById("resultsSection");
        const summaryInterval = document.getElementById("summaryInterval");
        const summaryNextOdo = document.getElementById("summaryNextOdo");
        const summaryNextDate = document.getElementById("summaryNextDate");
        const summaryRemainingDistance = document.getElementById(
          "summaryRemainingDistance"
        );
        const summaryRemainingTime = document.getElementById(
          "summaryRemainingTime"
        );
        const oilLifeBar = document.getElementById("oilLifeBar");
        const oilLifePercent = document.getElementById("oilLifePercent");
        const oilLifeRemaining = document.getElementById("oilLifeRemaining");
        const statusLabel = document.getElementById("statusLabel");

        function updateUnitLabels() {
          const unit = unitSystemEl.value === "km" ? "km" : "mi";
          avgPerMonthUnitEl.textContent = unit + "/month";
          manualIntervalUnitEl.textContent = unit;
        }

        unitSystemEl.addEventListener("change", updateUnitLabels);
        updateUnitLabels();

        resetBtn.addEventListener("click", function () {
          document.getElementById("oilChangeForm").reset();
          updateUnitLabels();
          errorMsg.textContent = "";
          resultsSection.classList.add("hidden");
          oilLifeBar.style.width = "0%";
          oilLifeBar.className = "h-3 rounded-full bg-green-500";
          oilLifePercent.textContent = "0%";
          oilLifeRemaining.textContent = "100%";
          statusLabel.textContent = "";
          statusLabel.className = "mt-2 text-sm font-medium";
        });

        calculateBtn.addEventListener("click", function () {
          errorMsg.textContent = "";

          const lastOdo = parseFloat(
            document.getElementById("lastChangeOdo").value
          );
          const currentOdo = parseFloat(
            document.getElementById("currentOdo").value
          );
          const lastDate = parseDateInput(
            document.getElementById("lastChangeDate").value
          );
          const avgPerMonth = parseFloat(
            document.getElementById("avgPerMonth").value
          );
          const manualInterval = parseFloat(
            document.getElementById("manualInterval").value
          );
          const manualIntervalMonths = parseFloat(
            document.getElementById("manualIntervalMonths").value
          );
          const oilType = document.getElementById("oilType").value;

          const cityHighway =
            parseFloat(document.getElementById("cityHighway").value) || 0;
          const shortTrips =
            parseFloat(document.getElementById("shortTrips").value) || 0;
          const towing =
            parseFloat(document.getElementById("towing").value) || 0;
          const climate =
            parseFloat(document.getElementById("climate").value) || 0;

          // Basic validation
          if (isNaN(lastOdo) || isNaN(currentOdo) || currentOdo < lastOdo) {
            errorMsg.textContent =
              "Please enter valid odometer readings (current must be ≥ last change).";
            resultsSection.classList.add("hidden");
            return;
          }
          if (!lastDate) {
            errorMsg.textContent =
              "Please enter the date of your last oil change.";
            resultsSection.classList.add("hidden");
            return;
          }
          if (
            isNaN(manualInterval) ||
            manualInterval <= 0 ||
            isNaN(manualIntervalMonths) ||
            manualIntervalMonths <= 0
          ) {
            errorMsg.textContent =
              "Please enter a valid manufacturer interval (distance and months).";
            resultsSection.classList.add("hidden");
            return;
          }

          const today = new Date();
          if (lastDate > today) {
            errorMsg.textContent =
              "The last oil change date cannot be in the future.";
            resultsSection.classList.add("hidden");
            return;
          }

          // Oil factor
          let oilFactor = 1.0;
          if (oilType === "conventional") oilFactor = 0.9;
          else if (oilType === "blend") oilFactor = 1.0;
          else if (oilType === "full") oilFactor = 1.1;

          // Severity factor: start at 1, subtract penalties
          let severity = 1.0;
          severity -= (cityHighway / 100) * 0.15; // up to -0.15
          severity -= (shortTrips / 100) * 0.2; // up to -0.20
          severity -= (towing / 100) * 0.2; // up to -0.20
          severity -= (climate / 100) * 0.15; // up to -0.15
          severity = clamp(severity, 0.6, 1.1);

          const adjustedInterval = manualInterval * oilFactor * severity;

          // Distance-based next change
          const nextOdo = lastOdo + adjustedInterval;
          const distanceSinceChange = currentOdo - lastOdo;
          const distanceRemaining = nextOdo - currentOdo;

          // Time-based next change
          const timeLimitDate = addMonths(lastDate, manualIntervalMonths);
          const daysSinceChange = daysBetween(lastDate, today);
          const daysTotal = daysBetween(lastDate, timeLimitDate);
          const daysRemaining = daysBetween(today, timeLimitDate);

          // Estimate next change date using average monthly distance if provided
          let nextDateByMileage = null;
          if (!isNaN(avgPerMonth) && avgPerMonth > 0) {
            const unitPerDay = avgPerMonth / 30.4375; // average days per month
            const daysToNext =
              distanceRemaining > 0 ? distanceRemaining / unitPerDay : 0;
            nextDateByMileage = new Date(
              today.getTime() + daysToNext * 24 * 60 * 60 * 1000
            );
          }

          // Choose earlier of mileage-based and time-based
          let finalNextDate = timeLimitDate;
          if (nextDateByMileage && nextDateByMileage < finalNextDate) {
            finalNextDate = nextDateByMileage;
          }

          // Oil life used (based on both distance and time)
          const distanceFraction =
            adjustedInterval > 0
              ? clamp(distanceSinceChange / adjustedInterval, 0, 1.5)
              : 0;
          const timeFraction =
            daysTotal > 0 ? clamp(daysSinceChange / daysTotal, 0, 1.5) : 0;
          const combinedFraction = Math.max(distanceFraction, timeFraction);
          const usedPercent = clamp(combinedFraction * 100, 0, 150);
          const remainingPercent = clamp(100 - usedPercent, -50, 100);

          // Update UI
          summaryInterval.textContent =
            adjustedInterval.toFixed(0) +
            " " +
            (unitSystemEl.value === "km" ? "km" : "mi");
          summaryNextOdo.textContent =
            nextOdo.toFixed(0) +
            " " +
            (unitSystemEl.value === "km" ? "km" : "mi");
          summaryNextDate.textContent = formatDateShort(finalNextDate);

          if (distanceRemaining >= 0) {
            summaryRemainingDistance.textContent =
              distanceRemaining.toFixed(0) +
              " " +
              (unitSystemEl.value === "km" ? "km" : "mi");
          } else {
            summaryRemainingDistance.textContent =
              Math.abs(distanceRemaining).toFixed(0) +
              " " +
              (unitSystemEl.value === "km" ? "km" : "mi") +
              " past due";
          }

          if (daysRemaining >= 0) {
            summaryRemainingTime.textContent =
              Math.round(daysRemaining) + " days";
          } else {
            summaryRemainingTime.textContent =
              Math.abs(Math.round(daysRemaining)) + " days past due";
          }

          const barWidth = clamp(usedPercent, 0, 150);
          oilLifeBar.style.width = Math.min(barWidth, 100) + "%";
          oilLifePercent.textContent = usedPercent.toFixed(0) + "%";
          oilLifeRemaining.textContent =
            Math.max(0, remainingPercent).toFixed(0) + "%";

          // Status color and label
          oilLifeBar.className = "h-3 rounded-full";
          statusLabel.className = "mt-2 text-sm font-medium";

          if (combinedFraction < 0.8) {
            oilLifeBar.classList.add("bg-green-500");
            statusLabel.classList.add("text-green-700");
            statusLabel.textContent =
              "Oil life OK – no immediate change needed based on your inputs.";
          } else if (combinedFraction < 1.0) {
            oilLifeBar.classList.add("bg-yellow-400");
            statusLabel.classList.add("text-yellow-700");
            statusLabel.textContent =
              "Plan your next oil change soon – you are approaching the recommended interval.";
          } else {
            oilLifeBar.classList.add("bg-red-500");
            statusLabel.classList.add("text-red-700");
            statusLabel.textContent =
              "Change oil now – you are at or past the recommended interval.";
          }

          resultsSection.classList.remove("hidden");
        });
      });
    </script>

