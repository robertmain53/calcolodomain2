        }

        function parseNumbers(raw) {
          const tokens = raw
            .replace(/[\n\r]+/g, ' ')
            .split(/[\s,;]+/)
            .map(t => t.trim())
            .filter(t => t.length > 0);
          const nums = [];
          tokens.forEach(t => {
            const val = Number(t);
            if (Number.isFinite(val) && Number.isInteger(val)) {
              nums.push(val);
            }
          });
          return nums;
        }

        function gcdTwo(a, b) {
          a = Math.abs(a);
          b = Math.abs(b);
          if (a === 0 && b === 0) return 0;
          if (b === 0) return a;
          while (b !== 0) {
            const r = a % b;
            a = b;
            b = r;
          }
          return a;
        }

        function gcdList(arr) {
          let g = 0;
          for (let i = 0; i < arr.length; i++) {
            g = gcdTwo(g, arr[i]);
            if (g === 1) return 1; // early stop
          }
          return Math.abs(g);
        }

        function factorize(n) {
          const factors = [];
          let x = Math.abs(n);
          if (x === 0 || x === 1) return factors;

          let count = 0;
          while (x % 2 === 0) {
            x / 2;
            count++;
          }
          if (count > 0) factors.push({ p: 2, a: count });

          let f = 3;
          const limit = Math.floor(Math.sqrt(x)) + 1;
          while (f <= limit && x > 1) {
            count = 0;
            while (x % f === 0) {
              x / f;
              count++;
            }
            if (count > 0) factors.push({ p: f, a: count });
            f += 2;
          }

          if (x > 1) {
            factors.push({ p: x, a: 1 });
          }

          return factors;
        }

        function formatFactorization(n, factors) {
          if (n === 0) return '0 (special case)';
          if (n === 1) return '1';
          if (!factors || factors.length === 0) return '1';
          const parts = factors.map(fa => fa.a === 1 ? String(fa.p) : (fa.p + '^' + fa.a));
          return parts.join(' · ');
        }

        function commonFactorsFromGcf(g) {
          const res = [];
          const limit = Math.floor(Math.sqrt(g));
          for (let d = 1; d <= limit; d++) {
            if (g % d === 0) {
              res.push(d);
              const other = g / d;
              if (other !== d) res.push(other);
            }
          }
          res.sort((a, b) => a - b);
          return res;
        }

        function buildEuclidSteps(a, b) {
          const steps = [];
          let x = Math.abs(a);
          let y = Math.abs(b);
          if (x < y) {
            const temp = x;
            x = y;
            y = temp;
          }
          if (y === 0) {
            steps.push('One of the numbers is 0. By convention gcd(a, 0) = |a| = ' + x + '.');
            return steps;
          }
          while (y !== 0) {
            const q = Math.floor(x / y);
            const r = x % y;
            steps.push(x + ' = ' + q + ' × ' + y + ' + ' + r);
            x = y;
            y = r;
          }
          steps.push('Last non-zero remainder is ' + x + ', hence gcd = ' + x + '.');
          return steps;
        }

        function computeGcf() {
          clearResults();

          const raw = numbersInput.value || '';
          const nums = parseNumbers(raw);
          if (nums.length < 2) {
            errorEl.textContent = 'Please enter at least two valid integers.';
            return;
          }

          // If all numbers are 0, GCF undefined
          const allZero = nums.every(n => n === 0);
          if (allZero) {
            errorEl.textContent = 'All numbers are 0. The GCF is undefined in this special case.';
            return;
          }

          const g = gcdList(nums);
          setResult(resGcf, String(g));
          setResult(resNumbers, nums.join(', '));

          // Ratio / simplified form (if exactly 2 numbers)
          if (nums.length === 2) {
            const a = nums[0];
            const b = nums[1];
            if (g !== 0) {
              const sa = a / g;
              const sb = b / g;
              setResult(resRatio, a + ' : ' + b + ' = ' + sa + ' : ' + sb + ' (dividing both by ' + g + ')');
            } else {
              setResult(resRatio, 'At least one of the numbers is 0; use the non-zero value as baseline.');
            }
          } else {
            if (g !== 0) {
              const simplified = nums.map(n => n / g);
              setResult(
                resRatio,
                'Divide all numbers by GCF = ' + g + ' to get the simplest integer ratio: ' + simplified.join(' : ')
              );
            } else {
              setResult(resRatio, 'At least one number is 0; only non-zero numbers contribute to the GCF.');
            }
          }

          // Basic checks
          const absNums = nums.map(Math.abs).filter(n => n !== 0);
          const minAbs = absNums.length > 0 ? Math.min.apply(null, absNums) : 0;
          const checks = [];
          checks.push('0 ≤ GCF ≤ min{|aᵢ|}. Here min{|aᵢ|} = ' + minAbs + ', GCF = ' + g + '.');
          if (g === 1) {
            checks.push('The numbers are pairwise coprime (no common factor greater than 1).');
          }
          if (nums.some(n => n === 0) && g !== 0) {
            checks.push('At least one input is 0. By convention, gcd(a, 0) = |a| for non-zero a.');
          }
          setResult(resChecks, checks.join(' '));

          // Prime factorization & common factors
          if (showFactorsCheckbox.checked) {
            sectionFactors.classList.remove('hidden');

            const factLines = nums.map(function (n) {
              const factors = factorize(n);
              return n + ' = ' + formatFactorization(n, factors);
            });
            setResult(resFactors, factLines.join(' ; '));

            if (g > 0) {
              const cf = commonFactorsFromGcf(g);
              const maxDisplay = 50;
              const list = cf.length <= maxDisplay
                ? cf.join(', ')
                : cf.slice(0, maxDisplay).join(', ') + ', …';
              setResult(
                resCommonFactors,
                'Common factors of all numbers are the factors of GCF = ' + g + '. ' +
                'List: ' + list + (cf.length > maxDisplay ? ' (truncated)' : '')
              );
            } else {
              setResult(
                resCommonFactors,
                'GCF is 0 (all numbers may be 0); common factor list is not meaningful.'
              );
            }
          } else {
            sectionFactors.classList.add('hidden');
          }

          // Euclidean steps for first two (if requested)
          if (showStepsCheckbox.checked && nums.length >= 2) {
            sectionSteps.classList.remove('hidden');
            const a = nums[0];
            const b = nums[1];
            const steps = buildEuclidSteps(a, b);
            setResult(resSteps, steps.join(' ⇒ '));
          } else {
            sectionSteps.classList.add('hidden');
          }
        }

        function clearAll() {
          numbersInput.value = '';
          showFactorsCheckbox.checked = true;
          showStepsCheckbox.checked = true;
          clearResults();
        }

        if (btnCalc) btnCalc.addEventListener('click', computeGcf);
        if (btnClear) btnClear.addEventListener('click', clearAll);

        // Example buttons
        exampleButtons.forEach(function (btn) {
          btn.addEventListener('click', function () {
            const val = btn.getAttribute('data-example') || '';
            numbersInput.value = val;
            clearResults();
          });
        });

        // GCF table for small pairs
        function genGcfTable() {
          if (!tableBody) return;
          tableBody.innerHTML = '';

          let maxA = Number(tableMaxA.value);
          let maxB = Number(tableMaxB.value);
          if (!Number.isInteger(maxA) || maxA < 1) maxA = 12;
          if (!Number.isInteger(maxB) || maxB < 1) maxB = 12;
          if (maxA > 30) maxA = 30;
          if (maxB > 30) maxB = 30;
          tableMaxA.value = String(maxA);
          tableMaxB.value = String(maxB);

          for (let a = 1; a <= maxA; a++) {
            for (let b = 1; b <= maxB; b++) {
              const g = gcdTwo(a, b);
              const tr = document.createElement('tr');
              tr.className = 'border-b';

              const tdA = document.createElement('td');
              tdA.className = 'px-3 py-1.5 text-gray-700';
              tdA.textContent = String(a);

              const tdB = document.createElement('td');
              tdB.className = 'px-3 py-1.5 text-gray-700';
              tdB.textContent = String(b);

              const tdG = document.createElement('td');
              tdG.className = 'px-3 py-1.5 text-gray-700';
              tdG.textContent = String(g);

              const tdC = document.createElement('td');
              tdC.className = 'px-3 py-1.5 text-gray-700';
              if (g === 1) {
                tdC.textContent = 'Yes (coprime)';
                tdC.classList.add('text-emerald-700');
              } else {
                tdC.textContent = 'No (share factor ' + g + ')';
                tdC.classList.add('text-blue-700');
              }

              tr.appendChild(tdA);
              tr.appendChild(tdB);
              tr.appendChild(tdG);
              tr.appendChild(tdC);
              tableBody.appendChild(tr);
            }
          }
        }

        if (btnGenTable) btnGenTable.addEventListener('click', genGcfTable);

        // Prepopulate table with defaults
        genGcfTable();
      });
    </script>

