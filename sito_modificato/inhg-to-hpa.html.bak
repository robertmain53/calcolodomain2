    <script>
      // Core conversion factor
      const FACTOR_INHG_TO_HPA = 33.8638866667;

      const inputEl = document.getElementById("input-value");
      const outputEl = document.getElementById("output-value");
      const inputUnitLabel = document.getElementById("input-unit-label");
      const outputUnitLabel = document.getElementById("output-unit-label");
      const factorTextEl = document.getElementById("conversion-factor-text");

      const modeInhgToHpaBtn = document.getElementById("mode-inhg-to-hpa");
      const modeHpaToInhgBtn = document.getElementById("mode-hpa-to-inhg");
      const swapBtn = document.getElementById("swap-button");

      let mode = "inhg-to-hpa"; // or 'hpa-to-inhg'

      function formatNumber(value, decimals) {
        if (!isFinite(value)) return "";
        return value.toFixed(decimals);
      }

      function convert() {
        const raw = parseFloat(inputEl.value);
        if (isNaN(raw)) {
          outputEl.value = "";
          return;
        }

        let result;
        if (mode === "inhg-to-hpa") {
          result = raw * FACTOR_INHG_TO_HPA;
          outputEl.value = formatNumber(result, 1);
        } else {
          result = raw / FACTOR_INHG_TO_HPA;
          outputEl.value = formatNumber(result, 2);
        }
      }

      function updateMode(newMode) {
        mode = newMode;

        if (mode === "inhg-to-hpa") {
          modeInhgToHpaBtn.classList.add("toggle-btn-active");
          modeInhgToHpaBtn.classList.remove("toggle-btn-inactive");
          modeHpaToInhgBtn.classList.add("toggle-btn-inactive");
          modeHpaToInhgBtn.classList.remove("toggle-btn-active");

          inputUnitLabel.textContent = "inHg";
          outputUnitLabel.textContent = "hPa";
          factorTextEl.textContent = "1 inHg = 33.8639 hPa";
        } else {
          modeHpaToInhgBtn.classList.add("toggle-btn-active");
          modeHpaToInhgBtn.classList.remove("toggle-btn-inactive");
          modeInhgToHpaBtn.classList.add("toggle-btn-inactive");
          modeInhgToHpaBtn.classList.remove("toggle-btn-active");

          inputUnitLabel.textContent = "hPa";
          outputUnitLabel.textContent = "inHg";
          factorTextEl.textContent = "1 hPa = 0.02953 inHg";
        }

        convert();
      }

      modeInhgToHpaBtn.addEventListener("click", () =>
        updateMode("inhg-to-hpa")
      );
      modeHpaToInhgBtn.addEventListener("click", () =>
        updateMode("hpa-to-inhg")
      );
      swapBtn.addEventListener("click", () => {
        updateMode(mode === "inhg-to-hpa" ? "hpa-to-inhg" : "inhg-to-hpa");
      });

      inputEl.addEventListener("input", convert);

      // Initialize with a common default value (standard pressure)
      window.addEventListener("DOMContentLoaded", () => {
        inputEl.value = "29.92";
        convert();
        buildReferenceTable();
      });

      // Build quick reference table
      function buildReferenceTable() {
        const tbody = document.getElementById("reference-table-body");
        if (!tbody) return;

        const rows = [
          { inhg: 28.0, note: "Low pressure" },
          { inhg: 28.5, note: "" },
          { inhg: 29.0, note: "" },
          { inhg: 29.5, note: "" },
          { inhg: 29.92, note: "Standard sea-level pressure" },
          { inhg: 30.0, note: "" },
          { inhg: 30.5, note: "" },
          { inhg: 31.0, note: "High pressure" },
        ];

        tbody.innerHTML = "";
        rows.forEach((row) => {
          const hpa = row.inhg * FACTOR_INHG_TO_HPA;
          const tr = document.createElement("tr");
          tr.className = "odd:bg-white even:bg-gray-50";

          const tdInhg = document.createElement("td");
          tdInhg.className = "px-3 py-2 border-b text-gray-800";
          tdInhg.textContent = row.inhg.toFixed(2);

          const tdHpa = document.createElement("td");
          tdHpa.className = "px-3 py-2 border-b text-gray-800";
          tdHpa.textContent = hpa.toFixed(1);

          const tdNote = document.createElement("td");
          tdNote.className =
            "px-3 py-2 border-b text-gray-500 hidden sm:table-cell";
          tdNote.textContent = row.note || "";

          tr.appendChild(tdInhg);
          tr.appendChild(tdHpa);
          tr.appendChild(tdNote);
          tbody.appendChild(tr);
        });
      }

      // Mobile menu script (simple)
      document
        .getElementById("mobile-menu-toggle")
        ?.addEventListener("click", () => {
          const menu = document.getElementById("mobile-menu");
          if (!menu) return;
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          document
            .getElementById("mobile-menu-toggle")
            .setAttribute("aria-expanded", String(isHidden));
        });
    </script>

