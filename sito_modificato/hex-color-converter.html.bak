    <script>
      // Utility: clamp and round
      function clamp(v, min, max) {
        return Math.min(max, Math.max(min, v));
      }
      function round(v, d) {
        const f = Math.pow(10, d);
        return Math.round(v * f) / f;
      }

      // Parsing helpers
      function parseHex(input) {
        if (!input) return null;
        let s = input.trim();
        if (s.startsWith("#")) s = s.slice(1);
        if (![3, 4, 6, 8].includes(s.length)) return null;
        if (!/^[0-9a-fA-F]+$/.test(s)) return null;

        if (s.length === 3 || s.length === 4) {
          s = s
            .split("")
            .map((ch) => ch + ch)
            .join("");
        }
        let r = parseInt(s.slice(0, 2), 16);
        let g = parseInt(s.slice(2, 4), 16);
        let b = parseInt(s.slice(4, 6), 16);
        let a = 255;
        if (s.length === 8) a = parseInt(s.slice(6, 8), 16);
        return { r, g, b, a: a / 255 };
      }

      function rgbToHex(r, g, b, a = 1) {
        const toHex = (v) =>
          clamp(v | 0, 0, 255)
            .toString(16)
            .padStart(2, "0")
            .toUpperCase();
        const base = "#" + toHex(r) + toHex(g) + toHex(b);
        if (a >= 1) return base;
        const alpha = toHex(Math.round(clamp(a, 0, 1) * 255));
        return base + alpha;
      }

      function parseRgb(input) {
        if (!input) return null;
        let s = input.trim().toLowerCase();
        const rgbRe =
          /^rgba?\s*\(\s*([0-9]{1,3})\s*,\s*([0-9]{1,3})\s*,\s*([0-9]{1,3})(?:\s*,\s*([0-9.]+))?\s*\)$/;
        const m = s.match(rgbRe);
        if (!m) return null;
        let r = clamp(parseInt(m[1], 10), 0, 255);
        let g = clamp(parseInt(m[2], 10), 0, 255);
        let b = clamp(parseInt(m[3], 10), 0, 255);
        let a = m[4] !== undefined ? clamp(parseFloat(m[4]), 0, 1) : 1;
        return { r, g, b, a };
      }

      function parseHsl(input) {
        if (!input) return null;
        let s = input.trim().toLowerCase();
        const hslRe =
          /^hsla?\s*\(\s*([0-9.]+)\s*,\s*([0-9.]+)%\s*,\s*([0-9.]+)%(?:\s*,\s*([0-9.]+))?\s*\)$/;
        const m = s.match(hslRe);
        if (!m) return null;
        let h = ((parseFloat(m[1]) % 360) + 360) % 360;
        let sat = clamp(parseFloat(m[2]), 0, 100) / 100;
        let l = clamp(parseFloat(m[3]), 0, 100) / 100;
        let a = m[4] !== undefined ? clamp(parseFloat(m[4]), 0, 1) : 1;
        return { h, s: sat, l, a };
      }

      // Conversions
      function rgbToHsl(r, g, b, a = 1) {
        r / 255;
        g / 255;
        b / 255;
        const max = Math.max(r, g, b),
          min = Math.min(r, g, b);
        let h,
          s,
          l = (max + min) / 2;
        if (max === min) {
          h = s = 0;
        } else {
          const d = max - min;
          s = d / (1 - Math.abs(2 * l - 1));
          switch (max) {
            case r:
              h = 60 * (((g - b) / d) % 6);
              break;
            case g:
              h = 60 * ((b - r) / d + 2);
              break;
            case b:
              h = 60 * ((r - g) / d + 4);
              break;
          }
        }
        if (h < 0) h += 360;
        return { h, s, l, a };
      }

      function hslToRgb(h, s, l, a = 1) {
        const C = (1 - Math.abs(2 * l - 1)) * s;
        const X = C * (1 - Math.abs(((h / 60) % 2) - 1));
        const m = l - C / 2;
        let r1, g1, b1;
        if (h < 60) {
          r1 = C;
          g1 = X;
          b1 = 0;
        } else if (h < 120) {
          r1 = X;
          g1 = C;
          b1 = 0;
        } else if (h < 180) {
          r1 = 0;
          g1 = C;
          b1 = X;
        } else if (h < 240) {
          r1 = 0;
          g1 = X;
          b1 = C;
        } else if (h < 300) {
          r1 = X;
          g1 = 0;
          b1 = C;
        } else {
          r1 = C;
          g1 = 0;
          b1 = X;
        }
        return {
          r: Math.round((r1 + m) * 255),
          g: Math.round((g1 + m) * 255),
          b: Math.round((b1 + m) * 255),
          a,
        };
      }

      function rgbToCmyk(r, g, b) {
        const R = r / 255,
          G = g / 255,
          B = b / 255;
        const K = 1 - Math.max(R, G, B);
        if (K === 0) return { c: 0, m: 0, y: 0, k: 0 };
        const C = (1 - R - K) / (1 - K);
        const M = (1 - G - K) / (1 - K);
        const Y = (1 - B - K) / (1 - K);
        return { c: C, m: M, y: Y, k: K };
      }

      // sRGB to XYZ (D65)
      function rgbToXyz(r, g, b) {
        let R = r / 255,
          G = g / 255,
          B = b / 255;
        const f = (v) =>
          v <= 0.04045 ? v / 12.92 : Math.pow((v + 0.055) / 1.055, 2.4);
        R = f(R);
        G = f(G);
        B = f(B);
        const X = R * 0.4124 + G * 0.3576 + B * 0.1805;
        const Y = R * 0.2126 + G * 0.7152 + B * 0.0722;
        const Z = R * 0.0193 + G * 0.1192 + B * 0.9505;
        return { X: X * 100, Y: Y * 100, Z: Z * 100 };
      }

      // XYZ to LAB (D65)
      function xyzToLab(X, Y, Z) {
        const refX = 95.047,
          refY = 100.0,
          refZ = 108.883;
        let x = X / refX,
          y = Y / refY,
          z = Z / refZ;
        const f = (t) => (t > 0.008856 ? Math.cbrt(t) : 7.787 * t + 16 / 116);
        x = f(x);
        y = f(y);
        z = f(z);
        const L = 116 * y - 16;
        const a = 500 * (x - y);
        const b = 200 * (y - z);
        return { L, a, b };
      }

      function labToLch(L, a, b) {
        const C = Math.sqrt(a * a + b * b);
        let H = (Math.atan2(b, a) * 180) / Math.PI;
        if (H < 0) H += 360;
        return { L, C, H };
      }

      // UI wiring
      document.addEventListener("DOMContentLoaded", () => {
        const hexInput = document.getElementById("hex-input");
        const rgbInput = document.getElementById("rgb-input");
        const hslInput = document.getElementById("hsl-input");
        const cmykInput = document.getElementById("cmyk-input");
        const labInput = document.getElementById("lab-input");
        const lchInput = document.getElementById("lch-input");
        const xyzInput = document.getElementById("xyz-input");
        const colorPicker = document.getElementById("color-picker");
        const preview = document.getElementById("color-preview");

        const sliderR = document.getElementById("slider-r");
        const sliderG = document.getElementById("slider-g");
        const sliderB = document.getElementById("slider-b");
        const sliderA = document.getElementById("slider-a");
        const sliderRVal = document.getElementById("slider-r-val");
        const sliderGVal = document.getElementById("slider-g-val");
        const sliderBVal = document.getElementById("slider-b-val");
        const sliderAVal = document.getElementById("slider-a-val");

        const sampleWhite = document.getElementById("sample-on-white");
        const sampleBlack = document.getElementById("sample-on-black");
        const tintsShades = document.getElementById("tints-shades");

        let internal = { r: 255, g: 87, b: 51, a: 1 };
        let updating = false;

        function updatePreview() {
          const { r, g, b, a } = internal;
          const rgba = `rgba(${r}, ${g}, ${b}, ${round(a, 2)})`;
          preview.style.backgroundColor = rgba;
          colorPicker.value = rgbToHex(r, g, b).slice(0, 7);

          // contrast samples
          sampleWhite.style.backgroundColor = "#ffffff";
          sampleWhite.style.color = rgba;
          sampleBlack.style.backgroundColor = "#000000";
          sampleBlack.style.color = rgba;

          // tints & shades
          tintsShades.innerHTML = "";
          const stops = [-0.4, -0.2, 0, 0.2, 0.4];
          stops.forEach((delta) => {
            const factor = delta;
            const rr = clamp(
              r + (delta > 0 ? (255 - r) * factor : r * factor),
              0,
              255
            );
            const gg = clamp(
              g + (delta > 0 ? (255 - g) * factor : g * factor),
              0,
              255
            );
            const bb = clamp(
              b + (delta > 0 ? (255 - b) * factor : b * factor),
              0,
              255
            );
            const div = document.createElement("div");
            div.style.backgroundColor = `rgb(${rr | 0}, ${gg | 0}, ${bb | 0})`;
            div.title = rgbToHex(rr, gg, bb);
            tintsShades.appendChild(div);
          });
        }

        function updateSliders() {
          sliderR.value = internal.r;
          sliderG.value = internal.g;
          sliderB.value = internal.b;
          sliderA.value = Math.round(internal.a * 100);
          sliderRVal.textContent = internal.r;
          sliderGVal.textContent = internal.g;
          sliderBVal.textContent = internal.b;
          sliderAVal.textContent = Math.round(internal.a * 100) + "%";
        }

        function updateTextFields() {
          const { r, g, b, a } = internal;
          const hex = rgbToHex(r, g, b, a);
          hexInput.value = hex;

          const rgbStr =
            a >= 1
              ? `rgb(${r}, ${g}, ${b})`
              : `rgba(${r}, ${g}, ${b}, ${round(a, 2)})`;
          rgbInput.value = rgbStr;

          const hsl = rgbToHsl(r, g, b, a);
          const h = round(hsl.h, 1);
          const s = Math.round(hsl.s * 100);
          const l = Math.round(hsl.l * 100);
          const hslStr =
            a >= 1
              ? `hsl(${h}, ${s}%, ${l}%)`
              : `hsla(${h}, ${s}%, ${l}%, ${round(a, 2)})`;
          hslInput.value = hslStr;

          const cmyk = rgbToCmyk(r, g, b);
          cmykInput.value = `cmyk(${Math.round(cmyk.c * 100)}%, ${Math.round(
            cmyk.m * 100
          )}%, ${Math.round(cmyk.y * 100)}%, ${Math.round(cmyk.k * 100)}%)`;

          const xyz = rgbToXyz(r, g, b);
          xyzInput.value = `xyz(${round(xyz.X, 2)}, ${round(xyz.Y, 2)}, ${round(
            xyz.Z,
            2
          )})`;

          const lab = xyzToLab(xyz.X, xyz.Y, xyz.Z);
          labInput.value = `lab(${round(lab.L, 2)}, ${round(lab.a, 2)}, ${round(
            lab.b,
            2
          )})`;

          const lch = labToLch(lab.L, lab.a, lab.b);
          lchInput.value = `lch(${round(lch.L, 2)}, ${round(lch.C, 2)}, ${round(
            lch.H,
            2
          )})`;
        }

        function syncAll() {
          if (updating) return;
          updating = true;
          updatePreview();
          updateSliders();
          updateTextFields();
          updating = false;
        }

        // Initial state
        syncAll();

        // Input handlers
        hexInput.addEventListener("input", () => {
          const parsed = parseHex(hexInput.value);
          if (!parsed) return;
          internal = parsed;
          syncAll();
        });

        rgbInput.addEventListener("input", () => {
          const parsed = parseRgb(rgbInput.value);
          if (!parsed) return;
          internal = parsed;
          syncAll();
        });

        hslInput.addEventListener("input", () => {
          const parsed = parseHsl(hslInput.value);
          if (!parsed) return;
          const rgb = hslToRgb(parsed.h, parsed.s, parsed.l, parsed.a);
          internal = rgb;
          syncAll();
        });

        colorPicker.addEventListener("input", () => {
          const parsed = parseHex(colorPicker.value);
          if (!parsed) return;
          parsed.a = internal.a; // keep alpha
          internal = parsed;
          syncAll();
        });

        [sliderR, sliderG, sliderB, sliderA].forEach((slider) => {
          slider.addEventListener("input", () => {
            internal.r = parseInt(sliderR.value, 10);
            internal.g = parseInt(sliderG.value, 10);
            internal.b = parseInt(sliderB.value, 10);
            internal.a = parseInt(sliderA.value, 10) / 100;
            syncAll();
          });
        });

        // Copy buttons
        document.querySelectorAll(".copy-btn").forEach((btn) => {
          btn.addEventListener("click", () => {
            const targetId = btn.getAttribute("data-copy-target");
            const input = document.getElementById(targetId);
            if (!input) return;
            navigator.clipboard
              .writeText(input.value)
              .then(() => {
                const original = btn.textContent;
                btn.textContent = "Copied";
                setTimeout(() => {
                  btn.textContent = original;
                }, 1200);
              })
              .catch(() => {});
          });
        });
      });
    </script>

