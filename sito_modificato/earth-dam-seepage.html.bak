    <script>
      (function () {
        const form = document.getElementById("seepage-form");
        const calcBtn = document.getElementById("calculateBtn");
        const resetBtn = document.getElementById("resetBtn");
        const errorMsg = document.getElementById("errorMsg");

        const qPerMeterEl = document.getElementById("qPerMeter");
        const qTotalEl = document.getElementById("qTotal");
        const exitGradientEl = document.getElementById("exitGradient");
        const fsPipingEl = document.getElementById("fsPiping");
        const fsPipingStatusEl = document.getElementById("fsPipingStatus");

        const canvas = document.getElementById("phreaticCanvas");
        const ctx = canvas.getContext("2d");

        function formatNumber(val, decimals) {
          if (!isFinite(val)) return "–";
          return Number(val).toFixed(decimals);
        }

        function clearResults() {
          qPerMeterEl.textContent = "–";
          qTotalEl.textContent = "–";
          exitGradientEl.textContent = "–";
          fsPipingEl.textContent = "–";
          fsPipingStatusEl.textContent = "";
          drawEmptyPhreatic();
        }

        function drawEmptyPhreatic() {
          ctx.clearRect(0, 0, canvas.width, canvas.height);
          ctx.fillStyle = "#f9fafb";
          ctx.fillRect(0, 0, canvas.width, canvas.height);
          ctx.fillStyle = "#9ca3af";
          ctx.font = "12px Inter, sans-serif";
          ctx.textAlign = "center";
          ctx.fillText(
            "Phreatic line will appear here after calculation.",
            canvas.width / 2,
            canvas.height / 2
          );
        }

        function drawPhreatic(Hu, Leff) {
          const width = canvas.width;
          const height = canvas.height;
          ctx.clearRect(0, 0, width, height);
          ctx.fillStyle = "#f9fafb";
          ctx.fillRect(0, 0, width, height);

          if (Hu <= 0 || Leff <= 0) {
            drawEmptyPhreatic();
            return;
          }

          const margin = 30;
          const plotWidth = width - 2 * margin;
          const plotHeight = height - 2 * margin;

          // Axes
          ctx.strokeStyle = "#d1d5db";
          ctx.lineWidth = 1;
          ctx.beginPath();
          ctx.moveTo(margin, margin);
          ctx.lineTo(margin, height - margin);
          ctx.lineTo(width - margin, height - margin);
          ctx.stroke();

          // Phreatic line: y = Hu * (1 - (x/Leff)^2)
          ctx.strokeStyle = "#2563eb";
          ctx.lineWidth = 2;
          ctx.beginPath();
          const steps = 50;
          for (let i = 0; i <= steps; i++) {
            const x = (Leff * i) / steps;
            const y = Hu * (1 - Math.pow(x / Leff, 2));
            const px = margin + (x / Leff) * plotWidth;
            const py = height - margin - (y / Hu) * plotHeight;
            if (i === 0) ctx.moveTo(px, py);
            else ctx.lineTo(px, py);
          }
          ctx.stroke();

          // Labels
          ctx.fillStyle = "#4b5563";
          ctx.font = "11px Inter, sans-serif";
          ctx.textAlign = "left";
          ctx.fillText("Upstream", margin + 4, margin + 12);
          ctx.textAlign = "right";
          ctx.fillText("Downstream", width - margin - 4, margin + 12);
        }

        function calculate() {
          errorMsg.textContent = "";
          const H = parseFloat(document.getElementById("damHeight").value);
          const B = parseFloat(document.getElementById("crestWidth").value);
          const m = parseFloat(document.getElementById("upstreamSlope").value);
          const n = parseFloat(
            document.getElementById("downstreamSlope").value
          );
          const hu = parseFloat(document.getElementById("upstreamHead").value);
          const hd = parseFloat(
            document.getElementById("downstreamHead").value
          );
          const k = parseFloat(document.getElementById("permeability").value);
          const Ldam = parseFloat(document.getElementById("damLength").value);
          const icr = parseFloat(
            document.getElementById("criticalGradient").value
          );
          const Ld = parseFloat(document.getElementById("toeDrain").value);

          if ([H, B, m, n, hu, k, Ldam, icr].some((v) => isNaN(v) || v <= 0)) {
            errorMsg.textContent =
              "Please enter positive values for all required fields.";
            clearResults();
            return;
          }
          if (hu > H + 0.001) {
            errorMsg.textContent =
              "Upstream water depth cannot exceed dam height.";
            clearResults();
            return;
          }
          if (hd < 0) {
            errorMsg.textContent =
              "Downstream tailwater depth cannot be negative.";
            clearResults();
            return;
          }
          const Hnet = hu - Math.max(hd, 0);
          if (Hnet <= 0) {
            errorMsg.textContent =
              "Net head must be positive (upstream water level must exceed downstream).";
            clearResults();
            return;
          }

          const Lbase = m * H + B + n * H;
          const Leff = Lbase + Math.max(Ld, 0);

          // Seepage discharge per unit length (simplified)
          const qPerMeter = (k * Hnet * Hnet) / Leff; // m3/s per m
          const qTotal = qPerMeter * Ldam;

          // Exit gradient and FS
          const iexit = Hnet / Leff;
          const fsPiping = icr / iexit;

          qPerMeterEl.textContent = formatNumber(qPerMeter, 6);
          qTotalEl.textContent = formatNumber(qTotal, 6);
          exitGradientEl.textContent = formatNumber(iexit, 3);
          fsPipingEl.textContent = formatNumber(fsPiping, 2);

          if (fsPiping < 1) {
            fsPipingStatusEl.textContent =
              "Unsafe: exit gradient exceeds critical gradient (FS < 1).";
            fsPipingStatusEl.className = "text-sm text-red-600 mt-1";
          } else if (fsPiping < 3) {
            fsPipingStatusEl.textContent =
              "Marginal: consider improving drainage or filters (FS between 1 and 3).";
            fsPipingStatusEl.className = "text-sm text-yellow-600 mt-1";
          } else {
            fsPipingStatusEl.textContent =
              "Acceptable for preliminary design (FS ≥ 3). Always verify with detailed analysis.";
            fsPipingStatusEl.className = "text-sm text-green-600 mt-1";
          }

          drawPhreatic(hu, Leff);
        }

        calcBtn.addEventListener("click", calculate);

        resetBtn.addEventListener("click", () => {
          form.reset();
          document.getElementById("damHeight").value = 20;
          document.getElementById("crestWidth").value = 8;
          document.getElementById("upstreamSlope").value = 3;
          document.getElementById("downstreamSlope").value = 2.5;
          document.getElementById("upstreamHead").value = 18;
          document.getElementById("downstreamHead").value = 0;
          document.getElementById("permeability").value = 1e-6;
          document.getElementById("damLength").value = 200;
          document.getElementById("criticalGradient").value = 0.9;
          document.getElementById("toeDrain").value = 5;
          errorMsg.textContent = "";
          clearResults();
        });

        // Initialize
        drawEmptyPhreatic();
      })();
    </script>
  
