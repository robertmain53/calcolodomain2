    <script>
      // Utility functions for normal distribution
      function normPDF(x) {
        return Math.exp(-0.5 * x * x) / Math.sqrt(2 * Math.PI);
      }

      function normCDF(x) {
        // Abramowitz & Stegun approximation
        var k = 1.0 / (1.0 + 0.2316419 * Math.abs(x));
        var kSum =
          k *
          (0.31938153 +
            k *
              (-0.356563782 +
                k * (1.781477937 + k * (-1.821255978 + 1.330274429 * k))));
        var cnd = 1.0 - normPDF(x) * kSum;
        return x < 0 ? 1.0 - cnd : cnd;
      }

      function blackScholesPrice(S, K, T, r, q, sigma, isCall) {
        if (T <= 0 || sigma <= 0 || S <= 0 || K <= 0) return NaN;
        var sqrtT = Math.sqrt(T);
        var d1 =
          (Math.log(S / K) + (r - q + 0.5 * sigma * sigma) * T) /
          (sigma * sqrtT);
        var d2 = d1 - sigma * sqrtT;
        if (isCall) {
          return (
            S * Math.exp(-q * T) * normCDF(d1) -
            K * Math.exp(-r * T) * normCDF(d2)
          );
        } else {
          return (
            K * Math.exp(-r * T) * normCDF(-d2) -
            S * Math.exp(-q * T) * normCDF(-d1)
          );
        }
      }

      function blackScholesVega(S, K, T, r, q, sigma) {
        if (T <= 0 || sigma <= 0 || S <= 0 || K <= 0) return 0;
        var sqrtT = Math.sqrt(T);
        var d1 =
          (Math.log(S / K) + (r - q + 0.5 * sigma * sigma) * T) /
          (sigma * sqrtT);
        return S * Math.exp(-q * T) * normPDF(d1) * sqrtT; // per 1.0 volatility (not %)
      }

      function blackScholesGreeks(S, K, T, r, q, sigma, isCall) {
        if (T <= 0 || sigma <= 0 || S <= 0 || K <= 0) {
          return { delta: NaN, gamma: NaN, vega: NaN, theta: NaN, rho: NaN };
        }
        var sqrtT = Math.sqrt(T);
        var d1 =
          (Math.log(S / K) + (r - q + 0.5 * sigma * sigma) * T) /
          (sigma * sqrtT);
        var d2 = d1 - sigma * sqrtT;
        var Nd1 = normCDF(d1);
        var Nd2 = normCDF(d2);
        var Nmd1 = normCDF(-d1);
        var Nmd2 = normCDF(-d2);
        var pdfd1 = normPDF(d1);

        var delta, theta, rho;
        if (isCall) {
          delta = Math.exp(-q * T) * Nd1;
          theta =
            (-S * pdfd1 * sigma * Math.exp(-q * T)) / (2 * sqrtT) -
            r * K * Math.exp(-r * T) * Nd2 +
            q * S * Math.exp(-q * T) * Nd1;
          rho = K * T * Math.exp(-r * T) * Nd2;
        } else {
          delta = -Math.exp(-q * T) * Nmd1;
          theta =
            (-S * pdfd1 * sigma * Math.exp(-q * T)) / (2 * sqrtT) +
            r * K * Math.exp(-r * T) * Nmd2 -
            q * S * Math.exp(-q * T) * Nmd1;
          rho = -K * T * Math.exp(-r * T) * Nmd2;
        }

        var gamma = (Math.exp(-q * T) * pdfd1) / (S * sigma * sqrtT);
        var vega = S * Math.exp(-q * T) * pdfd1 * sqrtT; // per 1.0 vol

        // Convert theta to per-day (assuming 365 days)
        var thetaPerDay = theta / 365.0;

        return {
          delta: delta,
          gamma: gamma,
          vega: vega,
          theta: thetaPerDay,
          rho: rho,
        };
      }

      function impliedVolatility(marketPrice, S, K, T, r, q, isCall) {
        var MAX_ITER = 100;
        var TOL = 1e-6;
        var sigmaLow = 0.0001;
        var sigmaHigh = 5.0; // 500% annualized
        var sigma = 0.2; // initial guess 20%

        for (var i = 0; i < MAX_ITER; i++) {
          var price = blackScholesPrice(S, K, T, r, q, sigma, isCall);
          if (!isFinite(price)) break;
          var diff = price - marketPrice;

          if (Math.abs(diff) < TOL) {
            return {
              sigma: sigma,
              price: price,
              error: diff,
              iterations: i + 1,
              converged: true,
            };
          }

          var vega = blackScholesVega(S, K, T, r, q, sigma);
          if (vega <= 1e-8) {
            // Fallback to bisection if vega is too small
            break;
          }

          var sigmaNew = sigma - diff / vega;
          if (
            sigmaNew < sigmaLow ||
            sigmaNew > sigmaHigh ||
            !isFinite(sigmaNew)
          ) {
            break;
          }
          sigma = sigmaNew;
        }

        // Bisection fallback
        var low = sigmaLow;
        var high = sigmaHigh;
        var mid = 0;
        var priceLow =
          blackScholesPrice(S, K, T, r, q, low, isCall) - marketPrice;
        var priceHigh =
          blackScholesPrice(S, K, T, r, q, high, isCall) - marketPrice;

        if (priceLow * priceHigh > 0) {
          return {
            sigma: NaN,
            price: NaN,
            error: NaN,
            iterations: MAX_ITER,
            converged: false,
          };
        }

        for (var j = 0; j < MAX_ITER; j++) {
          mid = 0.5 * (low + high);
          var priceMid =
            blackScholesPrice(S, K, T, r, q, mid, isCall) - marketPrice;

          if (Math.abs(priceMid) < TOL) {
            return {
              sigma: mid,
              price: marketPrice + priceMid,
              error: priceMid,
              iterations: MAX_ITER + j + 1,
              converged: true,
            };
          }

          if (priceLow * priceMid < 0) {
            high = mid;
            priceHigh = priceMid;
          } else {
            low = mid;
            priceLow = priceMid;
          }
        }

        return {
          sigma: mid,
          price: blackScholesPrice(S, K, T, r, q, mid, isCall),
          error: NaN,
          iterations: 2 * MAX_ITER,
          converged: false,
        };
      }

      document.addEventListener("DOMContentLoaded", function () {
        var isCall = true;
        var callBtn = document.getElementById("callBtn");
        var putBtn = document.getElementById("putBtn");
        var timeConvention = document.getElementById("timeConvention");
        var timeUnitLabel = document.getElementById("timeUnitLabel");
        var calcBtn = document.getElementById("calcBtn");
        var resetBtn = document.getElementById("resetBtn");
        var errorMsg = document.getElementById("errorMsg");

        function setOptionType(call) {
          isCall = call;
          if (call) {
            callBtn.classList.add("bg-blue-600", "text-white");
            callBtn.classList.remove("bg-white", "text-gray-700");
            putBtn.classList.remove("bg-blue-600", "text-white");
            putBtn.classList.add("bg-white", "text-gray-700");
          } else {
            putBtn.classList.add("bg-blue-600", "text-white");
            putBtn.classList.remove("bg-white", "text-gray-700");
            callBtn.classList.remove("bg-blue-600", "text-white");
            callBtn.classList.add("bg-white", "text-gray-700");
          }
        }

        callBtn.addEventListener("click", function () {
          setOptionType(true);
        });
        putBtn.addEventListener("click", function () {
          setOptionType(false);
        });

        timeConvention.addEventListener("change", function () {
          if (this.value === "years") {
            timeUnitLabel.textContent = "years";
          } else {
            timeUnitLabel.textContent = "days";
          }
        });

        function parseInput(id) {
          var el = document.getElementById(id);
          var val = parseFloat(el.value);
          return isNaN(val) ? NaN : val;
        }

        function formatPercent(x) {
          if (!isFinite(x)) return "–";
          return (x * 100).toFixed(2) + " %";
        }

        function formatNumber(x, digits) {
          if (!isFinite(x)) return "–";
          return x.toFixed(digits);
        }

        function clearResults() {
          document.getElementById("ivResult").textContent = "–";
          document.getElementById("theoPrice").textContent = "–";
          document.getElementById("priceError").textContent = "–";
          document.getElementById("iterations").textContent = "–";
          document.getElementById("converged").textContent = "–";
          document.getElementById("delta").textContent = "–";
          document.getElementById("gamma").textContent = "–";
          document.getElementById("vega").textContent = "–";
          document.getElementById("theta").textContent = "–";
          document.getElementById("rho").textContent = "–";
        }

        function computeIV() {
          errorMsg.classList.add("hidden");
          errorMsg.textContent = "";
          clearResults();

          var S = parseInput("underlyingPrice");
          var K = parseInput("strikePrice");
          var marketPrice = parseInput("optionPrice");
          var tVal = parseInput("timeToExpiry");
          var rPct = parseInput("rate");
          var qPct = parseInput("divYield");

          if (
            !isFinite(S) ||
            !isFinite(K) ||
            !isFinite(marketPrice) ||
            !isFinite(tVal) ||
            !isFinite(rPct) ||
            !isFinite(qPct)
          ) {
            errorMsg.textContent =
              "Please fill in all fields with valid numbers.";
            errorMsg.classList.remove("hidden");
            return;
          }
          if (S <= 0 || K <= 0 || marketPrice <= 0 || tVal <= 0) {
            errorMsg.textContent =
              "Prices and time to expiry must be positive.";
            errorMsg.classList.remove("hidden");
            return;
          }

          var T;
          var conv = timeConvention.value;
          if (conv === "years") {
            T = tVal;
          } else if (conv === "days_252") {
            T = tVal / 252.0;
          } else {
            T = tVal / 365.0;
          }

          var r = rPct / 100.0;
          var q = qPct / 100.0;

          // Basic arbitrage sanity checks
          var intrinsic = isCall ? Math.max(0, S - K) : Math.max(0, K - S);
          if (marketPrice < intrinsic - 1e-8) {
            errorMsg.textContent =
              "The option price is below intrinsic value. No valid implied volatility exists under Black–Scholes.";
            errorMsg.classList.remove("hidden");
            return;
          }

          var ivRes = impliedVolatility(marketPrice, S, K, T, r, q, isCall);
          if (!isFinite(ivRes.sigma) || ivRes.sigma <= 0) {
            errorMsg.textContent =
              "Could not find a stable implied volatility for these inputs. Try adjusting the price or parameters.";
            errorMsg.classList.remove("hidden");
            return;
          }

          document.getElementById("ivResult").textContent = formatPercent(
            ivRes.sigma
          );
          document.getElementById("theoPrice").textContent = formatNumber(
            ivRes.price,
            4
          );
          document.getElementById("priceError").textContent = isFinite(
            ivRes.error
          )
            ? formatNumber(ivRes.error, 6)
            : "–";
          document.getElementById("iterations").textContent = ivRes.iterations;
          document.getElementById("converged").textContent = ivRes.converged
            ? "Yes"
            : "Approximate";

          var greeks = blackScholesGreeks(S, K, T, r, q, ivRes.sigma, isCall);
          document.getElementById("delta").textContent = formatNumber(
            greeks.delta,
            4
          );
          document.getElementById("gamma").textContent = formatNumber(
            greeks.gamma,
            6
          );
          document.getElementById("vega").textContent = formatNumber(
            greeks.vega / 100.0,
            4
          ); // per 1% vol
          document.getElementById("theta").textContent = formatNumber(
            greeks.theta,
            6
          );
          document.getElementById("rho").textContent = formatNumber(
            greeks.rho / 100.0,
            4
          ); // per 1% rate
        }

        calcBtn.addEventListener("click", computeIV);

        resetBtn.addEventListener("click", function () {
          document.getElementById("underlyingPrice").value = 100;
          document.getElementById("strikePrice").value = 100;
          document.getElementById("optionPrice").value = 3;
          document.getElementById("timeToExpiry").value = 30;
          document.getElementById("timeConvention").value = "days_365";
          timeUnitLabel.textContent = "days";
          document.getElementById("rate").value = 2;
          document.getElementById("divYield").value = 0;
          setOptionType(true);
          clearResults();
          errorMsg.classList.add("hidden");
          errorMsg.textContent = "";
        });
      });

