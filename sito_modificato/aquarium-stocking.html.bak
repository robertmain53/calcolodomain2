    <script>
      // Aquarium stocking calculator logic
      (function () {
        const speciesContainer = document.getElementById("speciesContainer");
        const template = document.getElementById("speciesRowTemplate");
        const addBtn = document.getElementById("addSpeciesBtn");
        const calcBtn = document.getElementById("calculateBtn");

        function addSpeciesRow(preset) {
          const clone = template.content.cloneNode(true);
          const row = clone.querySelector("div.border");
          if (preset) {
            row.querySelector(".species-name").value = preset.name || "";
            row.querySelector(".species-size").value = preset.size || "";
            row.querySelector(".species-size-unit").value = preset.unit || "in";
            row.querySelector(".species-count").value = preset.count || 1;
            row.querySelector(".species-temperament").value =
              preset.temperament || "peaceful";
          }
          row.querySelector(".remove-species").addEventListener("click", () => {
            row.remove();
          });
          speciesContainer.appendChild(clone);
        }

        addBtn.addEventListener("click", () => addSpeciesRow());

        // Add a couple of example rows by default
        addSpeciesRow({
          name: "Neon tetra",
          size: 1.2,
          unit: "in",
          count: 6,
          temperament: "peaceful",
        });
        addSpeciesRow({
          name: "Corydoras",
          size: 2,
          unit: "in",
          count: 4,
          temperament: "bottom",
        });

        function getTemperamentFactor(temp) {
          switch (temp) {
            case "peaceful":
              return 1.0;
            case "shrimp-snail":
              return 0.7;
            case "bottom":
              return 0.9;
            case "semi":
              return 1.3;
            case "aggressive":
              return 1.6;
            default:
              return 1.0;
          }
        }

        function getTankTypeFactor(type) {
          switch (type) {
            case "fresh-community":
              return 1.0;
            case "fresh-semi":
              return 0.85;
            case "fresh-cichlid":
              return 0.75;
            case "nano-shrimp":
              return 0.9;
            case "lowtech-planted":
              return 0.95;
            default:
              return 1.0;
          }
        }

        function getExperienceFactor(level) {
          switch (level) {
            case "beginner":
              return 0.8;
            case "intermediate":
              return 1.0;
            case "advanced":
              return 1.1;
            default:
              return 1.0;
          }
        }

        function getFiltrationFactor(turnover) {
          if (!turnover || turnover <= 0) return 1.0;
          if (turnover < 3) return 0.8;
          if (turnover < 5) return 0.9;
          if (turnover < 8) return 1.0;
          if (turnover < 12) return 1.1;
          return 1.15;
        }

        function classifyStocking(pct) {
          if (pct < 60)
            return {
              label: "Lightly stocked (very safe)",
              color: "text-green-700",
            };
          if (pct < 90)
            return {
              label: "Moderately stocked (safe with regular maintenance)",
              color: "text-emerald-700",
            };
          if (pct < 110)
            return {
              label: "Heavily stocked (experienced keepers only)",
              color: "text-amber-700",
            };
          return { label: "Overstocked (high risk)", color: "text-red-700" };
        }

        function calculate() {
          const volumeValue = parseFloat(
            document.getElementById("volumeValue").value
          );
          const volumeUnit = document.getElementById("volumeUnit").value;
          const tankType = document.getElementById("tankType").value;
          const filterTurnover = parseFloat(
            document.getElementById("filterTurnover").value
          );
          const experienceLevel =
            document.getElementById("experienceLevel").value;

          const warningsPanel = document.getElementById("warningsPanel");
          const speciesTableBody = document.getElementById("speciesTableBody");
          warningsPanel.innerHTML = "";
          speciesTableBody.innerHTML = "";

          if (!volumeValue || volumeValue <= 0) {
            warningsPanel.innerHTML =
              '<p class="text-sm text-red-700">Please enter a valid tank volume.</p>';
            document.getElementById("resultsPanel").classList.remove("hidden");
            return;
          }

          let volGal =
            volumeUnit === "gallons" ? volumeValue : volumeValue / 3.785;
          const effectiveVolume = volGal * 0.9; // small displacement factor

          const rows = speciesContainer.querySelectorAll("div.border");
          if (rows.length === 0) {
            warningsPanel.innerHTML =
              '<p class="text-sm text-red-700">Add at least one species to calculate stocking.</p>';
            document.getElementById("resultsPanel").classList.remove("hidden");
            return;
          }

          let totalBioload = 0;
          let compatibilityWarnings = [];
          let aggressiveCount = 0;
          let shrimpCount = 0;
          let bottomCount = 0;

          rows.forEach((row) => {
            const name =
              row.querySelector(".species-name").value.trim() ||
              "Unnamed species";
            const sizeVal = parseFloat(
              row.querySelector(".species-size").value
            );
            const sizeUnit = row.querySelector(".species-size-unit").value;
            const countVal =
              parseInt(row.querySelector(".species-count").value, 10) || 0;
            const temperament = row.querySelector(".species-temperament").value;

            if (!sizeVal || sizeVal <= 0 || countVal <= 0) return;

            let lengthIn = sizeUnit === "in" ? sizeVal : sizeVal / 2.54;
            const tempFactor = getTemperamentFactor(temperament);

            const speciesBioload =
              countVal * Math.pow(lengthIn, 1.5) * tempFactor;
            totalBioload += speciesBioload;

            if (temperament === "aggressive") aggressiveCount += countVal;
            if (temperament === "shrimp-snail") shrimpCount += countVal;
            if (temperament === "bottom") bottomCount += countVal;

            const pct = 0; // filled after capacity known
            const tr = document.createElement("tr");
            tr.innerHTML = `
          <td class="border px-2 py-1">${name}</td>
          <td class="border px-2 py-1 text-right">${countVal}</td>
          <td class="border px-2 py-1 text-right">${lengthIn.toFixed(1)}</td>
          <td class="border px-2 py-1 text-right">${speciesBioload.toFixed(
            1
          )}</td>
          <td class="border px-2 py-1 text-right species-pct" data-bioload="${speciesBioload.toFixed(
            4
          )}">–</td>
        `;
            speciesTableBody.appendChild(tr);
          });

          if (totalBioload === 0) {
            warningsPanel.innerHTML =
              '<p class="text-sm text-red-700">Please enter valid sizes and counts for at least one species.</p>';
            document.getElementById("resultsPanel").classList.remove("hidden");
            return;
          }

          const baseCapacityPerGal = 1.8; // tuned constant
          let capacity = effectiveVolume * baseCapacityPerGal;
          capacity *= getTankTypeFactor(tankType);
          capacity *= getFiltrationFactor(filterTurnover);
          capacity *= getExperienceFactor(experienceLevel);

          const stockingPct = (totalBioload / capacity) * 100;

          document.getElementById("effectiveVolume").textContent =
            effectiveVolume.toFixed(1);
          document.getElementById("totalBioload").textContent =
            totalBioload.toFixed(1);
          document.getElementById("stockingPercent").textContent =
            stockingPct.toFixed(0);

          const cls = classifyStocking(stockingPct);
          const labelEl = document.getElementById("stockingLabel");
          labelEl.textContent = cls.label;
          labelEl.className = "text-xs font-semibold mt-1 " + cls.color;

          // Safe range: depends on experience
          let safeMin = 40;
          let safeMax = 85;
          if (experienceLevel === "beginner") {
            safeMin = 30;
            safeMax = 75;
          } else if (experienceLevel === "advanced") {
            safeMin = 50;
            safeMax = 95;
          }
          document.getElementById("safeMin").textContent = safeMin.toFixed(0);
          document.getElementById("safeMax").textContent = safeMax.toFixed(0);

          // Fill per-species percentages
          speciesTableBody.querySelectorAll(".species-pct").forEach((td) => {
            const b = parseFloat(td.getAttribute("data-bioload"));
            const pct = (b / capacity) * 100;
            td.textContent = pct.toFixed(1) + "%";
          });

          // Build warnings
          const warnings = [];

          if (!filterTurnover || filterTurnover < 4) {
            warnings.push({
              type: "warning",
              text: "Filtration turnover is low. Aim for at least 4–6× tank volume per hour for community tanks.",
            });
          }

          if (stockingPct > safeMax) {
            warnings.push({
              type: "danger",
              text: "Your planned stocking is above the recommended safe range for your experience level. Consider reducing fish numbers or upgrading filtration.",
            });
          } else if (stockingPct < safeMin) {
            warnings.push({
              type: "info",
              text: "Your tank is lightly stocked. This is very safe and leaves room to add more fish gradually.",
            });
          }

          if (aggressiveCount > 0 && rows.length > 1) {
            warnings.push({
              type: "warning",
              text: "You have aggressive species mixed with others. Research compatibility carefully and provide territories and hiding spots.",
            });
          }

          if (shrimpCount > 0 && aggressiveCount > 0) {
            warnings.push({
              type: "danger",
              text: "Aggressive or predatory fish may eat shrimp or snails. Consider separating them or providing dense cover.",
            });
          }

          if (bottomCount > 0 && tankType === "fresh-cichlid") {
            warnings.push({
              type: "warning",
              text: "Bottom dwellers with territorial cichlids can be harassed. Ensure plenty of caves and line-of-sight breaks.",
            });
          }

          if (effectiveVolume < 10) {
            warnings.push({
              type: "info",
              text: "Very small tanks are inherently unstable. Keep stocking light and perform frequent small water changes.",
            });
          }

          if (warnings.length === 0) {
            warnings.push({
              type: "success",
              text: "Your stocking plan looks reasonable on paper. Always confirm with water tests and observe fish behavior.",
            });
          }

          warningsPanel.innerHTML = warnings
            .map((w) => {
              let color = "bg-blue-50 text-blue-800 border-blue-200";
              if (w.type === "warning")
                color = "bg-amber-50 text-amber-800 border-amber-200";
              if (w.type === "danger")
                color = "bg-red-50 text-red-800 border-red-200";
              if (w.type === "success")
                color = "bg-emerald-50 text-emerald-800 border-emerald-200";
              return `<div class="border ${color} text-sm px-3 py-2 rounded">${w.text}</div>`;
            })
            .join("");

          document.getElementById("resultsPanel").classList.remove("hidden");
        }

        calcBtn.addEventListener("click", calculate);
      })();
    </script>

