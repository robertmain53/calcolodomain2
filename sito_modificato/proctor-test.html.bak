    <script src="https://cdn.jsdelivr.net/npm/chart.js@4.4.1/dist/chart.umd.min.js"></script>
    <script>
      // Utility: default rows
      const defaultPointsSI = [
        { wetMass: 1600, w: 8 },
        { wetMass: 1700, w: 10 },
        { wetMass: 1780, w: 12 },
        { wetMass: 1760, w: 14 },
      ];
      const defaultPointsUS = [
        { wetMass: 3.5, w: 8 },
        { wetMass: 3.7, w: 10 },
        { wetMass: 3.9, w: 12 },
        { wetMass: 3.85, w: 14 },
      ];

      const unitEls = {
        moldVolumeLabel: document.getElementById("moldVolumeUnitLabel"),
        wetMassLabel: document.getElementById("wetMassUnitLabel"),
        wetDensityLabel: document.getElementById("wetDensityUnitLabel"),
        dryDensityLabel: document.getElementById("dryDensityUnitLabel"),
        mddUnitLabel: document.getElementById("mddUnitLabel"),
        requiredDryDensityUnitLabel: document.getElementById(
          "requiredDryDensityUnitLabel"
        ),
      };

      const moldVolumeInput = document.getElementById("moldVolume");
      const specificGravityInput = document.getElementById("specificGravity");
      const targetCompactionInput = document.getElementById("targetCompaction");
      const testTypeSelect = document.getElementById("testType");
      const unitSystemSelect = document.getElementById("unitSystem");
      const tbody = document.getElementById("pointsTbody");
      const addRowBtn = document.getElementById("addRowBtn");
      const clearRowsBtn = document.getElementById("clearRowsBtn");
      const calculateBtn = document.getElementById("calculateBtn");

      const resultsSummary = document.getElementById("resultsSummary");
      const omcResult = document.getElementById("omcResult");
      const mddResult = document.getElementById("mddResult");
      const targetCompactionResult = document.getElementById(
        "targetCompactionResult"
      );
      const requiredDryDensityResult = document.getElementById(
        "requiredDryDensityResult"
      );
      const resultsTableWrapper = document.getElementById(
        "resultsTableWrapper"
      );
      const resultsTbody = document.getElementById("resultsTbody");

      let chart;
      const chartCanvas = document.getElementById("proctorChart");

      function updateUnitLabels() {
        const system = unitSystemSelect.value;
        if (system === "si") {
          unitEls.moldVolumeLabel.textContent = "(cm³)";
          unitEls.wetMassLabel.textContent = "(g)";
          unitEls.wetDensityLabel.textContent = "(kN/m³)";
          unitEls.dryDensityLabel.textContent = "(kN/m³)";
          unitEls.mddUnitLabel.textContent = "kN/m³";
          unitEls.requiredDryDensityUnitLabel.textContent = "kN/m³";
        } else {
          unitEls.moldVolumeLabel.textContent = "(ft³)";
          unitEls.wetMassLabel.textContent = "(lb)";
          unitEls.wetDensityLabel.textContent = "(pcf)";
          unitEls.dryDensityLabel.textContent = "(pcf)";
          unitEls.mddUnitLabel.textContent = "pcf";
          unitEls.requiredDryDensityUnitLabel.textContent = "pcf";
        }
      }

      function applyTestTypeDefaults() {
        const type = testTypeSelect.value;
        const system = unitSystemSelect.value;
        if (type === "standard") {
          moldVolumeInput.value = system === "si" ? 944 : (1 / 30).toFixed(4);
        } else if (type === "modified") {
          moldVolumeInput.value =
            system === "si" ? 2124 : (1 / 13.333).toFixed(4);
        }
      }

      function addRow(wetMass = "", w = "") {
        const rowIndex = tbody.children.length + 1;
        const tr = document.createElement("tr");
        tr.innerHTML = `
      <td class="px-2 py-1 border border-gray-200 text-center">${rowIndex}</td>
      <td class="px-2 py-1 border border-gray-200">
        <input type="number" step="any" class="w-24 border rounded px-2 py-1 wet-mass-input" value="${wetMass}">
      </td>
      <td class="px-2 py-1 border border-gray-200">
        <input type="number" step="any" class="w-20 border rounded px-2 py-1 moisture-input" value="${w}">
      </td>
      <td class="px-2 py-1 border border-gray-200 text-gray-500 text-xs wet-density-cell">–</td>
      <td class="px-2 py-1 border border-gray-200 text-gray-500 text-xs dry-density-cell">–</td>
      <td class="px-2 py-1 border border-gray-200 text-gray-500 text-xs saturation-cell">–</td>
      <td class="px-2 py-1 border border-gray-200 text-center">
        <button type="button" class="text-red-600 text-xs remove-row-btn">Remove</button>
      </td>
    `;
        tbody.appendChild(tr);
      }

      function resetRowsWithDefaults() {
        tbody.innerHTML = "";
        const system = unitSystemSelect.value;
        const defaults = system === "si" ? defaultPointsSI : defaultPointsUS;
        defaults.forEach((p) => addRow(p.wetMass, p.w));
      }

      function renumberRows() {
        Array.from(tbody.children).forEach((tr, idx) => {
          tr.children[0].textContent = idx + 1;
        });
      }

      function readRows() {
        const rows = [];
        Array.from(tbody.children).forEach((tr) => {
          const wetMass = parseFloat(tr.querySelector(".wet-mass-input").value);
          const w = parseFloat(tr.querySelector(".moisture-input").value);
          if (!isNaN(wetMass) && !isNaN(w)) {
            rows.push({ wetMass, w });
          }
        });
        return rows;
      }

      function computeDensities() {
        const rows = readRows();
        const system = unitSystemSelect.value;
        const V = parseFloat(moldVolumeInput.value);
        const Gs = parseFloat(specificGravityInput.value);
        if (!rows.length || !V || !Gs) return null;

        const gammaW = system === "si" ? 9.81 : 62.4; // kN/m³ or pcf
        const results = [];

        rows.forEach((row, idx) => {
          const wDec = row.w / 100;
          let gammaWet, gammaDry, Sr;

          if (system === "si") {
            // wetMass in g, V in cm³ → convert to kN/m³
            const rhoWet = row.wetMass / 1000 / (V / 1e6); // kg/m³
            gammaWet = (rhoWet * 9.81) / 1000; // kN/m³
          } else {
            // wetMass in lb, V in ft³ → pcf
            gammaWet = row.wetMass / V; // lb/ft³
          }

          gammaDry = gammaWet / (1 + wDec);
          Sr = ((wDec * Gs * gammaW) / gammaDry) * 100;

          results.push({
            index: idx + 1,
            w: row.w,
            gammaWet,
            gammaDry,
            Sr,
          });
        });

        return results;
      }

      function quadraticFit(points) {
        // Simple least-squares quadratic fit y = ax^2 + bx + c
        const n = points.length;
        if (n < 3) return null;

        let sumX = 0,
          sumX2 = 0,
          sumX3 = 0,
          sumX4 = 0;
        let sumY = 0,
          sumXY = 0,
          sumX2Y = 0;

        points.forEach((p) => {
          const x = p.x;
          const y = p.y;
          const x2 = x * x;
          const x3 = x2 * x;
          const x4 = x2 * x2;

          sumX += x;
          sumX2 += x2;
          sumX3 += x3;
          sumX4 += x4;
          sumY += y;
          sumXY += x * y;
          sumX2Y += x2 * y;
        });

        const A = [
          [sumX4, sumX3, sumX2],
          [sumX3, sumX2, sumX],
          [sumX2, sumX, n],
        ];
        const B = [sumX2Y, sumXY, sumY];

        function det3(m) {
          return (
            m[0][0] * (m[1][1] * m[2][2] - m[1][2] * m[2][1]) -
            m[0][1] * (m[1][0] * m[2][2] - m[1][2] * m[2][0]) +
            m[0][2] * (m[1][0] * m[2][1] - m[1][1] * m[2][0])
          );
        }

        const D = det3(A);
        if (Math.abs(D) < 1e-12) return null;

        const A1 = [
          [B[0], A[0][1], A[0][2]],
          [B[1], A[1][1], A[1][2]],
          [B[2], A[2][1], A[2][2]],
        ];
        const A2 = [
          [A[0][0], B[0], A[0][2]],
          [A[1][0], B[1], A[1][2]],
          [A[2][0], B[2], A[2][2]],
        ];
        const A3 = [
          [A[0][0], A[0][1], B[0]],
          [A[1][0], A[1][1], B[1]],
          [A[2][0], A[2][1], B[2]],
        ];

        const a = det3(A1) / D;
        const b = det3(A2) / D;
        const c = det3(A3) / D;

        return { a, b, c };
      }

      function updateResults() {
        const data = computeDensities();
        if (!data) {
          alert(
            "Please enter valid mold volume, specific gravity, and at least one valid test point."
          );
          return;
        }

        // Update per-row display
        Array.from(tbody.children).forEach((tr, idx) => {
          const rowRes = data[idx];
          if (!rowRes) return;
          tr.querySelector(".wet-density-cell").textContent =
            rowRes.gammaWet.toFixed(2);
          tr.querySelector(".dry-density-cell").textContent =
            rowRes.gammaDry.toFixed(2);
          tr.querySelector(".saturation-cell").textContent =
            rowRes.Sr.toFixed(1);
        });

        // Compute OMC & MDD via quadratic fit
        const points = data.map((d) => ({ x: d.w, y: d.gammaDry }));
        const fit = quadraticFit(points);
        let omc = null,
          mdd = null;

        if (fit && fit.a < 0) {
          omc = -fit.b / (2 * fit.a);
          mdd = fit.a * omc * omc + fit.b * omc + fit.c;
        } else {
          // Fallback: pick max point
          let max = points[0];
          points.forEach((p) => {
            if (p.y > max.y) max = p;
          });
          omc = max.x;
          mdd = max.y;
        }

        const targetComp = parseFloat(targetCompactionInput.value) || 0;
        const requiredDryDensity = mdd * (targetComp / 100);

        omcResult.textContent = omc.toFixed(2);
        mddResult.textContent = mdd.toFixed(2);
        targetCompactionResult.textContent = targetComp.toFixed(1);
        requiredDryDensityResult.textContent = requiredDryDensity.toFixed(2);

        resultsSummary.classList.remove("hidden");

        // Fill results table
        resultsTbody.innerHTML = "";
        data.forEach((d) => {
          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="px-2 py-1 border border-gray-200 text-center">${d.index}</td>
        <td class="px-2 py-1 border border-gray-200">${d.w.toFixed(2)}</td>
        <td class="px-2 py-1 border border-gray-200">${d.gammaWet.toFixed(
          2
        )}</td>
        <td class="px-2 py-1 border border-gray-200">${d.gammaDry.toFixed(
          2
        )}</td>
        <td class="px-2 py-1 border border-gray-200">${d.Sr.toFixed(1)}</td>
      `;
          resultsTbody.appendChild(tr);
        });
        resultsTableWrapper.classList.remove("hidden");

        // Update chart
        const labels = points.map((p) => p.x);
        const dryData = points.map((p) => p.y);

        // Smooth curve
        const minW = Math.min(...labels);
        const maxW = Math.max(...labels);
        const curveW = [];
        const curveY = [];
        const steps = 40;
        for (let i = 0; i <= steps; i++) {
          const w = minW + ((maxW - minW) * i) / steps;
          let y;
          if (fit) {
            y = fit.a * w * w + fit.b * w + fit.c;
          } else {
            // simple linear interpolation fallback
            y = mdd;
          }
          curveW.push(w);
          curveY.push(y);
        }

        if (chart) chart.destroy();
        chart = new Chart(chartCanvas, {
          type: "scatter",
          data: {
            datasets: [
              {
                type: "scatter",
                label: "Dry density points",
                data: points.map((p) => ({ x: p.x, y: p.y })),
                backgroundColor: "#2563eb",
                borderColor: "#2563eb",
                pointRadius: 4,
              },
              {
                type: "line",
                label: "Fitted curve",
                data: curveW.map((w, i) => ({ x: w, y: curveY[i] })),
                borderColor: "#16a34a",
                borderWidth: 2,
                fill: false,
                pointRadius: 0,
                tension: 0.2,
              },
            ],
          },
          options: {
            responsive: true,
            maintainAspectRatio: false,
            aspectRatio: 1.4,
            scales: {
              x: {
                type: "linear",
                title: { display: true, text: "Moisture content, w (%)" },
              },
              y: {
                title: { display: true, text: "Dry density" },
              },
            },
            plugins: {
              legend: { position: "top" },
              tooltip: {
                callbacks: {
                  label: (ctx) => {
                    const x = ctx.parsed.x.toFixed(2);
                    const y = ctx.parsed.y.toFixed(2);
                    return `w = ${x} %, γd = ${y}`;
                  },
                },
              },
            },
          },
        });
      }

      // Event listeners
      unitSystemSelect.addEventListener("change", () => {
        updateUnitLabels();
        applyTestTypeDefaults();
        resetRowsWithDefaults();
      });

      testTypeSelect.addEventListener("change", () => {
        applyTestTypeDefaults();
      });

      addRowBtn.addEventListener("click", () => addRow());
      clearRowsBtn.addEventListener("click", () => {
        tbody.innerHTML = "";
        renumberRows();
      });

      tbody.addEventListener("click", (e) => {
        if (e.target.classList.contains("remove-row-btn")) {
          const tr = e.target.closest("tr");
          tr.remove();
          renumberRows();
        }
      });

      calculateBtn.addEventListener("click", updateResults);

      // Initial setup
      updateUnitLabels();
      applyTestTypeDefaults();
      resetRowsWithDefaults();
    </script>

