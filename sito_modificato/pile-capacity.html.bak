    <script>
      // Utility functions
      function toMeters(value, unit) {
        if (!isFinite(value)) return NaN;
        return unit === "ft" ? value * 0.3048 : value;
      }
      function pressureToKPa(value, unit) {
        if (!isFinite(value)) return NaN;
        // 1 ksf = 47.8803 kPa
        return unit === "ksf" ? value * 47.8803 : value;
      }
      function forceTokN(value, unit) {
        if (!isFinite(value)) return NaN;
        // 1 kip = 4.44822 kN
        return unit === "kips" ? value * 4.44822 : value;
      }
      function kNToKip(value) {
        return value / 4.44822;
      }
      function formatForce(kN) {
        if (!isFinite(kN)) return "–";
        if (kN >= 1000) {
          return (kN / 1000).toFixed(2) + " MN";
        }
        return kN.toFixed(1) + " kN";
      }
      function formatForceAlt(kN) {
        if (!isFinite(kN)) return "";
        const kips = kNToKip(kN);
        return kips.toFixed(2) + " kips";
      }
      function formatArea(m2) {
        if (!isFinite(m2)) return "–";
        const ft2 = m2 * 10.7639;
        return m2.toFixed(3) + " m² (" + ft2.toFixed(2) + " ft²)";
      }

      function updatePileTypeVisibility() {
        const type = document.getElementById("pile-type").value;
        const circ = type === "circular";
        document
          .getElementById("circular-diameter-group")
          .classList.toggle("hidden", !circ);
        document
          .getElementById("rect-width-group")
          .classList.toggle("hidden", circ);
        document
          .getElementById("rect-depth-group")
          .classList.toggle("hidden", circ);
      }

      document
        .getElementById("pile-type")
        .addEventListener("change", updatePileTypeVisibility);
      updatePileTypeVisibility();

      document
        .getElementById("reset-btn")
        .addEventListener("click", function () {
          document.getElementById("pile-form").reset();
          document.getElementById("pile-length").value = 15;
          document.getElementById("pile-diameter").value = 0.6;
          document.getElementById("pile-width").value = 0.4;
          document.getElementById("pile-depth").value = 0.4;
          document.getElementById("fs").value = 80;
          document.getElementById("qb").value = 1500;
          document.getElementById("fs-global").value = 2.5;
          document.getElementById("design-mode").value = "compression";
          document.getElementById("error-msg").textContent = "";
          [
            "ult-comp-main",
            "ult-comp-alt",
            "allow-comp-main",
            "allow-comp-alt",
            "allow-ten-main",
            "allow-ten-alt",
            "As-out",
            "Ab-out",
            "Qs-out",
            "Qb-out",
            "Qult-comp-out",
            "Qult-ten-out",
          ].forEach((id) => {
            document.getElementById(id).textContent = id.endsWith("-alt")
              ? ""
              : "–";
          });
        });

      document
        .getElementById("calculate-btn")
        .addEventListener("click", function () {
          const errEl = document.getElementById("error-msg");
          errEl.textContent = "";

          const type = document.getElementById("pile-type").value;
          const L = toMeters(
            parseFloat(document.getElementById("pile-length").value),
            document.getElementById("length-unit").value
          );
          if (!isFinite(L) || L <= 0) {
            errEl.textContent = "Please enter a valid pile length.";
            return;
          }

          let As = NaN,
            Ab = NaN;

          if (type === "circular") {
            const D = toMeters(
              parseFloat(document.getElementById("pile-diameter").value),
              document.getElementById("diameter-unit").value
            );
            if (!isFinite(D) || D <= 0) {
              errEl.textContent = "Please enter a valid pile diameter.";
              return;
            }
            As = Math.PI * D * L;
            Ab = (Math.PI * D * D) / 4;
          } else {
            const B = toMeters(
              parseFloat(document.getElementById("pile-width").value),
              document.getElementById("width-unit").value
            );
            const b = toMeters(
              parseFloat(document.getElementById("pile-depth").value),
              document.getElementById("depth-unit").value
            );
            if (!isFinite(B) || B <= 0 || !isFinite(b) || b <= 0) {
              errEl.textContent =
                "Please enter valid rectangular pile dimensions.";
              return;
            }
            As = 2 * (B + b) * L;
            Ab = B * b;
          }

          // Resistances
          const fsVal = parseFloat(document.getElementById("fs").value);
          const qbVal = parseFloat(document.getElementById("qb").value);
          const fsKPa = pressureToKPa(
            fsVal,
            document.getElementById("fs-unit").value
          );
          const qbKPa = pressureToKPa(
            qbVal,
            document.getElementById("qb-unit").value
          );

          let Qs = forceTokN(
            parseFloat(document.getElementById("Qs-direct").value),
            document.getElementById("Qs-unit").value
          );
          let Qb = forceTokN(
            parseFloat(document.getElementById("Qb-direct").value),
            document.getElementById("Qb-unit").value
          );

          // If direct values are not provided, compute from unit resistances
          if (!isFinite(Qs) || Qs <= 0) {
            if (isFinite(fsKPa) && fsKPa > 0) {
              // kPa * m² = kN/m² * m² = kN
              Qs = fsKPa * As;
            } else {
              Qs = 0;
            }
          }
          if (!isFinite(Qb) || Qb <= 0) {
            if (isFinite(qbKPa) && qbKPa > 0) {
              Qb = qbKPa * Ab;
            } else {
              Qb = 0;
            }
          }

          const FS = parseFloat(document.getElementById("fs-global").value);
          if (!isFinite(FS) || FS <= 1) {
            errEl.textContent = "Please enter a safety factor greater than 1.";
            return;
          }

          const designMode = document.getElementById("design-mode").value;

          const QultComp = Qs + Qb;
          const QultTen = Qs; // shaft only
          const QallowComp = QultComp / FS;
          const QallowTen = QultTen / FS;

          // Update outputs
          document.getElementById("As-out").textContent = formatArea(As);
          document.getElementById("Ab-out").textContent = formatArea(Ab);
          document.getElementById("Qs-out").textContent =
            formatForce(Qs) + " (" + formatForceAlt(Qs) + ")";
          document.getElementById("Qb-out").textContent =
            formatForce(Qb) + " (" + formatForceAlt(Qb) + ")";
          document.getElementById("Qult-comp-out").textContent =
            formatForce(QultComp) + " (" + formatForceAlt(QultComp) + ")";
          document.getElementById("Qult-ten-out").textContent =
            formatForce(QultTen) + " (" + formatForceAlt(QultTen) + ")";

          if (designMode === "compression" || designMode === "both") {
            document.getElementById("ult-comp-main").textContent =
              formatForce(QultComp);
            document.getElementById("ult-comp-alt").textContent =
              formatForceAlt(QultComp);
            document.getElementById("allow-comp-main").textContent =
              formatForce(QallowComp);
            document.getElementById("allow-comp-alt").textContent =
              formatForceAlt(QallowComp);
          } else {
            document.getElementById("ult-comp-main").textContent = "–";
            document.getElementById("ult-comp-alt").textContent = "";
            document.getElementById("allow-comp-main").textContent = "–";
            document.getElementById("allow-comp-alt").textContent = "";
          }

          if (designMode === "tension" || designMode === "both") {
            document.getElementById("allow-ten-main").textContent =
              formatForce(QallowTen);
            document.getElementById("allow-ten-alt").textContent =
              formatForceAlt(QallowTen);
          } else {
            document.getElementById("allow-ten-main").textContent = "–";
            document.getElementById("allow-ten-alt").textContent = "";
          }
        });

      // Mobile menu toggle
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          this.setAttribute("aria-expanded", isHidden ? "true" : "false");
        });
    </script>

