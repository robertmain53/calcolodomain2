    <script>
      // Mode switching for main calculator
      const modeButtons = document.querySelectorAll(".mode-btn");
      const modeHint = document.getElementById("modeHint");
      let currentMode = "speed"; // 'speed', 'time', 'distance'

      function updateModeHint() {
        let text = "Mode: ";
        if (currentMode === "speed") {
          text +=
            "Calculate Speed – enter distance and time to get average speed.";
        } else if (currentMode === "time") {
          text +=
            "Calculate Time – enter distance and target speed to get ride time.";
        } else {
          text +=
            "Calculate Distance – enter time and average speed to get distance.";
        }
        modeHint.innerHTML = text.replace(
          "Mode:",
          'Mode: <span class="font-semibold">' +
            text.split("–")[0].replace("Mode: ", "") +
            "</span> –"
        );
      }

      modeButtons.forEach((btn) => {
        btn.addEventListener("click", () => {
          currentMode = btn.getAttribute("data-mode");
          modeButtons.forEach((b) => {
            b.classList.remove("bg-blue-600", "text-white", "border-blue-600");
            b.classList.add("bg-white", "text-gray-800", "border-gray-300");
          });
          btn.classList.add("bg-blue-600", "text-white", "border-blue-600");
          btn.classList.remove("bg-white", "text-gray-800", "border-gray-300");
          updateModeHint();
        });
      });

      updateModeHint();

      // Main calculator logic
      const calcMainBtn = document.getElementById("calcMainBtn");
      const mainResultContainer = document.getElementById(
        "mainResultContainer"
      );
      const mainResultPrimary = document.getElementById("mainResultPrimary");
      const mainResultSecondary = document.getElementById(
        "mainResultSecondary"
      );
      const mainResultPacing = document.getElementById("mainResultPacing");

      function getTimeHours() {
        const h = parseFloat(document.getElementById("timeHours").value) || 0;
        const m = parseFloat(document.getElementById("timeMinutes").value) || 0;
        const s = parseFloat(document.getElementById("timeSeconds").value) || 0;
        return h + m / 60 + s / 3600;
      }

      function setTimeFromHours(hours) {
        if (!isFinite(hours) || hours < 0) return;
        const totalSeconds = hours * 3600;
        const h = Math.floor(totalSeconds / 3600);
        const m = Math.floor((totalSeconds % 3600) / 60);
        const s = Math.round(totalSeconds % 60);
        document.getElementById("timeHours").value = h;
        document.getElementById("timeMinutes").value = m;
        document.getElementById("timeSeconds").value = s;
      }

      function kmToMiles(km) {
        return km * 0.621371;
      }
      function milesToKm(mi) {
        return mi / 0.621371;
      }

      function classifySpeed(kmh) {
        if (!isFinite(kmh) || kmh <= 0)
          return "Speed classification unavailable.";
        if (kmh < 15)
          return "Very easy pace – suitable for gentle recovery rides or complete beginners.";
        if (kmh < 20)
          return "Beginner pace – typical for new riders on mostly flat terrain.";
        if (kmh < 24)
          return "Recreational pace – comfortable endurance riding for many cyclists.";
        if (kmh < 28)
          return "Enthusiast pace – solid fitness, often seen on group rides.";
        if (kmh < 32)
          return "Strong amateur pace – requires structured training and good aerodynamics.";
        return "Race‑level pace – typical of competitive cyclists or very fast group rides.";
      }

      calcMainBtn.addEventListener("click", () => {
        const distanceInput = parseFloat(
          document.getElementById("distance").value
        );
        const distanceUnit = document.getElementById("distanceUnit").value;
        let distanceKm = null;

        if (!isNaN(distanceInput) && distanceInput > 0) {
          distanceKm =
            distanceUnit === "km" ? distanceInput : milesToKm(distanceInput);
        }

        const timeHours = getTimeHours();

        let speedKmh = null;
        let resultText = "";
        let secondaryText = "";

        if (currentMode === "speed") {
          if (!distanceKm || !timeHours) {
            alert("Please enter both distance and time to calculate speed.");
            return;
          }
          speedKmh = distanceKm / timeHours;
          const speedMph = kmToMiles(speedKmh);
          resultText = `Average speed: ${speedKmh.toFixed(
            1
          )} km/h (${speedMph.toFixed(1)} mph)`;
          secondaryText = `Distance: ${distanceInput} ${distanceUnit}, Time: ${timeHours.toFixed(
            2
          )} h`;
        } else if (currentMode === "time") {
          // Need distance and speed (from user via distance & time fields)
          if (!distanceKm) {
            alert("Please enter a distance.");
            return;
          }
          // Interpret time inputs as speed if user entered something? That would be confusing.
          // Instead, prompt for target speed via distanceUnit? Better: reuse distance as is, ask for target speed via timeHours field? Too hacky.
          // Implement a simple prompt:
          const speedPrompt = prompt(
            "Enter your target average speed in km/h (e.g. 25):"
          );
          const targetSpeed = parseFloat(speedPrompt);
          if (!targetSpeed || targetSpeed <= 0) {
            alert("Please enter a valid target speed in km/h.");
            return;
          }
          speedKmh = targetSpeed;
          const t = distanceKm / speedKmh;
          setTimeFromHours(t);
          const speedMph = kmToMiles(speedKmh);
          resultText = `Required time: ${t.toFixed(
            2
          )} hours for ${distanceInput} ${distanceUnit} at ${speedKmh.toFixed(
            1
          )} km/h (${speedMph.toFixed(1)} mph).`;
          secondaryText = `That is about ${Math.floor(t)} h ${Math.round(
            (t * 60) % 60
          )} min.`;
        } else if (currentMode === "distance") {
          if (!timeHours) {
            alert("Please enter a time (hours, minutes or seconds).");
            return;
          }
          const speedPrompt = prompt(
            "Enter your average speed in km/h (e.g. 22):"
          );
          const avgSpeed = parseFloat(speedPrompt);
          if (!avgSpeed || avgSpeed <= 0) {
            alert("Please enter a valid average speed in km/h.");
            return;
          }
          speedKmh = avgSpeed;
          const distanceKmResult = speedKmh * timeHours;
          const distanceResult =
            distanceUnit === "km"
              ? distanceKmResult
              : kmToMiles(distanceKmResult);
          document.getElementById("distance").value = distanceResult.toFixed(2);
          const speedMph = kmToMiles(speedKmh);
          resultText = `Distance: ${distanceResult.toFixed(
            2
          )} ${distanceUnit} at ${speedKmh.toFixed(1)} km/h (${speedMph.toFixed(
            1
          )} mph).`;
          secondaryText = `Time: ${timeHours.toFixed(2)} h.`;
        }

        mainResultPrimary.textContent = resultText;
        if (speedKmh) {
          mainResultSecondary.textContent = secondaryText;
          mainResultPacing.textContent = classifySpeed(speedKmh);
        } else {
          mainResultSecondary.textContent = secondaryText;
          mainResultPacing.textContent = "";
        }
        mainResultContainer.classList.remove("hidden");
      });

      // Power-based speed estimator
      const calcPowerBtn = document.getElementById("calcPowerBtn");
      const powerResultContainer = document.getElementById(
        "powerResultContainer"
      );
      const powerResultPrimary = document.getElementById("powerResultPrimary");
      const powerResultSecondary = document.getElementById(
        "powerResultSecondary"
      );

      function lbToKg(lb) {
        return lb * 0.45359237;
      }

      function solveSpeedFromPower(
        P,
        m,
        gradientPercent,
        bikeType,
        wind,
        windUnit
      ) {
        const rho = 1.225; // air density kg/m^3
        const g = 9.81;
        const grad = gradientPercent / 100;
        const theta = Math.atan(grad);

        let cda, crr;
        switch (bikeType) {
          case "aero":
            cda = 0.22;
            crr = 0.003;
            break;
          case "road":
            cda = 0.25;
            crr = 0.004;
            break;
          case "gravel":
            cda = 0.28;
            crr = 0.005;
            break;
          case "mtb":
            cda = 0.32;
            crr = 0.006;
            break;
          case "city":
          default:
            cda = 0.35;
            crr = 0.006;
            break;
        }

        let windMps = parseFloat(wind) || 0;
        if (windUnit === "kmh") windMps = windMps / 3.6;
        if (windUnit === "mph") windMps = windMps * 0.44704;

        // Numerical solve for v (m/s) using simple iteration
        let v = 5; // initial guess 18 km/h
        for (let i = 0; i < 40; i++) {
          const vAir = v + windMps; // headwind positive
          const dragPower = 0.5 * rho * cda * vAir * vAir * vAir;
          const rollPower = crr * m * g * v;
          const gravPower = m * g * Math.sin(theta) * v;
          const f = dragPower + rollPower + gravPower - P;
          // approximate derivative wrt v
          const dv = 0.1;
          const v2 = v + dv;
          const vAir2 = v2 + windMps;
          const dragPower2 = 0.5 * rho * cda * vAir2 * vAir2 * vAir2;
          const rollPower2 = crr * m * g * v2;
          const gravPower2 = m * g * Math.sin(theta) * v2;
          const f2 = dragPower2 + rollPower2 + gravPower2 - P;
          const df = (f2 - f) / dv;
          if (Math.abs(df) < 1e-6) break;
          v = v - f / df;
          if (v < 0.1) v = 0.1;
          if (v > 50) v = 50; // 180 km/h cap
        }
        return v; // m/s
      }

      calcPowerBtn.addEventListener("click", () => {
        const P = parseFloat(document.getElementById("powerWatts").value);
        let m = parseFloat(document.getElementById("totalMass").value);
        const massUnit = document.getElementById("massUnit").value;
        const gradient =
          parseFloat(document.getElementById("gradient").value) || 0;
        const bikeType = document.getElementById("bikeType").value;
        const wind = document.getElementById("windSpeed").value;
        const windUnit = document.getElementById("windUnit").value;

        if (!P || P <= 0) {
          alert("Please enter your power in watts.");
          return;
        }
        if (!m || m <= 0) {
          alert("Please enter rider + bike weight.");
          return;
        }
        if (massUnit === "lb") {
          m = lbToKg(m);
        }

        const vMps = solveSpeedFromPower(
          P,
          m,
          gradient,
          bikeType,
          wind,
          windUnit
        );
        const vKmh = vMps * 3.6;
        const vMph = vKmh * 0.621371;

        powerResultPrimary.textContent = `Estimated steady‑state speed: ${vKmh.toFixed(
          1
        )} km/h (${vMph.toFixed(1)} mph)`;
        powerResultSecondary.textContent = `Inputs: ${P.toFixed(
          0
        )} W, ${m.toFixed(1)} kg, gradient ${gradient.toFixed(
          1
        )}%, bike: ${bikeType}.`;
        powerResultContainer.classList.remove("hidden");
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
