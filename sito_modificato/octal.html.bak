    <script>
      // Utility: trim and normalize
      function norm(str) {
        return (str || "").trim();
      }

      function formatWithGrouping(value, base) {
        const grouping = document.getElementById("grouping").value;
        if (grouping === "none") return value;

        let groupSize;
        if (grouping === "octal" && base === 8) {
          groupSize = 3;
        } else if (grouping === "binary" && base === 2) {
          groupSize = 4;
        } else {
          return value;
        }

        const sign = value.startsWith("-") ? "-" : "";
        const abs = sign ? value.slice(1) : value;
        const parts = abs.split(".");
        const intPart = parts[0];
        const fracPart = parts[1] || "";

        // group integer part from right
        let groupedInt = "";
        for (let i = intPart.length; i > 0; i -= groupSize) {
          const start = Math.max(0, i - groupSize);
          const chunk = intPart.slice(start, i);
          groupedInt = (start > 0 ? chunk + " " : chunk) + groupedInt;
        }

        // group fractional part from left
        let groupedFrac = "";
        if (fracPart) {
          for (let i = 0; i < fracPart.length; i += groupSize) {
            const chunk = fracPart.slice(i, i + groupSize);
            groupedFrac += (i > 0 ? " " : "") + chunk;
          }
        }

        return sign + groupedInt + (groupedFrac ? "." + groupedFrac : "");
      }

      function parseToDecimal(str, base) {
        str = norm(str);
        if (!str) return null;

        const regexMap = {
          2: /^[+-]?[01]+(\.[01]+)?$/i,
          8: /^[+-]?[0-7]+(\.[0-7]+)?$/i,
          10: /^[+-]?\d+(\.\d+)?$/i,
          16: /^[+-]?[0-9a-f]+(\.[0-9a-f]+)?$/i,
        };
        const regex = regexMap[base];
        if (!regex.test(str)) return null;

        const sign = str.startsWith("-") ? -1 : 1;
        if (str[0] === "+" || str[0] === "-") str = str.slice(1);

        const [intPart, fracPart = ""] = str.split(".");

        let intVal = 0;
        for (let i = 0; i < intPart.length; i++) {
          const ch = intPart[i];
          const digit = parseInt(ch, 36); // works for 0-9a-z
          intVal = intVal * base + digit;
        }

        let fracVal = 0;
        let factor = 1 / base;
        for (let i = 0; i < fracPart.length; i++) {
          const ch = fracPart[i];
          const digit = parseInt(ch, 36);
          fracVal += digit * factor;
          factor / base;
        }

        return sign * (intVal + fracVal);
      }

      function decimalToBase(num, base, precision) {
        if (!isFinite(num)) return "";

        const sign = num < 0 ? "-" : "";
        num = Math.abs(num);

        const intPart = Math.floor(num);
        let frac = num - intPart;

        // integer part
        let intStr = "";
        if (intPart === 0) {
          intStr = "0";
        } else {
          let n = intPart;
          while (n > 0) {
            const digit = n % base;
            intStr = digit.toString(base).toUpperCase() + intStr;
            n = Math.floor(n / base);
          }
        }

        if (precision <= 0 || frac === 0) {
          return sign + intStr;
        }

        // fractional part
        let fracStr = "";
        let count = 0;
        while (frac > 0 && count < precision) {
          frac *= base;
          const digit = Math.floor(frac);
          fracStr += digit.toString(base).toUpperCase();
          frac -= digit;
          count++;
        }

        return sign + intStr + (fracStr ? "." + fracStr : "");
      }

      function updateFrom(source) {
        const precision = Math.max(
          1,
          Math.min(
            16,
            parseInt(document.getElementById("precision").value, 10) || 8
          )
        );
        const octalInput = document.getElementById("octalInput");
        const decimalInput = document.getElementById("decimalInput");
        const binaryInput = document.getElementById("binaryInput");
        const hexInput = document.getElementById("hexInput");
        const status = document.getElementById("statusMessage");
        const stepsBox = document.getElementById("stepsBox");

        status.textContent = "";
        let dec = null;
        let steps = "";

        const rawOct = norm(octalInput.value).replace(/\s+/g, "");
        const rawDec = norm(decimalInput.value).replace(/\s+/g, "");
        const rawBin = norm(binaryInput.value).replace(/\s+/g, "");
        const rawHex = norm(hexInput.value).replace(/\s+/g, "");

        if (!rawOct && !rawDec && !rawBin && !rawHex) {
          stepsBox.textContent =
            "Enter a value in any field to see the conversion steps here.";
          return;
        }

        if (source === "octal") {
          dec = parseToDecimal(rawOct, 8);
          if (dec === null) {
            status.textContent =
              "Invalid octal number (allowed digits: 0–7, optional sign and decimal point).";
            return;
          }
          steps += "Octal → Decimal conversion:\n";
          const sign = rawOct.startsWith("-") ? "-" : "";
          const abs = sign ? rawOct.slice(1) : rawOct;
          const [intPart, fracPart = ""] = abs.split(".");
          steps += `  Number: ${rawOct} (base 8)\n\n`;
          steps += "  Integer part:\n";
          for (let i = 0; i < intPart.length; i++) {
            const digit = parseInt(intPart[i], 10);
            const pos = intPart.length - 1 - i;
            const term = `${digit} × 8^${pos}`;
            steps += `    ${term}\n`;
          }
          if (fracPart) {
            steps += "\n  Fractional part:\n";
            for (let i = 0; i < fracPart.length; i++) {
              const digit = parseInt(fracPart[i], 10);
              const pos = -(i + 1);
              const term = `${digit} × 8^(${pos})`;
              steps += `    ${term}\n`;
            }
          }
          steps += `\n  Decimal result ≈ ${dec}\n`;
        } else if (source === "decimal") {
          dec = parseToDecimal(rawDec, 10);
          if (dec === null) {
            status.textContent = "Invalid decimal number.";
            return;
          }
          steps += "Decimal → Octal conversion (integer part):\n";
          const sign = dec < 0 ? "-" : "";
          let n = Math.floor(Math.abs(dec));
          if (n === 0) {
            steps += "  0 is 0 in any base.\n";
          } else {
            while (n > 0) {
              const q = Math.floor(n / 8);
              const r = n % 8;
              steps += `  ${n} ÷ 8 = ${q} remainder ${r}\n`;
              n = q;
            }
            steps +=
              "  Read remainders from bottom to top to get the octal integer part.\n";
          }
          const frac = Math.abs(dec) - Math.floor(Math.abs(dec));
          if (frac > 0) {
            steps += "\nDecimal → Octal conversion (fractional part):\n";
            let f = frac;
            let count = 0;
            while (f > 0 && count < precision) {
              f *= 8;
              const digit = Math.floor(f);
              steps += `  ×8 → ${f.toFixed(10)} → digit ${digit}\n`;
              f -= digit;
              count++;
            }
          }
          steps += `\n  Final octal ≈ ${decimalToBase(dec, 8, precision)}\n`;
        } else if (source === "binary") {
          dec = parseToDecimal(rawBin, 2);
          if (dec === null) {
            status.textContent =
              "Invalid binary number (allowed digits: 0 and 1).";
            return;
          }
          steps += "Binary → Octal via Decimal:\n";
          steps += `  Binary ${rawBin} → decimal ≈ ${dec}\n`;
          steps += `  Decimal ${dec} → octal ≈ ${decimalToBase(
            dec,
            8,
            precision
          )}\n`;
        } else if (source === "hex") {
          dec = parseToDecimal(rawHex, 16);
          if (dec === null) {
            status.textContent =
              "Invalid hexadecimal number (digits 0–9 and A–F).";
            return;
          }
          steps += "Hexadecimal → Octal via Decimal:\n";
          steps += `  Hex ${rawHex.toUpperCase()} → decimal ≈ ${dec}\n`;
          steps += `  Decimal ${dec} → octal ≈ ${decimalToBase(
            dec,
            8,
            precision
          )}\n`;
        } else {
          return;
        }

        const octStr = decimalToBase(dec, 8, precision);
        const binStr = decimalToBase(dec, 2, precision);
        const hexStr = decimalToBase(dec, 16, precision);
        const decStr = String(dec);

        octalInput.value = formatWithGrouping(octStr, 8);
        decimalInput.value = decStr;
        binaryInput.value = formatWithGrouping(binStr, 2);
        hexInput.value = hexStr.toUpperCase();

        stepsBox.textContent = steps;
      }

      document.addEventListener("DOMContentLoaded", function () {
        const octalInput = document.getElementById("octalInput");
        const decimalInput = document.getElementById("decimalInput");
        const binaryInput = document.getElementById("binaryInput");
        const hexInput = document.getElementById("hexInput");
        const precisionInput = document.getElementById("precision");
        const groupingSelect = document.getElementById("grouping");
        const clearBtn = document.getElementById("clearBtn");

        let lock = false;
        function handler(source) {
          if (lock) return;
          lock = true;
          try {
            updateFrom(source);
          } finally {
            lock = false;
          }
        }

        octalInput.addEventListener("input", () => handler("octal"));
        decimalInput.addEventListener("input", () => handler("decimal"));
        binaryInput.addEventListener("input", () => handler("binary"));
        hexInput.addEventListener("input", () => handler("hex"));
        precisionInput.addEventListener("change", () => {
          // re-run based on whichever field currently has a value
          if (norm(octalInput.value)) handler("octal");
          else if (norm(decimalInput.value)) handler("decimal");
          else if (norm(binaryInput.value)) handler("binary");
          else if (norm(hexInput.value)) handler("hex");
        });
        groupingSelect.addEventListener("change", () => {
          // just reformat existing values
          const octRaw = norm(octalInput.value).replace(/\s+/g, "");
          const binRaw = norm(binaryInput.value).replace(/\s+/g, "");
          if (octRaw) octalInput.value = formatWithGrouping(octRaw, 8);
          if (binRaw) binaryInput.value = formatWithGrouping(binRaw, 2);
        });

        clearBtn.addEventListener("click", () => {
          octalInput.value = "";
          decimalInput.value = "";
          binaryInput.value = "";
          hexInput.value = "";
          document.getElementById("statusMessage").textContent = "";
          document.getElementById("stepsBox").textContent =
            "Enter a value in any field to see the conversion steps here.";
        });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
