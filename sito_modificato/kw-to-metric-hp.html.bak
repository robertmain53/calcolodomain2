    <script>
      // Constants
      const METRIC_HP_PER_KW = 1.3596216173; // exact from definition
      const KW_PER_METRIC_HP = 0.73549875;
      const W_PER_METRIC_HP = 735.49875;
      const W_PER_MECH_HP = 745.69987158227022;

      const kwInput = document.getElementById("kwInput");
      const wInput = document.getElementById("wInput");
      const metricHpInput = document.getElementById("metricHpInput");
      const mechHpInput = document.getElementById("mechHpInput");
      const resultSummary = document.getElementById("resultSummary");

      const kwClear = document.getElementById("kwClear");
      const wClear = document.getElementById("wClear");
      const metricHpClear = document.getElementById("metricHpClear");
      const mechHpClear = document.getElementById("mechHpClear");
      const swapDirectionBtn = document.getElementById("swapDirection");
      const fillExampleBtn = document.getElementById("fillExample");
      const resetAllBtn = document.getElementById("resetAll");

      let mainDirection = "kw-to-ps"; // or 'ps-to-kw'
      let isUpdating = false;

      function parseVal(el) {
        const v = parseFloat(el.value);
        return isNaN(v) ? null : v;
      }

      function format(v, decimals = 4) {
        if (!isFinite(v)) return "";
        if (Math.abs(v) >= 1000) return v.toFixed(2);
        if (Math.abs(v) >= 100) return v.toFixed(3);
        return v.toFixed(decimals);
      }

      function updateFromKW() {
        const kw = parseVal(kwInput);
        if (kw === null) return;
        const w = kw * 1000;
        const metricHp = kw * METRIC_HP_PER_KW;
        const mechHp = w / W_PER_MECH_HP;

        wInput.value = format(w, 2);
        metricHpInput.value = format(metricHp, 4);
        mechHpInput.value = format(mechHp, 4);
        updateSummary("kw");
      }

      function updateFromW() {
        const w = parseVal(wInput);
        if (w === null) return;
        const kw = w / 1000;
        const metricHp = w / W_PER_METRIC_HP;
        const mechHp = w / W_PER_MECH_HP;

        kwInput.value = format(kw, 4);
        metricHpInput.value = format(metricHp, 4);
        mechHpInput.value = format(mechHp, 4);
        updateSummary("kw");
      }

      function updateFromMetricHP() {
        const ps = parseVal(metricHpInput);
        if (ps === null) return;
        const w = ps * W_PER_METRIC_HP;
        const kw = ps * KW_PER_METRIC_HP;
        const mechHp = w / W_PER_MECH_HP;

        kwInput.value = format(kw, 4);
        wInput.value = format(w, 2);
        mechHpInput.value = format(mechHp, 4);
        updateSummary("ps");
      }

      function updateFromMechHP() {
        const hp = parseVal(mechHpInput);
        if (hp === null) return;
        const w = hp * W_PER_MECH_HP;
        const kw = w / 1000;
        const metricHp = w / W_PER_METRIC_HP;

        kwInput.value = format(kw, 4);
        wInput.value = format(w, 2);
        metricHpInput.value = format(metricHp, 4);
        updateSummary("hp");
      }

      function updateSummary(source) {
        const kw = parseVal(kwInput);
        const ps = parseVal(metricHpInput);
        const hp = parseVal(mechHpInput);
        if (kw === null || ps === null) {
          resultSummary.classList.add("hidden");
          return;
        }

        let text = "";
        if (source === "kw" || mainDirection === "kw-to-ps") {
          text = `${format(kw, 4)} kW ≈ ${format(
            ps,
            4
          )} metric horsepower (PS)`;
          if (hp !== null) {
            text += `, or about ${format(hp, 4)} mechanical horsepower (hp).`;
          }
        } else {
          text = `${format(ps, 4)} metric horsepower (PS) ≈ ${format(
            kw,
            4
          )} kW`;
          if (hp !== null) {
            text += `, or about ${format(hp, 4)} mechanical horsepower (hp).`;
          }
        }

        resultSummary.textContent = text;
        resultSummary.classList.remove("hidden");
      }

      function attachListeners() {
        kwInput.addEventListener("input", () => {
          if (isUpdating) return;
          isUpdating = true;
          if (kwInput.value === "") {
            wInput.value = "";
            metricHpInput.value = "";
            mechHpInput.value = "";
            resultSummary.classList.add("hidden");
          } else {
            updateFromKW();
          }
          isUpdating = false;
        });

        wInput.addEventListener("input", () => {
          if (isUpdating) return;
          isUpdating = true;
          if (wInput.value === "") {
            kwInput.value = "";
            metricHpInput.value = "";
            mechHpInput.value = "";
            resultSummary.classList.add("hidden");
          } else {
            updateFromW();
          }
          isUpdating = false;
        });

        metricHpInput.addEventListener("input", () => {
          if (isUpdating) return;
          isUpdating = true;
          if (metricHpInput.value === "") {
            kwInput.value = "";
            wInput.value = "";
            mechHpInput.value = "";
            resultSummary.classList.add("hidden");
          } else {
            updateFromMetricHP();
          }
          isUpdating = false;
        });

        mechHpInput.addEventListener("input", () => {
          if (isUpdating) return;
          isUpdating = true;
          if (mechHpInput.value === "") {
            // don't clear others automatically; hp is secondary
            resultSummary.classList.add("hidden");
          } else {
            updateFromMechHP();
          }
          isUpdating = false;
        });

        kwClear.addEventListener("click", () => {
          kwInput.value = "";
          kwInput.dispatchEvent(new Event("input"));
        });
        wClear.addEventListener("click", () => {
          wInput.value = "";
          wInput.dispatchEvent(new Event("input"));
        });
        metricHpClear.addEventListener("click", () => {
          metricHpInput.value = "";
          metricHpInput.dispatchEvent(new Event("input"));
        });
        mechHpClear.addEventListener("click", () => {
          mechHpInput.value = "";
          mechHpInput.dispatchEvent(new Event("input"));
        });

        swapDirectionBtn.addEventListener("click", () => {
          mainDirection =
            mainDirection === "kw-to-ps" ? "ps-to-kw" : "kw-to-ps";
          swapDirectionBtn.textContent =
            mainDirection === "kw-to-ps"
              ? "Swap main direction (kW → PS / PS → kW)"
              : "Swap main direction (PS → kW / kW → PS)";
          updateSummary(mainDirection === "kw-to-ps" ? "kw" : "ps");
        });

        fillExampleBtn.addEventListener("click", () => {
          isUpdating = true;
          kwInput.value = "100";
          wInput.value = "";
          metricHpInput.value = "";
          mechHpInput.value = "";
          isUpdating = false;
          kwInput.dispatchEvent(new Event("input"));
        });

        resetAllBtn.addEventListener("click", () => {
          isUpdating = true;
          kwInput.value = "";
          wInput.value = "";
          metricHpInput.value = "";
          mechHpInput.value = "";
          resultSummary.classList.add("hidden");
          isUpdating = false;
        });
      }

      function buildQuickTable() {
        const tbody = document.getElementById("quickTableBody");
        if (!tbody) return;
        const rowsKw = [10, 20, 30, 50, 75, 100, 150, 200, 250, 300];
        tbody.innerHTML = "";
        rowsKw.forEach((kw) => {
          const ps = kw * METRIC_HP_PER_KW;
          const hp = (kw * 1000) / W_PER_MECH_HP;
          const tr = document.createElement("tr");
          tr.className = "odd:bg-white even:bg-gray-50";
          tr.innerHTML = `
        <td class="px-3 py-2 border-b">${kw}</td>
        <td class="px-3 py-2 border-b">${format(ps, 2)}</td>
        <td class="px-3 py-2 border-b">${format(hp, 2)}</td>
      `;
          tbody.appendChild(tr);
        });
      }

      document.addEventListener("DOMContentLoaded", () => {
        attachListeners();
        buildQuickTable();
      });

      // MathJax config
      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
