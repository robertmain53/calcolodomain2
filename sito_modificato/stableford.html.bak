    <script>
      // Build holes table
      const holesTableBody = document.getElementById("holesTableBody");
      const holesSelect = document.getElementById("holesSelect");
      const systemSelect = document.getElementById("systemSelect");
      const customSystemContainer = document.getElementById(
        "customSystemContainer"
      );
      const handicapInput = document.getElementById("handicapInput");
      const maxStrokesInput = document.getElementById("maxStrokesInput");
      const calculateBtn = document.getElementById("calculateBtn");
      const resetBtn = document.getElementById("resetBtn");
      const summaryBox = document.getElementById("summaryBox");
      const summaryPoints = document.getElementById("summaryPoints");
      const summaryVsPar = document.getElementById("summaryVsPar");
      const summaryGross = document.getElementById("summaryGross");
      const summaryHcp = document.getElementById("summaryHcp");

      const frontParEl = document.getElementById("frontPar");
      const backParEl = document.getElementById("backPar");
      const totalParEl = document.getElementById("totalPar");
      const frontStrokesEl = document.getElementById("frontStrokes");
      const backStrokesEl = document.getElementById("backStrokes");
      const totalStrokesEl = document.getElementById("totalStrokes");
      const frontPointsEl = document.getElementById("frontPoints");
      const backPointsEl = document.getElementById("backPoints");
      const totalPointsEl = document.getElementById("totalPoints");

      const presetPar72Btn = document.getElementById("presetPar72");
      const presetClearScoresBtn = document.getElementById("presetClearScores");

      function createHolesTable(numHoles) {
        holesTableBody.innerHTML = "";
        for (let i = 1; i <= numHoles; i++) {
          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="text-center font-medium">${i}</td>
        <td><input type="number" min="3" max="6" value="${defaultPar(
          i,
          numHoles
        )}" class="w-16 border border-gray-300 rounded px-1 py-0.5 text-right par-input" /></td>
        <td><input type="number" min="1" max="18" value="${i}" class="w-16 border border-gray-300 rounded px-1 py-0.5 text-right si-input" /></td>
        <td><input type="number" min="1" class="w-20 border border-gray-300 rounded px-1 py-0.5 text-right strokes-input" /></td>
        <td class="text-center text-xs hcp-cell">-</td>
        <td class="text-center text-xs net-cell">-</td>
        <td class="text-center text-xs points-cell">-</td>
      `;
          holesTableBody.appendChild(tr);
        }
        clearTotals();
      }

      function defaultPar(hole, numHoles) {
        // Simple generic par-72 layout for 18 holes, par-36 for 9
        if (numHoles === 18) {
          const pattern = [
            4, 4, 3, 4, 5, 4, 3, 5, 4, 4, 5, 4, 3, 4, 4, 5, 3, 4,
          ];
          return pattern[hole - 1] || 4;
        } else {
          const pattern9 = [4, 4, 3, 4, 5, 4, 3, 5, 4];
          return pattern9[hole - 1] || 4;
        }
      }

      function clearTotals() {
        frontParEl.textContent = "-";
        backParEl.textContent = "-";
        totalParEl.textContent = "-";
        frontStrokesEl.textContent = "-";
        backStrokesEl.textContent = "-";
        totalStrokesEl.textContent = "-";
        frontPointsEl.textContent = "-";
        backPointsEl.textContent = "-";
        totalPointsEl.textContent = "-";
        summaryBox.classList.add("hidden");
      }

      function getSystemPoints(diff) {
        const system = systemSelect.value;
        if (system === "standard") {
          if (diff >= 2) return 0;
          if (diff === 1) return 1;
          if (diff === 0) return 2;
          if (diff === -1) return 3;
          if (diff === -2) return 4;
          return 5; // -3 or better
        } else if (system === "modified") {
          if (diff >= 2) return -3;
          if (diff === 1) return -1;
          if (diff === 0) return 0;
          if (diff === -1) return 2;
          if (diff === -2) return 5;
          return 8; // -3 or better
        } else {
          // custom
          const ptsPlus2 = parseInt(
            document.getElementById("ptsPlus2").value || "0",
            10
          );
          const ptsPlus1 = parseInt(
            document.getElementById("ptsPlus1").value || "1",
            10
          );
          const pts0 = parseInt(
            document.getElementById("pts0").value || "2",
            10
          );
          const ptsMinus1 = parseInt(
            document.getElementById("ptsMinus1").value || "3",
            10
          );
          const ptsMinus2 = parseInt(
            document.getElementById("ptsMinus2").value || "4",
            10
          );
          const ptsMinus3 = parseInt(
            document.getElementById("ptsMinus3").value || "5",
            10
          );

          if (diff >= 2) return ptsPlus2;
          if (diff === 1) return ptsPlus1;
          if (diff === 0) return pts0;
          if (diff === -1) return ptsMinus1;
          if (diff === -2) return ptsMinus2;
          return ptsMinus3;
        }
      }

      function calculate() {
        const rows = Array.from(holesTableBody.querySelectorAll("tr"));
        const numHoles = rows.length;
        const handicap = parseFloat(handicapInput.value || "0");
        const maxStrokes = maxStrokesInput.value
          ? parseFloat(maxStrokesInput.value)
          : null;

        const fullRounds = Math.floor(handicap / 18);
        const remainder = handicap - fullRounds * 18;

        let frontPar = 0,
          backPar = 0,
          totalPar = 0;
        let frontStrokes = 0,
          backStrokes = 0,
          totalStrokes = 0;
        let frontPoints = 0,
          backPoints = 0,
          totalPoints = 0;
        let totalHcpStrokes = 0;

        // Collect stroke indexes to allocate remainder strokes correctly
        const siValues = rows
          .map((row, idx) => {
            const siInput = row.querySelector(".si-input");
            const si = parseInt(siInput.value || idx + 1, 10);
            return { index: idx, si: isNaN(si) ? idx + 1 : si };
          })
          .sort((a, b) => a.si - b.si);

        const extraStrokeHoles = new Set();
        for (let i = 0; i < Math.floor(remainder); i++) {
          if (siValues[i]) extraStrokeHoles.add(siValues[i].index);
        }

        rows.forEach((row, idx) => {
          const parInput = row.querySelector(".par-input");
          const siInput = row.querySelector(".si-input");
          const strokesInput = row.querySelector(".strokes-input");
          const hcpCell = row.querySelector(".hcp-cell");
          const netCell = row.querySelector(".net-cell");
          const pointsCell = row.querySelector(".points-cell");

          const par = parseFloat(parInput.value || "0");
          const si = parseInt(siInput.value || idx + 1, 10);
          let strokes = strokesInput.value
            ? parseFloat(strokesInput.value)
            : NaN;

          const baseHcp = fullRounds;
          const extra = extraStrokeHoles.has(idx) ? 1 : 0;
          const hcpStrokes = baseHcp + extra;

          hcpCell.textContent =
            hcpStrokes > 0 ? hcpStrokes.toFixed(1).replace(/\.0$/, "") : "-";
          totalHcpStrokes += hcpStrokes;

          if (!isNaN(strokes)) {
            if (maxStrokes && strokes > maxStrokes) strokes = maxStrokes;
            const netScore = strokes - hcpStrokes;
            const diff = netScore - par;
            const points = getSystemPoints(diff);

            netCell.textContent = netScore.toFixed(1).replace(/\.0$/, "");
            pointsCell.textContent = points;

            totalPar += par;
            totalStrokes += strokes;
            totalPoints += points;

            if (idx < 9) {
              frontPar += par;
              frontStrokes += strokes;
              frontPoints += points;
            } else {
              backPar += par;
              backStrokes += strokes;
              backPoints += points;
            }
          } else {
            netCell.textContent = "-";
            pointsCell.textContent = "-";
          }
        });

        if (numHoles === 9) {
          backPar = 0;
          backStrokes = 0;
          backPoints = 0;
        }

        frontParEl.textContent = frontPar ? frontPar.toFixed(0) : "-";
        backParEl.textContent = backPar
          ? backPar.toFixed(0)
          : numHoles === 18
          ? "-"
          : "n/a";
        totalParEl.textContent = totalPar ? totalPar.toFixed(0) : "-";

        frontStrokesEl.textContent = frontStrokes
          ? frontStrokes.toFixed(0)
          : "-";
        backStrokesEl.textContent = backStrokes
          ? backStrokes.toFixed(0)
          : numHoles === 18
          ? "-"
          : "n/a";
        totalStrokesEl.textContent = totalStrokes
          ? totalStrokes.toFixed(0)
          : "-";

        frontPointsEl.textContent =
          frontPoints || frontPoints === 0 ? frontPoints.toFixed(0) : "-";
        backPointsEl.textContent =
          numHoles === 18 ? backPoints.toFixed(0) : "n/a";
        totalPointsEl.textContent =
          totalPoints || totalPoints === 0 ? totalPoints.toFixed(0) : "-";

        if (totalPar > 0 && totalStrokes > 0) {
          summaryBox.classList.remove("hidden");
          summaryPoints.textContent = totalPoints.toFixed(0);

          const vsPar = totalStrokes - totalPar;
          const sign = vsPar > 0 ? "+" : "";
          summaryGross.textContent = `${totalStrokes.toFixed(
            0
          )} (${sign}${vsPar.toFixed(0)} vs par)`;

          summaryHcp.textContent = totalHcpStrokes
            .toFixed(1)
            .replace(/\.0$/, "");

          const avgPoints = totalPoints / numHoles;
          summaryVsPar.textContent = `Average ${avgPoints.toFixed(
            2
          )} pts/hole (${(avgPoints * 18).toFixed(
            1
          )} projected over 18 holes).`;
        } else {
          summaryBox.classList.add("hidden");
        }
      }

      function resetAll() {
        handicapInput.value = 18;
        maxStrokesInput.value = "";
        systemSelect.value = "standard";
        customSystemContainer.classList.add("hidden");
        createHolesTable(parseInt(holesSelect.value, 10));
      }

      systemSelect.addEventListener("change", () => {
        if (systemSelect.value === "custom") {
          customSystemContainer.classList.remove("hidden");
        } else {
          customSystemContainer.classList.add("hidden");
        }
      });

      holesSelect.addEventListener("change", () => {
        createHolesTable(parseInt(holesSelect.value, 10));
      });

      calculateBtn.addEventListener("click", calculate);
      resetBtn.addEventListener("click", resetAll);

      presetPar72Btn.addEventListener("click", () => {
        const numHoles = parseInt(holesSelect.value, 10);
        createHolesTable(numHoles);
      });

      presetClearScoresBtn.addEventListener("click", () => {
        const rows = Array.from(holesTableBody.querySelectorAll("tr"));
        rows.forEach((row) => {
          const strokesInput = row.querySelector(".strokes-input");
          const netCell = row.querySelector(".net-cell");
          const pointsCell = row.querySelector(".points-cell");
          strokesInput.value = "";
          netCell.textContent = "-";
          pointsCell.textContent = "-";
        });
        clearTotals();
      });

      // Initialize
      createHolesTable(18);
    </script>

