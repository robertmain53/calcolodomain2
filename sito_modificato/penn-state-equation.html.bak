    <script>
      // Utility conversions
      function toKg(weight, unit) {
        return unit === "lb" ? weight * 0.45359237 : weight;
      }
      function toCm(height, unit) {
        return unit === "in" ? height * 2.54 : height;
      }
      function toCelsius(temp, unit) {
        return unit === "F" ? ((temp - 32) * 5) / 9 : temp;
      }
      function toLmin(ve, unit, weightKg) {
        if (unit === "Lmin") return ve;
        // mL/kg/min -> L/min
        return (ve * weightKg) / 1000;
      }

      function calcBMI(weightKg, heightCm) {
        const hM = heightCm / 100;
        if (!hM) return NaN;
        return weightKg / (hM * hM);
      }

      function calcMifflin(sex, weightKg, heightCm, age) {
        const base = 10 * weightKg + 6.25 * heightCm - 5 * age;
        return sex === "male" ? base + 5 : base - 161;
      }

      function calcPSU1998(mifflin, tC, veLmin) {
        return 0.96 * mifflin + 167 * tC + 31 * veLmin - 6212;
      }

      function calcModifiedPSU(mifflin, tC, veLmin) {
        return 0.71 * mifflin + 85 * tC + 64 * veLmin - 3085;
      }

      document
        .getElementById("calculate-btn")
        .addEventListener("click", function () {
          const sex = document.getElementById("sex").value;
          const age = parseFloat(document.getElementById("age").value);
          const weight = parseFloat(document.getElementById("weight").value);
          const weightUnit = document.getElementById("weight-unit").value;
          const height = parseFloat(document.getElementById("height").value);
          const heightUnit = document.getElementById("height-unit").value;
          const tmax = parseFloat(document.getElementById("tmax").value);
          const tmaxUnit = document.getElementById("tmax-unit").value;
          const ve = parseFloat(document.getElementById("ve").value);
          const veUnit = document.getElementById("ve-unit").value;

          const autoNote = document.getElementById("auto-note");
          autoNote.textContent = "";

          if ([age, weight, height, tmax, ve].some((v) => isNaN(v) || v <= 0)) {
            alert("Please enter valid positive values for all fields.");
            return;
          }

          const weightKg = toKg(weight, weightUnit);
          const heightCm = toCm(height, heightUnit);
          const tC = toCelsius(tmax, tmaxUnit);
          const veLmin = toLmin(ve, veUnit, weightKg);

          const bmi = calcBMI(weightKg, heightCm);
          const mifflin = calcMifflin(sex, weightKg, heightCm, age);

          let version = document.querySelector(
            'input[name="ps-version"]:checked'
          ).value;
          if (version === "auto") {
            if (age >= 60) {
              version = "psu2003b";
              autoNote.textContent =
                "Auto-selected: Penn State 2003b (age ≥ 60).";
            } else if (bmi >= 30) {
              version = "modified";
              autoNote.textContent =
                "Auto-selected: Modified Penn State (BMI ≥ 30 and age < 60).";
            } else {
              version = "psu1998";
              autoNote.textContent =
                "Auto-selected: Penn State 1998 (BMI < 30 and age < 60).";
            }
          }

          let ree;
          let versionLabel;
          if (version === "psu1998") {
            ree = calcPSU1998(mifflin, tC, veLmin);
            versionLabel = "Penn State 1998";
          } else if (version === "psu2003b") {
            ree = calcPSU1998(mifflin, tC, veLmin); // same structure, different validation cohort
            versionLabel = "Penn State 2003b";
          } else {
            ree = calcModifiedPSU(mifflin, tC, veLmin);
            versionLabel = "Modified Penn State";
          }

          // Basic sanity: REE should not be negative
          if (ree < 0) ree = NaN;

          const bmiOutput = document.getElementById("bmi-output");
          const mifflinOutput = document.getElementById("mifflin-output");
          const versionOutput = document.getElementById("version-output");
          const reeOutput = document.getElementById("ree-output");
          const rangeOutput = document.getElementById("range-output");

          bmiOutput.textContent = isNaN(bmi) ? "–" : bmi.toFixed(1) + " kg/m²";
          mifflinOutput.textContent = mifflin.toFixed(0) + " kcal/day";
          versionOutput.textContent = versionLabel;

          if (isNaN(ree)) {
            reeOutput.textContent = "Calculation not valid";
            rangeOutput.textContent = "";
          } else {
            const reeRounded = Math.round(ree / 25) * 25;
            reeOutput.textContent = reeRounded.toLocaleString() + " kcal/day";
            const low = Math.round((0.7 * reeRounded) / 25) * 25;
            const high = Math.round((1.0 * reeRounded) / 25) * 25;
            rangeOutput.textContent =
              "Typical prescription range: " +
              low.toLocaleString() +
              "–" +
              high.toLocaleString() +
              " kcal/day (70–100% of REE), adjusted per protocol.";
          }

          document.getElementById("results-card").classList.remove("hidden");
        });

      document
        .getElementById("reset-btn")
        .addEventListener("click", function () {
          document.getElementById("sex").value = "male";
          document.getElementById("age").value = 60;
          document.getElementById("weight").value = 80;
          document.getElementById("weight-unit").value = "kg";
          document.getElementById("height").value = 170;
          document.getElementById("height-unit").value = "cm";
          document.getElementById("tmax").value = 38.5;
          document.getElementById("tmax-unit").value = "C";
          document.getElementById("ve").value = 10;
          document.getElementById("ve-unit").value = "Lmin";
          document.querySelector(
            'input[name="ps-version"][value="psu1998"]'
          ).checked = true;
          document.getElementById("results-card").classList.add("hidden");
          document.getElementById("auto-note").textContent = "";
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
