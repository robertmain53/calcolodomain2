    <script>
      // Tab switching
      const tabs = document.querySelectorAll("[data-tab]");
      const panels = {
        core: document.getElementById("panel-core"),
        planner: document.getElementById("panel-planner"),
        compare: document.getElementById("panel-compare"),
      };

      tabs.forEach((tab) => {
        tab.addEventListener("click", () => {
          const target = tab.getAttribute("data-tab");
          tabs.forEach((t) => t.classList.remove("tab-active"));
          tab.classList.add("tab-active");
          Object.keys(panels).forEach((key) => {
            panels[key].classList.toggle("hidden", key !== target);
          });
        });
      });

      // Currency sync
      const currencySelect = document.getElementById("currency");
      const currencyPrefix = document.getElementById("currency-prefix");
      const currencyPrefixCpm = document.getElementById("currency-prefix-cpm");
      const plannerCurrencyPrefix = document.getElementById(
        "planner-currency-prefix"
      );
      const plannerCurrencyPrefixMin = document.getElementById(
        "planner-currency-prefix-min"
      );
      const plannerCurrencyPrefixMax = document.getElementById(
        "planner-currency-prefix-max"
      );
      const cmpCurrencyEls = [
        document.getElementById("cmp-currency-1"),
        document.getElementById("cmp-currency-2"),
        document.getElementById("cmp-currency-3"),
      ];

      function updateCurrencySymbols() {
        const symbol = currencySelect.value || "$";
        [
          currencyPrefix,
          currencyPrefixCpm,
          plannerCurrencyPrefix,
          plannerCurrencyPrefixMin,
          plannerCurrencyPrefixMax,
          ...cmpCurrencyEls,
        ].forEach((el) => {
          if (el) el.textContent = symbol;
        });
      }
      currencySelect.addEventListener("change", updateCurrencySymbols);
      updateCurrencySymbols();

      // Core CPM calculator
      const totalCostInput = document.getElementById("total-cost");
      const impressionsInput = document.getElementById("impressions");
      const cpmInput = document.getElementById("cpm");
      const decimalsSelect = document.getElementById("decimals");
      const solveButtons = document.querySelectorAll("[data-solve]");
      let solveFor = "cpm";

      solveButtons.forEach((btn) => {
        btn.addEventListener("click", () => {
          solveFor = btn.getAttribute("data-solve");
          solveButtons.forEach((b) => {
            b.classList.remove(
              "border-blue-600",
              "text-blue-700",
              "bg-blue-50"
            );
            b.classList.add("border-gray-300", "text-gray-700");
          });
          btn.classList.add("border-blue-600", "text-blue-700", "bg-blue-50");
          btn.classList.remove("border-gray-300", "text-gray-700");
        });
      });

      function formatNumber(value, decimals) {
        if (!isFinite(value)) return "–";
        return Number(value).toLocaleString(undefined, {
          minimumFractionDigits: decimals,
          maximumFractionDigits: decimals,
        });
      }

      document.getElementById("btn-calc-core").addEventListener("click", () => {
        const cost = parseFloat(totalCostInput.value);
        const impr = parseFloat(impressionsInput.value);
        const cpm = parseFloat(cpmInput.value);
        const dec = parseInt(decimalsSelect.value, 10) || 2;
        const symbol = currencySelect.value || "$";
        const resultBox = document.getElementById("core-result");
        const resultText = document.getElementById("core-result-text");

        let message = "";
        let ok = true;

        if (solveFor === "cpm") {
          if (!(cost > 0) || !(impr > 0)) {
            ok = false;
          } else {
            const cpmVal = (cost / impr) * 1000;
            message = `Your CPM is approximately ${symbol}${formatNumber(
              cpmVal,
              dec
            )}. You spent ${symbol}${formatNumber(
              cost,
              dec
            )} for ${impr.toLocaleString()} impressions.`;
            cpmInput.value = cpmVal.toFixed(dec);
          }
        } else if (solveFor === "cost") {
          if (!(cpm > 0) || !(impr > 0)) {
            ok = false;
          } else {
            const costVal = (cpm * impr) / 1000;
            message = `You need a budget of about ${symbol}${formatNumber(
              costVal,
              dec
            )} to buy ${impr.toLocaleString()} impressions at a CPM of ${symbol}${formatNumber(
              cpm,
              dec
            )}.`;
            totalCostInput.value = costVal.toFixed(dec);
          }
        } else if (solveFor === "impressions") {
          if (!(cpm > 0) || !(cost > 0)) {
            ok = false;
          } else {
            const imprVal = (cost * 1000) / cpm;
            message = `Your budget of ${symbol}${formatNumber(
              cost,
              dec
            )} at a CPM of ${symbol}${formatNumber(
              cpm,
              dec
            )} will buy about ${Math.round(
              imprVal
            ).toLocaleString()} impressions.`;
            impressionsInput.value = Math.round(imprVal);
          }
        }

        if (!ok) {
          resultText.textContent =
            "Please enter valid positive values for the two known fields before calculating.";
          resultBox.classList.remove("hidden");
          return;
        }

        resultText.textContent = message;
        resultBox.classList.remove("hidden");
      });

      document
        .getElementById("btn-reset-core")
        .addEventListener("click", () => {
          totalCostInput.value = "";
          impressionsInput.value = "";
          cpmInput.value = "";
          document.getElementById("core-result").classList.add("hidden");
        });

      // Budget planner
      document
        .getElementById("btn-calc-planner")
        .addEventListener("click", () => {
          const budget = parseFloat(
            document.getElementById("planner-budget").value
          );
          const cpmMin = parseFloat(
            document.getElementById("planner-cpm-min").value
          );
          const cpmMax = parseFloat(
            document.getElementById("planner-cpm-max").value
          );
          const symbol = currencySelect.value || "$";
          const dec = parseInt(decimalsSelect.value, 10) || 2;
          const resultBox = document.getElementById("planner-result");
          const line1 = document.getElementById("planner-result-line1");
          const line2 = document.getElementById("planner-result-line2");

          if (
            !(budget > 0) ||
            !(cpmMin > 0) ||
            !(cpmMax > 0) ||
            cpmMax < cpmMin
          ) {
            line1.textContent =
              "Please enter a positive budget and a valid CPM range (max ≥ min).";
            line2.textContent = "";
            resultBox.classList.remove("hidden");
            return;
          }

          const imprMax = (budget * 1000) / cpmMax;
          const imprMin = (budget * 1000) / cpmMin;

          line1.textContent = `With a budget of ${symbol}${formatNumber(
            budget,
            dec
          )}, you can expect between ${Math.round(
            imprMax
          ).toLocaleString()} and ${Math.round(
            imprMin
          ).toLocaleString()} impressions.`;
          line2.textContent = `That corresponds to CPMs between ${symbol}${formatNumber(
            cpmMin,
            dec
          )} and ${symbol}${formatNumber(cpmMax, dec)}.`;
          resultBox.classList.remove("hidden");
        });

      document
        .getElementById("btn-reset-planner")
        .addEventListener("click", () => {
          document.getElementById("planner-budget").value = "";
          document.getElementById("planner-cpm-min").value = "";
          document.getElementById("planner-cpm-max").value = "";
          document.getElementById("planner-result").classList.add("hidden");
        });

      // Comparison
      document
        .getElementById("btn-calc-compare")
        .addEventListener("click", () => {
          const dec = parseInt(decimalsSelect.value, 10) || 2;
          const symbol = currencySelect.value || "$";
          const rows = [1, 2, 3].map((i) => {
            const name = (
              document.getElementById("cmp-name-" + i).value || `Campaign ${i}`
            ).trim();
            const cost = parseFloat(
              document.getElementById("cmp-cost-" + i).value
            );
            const impr = parseFloat(
              document.getElementById("cmp-impr-" + i).value
            );
            let cpm = NaN;
            if (cost > 0 && impr > 0) {
              cpm = (cost / impr) * 1000;
              document.getElementById("cmp-cpm-" + i).textContent =
                symbol + formatNumber(cpm, dec);
            } else {
              document.getElementById("cmp-cpm-" + i).textContent = "–";
            }
            return { index: i, name, cost, impr, cpm };
          });

          const validRows = rows.filter((r) => isFinite(r.cpm));
          const resultBox = document.getElementById("compare-result");
          const resultText = document.getElementById("compare-result-text");

          if (validRows.length === 0) {
            resultText.textContent =
              "Enter cost and impressions for at least one campaign to compare CPMs.";
            resultBox.classList.remove("hidden");
            return;
          }

          validRows.sort((a, b) => a.cpm - b.cpm);
          const best = validRows[0];
          let msg = `The lowest CPM is for ${
            best.name
          } at ${symbol}${formatNumber(best.cpm, dec)}.`;

          if (validRows.length > 1) {
            const worst = validRows[validRows.length - 1];
            const diff = worst.cpm - best.cpm;
            msg += ` The highest CPM is ${
              worst.name
            } at ${symbol}${formatNumber(
              worst.cpm,
              dec
            )}, which is ${symbol}${formatNumber(
              diff,
              dec
            )} more per 1,000 impressions.`;
          }

          resultText.textContent = msg;
          resultBox.classList.remove("hidden");
        });

      document
        .getElementById("btn-reset-compare")
        .addEventListener("click", () => {
          ["1", "2", "3"].forEach((i) => {
            document.getElementById("cmp-name-" + i).value = "";
            document.getElementById("cmp-cost-" + i).value = "";
            document.getElementById("cmp-impr-" + i).value = "";
            document.getElementById("cmp-cpm-" + i).textContent = "–";
          });
          document.getElementById("compare-result").classList.add("hidden");
        });

      // Mobile menu script safeguard (if mobile-menu.js not yet loaded)
      document
        .getElementById("mobile-menu-toggle")
        ?.addEventListener("click", () => {
          const menu = document.getElementById("mobile-menu");
          if (!menu) return;
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
