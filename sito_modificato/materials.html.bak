          notes:
            "Glass‑fiber reinforced polymer. High strength-to-weight, corrosion resistant.",
        },
        {
          id: "cfrp",
          name: "CFRP Composite",
          category: "composite",
          density: 1600,
          strength: 600,
          stiffness: 140,
          costIndex: 5,
          sustainability: 1,
          notes:
            "Carbon‑fiber composite. Extremely stiff and strong but very expensive.",
        },
        {
          id: "pvc",
          name: "PVC (Rigid)",
          category: "polymer",
          density: 1400,
          strength: 50,
          stiffness: 3,
          costIndex: 1,
          sustainability: 1,
          notes:
            "Used for pipes, profiles and cladding. Brittle at low temperatures.",
        },
        {
          id: "hdpe",
          name: "HDPE",
          category: "polymer",
          density: 950,
          strength: 25,
          stiffness: 1,
          costIndex: 1,
          sustainability: 2,
          notes:
            "Tough, impact resistant plastic for tanks, liners and piping.",
        },
        {
          id: "polycarbonate",
          name: "Polycarbonate",
          category: "polymer",
          density: 1200,
          strength: 65,
          stiffness: 2.4,
          costIndex: 3,
          sustainability: 2,
          notes:
            "Transparent, very tough; used for glazing and protective covers.",
        },
        {
          id: "glass_float",
          name: "Float Glass",
          category: "glass",
          density: 2500,
          strength: 45,
          stiffness: 70,
          costIndex: 2,
          sustainability: 3,
          notes:
            "Standard window glass. Brittle; design is stress‑limited and code‑driven.",
        },
        {
          id: "mineral_wool",
          name: "Mineral Wool Insulation",
          category: "insulation",
          density: 80,
          strength: 0.05,
          stiffness: 0.01,
          costIndex: 2,
          sustainability: 4,
          notes: "Non‑combustible thermal and acoustic insulation.",
        },
        {
          id: "eps",
          name: "EPS Foam Insulation",
          category: "insulation",
          density: 20,
          strength: 0.15,
          stiffness: 0.005,
          costIndex: 1,
          sustainability: 2,
          notes: "Expanded polystyrene boards for lightweight insulation.",
        },
      ];

      let currentMaterials = [...MATERIALS];
      let selectedMaterial = null;

      function normalize(values) {
        const min = Math.min(...values);
        const max = Math.max(...values);
        if (max === min) return values.map(() => 0.5);
        return values.map((v) => (v - min) / (max - min));
      }

      function computeScores(priority) {
        const densities = currentMaterials.map((m) => m.density);
        const strengths = currentMaterials.map((m) => m.strength);
        const stiff = currentMaterials.map((m) => m.stiffness);
        const costs = currentMaterials.map((m) => m.costIndex);
        const sus = currentMaterials.map((m) => m.sustainability);

        const nDensity = normalize(densities);
        const nStrength = normalize(strengths);
        const nStiff = normalize(stiff);
        const nCost = normalize(costs);
        const nSus = normalize(sus);

        let weights;
        switch (priority) {
          case "strength":
            weights = { s: 0.4, E: 0.25, rho: 0.1, c: 0.1, sus: 0.15 };
            break;
          case "stiffness":
            weights = { s: 0.25, E: 0.4, rho: 0.1, c: 0.1, sus: 0.15 };
            break;
          case "lightweight":
            weights = { s: 0.2, E: 0.2, rho: 0.35, c: 0.1, sus: 0.15 };
            break;
          case "lowcost":
            weights = { s: 0.2, E: 0.2, rho: 0.1, c: 0.35, sus: 0.15 };
            break;
          case "sustainable":
            weights = { s: 0.2, E: 0.2, rho: 0.1, c: 0.1, sus: 0.4 };
            break;
          default:
            weights = { s: 0.25, E: 0.25, rho: 0.2, c: 0.15, sus: 0.15 };
        }

        currentMaterials = currentMaterials.map((m, i) => {
          const score =
            weights.s * nStrength[i] +
            weights.E * nStiff[i] +
            weights.rho * (1 - nDensity[i]) +
            weights.c * (1 - nCost[i]) +
            weights.sus * nSus[i];
          return { ...m, score: Math.round(score * 100) };
        });
      }

      function renderTable() {
        const tbody = document.getElementById("materials-tbody");
        tbody.innerHTML = "";
        currentMaterials.forEach((m) => {
          const tr = document.createElement("tr");
          tr.className = "hover:bg-blue-50 cursor-pointer";
          tr.dataset.id = m.id;
          tr.innerHTML = `
        <td class="px-3 py-2 align-top">
          <div class="font-medium text-gray-800">${m.name}</div>
          <div class="text-[11px] text-gray-500 capitalize">${m.category}</div>
        </td>
        <td class="px-3 py-2 text-right align-top">${m.density.toLocaleString()}</td>
        <td class="px-3 py-2 text-right align-top">${m.strength}</td>
        <td class="px-3 py-2 text-right align-top">${m.stiffness}</td>
        <td class="px-3 py-2 text-center align-top">
          <span class="inline-flex items-center justify-center px-1.5 py-0.5 rounded-full text-[11px] ${
            m.costIndex <= 2
              ? "bg-emerald-50 text-emerald-700"
              : m.costIndex === 3
              ? "bg-amber-50 text-amber-700"
              : "bg-rose-50 text-rose-700"
          }">Cost ${m.costIndex}/5</span>
        </td>
        <td class="px-3 py-2 text-center align-top">
          <span class="inline-flex items-center justify-center px-1.5 py-0.5 rounded-full text-[11px] ${
            m.sustainability >= 4
              ? "bg-emerald-50 text-emerald-700"
              : m.sustainability >= 3
              ? "bg-amber-50 text-amber-700"
              : "bg-gray-50 text-gray-600"
          }">Sus ${m.sustainability}/5</span>
        </td>
        <td class="px-3 py-2 text-right align-top font-semibold text-gray-800">${
          m.score ?? "–"
        }</td>
      `;
          tr.addEventListener("click", () => selectMaterial(m.id));
          tbody.appendChild(tr);
        });
        document.getElementById("results-count").textContent = `Showing ${
          currentMaterials.length
        } material${currentMaterials.length === 1 ? "" : "s"}`;
      }

      function applyFilters() {
        const search = document
          .getElementById("material-search")
          .value.trim()
          .toLowerCase();
        const cat = document.getElementById("material-category").value;
        const dMax = parseFloat(document.getElementById("density-max").value);
        const sMin = parseFloat(document.getElementById("strength-min").value);
        const cMax = parseFloat(document.getElementById("cost-max").value);
        const priority = document.getElementById("priority").value;

        currentMaterials = MATERIALS.filter((m) => {
          if (cat !== "all" && m.category !== cat) return false;
          if (
            search &&
            !m.name.toLowerCase().includes(search) &&
            !m.category.toLowerCase().includes(search)
          )
            return false;
          if (!isNaN(dMax) && m.density > dMax) return false;
          if (!isNaN(sMin) && m.strength < sMin) return false;
          if (!isNaN(cMax) && m.costIndex > cMax) return false;
          return true;
        });

        computeScores(priority);
        currentMaterials.sort((a, b) => (b.score ?? 0) - (a.score ?? 0));
        renderTable();
      }

      function resetFilters() {
        document.getElementById("material-search").value = "";
        document.getElementById("material-category").value = "all";
        document.getElementById("density-max").value = "";
        document.getElementById("strength-min").value = "";
        document.getElementById("cost-max").value = "";
        document.getElementById("priority").value = "balanced";
        currentMaterials = [...MATERIALS];
        computeScores("balanced");
        currentMaterials.sort((a, b) => (b.score ?? 0) - (a.score ?? 0));
        renderTable();
      }

      function selectMaterial(id) {
        selectedMaterial =
          currentMaterials.find((m) => m.id === id) ||
          MATERIALS.find((m) => m.id === id);
        const nameInput = document.getElementById("selected-material-name");
        if (selectedMaterial) {
          nameInput.value = selectedMaterial.name;
          nameInput.classList.remove("border-gray-200");
          nameInput.classList.add("border-blue-300");
          document.getElementById("quick-calc-error").textContent = "";
        }
      }

      function useBlockAsVolume() {
        const L = parseFloat(document.getElementById("len").value);
        const W = parseFloat(document.getElementById("wid").value);
        const T = parseFloat(document.getElementById("thk").value);
        if ([L, W, T].some((v) => isNaN(v) || v <= 0)) {
          document.getElementById("quick-calc-error").textContent =
            "Enter positive L, W and T in meters to use the block volume.";
          return;
        }
        const V = L * W * T;
        document.getElementById("part-volume").value = V.toFixed(4);
        document.getElementById("volume-unit").value = "m3";
        document.getElementById("quick-calc-error").textContent = "";
      }

      function convertVolumeToM3(value, unit) {
        if (unit === "m3") return value;
        if (unit === "cm3") return value * 1e-6;
        if (unit === "in3") return value * 1.6387e-5;
        return value;
      }

      function calculateWeight() {
        const errEl = document.getElementById("quick-calc-error");
        errEl.textContent = "";
        if (!selectedMaterial) {
          errEl.textContent = "Select a material from the table first.";
          return;
        }
        const volRaw = parseFloat(document.getElementById("part-volume").value);
        const unit = document.getElementById("volume-unit").value;
        if (isNaN(volRaw) || volRaw <= 0) {
          errEl.textContent =
            "Enter a positive volume or use the block dimensions.";
          return;
        }
        const V_m3 = convertVolumeToM3(volRaw, unit);
        const weight = V_m3 * selectedMaterial.density; // kg
        const outVol = document.getElementById("out-volume");
        const outW = document.getElementById("out-weight");
        const outC = document.getElementById("out-cost");

        outVol.textContent = `${V_m3.toExponential(3)} m³`;
        outW.textContent = `${weight.toFixed(2)} kg`;
        outC.textContent = `Cost index ${selectedMaterial.costIndex}/5`;

        document.getElementById("quick-calc-output").classList.remove("hidden");
      }

      function exportShortlist() {
        const data = currentMaterials.map(
          ({
            id,
            name,
            category,
            density,
            strength,
            stiffness,
            costIndex,
            sustainability,
            score,
          }) => ({
            id,
            name,
            category,
            density,
            strength,
            stiffness,
            costIndex,
            sustainability,
            score,
          })
        );
        const blob = new Blob([JSON.stringify(data, null, 2)], {
          type: "application/json",
        });
        const url = URL.createObjectURL(blob);
        const a = document.createElement("a");
        a.href = url;
        a.download = "materials-shortlist.json";
        document.body.appendChild(a);
        a.click();
        document.body.removeChild(a);
        URL.revokeObjectURL(url);
      }

      document.addEventListener("DOMContentLoaded", () => {
        computeScores("balanced");
        currentMaterials.sort((a, b) => (b.score ?? 0) - (a.score ?? 0));
        renderTable();

        document
          .getElementById("apply-filters")
          .addEventListener("click", applyFilters);
        document
          .getElementById("reset-filters")
          .addEventListener("click", resetFilters);
        document
          .getElementById("use-block")
          .addEventListener("click", useBlockAsVolume);
        document
          .getElementById("calc-weight")
          .addEventListener("click", calculateWeight);
        document
          .getElementById("export-json")
          .addEventListener("click", exportShortlist);

        ["material-search", "material-category"].forEach((id) => {
          const el = document.getElementById(id);
          el.addEventListener("keyup", (e) => {
            if (e.key === "Enter") applyFilters();
          });
          el.addEventListener("change", applyFilters);
        });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
