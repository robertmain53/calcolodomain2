    <script>
      // Core constants
      const FACTOR_MECH = 1.341022; // kW -> mechanical HP
      const FACTOR_METRIC = 1.35962; // kW -> metric HP

      const modeKwToHpBtn = document.getElementById("mode-kw-to-hp");
      const modeHpToKwBtn = document.getElementById("mode-hp-to-kw");
      const hpMechBtn = document.getElementById("hp-mech-btn");
      const hpMetricBtn = document.getElementById("hp-metric-btn");
      const inputValueEl = document.getElementById("input-value");
      const outputValueEl = document.getElementById("output-value");
      const inputLabelEl = document.getElementById("input-label");
      const outputLabelEl = document.getElementById("output-label");
      const inputHelperEl = document.getElementById("input-helper");
      const outputHelperEl = document.getElementById("output-helper");
      const summaryLineEl = document.getElementById("summary-line");
      const tableBodyEl = document.getElementById("kw-hp-table-body");

      let currentMode = "kw-to-hp"; // or 'hp-to-kw'
      let currentHpType = "mechanical"; // or 'metric';

      function getFactor() {
        return currentHpType === "mechanical" ? FACTOR_MECH : FACTOR_METRIC;
      }

      function formatNumber(val, decimals = 4) {
        if (!isFinite(val)) return "";
        return Number(val)
          .toFixed(decimals)
          .replace(/\.?0+$/, "");
      }

      function updateLabels() {
        const hpLabel =
          currentHpType === "mechanical"
            ? "horsepower (HP, mechanical)"
            : "horsepower (HP, metric)";
        if (currentMode === "kw-to-hp") {
          inputLabelEl.textContent = "Power in kilowatts (kW)";
          outputLabelEl.textContent = "Power in " + hpLabel;
          inputHelperEl.textContent =
            "Enter power in kW to convert to " + hpLabel + ".";
          outputHelperEl.textContent = "Result in " + hpLabel + ".";
        } else {
          inputLabelEl.textContent = "Power in " + hpLabel;
          outputLabelEl.textContent = "Power in kilowatts (kW)";
          inputHelperEl.textContent =
            "Enter power in " + hpLabel + " to convert to kW.";
          outputHelperEl.textContent = "Result in kilowatts.";
        }
      }

      function updateModeButtons() {
        if (currentMode === "kw-to-hp") {
          modeKwToHpBtn.classList.add("toggle-btn-active");
          modeHpToKwBtn.classList.remove("toggle-btn-active");
        } else {
          modeHpToKwBtn.classList.add("toggle-btn-active");
          modeKwToHpBtn.classList.remove("toggle-btn-active");
        }
      }

      function updateHpTypeButtons() {
        if (currentHpType === "mechanical") {
          hpMechBtn.classList.add("toggle-btn-active");
          hpMetricBtn.classList.remove("toggle-btn-active");
        } else {
          hpMetricBtn.classList.add("toggle-btn-active");
          hpMechBtn.classList.remove("toggle-btn-active");
        }
      }

      function calculate() {
        const raw = inputValueEl.value;
        const x = parseFloat(raw);
        if (!isFinite(x)) {
          outputValueEl.value = "";
          summaryLineEl.textContent = "Enter a value to see the conversion.";
          return;
        }

        const factor = getFactor();
        let result;
        let summary;

        const hpLabelShort =
          currentHpType === "mechanical" ? "HP (mechanical)" : "HP (metric)";

        if (currentMode === "kw-to-hp") {
          result = x * factor;
          outputValueEl.value = formatNumber(result, 4);
          summary = `${formatNumber(x, 4)} kW ≈ ${formatNumber(
            result,
            4
          )} ${hpLabelShort}`;
        } else {
          result = x / factor;
          outputValueEl.value = formatNumber(result, 4);
          summary = `${formatNumber(x, 4)} ${hpLabelShort} ≈ ${formatNumber(
            result,
            4
          )} kW`;
        }
        summaryLineEl.textContent = summary;
      }

      // Event listeners
      modeKwToHpBtn.addEventListener("click", () => {
        currentMode = "kw-to-hp";
        updateModeButtons();
        updateLabels();
        calculate();
      });

      modeHpToKwBtn.addEventListener("click", () => {
        currentMode = "hp-to-kw";
        updateModeButtons();
        updateLabels();
        calculate();
      });

      hpMechBtn.addEventListener("click", () => {
        currentHpType = "mechanical";
        updateHpTypeButtons();
        updateLabels();
        calculate();
      });

      hpMetricBtn.addEventListener("click", () => {
        currentHpType = "metric";
        updateHpTypeButtons();
        updateLabels();
        calculate();
      });

      inputValueEl.addEventListener("input", calculate);

      // Preset buttons
      document.querySelectorAll("button[data-preset]").forEach((btn) => {
        btn.addEventListener("click", () => {
          const val = btn.getAttribute("data-preset");
          if (currentMode === "kw-to-hp") {
            inputValueEl.value = val;
          } else {
            // If in HP->kW mode, interpret presets as kW equivalents in HP
            const factor = getFactor();
            inputValueEl.value = formatNumber(parseFloat(val) * factor, 4);
          }
          calculate();
          inputValueEl.focus();
        });
      });

      // Build reference table
      function buildTable() {
        const rowsKw = [
          0.5, 0.75, 1, 1.5, 2, 3, 5, 7.5, 10, 15, 22, 30, 37, 45, 55, 75, 90,
          110,
        ];
        const frag = document.createDocumentFragment();
        rowsKw.forEach((kw) => {
          const tr = document.createElement("tr");
          const mech = kw * FACTOR_MECH;
          const metric = kw * FACTOR_METRIC;

          tr.innerHTML = `
        <td class="px-3 py-1.5 whitespace-nowrap">${formatNumber(kw, 2)}</td>
        <td class="px-3 py-1.5 whitespace-nowrap">${formatNumber(mech, 2)}</td>
        <td class="px-3 py-1.5 whitespace-nowrap">${formatNumber(
          metric,
          2
        )}</td>
      `;
          frag.appendChild(tr);
        });
        tableBodyEl.innerHTML = "";
        tableBodyEl.appendChild(frag);
      }

      // Initialize
      buildTable();
      updateModeButtons();
      updateHpTypeButtons();
      updateLabels();
      inputValueEl.value = "100";
      calculate();
    </script>

