    <script>
      // Simple helper: degrees to radians
      function deg2rad(deg) {
        return (deg * Math.PI) / 180;
      }

      function computeAirfoil() {
        const type = document.getElementById("airfoil-type").value;
        const c = parseFloat(document.getElementById("chord").value);
        const b = parseFloat(document.getElementById("span").value);
        const V = parseFloat(document.getElementById("velocity").value);
        const rho = parseFloat(document.getElementById("rho").value);
        let ReInput = parseFloat(document.getElementById("reynolds").value);
        const alphaDeg = parseFloat(document.getElementById("alpha").value);
        const W = parseFloat(document.getElementById("weight").value);
        const errorEl = document.getElementById("error-msg");
        errorEl.classList.add("hidden");
        errorEl.textContent = "";

        if (!(c > 0 && b > 0 && V > 0 && rho > 0)) {
          errorEl.textContent =
            "Chord, span, speed and density must be positive numbers.";
          errorEl.classList.remove("hidden");
          return;
        }

        const mu = 1.8e-5; // dynamic viscosity of air (Pa·s), approximate
        const S = c * b;
        const q = 0.5 * rho * V * V;
        const alphaRad = deg2rad(alphaDeg);

        // If user left Re blank or <=0, compute from chord
        let Re = ReInput > 0 ? ReInput : (rho * V * c) / mu;

        // Lift curve parameters
        const a = 2 * Math.PI; // per rad
        const alphaL0Deg = type === "symmetric" ? 0 : -2;
        const alphaL0Rad = deg2rad(alphaL0Deg);

        let Cl = a * (alphaRad - alphaL0Rad);

        // Stall model: cap Cl and roll off after stall angle
        const ClMax = type === "symmetric" ? 1.4 : 1.6;
        const stallDeg = 15;
        const stallRad = deg2rad(stallDeg);

        const absAlpha = Math.abs(alphaRad);
        if (absAlpha <= stallRad) {
          if (Math.abs(Cl) > ClMax) {
            Cl = ClMax * Math.sign(Cl);
          }
        } else {
          // Linear drop beyond stall
          const over = absAlpha - stallRad;
          const dropFactor = Math.max(0, 1 - over / deg2rad(10)); // fully stalled ~25°
          Cl = ClMax * dropFactor * Math.sign(Cl);
        }

        // Profile drag Cd0 as a simple function of Re
        // Cd0 ≈ 0.02 at Re=1e5, 0.012 at 1e6, 0.008 at 1e7
        const logRe = Math.log10(Re);
        let Cd0;
        if (logRe <= 5) Cd0 = 0.02;
        else if (logRe >= 7) Cd0 = 0.008;
        else {
          // linear interpolation between (5,0.02) and (7,0.008)
          Cd0 = 0.02 + (0.008 - 0.02) * ((logRe - 5) / (7 - 5));
        }

        // Induced drag factor k
        const AR = b / c;
        const e = 0.85; // Oswald efficiency factor
        const k = 1 / (Math.PI * e * AR);

        const Cd = Cd0 + k * Cl * Cl;

        const L = q * S * Cl;
        const D = q * S * Cd;
        const LD = D !== 0 ? L / D : NaN;

        // Update outputs
        document.getElementById("out-area").textContent = S.toFixed(3);
        document.getElementById("out-q").textContent = q.toFixed(1);
        document.getElementById("out-cl").textContent = Cl.toFixed(3);
        document.getElementById("out-cd").textContent = Cd.toFixed(4);
        document.getElementById("out-lift").textContent = L.toFixed(1);
        document.getElementById("out-drag").textContent = D.toFixed(1);
        document.getElementById("out-ld").textContent = isFinite(LD)
          ? LD.toFixed(1)
          : "–";
        document.getElementById("out-re").textContent = Re.toExponential(2);

        const weightNote = document.getElementById("weight-note");
        if (W && W > 0) {
          const ratio = L / W;
          let msg = `At this condition, lift is ${(ratio * 100).toFixed(
            1
          )}% of weight. `;
          if (ratio > 1.05)
            msg += "The wing can support the specified weight with margin.";
          else if (ratio > 0.95)
            msg +=
              "Lift is close to weight; you are near level-flight conditions.";
          else msg += "Lift is insufficient to support the specified weight.";
          weightNote.textContent = msg;
          weightNote.classList.remove("hidden");
        } else {
          weightNote.classList.add("hidden");
          weightNote.textContent = "";
        }

        drawAirfoilGeometry(type);
        drawCpDistribution(Cl);
      }

      function drawAirfoilGeometry(type) {
        const canvas = document.getElementById("airfoil-geometry");
        if (!canvas.getContext) return;
        const ctx = canvas.getContext("2d");
        const w = canvas.width;
        const h = canvas.height;
        ctx.clearRect(0, 0, w, h);

        // Axes
        ctx.strokeStyle = "#d1d5db";
        ctx.lineWidth = 1;
        ctx.beginPath();
        ctx.moveTo(40, h / 2);
        ctx.lineTo(w - 10, h / 2);
        ctx.moveTo(40, 20);
        ctx.lineTo(40, h - 20);
        ctx.stroke();

        // Generate simple NACA-like thickness and camber
        const N = 100;
        const ptsUpper = [];
        const ptsLower = [];
        const t = 0.12; // 12% thickness
        const m = type === "cambered" ? 0.02 : 0.0; // max camber 2%
        const p = 0.4; // camber position 40%

        for (let i = 0; i <= N; i++) {
          const x = i / N; // 0..1
          // thickness distribution (NACA 4-digit)
          const yt =
            5 *
            t *
            (0.2969 * Math.sqrt(x) -
              0.126 * x -
              0.3516 * x * x +
              0.2843 * x * x * x -
              0.1015 * x * x * x * x);

          // camber line
          let yc = 0,
            dyc_dx = 0;
          if (m > 0) {
            if (x < p) {
              yc = (m / (p * p)) * (2 * p * x - x * x);
              dyc_dx = ((2 * m) / (p * p)) * (p - x);
            } else {
              yc = (m / ((1 - p) * (1 - p))) * (1 - 2 * p + 2 * p * x - x * x);
              dyc_dx = ((2 * m) / ((1 - p) * (1 - p))) * (p - x);
            }
          }
          const theta = Math.atan(dyc_dx);
          const xu = x - yt * Math.sin(theta);
          const yu = yc + yt * Math.cos(theta);
          const xl = x + yt * Math.sin(theta);
          const yl = yc - yt * Math.cos(theta);

          ptsUpper.push({ x: xu, y: yu });
          ptsLower.push({ x: xl, y: yl });
        }

        // Transform to canvas coordinates
        function toCanvas(pt) {
          const marginX = 40;
          const marginY = 20;
          const plotW = w - marginX - 10;
          const plotH = h - 2 * marginY;
          const x = marginX + pt.x * plotW;
          const y = h / 2 - pt.y * plotH * 2; // scale thickness
          return { x, y };
        }

        ctx.strokeStyle = "#2563eb";
        ctx.lineWidth = 2;
        ctx.beginPath();
        ptsUpper.forEach((p, idx) => {
          const c = toCanvas(p);
          if (idx === 0) ctx.moveTo(c.x, c.y);
          else ctx.lineTo(c.x, c.y);
        });
        for (let i = ptsLower.length - 1; i >= 0; i--) {
          const c = toCanvas(ptsLower[i]);
          ctx.lineTo(c.x, c.y);
        }
        ctx.closePath();
        ctx.stroke();
        ctx.fillStyle = "rgba(37,99,235,0.08)";
        ctx.fill();

        // Label
        ctx.fillStyle = "#4b5563";
        ctx.font = "12px Inter, sans-serif";
        ctx.fillText("x/c", w - 30, h / 2 - 6);
        ctx.fillText("y/c", 22, 24);
      }

      function drawCpDistribution(Cl) {
        const canvas = document.getElementById("cp-plot");
        if (!canvas.getContext) return;
        const ctx = canvas.getContext("2d");
        const w = canvas.width;
        const h = canvas.height;
        ctx.clearRect(0, 0, w, h);

        // Axes
        ctx.strokeStyle = "#d1d5db";
        ctx.lineWidth = 1;
        ctx.beginPath();
        ctx.moveTo(40, h - 30);
        ctx.lineTo(w - 10, h - 30); // x-axis (x/c)
        ctx.moveTo(40, 20);
        ctx.lineTo(40, h - 10); // Cp axis
        ctx.stroke();

        // Generate Cp distribution (very approximate)
        const N = 80;
        const cps = [];
        let CpMin = 0,
          CpMax = 0;
        for (let i = 0; i <= N; i++) {
          const x = i / N;
          // Base Cp: mild suction near LE, relaxing to 0 at TE
          const base = -1.0 * Math.exp(-5 * x);
          // Lift-induced component: stronger near LE
          const shape = (1 - x) * Math.exp(-3 * x);
          const Cp = base - Cl * 0.2 * shape;
          cps.push({ x, Cp });
          if (i === 0) {
            CpMin = CpMax = Cp;
          } else {
            if (Cp < CpMin) CpMin = Cp;
            if (Cp > CpMax) CpMax = Cp;
          }
        }

        // Expand range a bit
        const pad = 0.2 * (CpMax - CpMin || 1);
        CpMin -= pad;
        CpMax += pad;

        function toCanvas(pt) {
          const marginX = 40;
          const marginY = 20;
          const plotW = w - marginX - 10;
          const plotH = h - marginY - 30;
          const x = marginX + pt.x * plotW;
          const y = h - 30 - ((pt.Cp - CpMin) / (CpMax - CpMin)) * plotH;
          return { x, y };
        }

        ctx.strokeStyle = "#16a34a";
        ctx.lineWidth = 2;
        ctx.beginPath();
        cps.forEach((p, idx) => {
          const c = toCanvas(p);
          if (idx === 0) ctx.moveTo(c.x, c.y);
          else ctx.lineTo(c.x, c.y);
        });
        ctx.stroke();

        // Labels
        ctx.fillStyle = "#4b5563";
        ctx.font = "12px Inter, sans-serif";
        ctx.fillText("x/c", w - 30, h - 34);
        ctx.fillText("C_p", 18, 24);
      }

      document
        .getElementById("compute-btn")
        .addEventListener("click", computeAirfoil);
      window.addEventListener("load", () => {
        computeAirfoil();
      });
    </script>

