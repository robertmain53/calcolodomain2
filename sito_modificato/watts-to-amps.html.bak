    <script>
      // State
      let currentMode = "dc"; // 'dc', 'ac1', 'ac3'
      let swapDirection = "watts-to-amps"; // or 'amps-to-watts'

      const modeButtons = {
        dc: document.getElementById("mode-dc"),
        ac1: document.getElementById("mode-ac1"),
        ac3: document.getElementById("mode-ac3"),
      };

      const powerInput = document.getElementById("power");
      const powerUnit = document.getElementById("powerUnit");
      const voltageInput = document.getElementById("voltage");
      const voltageUnit = document.getElementById("voltageUnit");
      const pfWrapper = document.getElementById("pf-wrapper");
      const pfInput = document.getElementById("powerFactor");

      const calculateBtn = document.getElementById("calculateBtn");
      const swapBtn = document.getElementById("swapBtn");
      const resetBtn = document.getElementById("resetBtn");

      const resultsCard = document.getElementById("resultsCard");
      const resultMain = document.getElementById("resultMain");
      const resultDetail = document.getElementById("resultDetail");
      const resultFormula = document.getElementById("resultFormula");

      function setMode(mode) {
        currentMode = mode;
        Object.keys(modeButtons).forEach((key) => {
          if (key === mode) {
            modeButtons[key].classList.add("toggle-btn-active");
            modeButtons[key].classList.remove("bg-white", "text-gray-700");
          } else {
            modeButtons[key].classList.remove("toggle-btn-active");
            modeButtons[key].classList.add("bg-white", "text-gray-700");
          }
        });

        if (mode === "dc") {
          pfWrapper.classList.add("hidden");
        } else {
          pfWrapper.classList.remove("hidden");
        }
        resultsCard.classList.add("hidden");
      }

      modeButtons.dc.addEventListener("click", () => setMode("dc"));
      modeButtons.ac1.addEventListener("click", () => setMode("ac1"));
      modeButtons.ac3.addEventListener("click", () => setMode("ac3"));

      function toWatts(value, unit) {
        if (isNaN(value)) return NaN;
        return unit === "kW" ? value * 1000 : value;
      }

      function toVolts(value, unit) {
        if (isNaN(value)) return NaN;
        return unit === "kV" ? value * 1000 : value;
      }

      function formatNumber(num, decimals = 3) {
        if (!isFinite(num)) return "";
        if (num === 0) return "0";
        if (Math.abs(num) >= 1000 || Math.abs(num) < 0.01) {
          return num.toExponential(2);
        }
        return num.toFixed(decimals).replace(/\.?0+$/, "");
      }

      function calculate() {
        const Praw = parseFloat(powerInput.value);
        const Vraw = parseFloat(voltageInput.value);
        const PF = parseFloat(pfInput.value);

        if (isNaN(Praw) || isNaN(Vraw) || Vraw <= 0) {
          alert("Please enter valid power and voltage values.");
          return;
        }

        const P = toWatts(Praw, powerUnit.value);
        const V = toVolts(Vraw, voltageUnit.value);

        let I, formulaText, detail;

        if (swapDirection === "watts-to-amps") {
          if (currentMode === "dc") {
            I = P / V;
            formulaText = `DC: I = P / V = ${P} W / ${V} V`;
            detail = `Current for ${formatNumber(P)} W at ${formatNumber(
              V
            )} V DC.`;
          } else {
            if (isNaN(PF) || PF <= 0 || PF > 1) {
              alert("Please enter a valid power factor between 0 and 1.");
              return;
            }
            if (currentMode === "ac1") {
              I = P / (V * PF);
              formulaText = `Single‑phase AC: I = P / (V × PF) = ${P} W / (${V} V × ${PF})`;
              detail = `Current for ${formatNumber(P)} W at ${formatNumber(
                V
              )} V, PF ${PF}.`;
            } else {
              const denom = Math.sqrt(3) * V * PF;
              I = P / denom;
              formulaText = `Three‑phase AC: I = P / (√3 × V × PF) = ${P} W / (1.732 × ${V} V × ${PF})`;
              detail = `Line current for ${formatNumber(P)} W at ${formatNumber(
                V
              )} V, PF ${PF}, three‑phase.`;
            }
          }

          resultMain.textContent = `${formatNumber(I, 3)} A`;
          resultDetail.textContent = detail;
          resultFormula.textContent = formulaText;
        } else {
          // amps to watts
          const Ival = Praw; // here "power" field holds current when swapped
          if (Ival < 0) {
            alert("Current cannot be negative.");
            return;
          }
          let Pcalc;
          if (currentMode === "dc") {
            Pcalc = Ival * V;
            formulaText = `DC: P = V × I = ${V} V × ${Ival} A`;
            detail = `Power for ${formatNumber(Ival)} A at ${formatNumber(
              V
            )} V DC.`;
          } else {
            if (isNaN(PF) || PF <= 0 || PF > 1) {
              alert("Please enter a valid power factor between 0 and 1.");
              return;
            }
            if (currentMode === "ac1") {
              Pcalc = V * Ival * PF;
              formulaText = `Single‑phase AC: P = V × I × PF = ${V} V × ${Ival} A × ${PF}`;
              detail = `Power for ${formatNumber(Ival)} A at ${formatNumber(
                V
              )} V, PF ${PF}.`;
            } else {
              Pcalc = Math.sqrt(3) * V * Ival * PF;
              formulaText = `Three‑phase AC: P = √3 × V × I × PF = 1.732 × ${V} V × ${Ival} A × ${PF}`;
              detail = `Total three‑phase power for ${formatNumber(
                Ival
              )} A at ${formatNumber(V)} V, PF ${PF}.`;
            }
          }

          const Pdisplay = powerUnit.value === "kW" ? Pcalc / 1000 : Pcalc;
          const unitLabel = powerUnit.value;

          resultMain.textContent = `${formatNumber(Pdisplay, 3)} ${unitLabel}`;
          resultDetail.textContent = detail;
          resultFormula.textContent = formulaText;
        }

        resultsCard.classList.remove("hidden");
      }

      calculateBtn.addEventListener("click", calculate);

      swapBtn.addEventListener("click", () => {
        if (swapDirection === "watts-to-amps") {
          swapDirection = "amps-to-watts";
          swapBtn.textContent = "Swap: Watts → Amps";
          document.querySelector('label[for="power"]').textContent = "Current";
          powerUnit.innerHTML = '<option value="A">A</option>';
          powerUnit.value = "A";
        } else {
          swapDirection = "watts-to-amps";
          swapBtn.textContent = "Swap: Amps → Watts";
          powerUnit.innerHTML =
            '<option value="W">W</option><option value="kW">kW</option>';
          powerUnit.value = "W";
          document.querySelector('label[for="power"]').textContent = "Power";
        }
        powerInput.value = "";
        resultsCard.classList.add("hidden");
      });

      resetBtn.addEventListener("click", () => {
        powerInput.value = "";
        voltageInput.value = "";
        pfInput.value = "";
        powerUnit.value =
          swapDirection === "watts-to-amps" ? "W" : powerUnit.value || "A";
        voltageUnit.value = "V";
        setMode("dc");
        resultsCard.classList.add("hidden");
      });

      // Preset buttons
      document.querySelectorAll("[data-preset]").forEach((btn) => {
        btn.addEventListener("click", () => {
          const preset = JSON.parse(btn.getAttribute("data-preset"));
          setMode(preset.mode);
          swapDirection = "watts-to-amps";
          swapBtn.textContent = "Swap: Amps → Watts";
          powerUnit.innerHTML =
            '<option value="W">W</option><option value="kW">kW</option>';
          document.querySelector('label[for="power"]').textContent = "Power";

          powerInput.value = preset.P;
          powerUnit.value = preset.Punit;
          voltageInput.value = preset.V;
          voltageUnit.value = preset.Vunit;
          if (preset.PF !== undefined) {
            pfInput.value = preset.PF;
          } else {
            pfInput.value = "";
          }
          resultsCard.classList.add("hidden");
        });
      });

      // Initialize
      setMode("dc");
    </script>

    <script>
      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
