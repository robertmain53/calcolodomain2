    <script>
      (function () {
        // Elements
        const alphaRange = document.getElementById("alpha");
        const betaRange = document.getElementById("beta");
        const gammaRange = document.getElementById("gamma");
        const deltaRange = document.getElementById("delta");

        const alphaInput = document.getElementById("alpha-input");
        const betaInput = document.getElementById("beta-input");
        const gammaInput = document.getElementById("gamma-input");
        const deltaInput = document.getElementById("delta-input");

        const prey0Input = document.getElementById("prey0");
        const pred0Input = document.getElementById("pred0");
        const tmaxInput = document.getElementById("tmax");
        const dtInput = document.getElementById("dt");
        const showEqCheckbox = document.getElementById("show-equilibrium");

        const simulateBtn = document.getElementById("simulate-btn");
        const resetBtn = document.getElementById("reset-btn");
        const exportBtn = document.getElementById("export-csv-btn");
        const copyParamsBtn = document.getElementById("copy-params-btn");
        const eqText = document.getElementById("equilibrium-text");

        let timeSeriesChart, phasePlaneChart;
        let lastSimulation = null;

        function syncPair(rangeEl, inputEl) {
          rangeEl.addEventListener("input", () => {
            inputEl.value = rangeEl.value;
            updateEquilibriumDisplay();
          });
          inputEl.addEventListener("input", () => {
            const v = parseFloat(inputEl.value);
            if (!isNaN(v)) {
              rangeEl.value = v;
              updateEquilibriumDisplay();
            }
          });
        }

        function getParams() {
          const alpha = parseFloat(alphaInput.value);
          const beta = parseFloat(betaInput.value);
          const gamma = parseFloat(gammaInput.value);
          const delta = parseFloat(deltaInput.value);
          const x0 = parseFloat(prey0Input.value);
          const y0 = parseFloat(pred0Input.value);
          const tmax = parseFloat(tmaxInput.value);
          const dt = parseFloat(dtInput.value);

          return { alpha, beta, gamma, delta, x0, y0, tmax, dt };
        }

        function updateEquilibriumDisplay() {
          const { alpha, beta, gamma, delta } = getParams();
          if (beta > 0 && delta > 0) {
            const xStar = gamma / delta;
            const yStar = alpha / beta;
            eqText.textContent = `x* = ${xStar.toFixed(
              2
            )}, y* = ${yStar.toFixed(2)}`;
          } else {
            eqText.textContent = "Equilibrium undefined (β and δ must be > 0).";
          }
        }

        function rk4Step(x, y, t, dt, params) {
          const { alpha, beta, gamma, delta } = params;

          function f1(x, y) {
            return alpha * x - beta * x * y;
          }
          function f2(x, y) {
            return delta * x * y - gamma * y;
          }

          const k1x = f1(x, y);
          const k1y = f2(x, y);

          const k2x = f1(x + 0.5 * dt * k1x, y + 0.5 * dt * k1y);
          const k2y = f2(x + 0.5 * dt * k1x, y + 0.5 * dt * k1y);

          const k3x = f1(x + 0.5 * dt * k2x, y + 0.5 * dt * k2y);
          const k3y = f2(x + 0.5 * dt * k2x, y + 0.5 * dt * k2y);

          const k4x = f1(x + dt * k3x, y + dt * k3y);
          const k4y = f2(x + dt * k3x, y + dt * k3y);

          const xNext = x + (dt / 6) * (k1x + 2 * k2x + 2 * k3x + k4x);
          const yNext = y + (dt / 6) * (k1y + 2 * k2y + 2 * k3y + k4y);

          return {
            x: Math.max(0, xNext),
            y: Math.max(0, yNext),
          };
        }

        function simulate() {
          const params = getParams();
          const { alpha, beta, gamma, delta, x0, y0, tmax, dt } = params;

          if (
            [alpha, beta, gamma, delta, x0, y0, tmax, dt].some((v) => isNaN(v))
          ) {
            alert("Please enter valid numeric values for all parameters.");
            return null;
          }
          if (dt <= 0 || tmax <= 0) {
            alert("Simulation time and time step must be positive.");
            return null;
          }
          if (tmax / dt > 20000) {
            if (
              !confirm(
                "This will generate more than 20,000 steps and may be slow. Continue?"
              )
            ) {
              return null;
            }
          }

          const times = [];
          const prey = [];
          const pred = [];

          let t = 0;
          let x = Math.max(0, x0);
          let y = Math.max(0, y0);

          while (t <= tmax + 1e-9) {
            times.push(t);
            prey.push(x);
            pred.push(y);

            const step = rk4Step(x, y, t, dt, params);
            x = step.x;
            y = step.y;
            t += dt;
          }

          lastSimulation = { params, times, prey, pred };
          return lastSimulation;
        }

        function createOrUpdateCharts(sim) {
          const { times, prey, pred, params } = sim;
          const { alpha, beta, gamma, delta } = params;

          const tsCtx = document
            .getElementById("timeSeriesChart")
            .getContext("2d");
          const ppCtx = document
            .getElementById("phasePlaneChart")
            .getContext("2d");

          const labels = times;
          const preyData = prey;
          const predData = pred;

          if (timeSeriesChart) timeSeriesChart.destroy();
          if (phasePlaneChart) phasePlaneChart.destroy();

          timeSeriesChart = new Chart(tsCtx, {
            type: "line",
            data: {
              labels,
              datasets: [
                {
                  label: "Prey",
                  data: preyData,
                  borderColor: "#10b981",
                  backgroundColor: "rgba(16,185,129,0.1)",
                  tension: 0.15,
                  pointRadius: 0,
                },
                {
                  label: "Predator",
                  data: predData,
                  borderColor: "#f97373",
                  backgroundColor: "rgba(248,113,113,0.1)",
                  tension: 0.15,
                  pointRadius: 0,
                },
              ],
            },
            options: {
              responsive: true,
              maintainAspectRatio: false,
              interaction: { mode: "index", intersect: false },
              plugins: {
                legend: { display: false },
                tooltip: {
                  callbacks: {
                    label: function (ctx) {
                      const label = ctx.dataset.label || "";
                      return `${label}: ${ctx.parsed.y.toFixed(3)}`;
                    },
                  },
                },
              },
              scales: {
                x: {
                  title: { display: true, text: "Time" },
                },
                y: {
                  title: { display: true, text: "Population" },
                  beginAtZero: true,
                },
              },
            },
          });

          // Phase plane
          const phaseData = preyData.map((x, i) => ({ x, y: predData[i] }));

          const datasets = [
            {
              label: "Trajectory",
              data: phaseData,
              borderColor: "#0ea5e9",
              backgroundColor: "rgba(14,165,233,0.1)",
              showLine: true,
              pointRadius: 0,
              tension: 0.1,
            },
          ];

          if (showEqCheckbox.checked && beta > 0 && delta > 0) {
            const xStar = gamma / delta;
            const yStar = alpha / beta;
            datasets.push({
              label: "Equilibrium",
              data: [{ x: xStar, y: yStar }],
              borderColor: "#111827",
              backgroundColor: "#111827",
              pointRadius: 4,
              showLine: false,
            });
          }

          phasePlaneChart = new Chart(ppCtx, {
            type: "scatter",
            data: { datasets },
            options: {
              responsive: true,
              maintainAspectRatio: false,
              plugins: {
                legend: { display: false },
                tooltip: {
                  callbacks: {
                    label: function (ctx) {
                      return `Prey: ${ctx.parsed.x.toFixed(
                        3
                      )}, Predator: ${ctx.parsed.y.toFixed(3)}`;
                    },
                  },
                },
              },
              scales: {
                x: {
                  title: { display: true, text: "Prey population (x)" },
                  beginAtZero: true,
                },
                y: {
                  title: { display: true, text: "Predator population (y)" },
                  beginAtZero: true,
                },
              },
            },
          });
        }

        function exportCSV() {
          if (!lastSimulation) {
            alert("Run a simulation first.");
            return;
          }
          const { times, prey, pred } = lastSimulation;
          let csv = "t,prey,predator\n";
          for (let i = 0; i < times.length; i++) {
            csv += `${times[i]},${prey[i]},${pred[i]}\n`;
          }
          const blob = new Blob([csv], { type: "text/csv;charset=utf-8;" });
          const url = URL.createObjectURL(blob);
          const a = document.createElement("a");
          a.href = url;
          a.download = "lotka-volterra-simulation.csv";
          document.body.appendChild(a);
          a.click();
          document.body.removeChild(a);
          URL.revokeObjectURL(url);
        }

        function copyParamsToClipboard() {
          const { alpha, beta, gamma, delta, x0, y0, tmax, dt } = getParams();
          const text = [
            "Lotka–Volterra parameters:",
            `alpha (prey growth) = ${alpha}`,
            `beta (predation rate) = ${beta}`,
            `gamma (predator death) = ${gamma}`,
            `delta (conversion efficiency) = ${delta}`,
            `x0 (initial prey) = ${x0}`,
            `y0 (initial predators) = ${y0}`,
            `T (simulation time) = ${tmax}`,
            `dt (time step) = ${dt}`,
          ].join("\n");

          navigator.clipboard
            .writeText(text)
            .then(() => {
              copyParamsBtn.textContent = "Copied!";
              setTimeout(() => {
                copyParamsBtn.textContent = "Copy parameters";
              }, 1500);
            })
            .catch(() => {
              alert(
                "Could not copy to clipboard. You can copy manually:\n\n" + text
              );
            });
        }

        function resetDefaults() {
          alphaRange.value = alphaInput.value = 1.0;
          betaRange.value = betaInput.value = 0.1;
          gammaRange.value = gammaInput.value = 1.5;
          deltaRange.value = deltaInput.value = 0.75;
          prey0Input.value = 10;
          pred0Input.value = 5;
          tmaxInput.value = 30;
          dtInput.value = 0.01;
          showEqCheckbox.checked = true;
          updateEquilibriumDisplay();
          const sim = simulate();
          if (sim) createOrUpdateCharts(sim);
        }

        // Event bindings
        syncPair(alphaRange, alphaInput);
        syncPair(betaRange, betaInput);
        syncPair(gammaRange, gammaInput);
        syncPair(deltaRange, deltaInput);

        showEqCheckbox.addEventListener("change", () => {
          if (lastSimulation) createOrUpdateCharts(lastSimulation);
        });

        simulateBtn.addEventListener("click", () => {
          const sim = simulate();
          if (sim) createOrUpdateCharts(sim);
        });

        resetBtn.addEventListener("click", resetDefaults);
        exportBtn.addEventListener("click", exportCSV);
        copyParamsBtn.addEventListener("click", copyParamsToClipboard);

        // Initial render
        updateEquilibriumDisplay();
        const initialSim = simulate();
        if (initialSim) createOrUpdateCharts(initialSim);
      })();
    </script>
  
