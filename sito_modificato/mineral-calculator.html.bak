    <script>
      // Mineral reference data (simplified, approximate)
      const MINERALS = [
        {
          id: "calcium",
          name: "Calcium",
          unit: "mg",
          base: {
            adult: 1000,
            femaleHigh: 1200,
            teen: 1300,
            child: 700,
            toddler: 500,
          },
          pregnancyDelta: 0,
          lactationDelta: 0,
          upper: 2500,
        },
        {
          id: "iron",
          name: "Iron",
          unit: "mg",
          base: {
            adultMale: 8,
            adultFemale: 18,
            postMenopause: 8,
            teen: 11,
            teenFemale: 15,
            child: 10,
            toddler: 7,
          },
          pregnancyDelta: 9, // extra for pregnancy
          lactationDelta: 0,
          upper: 45,
        },
        {
          id: "magnesium",
          name: "Magnesium",
          unit: "mg",
          base: {
            adultMale: 400,
            adultFemale: 310,
            teen: 360,
            child: 240,
            toddler: 80,
          },
          pregnancyDelta: 40,
          lactationDelta: 40,
          upper: 350, // supplemental upper; food usually safe
        },
        {
          id: "zinc",
          name: "Zinc",
          unit: "mg",
          base: {
            adultMale: 11,
            adultFemale: 8,
            teen: 11,
            child: 8,
            toddler: 3,
          },
          pregnancyDelta: 3,
          lactationDelta: 4,
          upper: 40,
        },
        {
          id: "potassium",
          name: "Potassium",
          unit: "mg",
          base: {
            adult: 3400,
            adultFemale: 2600,
            teen: 3000,
            child: 2300,
            toddler: 2000,
          },
          pregnancyDelta: 0,
          lactationDelta: 0,
          upper: null,
        },
        {
          id: "sodium",
          name: "Sodium",
          unit: "mg",
          base: { adult: 1500, teen: 1500, child: 1200, toddler: 1000 },
          pregnancyDelta: 0,
          lactationDelta: 0,
          upper: 2300,
        },
        {
          id: "phosphorus",
          name: "Phosphorus",
          unit: "mg",
          base: { adult: 700, teen: 1250, child: 1250, toddler: 460 },
          pregnancyDelta: 0,
          lactationDelta: 0,
          upper: 4000,
        },
        {
          id: "iodine",
          name: "Iodine",
          unit: "mcg",
          base: { adult: 150, teen: 150, child: 120, toddler: 90 },
          pregnancyDelta: 70,
          lactationDelta: 90,
          upper: 1100,
        },
        {
          id: "selenium",
          name: "Selenium",
          unit: "mcg",
          base: { adult: 55, teen: 55, child: 40, toddler: 20 },
          pregnancyDelta: 5,
          lactationDelta: 15,
          upper: 400,
        },
      ];

      function getNumericValue(id) {
        const el = document.getElementById(id);
        if (!el) return NaN;
        const v = parseFloat(el.value);
        return isNaN(v) ? NaN : v;
      }

      function computeTargets() {
        const sex = document.getElementById("sex").value;
        const age = getNumericValue("age");
        const ageUnit = document.getElementById("age-unit").value;
        const repro = document.getElementById("repro-status").value;

        let ageYears = age;
        if (ageUnit === "months") {
          ageYears = age / 12;
        }

        const isChild = ageYears >= 4 && ageYears < 9;
        const isTween = ageYears >= 9 && ageYears < 14;
        const isTeen = ageYears >= 14 && ageYears < 19;
        const isToddler = ageYears >= 1 && ageYears < 4;
        const isAdult = ageYears >= 19;

        const targets = {};

        MINERALS.forEach((m) => {
          let base = null;
          let note = "";

          if (m.id === "calcium") {
            if (isTeen) {
              base = m.base.teen;
              note = "Higher needs during adolescence.";
            } else if (isAdult && ageYears >= 51) {
              base = m.base.femaleHigh;
              note = "Higher needs for older adults.";
            } else if (isAdult) {
              base = m.base.adult;
            } else if (isChild || isTween) {
              base = m.base.child;
            } else if (isToddler) {
              base = m.base.toddler;
            }
          } else if (m.id === "iron") {
            if (isTeen) {
              base = sex === "female" ? m.base.teenFemale : m.base.teen;
            } else if (isAdult) {
              if (sex === "female") {
                if (ageYears >= 51) {
                  base = m.base.postMenopause;
                  note = "Post-menopause.";
                } else {
                  base = m.base.adultFemale;
                }
              } else {
                base = m.base.adultMale;
              }
            } else if (isChild || isTween) {
              base = m.base.child;
            } else if (isToddler) {
              base = m.base.toddler;
            }
          } else if (m.id === "magnesium") {
            if (isTeen) {
              base = m.base.teen;
            } else if (isAdult) {
              base = sex === "female" ? m.base.adultFemale : m.base.adultMale;
            } else if (isChild || isTween) {
              base = m.base.child;
            } else if (isToddler) {
              base = m.base.toddler;
            }
          } else if (m.id === "zinc") {
            if (isTeen || isAdult) {
              base = sex === "female" ? m.base.adultFemale : m.base.adultMale;
            } else if (isChild || isTween) {
              base = m.base.child;
            } else if (isToddler) {
              base = m.base.toddler;
            }
          } else if (m.id === "potassium") {
            if (isTeen || isAdult) {
              base = sex === "female" ? m.base.adultFemale : m.base.adult;
            } else if (isChild || isTween) {
              base = m.base.child;
            } else if (isToddler) {
              base = m.base.toddler;
            }
          } else if (m.id === "sodium") {
            if (isTeen || isAdult) {
              base = m.base.adult;
            } else if (isChild || isTween) {
              base = m.base.child;
            } else if (isToddler) {
              base = m.base.toddler;
            }
          } else if (m.id === "phosphorus") {
            if (isTeen || isChild || isTween) {
              base = m.base.teen;
            } else if (isAdult) {
              base = m.base.adult;
            } else if (isToddler) {
              base = m.base.toddler;
            }
          } else if (m.id === "iodine" || m.id === "selenium") {
            if (isTeen || isAdult) {
              base = m.base.adult;
            } else if (isChild || isTween) {
              base = m.base.child;
            } else if (isToddler) {
              base = m.base.toddler;
            }
          }

          if (sex === "female" && isAdult) {
            if (repro === "pregnant") {
              base += m.pregnancyDelta || 0;
              if (m.pregnancyDelta)
                note = (note ? note + " " : "") + "Adjusted for pregnancy.";
            } else if (repro === "lactating") {
              base += m.lactationDelta || 0;
              if (m.lactationDelta)
                note = (note ? note + " " : "") + "Adjusted for breastfeeding.";
            }
          }

          targets[m.id] = { value: base, note, upper: m.upper };
        });

        return targets;
      }

      function renderTargets() {
        const tbody = document.getElementById("targets-body");
        const targets = computeTargets();
        tbody.innerHTML = "";
        MINERALS.forEach((m) => {
          const t = targets[m.id];
          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="px-3 py-2">${m.name}</td>
        <td class="px-3 py-2 text-right">${
          t.value ? t.value.toLocaleString() : "—"
        }</td>
        <td class="px-3 py-2">${m.unit}</td>
        <td class="px-3 py-2 text-xs text-gray-600">${t.note || ""}</td>
      `;
          tbody.appendChild(tr);
        });
      }

      function renderIntakeInputs() {
        const tbody = document.getElementById("intake-body");
        tbody.innerHTML = "";
        MINERALS.forEach((m) => {
          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="px-3 py-2">${m.name}</td>
        <td class="px-3 py-2 text-right">
          <input id="intake-${m.id}" type="number" min="0" step="0.1"
                 class="w-28 border rounded-md px-2 py-1 text-right text-sm" />
        </td>
        <td class="px-3 py-2">${m.unit}</td>
      `;
          tbody.appendChild(tr);
        });
      }

      function applyPreset(type) {
        const targets = computeTargets();
        MINERALS.forEach((m) => {
          const input = document.getElementById("intake-" + m.id);
          if (!input) return;
          const t = targets[m.id]?.value || 0;
          let val = t;
          if (type === "balanced") {
            val = t * 0.95;
          } else if (type === "low-dairy") {
            if (m.id === "calcium") val = t * 0.6;
            else if (m.id === "iron" || m.id === "zinc") val = t * 0.7;
            else val = t * 0.9;
          }
          input.value = Math.round(val * 10) / 10;
        });
      }

      function classifyStatus(percent, mineral, intake, target, upper) {
        let status = "Probably adequate";
        let color = "bg-green-100 text-green-800";
        let detail = "";

        if (!isFinite(percent) || target <= 0 || intake === 0) {
          return {
            status: "No data",
            color: "bg-gray-100 text-gray-700",
            detail: "Enter an intake value.",
          };
        }

        if (percent < 80) {
          status = "Likely low";
          color = "bg-red-100 text-red-800";
          detail = "Below typical reference intake.";
        } else if (percent > 120) {
          status = "High";
          color = "bg-amber-100 text-amber-800";
          detail = "Above typical reference intake.";
        }

        if (upper && intake > upper) {
          status = "Above upper limit";
          color = "bg-red-200 text-red-900";
          detail = "Exceeds common tolerable upper intake level.";
        }

        if (mineral === "sodium" && intake > 2300) {
          detail = "Above recommended sodium limit for most adults.";
        }

        return { status, color, detail };
      }

      function calculateResults() {
        const targets = computeTargets();
        const tbody = document.getElementById("results-body");
        tbody.innerHTML = "";

        let lowList = [];
        let highList = [];

        MINERALS.forEach((m) => {
          const intake = parseFloat(
            document.getElementById("intake-" + m.id)?.value || "0"
          );
          const target = targets[m.id]?.value || 0;
          const upper = targets[m.id]?.upper || null;
          const percent = target > 0 ? (intake / target) * 100 : NaN;
          const cls = classifyStatus(percent, m.id, intake, target, upper);

          if (cls.status === "Likely low") lowList.push(m.name);
          if (cls.status === "High" || cls.status === "Above upper limit")
            highList.push(m.name);

          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="px-3 py-2">${m.name}</td>
        <td class="px-3 py-2 text-right">${
          intake ? intake.toLocaleString() : "—"
        } ${m.unit}</td>
        <td class="px-3 py-2 text-right">${
          target ? target.toLocaleString() : "—"
        } ${m.unit}</td>
        <td class="px-3 py-2 text-right">${
          isFinite(percent) ? percent.toFixed(0) + "%" : "—"
        }</td>
        <td class="px-3 py-2">
          <span class="badge ${cls.color}">${cls.status}</span>
          <span class="block text-xs text-gray-600">${cls.detail}</span>
        </td>
      `;
          tbody.appendChild(tr);
        });

        const summary = document.getElementById("summary-text");
        if (!lowList.length && !highList.length) {
          summary.textContent =
            "Your mineral intakes appear to be within typical reference ranges for all minerals entered. This does not replace professional advice or lab testing.";
        } else {
          let parts = [];
          if (lowList.length) {
            parts.push("Possible low intake for: " + lowList.join(", ") + ".");
          }
          if (highList.length) {
            parts.push(
              "High or above-upper-limit intake for: " +
                highList.join(", ") +
                "."
            );
          }
          parts.push(
            "Consider reviewing your diet and discussing these results with your doctor or a registered dietitian."
          );
          summary.textContent = parts.join(" ");
        }
      }

      document.addEventListener("DOMContentLoaded", () => {
        const femaleExtra = document.getElementById("female-extra-fields");
        const sexSelect = document.getElementById("sex");
        sexSelect.addEventListener("change", () => {
          if (sexSelect.value === "female") {
            femaleExtra.classList.remove("hidden");
          } else {
            femaleExtra.classList.add("hidden");
          }
          renderTargets();
        });

        document.getElementById("age").addEventListener("input", renderTargets);
        document
          .getElementById("age-unit")
          .addEventListener("change", renderTargets);
        document
          .getElementById("repro-status")
          .addEventListener("change", renderTargets);

        document
          .getElementById("preset-balanced")
          .addEventListener("click", () => applyPreset("balanced"));
        document
          .getElementById("preset-low-dairy")
          .addEventListener("click", () => applyPreset("low-dairy"));
        document
          .getElementById("calculate-btn")
          .addEventListener("click", calculateResults);

        renderTargets();
        renderIntakeInputs();
      });
    </script>

