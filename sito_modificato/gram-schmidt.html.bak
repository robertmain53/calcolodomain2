    <script>
      // Utility: parse number or simple fraction
      function parseNumber(str) {
        str = String(str).trim();
        if (!str) return 0;
        if (str.includes("/")) {
          const parts = str.split("/");
          if (parts.length === 2) {
            const num = parseFloat(parts[0]);
            const den = parseFloat(parts[1]);
            if (!isNaN(num) && !isNaN(den) && den !== 0) return num / den;
          }
        }
        const v = parseFloat(str);
        if (isNaN(v)) throw new Error('Invalid number: "' + str + '"');
        return v;
      }

      function formatNumber(x) {
        if (!isFinite(x)) return x.toString();
        if (Math.abs(x) < 1e-10) x = 0;
        if (Math.abs(x) >= 1e6 || (Math.abs(x) > 0 && Math.abs(x) < 1e-4)) {
          return x.toExponential(4);
        }
        return Number(x.toFixed(6)).toString();
      }

      function createMatrixInputs(n, k) {
        const container = document.getElementById("matrixContainer");
        container.innerHTML = "";
        const table = document.createElement("table");
        table.className = "min-w-max text-xs md:text-sm";
        const thead = document.createElement("thead");
        const headRow = document.createElement("tr");
        headRow.appendChild(document.createElement("th"));
        for (let j = 0; j < k; j++) {
          const th = document.createElement("th");
          th.className = "px-2 pb-1 text-center font-semibold";
          th.textContent = "v" + (j + 1);
          headRow.appendChild(th);
        }
        thead.appendChild(headRow);
        table.appendChild(thead);

        const tbody = document.createElement("tbody");
        for (let i = 0; i < n; i++) {
          const tr = document.createElement("tr");
          const labelTd = document.createElement("td");
          labelTd.className = "pr-2 text-right text-gray-500";
          labelTd.textContent = "row " + (i + 1);
          tr.appendChild(labelTd);
          for (let j = 0; j < k; j++) {
            const td = document.createElement("td");
            td.className = "px-1 py-0.5";
            const input = document.createElement("input");
            input.type = "text";
            input.inputMode = "decimal";
            input.className =
              "w-16 md:w-20 border border-gray-300 rounded px-1 py-0.5 text-xs md:text-sm matrix-input";
            input.dataset.row = i;
            input.dataset.col = j;
            td.appendChild(input);
            tr.appendChild(td);
          }
          tbody.appendChild(tr);
        }
        table.appendChild(tbody);
        container.appendChild(table);
      }

      function readVectors() {
        const n = parseInt(document.getElementById("dimension").value, 10);
        const k = parseInt(document.getElementById("numVectors").value, 10);
        const vectors = [];
        for (let j = 0; j < k; j++) {
          const v = [];
          for (let i = 0; i < n; i++) {
            const input = document.querySelector(
              'input[data-row="' + i + '"][data-col="' + j + '"]'
            );
            if (!input) throw new Error("Internal error reading matrix.");
            v.push(parseNumber(input.value || "0"));
          }
          vectors.push(v);
        }
        return { n, k, vectors };
      }

      function dot(a, b) {
        let s = 0;
        for (let i = 0; i < a.length; i++) s += a[i] * b[i];
        return s;
      }

      function norm(a) {
        return Math.sqrt(dot(a, a));
      }

      function scalarMul(a, c) {
        return a.map((x) => x * c);
      }

      function add(a, b) {
        return a.map((x, i) => x + b[i]);
      }

      function sub(a, b) {
        return a.map((x, i) => x - b[i]);
      }

      function cloneVec(a) {
        return a.slice();
      }

      function gramSchmidt(vectors) {
        const k = vectors.length;
        const n = vectors[0].length;
        const u = [];
        const e = [];
        const steps = [];
        const eps = 1e-10;

        for (let j = 0; j < k; j++) {
          const vj = cloneVec(vectors[j]);
          let uj = cloneVec(vj);
          const projTerms = [];
          const projVectors = [];

          for (let i = 0; i < j; i++) {
            const ui = u[i];
            const num = dot(vj, ui);
            const den = dot(ui, ui);
            if (Math.abs(den) < eps) {
              throw new Error(
                "Zero vector encountered at step " +
                  (i + 1) +
                  ". Input vectors are linearly dependent."
              );
            }
            const coeff = num / den;
            const proj = scalarMul(ui, coeff);
            projTerms.push({ i: i + 1, num, den, coeff });
            projVectors.push(proj);
            uj = sub(uj, proj);
          }

          const ujNorm = norm(uj);
          if (ujNorm < eps) {
            throw new Error(
              "Vector u_" +
                (j + 1) +
                " is (numerically) zero. The input vectors are linearly dependent or nearly so."
            );
          }

          const ej = scalarMul(uj, 1 / ujNorm);
          u.push(uj);
          e.push(ej);

          steps.push({
            j: j + 1,
            vj,
            uj,
            ej,
            ujNorm,
            projTerms,
            projVectors,
          });
        }

        return { u, e, steps };
      }

      function renderVectorList(containerId, vecs, prefix) {
        const n = vecs[0].length;
        const k = vecs.length;
        let html = '<table class="min-w-max"><thead><tr><th class="pr-2"></th>';
        for (let j = 0; j < k; j++) {
          html +=
            '<th class="px-2 text-center">' + prefix + "_" + (j + 1) + "</th>";
        }
        html += "</tr></thead><tbody>";
        for (let i = 0; i < n; i++) {
          html +=
            '<tr><td class="pr-2 text-right text-gray-500">row ' +
            (i + 1) +
            "</td>";
          for (let j = 0; j < k; j++) {
            html +=
              '<td class="px-2 text-right">' +
              formatNumber(vecs[j][i]) +
              "</td>";
          }
          html += "</tr>";
        }
        html += "</tbody></table>";
        document.getElementById(containerId).innerHTML = html;
      }

      function renderSteps(steps) {
        const container = document.getElementById("stepsContainer");
        container.innerHTML = "";
        steps.forEach((step) => {
          const div = document.createElement("div");
          div.className = "border border-gray-200 rounded-md p-3 bg-gray-50";
          let html =
            '<div class="font-semibold mb-1">Step ' + step.j + ":</div>";
          html += '<div class="text-xs md:text-sm space-y-1">';
          html +=
            '<div>Input vector: <span class="font-mono">v_' +
            step.j +
            " = [" +
            step.vj.map(formatNumber).join(", ") +
            "]</span></div>";

          if (step.projTerms.length === 0) {
            html +=
              "<div>No projections to subtract: u_" +
              step.j +
              " = v_" +
              step.j +
              ".</div>";
          } else {
            html +=
              "<div>Compute projections onto previous u<sub>i</sub>:</div>";
            step.projTerms.forEach((t, idx) => {
              html +=
                '<div class="ml-4">proj<sub>u_' +
                t.i +
                "</sub>(v_" +
                step.j +
                ") = " +
                "(" +
                formatNumber(t.num) +
                " / " +
                formatNumber(t.den) +
                ") · u_" +
                t.i +
                " = " +
                formatNumber(t.coeff) +
                " · u_" +
                t.i +
                "</div>";
            });
            html += "<div>Then subtract all projections:</div>";
            html +=
              '<div class="ml-4">u_' +
              step.j +
              " = v_" +
              step.j +
              " − " +
              step.projTerms
                .map((t) => "proj<sub>u_" + t.i + "</sub>(v_" + step.j + ")")
                .join(" − ") +
              "</div>";
          }

          html +=
            '<div class="mt-1">Resulting orthogonal vector: ' +
            '<span class="font-mono">u_' +
            step.j +
            " = [" +
            step.uj.map(formatNumber).join(", ") +
            "]</span></div>";

          html +=
            "<div>Norm: ||u_" +
            step.j +
            "|| = " +
            formatNumber(step.ujNorm) +
            "</div>";
          html +=
            '<div>Normalized vector: <span class="font-mono">e_' +
            step.j +
            " = u_" +
            step.j +
            " / ||u_" +
            step.j +
            "|| = [" +
            step.ej.map(formatNumber).join(", ") +
            "]</span></div>";

          html += "</div>";
          div.innerHTML = html;
          container.appendChild(div);
        });
      }

      function showError(msg) {
        const box = document.getElementById("errorBox");
        box.textContent = msg;
        box.classList.remove("hidden");
      }

      function clearError() {
        const box = document.getElementById("errorBox");
        box.textContent = "";
        box.classList.add("hidden");
      }

      document.addEventListener("DOMContentLoaded", () => {
        const dimSelect = document.getElementById("dimension");
        const numSelect = document.getElementById("numVectors");
        createMatrixInputs(
          parseInt(dimSelect.value, 10),
          parseInt(numSelect.value, 10)
        );

        document
          .getElementById("updateMatrix")
          .addEventListener("click", () => {
            createMatrixInputs(
              parseInt(dimSelect.value, 10),
              parseInt(numSelect.value, 10)
            );
            clearError();
            document.getElementById("resultsSection").classList.add("hidden");
          });

        document.getElementById("loadExample").addEventListener("click", () => {
          dimSelect.value = "3";
          numSelect.value = "3";
          createMatrixInputs(3, 3);
          const example = [
            [1, 1, 0],
            [1, 0, 1],
            [0, 1, 1],
          ];
          for (let j = 0; j < 3; j++) {
            for (let i = 0; i < 3; i++) {
              const input = document.querySelector(
                'input[data-row="' + i + '"][data-col="' + j + '"]'
              );
              if (input) input.value = example[i][j];
            }
          }
          clearError();
          document.getElementById("resultsSection").classList.add("hidden");
        });

        document.getElementById("clearAll").addEventListener("click", () => {
          document
            .querySelectorAll("#matrixContainer input")
            .forEach((inp) => (inp.value = ""));
          document.getElementById("resultsSection").classList.add("hidden");
          clearError();
        });

        document.getElementById("runGS").addEventListener("click", () => {
          clearError();
          try {
            const { n, k, vectors } = readVectors();
            if (k > n) {
              showError(
                "You entered more vectors (" +
                  k +
                  ") than the dimension (" +
                  n +
                  "). They must be linearly dependent."
              );
              document.getElementById("resultsSection").classList.add("hidden");
              return;
            }
            const allZero = vectors.every((v) =>
              v.every((x) => Math.abs(x) < 1e-12)
            );
            if (allZero) {
              showError(
                "All vectors are zero. Please enter at least one non‑zero vector."
              );
              document.getElementById("resultsSection").classList.add("hidden");
              return;
            }
            const { u, e, steps } = gramSchmidt(vectors);
            renderVectorList("orthogonalVectors", u, "u");
            renderVectorList("orthonormalVectors", e, "e");
            renderSteps(steps);
            document
              .getElementById("resultsSection")
              .classList.remove("hidden");
            if (window.MathJax && window.MathJax.typesetPromise) {
              window.MathJax.typesetPromise();
            }
          } catch (err) {
            showError(err.message || String(err));
            document.getElementById("resultsSection").classList.add("hidden");
          }
        });
      });
    </script>

    <script>
      window.MathJax = {
        tex: {
