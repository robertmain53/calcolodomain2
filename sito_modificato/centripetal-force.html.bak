    <script>
      // Mode switching
      const modeButtons = document.querySelectorAll(".mode-btn");
      const fieldGroups = {
        v: document.querySelector('[data-field="v"]'),
        omega: document.querySelector('[data-field="omega"]'),
        f: document.querySelector('[data-field="f"]'),
      };
      let currentMode = "v";

      modeButtons.forEach((btn) => {
        btn.addEventListener("click", () => {
          currentMode = btn.dataset.mode;
          modeButtons.forEach((b) => b.classList.remove("toggle-active"));
          btn.classList.add("toggle-active");

          // Toggle fields
          Object.keys(fieldGroups).forEach((key) => {
            if (key === currentMode) {
              fieldGroups[key].classList.remove("hidden");
            } else {
              fieldGroups[key].classList.add("hidden");
            }
          });
        });
      });

      function toKg(mass, unit) {
        if (unit === "kg") return mass;
        if (unit === "g") return mass / 1000;
        if (unit === "lb") return mass * 0.45359237;
        return mass;
      }

      function toMeters(length, unit) {
        switch (unit) {
          case "m":
            return length;
          case "cm":
            return length / 100;
          case "mm":
            return length / 1000;
          case "ft":
            return length * 0.3048;
          default:
            return length;
        }
      }

      function toMps(speed, unit) {
        switch (unit) {
          case "mps":
            return speed;
          case "kmph":
            return (speed * 1000) / 3600;
          case "mph":
            return (speed * 1609.344) / 3600;
          default:
            return speed;
        }
      }

      function toRadPerSec(value, unit) {
        if (unit === "radps") return value;
        if (unit === "rpm") return (value * 2 * Math.PI) / 60;
        return value;
      }

      function toHz(value, unit) {
        if (unit === "hz") return value;
        if (unit === "rpm") return value / 60;
        return value;
      }

      const calcBtn = document.getElementById("calc-btn");
      const resetBtn = document.getElementById("reset-btn");
      const resultsBox = document.getElementById("results");
      const errorBox = document.getElementById("error-box");
      const errorText = errorBox.querySelector("p");
      const resultForce = document.getElementById("result-force");
      const resultAccel = document.getElementById("result-accel");
      const resultNote = document.getElementById("result-note");

      calcBtn.addEventListener("click", () => {
        errorBox.classList.add("hidden");
        resultsBox.classList.add("hidden");

        const massVal = parseFloat(document.getElementById("mass").value);
        const massUnit = document.getElementById("mass-unit").value;
        const radiusVal = parseFloat(document.getElementById("radius").value);
        const radiusUnit = document.getElementById("radius-unit").value;

        if (isNaN(massVal) || massVal <= 0) {
          errorText.textContent = "Please enter a positive mass.";
          errorBox.classList.remove("hidden");
          return;
        }
        if (isNaN(radiusVal) || radiusVal <= 0) {
          errorText.textContent = "Please enter a positive radius.";
          errorBox.classList.remove("hidden");
          return;
        }

        const m = toKg(massVal, massUnit);
        const r = toMeters(radiusVal, radiusUnit);

        let Fc = null;
        let ac = null;
        let note = "";

        if (currentMode === "v") {
          const speedVal = parseFloat(document.getElementById("speed").value);
          const speedUnit = document.getElementById("speed-unit").value;
          if (isNaN(speedVal) || speedVal < 0) {
            errorText.textContent = "Please enter a non-negative speed.";
            errorBox.classList.remove("hidden");
            return;
          }
          const v = toMps(speedVal, speedUnit);
          ac = (v * v) / r;
          Fc = m * ac;
          note = `Computed using Fc = m·v²/r with v = ${v.toFixed(
            3
          )} m/s and r = ${r.toFixed(3)} m.`;
        } else if (currentMode === "omega") {
          const omegaVal = parseFloat(document.getElementById("omega").value);
          const omegaUnit = document.getElementById("omega-unit").value;
          if (isNaN(omegaVal) || omegaVal < 0) {
            errorText.textContent =
              "Please enter a non-negative angular speed.";
            errorBox.classList.remove("hidden");
            return;
          }
          const omega = toRadPerSec(omegaVal, omegaUnit);
          ac = omega * omega * r;
          Fc = m * ac;
          note = `Computed using Fc = m·ω²·r with ω = ${omega.toFixed(
            3
          )} rad/s and r = ${r.toFixed(3)} m.`;
        } else if (currentMode === "f") {
          const fVal = parseFloat(document.getElementById("frequency").value);
          const fUnit = document.getElementById("frequency-unit").value;
          if (isNaN(fVal) || fVal < 0) {
            errorText.textContent = "Please enter a non-negative frequency.";
            errorBox.classList.remove("hidden");
            return;
          }
          const f = toHz(fVal, fUnit);
          ac = 4 * Math.PI * Math.PI * r * f * f;
          Fc = m * ac;
          note = `Computed using Fc = 4π²mr f² with f = ${f.toFixed(
            3
          )} Hz and r = ${r.toFixed(3)} m.`;
        }

        resultForce.textContent = `${Fc.toFixed(3)} N`;
        resultAccel.textContent = `${ac.toFixed(3)} m/s²`;
        resultNote.textContent = note;
        resultsBox.classList.remove("hidden");
      });

      resetBtn.addEventListener("click", () => {
        document.getElementById("centripetal-form").reset();
        resultsBox.classList.add("hidden");
        errorBox.classList.add("hidden");
        resultForce.textContent = "–";
        resultAccel.textContent = "–";
        resultNote.textContent = "";
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
