    <script>
      (function () {
        const yearSelect = document.getElementById("sst-year");
        const roleSelect = document.getElementById("sst-role");
        const wagesInput = document.getElementById("sst-wages");
        const multiJobsSelect = document.getElementById("sst-multiple-jobs");
        const priorWagesWrapper = document.getElementById(
          "sst-prior-wages-wrapper"
        );
        const priorWagesInput = document.getElementById("sst-prior-wages");
        const customYearWrapper = document.getElementById(
          "sst-custom-year-wrapper"
        );
        const customWageBaseInput = document.getElementById(
          "sst-custom-wage-base"
        );
        const customEmpRateInput = document.getElementById(
          "sst-custom-emp-rate"
        );
        const customEmplRateInput = document.getElementById(
          "sst-custom-empl-rate"
        );

        const calcBtn = document.getElementById("sst-calc-btn");
        const resetBtn = document.getElementById("sst-reset-btn");
        const errorEl = document.getElementById("sst-error");

        const taxableEl = document.getElementById("sst-taxable-earnings");
        const wageBaseNoteEl = document.getElementById("sst-wage-base-note");
        const empLabelEl = document.getElementById("sst-employee-label");
        const emplLabelEl = document.getElementById("sst-employer-label");
        const empTaxEl = document.getElementById("sst-employee-tax");
        const emplTaxEl = document.getElementById("sst-employer-tax");
        const totalLabelEl = document.getElementById("sst-total-label");
        const totalTaxEl = document.getElementById("sst-total-tax");
        const selfNoteEl = document.getElementById("sst-self-employed-note");

        // Default wage base presets (can be updated when SSA publishes new values)
        const wageBaseByYear = {
          2024: 168600,
          2025: 174000, // example projected; user can override via custom
        };

        const defaultRate = 0.062;

        function formatCurrency(value) {
          return value.toLocaleString("en-US", {
            style: "currency",
            currency: "USD",
            maximumFractionDigits: 2,
          });
        }

        function updateYearUI() {
          const year = yearSelect.value;
          if (year === "custom") {
            customYearWrapper.classList.remove("hidden");
          } else {
            customYearWrapper.classList.add("hidden");
          }
        }

        function updateMultiJobsUI() {
          if (multiJobsSelect.value === "yes") {
            priorWagesWrapper.classList.remove("hidden");
          } else {
            priorWagesWrapper.classList.add("hidden");
          }
        }

        function updateRoleLabels() {
          const role = roleSelect.value;
          selfNoteEl.hidden = true;

          if (role === "employee") {
            empLabelEl.textContent = "Employee share (6.2%)";
            emplLabelEl.textContent = "Employer share (6.2%)";
            emplTaxEl.parentElement.classList.remove("hidden");
            totalLabelEl.textContent =
              "Total Social Security tax (employee + employer)";
          } else if (role === "employer") {
            empLabelEl.textContent = "Employee share (6.2%)";
            emplLabelEl.textContent = "Employer share (6.2%)";
            emplTaxEl.parentElement.classList.remove("hidden");
            totalLabelEl.textContent =
              "Total Social Security tax (employee + employer)";
          } else if (role === "both") {
            empLabelEl.textContent = "Employee share (6.2%)";
            emplLabelEl.textContent = "Employer share (6.2%)";
            emplTaxEl.parentElement.classList.remove("hidden");
            totalLabelEl.textContent =
              "Total Social Security tax (payroll cost)";
          } else if (role === "self") {
            empLabelEl.textContent = "Self‑employed (employee‑equivalent 6.2%)";
            emplLabelEl.textContent =
              "Self‑employed (employer‑equivalent 6.2%)";
            emplTaxEl.parentElement.classList.remove("hidden");
            totalLabelEl.textContent =
              "Total Social Security tax (self‑employment portion)";
            selfNoteEl.hidden = false;
          }
        }

        function getParameters() {
          const year = yearSelect.value;
          let wageBase;
          let empRate;
          let emplRate;

          if (year === "custom") {
            wageBase = Number(customWageBaseInput.value) || 0;
            empRate = (Number(customEmpRateInput.value) || 0) / 100;
            emplRate = (Number(customEmplRateInput.value) || 0) / 100;
          } else {
            wageBase = wageBaseByYear[year] || wageBaseByYear["2024"];
            empRate = defaultRate;
            emplRate = defaultRate;
          }
          return { wageBase, empRate, emplRate, year };
        }

        function calculate() {
          errorEl.textContent = "";
          const wages = Number(wagesInput.value);
          const multiJobs = multiJobsSelect.value === "yes";
          const priorWages = multiJobs ? Number(priorWagesInput.value) : 0;

          if (isNaN(wages) || wages < 0) {
            errorEl.textContent = "Enter a valid non‑negative income amount.";
            return;
          }
          if (multiJobs && (isNaN(priorWages) || priorWages < 0)) {
            errorEl.textContent = "Enter valid prior covered wages.";
            return;
          }

          const { wageBase, empRate, emplRate, year } = getParameters();
          if (wageBase <= 0) {
            errorEl.textContent = "Enter a valid wage base limit.";
            return;
          }

          const remainingBase = Math.max(0, wageBase - priorWages);
          const taxable = Math.min(Math.max(0, wages), remainingBase);

          const empTax = taxable * empRate;
          const emplTax = taxable * emplRate;
          const totalTax = empTax + emplTax;

          taxableEl.textContent = formatCurrency(taxable);

          let note = `Wage base limit for ${
            year === "custom" ? "custom year" : year
          }: ${formatCurrency(wageBase)}.`;
          if (multiJobs) {
            note += ` Prior covered wages: ${formatCurrency(
              priorWages
            )}. Remaining wage base: ${formatCurrency(remainingBase)}.`;
          }
          wageBaseNoteEl.textContent = note;

          empTaxEl.textContent = formatCurrency(empTax);
          emplTaxEl.textContent = formatCurrency(emplTax);
          totalTaxEl.textContent = formatCurrency(totalTax);
        }

        function reset() {
          yearSelect.value = "2024";
          roleSelect.value = "employee";
          wagesInput.value = 60000;
          multiJobsSelect.value = "no";
          priorWagesInput.value = 0;
          customWageBaseInput.value = 168600;
          customEmpRateInput.value = 6.2;
          customEmplRateInput.value = 6.2;
          updateYearUI();
          updateMultiJobsUI();
          updateRoleLabels();
          taxableEl.textContent = "$0";
          wageBaseNoteEl.textContent = "";
          empTaxEl.textContent = "$0";
          emplTaxEl.textContent = "$0";
          totalTaxEl.textContent = "$0";
          selfNoteEl.hidden = true;
          errorEl.textContent = "";
        }

        yearSelect.addEventListener("change", () => {
          updateYearUI();
          calculate();
        });
        roleSelect.addEventListener("change", () => {
          updateRoleLabels();
          calculate();
        });
        multiJobsSelect.addEventListener("change", () => {
          updateMultiJobsUI();
          calculate();
        });

        [
          wagesInput,
          priorWagesInput,
          customWageBaseInput,
          customEmpRateInput,
          customEmplRateInput,
        ].forEach((el) => {
          el.addEventListener("input", () => {
            errorEl.textContent = "";
          });
        });

        calcBtn.addEventListener("click", calculate);
        resetBtn.addEventListener("click", (e) => {
          e.preventDefault();
          reset();
        });

        // Initialize UI
        updateYearUI();
        updateMultiJobsUI();
        updateRoleLabels();
        calculate();
      })();
    </script>
  
