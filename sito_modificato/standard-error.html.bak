<script="">
        function calculateStandardError() {
            const sampleSize = document.getElementById('sampleSize').value;
            const sampleStandardDeviation = document.getElementById('sampleStandardDeviation').value;
            let standardError = 0;

            if (sampleSize > 0 && sampleStandardDeviation > 0) {
                standardError = (sampleStandardDeviation / Math.sqrt(sampleSize)).toFixed(2);
            }

            document.getElementById('standardErrorValue').innerText = standardError;
        }
        
        // Inline validation
        document.getElementById('sampleSize').addEventListener('blur', function() {
            const sampleSize = this.value;
            const errorElement = document.getElementById('sampleSizeError');
            if (sampleSize <= 0) {
                errorElement.innerText = 'Sample size must be greater than zero.';
            } else {
                errorElement.innerText = '';
            }
        });

        document.getElementById('sampleStandardDeviation').addEventListener('blur', function() {
            const sampleStandardDeviation = this.value;
            const errorElement = document.getElementById('sampleStandardDeviationError');
            if (sampleStandardDeviation <= 0) {
                errorElement.innerText = 'Standard deviation must be greater than zero.';
            } else {
                errorElement.innerText = '';
            }
        });
    </script>


