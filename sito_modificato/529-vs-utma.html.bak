    <script>
      function fmtMoney(value) {
        if (!isFinite(value)) return "-";
        const abs = Math.abs(value);
        const sign = value < 0 ? "-" : "";
        if (abs >= 1e6) return sign + "$" + (abs / 1e6).toFixed(2) + "M";
        if (abs >= 1e3) return sign + "$" + (abs / 1e3).toFixed(1) + "k";
        return sign + "$" + abs.toFixed(0);
      }

      function fmtPercent(value) {
        if (!isFinite(value)) return "-";
        return value.toFixed(1) + "%";
      }

      function calculateComparison() {
        const errorEl = document.getElementById("calc-error");
        errorEl.classList.add("hidden");
        errorEl.textContent = "";

        const childAge = parseFloat(document.getElementById("child-age").value);
        const collegeAge = parseFloat(
          document.getElementById("college-age").value
        );
        const initial = parseFloat(
          document.getElementById("initial-contribution").value
        );
        const annual = parseFloat(
          document.getElementById("annual-contribution").value
        );
        const r =
          parseFloat(document.getElementById("return-rate").value) / 100;
        const utmaTaxRate =
          parseFloat(document.getElementById("utma-tax-rate").value) / 100;
        const penaltyRate =
          parseFloat(document.getElementById("nonqualified-penalty").value) /
          100;
        const incomeTaxRate =
          parseFloat(document.getElementById("income-tax-rate").value) / 100;
        const qualifiedUsage = document.getElementById("qualified-usage").value;
        const stateDeductionRate =
          parseFloat(document.getElementById("state-deduction-rate").value) /
          100;

        if (isNaN(childAge) || isNaN(collegeAge) || collegeAge <= childAge) {
          errorEl.textContent =
            "Please enter a valid child age and college age (college age must be greater).";
          errorEl.classList.remove("hidden");
          return;
        }
        if (r < 0) {
          errorEl.textContent =
            "Expected annual return must be zero or positive.";
          errorEl.classList.remove("hidden");
          return;
        }

        const years = Math.max(0, Math.round(collegeAge - childAge));

        // 529 future value (tax-deferred)
        let fv529 = 0;
        let contrib529 = 0;
        if (years === 0) {
          fv529 = initial;
          contrib529 = initial;
        } else {
          fv529 = initial * Math.pow(r + 1, years);
          contrib529 = initial;
          for (let i = 1; i <= years; i++) {
            fv529 += annual * Math.pow(1 + r, years - i);
            contrib529 += annual;
          }
        }

        // UTMA with annual tax drag
        const rAfterTax = r * (1 - utmaTaxRate);
        let fvUtma = 0;
        let contribUtma = 0;
        if (years === 0) {
          fvUtma = initial;
          contribUtma = initial;
        } else if (rAfterTax === 0) {
          fvUtma = initial + annual * years;
          contribUtma = initial + annual * years;
        } else {
          fvUtma = initial * Math.pow(1 + rAfterTax, years);
          contribUtma = initial;
          for (let i = 1; i <= years; i++) {
            fvUtma += annual * Math.pow(1 + rAfterTax, years - i);
            contribUtma += annual;
          }
        }

        // Approximate total UTMA tax paid vs pre-tax growth
        let fvUtmaPretax = 0;
        if (years === 0) {
          fvUtmaPretax = initial;
        } else if (r === 0) {
          fvUtmaPretax = initial + annual * years;
        } else {
          fvUtmaPretax = initial * Math.pow(1 + r, years);
          for (let i = 1; i <= years; i++) {
            fvUtmaPretax += annual * Math.pow(1 + r, years - i);
          }
        }
        const utmaTaxPaid = Math.max(0, fvUtmaPretax - fvUtma);

        // 529 taxes/penalties depending on usage
        const earnings529 = Math.max(0, fv529 - contrib529);
        let taxableEarnings = 0;
        if (qualifiedUsage === "qualified") {
          taxableEarnings = 0;
        } else if (qualifiedUsage === "half-qualified") {
          taxableEarnings = earnings529 * 0.5;
        } else {
          taxableEarnings = earnings529;
        }
        const taxOn529 = taxableEarnings * incomeTaxRate;
        const penaltyOn529 = taxableEarnings * penaltyRate;
        const total529TaxPenalty = taxOn529 + penaltyOn529;
        const net529 = fv529 - total529TaxPenalty;
        const netUtma = fvUtma; // already after tax drag

        const taxDrag529 =
          earnings529 === 0 ? 0 : (total529TaxPenalty / earnings529) * 100;
        const taxDragUtma =
          fvUtmaPretax - fvUtma <= 0
            ? 0
            : ((fvUtmaPretax - fvUtma) / (fvUtmaPretax - contribUtma)) * 100;

        // Financial aid impact
        const fa529 = net529 * 0.0564;
        const faUtma = netUtma * 0.2;
        const faDiff = faUtma - fa529;

        // State benefit estimate
        const totalContrib = contrib529; // same as UTMA in this model
        const stateBenefit = totalContrib * stateDeductionRate;
        const stateBenefitExtra = stateBenefit; // treat as equivalent extra contributions

        // Populate UI
        document.getElementById("529-contrib").textContent =
          fmtMoney(contrib529);
        document.getElementById("529-gross").textContent = fmtMoney(fv529);
        document.getElementById("529-tax").textContent = fmtMoney(
          -total529TaxPenalty
        );
        document.getElementById("529-net").textContent = fmtMoney(net529);
        document.getElementById("529-tax-drag").textContent =
          fmtPercent(taxDrag529);

        document.getElementById("utma-contrib").textContent =
          fmtMoney(contribUtma);
        document.getElementById("utma-gross").textContent =
          fmtMoney(fvUtmaPretax);
        document.getElementById("utma-tax").textContent = fmtMoney(
          -utmaTaxPaid
        );
        document.getElementById("utma-net").textContent = fmtMoney(netUtma);
        document.getElementById("utma-tax-drag").textContent =
          fmtPercent(taxDragUtma);

        document.getElementById("fa-529").textContent = fmtMoney(fa529);
        document.getElementById("fa-utma").textContent = fmtMoney(faUtma);
        document.getElementById("fa-diff").textContent =
          (faDiff > 0 ? "+" : "") + fmtMoney(faDiff) + " / year";

        document.getElementById("state-benefit").textContent =
          fmtMoney(stateBenefit);
        document.getElementById("state-benefit-extra").textContent =
          fmtMoney(stateBenefitExtra);

        // Badges and summary
        const badge529 = document.getElementById("badge-529");
        const badgeUtma = document.getElementById("badge-utma");
        badge529.classList.add("hidden");
        badgeUtma.classList.add("hidden");

        let summary = "";
        if (net529 > netUtma * 1.01) {
          badge529.classList.remove("hidden");
          summary += `Based on your inputs, the 529 plan is projected to end with about ${fmtMoney(
            net529 - netUtma
          )} more after tax than the UTMA/UGMA. `;
        } else if (netUtma > net529 * 1.01) {
          badgeUtma.classList.remove("hidden");
          summary += `Based on your inputs, the UTMA/UGMA is projected to end with about ${fmtMoney(
            netUtma - net529
          )} more after tax than the 529 plan. `;
        } else {
          summary +=
            "Based on your inputs, the 529 plan and UTMA/UGMA end up with very similar after‑tax values. ";
        }

        if (faDiff > 0) {
          summary += `However, the UTMA/UGMA could reduce need‑based aid eligibility by roughly ${fmtMoney(
            faDiff
          )} more per year than the 529, because it is treated as a student asset. `;
        }

        if (stateBenefit > 0) {
          summary += `Your estimated state 529 tax benefit adds about ${fmtMoney(
            stateBenefit
          )} in value over the contribution period. `;
        }

        summary +=
          "Remember that taxes, financial aid rules, and investment returns can change. Use these results as a planning guide, not a guarantee.";

        document.getElementById("summary-text").textContent = summary;
        document.getElementById("results-section").classList.remove("hidden");
      }

      document
        .getElementById("calculate-btn")
        .addEventListener("click", calculateComparison);

      document
        .getElementById("reset-btn")
        .addEventListener("click", function () {
          document.getElementById("child-age").value = 3;
          document.getElementById("college-age").value = 18;
          document.getElementById("initial-contribution").value = 10000;
          document.getElementById("annual-contribution").value = 3000;
          document.getElementById("return-rate").value = 7;
          document.getElementById("utma-tax-rate").value = 15;
          document.getElementById("nonqualified-penalty").value = 10;
          document.getElementById("income-tax-rate").value = 22;
          document.getElementById("qualified-usage").value = "qualified";
          document.getElementById("parent-marginal-rate").value = 24;
          document.getElementById("state-deduction-rate").value = 5;
          document.getElementById("calc-error").classList.add("hidden");
          document.getElementById("results-section").classList.add("hidden");
        });

      [
        "child-age",
        "college-age",
        "initial-contribution",
        "annual-contribution",
        "return-rate",
        "utma-tax-rate",
        "nonqualified-penalty",
