    <script>
      // Simple chart renderer without external libraries
      function drawTideChart(canvas, data, unit) {
        if (!canvas || !data || data.length === 0) return;
        const ctx = canvas.getContext("2d");
        const width = (canvas.width =
          canvas.clientWidth * (window.devicePixelRatio || 1));
        const height = (canvas.height =
          canvas.clientHeight * (window.devicePixelRatio || 1));
        ctx.clearRect(0, 0, width, height);

        const times = data.map((d) => d.t);
        const heights = data.map((d) => d.h);
        const minH = Math.min(...heights);
        const maxH = Math.max(...heights);
        const padding = { left: 50, right: 10, top: 20, bottom: 30 };

        const xScale = (t) => {
          const frac =
            (t - times[0]) / (times[times.length - 1] - times[0] || 1);
          return padding.left + frac * (width - padding.left - padding.right);
        };
        const yScale = (h) => {
          const frac = (h - minH) / (maxH - minH || 1);
          return (
            height -
            padding.bottom -
            frac * (height - padding.top - padding.bottom)
          );
        };

        ctx.strokeStyle = "#e5e7eb";
        ctx.lineWidth = 1;
        ctx.beginPath();
        for (let i = 0; i <= 4; i++) {
          const y =
            padding.top + (i * (height - padding.top - padding.bottom)) / 4;
          ctx.moveTo(padding.left, y);
          ctx.lineTo(width - padding.right, y);
        }
        ctx.stroke();

        ctx.strokeStyle = "#2563eb";
        ctx.lineWidth = 2;
        ctx.beginPath();
        data.forEach((pt, idx) => {
          const x = xScale(pt.t);
          const y = yScale(pt.h);
          if (idx === 0) ctx.moveTo(x, y);
          else ctx.lineTo(x, y);
        });
        ctx.stroke();

        ctx.fillStyle = "#111827";
        ctx.font = "12px Inter, system-ui, sans-serif";
        ctx.textAlign = "right";
        ctx.textBaseline = "middle";
        for (let i = 0; i <= 4; i++) {
          const h = minH + (i * (maxH - minH)) / 4;
          const y = yScale(h);
          ctx.fillText(h.toFixed(2) + " " + unit, padding.left - 6, y);
        }

        ctx.textAlign = "center";
        ctx.textBaseline = "top";
        const hours = [0, 6, 12, 18, 24];
        hours.forEach((hr) => {
          const t = times[0] + hr;
          const x = xScale(t);
          ctx.fillText(
            hr.toString().padStart(2, "0") + ":00",
            x,
            height - padding.bottom + 6
          );
        });
      }

      document.addEventListener("DOMContentLoaded", () => {
        const locationPreset = document.getElementById("locationPreset");
        const dateInput = document.getElementById("dateInput");
        const timeStepInput = document.getElementById("timeStep");
        const unitSelect = document.getElementById("unitSelect");
        const calculateBtn = document.getElementById("calculateBtn");
        const resetBtn = document.getElementById("resetBtn");
        const statusMessage = document.getElementById("statusMessage");
        const constituentTableBody = document.getElementById(
          "constituentTableBody"
        );
        const addConstituentBtn = document.getElementById("addConstituentBtn");
        const newName = document.getElementById("newName");
        const newPeriod = document.getElementById("newPeriod");
        const newAmp = document.getElementById("newAmp");
        const newPhase = document.getElementById("newPhase");
        const noDataMessage = document.getElementById("noDataMessage");
        const resultsContainer = document.getElementById("resultsContainer");
        const resultsTableBody = document.getElementById("resultsTableBody");
        const tideChart = document.getElementById("tideChart");
        const downloadCsvBtn = document.getElementById("downloadCsvBtn");

        const summaryDate = document.getElementById("summaryDate");
        const summaryHigh = document.getElementById("summaryHigh");
        const summaryHighTime = document.getElementById("summaryHighTime");
        const summaryLow = document.getElementById("summaryLow");
        const summaryLowTime = document.getElementById("summaryLowTime");
        const summaryRange = document.getElementById("summaryRange");

        let currentData = [];

        function todayISO() {
          const d = new Date();
          const off = d.getTimezoneOffset();
          const local = new Date(d.getTime() - off * 60000);
          return local.toISOString().slice(0, 10);
        }
        dateInput.value = todayISO();

        const presets = {
          example_bay: {
            Z0: 0,
            constituents: [
              { name: "M2", period: 12.42, amp: 1.2, phaseDeg: 30 },
              { name: "S2", period: 12.0, amp: 0.4, phaseDeg: 60 },
              { name: "K1", period: 23.93, amp: 0.2, phaseDeg: 10 },
              { name: "O1", period: 25.82, amp: 0.15, phaseDeg: 200 },
            ],
          },
          example_coast: {
            Z0: 0,
            constituents: [
              { name: "M2", period: 12.42, amp: 0.8, phaseDeg: 120 },
              { name: "S2", period: 12.0, amp: 0.3, phaseDeg: 200 },
              { name: "K1", period: 23.93, amp: 0.5, phaseDeg: 45 },
              { name: "O1", period: 25.82, amp: 0.4, phaseDeg: 300 },
            ],
          },
        };

        let Z0 = 0;

        function renderConstituents(constituents) {
          constituentTableBody.innerHTML = "";
          constituents.forEach((c, idx) => {
            const tr = document.createElement("tr");
            tr.innerHTML = `
          <td class="px-2 py-1 border-b">
            <input type="text" data-idx="${idx}" data-field="name" value="${c.name}" class="w-20 border border-gray-200 rounded px-1 py-0.5 text-xs">
          </td>
          <td class="px-2 py-1 border-b text-right">
            <input type="number" step="0.001" data-idx="${idx}" data-field="period" value="${c.period}" class="w-20 border border-gray-200 rounded px-1 py-0.5 text-xs text-right">
          </td>
          <td class="px-2 py-1 border-b text-right">
            <input type="number" step="0.01" data-idx="${idx}" data-field="amp" value="${c.amp}" class="w-20 border border-gray-200 rounded px-1 py-0.5 text-xs text-right">
          </td>
          <td class="px-2 py-1 border-b text-right">
            <input type="number" step="0.1" data-idx="${idx}" data-field="phaseDeg" value="${c.phaseDeg}" class="w-20 border border-gray-200 rounded px-1 py-0.5 text-xs text-right">
          </td>
          <td class="px-2 py-1 border-b text-center">
            <button type="button" data-remove="${idx}" class="text-xs text-red-600 hover:underline">X</button>
          </td>
        `;
            constituentTableBody.appendChild(tr);
          });
        }

        function getConstituentsFromTable() {
          const rows = constituentTableBody.querySelectorAll("tr");
          const list = [];
          rows.forEach((row, idx) => {
            const inputs = row.querySelectorAll("input");
            const obj = { name: "", period: 0, amp: 0, phaseDeg: 0 };
            inputs.forEach((inp) => {
              const field = inp.dataset.field;
              if (field === "name") obj.name = inp.value || `C${idx + 1}`;
              else obj[field] = parseFloat(inp.value) || 0;
            });
            if (obj.period > 0 && obj.amp !== 0) list.push(obj);
          });
          return list;
        }

        function loadPreset() {
          const val = locationPreset.value;
          if (val === "custom") {
            Z0 = 0;
            renderConstituents([
              { name: "M2", period: 12.42, amp: 1.0, phaseDeg: 0 },
              { name: "S2", period: 12.0, amp: 0.3, phaseDeg: 0 },
            ]);
          } else if (presets[val]) {
            Z0 = presets[val].Z0;
            renderConstituents(presets[val].constituents);
          }
        }

        locationPreset.addEventListener("change", () => {
          loadPreset();
          statusMessage.textContent =
            "Preset loaded. You can fine‑tune the constituents below.";
        });

        constituentTableBody.addEventListener("click", (e) => {
          const idx = e.target.dataset.remove;
          if (idx !== undefined) {
            const list = getConstituentsFromTable();
            list.splice(parseInt(idx, 10), 1);
            renderConstituents(list);
          }
        });

        addConstituentBtn.addEventListener("click", () => {
          const name =
            newName.value.trim() ||
            "C" + (constituentTableBody.children.length + 1);
          const period = parseFloat(newPeriod.value);
          const amp = parseFloat(newAmp.value);
          const phaseDeg = parseFloat(newPhase.value) || 0;
          if (!period || !amp) {
            alert(
              "Please enter at least period and amplitude for the new constituent."
            );
            return;
          }
          const list = getConstituentsFromTable();
          list.push({ name, period, amp, phaseDeg });
          renderConstituents(list);
          newName.value = "";
          newPeriod.value = "";
          newAmp.value = "";
          newPhase.value = "";
        });

        function computeTides() {
          const dateStr = dateInput.value;
          if (!dateStr) {
            alert("Please select a date.");
            return;
          }
          const stepMin = parseInt(timeStepInput.value, 10);
          if (!stepMin || stepMin < 5 || stepMin > 120) {
            alert("Please enter a time step between 5 and 120 minutes.");
            return;
          }
          const constituents = getConstituentsFromTable();
          if (constituents.length === 0) {
            alert("Please define at least one tidal constituent.");
            return;
          }

          statusMessage.textContent = "Calculating...";
          const unit = unitSelect.value;

          const data = [];
          const totalMinutes = 24 * 60;
          for (let m = 0; m <= totalMinutes; m += stepMin) {
            const tHours = m / 60;
            let h = Z0;
            constituents.forEach((c) => {
              const omega = (2 * Math.PI) / c.period;
              const phaseRad = (c.phaseDeg * Math.PI) / 180;
              h += c.amp * Math.cos(omega * tHours + phaseRad);
            });
            data.push({ t: tHours, h });
          }

          const minH = Math.min(...data.map((d) => d.h));
          const maxH = Math.max(...data.map((d) => d.h));
          const range = maxH - minH;

          function formatTime(tHours) {
            const totalMinutes = Math.round(tHours * 60);
            const hh = Math.floor(totalMinutes / 60);
            const mm = totalMinutes % 60;
            return (
              String(hh).padStart(2, "0") + ":" + String(mm).padStart(2, "0")
            );
          }

          const high = data.reduce((a, b) => (b.h > a.h ? b : a), data[0]);
          const low = data.reduce((a, b) => (b.h < a.h ? b : a), data[0]);

          const factor = unit === "ft" ? 3.28084 : 1;
          const unitLabel = unit === "ft" ? "ft" : "m";

          summaryDate.textContent = dateStr;
          summaryHigh.textContent =
            (high.h * factor).toFixed(2) + " " + unitLabel;
          summaryHighTime.textContent = "at " + formatTime(high.t);
          summaryLow.textContent =
            (low.h * factor).toFixed(2) + " " + unitLabel;
          summaryLowTime.textContent = "at " + formatTime(low.t);
          summaryRange.textContent =
            (range * factor).toFixed(2) + " " + unitLabel;

          resultsTableBody.innerHTML = "";
          data.forEach((pt, idx) => {
            const prev = data[idx - 1];
            const next = data[idx + 1];
            let type = "";
            if (prev && next) {
              if (pt.h > prev.h && pt.h > next.h) type = "High tide";
              else if (pt.h < prev.h && pt.h < next.h) type = "Low tide";
            }
            const tr = document.createElement("tr");
            tr.innerHTML = `
          <td class="px-2 py-1 border-b">${formatTime(pt.t)}</td>
          <td class="px-2 py-1 border-b text-right">${(pt.h * factor).toFixed(
            2
          )} ${unitLabel}</td>
          <td class="px-2 py-1 border-b">${type}</td>
        `;
            resultsTableBody.appendChild(tr);
          });

          const chartData = data.map((d) => ({ t: d.t, h: d.h * factor }));
          drawTideChart(tideChart, chartData, unitLabel);

          currentData = chartData.map((d) => ({
            t: d.t,
            h: d.h,
            date: dateStr,
            unit: unitLabel,
          }));
          noDataMessage.classList.add("hidden");
          resultsContainer.classList.remove("hidden");
          statusMessage.textContent = "Done.";
        }

        calculateBtn.addEventListener("click", computeTides);

        resetBtn.addEventListener("click", () => {
          dateInput.value = todayISO();
          timeStepInput.value = 30;
          unitSelect.value = "m";
          locationPreset.value = "custom";
          loadPreset();
          resultsContainer.classList.add("hidden");
          noDataMessage.classList.remove("hidden");
          statusMessage.textContent = "";
          currentData = [];
          const ctx = tideChart.getContext("2d");
          ctx && ctx.clearRect(0, 0, tideChart.width, tideChart.height);
        });

        downloadCsvBtn.addEventListener("click", () => {
          if (!currentData || currentData.length === 0) {
            alert("No data to export. Please run a calculation first.");
            return;
          }
          let csv = "Date,Time,Height (" + currentData[0].unit + ")\n";
          currentData.forEach((row) => {
            const totalMinutes = Math.round(row.t * 60);
            const hh = Math.floor(totalMinutes / 60);
            const mm = totalMinutes % 60;
            const timeStr =
              String(hh).padStart(2, "0") + ":" + String(mm).padStart(2, "0");
            csv += `${row.date},${timeStr},${row.h.toFixed(3)}\n`;
          });
          const blob = new Blob([csv], { type: "text/csv;charset=utf-8;" });
          const url = URL.createObjectURL(blob);
          const a = document.createElement("a");
          a.href = url;
          a.download = `tide-prediction-${currentData[0].date}.csv`;
          document.body.appendChild(a);
          a.click();
          document.body.removeChild(a);
          URL.revokeObjectURL(url);
        });

        loadPreset();
        window.addEventListener("resize", () => {
          if (currentData && currentData.length > 0) {
            drawTideChart(tideChart, currentData, currentData[0].unit);
          }
        });
      });
    </script>

