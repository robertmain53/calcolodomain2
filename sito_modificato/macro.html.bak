    <script>
      // Gender toggle
      document.querySelectorAll(".gender-btn").forEach((btn) => {
        btn.addEventListener("click", () => {
          document.querySelectorAll(".gender-btn").forEach((b) => {
            b.classList.remove(
              "bg-blue-50",
              "border-blue-500",
              "text-blue-700"
            );
            b.classList.add("bg-white", "border-gray-300", "text-gray-700");
          });
          btn.classList.add("bg-blue-50", "border-blue-500", "text-blue-700");
          btn.classList.remove("bg-white", "border-gray-300", "text-gray-700");
          document.getElementById("gender").value = btn.dataset.gender;
        });
      });

      // Units toggle
      const unitsEl = document.getElementById("units");
      const heightMetric = document.getElementById("height-metric");
      const heightImperial = document.getElementById("height-imperial");
      const weightUnitLabel = document.getElementById("weight-unit-label");

      unitsEl.addEventListener("change", () => {
        if (unitsEl.value === "metric") {
          heightMetric.classList.remove("hidden");
          heightImperial.classList.add("hidden");
          weightUnitLabel.textContent = "kg";
        } else {
          heightMetric.classList.add("hidden");
          heightImperial.classList.remove("hidden");
          weightUnitLabel.textContent = "lb";
        }
      });

      // Macro presets
      const presets = {
        balanced: { protein: 30, carbs: 40, fat: 30 },
        highProtein: { protein: 35, carbs: 35, fat: 30 },
        lowCarb: { protein: 30, carbs: 20, fat: 50 },
        keto: { protein: 25, carbs: 5, fat: 70 },
      };

      function setPreset(name) {
        const p = presets[name];
        if (!p) return;
        document.getElementById("proteinPct").value = p.protein;
        document.getElementById("carbPct").value = p.carbs;
        document.getElementById("fatPct").value = p.fat;
        document.querySelectorAll(".macro-preset-btn").forEach((btn) => {
          btn.classList.remove(
            "bg-blue-50",
            "border-blue-500",
            "text-blue-700"
          );
          btn.classList.add("bg-white", "border-gray-300", "text-gray-700");
          if (btn.dataset.preset === name) {
            btn.classList.add("bg-blue-50", "border-blue-500", "text-blue-700");
            btn.classList.remove(
              "bg-white",
              "border-gray-300",
              "text-gray-700"
            );
          }
        });
      }

      document.querySelectorAll(".macro-preset-btn").forEach((btn) => {
        btn.addEventListener("click", () => setPreset(btn.dataset.preset));
      });

      // Initial preset
      setPreset("balanced");

      // Calculation
      const calculateBtn = document.getElementById("calculateBtn");
      const resetBtn = document.getElementById("resetBtn");
      const errorMsg = document.getElementById("errorMsg");
      const resultsSection = document.getElementById("resultsSection");

      function clamp(val, min, max) {
        return Math.min(Math.max(val, min), max);
      }

      calculateBtn.addEventListener("click", () => {
        errorMsg.textContent = "";

        const gender = document.getElementById("gender").value;
        const age = parseFloat(document.getElementById("age").value);
        const units = unitsEl.value;
        let heightCm, weightKg;

        if (!age || age < 14 || age > 100) {
          errorMsg.textContent = "Please enter a valid age between 14 and 100.";
          resultsSection.classList.add("hidden");
          return;
        }

        if (units === "metric") {
          heightCm = parseFloat(document.getElementById("heightCm").value);
          weightKg = parseFloat(document.getElementById("weight").value);
          if (!heightCm || heightCm < 120 || heightCm > 230) {
            errorMsg.textContent = "Please enter a realistic height in cm.";
            resultsSection.classList.add("hidden");
            return;
          }
          if (!weightKg || weightKg < 40 || weightKg > 250) {
            errorMsg.textContent = "Please enter a realistic weight in kg.";
            resultsSection.classList.add("hidden");
            return;
          }
        } else {
          const ft = parseFloat(document.getElementById("heightFt").value);
          const inch = parseFloat(document.getElementById("heightIn").value);
          const weightLb = parseFloat(document.getElementById("weight").value);
          if (!ft || ft < 4 || ft > 7 || inch < 0 || inch > 11) {
            errorMsg.textContent = "Please enter a realistic height in ft/in.";
            resultsSection.classList.add("hidden");
            return;
          }
          if (!weightLb || weightLb < 90 || weightLb > 550) {
            errorMsg.textContent = "Please enter a realistic weight in lb.";
            resultsSection.classList.add("hidden");
            return;
          }
          heightCm = (ft * 12 + inch) * 2.54;
          weightKg = weightLb * 0.45359237;
        }

        const activity = parseFloat(document.getElementById("activity").value);
        const goal = document.getElementById("goal").value;
        const rate = parseFloat(document.getElementById("rate").value);

        let proteinPct = parseFloat(
          document.getElementById("proteinPct").value
        );
        let carbPct = parseFloat(document.getElementById("carbPct").value);
        let fatPct = parseFloat(document.getElementById("fatPct").value);

        if ([proteinPct, carbPct, fatPct].some((v) => isNaN(v) || v <= 0)) {
          errorMsg.textContent = "Please enter positive macro percentages.";
          resultsSection.classList.add("hidden");
          return;
        }

        // Normalize macros to sum 100
        const sumPct = proteinPct + carbPct + fatPct;
        proteinPct = (proteinPct / sumPct) * 100;
        carbPct = (carbPct / sumPct) * 100;
        fatPct = (fatPct / sumPct) * 100;

        // BMR (Mifflin–St Jeor)
        let bmr;
        if (gender === "male") {
          bmr = 10 * weightKg + 6.25 * heightCm - 5 * age + 5;
        } else {
          bmr = 10 * weightKg + 6.25 * heightCm - 5 * age - 161;
        }

        const tdee = bmr * activity;

        // Calorie adjustment based on weekly rate (kg/week)
        // 1 kg ~ 7700 kcal
        const dailyDelta = (rate * 7700) / 7;
        let targetCalories = tdee + dailyDelta;

        // Clamp to reasonable range (e.g., 1200–4000 kcal)
        targetCalories = clamp(targetCalories, 1200, 4000);

        // Macro grams
        const proteinKcal = targetCalories * (proteinPct / 100);
        const carbKcal = targetCalories * (carbPct / 100);
        const fatKcal = targetCalories * (fatPct / 100);

        const proteinG = proteinKcal / 4;
        const carbG = carbKcal / 4;
        const fatG = fatKcal / 9;

        // Output
        document.getElementById("bmrOut").textContent = Math.round(bmr);
        document.getElementById("caloriesOut").textContent =
          Math.round(targetCalories);
        document.getElementById("proteinG").textContent = Math.round(proteinG);
        document.getElementById("carbG").textContent = Math.round(carbG);
        document.getElementById("fatG").textContent = Math.round(fatG);
        document.getElementById("proteinPctOut").textContent =
          Math.round(proteinPct);
        document.getElementById("carbPctOut").textContent = Math.round(carbPct);
        document.getElementById("fatPctOut").textContent = Math.round(fatPct);

        const proteinPerKg = proteinG / weightKg;
        document.getElementById("proteinPerKgNote").textContent =
          "This plan provides about " +
          proteinPerKg.toFixed(2) +
          " g of protein per kg of body weight per day.";

        resultsSection.classList.remove("hidden");
      });

      resetBtn.addEventListener("click", () => {
        document.getElementById("age").value = 30;
        unitsEl.value = "metric";
        heightMetric.classList.remove("hidden");
        heightImperial.classList.add("hidden");
        document.getElementById("heightCm").value = 175;
        document.getElementById("heightFt").value = 5;
        document.getElementById("heightIn").value = 9;
        document.getElementById("weight").value = 75;
        weightUnitLabel.textContent = "kg";
        document.getElementById("activity").value = "1.55";
        document.getElementById("goal").value = "maintain";
        document.getElementById("rate").value = "-0.5";
        setPreset("balanced");
        errorMsg.textContent = "";
        resultsSection.classList.add("hidden");
      });

      document
        .querySelectorAll("#faq-container .faq-question")
        .forEach((btn) => {
          btn.addEventListener("click", () => {
            const answer = btn.parentElement.querySelector(".faq-answer");
            const icon = btn.querySelector("span:last-child");
            const isHidden = answer.classList.contains("hidden");
            document
              .querySelectorAll("#faq-container .faq-answer")
              .forEach((a) => a.classList.add("hidden"));
            document
              .querySelectorAll("#faq-container .faq-question span:last-child")
              .forEach((i) => (i.textContent = "+"));
            if (isHidden) {
              answer.classList.remove("hidden");
              icon.textContent = "−";
            }
          });
        });
    </script>

