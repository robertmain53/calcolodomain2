    <script>
      // Tab switching
      const tabReturns = document.getElementById("tab-returns");
      const tabPrices = document.getElementById("tab-prices");
      const panelReturns = document.getElementById("panel-returns");
      const panelPrices = document.getElementById("panel-prices");

      function activateTab(tab) {
        if (tab === "returns") {
          tabReturns.classList.add("border-blue-600", "text-blue-600");
          tabReturns.classList.remove("border-transparent", "text-gray-500");
          tabPrices.classList.remove("border-blue-600", "text-blue-600");
          tabPrices.classList.add("border-transparent", "text-gray-500");
          panelReturns.classList.remove("hidden");
          panelPrices.classList.add("hidden");
        } else {
          tabPrices.classList.add("border-blue-600", "text-blue-600");
          tabPrices.classList.remove("border-transparent", "text-gray-500");
          tabReturns.classList.remove("border-blue-600", "text-blue-600");
          tabReturns.classList.add("border-transparent", "text-gray-500");
          panelPrices.classList.remove("hidden");
          panelReturns.classList.add("hidden");
        }
      }

      tabReturns.addEventListener("click", () => activateTab("returns"));
      tabPrices.addEventListener("click", () => activateTab("prices"));

      // Utility: parse numeric list from textarea
      function parseNumberList(text) {
        if (!text) return [];
        const tokens = text
          .replace(/[\n\r]+/g, " ")
          .split(/[\s,;]+/)
          .map((t) => t.trim())
          .filter((t) => t.length > 0);
        const nums = [];
        for (const tok of tokens) {
          const v = Number(tok);
          if (!isNaN(v) && isFinite(v)) {
            nums.push(v);
          }
        }
        return nums;
      }

      function mean(arr) {
        if (!arr.length) return NaN;
        return arr.reduce((a, b) => a + b, 0) / arr.length;
      }

      function stdDev(arr, usePopulation) {
        const n = arr.length;
        if (n === 0) return NaN;
        if (!usePopulation && n < 2) return NaN;
        const m = mean(arr);
        const variance =
          arr.reduce((sum, x) => sum + Math.pow(x - m, 2), 0) /
          (usePopulation ? n : n - 1);
        return Math.sqrt(variance);
      }

      function formatPercent(x) {
        if (!isFinite(x)) return "–";
        return x.toFixed(3) + " %";
      }

      function formatNumber(x, digits = 4) {
        if (!isFinite(x)) return "–";
        return x.toFixed(digits);
      }

      function qualitativeSharpe(sAnnual) {
        if (!isFinite(sAnnual)) return "";
        if (sAnnual < 0)
          return "Negative Sharpe: the investment underperformed the risk‑free rate on a risk‑adjusted basis.";
        if (sAnnual < 0.5)
          return "Weak risk‑adjusted performance compared to the risk‑free rate.";
        if (sAnnual < 1.0)
          return "Acceptable but not outstanding risk‑adjusted performance.";
        if (sAnnual < 1.5) return "Good risk‑adjusted performance.";
        if (sAnnual < 2.0) return "Very good risk‑adjusted performance.";
        return "Excellent risk‑adjusted performance (verify that the sample is long enough and realistic).";
      }

      // DOM elements for results
      const sharpePeriodEl = document.getElementById("sharpe-period");
      const sharpeAnnualEl = document.getElementById("sharpe-annual");
      const sharpeQualEl = document.getElementById("sharpe-qualitative");
      const meanReturnEl = document.getElementById("mean-return");
      const rfPeriodEl = document.getElementById("rf-period");
      const excessReturnEl = document.getElementById("excess-return");
      const stdDevEl = document.getElementById("std-dev");
      const nObsEl = document.getElementById("num-observations");
      const errorEl = document.getElementById("error-message");

      let lastResult = null; // store last computed stats for comparison table

      function showError(msg) {
        errorEl.textContent = msg;
        errorEl.classList.remove("hidden");
      }

      function clearError() {
        errorEl.textContent = "";
        errorEl.classList.add("hidden");
      }

      function updateResults(stats) {
        if (!stats) {
          sharpePeriodEl.textContent = "–";
          sharpeAnnualEl.textContent = "–";
          sharpeQualEl.textContent = "";
          meanReturnEl.textContent = "–";
          rfPeriodEl.textContent = "–";
          excessReturnEl.textContent = "–";
          stdDevEl.textContent = "–";
          nObsEl.textContent = "–";
          lastResult = null;
          return;
        }
        sharpePeriodEl.textContent = formatNumber(stats.sharpePeriod, 4);
        sharpeAnnualEl.textContent = formatNumber(stats.sharpeAnnual, 4);
        sharpeQualEl.textContent = qualitativeSharpe(stats.sharpeAnnual);
        meanReturnEl.textContent = formatPercent(stats.meanReturn);
        rfPeriodEl.textContent = formatPercent(stats.rfPeriod);
        excessReturnEl.textContent = formatPercent(stats.excessReturn);
        stdDevEl.textContent = formatPercent(stats.stdDev);
        nObsEl.textContent = String(stats.n);
        lastResult = stats;
      }

      // Calculate using returns
      document
        .getElementById("calc-returns-btn")
        .addEventListener("click", () => {
          clearError();
          const returnsText = document.getElementById("returns-input").value;
          const returnsPct = parseNumberList(returnsText);
          if (returnsPct.length < 2) {
            showError(
              "Please enter at least two valid return values to compute a Sharpe ratio."
            );
            updateResults(null);
            return;
          }
          const rfInput = Number(
            document.getElementById("riskfree-input").value
          );
          const freq =
            Number(document.getElementById("freq-select").value) || 1;
          const usePopulation =
            document.getElementById("population-std").checked;

          const r = returnsPct.map((v) => v / 100); // convert to decimals
          const rfPeriod = (isNaN(rfInput) ? 0 : rfInput) / 100;
          const avg = mean(r);
          const sigma = stdDev(r, usePopulation);

          if (!isFinite(sigma) || sigma === 0) {
            showError(
              "Standard deviation is zero or undefined. Sharpe ratio cannot be computed."
            );
            updateResults(null);
            return;
          }

          const excess = avg - rfPeriod;
          const sharpePeriod = excess / sigma;
          const sharpeAnnual = sharpePeriod * Math.sqrt(freq);

          updateResults({
            sharpePeriod,
            sharpeAnnual,
            meanReturn: avg * 100,
            rfPeriod: rfPeriod * 100,
            excessReturn: excess * 100,
            stdDev: sigma * 100,
            n: returnsPct.length,
          });
        });

      document
        .getElementById("clear-returns-btn")
        .addEventListener("click", () => {
          document.getElementById("returns-input").value = "";
          document.getElementById("riskfree-input").value = "0";
          updateResults(null);
          clearError();
        });

      // Calculate using prices
      document
        .getElementById("calc-prices-btn")
        .addEventListener("click", () => {
          clearError();
          const pricesText = document.getElementById("prices-input").value;
          const prices = parseNumberList(pricesText);
          if (prices.length < 2) {
            showError(
              "Please enter at least two valid price values to compute returns."
            );
            updateResults(null);
            return;
          }
          const rfAnnualInput = Number(
            document.getElementById("riskfree-annual-input").value
          );
          const freq =
            Number(document.getElementById("freq-prices-select").value) || 1;
          const usePopulation = document.getElementById(
            "population-std-prices"
          ).checked;

          // Compute simple returns from prices: (P_t / P_{t-1}) - 1
          const r = [];
          for (let i = 1; i < prices.length; i++) {
            const prev = prices[i - 1];
            const curr = prices[i];
            if (prev <= 0 || curr <= 0) {
              showError("Prices must be positive numbers to compute returns.");
              updateResults(null);
              return;
            }
            r.push(curr / prev - 1);
          }
          if (r.length < 2 && !usePopulation) {
            showError(
              "At least two return observations are required when using sample standard deviation."
            );
            updateResults(null);
            return;
          }

          // Convert annual risk-free to per-period using simple approximation:
          // rf_period ≈ (1 + rf_annual)^(1/freq) - 1
          const rfAnnualDec = (isNaN(rfAnnualInput) ? 0 : rfAnnualInput) / 100;
          const rfPeriod = Math.pow(1 + rfAnnualDec, 1 / freq) - 1;

          const avg = mean(r);
          const sigma = stdDev(r, usePopulation);
          if (!isFinite(sigma) || sigma === 0) {
            showError(
              "Standard deviation of returns is zero or undefined. Sharpe ratio cannot be computed."
            );
            updateResults(null);
            return;
          }

          const excess = avg - rfPeriod;
          const sharpePeriod = excess / sigma;
          const sharpeAnnual = sharpePeriod * Math.sqrt(freq);

          updateResults({
            sharpePeriod,
            sharpeAnnual,
            meanReturn: avg * 100,
            rfPeriod: rfPeriod * 100,
            excessReturn: excess * 100,
            stdDev: sigma * 100,
            n: r.length,
          });
        });

      document
        .getElementById("clear-prices-btn")
        .addEventListener("click", () => {
          document.getElementById("prices-input").value = "";
          document.getElementById("riskfree-annual-input").value = "2";
          updateResults(null);
          clearError();
        });

      // Comparison table
      const comparisonTbody = document.getElementById("comparison-tbody");
      const portfolioNameInput = document.getElementById(
        "portfolio-name-input"
      );

      function renderEmptyComparisonRow() {
        comparisonTbody.innerHTML =
          '<tr class="text-center text-gray-500"><td colspan="6" class="px-3 py-3">No portfolios added yet.</td></tr>';
      }

      function addComparisonRow(name, stats) {
        if (!stats) return;
        // Remove placeholder row if present
        if (
          comparisonTbody.children.length === 1 &&
          comparisonTbody.children[0].children.length === 1
        ) {
          comparisonTbody.innerHTML = "";
        }
        const tr = document.createElement("tr");
        tr.className = "border-t border-gray-200";
        tr.innerHTML = `
      <td class="px-3 py-2 text-left">${name}</td>
      <td class="px-3 py-2 text-right">${formatNumber(
        stats.sharpeAnnual,
        4
      )}</td>
      <td class="px-3 py-2 text-right">${formatNumber(
        stats.sharpePeriod,
        4
      )}</td>
      <td class="px-3 py-2 text-right">${formatPercent(stats.meanReturn)}</td>
      <td class="px-3 py-2 text-right">${formatPercent(stats.stdDev)}</td>
      <td class="px-3 py-2 text-center">
        <button type="button" class="text-xs text-red-600 hover:underline remove-row-btn">Remove</button>
      </td>
    `;
        comparisonTbody.appendChild(tr);
      }

      document
        .getElementById("add-portfolio-btn")
        .addEventListener("click", () => {
          clearError();
          if (!lastResult) {
            showError(
              "Calculate a Sharpe ratio first, then add it to the comparison table."
            );
            return;
          }
          let name = portfolioNameInput.value.trim();
          if (!name) {
            name = "Portfolio " + (comparisonTbody.children.length + 1);
          }
          addComparisonRow(name, lastResult);
          portfolioNameInput.value = "";
        });

      comparisonTbody.addEventListener("click", (e) => {
        if (e.target && e.target.classList.contains("remove-row-btn")) {
          const row = e.target.closest("tr");
          if (row) {
            row.remove();
            if (!comparisonTbody.children.length) {
              renderEmptyComparisonRow();
            }
          }
        }
      });

      // Initialize comparison table
      renderEmptyComparisonRow();

      // Mobile menu script (simple)
      const mobileToggle = document.getElementById("mobile-menu-toggle");
      const mobileMenu = document.getElementById("mobile-menu");
      mobileToggle.addEventListener("click", () => {
        const isHidden = mobileMenu.classList.contains("hidden");
        mobileMenu.classList.toggle("hidden", !isHidden);
        mobileToggle.setAttribute("aria-expanded", String(isHidden));
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
