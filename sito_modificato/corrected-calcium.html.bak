    <script>
      (function () {
        const unitSelect = document.getElementById("unit-system");
        const formulaSelect = document.getElementById("formula-select");
        const calciumInput = document.getElementById("calcium-input");
        const albuminInput = document.getElementById("albumin-input");
        const normalAlbuminInput = document.getElementById(
          "normal-albumin-input"
        );
        const calciumUnitLabel = document.getElementById("calcium-unit-label");
        const albuminUnitLabel = document.getElementById("albumin-unit-label");
        const normalAlbuminUnitLabel = document.getElementById(
          "normal-albumin-unit-label"
        );
        const refRangeSelect = document.getElementById("ref-range-select");
        const customRangeWrapper = document.getElementById(
          "custom-range-wrapper"
        );
        const refLowInput = document.getElementById("ref-low-input");
        const refHighInput = document.getElementById("ref-high-input");
        const refRangeHint = document.getElementById("ref-range-hint");

        const calcBtn = document.getElementById("calculate-btn");
        const resetBtn = document.getElementById("reset-btn");
        const resultsContainer = document.getElementById("results-container");
        const measuredDisplay = document.getElementById("measured-display");
        const correctedDisplay = document.getElementById("corrected-display");
        const deltaDisplay = document.getElementById("delta-display");
        const interpretationDisplay = document.getElementById(
          "interpretation-display"
        );
        const correctedAltDisplay = document.getElementById(
          "corrected-alt-display"
        );
        const measuredAltDisplay = document.getElementById(
          "measured-alt-display"
        );
        const warningBadge = document.getElementById("warning-badge");

        function updateUnits() {
          if (unitSelect.value === "mgdl") {
            calciumUnitLabel.textContent = "(mg/dL)";
            albuminUnitLabel.textContent = "(g/dL)";
            normalAlbuminUnitLabel.textContent = "(g/dL)";
            if (!normalAlbuminInput.value) normalAlbuminInput.value = "4.0";
            refRangeHint.textContent = "mg/dL: 8.5–10.5 | mmol/L: 2.12–2.62";
            if (refRangeSelect.value === "default") {
              refLowInput.value = "";
              refHighInput.value = "";
            }
          } else {
            calciumUnitLabel.textContent = "(mmol/L)";
            albuminUnitLabel.textContent = "(g/L)";
            normalAlbuminUnitLabel.textContent = "(g/L)";
            if (
              !normalAlbuminInput.value ||
              Number(normalAlbuminInput.value) <= 10
            ) {
              normalAlbuminInput.value = "40";
            }
            refRangeHint.textContent = "mmol/L: 2.12–2.62 | mg/dL: 8.5–10.5";
            if (refRangeSelect.value === "default") {
              refLowInput.value = "";
              refHighInput.value = "";
            }
          }
        }

        unitSelect.addEventListener("change", updateUnits);

        refRangeSelect.addEventListener("change", () => {
          if (refRangeSelect.value === "custom") {
            customRangeWrapper.classList.remove("hidden");
          } else {
            customRangeWrapper.classList.add("hidden");
            refLowInput.value = "";
            refHighInput.value = "";
          }
        });

        function getRefRange(unit) {
          let low, high;
          if (
            refRangeSelect.value === "custom" &&
            refLowInput.value &&
            refHighInput.value
          ) {
            low = parseFloat(refLowInput.value);
            high = parseFloat(refHighInput.value);
            if (isFinite(low) && isFinite(high) && low < high) {
              return { low, high };
            }
          }
          // default ranges
          if (unit === "mgdl") {
            return { low: 8.5, high: 10.5 };
          } else {
            return { low: 2.12, high: 2.62 };
          }
        }

        function convertCa(value, fromUnit) {
          const factor = 0.2495;
          if (!isFinite(value)) return null;
          if (fromUnit === "mgdl") {
            return value * factor; // to mmol/L
          } else {
            return value / factor; // to mg/dL
          }
        }

        function classify(value, range) {
          if (!isFinite(value)) return "–";
          if (value < range.low) return "Low (possible hypocalcemia)";
          if (value > range.high) return "High (possible hypercalcemia)";
          return "Within typical reference range";
        }

        function shouldShowWarning(albumin, unit) {
          if (!isFinite(albumin)) return false;
          if (unit === "mgdl") {
            return albumin < 2.0;
          } else {
            return albumin < 20;
          }
        }

        calcBtn.addEventListener("click", () => {
          const unit = unitSelect.value;
          const formula = formulaSelect.value;

          const ca = parseFloat(calciumInput.value);
          const alb = parseFloat(albuminInput.value);
          const albNorm = parseFloat(normalAlbuminInput.value);

          if (!isFinite(ca) || !isFinite(alb) || !isFinite(albNorm)) {
            alert("Please enter valid numeric values for calcium and albumin.");
            return;
          }

          let corrected = ca;
          if (formula !== "none") {
            if (unit === "mgdl") {
              const factor = formula === "standard" ? 0.8 : 0.6;
              corrected = ca + factor * (albNorm - alb);
            } else {
              const factor = formula === "standard" ? 0.02 : 0.015;
              corrected = ca + factor * (albNorm - alb);
            }
          }

          const delta = corrected - ca;
          const range = getRefRange(unit);

          measuredDisplay.textContent = `${ca.toFixed(2)} ${
            unit === "mgdl" ? "mg/dL" : "mmol/L"
          }`;
          correctedDisplay.textContent = `${corrected.toFixed(2)} ${
            unit === "mgdl" ? "mg/dL" : "mmol/L"
          }`;
          deltaDisplay.textContent = `${delta >= 0 ? "+" : ""}${delta.toFixed(
            2
          )} ${unit === "mgdl" ? "mg/dL" : "mmol/L"}`;
          interpretationDisplay.textContent = classify(corrected, range);

          const correctedAlt = convertCa(corrected, unit);
          const measuredAlt = convertCa(ca, unit);
          if (correctedAlt !== null && measuredAlt !== null) {
            if (unit === "mgdl") {
              correctedAltDisplay.textContent = `${correctedAlt.toFixed(
                2
              )} mmol/L`;
              measuredAltDisplay.textContent = `${measuredAlt.toFixed(
                2
              )} mmol/L`;
            } else {
              correctedAltDisplay.textContent = `${correctedAlt.toFixed(
                2
              )} mg/dL`;
              measuredAltDisplay.textContent = `${measuredAlt.toFixed(
                2
              )} mg/dL`;
            }
          } else {
            correctedAltDisplay.textContent = "–";
            measuredAltDisplay.textContent = "–";
          }

          if (shouldShowWarning(alb, unit)) {
            warningBadge.classList.remove("hidden");
          } else {
            warningBadge.classList.add("hidden");
          }

          resultsContainer.classList.remove("hidden");
        });

        resetBtn.addEventListener("click", () => {
          calciumInput.value = "";
          albuminInput.value = "";
          normalAlbuminInput.value = unitSelect.value === "mgdl" ? "4.0" : "40";
          refRangeSelect.value = "default";
          customRangeWrapper.classList.add("hidden");
          refLowInput.value = "";
          refHighInput.value = "";
          resultsContainer.classList.add("hidden");
          warningBadge.classList.add("hidden");
        });

        document
          .querySelectorAll("#faq-container .faq-question")
          .forEach((btn) => {
            btn.addEventListener("click", () => {
              const answer = btn.parentElement.querySelector(".faq-answer");
              const isHidden = answer.classList.contains("hidden");
              document
                .querySelectorAll("#faq-container .faq-answer")
                .forEach((a) => a.classList.add("hidden"));
              document
                .querySelectorAll(
                  "#faq-container .faq-question span:last-child"
                )
                .forEach((icon) => (icon.textContent = "+"));
              if (isHidden) {
                answer.classList.remove("hidden");
                btn.querySelector("span:last-child").textContent = "−";
              }
            });
          });

        // Initialize units on load
        updateUnits();
      })();
    </script>
  
