    <script>
      // Utility conversions
      function toMeters(value, unit) {
        if (!isFinite(value)) return NaN;
        switch (unit) {
          case "m":
            return value;
          case "mm":
            return value / 1000;
          case "in":
            return value * 0.0254;
          default:
            return value;
        }
      }

      function fromPressurePa(valuePa, unit) {
        switch (unit) {
          case "Pa":
            return valuePa;
          case "kPa":
            return valuePa / 1000;
          case "bar":
            return valuePa / 1e5;
          case "psi":
            return valuePa / 6894.757;
          default:
            return valuePa;
        }
      }

      function toPressurePa(value, unit) {
        if (!isFinite(value)) return NaN;
        switch (unit) {
          case "Pa":
            return value;
          case "kPa":
            return value * 1000;
          case "bar":
            return value * 1e5;
          case "psi":
            return value * 6894.757;
          default:
            return value;
        }
      }

      function toDensitySI(value, unit) {
        if (!isFinite(value)) return NaN;
        switch (unit) {
          case "kgm3":
            return value;
          case "lbft3":
            return value * 16.018463;
          default:
            return value;
        }
      }

      function toViscositySI(value, unit) {
        if (!isFinite(value)) return NaN;
        switch (unit) {
          case "Pa.s":
            return value;
          case "cP":
            return value / 1000; // 1 cP = 0.001 Pa·s
          default:
            return value;
        }
      }

      function toFlowSI(value, unit) {
        if (!isFinite(value)) return NaN;
        switch (unit) {
          case "m3s":
            return value;
          case "m3h":
            return value / 3600;
          case "ls":
            return value / 1000;
          case "gpm":
            return value * 0.0000630902; // US gpm to m3/s
          default:
            return value;
        }
      }

      function formatEngineering(value, unit) {
        if (!isFinite(value)) return "–";
        if (value === 0) return "0 " + unit;
        const abs = Math.abs(value);
        let scaled = value;
        let prefix = "";
        if (abs >= 1e6) {
          scaled = value / 1e6;
          prefix = "M";
        } else if (abs >= 1e3) {
          scaled = value / 1e3;
          prefix = "k";
        } else if (abs < 1 && abs >= 1e-3) {
          scaled = value * 1e3;
          prefix = "m";
        } else if (abs < 1e-3 && abs >= 1e-6) {
          scaled = value * 1e6;
          prefix = "µ";
        } else if (abs < 1e-6 && abs >= 1e-9) {
          scaled = value * 1e9;
          prefix = "n";
        }
        return scaled.toFixed(3) + " " + prefix + unit;
      }

      function formatNumber(value, decimals) {
        if (!isFinite(value)) return "–";
        return value.toFixed(decimals);
      }

      function computeCd(beta, Re, tapType) {
        if (
          !isFinite(beta) ||
          beta <= 0 ||
          beta >= 1 ||
          !isFinite(Re) ||
          Re <= 0
        ) {
          return NaN;
        }
        // Simplified ISO 5167-style correlation for corner taps
        let Cd =
          0.5961 +
          0.0261 * Math.pow(beta, 2) -
          0.216 * Math.pow(beta, 8) +
          0.000521 / Math.pow(Re, 0.7);
        // Adjust for tapping type (very approximate)
        if (tapType === "flange") {
          Cd += 0.005;
        } else if (tapType === "dD") {
          Cd -= 0.003;
        }
        return Cd;
      }

      function calculateOrifice() {
        const errorEl = document.getElementById("errorMsg");
        errorEl.textContent = "";

        const solveFor = document.getElementById("solveFor").value;
        const fluidType = document.getElementById("fluidType").value;
        const tapType = document.getElementById("tapType").value;

        const D = toMeters(
          parseFloat(document.getElementById("pipeDiameter").value),
          document.getElementById("pipeDiameterUnit").value
        );
        const d = toMeters(
          parseFloat(document.getElementById("orificeDiameter").value),
          document.getElementById("orificeDiameterUnit").value
        );

        const rho = toDensitySI(
          parseFloat(document.getElementById("density").value),
          document.getElementById("densityUnit").value
        );
        const mu = toViscositySI(
          parseFloat(document.getElementById("viscosity").value),
          document.getElementById("viscosityUnit").value
        );

        let eps =
          fluidType === "gas"
            ? parseFloat(document.getElementById("expansibility").value)
            : 1.0;
        if (!isFinite(eps) || eps <= 0 || eps > 1.1) eps = 1.0;

        let deltaP;
        let Q;

        if (!isFinite(D) || !isFinite(d) || D <= 0 || d <= 0 || d >= D) {
          errorEl.textContent =
            "Check pipe and orifice diameters (d must be less than D).";
          return;
        }
        if (!isFinite(rho) || rho <= 0) {
          errorEl.textContent = "Density must be a positive number.";
          return;
        }
        if (!isFinite(mu) || mu <= 0) {
          errorEl.textContent = "Viscosity must be a positive number.";
          return;
        }

        const beta = d / D;
        const A2 = (Math.PI * d * d) / 4;

        if (solveFor === "flow") {
          deltaP = toPressurePa(
            parseFloat(document.getElementById("deltaP").value),
            document.getElementById("deltaPUnit").value
          );
          if (!isFinite(deltaP) || deltaP <= 0) {
            errorEl.textContent =
              "Differential pressure must be a positive number.";
            return;
          }
          // Initial guess for Cd using a nominal Re (will refine)
          let CdManual = parseFloat(document.getElementById("CdManual").value);
          let Cd;
          if (isFinite(CdManual) && CdManual > 0.3 && CdManual < 1.2) {
            Cd = CdManual;
          } else {
            // First estimate Q with Cd ~0.61
            let CdGuess = 0.61;
            let Qguess =
              CdGuess *
              eps *
              A2 *
              Math.sqrt((2 * deltaP) / (rho * (1 - Math.pow(beta, 4))));
            let vGuess = Qguess / ((Math.PI * D * D) / 4);
            let ReGuess = (rho * vGuess * D) / mu;
            Cd = computeCd(beta, ReGuess, tapType);
            if (!isFinite(Cd)) Cd = CdGuess;
          }
          Q =
            Cd *
            eps *
            A2 *
            Math.sqrt((2 * deltaP) / (rho * (1 - Math.pow(beta, 4))));
        } else {
          // solve for deltaP from flow
          Q = toFlowSI(
            parseFloat(document.getElementById("flowRateInput").value),
            document.getElementById("flowRateInputUnit").value
          );
          if (!isFinite(Q) || Q <= 0) {
            errorEl.textContent = "Flow rate must be a positive number.";
            return;
          }
          const v = Q / ((Math.PI * D * D) / 4);
          const Re = (rho * v * D) / mu;
          let CdManual = parseFloat(document.getElementById("CdManual").value);
          let Cd;
          if (isFinite(CdManual) && CdManual > 0.3 && CdManual < 1.2) {
            Cd = CdManual;
          } else {
            Cd = computeCd(beta, Re, tapType);
            if (!isFinite(Cd)) Cd = 0.61;
          }
          deltaP =
            (rho * Math.pow(Q / (Cd * eps * A2), 2) * (1 - Math.pow(beta, 4))) /
            2;
        }

        const A1 = (Math.PI * D * D) / 4;
        const vPipe = Q / A1;
        const ReFinal = (rho * vPipe * D) / mu;
        let CdFinal;
        let CdManual2 = parseFloat(document.getElementById("CdManual").value);
        if (isFinite(CdManual2) && CdManual2 > 0.3 && CdManual2 < 1.2) {
          CdFinal = CdManual2;
        } else {
          CdFinal = computeCd(beta, ReFinal, tapType);
          if (!isFinite(CdFinal)) CdFinal = 0.61;
        }

        const mDot = rho * Q;

        // Update results
        document.getElementById("resQ").textContent = formatEngineering(
          Q,
          "m³/s"
        );
        document.getElementById("resMdot").textContent = formatEngineering(
          mDot,
          "kg/s"
        );
        const dpDisplayUnit = "kPa";
        document.getElementById("resDeltaP").textContent =
          formatNumber(fromPressurePa(deltaP, dpDisplayUnit), 3) +
          " " +
          dpDisplayUnit;
        document.getElementById("resVelocity").textContent =
          formatNumber(vPipe, 3) + " m/s";

        document.getElementById("resBeta").textContent = formatNumber(beta, 4);
        document.getElementById("resCd").textContent = formatNumber(CdFinal, 4);
        document.getElementById("resEps").textContent = formatNumber(eps, 4);
        document.getElementById("resRe").textContent =
          ReFinal > 0 ? ReFinal.toExponential(3) : "–";

        if (ReFinal < 1e4) {
          document.getElementById("resultsNote").textContent =
            "Note: Reynolds number is below 1e4; ISO 5167 correlations may be less accurate in this regime.";
        } else {
          document.getElementById("resultsNote").textContent =
            "Note: Results are approximate and assume fully developed flow and proper upstream/downstream straight lengths.";
        }
      }

      document.addEventListener("DOMContentLoaded", function () {
        const fluidTypeEl = document.getElementById("fluidType");
        const expansibilityContainer = document.getElementById(
          "expansibilityContainer"
        );
        const solveForEl = document.getElementById("solveFor");
        const dpInputGroup = document.getElementById("dpInputGroup");
        const flowInputGroup = document.getElementById("flowInputGroup");
        const btnUseAutoCd = document.getElementById("btnUseAutoCd");

        fluidTypeEl.addEventListener("change", function () {
          if (this.value === "gas") {
            expansibilityContainer.classList.remove("hidden");
          } else {
            expansibilityContainer.classList.add("hidden");
          }
        });

        solveForEl.addEventListener("change", function () {
          if (this.value === "flow") {
            dpInputGroup.classList.remove("hidden");
            flowInputGroup.classList.add("hidden");
          } else {
            dpInputGroup.classList.add("hidden");
            flowInputGroup.classList.remove("hidden");
          }
        });

        document
          .getElementById("calculateBtn")
          .addEventListener("click", calculateOrifice);

        document
          .getElementById("resetBtn")
          .addEventListener("click", function () {
            document.getElementById("pipeDiameter").value = 0.1;
            document.getElementById("pipeDiameterUnit").value = "m";
            document.getElementById("orificeDiameter").value = 0.05;
            document.getElementById("orificeDiameterUnit").value = "m";
            document.getElementById("tapType").value = "corner";
            document.getElementById("density").value = 1000;
            document.getElementById("densityUnit").value = "kgm3";
            document.getElementById("viscosity").value = 0.001;
            document.getElementById("viscosityUnit").value = "Pa.s";
            document.getElementById("fluidType").value = "liquid";
            expansibilityContainer.classList.add("hidden");
            document.getElementById("expansibility").value = 0.98;
            document.getElementById("deltaP").value = 10;
            document.getElementById("deltaPUnit").value = "kPa";
            document.getElementById("flowRateInput").value = 0.01;
            document.getElementById("flowRateInputUnit").value = "m3s";
            document.getElementById("CdManual").value = "";
            document.getElementById("solveFor").value = "flow";
            dpInputGroup.classList.remove("hidden");
            flowInputGroup.classList.add("hidden");
            document.getElementById("errorMsg").textContent = "";
            document.getElementById("resQ").textContent = "–";
            document.getElementById("resMdot").textContent = "–";
            document.getElementById("resDeltaP").textContent = "–";
            document.getElementById("resVelocity").textContent = "–";
            document.getElementById("resBeta").textContent = "–";
            document.getElementById("resCd").textContent = "–";
            document.getElementById("resEps").textContent = "–";
            document.getElementById("resRe").textContent = "–";
            document.getElementById("resultsNote").textContent =
              "Note: Results are approximate and assume fully developed flow and proper upstream/downstream straight lengths.";
          });

        btnUseAutoCd.addEventListener("click", function () {
          document.getElementById("CdManual").value = "";
        });
