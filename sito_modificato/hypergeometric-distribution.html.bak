  <script>
    // mobile menu toggle
    const mobileToggle = document.getElementById('mobile-menu-toggle');
    const mobileMenu = document.getElementById('mobile-menu');
    if (mobileToggle && mobileMenu) {
      mobileToggle.addEventListener('click', () => {
        const isHidden = mobileMenu.classList.contains('hidden');
        mobileMenu.classList.toggle('hidden', !isHidden);
        mobileToggle.setAttribute('aria-expanded', String(isHidden));
      });
    }

    // Hypergeometric calculator logic
    const NEl = document.getElementById('N');
    const KEl = document.getElementById('K');
    const nEl = document.getElementById('n');
    const kEl = document.getElementById('k');
    const btn = document.getElementById('calc-btn');
    const errEl = document.getElementById('input-error');

    const pmfOut = document.getElementById('pmf-output');
    const pmfFormula = document.getElementById('pmf-formula');
    const cdfLeq = document.getElementById('cdf-leq');
    const cdfGeq = document.getElementById('cdf-geq');
    const meanOut = document.getElementById('mean-output');
    const varOut = document.getElementById('var-output');
    const distTableBody = document.querySelector('#dist-table tbody');

    function comb(n, r) {
      n = Number(n); r = Number(r);
      if (r < 0 || r > n) return 0;
      if (r === 0 || r === n) return 1;
      r = Math.min(r, n - r);
      let num = 1;
      let den = 1;
      for (let i = 1; i <= r; i++) {
        num *= (n - r + i);
        den *= i;
      }
      return num / den;
    }

    function hypergeomPMF(N, K, n, k) {
      const top = comb(K, k) * comb(N - K, n - k);
      const bottom = comb(N, n);
      if (bottom === 0) return 0;
      return top / bottom;
    }

    function validKRange(N, K, n) {
      // k ranges from max(0, n - (N - K)) to min(n, K)
      const kmin = Math.max(0, n - (N - K));
      const kmax = Math.min(n, K);
      return [kmin, kmax];
    }

    function updateCalc() {
      errEl.textContent = "";
      let N = parseInt(NEl.value, 10);
      let K = parseInt(KEl.value, 10);
      let n = parseInt(nEl.value, 10);
      let k = parseInt(kEl.value, 10);

      if (isNaN(N) || isNaN(K) || isNaN(n) || isNaN(k)) {
        errEl.textContent = "Please enter valid integers for all fields.";
        return;
      }
      if (N <= 0) { errEl.textContent = "N must be > 0."; return; }
      if (K < 0 || K > N) { errEl.textContent = "K must be between 0 and N."; return; }
      if (n < 0 || n > N) { errEl.textContent = "n must be between 0 and N."; return; }

      const [kmin, kmax] = validKRange(N, K, n);
      if (k < kmin || k > kmax) {
        errEl.textContent = "k must be between " + kmin + " and " + kmax + " for the chosen N, K, n.";
      }

      // pmf
      const pmf = hypergeomPMF(N, K, n, k);
      pmfOut.textContent = pmf.toFixed(6);
      pmfFormula.textContent = "C(" + K + "," + k + ")·C(" + (N-K) + "," + (n-k) + ") / C(" + N + "," + n + ")";

      // cdf <= k
      let cdf1 = 0;
      for (let i = kmin; i <= k; i++) {
        cdf1 += hypergeomPMF(N, K, n, i);
      }
      cdfLeq.textContent = "P(X ≤ " + k + "): " + cdf1.toFixed(6);

      // cdf >= k
      let cdf2 = 0;
      for (let i = k; i <= kmax; i++) {
        cdf2 += hypergeomPMF(N, K, n, i);
      }
      cdfGeq.textContent = "P(X ≥ " + k + "): " + cdf2.toFixed(6);

      // mean / variance
      const mean = n * (K / N);
      const variance = n * (K / N) * ((N - K) / N) * ((N - n) / (N - 1 || 1));
      meanOut.textContent = mean.toFixed(4);
      varOut.textContent = variance.toFixed(4);

      // table
      distTableBody.innerHTML = "";
      for (let i = kmin; i <= kmax; i++) {
        const p = hypergeomPMF(N, K, n, i);
        let c = 0;
        for (let j = kmin; j <= i; j++) {
          c += hypergeomPMF(N, K, n, j);
        }
        const tr = document.createElement('tr');
        const tdK = document.createElement('td');
        const tdP = document.createElement('td');
        const tdC = document.createElement('td');
        tdK.textContent = i;
        tdP.textContent = p.toFixed(6);
        tdC.textContent = c.toFixed(6);
        tr.appendChild(tdK);
        tr.appendChild(tdP);
        tr.appendChild(tdC);
        distTableBody.appendChild(tr);
      }
    }

    btn.addEventListener('click', updateCalc);
    NEl.addEventListener('input', updateCalc);
    KEl.addEventListener('input', updateCalc);
    nEl.addEventListener('input', updateCalc);
    kEl.addEventListener('input', updateCalc);

    // initial
    updateCalc();
  </script>

