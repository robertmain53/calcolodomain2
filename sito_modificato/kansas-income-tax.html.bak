    <script>
      // --- Kansas tax logic (simplified) ---
      // Assumed 2024-2025 standard deductions & exemptions (approximate, for demo only)
      const KS_STANDARD_DEDUCTION = {
        single: 3500,
        married_joint: 8000,
        married_separate: 4000,
        hoh: 6000,
      };

      const KS_EXEMPTION_AMOUNT = 2250; // per exemption (approximate)

      // Simplified Kansas brackets (single-style thresholds; joint uses doubled thresholds)
      const KS_BRACKETS_SINGLE = [
        { upTo: 15000, rate: 0.031 },
        { upTo: 30000, rate: 0.0525 },
        { upTo: Infinity, rate: 0.057 },
      ];

      function getKansasBrackets(status) {
        // For married filing jointly / HOH, approximate by doubling thresholds
        if (status === "married_joint" || status === "hoh") {
          return KS_BRACKETS_SINGLE.map((b) => ({
            upTo: b.upTo === Infinity ? Infinity : b.upTo * 2,
            rate: b.rate,
          }));
        }
        // Married filing separately: use single thresholds
        return KS_BRACKETS_SINGLE;
      }

      function formatMoney(value) {
        return value.toLocaleString("en-US", {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 0,
        });
      }

      function formatPercent(value) {
        return (value * 100).toFixed(1) + "%";
      }

      function calculateKansasTax() {
        const status = document.getElementById("ks-filing-status").value;
        const income = Math.max(
          0,
          Number(document.getElementById("ks-income").value) || 0
        );
        const deductionType = document.getElementById("ks-deduction-itemized")
          .checked
          ? "itemized"
          : "standard";
        const exemptions = Math.max(
          0,
          Number(document.getElementById("ks-exemptions").value) || 0
        );
        const itemized = Math.max(
          0,
          Number(document.getElementById("ks-itemized").value) || 0
        );

        const standardDeduction =
          KS_STANDARD_DEDUCTION[status] || KS_STANDARD_DEDUCTION.single;
        const deductions =
          deductionType === "standard" ? standardDeduction : itemized;
        const exemptionTotal = exemptions * KS_EXEMPTION_AMOUNT;

        let taxable = income - deductions - exemptionTotal;
        if (taxable < 0) taxable = 0;

        const brackets = getKansasBrackets(status);
        let remaining = taxable;
        let tax = 0;
        let lastLimit = 0;
        let marginalRate = 0;
        const breakdownLines = [];

        for (const bracket of brackets) {
          if (remaining <= 0) break;
          const bracketAmount = Math.min(
            remaining,
            bracket.upTo === Infinity ? remaining : bracket.upTo - lastLimit
          );
          const bracketTax = bracketAmount * bracket.rate;
          tax += bracketTax;
          remaining -= bracketAmount;
          if (bracketAmount > 0) {
            marginalRate = bracket.rate;
            const rangeLabel =
              bracket.upTo === Infinity
                ? `${formatMoney(lastLimit + 1)} and above`
                : `${formatMoney(lastLimit + 1)} – ${formatMoney(
                    bracket.upTo
                  )}`;
            breakdownLines.push(
              `${rangeLabel}: ${formatMoney(bracketAmount)} × ${formatPercent(
                bracket.rate
              )} = ${formatMoney(bracketTax)}`
            );
          }
          lastLimit = bracket.upTo === Infinity ? lastLimit : bracket.upTo;
        }

        const averageRate = income > 0 ? tax / income : 0;
        const takeHome = income - tax;

        document.getElementById("ks-result-taxable").textContent =
          formatMoney(taxable);
        document.getElementById("ks-result-tax").textContent = formatMoney(tax);
        document.getElementById("ks-result-average-rate").textContent =
          formatPercent(averageRate);
        document.getElementById("ks-result-marginal-rate").textContent =
          formatPercent(marginalRate);
        document.getElementById("ks-result-takehome").textContent =
          formatMoney(takeHome);

        const breakdownDiv = document.getElementById("ks-bracket-breakdown");
        breakdownDiv.innerHTML = "";
        if (breakdownLines.length === 0) {
          breakdownDiv.textContent =
            "No Kansas income tax is due at this income level.";
        } else {
          breakdownLines.forEach((line) => {
            const p = document.createElement("p");
            p.textContent = line;
            breakdownDiv.appendChild(p);
          });
        }
      }

      document.addEventListener("DOMContentLoaded", () => {
        const deductionRadios = document.querySelectorAll(
          'input[name="ks-deduction-type"]'
        );
        deductionRadios.forEach((r) => {
          r.addEventListener("change", () => {
            const showItemized = document.getElementById(
              "ks-deduction-itemized"
            ).checked;
            document
              .getElementById("ks-itemized-wrapper")
              .classList.toggle("hidden", !showItemized);
          });
        });

        document
          .getElementById("ks-calc-btn")
          .addEventListener("click", (e) => {
            e.preventDefault();
            calculateKansasTax();
          });

        // Auto-calc on load
        calculateKansasTax();
      });
    </script>

