    <script>
      // Utility functions
      function addDays(date, days) {
        const d = new Date(date.getTime());
        d.setDate(d.getDate() + days);
        return d;
      }

      function formatDate(date) {
        return date.toLocaleDateString(undefined, {
          year: "numeric",
          month: "short",
          day: "numeric",
        });
      }

      function dateKey(date) {
        return date.toISOString().slice(0, 10);
      }

      function generatePredictions(
        startDate,
        cycleLength,
        periodLength,
        monthsToPredict
      ) {
        const predictions = [];
        const endLimit = addDays(startDate, monthsToPredict * 31); // rough upper bound

        let currentStart = new Date(startDate.getTime());
        // Ensure we start from last known period start
        while (currentStart < endLimit) {
          const nextPeriodStart = addDays(currentStart, cycleLength);
          const nextPeriodEnd = addDays(nextPeriodStart, periodLength - 1);
          const ovulation = addDays(nextPeriodStart, -14);
          const fertileStart = addDays(ovulation, -5);
          const fertileEnd = addDays(ovulation, 1);

          predictions.push({
            periodStart: nextPeriodStart,
            periodEnd: nextPeriodEnd,
            ovulation,
            fertileStart,
            fertileEnd,
          });

          currentStart = nextPeriodStart;
        }
        return predictions;
      }

      function buildCalendar(predictions) {
        const container = document.getElementById("calendar-container");
        container.innerHTML = "";

        if (!predictions.length) return;

        // Build a map of date -> status
        const markers = {};
        predictions.forEach((p) => {
          // Period days
          let d = new Date(p.periodStart.getTime());
          while (d <= p.periodEnd) {
            markers[dateKey(d)] = markers[dateKey(d)] || {};
            markers[dateKey(d)].period = true;
            d = addDays(d, 1);
          }
          // Fertile window
          d = new Date(p.fertileStart.getTime());
          while (d <= p.fertileEnd) {
            markers[dateKey(d)] = markers[dateKey(d)] || {};
            markers[dateKey(d)].fertile = true;
            d = addDays(d, 1);
          }
          // Ovulation
          const ovKey = dateKey(p.ovulation);
          markers[ovKey] = markers[ovKey] || {};
          markers[ovKey].ovulation = true;
        });

        // Determine overall date range to show
        const first = predictions[0].periodStart;
        const last = predictions[predictions.length - 1].periodEnd;
        let monthCursor = new Date(first.getFullYear(), first.getMonth(), 1);

        while (monthCursor <= last) {
          const year = monthCursor.getFullYear();
          const month = monthCursor.getMonth();

          const monthTitle = monthCursor.toLocaleDateString(undefined, {
            month: "long",
            year: "numeric",
          });
          const monthDiv = document.createElement("div");
          monthDiv.className = "border border-gray-200 rounded-lg p-3";

          const header = document.createElement("div");
          header.className =
            "flex justify-between items-center mb-2 text-sm font-semibold";
          header.textContent = monthTitle;
          monthDiv.appendChild(header);

          const weekdays = document.createElement("div");
          weekdays.className =
            "calendar-grid mb-1 text-[0.65rem] text-gray-500";
          ["Su", "Mo", "Tu", "We", "Th", "Fr", "Sa"].forEach((d) => {
            const span = document.createElement("div");
            span.className = "text-center";
            span.textContent = d;
            weekdays.appendChild(span);
          });
          monthDiv.appendChild(weekdays);

          const grid = document.createElement("div");
          grid.className = "calendar-grid";

          const firstDayOfMonth = new Date(year, month, 1);
          const startWeekday = firstDayOfMonth.getDay();
          const daysInMonth = new Date(year, month + 1, 0).getDate();

          // Empty cells before first day
          for (let i = 0; i < startWeekday; i++) {
            const empty = document.createElement("div");
            grid.appendChild(empty);
          }

          for (let day = 1; day <= daysInMonth; day++) {
            const cellDate = new Date(year, month, day);
            const key = dateKey(cellDate);
            const info = markers[key] || {};

            const cell = document.createElement("div");
            cell.className =
              "calendar-day rounded-md border text-center flex flex-col items-center justify-center";

            let bg = "bg-white";
            let border = "border-gray-200";
            let text = "text-gray-800";

            if (info.period) {
              bg = "bg-red-100";
              border = "border-red-300";
            }
            if (info.fertile) {
              bg = "bg-green-100";
              border = "border-green-300";
            }
            if (info.ovulation) {
              bg = "bg-emerald-600";
              border = "border-emerald-700";
              text = "text-white";
            }

            cell.className += ` ${bg} ${border} ${text}`;

            const daySpan = document.createElement("div");
            daySpan.textContent = day;
            daySpan.className = "font-semibold";

            cell.appendChild(daySpan);

            if (info.ovulation) {
              const badge = document.createElement("div");
              badge.textContent = "Ov";
              badge.className =
                "mt-0.5 text-[0.55rem] font-semibold badge-pill px-1.5 py-0.5 bg-white bg-opacity-80 text-emerald-700";
              cell.appendChild(badge);
            } else if (info.period) {
              const badge = document.createElement("div");
              badge.textContent = "P";
              badge.className =
                "mt-0.5 text-[0.55rem] font-semibold badge-pill px-1.5 py-0.5 bg-red-500 text-white";
              cell.appendChild(badge);
            } else if (info.fertile) {
              const badge = document.createElement("div");
              badge.textContent = "F";
              badge.className =
                "mt-0.5 text-[0.55rem] font-semibold badge-pill px-1.5 py-0.5 bg-green-500 text-white";
              cell.appendChild(badge);
            }

            grid.appendChild(cell);
          }

          monthDiv.appendChild(grid);
          container.appendChild(monthDiv);

          // Next month
          monthCursor = new Date(year, month + 1, 1);
        }
      }

      document
        .getElementById("period-form")
        .addEventListener("submit", function (e) {
          e.preventDefault();

          const lastStartInput =
            document.getElementById("last-period-start").value;
          const cycleLength = parseInt(
            document.getElementById("cycle-length").value,
            10
          );
          const periodLength = parseInt(
            document.getElementById("period-length").value,
            10
          );
          const monthsToPredict = parseInt(
            document.getElementById("months-to-predict").value,
            10
          );

          if (!lastStartInput) {
            alert("Please select the first day of your last period.");
            return;
          }
          if (isNaN(cycleLength) || cycleLength < 15 || cycleLength > 60) {
            alert(
              "Please enter a realistic average cycle length between 15 and 60 days."
            );
            return;
          }
          if (isNaN(periodLength) || periodLength < 1 || periodLength > 10) {
            alert(
              "Please enter a realistic period length between 1 and 10 days."
            );
            return;
          }

          const lastStartDate = new Date(lastStartInput + "T00:00:00");

          const predictions = generatePredictions(
            lastStartDate,
            cycleLength,
            periodLength,
            monthsToPredict
          );
          if (!predictions.length) {
            alert(
              "Unable to generate predictions. Try increasing the months to predict or checking your inputs."
            );
            return;
          }

          const firstPrediction = predictions[0];

          // Update summary
          document.getElementById("next-period-start").textContent = formatDate(
            firstPrediction.periodStart
          );
          document.getElementById("next-period-end").textContent = formatDate(
            firstPrediction.periodEnd
          );
          document.getElementById("ovulation-date").textContent = formatDate(
            firstPrediction.ovulation
          );
          document.getElementById("fertile-start").textContent = formatDate(
            firstPrediction.fertileStart
          );
          document.getElementById("fertile-end").textContent = formatDate(
            firstPrediction.fertileEnd
          );
          document.getElementById("summary-cycle-length").textContent =
            cycleLength;
          document.getElementById("summary-period-length").textContent =
            periodLength;

          document.getElementById("results-section").classList.remove("hidden");

          // Build calendar
          buildCalendar(predictions);
          document
            .getElementById("calendar-section")
            .classList.remove("hidden");

          // Build export text
          const lines = [];
          lines.push("Period & Ovulation Prediction Summary");
          lines.push("-----------------------------------");
          lines.push("Last period start: " + formatDate(lastStartDate));
          lines.push("Average cycle length: " + cycleLength + " days");
          lines.push("Average period length: " + periodLength + " days");
          lines.push("");
          predictions.slice(0, 6).forEach((p, idx) => {
            lines.push("Cycle " + (idx + 1) + ":");
            lines.push(
              "  Period: " +
                formatDate(p.periodStart) +
                " to " +
                formatDate(p.periodEnd)
            );
            lines.push("  Ovulation (estimated): " + formatDate(p.ovulation));
            lines.push(
              "  Fertile window: " +
                formatDate(p.fertileStart) +
                " to " +
                formatDate(p.fertileEnd)
            );
            lines.push("");
          });

          document.getElementById("export-text").value = lines.join("\n");
          document.getElementById("export-section").classList.remove("hidden");
        });

      document
        .getElementById("reset-btn")
        .addEventListener("click", function () {
          document.getElementById("period-form").reset();
          document.getElementById("cycle-length").value = 28;
          document.getElementById("period-length").value = 5;
          document.getElementById("months-to-predict").value = 3;
          document.getElementById("results-section").classList.add("hidden");
          document.getElementById("calendar-section").classList.add("hidden");
          document.getElementById("export-section").classList.add("hidden");
          document.getElementById("calendar-container").innerHTML = "";
          document.getElementById("export-text").value = "";
        });

      document
        .getElementById("export-btn")
        .addEventListener("click", function () {
          const exportText = document.getElementById("export-text");
          if (!exportText.value.trim()) {
            alert("Please run a calculation first.");
            return;
          }
          exportText.focus();
          exportText.select();
          try {
            document.execCommand("copy");
          } catch (e) {
            // ignore; user can copy manually
          }
        });

      // Prefill last-period date with today minus 28 days as a neutral default
      (function initDefaultDate() {
        const input = document.getElementById("last-period-start");
        if (!input.value) {
          const today = new Date();
          const approxLast = addDays(today, -28);
          input.value = dateKey(approxLast);
        }
      })();
    </script>

