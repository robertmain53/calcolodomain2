<script>
  // Basic text stats
  function countWords(text) {
    const trimmed = text.trim();
    if (!trimmed) return 0;
    return trimmed.split(/\s+/).length;
  }

  const inputTextEl = document.getElementById('inputText');
  const referenceTextEl = document.getElementById('referenceText');
  const charCountEl = document.getElementById('charCount');
  const wordCountEl = document.getElementById('wordCount');
  const minPhraseLengthEl = document.getElementById('minPhraseLength');
  const sensitivityEl = document.getElementById('sensitivity');
  const checkBtn = document.getElementById('checkBtn');
  const clearBtn = document.getElementById('clearBtn');

  const resultsSection = document.getElementById('resultsSection');
  const noResultsHint = document.getElementById('noResultsHint');
  const similarityScoreEl = document.getElementById('similarityScore');
  const uniquenessScoreEl = document.getElementById('uniquenessScore');
  const matchCountEl = document.getElementById('matchCount');
  const highlightedOutputEl = document.getElementById('highlightedOutput');
  const matchListEl = document.getElementById('matchList');

  function updateStats() {
    const text = inputTextEl.value;
    charCountEl.textContent = text.length + ' characters';
    wordCountEl.textContent = countWords(text) + ' words';
  }

  inputTextEl.addEventListener('input', updateStats);
  document.addEventListener('DOMContentLoaded', updateStats);

  function tokenize(text) {
    // Keep punctuation as separate tokens but track original spacing
    const tokens = [];
    const regex = /(\s+|[^\s]+)/g;
    let match;
    while ((match = regex.exec(text)) !== null) {
      tokens.push(match[0]);
    }
    return tokens;
  }

  function wordsOnlyTokens(tokens) {
    // Return indices of tokens that are "word-like"
    const wordIndices = [];
    for (let i = 0; i < tokens.length; i++) {
      if (/\S/.test(tokens[i]) && /[A-Za-z0-9]/.test(tokens[i])) {
        wordIndices.push(i);
      }
    }
    return wordIndices;
  }

  function buildNgrams(tokens, wordIndices, n) {
    // Build n-grams over word tokens, but store ranges in full token index space
    const ngrams = new Map(); // key -> array of {startIdx, endIdx}
    for (let i = 0; i + n <= wordIndices.length; i++) {
      const startWordIdx = wordIndices[i];
      const endWordIdx = wordIndices[i + n - 1];
      const startIdx = startWordIdx;
      const endIdx = endWordIdx;
      const phrase = tokens.slice(startIdx, endIdx + 1).join('').toLowerCase();
      if (!ngrams.has(phrase)) {
        ngrams.set(phrase, []);
      }
      ngrams.get(phrase).push({ startIdx, endIdx });
    }
    return ngrams;
  }

  function markMatches(inputTokens, refTokens, minPhraseLength, sensitivityLevel) {
    const inputWordIdx = wordsOnlyTokens(inputTokens);
    const refWordIdx = wordsOnlyTokens(refTokens);

    if (inputWordIdx.length === 0) return { highlights: [], matches: [], matchedWordIndices: new Set() };

    const n = minPhraseLength;
    const inputNgrams = buildNgrams(inputTokens, inputWordIdx, n);
    const refNgrams = buildNgrams(refTokens, refWordIdx, n);

    const highlights = []; // {startIdx, endIdx, strength}
    const matches = [];    // {phrase, count, length}

    const matchedWordIndices = new Set();

    // Determine threshold for "strong" vs "weak" based on sensitivity
    const strongLength = n + (sensitivityLevel === 2 ? 1 : 0); // strict requires slightly longer to be strong

    inputNgrams.forEach((ranges, phrase) => {
      if (!refNgrams.has(phrase)) return;

      const refRanges = refNgrams.get(phrase);
      const totalOccurrences = ranges.length * refRanges.length;

      // Determine strength: longer phrases or multiple occurrences are stronger
      const wordCount = phrase.trim().split(/\s+/).length;
      const strength = (wordCount >= strongLength || totalOccurrences > 1) ? 'strong' : 'weak';

      // For lenient sensitivity, include all; for strict, maybe skip weak very short phrases
      if (sensitivityLevel === 0 && wordCount === n && totalOccurrences === 1) {
        // lenient: skip the weakest possible matches
        return;
      }

      // Record matches summary
      matches.push({
        phrase: phrase,
        count: ranges.length,
        length: wordCount
      });

      // Mark highlight ranges
      ranges.forEach(r => {
        highlights.push({ startIdx: r.startIdx, endIdx: r.endIdx, strength });
        // Mark matched word indices for similarity calculation
        for (let i = r.startIdx; i <= r.endIdx; i++) {
          if (/\S/.test(inputTokens[i]) && /[A-Za-z0-9]/.test(inputTokens[i])) {
            matchedWordIndices.add(i);
          }
        }
      });
    });

    // Merge overlapping highlights, preferring stronger strength
    highlights.sort((a, b) => a.startIdx - b.startIdx || b.endIdx - a.endIdx);
    const merged = [];
    for (const h of highlights) {
      if (!merged.length) {
        merged.push({ ...h });
      } else {
        const last = merged[merged.length - 1];
        if (h.startIdx <= last.endIdx) {
          // overlap
          last.endIdx = Math.max(last.endIdx, h.endIdx);
          if (h.strength === 'strong') last.strength = 'strong';
        } else {
          merged.push({ ...h });
        }
      }
    }

    return { highlights: merged, matches, matchedWordIndices };
  }

  function renderHighlightedText(tokens, highlights) {
    if (!tokens.length) return '';
    if (!highlights.length) {
      return tokens.join('');
    }

    let html = '';
    let current = 0;

    highlights.forEach(h => {
      const { startIdx, endIdx, strength } = h;
      if (startIdx > current) {
        html += tokens.slice(current, startIdx).join('');
      }
      const spanClass = strength === 'strong' ? 'match-highlight' : 'match-partial';
      const segment = tokens.slice(startIdx, endIdx + 1).join('');
      html += `<span class="${spanClass}">${segment}</span>`;
      current = endIdx + 1;
    });

    if (current < tokens.length) {
      html += tokens.slice(current).join('');
    }

    return html;
  }

  function renderMatchList(matches) {
    if (!matches.length) {
      return '<p class="text-gray-500">No overlapping phrases found with the current settings.</p>';
    }
    // Aggregate by phrase
    const map = new Map();
    matches.forEach(m => {
      if (!map.has(m.phrase)) {
        map.set(m.phrase, { phrase: m.phrase, count: 0, length: m.length });
      }
      map.get(m.phrase).count += m.count;
    });

    const aggregated = Array.from(map.values());
    aggregated.sort((a, b) => b.length - a.length || b.count - a.count);

    let html = '';
    aggregated.forEach(m => {
      html += `
        <div class="border border-gray-200 rounded-md p-2">
          <p class="text-gray-800 mb-1">&ldquo;${m.phrase.replace(/"/g, '&quot;')}&rdquo;</p>
          <div class="flex justify-between text-[11px] text-gray-600">
            <span>${m.length} words</span>
            <span>${m.count} occurrence${m.count > 1 ? 's' : ''}</span>
          </div>
        </div>
      `;
    });
    return html;
  }

  function estimateSimilarity(inputTokens, matchedWordIndices) {
    const totalWordIndices = wordsOnlyTokens(inputTokens);
    const totalWords = totalWordIndices.length;
    if (!totalWords) return 0;

    let matchedWords = 0;
    totalWordIndices.forEach(idx => {
      if (matchedWordIndices.has(idx)) matchedWords++;
    });

    return (matchedWords / totalWords) * 100;
  }

  checkBtn.addEventListener('click', () => {
    const inputText = inputTextEl.value || '';
    const refTextRaw = referenceTextEl.value || '';
    const referenceText = refTextRaw.trim() ? refTextRaw : inputText; // self-comparison if no reference

    const minPhraseLength = parseInt(minPhraseLengthEl.value, 10) || 4;
    const sensitivityLevel = parseInt(sensitivityEl.value, 10) || 1;

    if (!inputText.trim()) {
      alert('Please paste or type some text to check.');
      return;
    }

    const inputTokens = tokenize(inputText);
    const refTokens = tokenize(referenceText);

    const { highlights, matches, matchedWordIndices } = markMatches(
      inputTokens,
      refTokens,
      minPhraseLength,
      sensitivityLevel
    );

    const similarity = estimateSimilarity(inputTokens, matchedWordIndices);
    const uniqueness = 100 - similarity;

    similarityScoreEl.textContent = similarity.toFixed(1) + '%';
    uniquenessScoreEl.textContent = uniqueness.toFixed(1) + '%';
    matchCountEl.textContent = matches.length.toString();

    highlightedOutputEl.innerHTML = renderHighlightedText(inputTokens, highlights);
    matchListEl.innerHTML = renderMatchList(matches);

    resultsSection.classList.remove('hidden');
    noResultsHint.classList.toggle('hidden', matches.length > 0);
  });

  clearBtn.addEventListener('click', () => {
    inputTextEl.value = '';
    referenceTextEl.value = '';
    updateStats();
    resultsSection.classList.add('hidden');
    noResultsHint.classList.add('hidden');
    highlightedOutputEl.innerHTML = '';
    matchListEl.innerHTML = '';
    similarityScoreEl.textContent = '0%';
    uniquenessScoreEl.textContent = '100%';
    matchCountEl.textContent = '0';
  });
</script>

