      function parseNumber(value) {
        const n = parseFloat(value);
        return isNaN(n) ? 0 : n;
      }

      function formatCurrency(value) {
        if (!isFinite(value)) return "$0";
        return (
          "$" + value.toLocaleString(undefined, { maximumFractionDigits: 0 })
        );
      }

      function recalc() {
