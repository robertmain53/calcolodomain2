            `input[data-ft="${ft.key}"][data-level="avg"]`
          );
          const highInput = document.querySelector(
            `input[data-ft="${ft.key}"][data-level="high"]`
          );

          const lowCount = parseNumber(lowInput.value);
          const avgCount = parseNumber(avgInput.value);
          const highCount = parseNumber(highInput.value);

          const total =
            lowCount * ft.weights.low +
            avgCount * ft.weights.avg +
            highCount * ft.weights.high;

          ufp += total;
          const cell = document.getElementById(`fp-${ft.key}-total`);
          if (cell) cell.textContent = total.toString();
        });

        // 2. TDI and VAF
        let tdi = 0;
        gscList.forEach((_, idx) => {
          const slider = document.querySelector(
            `input[data-gsc-index="${idx}"]`
          );
          const val = parseNumber(slider.value);
          tdi += val;
          const span = document.getElementById(`gsc-val-${idx}`);
          if (span) span.textContent = val.toString();
        });

        const vaf = 0.65 + 0.01 * tdi;
        const afp = ufp * vaf;

        // 3. Effort & cost
        const hoursPerFP = parseNumber(
          document.getElementById("hours-per-fp").value
        );
        const hourlyRate = parseNumber(
          document.getElementById("hourly-rate").value
        );
        const hoursPerWeek = parseNumber(
          document.getElementById("hours-per-week").value
        );

        const effortHours = afp * hoursPerFP;
        const durationWeeks = hoursPerWeek > 0 ? effortHours / hoursPerWeek : 0;
        const cost = effortHours * hourlyRate;

        // Update UI
        document.getElementById("ufp-result").textContent = ufp.toFixed(0);
        document.getElementById("tdi-value").textContent = tdi.toFixed(0);
        document.getElementById("vaf-value").textContent = vaf.toFixed(2);
        document.getElementById("vaf-result").textContent = vaf.toFixed(2);
        document.getElementById("afp-result").textContent = afp.toFixed(1);
        document.getElementById("effort-hours").textContent =
          effortHours.toFixed(1);
        document.getElementById("duration-weeks").textContent =
          durationWeeks.toFixed(1);
        document.getElementById("estimated-cost").textContent =
          formatCurrency(cost);
      }

      function resetAll() {
        // Reset function counts
        document
          .querySelectorAll('#fp-function-rows input[type="number"]')
          .forEach((inp) => {
            inp.value = "0";
          });
        // Reset GSC
        document
          .querySelectorAll('#gsc-container input[type="range"]')
          .forEach((slider) => {
            slider.value = "0";
          });
        // Reset assumptions
        document.getElementById("hours-per-fp").value = "8";
        document.getElementById("hourly-rate").value = "80";
        document.getElementById("hours-per-week").value = "30";
        recalc();
      }

      document.addEventListener("DOMContentLoaded", () => {
        initFunctionRows();
        initGSC();
        recalc();

        // Attach listeners
        document
          .getElementById("fp-function-rows")
          .addEventListener("input", recalc);
        document
          .getElementById("gsc-container")
          .addEventListener("input", recalc);
        document
          .getElementById("hours-per-fp")
          .addEventListener("input", recalc);
        document
          .getElementById("hourly-rate")
          .addEventListener("input", recalc);
        document
          .getElementById("hours-per-week")
          .addEventListener("input", recalc);
        document
          .getElementById("reset-btn")
          .addEventListener("click", resetAll);
      });

      // MathJax config
      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
