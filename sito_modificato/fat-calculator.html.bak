      // Utility conversions
      function cmFromHeight() {
        const units = document.querySelector(
          'input[name="units"]:checked'
        ).value;
        if (units === "metric") {
          const h = parseFloat(document.getElementById("height-cm").value);
          return isFinite(h) ? h : NaN;
        } else {
          const ft = parseFloat(document.getElementById("height-ft").value);
          const inch = parseFloat(document.getElementById("height-in").value);
          if (!isFinite(ft) || !isFinite(inch)) return NaN;
          return (ft * 12 + inch) * 2.54;
        }
      }

      function kgFromWeight() {
        const units = document.querySelector(
          'input[name="units"]:checked'
        ).value;
        if (units === "metric") {
          const w = parseFloat(document.getElementById("weight-kg").value);
          return isFinite(w) ? w : NaN;
        } else {
          const lb = parseFloat(document.getElementById("weight-lb").value);
          return isFinite(lb) ? lb * 0.45359237 : NaN;
        }
      }

      function circumferenceToCm(value) {
        const units = document.querySelector(
          'input[name="units"]:checked'
        ).value;
        const v = parseFloat(value);
        if (!isFinite(v)) return NaN;
        return units === "metric" ? v : v * 2.54;
      }

      function cmToDisplay(value) {
        const units = document.querySelector(
          'input[name="units"]:checked'
        ).value;
        if (!isFinite(value)) return "–";
        if (units === "metric") {
          return value.toFixed(1) + " kg";
        } else {
          const lb = value * 2.20462262;
          return lb.toFixed(1) + " lb";
        }
      }

      function classifyBodyFat(sex, bf) {
        if (!isFinite(bf))
          return {
            category: "Unknown",
            badgeClass: "bg-gray-100 text-gray-700",
          };
        const s = sex === "female" ? "female" : "male";
        let category;
        if (s === "male") {
          if (bf < 2) category = "Below essential";
          else if (bf <= 5) category = "Essential fat";
          else if (bf <= 13) category = "Athlete";
          else if (bf <= 17) category = "Fitness";
          else if (bf <= 24) category = "Average";
          else category = "Obese";
        } else {
          if (bf < 10) category = "Below essential";
          else if (bf <= 13) category = "Essential fat";
          else if (bf <= 20) category = "Athlete";
          else if (bf <= 24) category = "Fitness";
          else if (bf <= 31) category = "Average";
          else category = "Obese";
        }
        let badgeClass = "bg-green-100 text-green-800";
        if (category === "Obese" || category === "Below essential")
          badgeClass = "bg-red-100 text-red-800";
        else if (category === "Average")
          badgeClass = "bg-yellow-100 text-yellow-800";
        return { category, badgeClass };
      }

      // Method handling
      const methodTabs = document.querySelectorAll(".method-tab");
      const methodPanels = {
        navy: document.getElementById("method-navy"),
        bmi: document.getElementById("method-bmi"),
        skinfold: document.getElementById("method-skinfold"),
      };

      methodTabs.forEach((tab) => {
        tab.addEventListener("click", () => {
          const method = tab.getAttribute("data-method");
          methodTabs.forEach((t) =>
            t.classList.remove("method-tab-active", "text-blue-600")
          );
          tab.classList.add("method-tab-active", "text-blue-600");
          Object.keys(methodPanels).forEach((key) => {
            methodPanels[key].classList.toggle("hidden", key !== method);
          });
          document.getElementById("result-method").textContent =
            method === "navy"
              ? "Navy circumference"
              : method === "bmi"
              ? "BMI estimate"
              : "Skinfold (Jackson–Pollock)";
        });
      });

      // Units toggle
      const unitRadios = document.querySelectorAll(".unit-radio");
      unitRadios.forEach((r) => {
        r.addEventListener("change", () => {
          const units = document.querySelector(
            'input[name="units"]:checked'
          ).value;
          const heightMetric = document.getElementById("height-metric");
          const heightUs = document.getElementById("height-us");
          const weightMetric = document.getElementById("weight-metric");
          const weightUs = document.getElementById("weight-us");
          const neckUnit = document.getElementById("neck-unit");
          const waistUnit = document.getElementById("waist-unit");
          const hipUnit = document.getElementById("hip-unit");

          if (units === "metric") {
            heightMetric.classList.remove("hidden");
            heightUs.classList.add("hidden");
            weightMetric.classList.remove("hidden");
            weightUs.classList.add("hidden");
            neckUnit.textContent = "cm";
            waistUnit.textContent = "cm";
            hipUnit.textContent = "cm";
          } else {
            heightMetric.classList.add("hidden");
            heightUs.classList.remove("hidden");
            weightMetric.classList.add("hidden");
            weightUs.classList.remove("hidden");
            neckUnit.textContent = "in";
            waistUnit.textContent = "in";
            hipUnit.textContent = "in";
          }
        });
      });

      // Sex-dependent fields (hip visibility & skinfold sites)
      const sexSelect = document.getElementById("sex");
      const hipWrapper = document.getElementById("hip-wrapper");
      const skinfoldSitesSelect = document.getElementById("skinfold-sites");
      const skinfoldInputs = document.getElementById("skinfold-inputs");

      function renderSkinfoldInputs() {
        const sex = sexSelect.value;
        const sites = skinfoldSitesSelect.value === "7" ? 7 : 3;
        skinfoldInputs.innerHTML = "";
        let fields = [];
        if (sites === 3) {
          if (sex === "male") {
            fields = ["Chest", "Abdomen", "Thigh"];
          } else {
            fields = ["Triceps", "Suprailiac", "Thigh"];
          }
        } else {
          // 7-site: chest, midaxillary, triceps, subscapular, abdomen, suprailiac, thigh
          fields = [
            "Chest",
            "Midaxillary",
            "Triceps",
            "Subscapular",
            "Abdomen",
            "Suprailiac",
            "Thigh",
          ];
        }
        fields.forEach((name) => {
          const id = "sf-" + name.toLowerCase().replace(/\s+/g, "-");
          const wrapper = document.createElement("div");
          wrapper.innerHTML = `
        <label class="block text-sm font-medium mb-1" for="${id}">${name} skinfold</label>
        <div class="flex gap-2">
          <input id="${id}" type="number" min="1" max="60" class="w-full border border-gray-300 rounded-md px-3 py-2" />
          <span class="self-center text-sm text-gray-500">mm</span>
        </div>
      `;
          skinfoldInputs.appendChild(wrapper);
        });
      }

      sexSelect.addEventListener("change", () => {
        hipWrapper.classList.toggle("hidden", sexSelect.value === "male");
        renderSkinfoldInputs();
      });
      skinfoldSitesSelect.addEventListener("change", renderSkinfoldInputs);

      // Initial setup
      document.getElementById("result-method").textContent =
        "Navy circumference";
      hipWrapper.classList.add("hidden"); // default male
      renderSkinfoldInputs();

      // Calculation
      document.getElementById("calculate-btn").addEventListener("click", () => {
        const errorEl = document.getElementById("error-msg");
        errorEl.textContent = "";
        const sex = sexSelect.value;
        const age = parseFloat(document.getElementById("age").value);
        const heightCm = cmFromHeight();
        const weightKg = kgFromWeight();

        if (!isFinite(age) || age < 10 || age > 100) {
          errorEl.textContent = "Please enter a valid age between 10 and 100.";
          return;
        }
        if (!isFinite(heightCm) || heightCm < 120 || heightCm > 250) {
          errorEl.textContent = "Please enter a realistic height.";
          return;
        }
        if (!isFinite(weightKg) || weightKg < 30 || weightKg > 300) {
          errorEl.textContent = "Please enter a realistic weight.";
          return;
        }

        const activeMethodTab = document.querySelector(".method-tab-active");
        const method = activeMethodTab
          ? activeMethodTab.getAttribute("data-method")
          : "navy";
        let bf = NaN;

        if (method === "navy") {
          const neckCm = circumferenceToCm(
            document.getElementById("neck").value
          );
          const waistCm = circumferenceToCm(
            document.getElementById("waist").value
          );
          const hipCm = circumferenceToCm(document.getElementById("hip").value);

          if (
            !isFinite(neckCm) ||
            !isFinite(waistCm) ||
            neckCm <= 0 ||
            waistCm <= 0
          ) {
            errorEl.textContent =
              "Please enter valid neck and waist measurements.";
            return;
          }
          if (sex === "male") {
            if (waistCm <= neckCm) {
              errorEl.textContent =
                "Waist must be larger than neck for the Navy formula.";
              return;
            }
            const log1 = Math.log10(waistCm - neckCm);
            const log2 = Math.log10(heightCm);
            const density = 1.0324 - 0.19077 * log1 + 0.15456 * log2;
            bf = 495 / density - 450;
          } else {
            if (!isFinite(hipCm) || hipCm <= 0) {
              errorEl.textContent =
                "Please enter a valid hip measurement for women.";
              return;
            }
            if (waistCm + hipCm <= neckCm) {
              errorEl.textContent =
                "Waist + hip must be larger than neck for the Navy formula.";
              return;
            }
            const log1 = Math.log10(waistCm + hipCm - neckCm);
            const log2 = Math.log10(heightCm);
            const density = 1.29579 - 0.35004 * log1 + 0.221 * log2;
            bf = 495 / density - 450;
          }
        } else if (method === "bmi") {
          const heightM = heightCm / 100;
          const bmi = weightKg / (heightM * heightM);
          const S = sex === "male" ? 1 : 0;
          bf = 1.2 * bmi + 0.23 * age - 10.8 * S - 5.4;
        } else if (method === "skinfold") {
          const sites = skinfoldSitesSelect.value === "7" ? 7 : 3;
          const ids = Array.from(skinfoldInputs.querySelectorAll("input")).map(
            (i) => i.id
          );
          let sum = 0;
          for (const id of ids) {
            const v = parseFloat(document.getElementById(id).value);
            if (!isFinite(v) || v <= 0) {
              errorEl.textContent =
                "Please enter all skinfold measurements in millimeters.";
              return;
            }
            sum += v;
          }
          let density;
          if (sites === 3) {
            if (sex === "male") {
              // chest, abdomen, thigh
              density =
                1.10938 -
                0.0008267 * sum +
                0.0000016 * sum * sum -
                0.0002574 * age;
            } else {
              // triceps, suprailiac, thigh
              density =
                1.0994921 -
                0.0009929 * sum +
                0.0000023 * sum * sum -
                0.0001392 * age;
            }
          } else {
            // 7-site
            if (sex === "male") {
              density =
                1.112 -
                0.00043499 * sum +
                0.00000055 * sum * sum -
                0.00028826 * age;
            } else {
              density =
                1.097 -
                0.00046971 * sum +
                0.00000056 * sum * sum -
                0.00012828 * age;
            }
          }
          bf = 495 / density - 450;
        }

        if (!isFinite(bf)) {
          errorEl.textContent =
            "Unable to compute body fat with the provided values.";
          return;
        }

        // Clamp to reasonable range
        bf = Math.max(2, Math.min(60, bf));

        const leanMassKg = weightKg * (1 - bf / 100);
        const fatMassKg = weightKg - leanMassKg;
        const heightM = heightCm / 100;
        const bmi = weightKg / (heightM * heightM);

        document.getElementById("result-bf").textContent = bf.toFixed(1) + " %";
        document.getElementById("result-lean").textContent =
          cmToDisplay(leanMassKg);
        document.getElementById("result-fat-mass").textContent =
          cmToDisplay(fatMassKg);
        document.getElementById("result-bmi").textContent = bmi.toFixed(1);

        const { category, badgeClass } = classifyBodyFat(sex, bf);
        document.getElementById("result-category").textContent = category;
        const badge = document.getElementById("result-badge");
        badge.textContent = category;
        badge.className = "inline-block mt-2 result-badge " + badgeClass;
      });

      document.getElementById("reset-btn").addEventListener("click", () => {
        document.getElementById("age").value = 30;
        document.getElementById("height-cm").value = 175;
        document.getElementById("height-ft").value = 5;
        document.getElementById("height-in").value = 9;
        document.getElementById("weight-kg").value = 75;
        document.getElementById("weight-lb").value = 165;
        document.getElementById("neck").value = 38;
        document.getElementById("waist").value = 85;
        document.getElementById("hip").value = 95;
        skinfoldInputs.querySelectorAll("input").forEach((i) => (i.value = ""));
        document.getElementById("error-msg").textContent = "";
        document.getElementById("result-bf").textContent = "–";
        document.getElementById("result-lean").textContent = "–";
        document.getElementById("result-fat-mass").textContent = "–";
        document.getElementById("result-category").textContent = "–";
        document.getElementById("result-bmi").textContent = "–";
        const badge = document.getElementById("result-badge");
        badge.textContent = "No result yet";
        badge.className =
          "inline-block mt-2 result-badge bg-gray-100 text-gray-700";
      });

      // Mobile menu script (simple)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", () => {
          const menu = document.getElementById("mobile-menu");
          const expanded = menu.classList.contains("hidden") ? "true" : "false";
          menu.classList.toggle("hidden");
          document
            .getElementById("mobile-menu-toggle")
            .setAttribute("aria-expanded", expanded);
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
