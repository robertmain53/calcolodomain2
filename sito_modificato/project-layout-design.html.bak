<script>
  // Basic utilities
  function parseDate(value) {
    if (!value) return null;
    const d = new Date(value + 'T00:00:00');
    return isNaN(d.getTime()) ? null : d;
  }
  function formatDate(d) {
    if (!d) return '';
    const y = d.getFullYear();
    const m = String(d.getMonth() + 1).padStart(2, '0');
    const day = String(d.getDate()).padStart(2, '0');
    return y + '-' + m + '-' + day;
  }
  function daysBetween(start, end) {
    if (!start || !end) return null;
    const ms = end - start;
    return Math.floor(ms / 86400000) + 1;
  }

  const state = {
    tasks: [],
    nextId: 1
  };

  const els = {
    projectName: document.getElementById('project-name'),
    projectStart: document.getElementById('project-start'),
    projectEnd: document.getElementById('project-end'),
    taskName: document.getElementById('task-name'),
    taskPhase: document.getElementById('task-phase'),
    taskOwner: document.getElementById('task-owner'),
    taskStart: document.getElementById('task-start'),
    taskEnd: document.getElementById('task-end'),
    taskTags: document.getElementById('task-tags'),
    taskDependency: document.getElementById('task-dependency'),
    addTaskBtn: document.getElementById('add-task-btn'),
    clearTasksBtn: document.getElementById('clear-tasks-btn'),
    taskError: document.getElementById('task-error'),
    tasksTbody: document.getElementById('tasks-tbody'),
    timelineEmptyMsg: document.getElementById('timeline-empty-msg'),
    timelineContent: document.getElementById('timeline-content'),
    timelineHeader: document.getElementById('timeline-header'),
    timelineRows: document.getElementById('timeline-rows'),
    copySummaryBtn: document.getElementById('copy-summary-btn'),
    exportJsonBtn: document.getElementById('export-json-btn'),
    exportOutput: document.getElementById('export-output'),
    exportStatus: document.getElementById('export-status')
  };

  function showError(msg) {
    els.taskError.textContent = msg;
    els.taskError.classList.remove('hidden');
  }
  function clearError() {
    els.taskError.textContent = '';
    els.taskError.classList.add('hidden');
  }

  function addTask() {
    clearError();
    const name = els.taskName.value.trim();
    const phase = els.taskPhase.value.trim();
    const owner = els.taskOwner.value.trim();
    const start = parseDate(els.taskStart.value);
    const end = parseDate(els.taskEnd.value);
    const tags = els.taskTags.value.trim();
    const dependencyId = els.taskDependency.value ? Number(els.taskDependency.value) : null;

    if (!name) {
      showError('Task name is required.');
      return;
    }
    if (!start || !end) {
      showError('Start and end dates are required.');
      return;
    }
    if (end < start) {
      showError('End date cannot be before start date.');
      return;
    }

    const projectStart = parseDate(els.projectStart.value);
    const projectEnd = parseDate(els.projectEnd.value);
    if (projectStart && start < projectStart) {
      showError('Task start is before the project start date.');
      return;
    }
    if (projectEnd && end > projectEnd) {
      showError('Task end is after the project end date.');
      return;
    }

    const id = state.nextId++;
    state.tasks.push({
      id,
      name,
      phase,
      owner,
      start,
      end,
      tags,
      dependencyId
    });

    els.taskName.value = '';
    els.taskPhase.value = '';
    els.taskOwner.value = '';
    els.taskStart.value = '';
    els.taskEnd.value = '';
    els.taskTags.value = '';
    els.taskDependency.value = '';

    renderAll();
  }

  function deleteTask(id) {
    state.tasks = state.tasks.filter(t => t.id !== id);
    // Remove dependencies pointing to deleted task
    state.tasks.forEach(t => {
      if (t.dependencyId === id) t.dependencyId = null;
    });
    renderAll();
  }

  function updateTaskField(id, field, value) {
    const task = state.tasks.find(t => t.id === id);
    if (!task) return;

    if (field === 'start' || field === 'end') {
      const d = parseDate(value);
      if (!d) return;
      if (field === 'start' && task.end && d > task.end) return;
      if (field === 'end' && task.start && d < task.start) return;
      task[field] = d;
    } else {
      task[field] = value.trim();
    }
    renderAll();
  }

  function renderDependencyOptions() {
    const select = els.taskDependency;
    const current = select.value;
    select.innerHTML = '<option value="">None</option>';
    state.tasks.forEach(t => {
      const opt = document.createElement('option');
      opt.value = String(t.id);
      opt.textContent = `${t.id}. ${t.name}`;
      select.appendChild(opt);
    });
    if (current) select.value = current;
  }

  function computeCriticalPath() {
    // Simple longest-path in DAG based on dependencies
    const tasks = state.tasks;
    const byId = new Map(tasks.map(t => [t.id, t]));
    const memo = new Map();

    function lengthFrom(id) {
      if (memo.has(id)) return memo.get(id);
      const t = byId.get(id);
      if (!t) return 0;
      const dur = daysBetween(t.start, t.end) || 0;
      if (!t.dependencyId) {
        memo.set(id, dur);
        return dur;
      }
      const total = dur + lengthFrom(t.dependencyId);
      memo.set(id, total);
      return total;
    }

    let maxLen = 0;
    let endTaskId = null;
    tasks.forEach(t => {
      const len = lengthFrom(t.id);
      if (len > maxLen) {
        maxLen = len;
        endTaskId = t.id;
      }
    });

    const criticalIds = new Set();
    let cur = endTaskId;
    while (cur != null) {
      criticalIds.add(cur);
      const t = byId.get(cur);
      cur = t && t.dependencyId ? t.dependencyId : null;
    }
    return criticalIds;
  }

  function renderTasksTable() {
    els.tasksTbody.innerHTML = '';
    if (!state.tasks.length) return;

    const criticalIds = computeCriticalPath();

    state.tasks
      .slice()
      .sort((a, b) => a.start - b.start || a.id - b.id)
      .forEach((task, index) => {
        const tr = document.createElement('tr');
        tr.className = index % 2 === 0 ? 'bg-white' : 'bg-gray-50';

        const duration = daysBetween(task.start, task.end);
        const depTask = state.tasks.find(t => t.id === task.dependencyId);
        const isCritical = criticalIds.has(task.id);

        const cells = [
          index + 1,
          task.name,
          task.phase || '',
          task.owner || '',
          formatDate(task.start),
          formatDate(task.end),
          duration != null ? duration : '',
          depTask ? `${depTask.id}. ${depTask.name}` : '',
          task.tags || '',
          isCritical ? 'Yes' : 'No'
        ];

        cells.forEach((val, i) => {
          const td = document.createElement('td');
          td.className = 'px-3 py-2 align-top';
          if ([1,2,3,4,5,8].includes(i)) {
            td.contentEditable = i !== 5 && i !== 4; // allow editing name, phase, owner, tags
            td.textContent = val;
            if (td.isContentEditable) {
              td.classList.add('cursor-text');
              td.addEventListener('blur', () => {
                if (i === 1) updateTaskField(task.id, 'name', td.textContent);
                if (i === 2) updateTaskField(task.id, 'phase', td.textContent);
                if (i === 3) updateTaskField(task.id, 'owner', td.textContent);
                if (i === 8) updateTaskField(task.id, 'tags', td.textContent);
              });
            }
          } else {
            td.textContent = val;
          }
          tr.appendChild(td);
        });

        const actionsTd = document.createElement('td');
        actionsTd.className = 'px-3 py-2 text-right';
        const delBtn = document.createElement('button');
        delBtn.type = 'button';
        delBtn.className = 'text-xs text-red-600 hover:underline';
        delBtn.textContent = 'Delete';
        delBtn.addEventListener('click', () => deleteTask(task.id));
        actionsTd.appendChild(delBtn);
        tr.appendChild(actionsTd);

        els.tasksTbody.appendChild(tr);
      });
  }

  function renderTimeline() {
    const projectStart = parseDate(els.projectStart.value);
    const projectEnd = parseDate(els.projectEnd.value);
    if (!projectStart || !projectEnd || projectEnd < projectStart || !state.tasks.length) {
      els.timelineEmptyMsg.classList.remove('hidden');
      els.timelineContent.classList.add('hidden');
      return;
    }

    const totalDays = daysBetween(projectStart, projectEnd);
    els.timelineEmptyMsg.classList.add('hidden');
    els.timelineContent.classList.remove('hidden');

    // Header
    els.timelineHeader.innerHTML = '';
    const spacer = document.createElement('div');
    spacer.className = 'w-40 flex-shrink-0';
    els.timelineHeader.appendChild(spacer);

    for (let i = 0; i < totalDays; i++) {
      const d = new Date(projectStart.getTime() + i * 86400000);
      const cell = document.createElement('div');
      cell.className = 'flex-shrink-0 w-8 text-center';
      cell.textContent = d.getDate();
      els.timelineHeader.appendChild(cell);
    }

    // Critical path & conflicts
    const criticalIds = computeCriticalPath();

    els.timelineRows.innerHTML = '';
    const sorted = state.tasks.slice().sort((a, b) => a.start - b.start || a.id - b.id);

    sorted.forEach(task => {
      const row = document.createElement('div');
      row.className = 'flex items-center text-xs';

      const label = document.createElement('div');
      label.className = 'w-40 flex-shrink-0 pr-2';
      label.innerHTML = `<div class="font-medium truncate" title="${task.name}">${task.name}</div>
                         <div class="text-gray-500 truncate">${task.phase || ''}</div>`;
      row.appendChild(label);

      const barContainer = document.createElement('div');
      barContainer.className = 'flex-1 flex items-center';

      const offsetDays = Math.max(0, daysBetween(projectStart, task.start) - 1);
      const duration = daysBetween(task.start, task.end);

      if (offsetDays > 0) {
        const spacer = document.createElement('div');
        spacer.style.width = (offsetDays * 2) + 'rem';
        spacer.className = 'flex-shrink-0';
        barContainer.appendChild(spacer);
      }

      const bar = document.createElement('div');
      const isCritical = criticalIds.has(task.id);
      const depTask = state.tasks.find(t => t.id === task.dependencyId);
      let hasConflict = false;
      if (depTask && task.start < depTask.end) {
        hasConflict = true;
      }

      bar.style.width = (duration * 2) + 'rem';
      bar.className = 'flex-shrink-0 h-6 rounded pill flex items-center justify-center text-[10px] text-white';
      if (hasConflict) {
        bar.classList.add('bg-yellow-400');
      } else if (isCritical) {
        bar.classList.add('bg-red-500');
      } else {
        bar.classList.add('bg-blue-500');
      }
      bar.title = `${task.name} (${formatDate(task.start)} → ${formatDate(task.end)})`;
      bar.textContent = duration + 'd';
      barContainer.appendChild(bar);

      row.appendChild(barContainer);
      els.timelineRows.appendChild(row);
    });
  }

  function buildSummaryText() {
    const projectName = els.projectName.value.trim() || 'Untitled project';
    const projectStart = els.projectStart.value || 'N/A';
    const projectEnd = els.projectEnd.value || 'N/A';

    let lines = [];
    lines.push(`Project: ${projectName}`);
    lines.push(`Range: ${projectStart} → ${projectEnd}`);
    lines.push('');
    lines.push('Tasks:');

    const criticalIds = computeCriticalPath();

    state.tasks
      .slice()
      .sort((a, b) => a.start - b.start || a.id - b.id)
      .forEach((t, idx) => {
        const depTask = state.tasks.find(x => x.id === t.dependencyId);
        const duration = daysBetween(t.start, t.end);
        const tags = t.tags ? ` [${t.tags}]` : '';
        const crit = criticalIds.has(t.id) ? ' (CRITICAL)' : '';
        const dep = depTask ? `, depends on: ${depTask.name}` : '';
        lines.push(
          `${idx + 1}. ${t.name}${crit}
   Phase: ${t.phase || '-'}, Owner: ${t.owner || '-'}
   Dates: ${formatDate(t.start)} → ${formatDate(t.end)} (${duration} days)${dep}${tags}`
        );
      });

    return lines.join('\n');
  }

  function exportSummary() {
    const text = buildSummaryText();
    els.exportOutput.value = text;
    if (navigator.clipboard && window.isSecureContext) {
      navigator.clipboard.writeText(text).then(() => {
        els.exportStatus.textContent = 'Summary copied to clipboard.';
        els.exportStatus.classList.remove('hidden');
        setTimeout(() => els.exportStatus.classList.add('hidden'), 2000);
      }).catch(() => {
        els.exportStatus.textContent = 'Summary generated below.';
        els.exportStatus.classList.remove('hidden');
        setTimeout(() => els.exportStatus.classList.add('hidden'), 2000);
      });
    } else {
      els.exportStatus.textContent = 'Summary generated below.';
      els.exportStatus.classList.remove('hidden');
      setTimeout(() => els.exportStatus.classList.add('hidden'), 2000);
    }
  }

  function exportJson() {
    const data = {
      projectName: els.projectName.value.trim() || null,
      projectStart: els.projectStart.value || null,
      projectEnd: els.projectEnd.value || null,
      tasks: state.tasks.map(t => ({
        id: t.id,
        name: t.name,
        phase: t.phase || null,
        owner: t.owner || null,
        start: formatDate(t.start),
        end: formatDate(t.end),
        tags: t.tags || null,
        dependencyId: t.dependencyId
      }))
    };
    const json = JSON.stringify(data, null, 2);
    els.exportOutput.value = json;
    if (navigator.clipboard && window.isSecureContext) {
      navigator.clipboard.writeText(json).then(() => {
        els.exportStatus.textContent = 'JSON copied to clipboard.';
        els.exportStatus.classList.remove('hidden');
        setTimeout(() => els.exportStatus.classList.add('hidden'), 2000);
      }).catch(() => {
        els.exportStatus.textContent = 'JSON generated below.';
        els.exportStatus.classList.remove('hidden');
        setTimeout(() => els.exportStatus.classList.add('hidden'), 2000);
      });
    } else {
      els.exportStatus.textContent = 'JSON generated below.';
      els.exportStatus.classList.remove('hidden');
      setTimeout(() => els.exportStatus.classList.add('hidden'), 2000);
    }
  }

  function clearTasks() {
    if (!state.tasks.length) return;
    if (!confirm('Remove all tasks from this layout?')) return;
    state.tasks = [];
    state.nextId = 1;
    renderAll();
  }

  function renderAll() {
    renderDependencyOptions();
    renderTasksTable();
    renderTimeline();
  }

  els.addTaskBtn.addEventListener('click', addTask);
  els.clearTasksBtn.addEventListener('click', clearTasks);
  els.projectStart.addEventListener('change', renderTimeline);
  els.projectEnd.addEventListener('change', renderTimeline);
  els.copySummaryBtn.addEventListener('click', exportSummary);
  els.exportJsonBtn.addEventListener('click', exportJson);

  // Simple FAQ toggle (in addition to global script_faq if any)
  document.querySelectorAll('#faq-container .faq-question').forEach(btn => {
    btn.addEventListener('click', () => {
      const answer = btn.parentElement.querySelector('.faq-answer');
      const symbol = btn.querySelector('span:last-child');
      const isHidden = answer.classList.contains('hidden');
      document.querySelectorAll('#faq-container .faq-answer').forEach(a => a.classList.add('hidden'));
      document.querySelectorAll('#faq-container .faq-question span:last-child').forEach(s => s.textContent = '+');
      if (isHidden) {
        answer.classList.remove('hidden');
        symbol.textContent = '−';
      }
    });
  });

  // Mobile menu toggle
  document.getElementById('mobile-menu-toggle').addEventListener('click', () => {
    const menu = document.getElementById('mobile-menu');
    const expanded = menu.classList.contains('hidden');
    menu.classList.toggle('hidden');
    document.getElementById('mobile-menu-toggle').setAttribute('aria-expanded', expanded ? 'true' : 'false');
  });
</script>

<script>
  window.MathJax = {
    tex: { inlineMath: [['\\(','\\)'], ['$', '$']], displayMath: [['$','$'], ['\\[','\\]']] },
    svg: { fontCache: 'global' }
  };
</script>
<script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
