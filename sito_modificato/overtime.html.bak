    <script>
      // Mobile menu toggle
      document
        .getElementById("mobile-menu-toggle")
        ?.addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          if (!menu) return;
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          this.setAttribute("aria-expanded", String(isHidden));
        });

      function formatMoney(value) {
        if (isNaN(value)) return "$0.00";
        return "$" + value.toFixed(2);
      }

      const payTypeEl = document.getElementById("pay-type");
      const salaryContainer = document.getElementById("salary-container");
      const salaryPeriodContainer = document.getElementById(
        "salary-period-container"
      );
      const hourlyRateContainer = document.getElementById(
        "hourly-rate-container"
      );
      const hourlyRateEl = document.getElementById("hourly-rate");
      const salaryAmountEl = document.getElementById("salary-amount");
      const salaryPeriodEl = document.getElementById("salary-period");
      const regularHoursEl = document.getElementById("regular-hours");
      const otThresholdEl = document.getElementById("ot-threshold");
      const ot15HoursEl = document.getElementById("ot15-hours");
      const ot20HoursEl = document.getElementById("ot20-hours");
      const customOtHoursEl = document.getElementById("custom-ot-hours");
      const customOtMultiplierEl = document.getElementById(
        "custom-ot-multiplier"
      );

      const resCard = document.getElementById("results-card");
      const resHourlyRate = document.getElementById("res-hourly-rate");
      const resRegularPay = document.getElementById("res-regular-pay");
      const resOt15Pay = document.getElementById("res-ot15-pay");
      const resOt20Pay = document.getElementById("res-ot20-pay");
      const resCustomOtPay = document.getElementById("res-custom-ot-pay");
      const resTotalOtPay = document.getElementById("res-total-ot-pay");
      const resTotalGross = document.getElementById("res-total-gross");
      const resSummary = document.getElementById("res-summary");

      payTypeEl.addEventListener("change", () => {
        const type = payTypeEl.value;
        if (type === "salary") {
          salaryContainer.classList.remove("hidden");
          salaryPeriodContainer.classList.remove("hidden");
          hourlyRateContainer.classList.add("hidden");
        } else {
          salaryContainer.classList.add("hidden");
          salaryPeriodContainer.classList.add("hidden");
          hourlyRateContainer.classList.remove("hidden");
        }
      });

      function getEffectiveHourlyRate() {
        const type = payTypeEl.value;
        if (type === "hourly") {
          const rate = parseFloat(hourlyRateEl.value);
          return isNaN(rate) || rate <= 0 ? null : rate;
        }
        const salary = parseFloat(salaryAmountEl.value);
        const regularHours = parseFloat(regularHoursEl.value);
        if (
          isNaN(salary) ||
          salary <= 0 ||
          isNaN(regularHours) ||
          regularHours <= 0
        )
          return null;

        let weeklySalary;
        switch (salaryPeriodEl.value) {
          case "year":
            weeklySalary = salary / 52;
            break;
          case "month":
            weeklySalary = (salary * 12) / 52;
            break;
          case "biweek":
            weeklySalary = salary / 2;
            break;
          case "week":
          default:
            weeklySalary = salary;
            break;
        }
        return weeklySalary / regularHours;
      }

      document.getElementById("calc-btn").addEventListener("click", () => {
        const hourlyRate = getEffectiveHourlyRate();
        const regularHours = parseFloat(regularHoursEl.value) || 0;
        const otThreshold = parseFloat(otThresholdEl.value) || 0;
        const ot15Hours = parseFloat(ot15HoursEl.value) || 0;
        const ot20Hours = parseFloat(ot20HoursEl.value) || 0;
        const customOtHours = parseFloat(customOtHoursEl.value) || 0;
        const customOtMultiplier = parseFloat(customOtMultiplierEl.value) || 0;

        if (!hourlyRate || hourlyRate <= 0) {
          alert("Please enter a valid hourly rate or salary.");
          return;
        }
        if (regularHours < 0) {
          alert("Regular hours cannot be negative.");
          return;
        }

        const regularPay = hourlyRate * regularHours;
        const ot15Pay = hourlyRate * 1.5 * Math.max(0, ot15Hours);
        const ot20Pay = hourlyRate * 2.0 * Math.max(0, ot20Hours);
        const customPay =
          customOtMultiplier > 0
            ? hourlyRate * customOtMultiplier * Math.max(0, customOtHours)
            : 0;
        const totalOtPay = ot15Pay + ot20Pay + customPay;
        const totalGross = regularPay + totalOtPay;

        resHourlyRate.textContent = formatMoney(hourlyRate);
        resRegularPay.textContent = formatMoney(regularPay);
        resOt15Pay.textContent = formatMoney(ot15Pay);
        resOt20Pay.textContent = formatMoney(ot20Pay);
        resCustomOtPay.textContent = formatMoney(customPay);
        resTotalOtPay.textContent = formatMoney(totalOtPay);
        resTotalGross.textContent = formatMoney(totalGross);

        const totalHours =
          regularHours +
          Math.max(0, ot15Hours) +
          Math.max(0, ot20Hours) +
          Math.max(0, customOtHours);
        let summary = `You worked approximately ${totalHours.toFixed(
          2
        )} hours this week. `;
        if (otThreshold > 0 && totalHours > otThreshold) {
          summary += `That is ${(totalHours - otThreshold).toFixed(
            2
          )} hours above your overtime threshold of ${otThreshold} hours. `;
        }
        summary += `Your estimated overtime pay is ${formatMoney(
          totalOtPay
        )}, for a total gross pay of ${formatMoney(totalGross)}.`;
        resSummary.textContent = summary;

        resCard.classList.remove("hidden");
      });

      document.getElementById("reset-btn").addEventListener("click", () => {
        payTypeEl.value = "hourly";
        salaryAmountEl.value = "";
        salaryPeriodEl.value = "year";
        hourlyRateEl.value = "";
        regularHoursEl.value = "";
        otThresholdEl.value = "40";
        ot15HoursEl.value = "";
        ot20HoursEl.value = "";
        customOtHoursEl.value = "";
        customOtMultiplierEl.value = "";
        salaryContainer.classList.add("hidden");
        salaryPeriodContainer.classList.add("hidden");
        hourlyRateContainer.classList.remove("hidden");
        resCard.classList.add("hidden");
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
