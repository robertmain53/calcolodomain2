    <script>
      (function () {
        const FUR_TO_M = 201.168;
        const inputEl = document.getElementById("input-value");
        const outputEl = document.getElementById("output-value");
        const modeFurToM = document.getElementById("mode-fur-to-m");
        const modeMToFur = document.getElementById("mode-m-to-fur");
        const inputUnitLabel = document.getElementById("input-unit-label");
        const outputUnitLabel = document.getElementById("output-unit-label");
        const inputUnitSuffix = document.getElementById("input-unit-suffix");
        const decimalsRange = document.getElementById("decimals-range");
        const decimalsLabel = document.getElementById("decimals-label");
        const summaryLine = document.getElementById("summary-line");
        const quickButtons = document.querySelectorAll("button[data-quick]");

        let mode = "fur-to-m"; // or 'm-to-fur'

        function formatNumber(value, decimals) {
          if (!isFinite(value)) return "";
          return value.toFixed(decimals);
        }

        function updateSummary(inputVal, resultVal) {
          if (inputVal === "" || isNaN(parseFloat(inputVal))) {
            summaryLine.textContent = "1 furlong = 201.168 meters";
            return;
          }
          const v = parseFloat(inputVal);
          if (mode === "fur-to-m") {
            summaryLine.textContent =
              v +
              " furlong" +
              (v === 1 ? "" : "s") +
              " = " +
              resultVal +
              " meters";
          } else {
            summaryLine.textContent =
              v +
              " meter" +
              (v === 1 ? "" : "s") +
              " = " +
              resultVal +
              " furlongs";
          }
        }

        function recalc() {
          const decimals = parseInt(decimalsRange.value, 10);
          const raw = inputEl.value.trim();
          if (raw === "" || isNaN(parseFloat(raw))) {
            outputEl.value = "";
            updateSummary("", "");
            return;
          }
          const val = parseFloat(raw);
          let result;
          if (mode === "fur-to-m") {
            result = val * FUR_TO_M;
          } else {
            result = val / FUR_TO_M;
          }
          const formatted = formatNumber(result, decimals);
          outputEl.value = formatted;
          updateSummary(raw, formatted);
        }

        function setMode(newMode) {
          mode = newMode;
          if (mode === "fur-to-m") {
            modeFurToM.classList.add("toggle-active");
            modeFurToM.classList.remove("toggle-inactive");
            modeMToFur.classList.add("toggle-inactive");
            modeMToFur.classList.remove("toggle-active");
            inputUnitLabel.textContent = "(furlongs)";
            outputUnitLabel.textContent = "(meters)";
            inputUnitSuffix.textContent = "fur";
            inputEl.placeholder = "e.g. 6";
          } else {
            modeMToFur.classList.add("toggle-active");
            modeMToFur.classList.remove("toggle-inactive");
            modeFurToM.classList.add("toggle-inactive");
            modeFurToM.classList.remove("toggle-active");
            inputUnitLabel.textContent = "(meters)";
            outputUnitLabel.textContent = "(furlongs)";
            inputUnitSuffix.textContent = "m";
            inputEl.placeholder = "e.g. 1200";
          }
          recalc();
        }

        modeFurToM.addEventListener("click", function () {
          setMode("fur-to-m");
        });
        modeMToFur.addEventListener("click", function () {
          setMode("m-to-fur");
        });

        inputEl.addEventListener("input", recalc);
        decimalsRange.addEventListener("input", function () {
          decimalsLabel.textContent = decimalsRange.value;
          recalc();
        });

        quickButtons.forEach((btn) => {
          btn.addEventListener("click", () => {
            inputEl.value = btn.getAttribute("data-quick");
            recalc();
            inputEl.focus();
          });
        });

        // Initialize
        decimalsLabel.textContent = decimalsRange.value;
        recalc();
      })();
    </script>

