    <script>
      // HSA calculator logic
      function formatMoney(value) {
        if (!isFinite(value)) return "$–";
        const abs = Math.abs(value);
        const opts =
          abs >= 100000
            ? { maximumFractionDigits: 0 }
            : { maximumFractionDigits: 2 };
        return "$" + value.toLocaleString(undefined, opts);
      }

      function calcHsa() {
        const age = Number(document.getElementById("hsa-age").value) || 0;
        const retireAge =
          Number(document.getElementById("hsa-retire-age").value) || 0;
        const taxRate =
          (Number(document.getElementById("hsa-tax-rate").value) || 0) / 100;
        const balance0 =
          Number(document.getElementById("hsa-current-balance").value) || 0;
        const contribEmployee =
          Number(document.getElementById("hsa-employee-annual").value) || 0;
        const contribEmployer =
          Number(document.getElementById("hsa-employer-annual").value) || 0;
        const annualExpenses =
          Number(document.getElementById("hsa-annual-expenses").value) || 0;
        const r =
          (Number(document.getElementById("hsa-return-rate").value) || 0) / 100;
        const includeCatchup = document.getElementById("hsa-catchup").checked;
        const coverage =
          document.querySelector("input.hsa-coverage:checked")?.value || "self";

        const warningEl = document.getElementById("hsa-warning");
        warningEl.classList.add("hidden");
        warningEl.textContent = "";

        if (retireAge <= age) {
          warningEl.textContent =
            "Retirement age must be greater than current age.";
          warningEl.classList.remove("hidden");
          return;
        }

        const years = retireAge - age;
        const annualContribBase = contribEmployee + contribEmployer;

        // Simple soft check vs typical limits (not exact IRS values)
        let softLimit = coverage === "family" ? 8000 : 4500;
        if (includeCatchup && age >= 55) softLimit += 1000;
        if (annualContribBase > softLimit * 1.2) {
          warningEl.textContent =
            "Your annual HSA contribution is significantly above typical IRS limits. This is allowed for “what‑if” modeling, but may not be permitted in real life.";
          warningEl.classList.remove("hidden");
        }

        let balance = balance0;
        let totalContrib = 0;
        let totalWithdrawals = 0;
        let totalGrowth = 0;
        let projectionRows = "";

        for (let i = 0; i < years; i++) {
          const yearIndex = i + 1;
          const startBalance = balance;

          // Simple catch-up logic: assume catch-up allowed from age 55 onward
          let yearContrib = annualContribBase;
          const thisAge = age + i;
          if (includeCatchup && thisAge >= 55) {
            yearContrib += 1000;
          }

          const withdrawals = Math.min(
            annualExpenses,
            startBalance + yearContrib
          );
          const preGrowth = startBalance + yearContrib - withdrawals;
          const growth = preGrowth * r;
          const endBalance = preGrowth + growth;

          totalContrib += yearContrib;
          totalWithdrawals += withdrawals;
          totalGrowth += growth;
          balance = endBalance;

          projectionRows += `
        <tr class="odd:bg-white even:bg-gray-50">
          <td class="px-2 py-1 border-b">${yearIndex}</td>
          <td class="px-2 py-1 border-b text-right">${formatMoney(
            startBalance
          )}</td>
          <td class="px-2 py-1 border-b text-right">${formatMoney(
            yearContrib
          )}</td>
          <td class="px-2 py-1 border-b text-right">${formatMoney(
            withdrawals
          )}</td>
          <td class="px-2 py-1 border-b text-right">${formatMoney(growth)}</td>
          <td class="px-2 py-1 border-b text-right">${formatMoney(
            endBalance
          )}</td>
        </tr>`;
        }

        const annualTaxSavings = contribEmployee * taxRate;
        const totalTaxSavings = annualTaxSavings * years;

        document.getElementById("hsa-years").textContent = years.toString();
        document.getElementById("hsa-total-contrib").textContent =
          formatMoney(totalContrib);
        document.getElementById("hsa-investment-growth").textContent =
          formatMoney(totalGrowth);
        document.getElementById("hsa-total-withdrawals").textContent =
          formatMoney(totalWithdrawals);
        document.getElementById("hsa-final-balance").textContent =
          formatMoney(balance);
        document.getElementById("hsa-annual-tax-savings").textContent =
          formatMoney(annualTaxSavings);
        document.getElementById(
          "hsa-total-tax-savings"
        ).textContent = `≈ ${formatMoney(
          totalTaxSavings
        )} total through retirement`;

        document.getElementById("hsa-projection-body").innerHTML =
          projectionRows;
        document
          .getElementById("hsa-results-section")
          .classList.remove("hidden");
      }

      document.addEventListener("DOMContentLoaded", () => {
        const btn = document.getElementById("hsa-calc-btn");
        if (btn) {
          btn.addEventListener("click", calcHsa);
        }
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
