    <script>
      // Utility: normalize card number (digits only)
      function normalizeCardNumber(input) {
        return (input || "").replace(/[^0-9]/g, "");
      }

      // Luhn check
      function luhnCheck(num) {
        if (!num || !/^[0-9]+$/.test(num)) return false;
        let sum = 0;
        let shouldDouble = false;
        for (let i = num.length - 1; i >= 0; i--) {
          let digit = parseInt(num.charAt(i), 10);
          if (shouldDouble) {
            digit *= 2;
            if (digit > 9) digit -= 9;
          }
          sum += digit;
          shouldDouble = !shouldDouble;
        }
        return sum % 10 === 0;
      }

      // Brand detection based on IIN/BIN ranges and length
      function detectBrand(num) {
        const len = num.length;
        const first2 = parseInt(num.slice(0, 2), 10);
        const first3 = parseInt(num.slice(0, 3), 10);
        const first4 = parseInt(num.slice(0, 4), 10);
        const first6 = parseInt(num.slice(0, 6), 10);

        // Visa
        if (num.startsWith("4") && [13, 16, 19].includes(len)) {
          return "Visa";
        }

        // Mastercard (51–55, 2221–2720)
        if (
          len === 16 &&
          ((first2 >= 51 && first2 <= 55) || (first4 >= 2221 && first4 <= 2720))
        ) {
          return "Mastercard";
        }

        // American Express
        if (len === 15 && (first2 === 34 || first2 === 37)) {
          return "American Express";
        }

        // Discover
        if (
          len >= 16 &&
          len <= 19 &&
          (num.startsWith("6011") ||
            num.startsWith("65") ||
            (first3 >= 644 && first3 <= 649))
        ) {
          return "Discover";
        }

        // JCB
        if (len === 16 && first4 >= 3528 && first4 <= 3589) {
          return "JCB";
        }

        // Diners Club (classic ranges)
        if (
          (len === 14 || len === 16) &&
          ((first3 >= 300 && first3 <= 305) ||
            first2 === 36 ||
            (first2 >= 38 && first2 <= 39))
        ) {
          return "Diners Club";
        }

        // UnionPay (very broad, simplified)
        if (len >= 16 && len <= 19 && num.startsWith("62")) {
          return "UnionPay";
        }

        // Maestro (simplified)
        if (
          len >= 12 &&
          len <= 19 &&
          (["50", "56", "57", "58"].includes(num.slice(0, 2)) ||
            ["6304", "6759", "6761", "6762", "6763"].includes(num.slice(0, 4)))
        ) {
          return "Maestro";
        }

        return "Unknown / Other";
      }

      // Length rules per brand (simplified)
      function checkLengthForBrand(num, brand) {
        const len = num.length;
        switch (brand) {
          case "Visa":
            return [13, 16, 19].includes(len);
          case "Mastercard":
            return len === 16;
          case "American Express":
            return len === 15;
          case "Discover":
            return len >= 16 && len <= 19;
          case "JCB":
            return len === 16;
          case "Diners Club":
            return len === 14 || len === 16;
          case "UnionPay":
            return len >= 16 && len <= 19;
          case "Maestro":
            return len >= 12 && len <= 19;
          default:
            // For unknown brands, require at least 12 digits
            return len >= 12 && len <= 19;
        }
      }

      function updateStatusElement(el, badgeText, badgeColorClasses, message) {
        const badge = el.querySelector(".badge");
        const msgSpan = el.querySelector("span:nth-child(2)");
        if (badge) {
          badge.className = "badge " + badgeColorClasses;
          badge.textContent = badgeText;
        }
        if (msgSpan) {
          msgSpan.textContent = message || "";
        }
      }

      function validateCard() {
        const inputEl = document.getElementById("card-number-input");
        const raw = inputEl.value || "";
        const normalized = normalizeCardNumber(raw);

        const brandEl = document.getElementById("card-brand-output");
        const binEl = document.getElementById("card-bin-output");
        const luhnEl = document.getElementById("luhn-status");
        const lengthEl = document.getElementById("length-status");
        const overallEl = document.getElementById("overall-status");

        const advPan = document.getElementById("advanced-pan");
        const advLen = document.getElementById("advanced-length");
        const advPrefix = document.getElementById("advanced-prefix");

        if (!normalized) {
          brandEl.textContent = "–";
          binEl.textContent = "–";
          updateStatusElement(
            luhnEl,
            "Not checked",
            "bg-gray-200 text-gray-700",
            ""
          );
          updateStatusElement(
            lengthEl,
            "Not checked",
            "bg-gray-200 text-gray-700",
            ""
          );
          updateStatusElement(
            overallEl,
            "Waiting",
            "bg-gray-200 text-gray-700",
            ""
          );
          advPan.innerHTML =
            '<span class="font-semibold">Normalized PAN:</span> –';
          advLen.innerHTML =
            '<span class="font-semibold">Digit count:</span> –';
          advPrefix.innerHTML =
            '<span class="font-semibold">Leading digits:</span> –';
          return;
        }

        // Advanced info
        advPan.innerHTML =
          '<span class="font-semibold">Normalized PAN:</span> ' + normalized;
        advLen.innerHTML =
          '<span class="font-semibold">Digit count:</span> ' +
          normalized.length;
        advPrefix.innerHTML =
          '<span class="font-semibold">Leading digits:</span> ' +
          normalized.slice(0, 4) +
          (normalized.length > 4 ? "…" : "");

        // Brand & BIN
        const brand = detectBrand(normalized);
        brandEl.textContent = brand;
        binEl.textContent =
          normalized.length >= 6 ? normalized.slice(0, 6) : "Not enough digits";

        // Luhn
        const luhnValid = luhnCheck(normalized);
        if (luhnValid) {
          updateStatusElement(
            luhnEl,
            "Valid",
            "bg-green-100 text-green-800",
            "Number passes the Luhn checksum."
          );
        } else {
          updateStatusElement(
            luhnEl,
            "Invalid",
            "bg-red-100 text-red-800",
            "Number fails the Luhn checksum (likely a typo or test value)."
          );
        }

        // Length & format
        const lengthOk = checkLengthForBrand(normalized, brand);
        if (lengthOk) {
          updateStatusElement(
            lengthEl,
            "OK",
            "bg-green-100 text-green-800",
            `Length ${normalized.length} is allowed for ${
              brand === "Unknown / Other" ? "this number" : brand
            }.`
          );
        } else {
          updateStatusElement(
            lengthEl,
            "Unexpected",
            "bg-yellow-100 text-yellow-800",
            `Length ${normalized.length} is unusual for ${
              brand === "Unknown / Other" ? "this number" : brand
            }.`
          );
        }

        // Overall
        if (luhnValid && lengthOk) {
          updateStatusElement(
            overallEl,
            "Structurally valid",
            "bg-green-600 text-white",
            "The number looks structurally valid, but only a payment gateway can confirm if it is usable."
          );
        } else if (!luhnValid && lengthOk) {
          updateStatusElement(
            overallEl,
            "Likely invalid",
            "bg-red-600 text-white",
            "Fails Luhn checksum; check for typos or missing digits."
          );
        } else {
          updateStatusElement(
            overallEl,
            "Suspicious",
            "bg-yellow-600 text-white",
            "Both length and checksum look unusual; this is probably not a real card number."
          );
        }
      }

      document.addEventListener("DOMContentLoaded", function () {
        const inputEl = document.getElementById("card-number-input");
        const validateBtn = document.getElementById("validate-btn");

        // Format input as user types: group digits in blocks of 4 for readability
        inputEl.addEventListener("input", function (e) {
          const cursorPos = inputEl.selectionStart;
          const before = inputEl.value;
          const digits = before.replace(/[^0-9]/g, "");
          const grouped = digits.replace(/(.{4})/g, "$1 ").trim();
          inputEl.value = grouped;

          // Basic cursor handling: move to end if formatting changed length significantly
          if (document.activeElement === inputEl) {
            const diff = grouped.length - before.length;
            inputEl.selectionStart = inputEl.selectionEnd = cursorPos + diff;
          }
        });

        // Validate on button click
        validateBtn.addEventListener("click", function () {
          validateCard();
        });

        // Validate on Enter key
        inputEl.addEventListener("keydown", function (e) {
          if (e.key === "Enter") {
            e.preventDefault();
            validateCard();
          }
        });

        // Test card buttons
        document.querySelectorAll(".test-card-btn").forEach(function (btn) {
          btn.addEventListener("click", function () {
            const value = btn.getAttribute("data-test-card") || "";
            inputEl.value = value;
            inputEl.dispatchEvent(new Event("input"));
            validateCard();
          });
        });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
