    <script>
      (function () {
        const PC_TO_LY = 3.26156;

        const pcInput = document.getElementById("pc-input");
        const lyInput = document.getElementById("ly-input");
        const swapBtn = document.getElementById("swap-btn");
        const clearBtn = document.getElementById("clear-btn");
        const resultSummary = document.getElementById("result-summary");
        const tableBody = document.getElementById("quick-table-body");

        let isUpdating = false;

        function formatNumber(value) {
          if (!isFinite(value)) return "";
          const abs = Math.abs(value);
          if (abs !== 0 && (abs < 0.001 || abs >= 1e6)) {
            return value.toExponential(6);
          }
          return value.toLocaleString(undefined, {
            maximumFractionDigits: 8,
            useGrouping: true,
          });
        }

        function updateFromPc() {
          if (isUpdating) return;
          const pcVal = parseFloat(pcInput.value);
          if (isNaN(pcVal)) {
            lyInput.value = "";
            resultSummary.classList.add("hidden");
            return;
          }
          isUpdating = true;
          const lyVal = pcVal * PC_TO_LY;
          lyInput.value = lyVal === 0 ? "0" : lyVal.toString();
          showSummary(pcVal, lyVal, "pc");
          isUpdating = false;
        }

        function updateFromLy() {
          if (isUpdating) return;
          const lyVal = parseFloat(lyInput.value);
          if (isNaN(lyVal)) {
            pcInput.value = "";
            resultSummary.classList.add("hidden");
            return;
          }
          isUpdating = true;
          const pcVal = lyVal / PC_TO_LY;
          pcInput.value = pcVal === 0 ? "0" : pcVal.toString();
          showSummary(pcVal, lyVal, "ly");
          isUpdating = false;
        }

        function showSummary(pcVal, lyVal, source) {
          const pcStr = formatNumber(pcVal);
          const lyStr = formatNumber(lyVal);
          if (source === "pc") {
            resultSummary.textContent = `${pcStr} parsec${
              Math.abs(pcVal) === 1 ? "" : "s"
            } ≈ ${lyStr} light year${Math.abs(lyVal) === 1 ? "" : "s"}.`;
          } else {
            resultSummary.textContent = `${lyStr} light year${
              Math.abs(lyVal) === 1 ? "" : "s"
            } ≈ ${pcStr} parsec${Math.abs(pcVal) === 1 ? "" : "s"}.`;
          }
          resultSummary.classList.remove("hidden");
        }

        function swapValues() {
          const pcVal = pcInput.value;
          const lyVal = lyInput.value;
          pcInput.value = lyVal;
          lyInput.value = pcVal;
          if (lyVal !== "") {
            updateFromPc();
          } else if (pcVal !== "") {
            updateFromLy();
          } else {
            resultSummary.classList.add("hidden");
          }
        }

        function clearValues() {
          pcInput.value = "";
          lyInput.value = "";
          resultSummary.classList.add("hidden");
          pcInput.focus();
        }

        function buildQuickTable() {
          const rows = [0.1, 0.5, 1, 2, 5, 10, 50, 100, 1000];
          const fragment = document.createDocumentFragment();
          rows.forEach((pc) => {
            const tr = document.createElement("tr");
            const ly = pc * PC_TO_LY;
            tr.innerHTML = `
          <td class="px-3 py-2 whitespace-nowrap">${formatNumber(pc)} pc</td>
          <td class="px-3 py-2 whitespace-nowrap">${formatNumber(ly)} ly</td>
        `;
            fragment.appendChild(tr);
          });
          tableBody.appendChild(fragment);
        }

        pcInput.addEventListener("input", updateFromPc);
        lyInput.addEventListener("input", updateFromLy);
        swapBtn.addEventListener("click", swapValues);
        clearBtn.addEventListener("click", clearValues);

        buildQuickTable();
      })();
    </script>

