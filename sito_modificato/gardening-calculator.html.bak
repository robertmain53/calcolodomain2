    <script>
      // Tab handling
      const tabButtons = document.querySelectorAll(".tab-btn");
      const tabPanels = {
        spacing: document.getElementById("tab-spacing"),
        soil: document.getElementById("tab-soil"),
        squarefoot: document.getElementById("tab-squarefoot"),
      };

      function activateTab(name) {
        tabButtons.forEach((btn) => {
          if (btn.dataset.tab === name) {
            btn.classList.add("tab-active", "border-blue-600", "text-blue-700");
          } else {
            btn.classList.remove(
              "tab-active",
              "border-blue-600",
              "text-blue-700"
            );
          }
        });
        Object.keys(tabPanels).forEach((key) => {
          tabPanels[key].classList.toggle("hidden", key !== name);
        });
      }

      tabButtons.forEach((btn) => {
        btn.addEventListener("click", () => activateTab(btn.dataset.tab));
      });
      // Default tab
      activateTab("spacing");

      // Crop presets: spacing in inches and yield per plant (lb)
      const cropPresets = {
        lettuce: {
          inRowIn: 12,
          betweenRowIn: 12,
          yield: 0.75,
          yieldUnit: "lb",
        },
        carrot: { inRowIn: 3, betweenRowIn: 3, yield: 0.12, yieldUnit: "lb" },
        tomato: { inRowIn: 24, betweenRowIn: 30, yield: 10, yieldUnit: "lb" },
        pepper: { inRowIn: 18, betweenRowIn: 18, yield: 3, yieldUnit: "lb" },
        bean: { inRowIn: 6, betweenRowIn: 18, yield: 0.25, yieldUnit: "lb" },
        cucumber: { inRowIn: 18, betweenRowIn: 24, yield: 4, yieldUnit: "lb" },
        onion: { inRowIn: 4, betweenRowIn: 4, yield: 0.25, yieldUnit: "lb" },
        cabbage: { inRowIn: 18, betweenRowIn: 18, yield: 2.5, yieldUnit: "lb" },
      };

      const cropSelect = document.getElementById("crop-select");
      const spacingUnitEl = document.getElementById("spacing-unit");
      const spacingInRowEl = document.getElementById("spacing-inrow");
      const spacingBetweenRowEl = document.getElementById("spacing-betweenrow");
      const yieldPerPlantEl = document.getElementById("yield-per-plant");
      const yieldUnitEl = document.getElementById("yield-unit");

      cropSelect.addEventListener("change", () => {
        const key = cropSelect.value;
        if (!key || !cropPresets[key]) return;
        const preset = cropPresets[key];

        // If current spacing unit is cm, convert inches to cm
        if (spacingUnitEl.value === "in") {
          spacingInRowEl.value = preset.inRowIn;
          spacingBetweenRowEl.value = preset.betweenRowIn;
        } else {
          spacingInRowEl.value = (preset.inRowIn * 2.54).toFixed(1);
          spacingBetweenRowEl.value = (preset.betweenRowIn * 2.54).toFixed(1);
        }

        // Yield per plant
        yieldPerPlantEl.value = preset.yield;
        yieldUnitEl.value = preset.yieldUnit;
      });

      // Spacing calculation
      document
        .getElementById("btn-calc-spacing")
        .addEventListener("click", () => {
          const bedUnit = document.getElementById("bed-unit").value;
          const bedLength = parseFloat(
            document.getElementById("bed-length").value
          );
          const bedWidth = parseFloat(
            document.getElementById("bed-width").value
          );
          const spacingUnit = spacingUnitEl.value;
          const inRow = parseFloat(spacingInRowEl.value);
          const betweenRow = parseFloat(spacingBetweenRowEl.value);
          const yieldPerPlant = parseFloat(yieldPerPlantEl.value);
          const yieldUnit = yieldUnitEl.value;

          if (!(bedLength > 0 && bedWidth > 0 && inRow > 0 && betweenRow > 0)) {
            alert("Please enter positive values for bed size and spacing.");
            return;
          }

          // Convert bed dimensions to feet
          let bedLengthFt = bedLength;
          let bedWidthFt = bedWidth;
          if (bedUnit === "m") {
            bedLengthFt = bedLength * 3.28084;
            bedWidthFt = bedWidth * 3.28084;
          }

          // Convert spacing to feet
          let inRowFt = inRow;
          let betweenRowFt = betweenRow;
          if (spacingUnit === "in") {
            inRowFt = inRow / 12;
            betweenRowFt = betweenRow / 12;
          } else {
            // cm
            inRowFt = inRow / 30.48;
            betweenRowFt = betweenRow / 30.48;
          }

          const bedAreaSqFt = bedLengthFt * bedWidthFt;

          const plantsPerRow = Math.floor(bedLengthFt / inRowFt);
          const rows = Math.floor(bedWidthFt / betweenRowFt);
          const totalPlants = plantsPerRow * rows;

          const plantsPerSqFt = bedAreaSqFt > 0 ? totalPlants / bedAreaSqFt : 0;

          // Yield
          let totalYieldText = "–";
          let yieldPerPlantText = "–";
          if (yieldPerPlant > 0 && totalPlants > 0) {
            const totalYield = yieldPerPlant * totalPlants;
            yieldPerPlantText = `${yieldPerPlant.toFixed(
              2
            )} ${yieldUnit} / plant`;
            totalYieldText = `${totalYield.toFixed(1)} ${yieldUnit}`;
          }

          // Update UI
          document.getElementById(
            "res-bed-area"
          ).textContent = `${bedAreaSqFt.toFixed(1)} sq ft`;
          document.getElementById("res-plants-per-row").textContent =
            plantsPerRow > 0 ? plantsPerRow : "0";
          document.getElementById("res-rows").textContent =
            rows > 0 ? rows : "0";
          document.getElementById("res-total-plants").textContent =
            totalPlants > 0 ? totalPlants : "0";
          document.getElementById("res-plants-per-sqft").textContent =
            plantsPerSqFt > 0 ? plantsPerSqFt.toFixed(2) : "0";

          document.getElementById("res-yield-per-plant").textContent =
            yieldPerPlantText;
          document.getElementById("res-total-yield").textContent =
            totalYieldText;
        });

      // Soil & mulch calculation
      document.getElementById("btn-calc-soil").addEventListener("click", () => {
        const bedUnit = document.getElementById("soil-bed-unit").value;
        const length = parseFloat(
          document.getElementById("soil-bed-length").value
        );
        const width = parseFloat(
          document.getElementById("soil-bed-width").value
        );
        const depthUnit = document.getElementById("soil-depth-unit").value;
        const depth = parseFloat(document.getElementById("soil-depth").value);
        const bagSize = parseFloat(document.getElementById("bag-size").value);

        if (!(length > 0 && width > 0 && depth > 0)) {
          alert("Please enter positive values for bed dimensions and depth.");
          return;
        }

        // Convert length & width to feet
        let lengthFt = length;
        let widthFt = width;
        if (bedUnit === "m") {
          lengthFt = length * 3.28084;
          widthFt = width * 3.28084;
        }
        const areaSqFt = lengthFt * widthFt;

        // Convert depth to feet
        let depthFt;
        if (depthUnit === "in") {
          depthFt = depth / 12;
        } else {
          depthFt = depth / 30.48;
        }

        const volumeFt3 = areaSqFt * depthFt;
        const volumeYd3 = volumeFt3 / 27;

        // Convert to cubic meters and liters
        const volumeM3 = volumeFt3 * 0.0283168;
        const volumeL = volumeM3 * 1000;

        // Bags
        let bagsNeeded = "–";
        let bagSizeText = "–";
        if (bagSize > 0) {
          // We don't know if bagSize is in L or ft³, so show both interpretations
          const bagsIfFt3 = Math.ceil(volumeFt3 / bagSize);
          const bagsIfL = Math.ceil(volumeL / bagSize);
          bagsNeeded = `${bagsIfFt3} bags if ${bagSize} ft³ each, or ${bagsIfL} bags if ${bagSize} L each`;
          bagSizeText = `${bagSize} (interpreted as ft³ or L)`;
        }

        document.getElementById(
          "soil-res-area"
        ).textContent = `${areaSqFt.toFixed(1)} sq ft`;
        document.getElementById(
          "soil-res-depth"
        ).textContent = `${depth.toFixed(1)} ${depthUnit}`;
        document.getElementById(
          "soil-res-volume"
        ).textContent = `${volumeFt3.toFixed(1)} ft³  ≈ ${volumeYd3.toFixed(
          2
        )} yd³  ≈ ${volumeL.toFixed(0)} L`;

        document.getElementById("soil-res-bag-size").textContent = bagSizeText;
        document.getElementById("soil-res-bags").textContent = bagsNeeded;
        document.getElementById(
          "soil-res-bulk"
        ).textContent = `${volumeYd3.toFixed(
          2
        )} cubic yards  (${volumeM3.toFixed(2)} m³)`;
      });

      // Square-foot gardening calculation
      document.getElementById("btn-calc-sf").addEventListener("click", () => {
        const lengthFt = parseFloat(document.getElementById("sf-length").value);
        const widthFt = parseFloat(document.getElementById("sf-width").value);
        const plantsPerSq = parseInt(
          document.getElementById("sf-crop").value,
          10
        );

        if (!(lengthFt > 0 && widthFt > 0 && plantsPerSq > 0)) {
          alert(
            "Please enter positive values for bed size and plants per square."
          );
          return;
        }

        const areaSqFt = lengthFt * widthFt;
        const lenSquares = Math.floor(lengthFt);
        const widSquares = Math.floor(widthFt);
        const totalSquares = lenSquares * widSquares;
        const totalPlants = totalSquares * plantsPerSq;

        document.getElementById(
          "sf-res-area"
        ).textContent = `${areaSqFt.toFixed(1)} sq ft`;
        document.getElementById("sf-res-len-sq").textContent = lenSquares;
        document.getElementById("sf-res-wid-sq").textContent = widSquares;
        document.getElementById("sf-res-total-sq").textContent = totalSquares;
        document.getElementById("sf-res-plants-per-sq").textContent =
          plantsPerSq;
        document.getElementById("sf-res-total-plants").textContent =
          totalPlants;
      });

      // Keep soil tab bed size in sync with spacing tab by default
      function syncSoilBedFromSpacing() {
        const bedUnit = document.getElementById("bed-unit").value;
        const length = parseFloat(document.getElementById("bed-length").value);
        const width = parseFloat(document.getElementById("bed-width").value);
        document.getElementById("soil-bed-unit").value = bedUnit;
        if (length > 0)
          document.getElementById("soil-bed-length").value = length;
        if (width > 0) document.getElementById("soil-bed-width").value = width;
      }
      // Initial sync
      syncSoilBedFromSpacing();
      document
        .getElementById("bed-length")
        .addEventListener("input", syncSoilBedFromSpacing);
      document
        .getElementById("bed-width")
        .addEventListener("input", syncSoilBedFromSpacing);
      document
        .getElementById("bed-unit")
        .addEventListener("change", syncSoilBedFromSpacing);

      // Mobile menu script (simple)
      const mobileToggle = document.getElementById("mobile-menu-toggle");
      const mobileMenu = document.getElementById("mobile-menu");
      if (mobileToggle && mobileMenu) {
        mobileToggle.addEventListener("click", () => {
          const isHidden = mobileMenu.classList.contains("hidden");
          mobileMenu.classList.toggle("hidden", !isHidden);
          mobileToggle.setAttribute(
            "aria-expanded",
            isHidden ? "true" : "false"
          );
        });
      }

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
