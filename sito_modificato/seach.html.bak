    <script>
        // Search Page Specific Functionality
        class SearchPage {
            constructor() {
                this.currentQuery = '';
                this.currentFilters = {
                    category: '',
                    subcategory: ''
                };
                this.popularCalculators = [
                    'mortgage-calculator',
                    'bmi-calculator',
                    'percentage-calculator',
                    'auto-loan-calculator',
                    'calorie-calculator',
                    'compound-interest-calculator',
                    'area',
                    'age-calculator'
                ];
                
                this.init();
            }

            init() {
                this.setupEventListeners();
                this.loadPopularCalculators();
                this.setupFilters();
                this.handleUrlParams();
            }

            setupEventListeners() {
                // Main search input
                const mainInput = document.getElementById('main-search-input');
                if (mainInput) {
                    mainInput.addEventListener('input', (e) => {
                        this.handleSearch(e.target.value);
                    });
                }

                // Header search input
                const headerInput = document.getElementById('header-search-input') || document.getElementById('search-input');
                if (headerInput) {
                    headerInput.addEventListener('input', (e) => {
                        mainInput.value = e.target.value;
                        this.handleSearch(e.target.value);
                    });
                }

                // Suggestion buttons
                document.querySelectorAll('.suggestion-btn').forEach(btn => {
                    btn.addEventListener('click', (e) => {
                        const query = e.target.dataset.query;
                        mainInput.value = query;
                        if (headerInput) headerInput.value = query;
                        this.handleSearch(query);
                    });
                });

                // Filter inputs
                document.querySelectorAll('input[name="category"]').forEach(input => {
                    input.addEventListener('change', () => {
                        this.handleFilterChange();
                    });
                });

                document.getElementById('subcategory-filter').addEventListener('change', () => {
                    this.handleFilterChange();
                });

                // Clear filters
                document.getElementById('clear-filters').addEventListener('click', () => {
                    this.clearFilters();
                });

                // Clear search
                document.getElementById('clear-search').addEventListener('click', () => {
                    this.clearSearch();
                });

                // Mobile filter toggle
                document.getElementById('mobile-filter-toggle').addEventListener('click', () => {
                    this.toggleMobileFilters();
                });
            }

            handleSearch(query) {
                this.currentQuery = query.trim();
                
                if (this.currentQuery.length === 0) {
                    this.showDefaultContent();
                    this.updateSearchStatus('Ready to search through 1,700+ calculators');
                    return;
                }

                if (this.currentQuery.length < 2) {
                    this.showDefaultContent();
                    this.updateSearchStatus('Type at least 2 characters to search');
                    return;
                }

                this.performSearch();
            }

            performSearch() {
                this.showLoading();
                this.updateSearchStatus('Searching...');

                // Simulate search delay for better UX
                setTimeout(() => {
                    const results = this.getFilteredResults();
                    this.displayResults(results);
                    
                    const count = results.length;
                    const statusText = count === 0 
                        ? `No results found for "${this.currentQuery}"` 
                        : `Found ${count} calculator${count !== 1 ? 's' : ''} for "${this.currentQuery}"`;
                    
                    this.updateSearchStatus(statusText);
                }, 300);
            }

            getFilteredResults() {
                if (!window.calcSearch || !window.calcSearch.isInitialized) {
                    return [];
                }

                let results = window.calcSearch.searchCalculators(this.currentQuery);

                // Apply category filter
                if (this.currentFilters.category) {
                    results = results.filter(calc => calc.category === this.currentFilters.category);
                }

                // Apply subcategory filter
                if (this.currentFilters.subcategory) {
                    results = results.filter(calc => calc.subcategory === this.currentFilters.subcategory);
                }

                return results;
            }

            displayResults(results) {
                const container = document.getElementById('results-grid');
                
                if (results.length === 0) {
                    this.showNoResults();
                    return;
                }

                const html = results.map(calc => this.createResultCard(calc)).join('');
                container.innerHTML = html;
                this.showResults();
            }

            createResultCard(calc) {
                const highlightedTitle = this.highlightMatches(calc.title, this.currentQuery);
                const highlightedDesc = this.highlightMatches(calc.description, this.currentQuery);
                const href = this.resolveCalculatorUrl(calc);
                
                return `
                    <a href="${href}" class="block p-5 border border-gray-200 rounded-lg hover:shadow-md hover:-translate-y-0.5 transition transform bg-white">
                        <div class="flex items-start justify-between">
                            <div class="flex-1">
                                <h3 class="text-lg font-semibold text-gray-900 mb-2">${highlightedTitle}</h3>
                                <p class="text-gray-600 mb-3">${highlightedDesc}</p>
                                <div class="flex items-center text-sm text-gray-500">
                                    <span class="bg-blue-100 text-blue-800 px-2 py-1 rounded text-xs font-medium">${calc.category}</span>
                                    <span class="mx-2">â€¢</span>
                                    <span>${calc.subcategory}</span>
                                </div>
                            </div>
                            <svg class="w-5 h-5 text-gray-400 ml-4 flex-shrink-0" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7"></path>
                            </svg>
                        </div>
                    </a>
                `;
            }

            highlightMatches(text, query) {
                if (!query || query.length < 2) return text;
                
                const regex = new RegExp(`(${query.replace(/[.*+?^${}()|[\]\\]/g, '\\$&')})`, 'gi');
                return text.replace(regex, '<span class="search-highlight">$1</span>');
            }

            resolveCalculatorUrl(calc) {
                if (calc.url) return calc.url;
                if (!calc.slug) return '#';

                const normalized = calc.slug.replace(/^\//, '');
                if (normalized.endsWith('.html')) return `/${normalized}`;
                return `/${normalized}.html`;
            }

            loadPopularCalculators() {
                if (!window.calcSearch || !window.calcSearch.isInitialized) {
                    setTimeout(() => this.loadPopularCalculators(), 100);
                    return;
                }

                const container = document.getElementById('popular-calculators');
                const popular = window.calcSearch.calculators.filter(calc => 
                    this.popularCalculators.includes(calc.slug)
                );

                const html = popular.map(calc => this.createResultCard(calc)).join('');
                container.innerHTML = html;
            }

            handleFilterChange() {
                // Get selected category
                const selectedCategory = document.querySelector('input[name="category"]:checked').value;
                this.currentFilters.category = selectedCategory;

                // Update subcategory options
                this.updateSubcategoryOptions(selectedCategory);

                // Get selected subcategory
                this.currentFilters.subcategory = document.getElementById('subcategory-filter').value;

                // Rerun search if there's an active query
                if (this.currentQuery.length >= 2) {
                    this.performSearch();
                }
            }

            updateSubcategoryOptions(category) {
                const subcategorySelect = document.getElementById('subcategory-filter');
                const subcategories = this.getSubcategoriesForCategory(category);

                let html = '<option value="">All Subcategories</option>';
                subcategories.forEach(subcat => {
                    html += `<option value="${subcat}">${subcat}</option>`;
                });

                subcategorySelect.innerHTML = html;
            }

            getSubcategoriesForCategory(category) {
                if (!window.calcSearch || !category) return [];

                const subcats = new Set();
                window.calcSearch.calculators
                    .filter(calc => calc.category === category)
                    .forEach(calc => subcats.add(calc.subcategory));

                return Array.from(subcats).sort();
            }

            clearFilters() {
                // Reset category filter
                document.querySelector('input[name="category"][value=""]').checked = true;
                
                // Reset subcategory filter
                document.getElementById('subcategory-filter').value = '';
                
                // Clear filter state
                this.currentFilters = { category: '', subcategory: '' };
                
                // Update subcategory options
                this.updateSubcategoryOptions('');
                
                // Rerun search if active
                if (this.currentQuery.length >= 2) {
                    this.performSearch();
                }
            }

            clearSearch() {
                document.getElementById('main-search-input').value = '';
                const headerInput = document.getElementById('header-search-input') || document.getElementById('search-input');
                if (headerInput) headerInput.value = '';
                this.currentQuery = '';
                this.showDefaultContent();
                this.updateSearchStatus('Ready to search through 1,700+ calculators');
            }

            toggleMobileFilters() {
                const filters = document.getElementById('search-filters');
                const toggle = document.getElementById('filter-toggle-text');
                const icon = document.getElementById('filter-toggle-icon');
                
                filters.classList.toggle('active');
                
                if (filters.classList.contains('active')) {
                    toggle.textContent = 'Hide';
                    icon.style.transform = 'rotate(180deg)';
                } else {
                    toggle.textContent = 'Show';
                    icon.style.transform = 'rotate(0deg)';
                }
            }

            setupFilters() {
                // Initialize subcategory options
                this.updateSubcategoryOptions('');
            }

            handleUrlParams() {
                const urlParams = new URLSearchParams(window.location.search);
                const query = urlParams.get('q');
                
                if (query) {
                    document.getElementById('main-search-input').value = query;
                    document.getElementById('header-search-input').value = query;
                    this.handleSearch(query);
                }
            }

            updateSearchStatus(text) {
                document.getElementById('search-status').textContent = text;
            }

            showDefaultContent() {
                document.getElementById('default-content').classList.remove('hidden');
                document.getElementById('search-results').classList.add('hidden');
                document.getElementById('no-results').classList.add('hidden');
                document.getElementById('search-loading').classList.add('hidden');
            }

            showResults() {
                document.getElementById('default-content').classList.add('hidden');
                document.getElementById('search-results').classList.remove('hidden');
                document.getElementById('no-results').classList.add('hidden');
                document.getElementById('search-loading').classList.add('hidden');
            }

            showNoResults() {
                document.getElementById('default-content').classList.add('hidden');
                document.getElementById('search-results').classList.add('hidden');
                document.getElementById('no-results').classList.remove('hidden');
                document.getElementById('search-loading').classList.add('hidden');
            }

            showLoading() {
                document.getElementById('default-content').classList.add('hidden');
                document.getElementById('search-results').classList.add('hidden');
                document.getElementById('no-results').classList.add('hidden');
                document.getElementById('search-loading').classList.remove('hidden');
            }
        }

        // Initialize search page when DOM is ready
        document.addEventListener('DOMContentLoaded', () => {
            window.searchPage = new SearchPage();
        });
    </script>
