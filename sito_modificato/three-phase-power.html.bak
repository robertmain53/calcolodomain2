    <script>
      // Tab handling
      const tabs = {
        "tab-3ph-balanced": "panel-3ph-balanced",
        "tab-3ph-unbalanced": "panel-3ph-unbalanced",
        "tab-1ph": "panel-1ph",
      };
      Object.keys(tabs).forEach((tabId) => {
        document.getElementById(tabId).addEventListener("click", () => {
          // activate tab
          Object.keys(tabs).forEach((id) => {
            const btn = document.getElementById(id);
            const panel = document.getElementById(tabs[id]);
            if (id === tabId) {
              btn.classList.add("tab-active");
              panel.classList.remove("hidden");
            } else {
              btn.classList.remove("tab-active");
              panel.classList.add("hidden");
            }
          });
          document.getElementById("results").classList.add("hidden");
          document.getElementById("error-msg").textContent = "";
        });
      });

      function round(val, digits) {
        if (!isFinite(val)) return NaN;
        const f = Math.pow(10, digits);
        return Math.round(val * f) / f;
      }

      function setResults(p_kw, s_kva, q_kvar, pf, perPhase, pin_kw, loss_kw) {
        document.getElementById("res-pkw").textContent = isNaN(p_kw)
          ? "–"
          : round(p_kw, 3);
        document.getElementById("res-skva").textContent = isNaN(s_kva)
          ? "–"
          : round(s_kva, 3);
        document.getElementById("res-qkvar").textContent = isNaN(q_kvar)
          ? "–"
          : round(q_kvar, 3);
        document.getElementById("res-pf").textContent = isNaN(pf)
          ? "–"
          : round(pf, 3);
        document.getElementById("res-p-phase").textContent = isNaN(perPhase.p)
          ? "–"
          : round(perPhase.p, 3);
        document.getElementById("res-s-phase").textContent = isNaN(perPhase.s)
          ? "–"
          : round(perPhase.s, 3);
        document.getElementById("res-pin").textContent = isNaN(pin_kw)
          ? "–"
          : round(pin_kw, 3);
        document.getElementById("res-loss").textContent = isNaN(loss_kw)
          ? "–"
          : round(loss_kw, 3);
        document.getElementById("results").classList.remove("hidden");
      }

      function parseOrNaN(id) {
        const v = document.getElementById(id).value.trim();
        return v === "" ? NaN : parseFloat(v);
      }

      document.getElementById("btn-calc").addEventListener("click", () => {
        const errEl = document.getElementById("error-msg");
        errEl.textContent = "";
        let p_kw = NaN,
          s_kva = NaN,
          q_kvar = NaN,
          pf = NaN,
          perPhase = { p: NaN, s: NaN },
          pin_kw = NaN,
          loss_kw = NaN;

        const activeTab = Object.keys(tabs).find((id) =>
          document.getElementById(id).classList.contains("tab-active")
        );

        try {
          if (activeTab === "tab-3ph-balanced") {
            const mode = document.getElementById("solve-for-3ph").value;
            let V = parseOrNaN("volt-3ph");
            let I = parseOrNaN("curr-3ph");
            let PF = parseOrNaN("pf-3ph");
            const eff = parseOrNaN("eff-3ph");
            const sysType = document.getElementById("sys-type").value; // ll or ln

            if (PF > 1 || PF < 0)
              throw new Error("Power factor must be between 0 and 1.");
            if (mode === "power") {
              if (isNaN(V) || isNaN(I) || isNaN(PF))
                throw new Error("Enter voltage, current, and power factor.");
            } else if (mode === "current") {
              if (isNaN(V) || isNaN(PF) || isNaN(parseOrNaN("volt-3ph")))
                throw new Error(
                  "Enter voltage, power factor, and either kW or kVA is not yet supported."
                );
            } else if (mode === "voltage") {
              // For simplicity, treat same as power mode; solving for V or I from P is more advanced.
              if (isNaN(V) || isNaN(I) || isNaN(PF))
                throw new Error("Enter voltage, current, and power factor.");
            }

            // Convert line-neutral to line-line if needed
            let Vll;
            if (sysType === "ll") {
              Vll = V;
            } else {
              Vll = V * Math.sqrt(3);
            }

            const S = Math.sqrt(3) * Vll * I; // VA
            const P = S * PF; // W
            const Q = Math.sqrt(Math.max(S * S - P * P, 0)); // VAR

            p_kw = P / 1000;
            s_kva = S / 1000;
            q_kvar = Q / 1000;
            pf = PF;
            perPhase = { p: p_kw / 3, s: s_kva / 3 };

            if (!isNaN(eff) && eff > 0) {
              const eta = eff / 100;
              pin_kw = p_kw / eta;
              loss_kw = pin_kw - p_kw;
            }
          } else if (activeTab === "tab-3ph-unbalanced") {
            const Va = parseOrNaN("ua-v"),
              Ia = parseOrNaN("ua-i"),
              PFa = parseOrNaN("ua-pf");
            const Vb = parseOrNaN("ub-v"),
              Ib = parseOrNaN("ub-i"),
              PFb = parseOrNaN("ub-pf");
            const Vc = parseOrNaN("uc-v"),
              Ic = parseOrNaN("uc-i"),
              PFc = parseOrNaN("uc-pf");
            const eff = parseOrNaN("eff-unbal");

            if ([Va, Ia, PFa, Vb, Ib, PFb, Vc, Ic, PFc].some((x) => isNaN(x))) {
              throw new Error(
                "Enter voltage, current, and PF for all three phases."
              );
            }
            if ([PFa, PFb, PFc].some((x) => x < 0 || x > 1)) {
              throw new Error(
                "Power factor must be between 0 and 1 for each phase."
              );
            }

            function phasePower(V, I, PF) {
              const S = V * I;
              const P = S * PF;
              const Q = Math.sqrt(Math.max(S * S - P * P, 0));
              return { S, P, Q };
            }

            const A = phasePower(Va, Ia, PFa);
            const B = phasePower(Vb, Ib, PFb);
            const C = phasePower(Vc, Ic, PFc);

            const Ptot = A.P + B.P + C.P;
            const Stot = A.S + B.S + C.S;
            const Qtot = A.Q + B.Q + C.Q;

            p_kw = Ptot / 1000;
            s_kva = Stot / 1000;
            q_kvar = Qtot / 1000;
            pf = Stot === 0 ? NaN : Ptot / Stot;
            perPhase = { p: p_kw / 3, s: s_kva / 3 };

            if (!isNaN(eff) && eff > 0) {
              const eta = eff / 100;
              pin_kw = p_kw / eta;
              loss_kw = pin_kw - p_kw;
            }
          } else if (activeTab === "tab-1ph") {
            const V = parseOrNaN("volt-1ph");
            const I = parseOrNaN("curr-1ph");
            const PF = parseOrNaN("pf-1ph");
            const eff = parseOrNaN("eff-1ph");

            if (isNaN(V) || isNaN(I) || isNaN(PF))
              throw new Error("Enter voltage, current, and power factor.");
            if (PF < 0 || PF > 1)
              throw new Error("Power factor must be between 0 and 1.");

            const S = V * I;
            const P = S * PF;
            const Q = Math.sqrt(Math.max(S * S - P * P, 0));

            p_kw = P / 1000;
            s_kva = S / 1000;
            q_kvar = Q / 1000;
            pf = PF;
            perPhase = { p: p_kw, s: s_kva };

            if (!isNaN(eff) && eff > 0) {
              const eta = eff / 100;
              pin_kw = p_kw / eta;
              loss_kw = pin_kw - p_kw;
            }
          }

          setResults(p_kw, s_kva, q_kvar, pf, perPhase, pin_kw, loss_kw);
        } catch (e) {
          errEl.textContent = e.message || "Invalid input.";
          document.getElementById("results").classList.add("hidden");
        }
      });

      document.getElementById("btn-clear").addEventListener("click", () => {
        document.querySelectorAll("input").forEach((inp) => (inp.value = ""));
        document.getElementById("results").classList.add("hidden");
        document.getElementById("error-msg").textContent = "";
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
