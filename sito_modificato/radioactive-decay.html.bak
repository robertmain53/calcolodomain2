    <script>
      // Utility: convert time to seconds
      function toSeconds(value, unit) {
        const v = Number(value);
        switch (unit) {
          case "s":
            return v;
          case "min":
            return v * 60;
          case "h":
            return v * 3600;
          case "day":
            return v * 86400;
          case "year":
            return v * 365.25 * 86400;
          default:
            return v;
        }
      }
      function fromSeconds(seconds, unit) {
        const s = Number(seconds);
        switch (unit) {
          case "s":
            return s;
          case "min":
            return s / 60;
          case "h":
            return s / 3600;
          case "day":
            return s / 86400;
          case "year":
            return s / (365.25 * 86400);
          default:
            return s;
        }
      }

      function formatNumber(x, digits = 4) {
        if (!isFinite(x)) return "–";
        if (x === 0) return "0";
        const abs = Math.abs(x);
        if (abs >= 1e4 || abs < 1e-3) {
          return x.toExponential(3);
        }
        return Number(x.toFixed(digits)).toString();
      }

      function buildHalfLifeTable() {
        const tbody = document.getElementById("halflife-table-body");
        tbody.innerHTML = "";
        for (let n = 0; n <= 10; n++) {
          const frac = Math.pow(0.5, n);
          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="px-2 py-1 border-b">${n}</td>
        <td class="px-2 py-1 border-b">${formatNumber(frac, 4)}</td>
        <td class="px-2 py-1 border-b">${formatNumber(frac * 100, 2)}%</td>
      `;
          tbody.appendChild(tr);
        }
      }

      function updateSliderOutputs() {
        const n = Number(document.getElementById("slider-n").value);
        const frac = Math.pow(0.5, n);
        document.getElementById("slider-n-value").textContent = n.toFixed(1);
        document.getElementById("slider-fraction").textContent = formatNumber(
          frac,
          4
        );
        document.getElementById("slider-percent").textContent = formatNumber(
          frac * 100,
          2
        );
      }

      document.addEventListener("DOMContentLoaded", () => {
        buildHalfLifeTable();
        updateSliderOutputs();

        document
          .getElementById("slider-n")
          .addEventListener("input", updateSliderOutputs);

        const nuclideSelect = document.getElementById("nuclide-select");
        nuclideSelect.addEventListener("change", () => {
          const val = nuclideSelect.value;
          if (!val) return;
          const [hl, unit] = val.split("|");
          const halfLifeInput = document.getElementById("half-life");
          const halfLifeUnit = document.getElementById("half-life-unit");
          halfLifeInput.value = hl;
          halfLifeUnit.value = unit;
        });

        document.getElementById("reset-btn").addEventListener("click", () => {
          [
            "initial-value",
            "final-value",
            "half-life",
            "time",
            "decay-constant",
            "num-halflives",
          ].forEach((id) => {
            document.getElementById(id).value = "";
          });
          document.getElementById("quantity-type").value = "activity";
          document.getElementById("initial-unit").value = "Bq";
          document.getElementById("final-unit").value = "same";
          document.getElementById("half-life-unit").value = "year";
          document.getElementById("time-unit").value = "year";
          document.getElementById("nuclide-select").value = "";
          document.getElementById("error-msg").textContent = "";
          [
            "res-initial",
            "res-final",
            "res-fraction",
            "res-percent",
            "res-percent-decayed",
            "res-halflife",
            "res-lambda",
            "res-time",
            "res-n",
          ].forEach((id) => {
            document.getElementById(id).textContent = "–";
          });
          document.getElementById("slider-n").value = 0;
          updateSliderOutputs();
        });

        document.getElementById("compute-btn").addEventListener("click", () => {
          const errEl = document.getElementById("error-msg");
          errEl.textContent = "";

          const A0Input = document.getElementById("initial-value").value.trim();
          const AInput = document.getElementById("final-value").value.trim();
          const TInput = document.getElementById("half-life").value.trim();
          const tInput = document.getElementById("time").value.trim();
          const lambdaInput = document
            .getElementById("decay-constant")
            .value.trim();
          const nInput = document.getElementById("num-halflives").value.trim();

          const halfLifeUnit = document.getElementById("half-life-unit").value;
          const timeUnit = document.getElementById("time-unit").value;

          let A0 = A0Input === "" ? null : Number(A0Input);
          let A = AInput === "" ? null : Number(AInput);
          let T = TInput === "" ? null : Number(TInput);
          let t = tInput === "" ? null : Number(tInput);
          let lambda = lambdaInput === "" ? null : Number(lambdaInput);
          let n = nInput === "" ? null : Number(nInput);

          // Basic validation
          function isValidPositive(x) {
            return x !== null && isFinite(x) && x > 0;
          }

          if (A0 !== null && !isFinite(A0)) {
            errEl.textContent = "Initial value is invalid.";
            return;
          }
          if (A !== null && !isFinite(A)) {
            errEl.textContent = "Final value is invalid.";
            return;
          }
          if (T !== null && (!isFinite(T) || T <= 0)) {
            errEl.textContent = "Half-life must be positive.";
            return;
          }
          if (t !== null && (!isFinite(t) || t < 0)) {
            errEl.textContent = "Elapsed time must be non-negative.";
            return;
          }
          if (lambda !== null && (!isFinite(lambda) || lambda <= 0)) {
            errEl.textContent = "Decay constant must be positive.";
            return;
          }
          if (n !== null && (!isFinite(n) || n < 0)) {
            errEl.textContent = "Number of half-lives must be non-negative.";
            return;
          }

          // Need at least two of (A0, A, t or n, lambda or T)
          if (A0 === null && A === null) {
            errEl.textContent = "Enter at least an initial or final value.";
            return;
          }

          // Determine lambda from T if not given
          let Tsec = null;
          if (isValidPositive(T)) {
            Tsec = toSeconds(T, halfLifeUnit);
            if (!isValidPositive(lambda)) {
              lambda = Math.log(2) / Tsec;
            }
          }

          // If n is given, use it to get t (or T)
          if (n !== null) {
            if (isValidPositive(Tsec)) {
              const tsec = n * Tsec;
              t = fromSeconds(tsec, timeUnit);
            } else if (isValidPositive(lambda)) {
              // n = t / T1/2 = t * lambda / ln2 => t = n * ln2 / lambda
              const tsec = (n * Math.log(2)) / lambda;
              t = fromSeconds(tsec, timeUnit);
              Tsec = Math.log(2) / lambda;
              T = fromSeconds(Tsec, halfLifeUnit);
            } else {
              errEl.textContent =
                "To use number of half-lives, provide half-life or decay constant.";
              return;
            }
          }

          // If lambda is given but T is not, compute T
          if (isValidPositive(lambda) && Tsec === null) {
            Tsec = Math.log(2) / lambda;
            T = fromSeconds(Tsec, halfLifeUnit);
          }

          // Now we should have lambda if we have T or lambda
          if (!isValidPositive(lambda)) {
            errEl.textContent = "Provide at least half-life or decay constant.";
            return;
          }

          // If t is missing, solve for t from A0 and A
          if (t === null) {
            if (!isValidPositive(A0) || !isValidPositive(A)) {
              errEl.textContent =
                "To solve for time, provide both initial and final values.";
              return;
            }
            const ratio = A / A0;
            if (ratio <= 0 || ratio >= 1) {
              errEl.textContent =
                "Final value must be between 0 and initial value to solve for time.";
              return;
            }
            const tsec = -Math.log(ratio) / lambda;
            t = fromSeconds(tsec, timeUnit);
          }

          // Now compute A or A0 if missing
          const tsecFinal = toSeconds(t, timeUnit);
          const decayFactor = Math.exp(-lambda * tsecFinal);

          if (A0 === null && isValidPositive(A)) {
            A0 = A / decayFactor;
          } else if (A === null && isValidPositive(A0)) {
            A = A0 * decayFactor;
          }

          if (!isValidPositive(A0) || !isValidPositive(A)) {
            errEl.textContent =
              "Could not compute a valid initial and final value. Check your inputs.";
            return;
          }

          // Compute n from t and T
          if (Tsec === null) {
            Tsec = Math.log(2) / lambda;
            T = fromSeconds(Tsec, halfLifeUnit);
          }
          n = tsecFinal / Tsec;

          // Units handling for display
          const initialUnit = document.getElementById("initial-unit").value;
          const finalUnitSel = document.getElementById("final-unit").value;
          const finalUnit =
            finalUnitSel === "same" ? initialUnit : finalUnitSel;

          function convertValue(val, fromUnit, toUnit) {
            if (fromUnit === toUnit) return val;
            // Simple scaling for Bq/kBq/MBq and g/kg
            const scale = {
              Bq: 1,
              kBq: 1e3,
              MBq: 1e6,
              g: 1,
              kg: 1e3,
              atoms: 1,
              Ci: 3.7e10,
            };
            if (!(fromUnit in scale) || !(toUnit in scale)) return val;
            return (val * scale[fromUnit]) / scale[toUnit];
          }

          const A0Display = convertValue(A0, initialUnit, initialUnit);
          const ADisplay = convertValue(A, initialUnit, finalUnit);

          const fraction = A / A0;
          const percentRemaining = fraction * 100;
          const percentDecayed = 100 - percentRemaining;

          document.getElementById("res-initial").textContent = `${formatNumber(
            A0Display
          )} ${initialUnit}`;
          document.getElementById("res-final").textContent = `${formatNumber(
            ADisplay
          )} ${finalUnit}`;
          document.getElementById("res-fraction").textContent = formatNumber(
            fraction,
            6
          );
          document.getElementById("res-percent").textContent =
            formatNumber(percentRemaining, 3) + " %";
          document.getElementById("res-percent-decayed").textContent =
            formatNumber(percentDecayed, 3) + " %";

          document.getElementById("res-halflife").textContent = `${formatNumber(
            T,
            6
          )} ${halfLifeUnit}`;
          document.getElementById("res-lambda").textContent = `${formatNumber(
            lambda,
            6
          )} 1/${timeUnit}`;
          document.getElementById("res-time").textContent = `${formatNumber(
            t,
            6
          )} ${timeUnit}`;
          document.getElementById("res-n").textContent = formatNumber(n, 4);
        });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
