    <script>
      // Utility: format numbers with currency and thousands separators
      function formatCurrency(value, currencySymbol) {
        if (!isFinite(value)) return "–";
        const absVal = Math.abs(value);
        const sign = value < 0 ? "-" : "";
        const formatted = absVal.toLocaleString(undefined, {
          minimumFractionDigits: 2,
          maximumFractionDigits: 2,
        });
        return sign + currencySymbol + formatted;
      }

      function formatNumber(value, decimals = 2) {
        if (!isFinite(value)) return "–";
        return value.toLocaleString(undefined, {
          minimumFractionDigits: decimals,
          maximumFractionDigits: decimals,
        });
      }

      function formatInteger(value) {
        if (!isFinite(value)) return "–";
        return Math.round(value).toLocaleString();
      }

      function calculateContributionMargin() {
        const price = parseFloat(document.getElementById("pricePerUnit").value);
        const variableCost = parseFloat(
          document.getElementById("variableCostPerUnit").value
        );
        const units = parseFloat(document.getElementById("unitsSold").value);
        const fixedCosts = parseFloat(
          document.getElementById("fixedCosts").value
        );
        const targetProfit = parseFloat(
          document.getElementById("targetProfit").value
        );
        const currencySymbol =
          document.getElementById("currencySymbol").value || "$";

        const errorEl = document.getElementById("errorMessage");
        errorEl.classList.add("hidden");
        errorEl.textContent = "";

        // Basic validation
        if (isNaN(price) || price <= 0) {
          errorEl.textContent =
            "Please enter a valid selling price per unit greater than 0.";
          errorEl.classList.remove("hidden");
          return;
        }
        if (isNaN(variableCost) || variableCost < 0) {
          errorEl.textContent =
            "Please enter a valid variable cost per unit (0 or higher).";
          errorEl.classList.remove("hidden");
          return;
        }
        if (!isNaN(units) && units < 0) {
          errorEl.textContent = "Units sold cannot be negative.";
          errorEl.classList.remove("hidden");
          return;
        }
        if (!isNaN(fixedCosts) && fixedCosts < 0) {
          errorEl.textContent = "Fixed costs cannot be negative.";
          errorEl.classList.remove("hidden");
          return;
        }

        const cmPerUnit = price - variableCost;
        const cmRatio = cmPerUnit / price; // decimal
        const totalCM = !isNaN(units) ? cmPerUnit * units : NaN;
        const operatingProfit =
          !isNaN(totalCM) && !isNaN(fixedCosts) ? totalCM - fixedCosts : NaN;

        // Break-even
        let breakEvenUnits = NaN;
        let breakEvenSales = NaN;
        if (!isNaN(fixedCosts) && cmPerUnit > 0) {
          breakEvenUnits = fixedCosts / cmPerUnit;
          breakEvenSales = cmRatio > 0 ? fixedCosts / cmRatio : NaN;
        }

        // Target profit
        let targetUnits = NaN;
        let targetSales = NaN;
        if (
          !isNaN(targetProfit) &&
          !isNaN(fixedCosts) &&
          cmPerUnit > 0 &&
          cmRatio > 0
        ) {
          const numerator = fixedCosts + targetProfit;
          targetUnits = numerator / cmPerUnit;
          targetSales = numerator / cmRatio;
        }

        // Update UI
        document.getElementById("cmPerUnit").textContent = formatCurrency(
          cmPerUnit,
          currencySymbol
        );
        document.getElementById("cmRatio").textContent = isFinite(cmRatio)
          ? (cmRatio * 100).toFixed(2) + "%"
          : "–";
        document.getElementById("totalCM").textContent = formatCurrency(
          totalCM,
          currencySymbol
        );
        document.getElementById("operatingProfit").textContent = formatCurrency(
          operatingProfit,
          currencySymbol
        );

        document.getElementById("breakEvenUnits").textContent =
          formatInteger(breakEvenUnits);
        document.getElementById("breakEvenSales").textContent = formatCurrency(
          breakEvenSales,
          currencySymbol
        );

        document.getElementById("targetUnits").textContent =
          formatInteger(targetUnits);
        document.getElementById("targetSales").textContent = formatCurrency(
          targetSales,
          currencySymbol
        );
        document
          .getElementById("targetNote")
          .classList.toggle("hidden", isNaN(targetProfit));

        // Show badges if we have valid CM
        const showBadges = isFinite(cmPerUnit) && isFinite(cmRatio);
        document
          .getElementById("cmPerUnitBadge")
          .classList.toggle("hidden", !showBadges);
        document
          .getElementById("cmRatioBadge")
          .classList.toggle("hidden", !showBadges);
      }

      function resetContributionMargin() {
        [
          "pricePerUnit",
          "variableCostPerUnit",
          "unitsSold",
          "fixedCosts",
          "targetProfit",
        ].forEach((id) => {
          document.getElementById(id).value = "";
        });
        document.getElementById("currencySymbol").value = "$";

        const currencySymbol = "$";
        document.getElementById("cmPerUnit").textContent = "–";
        document.getElementById("cmRatio").textContent = "–";
        document.getElementById("totalCM").textContent = "–";
        document.getElementById("operatingProfit").textContent = "–";
        document.getElementById("breakEvenUnits").textContent = "–";
        document.getElementById("breakEvenSales").textContent = "–";
        document.getElementById("targetUnits").textContent = "–";
        document.getElementById("targetSales").textContent = "–";
        document.getElementById("targetNote").classList.add("hidden");
        document.getElementById("cmPerUnitBadge").classList.add("hidden");
        document.getElementById("cmRatioBadge").classList.add("hidden");

        const errorEl = document.getElementById("errorMessage");
        errorEl.classList.add("hidden");
        errorEl.textContent = "";
      }

      function applyScenario(type) {
        const priceInput = document.getElementById("pricePerUnit");
        const varCostInput = document.getElementById("variableCostPerUnit");
        const unitsInput = document.getElementById("unitsSold");
        const fixedInput = document.getElementById("fixedCosts");
        const targetInput = document.getElementById("targetProfit");
        const currencyInput = document.getElementById("currencySymbol");

        if (type === "saas") {
          currencyInput.value = "$";
          priceInput.value = "29";
          varCostInput.value = "5";
          unitsInput.value = "500";
          fixedInput.value = "8000";
          targetInput.value = "5000";
        } else if (type === "ecommerce") {
          currencyInput.value = "$";
          priceInput.value = "60";
          varCostInput.value = "35";
          unitsInput.value = "1200";
          fixedInput.value = "15000";
          targetInput.value = "10000";
        } else if (type === "manufacturing") {
          currencyInput.value = "$";
          priceInput.value = "120";
          varCostInput.value = "70";
          unitsInput.value = "2000";
          fixedInput.value = "50000";
          targetInput.value = "30000";
        }
        calculateContributionMargin();
      }

      document.addEventListener("DOMContentLoaded", function () {
        document
          .getElementById("calculateBtn")
          .addEventListener("click", calculateContributionMargin);
        document
          .getElementById("resetBtn")
          .addEventListener("click", resetContributionMargin);

        // Recalculate on Enter key in any input
        [
          "pricePerUnit",
          "variableCostPerUnit",
          "unitsSold",
          "fixedCosts",
          "targetProfit",
          "currencySymbol",
        ].forEach((id) => {
          const el = document.getElementById(id);
          el.addEventListener("keydown", function (e) {
            if (e.key === "Enter") {
              e.preventDefault();
              calculateContributionMargin();
            }
          });
        });

        // Scenario buttons
        document.querySelectorAll(".scenario-btn").forEach((btn) => {
          btn.addEventListener("click", () => {
            const scenario = btn.getAttribute("data-scenario");
            applyScenario(scenario);
          });
        });
      });
    </script>

