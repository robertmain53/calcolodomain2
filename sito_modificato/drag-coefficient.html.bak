    <script>
      // Utility conversions
      const MPH_TO_MPS = 0.44704;
      const KMH_TO_MPS = 1000 / 3600;
      const FTPS_TO_MPS = 0.3048;
      const LBF_TO_N = 4.4482216153;
      const FT2_TO_M2 = 0.09290304;
      const SLUGFT3_TO_KGM3 = 515.3788184;

      function toSIForce(value, unit) {
        if (unit === "N") return value;
        if (unit === "lbf") return value * LBF_TO_N;
        return value;
      }

      function toSISpeed(value, unit) {
        switch (unit) {
          case "mps":
            return value;
          case "kmh":
            return value * KMH_TO_MPS;
          case "mph":
            return value * MPH_TO_MPS;
          case "ftps":
            return value * FTPS_TO_MPS;
          default:
            return value;
        }
      }

      function toSIDensity(value, unit) {
        if (unit === "kgm3") return value;
        if (unit === "slugsft3") return value * SLUGFT3_TO_KGM3;
        return value;
      }

      function toSIArea(value, unit) {
        if (unit === "m2") return value;
        if (unit === "ft2") return value * FT2_TO_M2;
        return value;
      }

      function fromSIForce(N, unit) {
        if (unit === "N") return N;
        if (unit === "lbf") return N / LBF_TO_N;
        return N;
      }

      function fromSISpeed(mps, unit) {
        switch (unit) {
          case "mps":
            return mps;
          case "kmh":
            return mps / KMH_TO_MPS;
          case "mph":
            return mps / MPH_TO_MPS;
          case "ftps":
            return mps / FTPS_TO_MPS;
          default:
            return mps;
        }
      }

      function fromSIArea(m2, unit) {
        if (unit === "m2") return m2;
        if (unit === "ft2") return m2 / FT2_TO_M2;
        return m2;
      }

      document.addEventListener("DOMContentLoaded", () => {
        const solveForEl = document.getElementById("solveFor");
        const unitSystemEl = document.getElementById("unitSystem");
        const presetEl = document.getElementById("presetSelect");

        const FdInput = document.getElementById("FdInput");
        const FdUnit = document.getElementById("FdUnit");
        const vInput = document.getElementById("vInput");
        const vUnit = document.getElementById("vUnit");
        const rhoInput = document.getElementById("rhoInput");
        const rhoUnit = document.getElementById("rhoUnit");
        const AInput = document.getElementById("AInput");
        const AUnit = document.getElementById("AUnit");
        const CdInput = document.getElementById("CdInput");

        const computeBtn = document.getElementById("computeBtn");
        const resetBtn = document.getElementById("resetBtn");
        const errorMsg = document.getElementById("errorMsg");
        const resultsCard = document.getElementById("resultsCard");
        const resultText = document.getElementById("resultText");

        function setUnitSystem(system) {
          if (system === "si") {
            FdUnit.value = "N";
            vUnit.value = "mps";
            rhoUnit.value = "kgm3";
            AUnit.value = "m2";
          } else {
            FdUnit.value = "lbf";
            vUnit.value = "mph";
            rhoUnit.value = "slugsft3";
            AUnit.value = "ft2";
          }
        }

        unitSystemEl.addEventListener("change", () => {
          setUnitSystem(unitSystemEl.value);
        });

        presetEl.addEventListener("change", () => {
          const preset = presetEl.value;
          if (!preset) return;

          if (preset === "car") {
            CdInput.value = 0.3;
            AInput.value = 2.2;
            AUnit.value = "m2";
          } else if (preset === "suv") {
            CdInput.value = 0.38;
            AInput.value = 2.8;
            AUnit.value = "m2";
          } else if (preset === "cyclist") {
            CdInput.value = 0.9;
            AInput.value = 0.5;
            AUnit.value = "m2";
          } else if (preset === "sphere") {
            CdInput.value = 0.47;
          } else if (preset === "flatPlate") {
            CdInput.value = 1.2;
          }
        });

        function clearResults() {
          errorMsg.textContent = "";
          resultsCard.classList.add("hidden");
          resultText.textContent = "";
        }

        function calculate() {
          clearResults();
          const solveFor = solveForEl.value;

          const FdVal = FdInput.value !== "" ? parseFloat(FdInput.value) : null;
          const vVal = vInput.value !== "" ? parseFloat(vInput.value) : null;
          const rhoVal =
            rhoInput.value !== "" ? parseFloat(rhoInput.value) : null;
          const AVal = AInput.value !== "" ? parseFloat(AInput.value) : null;
          const CdVal = CdInput.value !== "" ? parseFloat(CdInput.value) : null;

          try {
            let FdSI = FdVal !== null ? toSIForce(FdVal, FdUnit.value) : null;
            let vSI = vVal !== null ? toSISpeed(vVal, vUnit.value) : null;
            let rhoSI =
              rhoVal !== null ? toSIDensity(rhoVal, rhoUnit.value) : null;
            let ASI = AVal !== null ? toSIArea(AVal, AUnit.value) : null;
            let Cd = CdVal;

            if (solveFor === "Cd") {
              if (
                FdSI === null ||
                vSI === null ||
                rhoSI === null ||
                ASI === null
              ) {
                throw new Error(
                  "Please enter drag force, speed, density, and area to solve for Cd."
                );
              }
              if (vSI <= 0 || rhoSI <= 0 || ASI <= 0) {
                throw new Error("Speed, density, and area must be positive.");
              }
              Cd = (2 * FdSI) / (rhoSI * vSI * vSI * ASI);
              CdInput.value = Cd.toFixed(4);
              resultText.innerHTML = `Drag coefficient C<sub>d</sub> ≈ <strong>${Cd.toFixed(
                4
              )}</strong>`;
            } else if (solveFor === "Fd") {
              if (
                vSI === null ||
                rhoSI === null ||
                ASI === null ||
                Cd == null
              ) {
                throw new Error(
                  "Please enter speed, density, area, and Cd to solve for drag force."
                );
              }
              if (vSI <= 0 || rhoSI <= 0 || ASI <= 0 || Cd <= 0) {
                throw new Error(
                  "Speed, density, area, and Cd must be positive."
                );
              }
              FdSI = 0.5 * rhoSI * vSI * vSI * Cd * ASI;
              const FdOut = fromSIForce(FdSI, FdUnit.value);
              FdInput.value = FdOut.toFixed(3);
              resultText.innerHTML = `Drag force F<sub>d</sub> ≈ <strong>${FdOut.toFixed(
                3
              )} ${FdUnit.value}</strong>`;
            } else if (solveFor === "v") {
              if (
                FdSI === null ||
                rhoSI === null ||
                ASI === null ||
                Cd == null
              ) {
                throw new Error(
                  "Please enter drag force, density, area, and Cd to solve for speed."
                );
              }
              if (FdSI <= 0 || rhoSI <= 0 || ASI <= 0 || Cd <= 0) {
                throw new Error(
                  "Drag force, density, area, and Cd must be positive."
                );
              }
              vSI = Math.sqrt((2 * FdSI) / (rhoSI * Cd * ASI));
              const vOut = fromSISpeed(vSI, vUnit.value);
              vInput.value = vOut.toFixed(3);
              resultText.innerHTML = `Speed v ≈ <strong>${vOut.toFixed(3)} ${
                vUnit.options[vUnit.selectedIndex].text
              }</strong>`;
            } else if (solveFor === "A") {
              if (
                FdSI === null ||
                rhoSI === null ||
                vSI === null ||
                Cd == null
              ) {
                throw new Error(
                  "Please enter drag force, density, speed, and Cd to solve for area."
                );
              }
              if (FdSI <= 0 || rhoSI <= 0 || vSI <= 0 || Cd <= 0) {
                throw new Error(
                  "Drag force, density, speed, and Cd must be positive."
                );
              }
              ASI = (2 * FdSI) / (rhoSI * vSI * vSI * Cd);
              const AOut = fromSIArea(ASI, AUnit.value);
              AInput.value = AOut.toFixed(4);
              resultText.innerHTML = `Reference area A ≈ <strong>${AOut.toFixed(
                4
              )} ${AUnit.options[AUnit.selectedIndex].text}</strong>`;
            }

            resultsCard.classList.remove("hidden");
          } catch (err) {
            errorMsg.textContent = err.message;
          }
        }

        computeBtn.addEventListener("click", calculate);

        resetBtn.addEventListener("click", () => {
          FdInput.value = "";
          vInput.value = "";
          rhoInput.value = "";
          AInput.value = "";
          CdInput.value = "";
          presetEl.value = "";
          solveForEl.value = "Cd";
          unitSystemEl.value = "si";
          setUnitSystem("si");
          clearResults();
        });

        // Initialize defaults
        setUnitSystem("si");
        rhoInput.value = 1.225; // standard air
      });
    </script>

