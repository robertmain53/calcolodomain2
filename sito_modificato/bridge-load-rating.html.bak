    <script>
      // Mobile menu toggle
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden");
          this.setAttribute("aria-expanded", String(isHidden));
        });

      // Unit system handling
      const unitUSBtn = document.getElementById("unit-us");
      const unitMetricBtn = document.getElementById("unit-metric");
      const unitLabels = document.querySelectorAll(".unit-label");
      const userTruckUnit = document.querySelector(".user-truck-unit");

      let currentUnit = "us"; // 'us' => kips, 'metric' => kN

      function setUnits(unit) {
        currentUnit = unit;
        if (unit === "us") {
          unitUSBtn.classList.add("bg-blue-600", "text-white");
          unitUSBtn.classList.remove("bg-white", "text-gray-700");
          unitMetricBtn.classList.remove("bg-blue-600", "text-white");
          unitMetricBtn.classList.add("bg-white", "text-gray-700");
          unitLabels.forEach((l) => (l.textContent = "kips"));
          if (userTruckUnit) userTruckUnit.textContent = "kips";
        } else {
          unitMetricBtn.classList.add("bg-blue-600", "text-white");
          unitMetricBtn.classList.remove("bg-white", "text-gray-700");
          unitUSBtn.classList.remove("bg-blue-600", "text-white");
          unitUSBtn.classList.add("bg-white", "text-gray-700");
          unitLabels.forEach((l) => (l.textContent = "kN"));
          if (userTruckUnit) userTruckUnit.textContent = "kN";
        }
      }

      unitUSBtn.addEventListener("click", () => setUnits("us"));
      unitMetricBtn.addEventListener("click", () => setUnits("metric"));

      // Show/hide user truck section
      const truckTypeSelect = document.getElementById("truck-type");
      const userTruckSection = document.getElementById("user-truck-section");
      truckTypeSelect.addEventListener("change", () => {
        if (truckTypeSelect.value === "user") {
          userTruckSection.classList.remove("hidden");
        } else {
          userTruckSection.classList.add("hidden");
        }
      });

      // Calculation logic
      const calcBtn = document.getElementById("calculate-btn");
      const resetBtn = document.getElementById("reset-btn");
      const errorEl = document.getElementById("calc-error");
      const resultsContent = document.getElementById("results-content");
      const ratingFactorDisplay = document.getElementById(
        "rating-factor-display"
      );
      const demandCapacityDisplay = document.getElementById(
        "demand-capacity-display"
      );
      const limitStateDisplay = document.getElementById("limit-state-display");
      const ratingLevelDisplay = document.getElementById(
        "rating-level-display"
      );
      const postingDisplay = document.getElementById("posting-display");
      const statusBadge = document.getElementById("rating-status-badge");

      function classifyRating(RF) {
        if (RF >= 1.0) {
          statusBadge.textContent = "Adequate for selected rating";
          statusBadge.className =
            "result-badge bg-green-100 text-green-700 border border-green-200";
          const dot = document.createElement("span");
          dot.className = "w-2 h-2 rounded-full bg-green-500 inline-block";
          statusBadge.prepend(dot);
        } else if (RF >= 0.75) {
          statusBadge.textContent =
            "Marginal – consider posting or further analysis";
          statusBadge.className =
            "result-badge bg-yellow-100 text-yellow-700 border border-yellow-200";
          const dot = document.createElement("span");
          dot.className = "w-2 h-2 rounded-full bg-yellow-500 inline-block";
          statusBadge.prepend(dot);
        } else {
          statusBadge.textContent =
            "Deficient – rating below typical thresholds";
          statusBadge.className =
            "result-badge bg-red-100 text-red-700 border border-red-200";
          const dot = document.createElement("span");
          dot.className = "w-2 h-2 rounded-full bg-red-500 inline-block";
          statusBadge.prepend(dot);
        }
      }

      function estimatePosting(RF, truckType, unit) {
        // Very approximate posting estimate
        let baseTonsUS;
        if (truckType === "hl93") {
          baseTonsUS = 40; // approximate legal truck
        } else if (truckType === "hs20") {
          baseTonsUS = 36;
        } else {
          const gvwInput = parseFloat(
            document.getElementById("user-truck-gvw").value || "0"
          );
          if (!isFinite(gvwInput) || gvwInput <= 0)
            return "Enter a valid permit truck GVW to estimate posting.";
          // Convert to tons if needed
          let gvwTons;
          if (unit === "us") {
            gvwTons = gvwInput / 2.0; // kips to tons (approx)
          } else {
            // kN to tons: 1 ton ≈ 8.896 kN
            gvwTons = gvwInput / 8.896;
          }
          const laneFactor = parseFloat(
            document.getElementById("user-truck-lane-factor").value || "1"
          );
          const impact = parseFloat(
            document.getElementById("user-truck-impact").value || "1.33"
          );
          const effTons = gvwTons * laneFactor * impact;
          const allowedTons = RF * effTons;
          return `Approximate permit posting capacity ≈ ${allowedTons.toFixed(
            1
          )} tons for the user-defined truck (including lane and dynamic factors).`;
        }

        const allowedTons = RF * baseTonsUS;
        if (unit === "metric") {
          const allowedTonnes = allowedTons * 0.907; // short ton to metric tonne
          return `Approximate posting: ${allowedTonnes.toFixed(
            1
          )} t (based on ${truckType.toUpperCase()} equivalent).`;
        }
        return `Approximate posting: ${allowedTons.toFixed(
          1
        )} tons (based on ${truckType.toUpperCase()} equivalent).`;
      }

      calcBtn.addEventListener("click", () => {
        const cap = parseFloat(document.getElementById("capacity").value);
        const dead = parseFloat(document.getElementById("dead-load").value);
        const live = parseFloat(document.getElementById("live-load").value);
        const level = document.getElementById("rating-level").value;
        const truckType = truckTypeSelect.value;

        if (
          ![cap, dead, live].every((v) => isFinite(v) && v >= 0) ||
          live === 0
        ) {
          errorEl.classList.remove("hidden");
          resultsContent.classList.add("hidden");
          return;
        }
        errorEl.classList.add("hidden");

        const RF = (cap - dead) / live;
        const RFclamped = isFinite(RF) ? RF : 0;

        ratingFactorDisplay.textContent = RFclamped.toFixed(2);
        demandCapacityDisplay.textContent = `${(dead + live).toFixed(
          2
        )} / ${cap.toFixed(2)} (${(((dead + live) / cap) * 100).toFixed(
          1
        )}% utilized)`;
        limitStateDisplay.textContent = "Strength limit state (simplified)";
        ratingLevelDisplay.textContent =
          level === "inventory"
            ? "Inventory rating (conservative)"
            : "Operating rating (occasional)";

        classifyRating(RFclamped);
        postingDisplay.textContent = estimatePosting(
          RFclamped,
          truckType,
          currentUnit
        );

        resultsContent.classList.remove("hidden");
      });

      resetBtn.addEventListener("click", () => {
        document.getElementById("bridge-rating-form").reset();
        setUnits("us");
        userTruckSection.classList.add("hidden");
        errorEl.classList.add("hidden");
        resultsContent.classList.add("hidden");
      });
    </script>

