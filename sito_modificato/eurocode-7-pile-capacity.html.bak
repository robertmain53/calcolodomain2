    <script>
      // Mobile menu
      document
        .getElementById("mobile-menu-toggle")
        ?.addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          if (!menu) return;
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          this.setAttribute("aria-expanded", String(isHidden));
        });

      // Default soil layers
      const layersTbody = document.getElementById("layersTbody");

      function createLayerRow(index, data) {
        const tr = document.createElement("tr");
        tr.className = index % 2 === 0 ? "bg-white" : "bg-gray-50";
        tr.innerHTML = `
      <td class="px-2 py-1 text-gray-700">L${index + 1}</td>
      <td class="px-2 py-1"><input type="number" class="zTop w-20 border rounded px-1 py-0.5" step="0.1" value="${
        data.zTop
      }"></td>
      <td class="px-2 py-1"><input type="number" class="zBot w-20 border rounded px-1 py-0.5" step="0.1" value="${
        data.zBot
      }"></td>
      <td class="px-2 py-1"><input type="number" class="gamma w-24 border rounded px-1 py-0.5" step="0.1" value="${
        data.gamma
      }"></td>
      <td class="px-2 py-1">
        <select class="soilType w-28 border rounded px-1 py-0.5">
          <option value="clay"${
            data.soilType === "clay" ? " selected" : ""
          }>Clay (undrained)</option>
          <option value="sand"${
            data.soilType === "sand" ? " selected" : ""
          }>Sand (drained)</option>
        </select>
      </td>
      <td class="px-2 py-1"><input type="number" class="cu w-20 border rounded px-1 py-0.5" step="1" value="${
        data.cu
      }"></td>
      <td class="px-2 py-1"><input type="number" class="phi w-20 border rounded px-1 py-0.5" step="0.5" value="${
        data.phi
      }"></td>
      <td class="px-2 py-1"><input type="number" class="alphaBeta w-20 border rounded px-1 py-0.5" step="0.01" value="${
        data.alphaBeta
      }"></td>
      <td class="px-2 py-1"><input type="number" class="Nq w-20 border rounded px-1 py-0.5" step="1" value="${
        data.Nq
      }"></td>
      <td class="px-2 py-1 text-right">
        <button type="button" class="removeLayer text-red-600 text-xs hover:underline">Remove</button>
      </td>
    `;
        return tr;
      }

      function initDefaultLayers() {
        layersTbody.innerHTML = "";
        const defaults = [
          {
            zTop: 0,
            zBot: 5,
            gamma: 18,
            soilType: "sand",
            phi: 32,
            cu: 0,
            alphaBeta: 0.02,
            Nq: 18,
          },
          {
            zTop: 5,
            zBot: 15,
            gamma: 19,
            soilType: "sand",
            phi: 35,
            cu: 0,
            alphaBeta: 0.03,
            Nq: 25,
          },
        ];
        defaults.forEach((d, i) => {
          layersTbody.appendChild(createLayerRow(i, d));
        });
      }

      initDefaultLayers();

      document.getElementById("addLayerBtn").addEventListener("click", () => {
        const rows = layersTbody.querySelectorAll("tr");
        let lastZ = 0;
        if (rows.length) {
          const lastRow = rows[rows.length - 1];
          lastZ = parseFloat(lastRow.querySelector(".zBot").value) || 0;
        }
        const newData = {
          zTop: lastZ,
          zBot: lastZ + 2,
          gamma: 18,
          soilType: "sand",
          cu: 0,
          phi: 32,
          alphaBeta: 0.02,
          Nq: 18,
        };
        layersTbody.appendChild(createLayerRow(rows.length, newData));
      });

      layersTbody.addEventListener("click", (e) => {
        if (e.target.classList.contains("removeLayer")) {
          const row = e.target.closest("tr");
          row?.remove();
          // re-label rows
          [...layersTbody.querySelectorAll("tr")].forEach((tr, i) => {
            tr.firstElementChild.textContent = "L" + (i + 1);
            tr.className = i % 2 === 0 ? "bg-white" : "bg-gray-50";
          });
        }
      });

      // Design approach selection
      document
        .getElementById("designApproach")
        .addEventListener("change", function () {
          const gammaRb = document.getElementById("gammaRb");
          const gammaRs = document.getElementById("gammaRs");
          const gammaF = document.getElementById("gammaF");
          if (this.value === "DA1-1") {
            gammaRb.value = 1.4;
            gammaRs.value = 1.4;
            gammaF.value = 1.35;
          } else if (this.value === "DA1-2") {
            gammaRb.value = 1.1;
            gammaRs.value = 1.1;
            gammaF.value = 1.35;
          }
        });

      function autoNq(phiDeg) {
        const phi = (phiDeg * Math.PI) / 180;
        if (phiDeg <= 0) return 0;
        const Nq =
          Math.exp(Math.PI * Math.tan(phi)) *
          Math.tan(Math.PI / 4 + phi / 2) ** 2;
        return Math.max(1, Math.min(80, Nq));
      }

      function formatNumber(x, decimals = 1) {
        if (!isFinite(x)) return "–";
        return x.toFixed(decimals);
      }

      document.getElementById("calculateBtn").addEventListener("click", () => {
        const errorEl = document.getElementById("calcError");
        errorEl.textContent = "";
        const pileDiameter = parseFloat(
          document.getElementById("pileDiameter").value
        );
        const pileLength = parseFloat(
          document.getElementById("pileLength").value
        );
        const gwLevel = parseFloat(document.getElementById("gwLevel").value);
        const gammaW = parseFloat(document.getElementById("gammaW").value);
        const gammaRb = parseFloat(document.getElementById("gammaRb").value);
        const gammaRs = parseFloat(document.getElementById("gammaRs").value);
        const gammaF = parseFloat(document.getElementById("gammaF").value);
        const NkChar = parseFloat(document.getElementById("NkChar").value) || 0;
        const TChar = parseFloat(document.getElementById("TChar").value) || 0;

        if (!(pileDiameter > 0 && pileLength > 0)) {
          errorEl.textContent = "Enter valid pile diameter and length.";
          return;
        }

        const rows = [...layersTbody.querySelectorAll("tr")];
        if (!rows.length) {
          errorEl.textContent = "Define at least one soil layer.";
          return;
        }

        // Collect layers
        const layers = rows
          .map((row) => {
            const zTop = parseFloat(row.querySelector(".zTop").value);
            const zBot = parseFloat(row.querySelector(".zBot").value);
            const gamma = parseFloat(row.querySelector(".gamma").value);
            const soilType = row.querySelector(".soilType").value;
            const cu = parseFloat(row.querySelector(".cu").value) || 0;
            const phi = parseFloat(row.querySelector(".phi").value) || 0;
            const alphaBeta =
              parseFloat(row.querySelector(".alphaBeta").value) || 0;
            let Nq = parseFloat(row.querySelector(".Nq").value);
            if (!Nq && phi > 0) Nq = autoNq(phi);
            return { zTop, zBot, gamma, soilType, cu, phi, alphaBeta, Nq };
          })
          .sort((a, b) => a.zTop - b.zTop);

        if (layers[0].zTop > 0 || layers[layers.length - 1].zBot < pileLength) {
          errorEl.textContent =
            "Soil layers must start at z = 0 m and extend at least to pile tip depth.";
          return;
        }

        const Ab = (Math.PI * Math.pow(pileDiameter, 2)) / 4;
        const u = Math.PI * pileDiameter;

        let RbChar = 0;
        let RsChar = 0;
        let sigmaVeffAtBase = 0;
        let layerBreakdownHtml = "";

        // Compute effective vertical stress at base and integrate shaft
        let z = 0;
        let sigmaVeff = 0;
        const dzStep = 0.1; // m

        while (z < pileLength - 1e-6) {
          const nextZ = Math.min(z + dzStep, pileLength);
          const midZ = 0.5 * (z + nextZ);

          const layer = layers.find((L) => midZ >= L.zTop && midZ <= L.zBot);
          if (!layer) {
            z = nextZ;
            continue;
          }

          const gamma = layer.gamma;
          const gammaEff =
            midZ <= gwLevel ? gamma : Math.max(0, gamma - gammaW);
          sigmaVeff += gammaEff * (nextZ - z);

          // Shaft resistance
          let qs = 0;
          if (layer.soilType === "clay" && layer.cu > 0) {
            qs = layer.alphaBeta * layer.cu; // kPa
          } else {
            qs = layer.alphaBeta * sigmaVeff; // kPa
          }
          const dRs = (qs * u * (nextZ - z)) / 1000; // kN (kPa * m² / 1000)

          RsChar += dRs;
          z = nextZ;
        }

        sigmaVeffAtBase = sigmaVeff;

        // Base resistance from layer at pile tip
        const baseLayer =
          layers.find((L) => pileLength >= L.zTop && pileLength <= L.zBot) ||
          layers[layers.length - 1];
        if (baseLayer.soilType === "clay" && baseLayer.cu > 0) {
          const Nc = 9;
          RbChar = (Nc * baseLayer.cu * Ab) / 1000; // kN
          layerBreakdownHtml += `<p>Base in clay: Nc = 9, c<sub>u</sub> = ${formatNumber(
            baseLayer.cu,
            1
          )} kPa → R<sub>b;k</sub> = ${formatNumber(RbChar, 1)} kN</p>`;
        } else {
          const Nq = baseLayer.Nq || autoNq(baseLayer.phi);
          const qb = sigmaVeffAtBase * Nq; // kPa
          RbChar = (qb * Ab) / 1000; // kN
          layerBreakdownHtml += `<p>Base in sand: σ'<sub>v;b</sub> = ${formatNumber(
            sigmaVeffAtBase,
            1
          )} kPa, N<sub>q</sub> = ${formatNumber(
            Nq,
            1
          )} → R<sub>b;k</sub> = ${formatNumber(RbChar, 1)} kN</p>`;
        }

        // Layer-by-layer shaft summary
        layers.forEach((L, i) => {
          const segTop = Math.max(L.zTop, 0);
          const segBot = Math.min(L.zBot, pileLength);
          if (segBot <= segTop) return;
          const segLen = segBot - segTop;
          // approximate average effective stress in layer segment
          let sigmaTop = 0,
            sigmaBot = 0;
          let zTmp = 0,
            sigmaTmp = 0;
          const computeSigma = (targetZ) => {
            if (targetZ <= 0) return 0;
            let s = 0,
              zc = 0;
            layers.forEach((layer) => {
              const lt = layer.zTop;
              const lb = layer.zBot;
              if (targetZ <= lt) return;
              const from = Math.max(lt, 0);
              const to = Math.min(lb, targetZ);
              if (to <= from) return;
              const mid = 0.5 * (from + to);
              const gammaEff =
                mid <= gwLevel
                  ? layer.gamma
                  : Math.max(0, layer.gamma - gammaW);
              s += gammaEff * (to - from);
            });
            return s;
          };
          sigmaTop = computeSigma(segTop);
          sigmaBot = computeSigma(segBot);
          const sigmaAvg = 0.5 * (sigmaTop + sigmaBot);

          let qs;
          if (L.soilType === "clay" && L.cu > 0) {
            qs = L.alphaBeta * L.cu;
          } else {
            qs = L.alphaBeta * sigmaAvg;
          }
          const RsLayer = (qs * u * segLen) / 1000;
          layerBreakdownHtml += `<p>Layer L${i + 1}: z = ${formatNumber(
            segTop,
            1
          )}–${formatNumber(segBot, 1)} m, q<sub>s</sub> ≈ ${formatNumber(
            qs,
            1
          )} kPa → R<sub>s;k</sub> ≈ ${formatNumber(RsLayer, 1)} kN</p>`;
        });

        const RcChar = RbChar + RsChar;
        const RtChar = RsChar;

        const RcDesign = RbChar / gammaRb + RsChar / gammaRs;
        const RtDesign = RsChar / gammaRs;

        const NdDesign = NkChar * gammaF;
        const TdDesign = TChar * gammaF;

        document.getElementById("RbChar").textContent = formatNumber(RbChar, 1);
        document.getElementById("RsChar").textContent = formatNumber(RsChar, 1);
        document.getElementById("RcChar").textContent = formatNumber(RcChar, 1);
        document.getElementById("RtChar").textContent = formatNumber(RtChar, 1);

        document.getElementById("RcDesign").textContent = formatNumber(
          RcDesign,
          1
        );
        document.getElementById("RtDesign").textContent = formatNumber(
          RtDesign,
          1
        );
        document.getElementById("NdDesign").textContent = formatNumber(
          NdDesign,
          1
        );
        document.getElementById("TdDesign").textContent = formatNumber(
          TdDesign,
          1
        );

        const utilComp = NdDesign > 0 && RcDesign > 0 ? NdDesign / RcDesign : 0;
        const utilTens = TdDesign > 0 && RtDesign > 0 ? TdDesign / RtDesign : 0;

        document.getElementById("utilComp").textContent = utilComp
          ? formatNumber(utilComp, 2)
          : "–";
        document.getElementById("utilTens").textContent = utilTens
          ? formatNumber(utilTens, 2)
          : "–";

        const governing =
          utilComp >= utilTens
            ? "Compression governs the design."
            : "Tension governs the design.";
        document.getElementById("governingCheck").textContent =
          utilComp || utilTens ? governing : "No design load specified.";
        document.getElementById("ulsCheckBox").hidden = false;

        document.getElementById("layerBreakdown").innerHTML =
          layerBreakdownHtml || "<p>No breakdown available.</p>";
      });

      document.getElementById("resetBtn").addEventListener("click", () => {
        document.getElementById("pileType").value = "bored";
        document.getElementById("pileDiameter").value = 0.6;
        document.getElementById("pileLength").value = 15;
        document.getElementById("gwLevel").value = 3;
        document.getElementById("gammaW").value = 9.81;
        document.getElementById("designApproach").value = "DA1-1";
        document.getElementById("gammaRb").value = 1.4;
        document.getElementById("gammaRs").value = 1.4;
        document.getElementById("gammaF").value = 1.35;
        document.getElementById("NkChar").value = 1500;
        document.getElementById("TChar").value = 200;
        initDefaultLayers();
        document.getElementById("calcError").textContent = "";
        [
          "RbChar",
          "RsChar",
          "RcChar",
          "RtChar",
          "RcDesign",
          "RtDesign",
          "NdDesign",
          "TdDesign",
          "utilComp",
          "utilTens",
          "governingCheck",
        ].forEach((id) => {
          document.getElementById(id).textContent = "–";
        });
        document.getElementById("ulsCheckBox").hidden = true;
        document.getElementById("layerBreakdown").innerHTML = "";
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
