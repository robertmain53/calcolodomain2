      function calcGcsPoints(e, v, m) {
        const eVal = parseInt(e, 10);
        const vVal = parseInt(v, 10);
        const mVal = parseInt(m, 10);
        if (isNaN(eVal) || isNaN(vVal) || isNaN(mVal)) return 0;
        const total = eVal + vVal + mVal;
        const pts = 15 - total;
        return pts < 0 ? 0 : pts;
      }

      function calcMortality(score, emergencyPostop) {
        if (score == null || isNaN(score)) return null;
        const E = emergencyPostop ? 1 : 0;
        const logit = -3.517 + 0.146 * score + 0.603 * E;
        const odds = Math.exp(logit);
        const prob = odds / (1 + odds);
        return prob;
      }

      function categorizeRisk(score) {
        if (score == null || isNaN(score))
          return { label: "No category", color: "bg-gray-100 text-gray-700" };
        if (score < 10)
          return { label: "Low risk", color: "bg-green-100 text-green-800" };
        if (score < 20)
          return {
            label: "Moderate risk",
            color: "bg-yellow-100 text-yellow-800",
          };
        if (score < 30)
          return { label: "High risk", color: "bg-orange-100 text-orange-800" };
        return { label: "Very high risk", color: "bg-red-100 text-red-800" };
      }

      document.getElementById("calculate-btn").addEventListener("click", () => {
        const age = parseFloat(document.getElementById("age").value);
        let map = parseFloat(document.getElementById("map").value);
        const sbp = parseFloat(document.getElementById("sbp").value);
        const dbp = parseFloat(document.getElementById("dbp").value);
        if (!isNaN(sbp) && !isNaN(dbp)) {
          map = (sbp + 2 * dbp) / 3;
        }

        const temp = parseFloat(document.getElementById("temp").value);
        const hr = parseFloat(document.getElementById("hr").value);
        const rr = parseFloat(document.getElementById("rr").value);
        const pao2 = parseFloat(document.getElementById("pao2").value);
        const aagrad = parseFloat(document.getElementById("aagrad").value);
        const ph = parseFloat(document.getElementById("ph").value);
        const na = parseFloat(document.getElementById("na").value);
        const k = parseFloat(document.getElementById("k").value);
        const creat = parseFloat(document.getElementById("creat").value);
        const arf = document.getElementById("creat-arf").checked;
        const hct = parseFloat(document.getElementById("hct").value);
        const wbc = parseFloat(document.getElementById("wbc").value);

        const gcsE = document.getElementById("gcs-e").value;
        const gcsV = document.getElementById("gcs-v").value;
        const gcsM = document.getElementById("gcs-m").value;

        const chronicRadio = document.querySelector(
          'input[name="chronic"]:checked'
        );
        const chronicPoints = chronicRadio
          ? parseInt(chronicRadio.value, 10)
          : 0;

        const admissionTypeRadio = document.querySelector(
          'input[name="admission-type"]:checked'
        );
        const isEmergencyPostop =
          admissionTypeRadio && admissionTypeRadio.value === "1";

        const agePts = calcAgePoints(age);
        const tempPts = calcTempPoints(temp);
        const mapPts = calcMapPoints(map);
        const hrPts = calcHrPoints(hr);
        const rrPts = calcRrPoints(rr);
        const oxyPts = calcOxyPoints(pao2, aagrad, useAa.checked);
        const phPts = calcPhPoints(ph);
        const naPts = calcNaPoints(na);
        const kPts = calcKPoints(k);
        const creatPts = calcCreatPoints(creat, arf);
        const hctPts = calcHctPoints(hct);
        const wbcPts = calcWbcPoints(wbc);
        const gcsPts = calcGcsPoints(gcsE, gcsV, gcsM);

        const physioScore =
          tempPts +
          mapPts +
          hrPts +
          rrPts +
          oxyPts +
          phPts +
          naPts +
          kPts +
          creatPts +
          hctPts +
          wbcPts +
          gcsPts;
        const totalScore = physioScore + agePts + chronicPoints;

        const mort = calcMortality(totalScore, isEmergencyPostop);

        document.getElementById("physio-score").textContent = isNaN(physioScore)
          ? "–"
          : physioScore.toString();
        document.getElementById("age-score").textContent = isNaN(agePts)
          ? "–"
          : agePts.toString();
        document.getElementById("gcs-score").textContent = isNaN(gcsPts)
          ? "–"
          : gcsPts.toString();
        document.getElementById("chronic-score").textContent = isNaN(
          chronicPoints
        )
          ? "–"
          : chronicPoints.toString();
        document.getElementById("apache-score").textContent = isNaN(totalScore)
          ? "–"
          : totalScore.toString();

        const mortEl = document.getElementById("mortality");
        if (mort == null || isNaN(mort)) {
          mortEl.textContent = "–";
        } else {
          mortEl.textContent = (mort * 100).toFixed(1) + "%";
        }

        const riskCat = categorizeRisk(totalScore);
        const riskEl = document.getElementById("risk-category");
        riskEl.textContent = riskCat.label;
        riskEl.className = "mt-1 text-xs badge inline-block " + riskCat.color;
      });

      document.getElementById("reset-btn").addEventListener("click", () => {
        document
          .querySelectorAll('input[type="number"]')
          .forEach((el) => (el.value = ""));
        document.getElementById("creat-arf").checked = false;
        document.getElementById("gcs-e").value = "";
        document.getElementById("gcs-v").value = "";
        document.getElementById("gcs-m").value = "";
        document.querySelector(
          'input[name="chronic"][value="0"]'
        ).checked = true;
        document.querySelector(
          'input[name="admission-type"][value="0"]'
        ).checked = true;
        usePao2.checked = true;
        pao2Input.disabled = false;
        aagradInput.disabled = true;

        document.getElementById("apache-score").textContent = "–";
        document.getElementById("physio-score").textContent = "–";
        document.getElementById("age-score").textContent = "–";
        document.getElementById("gcs-score").textContent = "–";
        document.getElementById("chronic-score").textContent = "–";
        document.getElementById("mortality").textContent = "–";
        const riskEl = document.getElementById("risk-category");
        riskEl.textContent = "No category";
        riskEl.className =
          "mt-1 text-xs badge inline-block bg-gray-100 text-gray-700";
      });

