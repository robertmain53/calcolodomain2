              : r.inclusiveMin
              ? value >= r.min
              : value > r.min;
          const maxOk =
            r.max === null
              ? true
              : r.inclusiveMax
              ? value <= r.max
              : value < r.max;
          if (minOk && maxOk) return r.points;
        }
        return 0;
      }

