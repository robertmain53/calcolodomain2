    <script>
      // Utility: format currency
      function fmt(n) {
        if (isNaN(n)) return "$0.00";
        return n.toLocaleString("en-US", {
          style: "currency",
          currency: "USD",
        });
      }

      function getPayPeriods(freq) {
        switch (freq) {
          case "weekly":
            return 52;
          case "biweekly":
            return 26;
          case "semimonthly":
            return 24;
          case "monthly":
            return 12;
          case "annual":
            return 1;
          default:
            return 26;
        }
      }

      function calcNjTaxAnnual(annualTaxable, status, allowances) {
        if (annualTaxable <= 0) return 0;

        // Very rough allowance adjustment (illustrative only)
        const allowanceValue = 1000; // per allowance
        const adjIncome = Math.max(
          0,
          annualTaxable - allowances * allowanceValue
        );

        let tax = 0;
        const brackets = [
          { upTo: 20000, rate: 0.014 },
          { upTo: 35000, rate: 0.0175 },
          { upTo: 40000, rate: 0.035 },
          { upTo: 75000, rate: 0.05525 },
          { upTo: 500000, rate: 0.0637 },
          { upTo: Infinity, rate: 0.0897 },
        ];

        let prev = 0;
        for (const b of brackets) {
          const taxableHere = Math.min(adjIncome, b.upTo) - prev;
          if (taxableHere > 0) {
            tax += taxableHere * b.rate;
            prev = b.upTo;
          } else {
            break;
          }
        }

        // Simple marital adjustment: married gets 5% discount in this approximation
        if (status === "married") {
          tax *= 0.95;
        }
        return tax;
      }

      function calcFicaPerPeriod(taxableWages, annualTaxable, periodsPerYear) {
        const ssRate = 0.062;
        const medicareRate = 0.0145;
        const ssWageBase = 168600; // example 2024 base; illustrative

        const annualSSWages = Math.min(annualTaxable, ssWageBase);
        const annualMedicareWages = annualTaxable;

        const annualEmployeeFica =
          annualSSWages * ssRate + annualMedicareWages * medicareRate;
        const perPeriodEmployeeFica = annualEmployeeFica / periodsPerYear;

        return {
          employee: perPeriodEmployeeFica,
          employer: perPeriodEmployeeFica, // match
        };
      }

      function calcLocalPayrollTaxPerPeriod(
        taxableWages,
        workCity,
        residence,
        periodsPerYear
      ) {
        let rate = 0;
        let note = "";

        if (workCity === "jersey-city") {
          // Example: 1% on non-residents only
          if (residence === "resident") {
            rate = 0;
            note =
              "Jersey City resident selected – local payroll tax assumed 0% on resident wages (illustrative).";
          } else {
            rate = 0.01;
            note =
              "Jersey City non-resident – local payroll tax assumed 1% of wages (illustrative).";
          }
        } else if (workCity === "newark") {
          // Example: 1% on all wages
          rate = 0.01;
          note =
            "Newark – local payroll tax assumed 1% of wages (illustrative).";
        } else {
          rate = 0;
          note = "No local payroll tax for selected work location.";
        }

        const annualLocal = taxableWages * periodsPerYear * rate;
        const perPeriodLocal = annualLocal / periodsPerYear;

        return { perPeriodLocal, rate, note };
      }

      function recalc() {
        const freq = document.getElementById("pay-frequency").value;
        const periods = getPayPeriods(freq);

        const gross =
          parseFloat(document.getElementById("gross-pay").value) || 0;
        const pretax401k =
          parseFloat(document.getElementById("pre-tax-401k").value) || 0;
        const pretax125 =
          parseFloat(document.getElementById("pre-tax-section125").value) || 0;

        const workCity = document.getElementById("work-city").value;
        const residence = document.getElementById("residence-city").value;
        const status = document.getElementById("filing-status").value;
        const allowances =
          parseInt(document.getElementById("allowances").value, 10) || 0;

        const pretaxTotal = Math.min(gross, pretax401k + pretax125);
        const taxable = Math.max(0, gross - pretaxTotal);
        const annualTaxable = taxable * periods;

        const annualNjTax = calcNjTaxAnnual(annualTaxable, status, allowances);
        const njTaxPerPeriod = annualNjTax / periods;

        const fica = calcFicaPerPeriod(taxable, annualTaxable, periods);
        const otherEmployeeTaxes = 0; // placeholder

        const local = calcLocalPayrollTaxPerPeriod(
          taxable,
          workCity,
          residence,
          periods
        );

        const netPay =
          gross -
          pretaxTotal -
          njTaxPerPeriod -
          fica.employee -
          otherEmployeeTaxes;

        const employerUnempPlaceholder = 15; // flat placeholder per period
        const employerTotal =
          gross +
          fica.employer +
          employerUnempPlaceholder +
          local.perPeriodLocal;

        // Update UI
        document.getElementById("res-gross").textContent = fmt(gross);
        document.getElementById("res-pretax").textContent =
          "-" + fmt(pretaxTotal);
        document.getElementById("res-taxable").textContent = fmt(taxable);

        document.getElementById("res-nj-tax").textContent =
          "-" + fmt(njTaxPerPeriod);
        document.getElementById("res-fica").textContent =
          "-" + fmt(fica.employee);
        document.getElementById("res-other-taxes").textContent =
          "-" + fmt(otherEmployeeTaxes);
        document.getElementById("res-net").textContent = fmt(netPay);

        document.getElementById("res-employer-fica").textContent = fmt(
          fica.employer
        );
        document.getElementById("res-employer-unemp").textContent = fmt(
          employerUnempPlaceholder
        );
        document.getElementById("res-local-tax").textContent = fmt(
          local.perPeriodLocal
        );
        document.getElementById("res-employer-total").textContent =
          fmt(employerTotal);

        const noteEl = document.getElementById("res-local-note");
        noteEl.textContent =
          local.note + " This is an approximation for planning only.";
      }

      document
        .getElementById("calculate-btn")
        .addEventListener("click", function (e) {
          e.preventDefault();
          recalc();
        });

      // Auto-recalc on input changes for better UX
      [
        "pay-frequency",
        "gross-pay",
        "pre-tax-401k",
        "pre-tax-section125",
        "work-city",
        "residence-city",
        "filing-status",
        "allowances",
      ].forEach((id) => {
        const el = document.getElementById(id);
        if (el) {
          el.addEventListener("change", recalc);
          el.addEventListener("input", recalc);
        }
      });

      // Initial calculation
      recalc();

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
