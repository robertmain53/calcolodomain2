    <script>
      // NATO mapping
      const NATO_MAP = {
        A: "Alfa",
        B: "Bravo",
        C: "Charlie",
        D: "Delta",
        E: "Echo",
        F: "Foxtrot",
        G: "Golf",
        H: "Hotel",
        I: "India",
        J: "Juliett",
        K: "Kilo",
        L: "Lima",
        M: "Mike",
        N: "November",
        O: "Oscar",
        P: "Papa",
        Q: "Quebec",
        R: "Romeo",
        S: "Sierra",
        T: "Tango",
        U: "Uniform",
        V: "Victor",
        W: "Whiskey",
        X: "X-ray",
        Y: "Yankee",
        Z: "Zulu",
        0: "Zero",
        1: "One",
        2: "Two",
        3: "Three",
        4: "Four",
        5: "Five",
        6: "Six",
        7: "Seven",
        8: "Eight",
        9: "Nine",
      };

      const NATO_REVERSE_MAP = {};
      Object.keys(NATO_MAP).forEach((ch) => {
        NATO_REVERSE_MAP[NATO_MAP[ch].toLowerCase()] = ch;
      });

      function getSeparator() {
        const sel = document.getElementById("separator-select").value;
        if (sel === "space") return " ";
        if (sel === "hyphen") return "-";
        if (sel === "newline") return "\n";
        return " ";
      }

      function textToNato(text) {
        const sep = getSeparator();
        const result = [];
        for (let i = 0; i < text.length; i++) {
          const ch = text[i];
          const upper = ch.toUpperCase();
          if (NATO_MAP[upper]) {
            result.push(NATO_MAP[upper]);
          } else if (ch === " ") {
            // preserve word boundary as double separator
            result.push(sep === "\n" ? "\n\n" : "  ");
          } else {
            // punctuation or other characters: keep as-is
            result.push(ch);
          }
          if (i < text.length - 1 && NATO_MAP[upper]) {
            result.push(sep);
          }
        }
        return result.join("").replace(/(\s|\n)+$/, "");
      }

      function natoToText(text) {
        // Normalize separators: treat hyphens and newlines as spaces
        const normalized = text.replace(/[\n\r\-]+/g, " ");
        const tokens = normalized.split(/\s+/).filter((t) => t.length > 0);
        let output = "";
        tokens.forEach((token, idx) => {
          const key = token.toLowerCase();
          if (NATO_REVERSE_MAP[key]) {
            output += NATO_REVERSE_MAP[key];
          } else if (/^[0-9]$/.test(token)) {
            // raw digit
            output += token;
          } else {
            // unknown token: keep as-is, separated by space
            if (idx > 0) output += " ";
            output += token;
          }
        });
        return output;
      }

      function updateCharCount() {
        const len = document.getElementById("input-text").value.length;
        document.getElementById("char-count").textContent =
          len + " character" + (len === 1 ? "" : "s");
      }

      function updateHint() {
        const dir = document.getElementById("direction-select").value;
        const hint = document.getElementById("input-hint");
        if (dir === "text-to-nato") {
          hint.textContent =
            "Tip: Type any text. Letters A–Z and digits 0–9 are converted to NATO words; punctuation is preserved.";
        } else {
          hint.textContent =
            "Tip: Paste NATO words like “Hotel Echo Lima Lima Oscar 1 2 3”. Unknown words are kept as-is.";
        }
      }

      function translate() {
        const dir = document.getElementById("direction-select").value;
        const input = document.getElementById("input-text").value;
        let output = "";
        if (dir === "text-to-nato") {
          output = textToNato(input);
        } else {
          output = natoToText(input);
        }
        document.getElementById("output-text").value = output;
        updateCharCount();
      }

      document.addEventListener("DOMContentLoaded", () => {
        const inputEl = document.getElementById("input-text");
        const directionEl = document.getElementById("direction-select");
        const separatorEl = document.getElementById("separator-select");

        inputEl.addEventListener("input", translate);
        directionEl.addEventListener("change", () => {
          updateHint();
          translate();
        });
        separatorEl.addEventListener("change", translate);

        document.getElementById("clear-input").addEventListener("click", () => {
          inputEl.value = "";
          translate();
        });

        document
          .getElementById("clear-output")
          .addEventListener("click", () => {
            document.getElementById("output-text").value = "";
          });

        document
          .getElementById("copy-output")
          .addEventListener("click", async () => {
            const out = document.getElementById("output-text").value;
            if (!out) return;
            try {
              await navigator.clipboard.writeText(out);
              const btn = document.getElementById("copy-output");
              const old = btn.textContent;
              btn.textContent = "Copied!";
              setTimeout(() => {
                btn.textContent = old;
              }, 1200);
            } catch (e) {
              alert("Copy failed. Please copy manually.");
            }
          });

        document
          .getElementById("swap-direction")
          .addEventListener("click", () => {
            const dir = directionEl.value;
            directionEl.value =
              dir === "text-to-nato" ? "nato-to-text" : "text-to-nato";
            // Move current output into input for quick reverse
            const currentOutput = document.getElementById("output-text").value;
            inputEl.value = currentOutput;
            updateHint();
            translate();
          });

        document.getElementById("example-btn").addEventListener("click", () => {
          const dir = directionEl.value;
          if (dir === "text-to-nato") {
            inputEl.value = "CALLSIGN AB123 – MEET AT GATE 4B";
          } else {
            inputEl.value =
              "Charlie Alfa Lima Lima Sierra India Golf November Alfa Bravo One Two Three";
          }
          translate();
        });

        updateHint();
        translate();
      });
    </script>

