    <script>
      // Utility: unit conversions
      function lbToKg(lb) {
        return lb * 0.45359237;
      }
      function kgToLb(kg) {
        return kg / 0.45359237;
      }

      function round(value, decimals) {
        if (!isFinite(value)) return NaN;
        const factor = Math.pow(10, decimals);
        return Math.round(value * factor) / factor;
      }

      function computeRER(weightKg) {
        if (weightKg <= 0) return NaN;
        return 70 * Math.pow(weightKg, 0.75);
      }

      function getBaseFactor(lifeStage, neuterStatus, activityLevel) {
        // Base factors before goal adjustment
        if (lifeStage === "puppy-0-4") return 3.0;
        if (lifeStage === "puppy-4-12") return 2.0;
        if (lifeStage === "lactating") return 2.5; // mid-range; real needs may be higher with large litters

        // Adult / senior
        let factor = 1.6; // neutered, moderate
        if (neuterStatus === "intact") factor = 1.8;

        if (activityLevel === "low") {
          factor -= 0.2;
        } else if (activityLevel === "high") {
          factor += 0.4;
        }

        // Seniors often need slightly fewer calories
        if (lifeStage === "senior") {
          factor -= 0.1;
        }

        return Math.max(1.0, factor);
      }

      function applyGoalAdjustment(baseFactor, goal) {
        if (goal === "lose") {
          // For weight loss we will instead compute from target weight with ~0.8 factor
          return baseFactor; // placeholder, handled separately
        }
        if (goal === "gain") {
          return baseFactor + 0.2;
        }
        return baseFactor;
      }

      function calculateDogCalories() {
        const weightInput = document.getElementById("dog-weight");
        const weightUnit = document.getElementById("dog-weight-unit").value;
        const targetWeightInput = document.getElementById("target-weight");
        const targetWeightUnit =
          document.getElementById("target-weight-unit").value;
        const lifeStage = document.getElementById("life-stage").value;
        const neuterStatus = document.getElementById("neuter-status").value;
        const activityLevel = document.getElementById("activity-level").value;
        const goal = document.getElementById("goal").value;
        const foodKcalPerUnit = parseFloat(
          document.getElementById("food-kcal-per-unit").value
        );
        const foodUnit = document.getElementById("food-unit").value;

        const errorEl = document.getElementById("error-message");
        errorEl.classList.add("hidden");
        errorEl.textContent = "";

        let weight = parseFloat(weightInput.value);
        if (!isFinite(weight) || weight <= 0) {
          errorEl.textContent =
            "Please enter a valid current weight for your dog.";
          errorEl.classList.remove("hidden");
          return;
        }
        if (weightUnit === "lb") weight = lbToKg(weight);

        let targetWeight = parseFloat(targetWeightInput.value);
        if (!isFinite(targetWeight) || targetWeight <= 0) {
          targetWeight = weight; // default to current if not provided
        } else if (targetWeightUnit === "lb") {
          targetWeight = lbToKg(targetWeight);
        }

        // Compute RER based on appropriate weight
        const rerCurrent = computeRER(weight);
        const rerTarget = computeRER(targetWeight);

        if (!isFinite(rerCurrent) || rerCurrent <= 0) {
          errorEl.textContent =
            "Unable to compute energy requirement. Please check the weight value.";
          errorEl.classList.remove("hidden");
          return;
        }

        const baseFactor = getBaseFactor(
          lifeStage,
          neuterStatus,
          activityLevel
        );
        let factorForDisplay = baseFactor;
        let mer;

        if (goal === "lose") {
          // For weight loss, use target weight and reduced factor
          const lossFactor = 0.8; // conservative; many vets use 0.8 x RER(target)
          mer = rerTarget * lossFactor;
          factorForDisplay = round(mer / rerCurrent, 2); // relative to current RER for info
        } else if (goal === "gain") {
          const gainFactor = applyGoalAdjustment(baseFactor, goal);
          mer = rerTarget * gainFactor;
          factorForDisplay = gainFactor;
        } else {
          const adjFactor = applyGoalAdjustment(baseFactor, goal);
          mer = rerCurrent * adjFactor;
          factorForDisplay = adjFactor;
        }

        // Suggested range ±10%
        const lower = mer * 0.9;
        const upper = mer * 1.1;

        // Update UI
        document.getElementById("results-section").classList.remove("hidden");
        document.getElementById("rer-output").textContent =
          Math.round(rerCurrent);
        document.getElementById("mer-output").textContent = Math.round(mer);
        document.getElementById("factor-output").textContent = factorForDisplay
          ? factorForDisplay.toFixed(2)
          : "–";
        document.getElementById("range-output").textContent = `${Math.round(
          lower
        )} – ${Math.round(upper)}`;

        // Food amount
        const foodRow = document.getElementById("food-amount-row");
        if (isFinite(foodKcalPerUnit) && foodKcalPerUnit > 0) {
          const kcalPerUnit = foodKcalPerUnit;
          let amountPerDay;
          let label;
          if (foodUnit === "cup") {
            amountPerDay = mer / kcalPerUnit;
            label = `${round(amountPerDay, 2)} cups/day`;
          } else if (foodUnit === "100g") {
            const kcalPerGram = kcalPerUnit / 100;
            amountPerDay = mer / kcalPerGram; // grams
            label = `${Math.round(amountPerDay)} g/day`;
          } else {
            // per kg
            const kcalPerGram = kcalPerUnit / 1000;
            amountPerDay = mer / kcalPerGram; // grams
            label = `${Math.round(amountPerDay)} g/day`;
          }
          document.getElementById("food-amount-output").textContent = label;

          // Suggest splitting into meals
          let mealsText;
          if (lifeStage === "puppy-0-4" || lifeStage === "puppy-4-12") {
            mealsText = "Split into 3–4 meals per day for puppies.";
          } else if (lifeStage === "lactating") {
            mealsText =
              "Offer multiple small meals or free-choice feeding as advised by your vet.";
          } else {
            mealsText =
              "Split into 2 meals per day for most adult dogs (some do well on 3 smaller meals).";
          }
          document.getElementById("meals-output").textContent = mealsText;

          const noteEl = document.getElementById("food-amount-note");
          noteEl.textContent =
            foodUnit === "cup"
              ? "Based on the kcal per cup value you entered for this specific food."
              : "Based on the kcal per 100 g or per kg value you entered for this specific food.";

          foodRow.classList.remove("hidden");
        } else {
          foodRow.classList.add("hidden");
        }

        // Weight plan info
        const planRow = document.getElementById("weight-plan-row");
        const planSummary = document.getElementById("weight-plan-summary");
        const planNote = document.getElementById("weight-plan-note");

        if (goal === "lose" || goal === "gain") {
          const diffKg = targetWeight - weight;
          const direction = diffKg < 0 ? "lose" : "gain";
          const absDiff = Math.abs(diffKg);
          // Rough estimate: 1 kg body weight ≈ 7,000 kcal (varies by body composition)
          const kcalPerKg = 7000;
          const totalKcalChange = absDiff * kcalPerKg;
          const dailyDeficitOrSurplus = Math.abs(mer - rerCurrent);
          let weeks = NaN;
          if (dailyDeficitOrSurplus > 0) {
            weeks = totalKcalChange / (dailyDeficitOrSurplus * 7);
          }

          if (absDiff > 0.1 && isFinite(weeks)) {
            planSummary.textContent = `At this calorie level, your dog could ${direction} about ${absDiff.toFixed(
              1
            )} kg over roughly ${weeks.toFixed(
              1
            )} weeks, assuming steady progress.`;
          } else {
            planSummary.textContent =
              "This plan makes only a small change from current weight, so expected weight change per week will be modest.";
          }

          planNote.textContent =
            "These timelines are rough estimates based on average energy density of body tissue. Healthy weight change is usually around 1–2% of body weight per week. Your veterinarian can tailor a safer, more precise plan.";
          planRow.classList.remove("hidden");
        } else {
          planRow.classList.add("hidden");
        }
      }

      document
        .getElementById("calculate-btn")
        .addEventListener("click", function (e) {
          e.preventDefault();
          calculateDogCalories();
        });
    </script>

