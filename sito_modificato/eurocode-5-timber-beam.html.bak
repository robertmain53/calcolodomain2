    <script>
      // Timber class database (approximate EN 338 values)
      const timberClasses = {
        C14: { fmk: 14, fvk: 2.0, E0: 7000, rho: 290 },
        C16: { fmk: 16, fvk: 2.5, E0: 8000, rho: 350 },
        C18: { fmk: 18, fvk: 2.5, E0: 9000, rho: 370 },
        C24: { fmk: 24, fvk: 2.5, E0: 11000, rho: 420 },
        C30: { fmk: 30, fvk: 3.0, E0: 12000, rho: 460 },
        GL24h: { fmk: 24, fvk: 2.5, E0: 11500, rho: 385 },
        GL28h: { fmk: 28, fvk: 3.5, E0: 13000, rho: 410 },
      };

      const gammaG = 1.35;
      const gammaQ = 1.5;

      function setTimberDefaults() {
        const cls = document.getElementById("timber-class").value;
        if (cls === "custom") return;
        const data = timberClasses[cls];
        if (!data) return;
        document.getElementById("fmk").value = data.fmk;
        document.getElementById("fvk").value = data.fvk;
        document.getElementById("e0mean").value = data.E0;
        document.getElementById("rho-k").value = data.rho;
      }

      document
        .getElementById("timber-class")
        .addEventListener("change", setTimberDefaults);

      document.getElementById("reset-btn").addEventListener("click", () => {
        document.getElementById("span").value = 4;
        document.getElementById("width").value = 100;
        document.getElementById("depth").value = 300;
        document.getElementById("gk").value = 3.0;
        document.getElementById("qk").value = 2.0;
        document.getElementById("kmod").value = 0.8;
        document.getElementById("gamma-m").value = 1.3;
        document.getElementById("timber-class").value = "C16";
        setTimberDefaults();
        document.getElementById("calc-error").textContent = "";
        document.getElementById("results-container").innerHTML =
          '<div class="p-4 bg-gray-50 border border-dashed border-gray-300 rounded text-sm text-gray-600">Enter span, section, loads and timber class, then click “Calculate design checks” to see utilisation ratios for bending, shear and deflection.</div>';
      });

      document.getElementById("calculate-btn").addEventListener("click", () => {
        const errEl = document.getElementById("calc-error");
        errEl.textContent = "";

        const L = parseFloat(document.getElementById("span").value); // m
        const bmm = parseFloat(document.getElementById("width").value); // mm
        const hmm = parseFloat(document.getElementById("depth").value); // mm
        const Gk = parseFloat(document.getElementById("gk").value); // kN/m
        const Qk = parseFloat(document.getElementById("qk").value); // kN/m
        const kmod = parseFloat(document.getElementById("kmod").value);
        const gammaM = parseFloat(document.getElementById("gamma-m").value);
        const fmk = parseFloat(document.getElementById("fmk").value);
        const fvk = parseFloat(document.getElementById("fvk").value);
        const E0 = parseFloat(document.getElementById("e0mean").value);
        const rho = parseFloat(document.getElementById("rho-k").value);

        if (
          [L, bmm, hmm, Gk, Qk, kmod, gammaM, fmk, fvk, E0, rho].some(
            (v) => !isFinite(v) || v <= 0
          )
        ) {
          errEl.textContent =
            "Please enter positive numeric values for all inputs.";
          return;
        }

        // Convert section to meters
        const b = bmm / 1000; // m
        const h = hmm / 1000; // m

        // Section properties
        const W = (b * Math.pow(h, 2)) / 6; // m^3
        const I = (b * Math.pow(h, 3)) / 12; // m^4

        // Design loads
        const qd = gammaG * Gk + gammaQ * Qk; // kN/m
        const qser = Gk + Qk; // kN/m

        // Internal forces (simply supported, UDL)
        const MEd_kNm = (qd * Math.pow(L, 2)) / 8; // kNm
        const VEd_kN = (qd * L) / 2; // kN

        // Convert to N and Nmm for stresses
        const MEd_Nmm = MEd_kNm * 1e6; // 1 kNm = 1e6 Nmm
        const VEd_N = VEd_kN * 1e3; // 1 kN = 1e3 N

        const W_mm3 = (bmm * Math.pow(hmm, 2)) / 6; // mm^3
        const A_mm2 = bmm * hmm; // mm^2

        // Stresses
        const sigma_m_d = MEd_Nmm / W_mm3; // N/mm2
        const tau_v_d = (1.5 * VEd_N) / A_mm2; // N/mm2

        // Design strengths
        const fmd = (kmod * fmk) / gammaM;
        const fvd = (kmod * fvk) / gammaM;

        const eta_m = sigma_m_d / fmd;
        const eta_v = tau_v_d / fvd;

        // Deflection (instantaneous, service loads)
        const qser_Nm = qser * 1e3; // kN/m -> N/m
        const E0_Nmm2 = E0; // N/mm2
        const E0_Nm2 = E0_Nmm2 * 1e6; // 1 N/mm2 = 1e6 N/m2

        const winst_m = (5 * qser_Nm * Math.pow(L, 4)) / (384 * E0_Nm2 * I);
        const winst_mm = winst_m * 1000;

        // Approximate creep factor kdef (simple default based on class)
        let kdef = 2.0;
        const cls = document.getElementById("timber-class").value;
        if (cls.startsWith("GL")) kdef = 1.5;

        const wfin_mm = (1 + kdef) * winst_mm;

        const wlim_inst = (L * 1000) / 300; // L/300
        const wlim_fin = (L * 1000) / 250; // L/250

        const eta_w_inst = winst_mm / wlim_inst;
        const eta_w_fin = wfin_mm / wlim_fin;

        const utilMax = Math.max(eta_m, eta_v, eta_w_inst, eta_w_fin);

        const statusClass =
          utilMax <= 1
            ? "text-green-700 bg-green-50 border-green-200"
            : "text-red-700 bg-red-50 border-red-200";
        const statusText =
          utilMax <= 1
            ? "OK – All checks satisfied"
            : "Not OK – At least one check fails";

        const resultsHtml = `
      <div class="border ${statusClass} rounded p-4 mb-4">
        <div class="flex justify-between items-center">
          <div>
            <p class="font-semibold">${statusText}</p>
            <p class="text-sm mt-1">Governing utilisation: <span class="font-mono">${utilMax.toFixed(
              2
            )}</span></p>
          </div>
          <span class="badge ${
            utilMax <= 1 ? "bg-green-600 text-white" : "bg-red-600 text-white"
          }">
            ${utilMax <= 1 ? "PASS" : "FAIL"}
          </span>
        </div>
      </div>

      <div class="grid md:grid-cols-2 gap-4">
        <div class="border rounded p-4">
          <h3 class="font-semibold mb-2 text-sm">Section &amp; Actions</h3>
          <ul class="text-sm space-y-1">
            <li>Span L = <strong>${L.toFixed(2)} m</strong></li>
            <li>Section = <strong>${bmm.toFixed(0)} × ${hmm.toFixed(
          0
        )} mm</strong></li>
            <li>G<sub>k</sub> = <strong>${Gk.toFixed(
              2
            )} kN/m</strong>, Q<sub>k</sub> = <strong>${Qk.toFixed(
          2
        )} kN/m</strong></li>
            <li>q<sub>d</sub> = <strong>${qd.toFixed(2)} kN/m</strong></li>
            <li>M<sub>Ed</sub> = <strong>${MEd_kNm.toFixed(2)} kNm</strong></li>
            <li>V<sub>Ed</sub> = <strong>${VEd_kN.toFixed(2)} kN</strong></li>
          </ul>
        </div>

        <div class="border rounded p-4">
          <h3 class="font-semibold mb-2 text-sm">Material (design values)</h3>
          <ul class="text-sm space-y-1">
            <li>f<sub>m,k</sub> = ${fmk.toFixed(
              1
            )} N/mm², f<sub>v,k</sub> = ${fvk.toFixed(2)} N/mm²</li>
            <li>k<sub>mod</sub> = ${kmod.toFixed(
              2
            )}, &gamma;<sub>M</sub> = ${gammaM.toFixed(2)}</li>
            <li>f<sub>m,d</sub> = <strong>${fmd.toFixed(2)} N/mm²</strong></li>
            <li>f<sub>v,d</sub> = <strong>${fvd.toFixed(2)} N/mm²</strong></li>
            <li>E<sub>0,mean</sub> = ${E0.toFixed(0)} N/mm²</li>
            <li>&rho;<sub>k</sub> = ${rho.toFixed(0)} kg/m³</li>
          </ul>
        </div>
      </div>

      <div class="grid md:grid-cols-2 gap-4 mt-4">
        <div class="border rounded p-4">
          <h3 class="font-semibold mb-2 text-sm">ULS – Bending &amp; Shear</h3>
          <ul class="text-sm space-y-1">
            <li>Bending stress &sigma;<sub>m,d</sub> = ${sigma_m_d.toFixed(
              2
            )} N/mm²</li>
            <li>Capacity f<sub>m,d</sub> = ${fmd.toFixed(2)} N/mm²</li>
            <li>Utilisation &eta;<sub>m</sub> = <strong>${eta_m.toFixed(
              2
            )}</strong></li>
            <li class="${eta_m <= 1 ? "text-green-700" : "text-red-700"}">
              ${eta_m <= 1 ? "OK in bending" : "Bending check fails"}
            </li>
            <li class="mt-2">Shear stress &tau;<sub>v,d</sub> = ${tau_v_d.toFixed(
              2
            )} N/mm²</li>
            <li>Capacity f<sub>v,d</sub> = ${fvd.toFixed(2)} N/mm²</li>
            <li>Utilisation &eta;<sub>v</sub> = <strong>${eta_v.toFixed(
              2
            )}</strong></li>
            <li class="${eta_v <= 1 ? "text-green-700" : "text-red-700"}">
              ${eta_v <= 1 ? "OK in shear" : "Shear check fails"}
            </li>
          </ul>
        </div>

        <div class="border rounded p-4">
          <h3 class="font-semibold mb-2 text-sm">SLS – Deflection</h3>
          <ul class="text-sm space-y-1">
            <li>Instantaneous deflection w<sub>inst</sub> = <strong>${winst_mm.toFixed(
              1
            )} mm</strong></li>
            <li>Approx. final deflection w<sub>fin</sub> (k<sub>def</sub> ≈ ${kdef.toFixed(
              1
            )}) = <strong>${wfin_mm.toFixed(1)} mm</strong></li>
            <li>Limit w<sub>inst,lim</sub> (L/300) = ${wlim_inst.toFixed(
              1
            )} mm</li>
            <li>Limit w<sub>fin,lim</sub> (L/250) = ${wlim_fin.toFixed(
              1
            )} mm</li>
            <li>Utilisation &eta;<sub>w,inst</sub> = <strong>${eta_w_inst.toFixed(
              2
            )}</strong></li>
            <li>Utilisation &eta;<sub>w,fin</sub> = <strong>${eta_w_fin.toFixed(
              2
            )}</strong></li>
            <li class="${
              eta_w_inst <= 1 && eta_w_fin <= 1
                ? "text-green-700"
                : "text-red-700"
            }">
              ${
                eta_w_inst <= 1 && eta_w_fin <= 1
                  ? "OK in deflection"
                  : "Deflection limit exceeded"
              }
            </li>
          </ul>
        </div>
      </div>
    `;

        document.getElementById("results-container").innerHTML = resultsHtml;
      });

      // Initialize defaults on load
      setTimberDefaults();
    </script>

