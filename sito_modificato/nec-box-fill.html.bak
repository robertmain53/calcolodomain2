    <script>
      // NEC box fill calculator logic
      (function () {
        const awgSizeEl = document.getElementById("awgSize");
        const boxVolumeEl = document.getElementById("boxVolume");
        const loadStandardBoxBtn = document.getElementById("loadStandardBox");
        const standardBoxPanel = document.getElementById("standardBoxPanel");
        const standardBoxSelect = document.getElementById("standardBoxSelect");

        const numCurrentCarryingEl =
          document.getElementById("numCurrentCarrying");
        const numPigtailsEl = document.getElementById("numPigtails");
        const numDevicesEl = document.getElementById("numDevices");
        const numInternalClampsEl =
          document.getElementById("numInternalClamps");
        const numEquipmentGroundsEl = document.getElementById(
          "numEquipmentGrounds"
        );
        const numSupportFittingsEl =
          document.getElementById("numSupportFittings");

        const calculateBtn = document.getElementById("calculateBtn");
        const resetBtn = document.getElementById("resetBtn");

        const volumePerConductorEl =
          document.getElementById("volumePerConductor");
        const boxVolumeDisplayEl = document.getElementById("boxVolumeDisplay");
        const maxEquivalentsEl = document.getElementById("maxEquivalents");

        const eqCurrentEl = document.getElementById("eqCurrent");
        const eqDevicesEl = document.getElementById("eqDevices");
        const eqClampsEl = document.getElementById("eqClamps");
        const eqGroundsEl = document.getElementById("eqGrounds");
        const eqSupportEl = document.getElementById("eqSupport");
        const totalEquivalentsEl = document.getElementById("totalEquivalents");
        const usedVolumeEl = document.getElementById("usedVolume");

        const statusBadgeEl = document.getElementById("statusBadge");
        const interpretationTextEl =
          document.getElementById("interpretationText");

        const volumeTable = {
          14: 2.0,
          12: 2.25,
          10: 2.5,
          8: 3.0,
          6: 5.0,
        };

        function getVolumePerConductor() {
          const awg = awgSizeEl.value;
          return volumeTable[awg] || 2.0;
        }

        function updateVolumePerConductorDisplay() {
          const v = getVolumePerConductor();
          volumePerConductorEl.textContent = v.toFixed(2);
        }

        function parseIntSafe(el) {
          const v = parseInt(el.value, 10);
          return isNaN(v) || v < 0 ? 0 : v;
        }

        function parseFloatSafe(el) {
          const v = parseFloat(el.value);
          return isNaN(v) || v < 0 ? NaN : v;
        }

        function calculate() {
          const vPer = getVolumePerConductor();
          updateVolumePerConductorDisplay();

          const boxVol = parseFloatSafe(boxVolumeEl);
          if (isNaN(boxVol) || boxVol <= 0) {
            boxVolumeDisplayEl.textContent = "–";
            maxEquivalentsEl.textContent = "–";
            statusBadgeEl.textContent =
              "Enter a valid box volume to calculate.";
            statusBadgeEl.className =
              "inline-flex items-center px-3 py-1 rounded-full text-xs font-semibold bg-yellow-100 text-yellow-800";
            interpretationTextEl.textContent =
              "Please enter the internal volume of the box in cubic inches (from the manufacturer marking) and try again.";
            return;
          }

          boxVolumeDisplayEl.textContent = boxVol.toFixed(2);

          const nCurrent = parseIntSafe(numCurrentCarryingEl);
          const nDevices = parseIntSafe(numDevicesEl);
          const nClamps = parseIntSafe(numInternalClampsEl);
          const nGrounds = parseIntSafe(numEquipmentGroundsEl);
          const nSupport = parseIntSafe(numSupportFittingsEl);

          const eqCurrent = nCurrent;
          const eqDevices = nDevices * 2;
          const eqClamps = nClamps > 0 ? 1 : 0;
          const eqGrounds = nGrounds > 0 ? 1 : 0;
          const eqSupport = nSupport > 0 ? 1 : 0;

          const totalEq =
            eqCurrent + eqDevices + eqClamps + eqGrounds + eqSupport;
          const usedVol = totalEq * vPer;
          const maxEq = Math.floor(boxVol / vPer);

          eqCurrentEl.textContent = eqCurrent;
          eqDevicesEl.textContent = eqDevices;
          eqClampsEl.textContent = eqClamps;
          eqGroundsEl.textContent = eqGrounds;
          eqSupportEl.textContent = eqSupport;
          totalEquivalentsEl.textContent = totalEq;
          usedVolumeEl.textContent = usedVol.toFixed(2);
          maxEquivalentsEl.textContent = maxEq;

          if (totalEq === 0) {
            statusBadgeEl.textContent = "No conductors counted yet.";
            statusBadgeEl.className =
              "inline-flex items-center px-3 py-1 rounded-full text-xs font-semibold bg-gray-100 text-gray-700";
            interpretationTextEl.textContent =
              "Increase the number of conductors, devices, or grounds to perform a meaningful box fill check.";
            return;
          }

          if (usedVol <= boxVol) {
            statusBadgeEl.textContent =
              "Compliant – box fill within NEC limits";
            statusBadgeEl.className =
              "inline-flex items-center px-3 py-1 rounded-full text-xs font-semibold bg-green-100 text-green-800";
            const margin = boxVol - usedVol;
            interpretationTextEl.textContent =
              `The box is within NEC 314.16 fill limits. You are using approximately ${usedVol.toFixed(
                2
              )} in³ ` +
              `out of ${boxVol.toFixed(2)} in³, leaving about ${margin.toFixed(
                2
              )} in³ of free volume. ` +
              `Maximum allowed conductor equivalents at this size are ${maxEq}.`;
          } else {
            statusBadgeEl.textContent = "Overfilled – exceeds NEC box fill";
            statusBadgeEl.className =
              "inline-flex items-center px-3 py-1 rounded-full text-xs font-semibold bg-red-100 text-red-800";
            const over = usedVol - boxVol;
            interpretationTextEl.textContent =
              `The box appears overfilled per NEC 314.16. You are using approximately ${usedVol.toFixed(
                2
              )} in³ ` +
              `but the box only provides ${boxVol.toFixed(
                2
              )} in³, exceeding the limit by about ${over.toFixed(2)} in³. ` +
              `Reduce the number of conductors/devices or choose a larger box.`;
          }
        }

        function resetForm() {
          awgSizeEl.value = "14";
          boxVolumeEl.value = "";
          numCurrentCarryingEl.value = 4;
          numPigtailsEl.value = 0;
          numDevicesEl.value = 1;
          numInternalClampsEl.value = 0;
          numEquipmentGroundsEl.value = 1;
          numSupportFittingsEl.value = 0;

          standardBoxPanel.classList.add("hidden");
          standardBoxSelect.value = "";

          volumePerConductorEl.textContent = getVolumePerConductor().toFixed(2);
          boxVolumeDisplayEl.textContent = "–";
          maxEquivalentsEl.textContent = "–";
          eqCurrentEl.textContent = "0";
          eqDevicesEl.textContent = "0";
          eqClampsEl.textContent = "0";
          eqGroundsEl.textContent = "0";
          eqSupportEl.textContent = "0";
          totalEquivalentsEl.textContent = "0";
          usedVolumeEl.textContent = "0.00";

          statusBadgeEl.textContent = "Waiting for input…";
          statusBadgeEl.className =
            "inline-flex items-center px-3 py-1 rounded-full text-xs font-semibold bg-gray-100 text-gray-700";
          interpretationTextEl.textContent =
            "Enter your box volume and conductor counts, then click “Calculate box fill” to see if the box is compliant.";
        }

        awgSizeEl.addEventListener("change", updateVolumePerConductorDisplay);

        loadStandardBoxBtn.addEventListener("click", () => {
          standardBoxPanel.classList.toggle("hidden");
        });

        standardBoxSelect.addEventListener("change", () => {
          const v = parseFloat(standardBoxSelect.value);
          if (!isNaN(v)) {
            boxVolumeEl.value = v.toFixed(1);
          }
        });

        calculateBtn.addEventListener("click", calculate);
        resetBtn.addEventListener("click", resetForm);

        // Initialize
        resetForm();
      })();
    </script>

