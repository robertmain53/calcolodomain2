    <script>
      // Maintenance dose calculator logic
      function showError(msg) {
        const box = document.getElementById("error-box");
        const msgEl = document.getElementById("error-message");
        box.classList.remove("hidden");
        msgEl.textContent = msg;
      }

      function clearError() {
        document.getElementById("error-box").classList.add("hidden");
        document.getElementById("error-message").textContent = "";
      }

      function updateVisibility() {
        const dosingType = document.getElementById("dosing-type").value;
        const intervalGroup = document.getElementById("interval-group");
        if (dosingType === "continuous") {
          intervalGroup.classList.add("opacity-50");
          Array.from(intervalGroup.querySelectorAll("input,select")).forEach(
            (el) => (el.disabled = true)
          );
        } else {
          intervalGroup.classList.remove("opacity-50");
          Array.from(intervalGroup.querySelectorAll("input,select")).forEach(
            (el) => (el.disabled = false)
          );
        }
      }

      document
        .getElementById("dosing-type")
        .addEventListener("change", updateVisibility);

      document.getElementById("route").addEventListener("change", () => {
        const route = document.getElementById("route").value;
        const bio = document.getElementById("bioavailability");
        const bioUnit = document.getElementById("bioavailability-unit");
        if (route === "iv") {
          bio.value = "1";
          bioUnit.value = "fraction";
          bio.disabled = true;
          bioUnit.disabled = true;
        } else {
          if (!bio.value) bio.value = "0.7";
          bio.disabled = false;
          bioUnit.disabled = false;
        }
      });

      document
        .getElementById("calculate-btn")
        .addEventListener("click", function () {
          clearError();

          const dosingType = document.getElementById("dosing-type").value;
          const weight = parseFloat(document.getElementById("weight").value);
          const weightUnit = document.getElementById("weight-unit").value;

          const cl = parseFloat(document.getElementById("clearance").value);
          const clUnit = document.getElementById("clearance-unit").value;

          const ctarget = parseFloat(document.getElementById("ctarget").value);
          const ctargetUnit = document.getElementById("ctarget-unit").value;

          const interval = parseFloat(
            document.getElementById("interval").value
          );
          const intervalUnit = document.getElementById("interval-unit").value;

          let F = parseFloat(document.getElementById("bioavailability").value);
          const Funit = document.getElementById("bioavailability-unit").value;

          const rounding =
            parseFloat(document.getElementById("rounding").value) || 1;

          if (!isFinite(cl) || cl <= 0) {
            showError("Please enter a valid positive clearance.");
            return;
          }
          if (!isFinite(ctarget) || ctarget <= 0) {
            showError("Please enter a valid positive target concentration.");
            return;
          }
          if (dosingType === "intermittent") {
            if (!isFinite(interval) || interval <= 0) {
              showError("Please enter a valid positive dosing interval.");
              return;
            }
          }
          if (!isFinite(F) || F <= 0) {
            showError("Please enter a valid positive bioavailability.");
            return;
          }
          if (Funit === "percent") {
            F = F / 100;
          }
          if (F <= 0 || F > 1.5) {
            showError(
              "Bioavailability F should usually be between 0 and 1 (0–100%)."
            );
            return;
          }

          // Convert clearance to L/h
          let cl_L_h;
          if (clUnit === "L_h") {
            cl_L_h = cl;
          } else if (clUnit === "mL_min") {
            cl_L_h = cl * 0.06; // mL/min -> L/h
          }

          // Convert Ctarget to mg/L
          let ctarget_mg_L;
          if (ctargetUnit === "mg_L") {
            ctarget_mg_L = ctarget;
          } else if (ctargetUnit === "mcg_mL") {
            ctarget_mg_L = ctarget; // 1 µg/mL = 1 mg/L
          }

          // Convert interval to hours
          let tau_h = 1;
          if (dosingType === "intermittent") {
            if (intervalUnit === "h") {
              tau_h = interval;
            } else if (intervalUnit === "min") {
              tau_h = interval / 60;
            }
          }

          let dose_mg = 0;
          let rate_mg_h = 0;

          if (dosingType === "intermittent") {
            // Dose = (CL * Ctarget * tau) / F
            dose_mg = (cl_L_h * ctarget_mg_L * tau_h) / F;
          } else {
            // Continuous infusion: Rate = CL * Ctarget
            rate_mg_h = cl_L_h * ctarget_mg_L;
          }

          // Rounding
          let roundedDose = dose_mg;
          if (dosingType === "intermittent" && rounding > 0) {
            roundedDose = Math.round(dose_mg / rounding) * rounding;
          }

          // Weight-based expression
          let dosePerKg = null;
          if (dosingType === "intermittent" && isFinite(weight) && weight > 0) {
            let weightKg = weightUnit === "kg" ? weight : weight * 0.45359237;
            dosePerKg = roundedDose / weightKg;
          }

          const resultsDiv = document.getElementById("results");
          const mainEl = document.getElementById("result-main");
          const secondaryEl = document.getElementById("result-secondary");
          const detailsEl = document.getElementById("result-details");

          resultsDiv.classList.remove("hidden");

          if (dosingType === "intermittent") {
            mainEl.textContent = `Recommended maintenance dose ≈ ${roundedDose.toFixed(
              1
            )} mg every ${tau_h.toFixed(2)} h`;
            let secondary = `Unrounded theoretical dose: ${dose_mg.toFixed(
              1
            )} mg per dose.`;
            if (dosePerKg !== null && isFinite(dosePerKg)) {
              secondary += ` This corresponds to approximately ${dosePerKg.toFixed(
                1
              )} mg/kg per dose.`;
            }
            secondaryEl.textContent = secondary;

            detailsEl.innerHTML = `
        <p><strong>Assumptions:</strong> linear pharmacokinetics, steady state, and constant clearance.</p>
        <p><strong>Inputs (normalized):</strong></p>
        <ul class="list-disc ml-5">
          <li>Clearance CL = ${cl_L_h.toFixed(3)} L/h</li>
          <li>Target concentration C<sub>target</sub> = ${ctarget_mg_L.toFixed(
            3
          )} mg/L</li>
          <li>Dosing interval τ = ${tau_h.toFixed(3)} h</li>
          <li>Bioavailability F = ${F.toFixed(3)}</li>
        </ul>
        <p><strong>Formula used:</strong> Dose = (CL × C<sub>target</sub> × τ) / F</p>
      `;
          } else {
            mainEl.textContent = `Recommended infusion rate ≈ ${rate_mg_h.toFixed(
              1
            )} mg/h`;
            secondaryEl.textContent = `This is the maintenance rate required to maintain an average concentration of ${ctarget_mg_L.toFixed(
              3
            )} mg/L at steady state.`;

            detailsEl.innerHTML = `
        <p><strong>Inputs (normalized):</strong></p>
        <ul class="list-disc ml-5">
          <li>Clearance CL = ${cl_L_h.toFixed(3)} L/h</li>
          <li>Target concentration C<sub>target</sub> = ${ctarget_mg_L.toFixed(
            3
          )} mg/L</li>
        </ul>
        <p><strong>Formula used:</strong> Infusion rate = CL × C<sub>target</sub></p>
      `;
          }
        });

      document
        .getElementById("reset-btn")
        .addEventListener("click", function () {
          document.getElementById("route").value = "oral";
          document.getElementById("dosing-type").value = "intermittent";
          document.getElementById("weight").value = "";
          document.getElementById("weight-unit").value = "kg";
          document.getElementById("clearance").value = "";
          document.getElementById("clearance-unit").value = "L_h";
          document.getElementById("ctarget").value = "";
          document.getElementById("ctarget-unit").value = "mg_L";
          document.getElementById("interval").value = "";
          document.getElementById("interval-unit").value = "h";
          document.getElementById("bioavailability").value = "";
          document.getElementById("bioavailability-unit").value = "fraction";
          document.getElementById("rounding").value = 25;
          document.getElementById("results").classList.add("hidden");
          clearError();
          updateVisibility();
        });

      // Initialize state
      updateVisibility();
    </script>

