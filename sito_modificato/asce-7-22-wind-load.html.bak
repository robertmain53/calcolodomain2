    <script>
      // Simple exposure coefficient approximation (low- to mid-rise)
      function exposureKz(exposure, z_ft) {
        const z = Math.max(z_ft, 15); // minimum height
        let alpha, z_g;
        switch (exposure) {
          case "B":
            alpha = 7.0;
            z_g = 1200;
            break;
          case "C":
            alpha = 9.5;
            z_g = 900;
            break;
          case "D":
            alpha = 11.5;
            z_g = 700;
            break;
          default:
            alpha = 9.5;
            z_g = 900;
        }
        return 2.01 * Math.pow(z / z_g, 2 / alpha);
      }

      function toFt(value, unit) {
        return unit === "m" ? value * 3.28084 : value;
      }

      function toMph(value, unit) {
        return unit === "mps" ? value * 2.236936 : value;
      }

      document
        .getElementById("calculateBtn")
        .addEventListener("click", function () {
          const v = parseFloat(document.getElementById("v").value);
          const vUnit = document.getElementById("vUnit").value;
          const exposure = document.getElementById("exposure").value;
          const kzt = parseFloat(document.getElementById("kzt").value) || 1.0;
          const kd = parseFloat(document.getElementById("kd").value) || 0.85;
          const ke = parseFloat(document.getElementById("ke").value) || 1.0;
          const h = parseFloat(document.getElementById("h").value);
          const hUnit = document.getElementById("hUnit").value;
          const z = parseFloat(document.getElementById("z").value);
          const zUnit = document.getElementById("zUnit").value;
          const g = parseFloat(document.getElementById("g").value) || 0.85;
          const cpWall =
            parseFloat(document.getElementById("cpWall").value) || 0.8;
          const gcpi =
            parseFloat(document.getElementById("gcpi").value) || 0.18;

          if (isNaN(v) || isNaN(h) || isNaN(z)) {
            alert(
              "Please enter valid numeric values for wind speed and heights."
            );
            return;
          }

          const V_mph = toMph(v, vUnit);
          const h_ft = toFt(h, hUnit);
          const z_ft = toFt(z, zUnit);

          const Kz = exposureKz(exposure, z_ft);
          const Kh = exposureKz(exposure, h_ft);

          const qz = 0.00256 * Kz * kzt * kd * ke * V_mph * V_mph;
          const qh = 0.00256 * Kh * kzt * kd * ke * V_mph * V_mph;

          const qi = qh; // internal velocity pressure approximation

          const pWindward = qz * g * cpWall - qi * gcpi;
          const pLeeward = qz * g * -0.5 - qi * gcpi; // simple leeward Cp = -0.5

          const fmt = (x) => (isFinite(x) ? x.toFixed(1) : "–");

          document.getElementById("qzResult").textContent = fmt(qz);
          document.getElementById("qhResult").textContent = fmt(qh);
          document.getElementById("pWindward").textContent = fmt(pWindward);
          document.getElementById("pLeeward").textContent = fmt(pLeeward);

          const refInfo = `V = ${V_mph.toFixed(
            1
          )} mph, Exposure ${exposure}, Kz = ${Kz.toFixed(
            3
          )}, Kh = ${Kh.toFixed(3)}, Kzt = ${kzt}, Kd = ${kd}, Ke = ${ke}`;
          document.getElementById("refInfo").textContent = refInfo;
        });
    </script>

