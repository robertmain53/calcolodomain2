    <script>
      // dataset of bolts
      const boltData = {
        metric: [
          { label: "M6", d_mm: 6, area_mm2: 20.1 },
          { label: "M8", d_mm: 8, area_mm2: 36.6 },
          { label: "M10", d_mm: 10, area_mm2: 58 },
          { label: "M12", d_mm: 12, area_mm2: 84.3 },
          { label: "M16", d_mm: 16, area_mm2: 157 },
          { label: "M20", d_mm: 20, area_mm2: 245 },
          { label: "M24", d_mm: 24, area_mm2: 353 }
        ],
        imperial: [
          // diameter in inches, area in in^2
          { label: '1/4"-20', d_in: 0.25, area_in2: 0.0318 },
          { label: '5/16"-18', d_in: 0.3125, area_in2: 0.0524 },
          { label: '3/8"-16', d_in: 0.375, area_in2: 0.0775 },
          { label: '1/2"-13', d_in: 0.5, area_in2: 0.1419 },
          { label: '5/8"-11', d_in: 0.625, area_in2: 0.226 },
          { label: '3/4"-10', d_in: 0.75, area_in2: 0.334 }
        ]
      };

      const systemEl = document.getElementById('system');
      const boltSizeEl = document.getElementById('bolt-size');
      const gradeEl = document.getElementById('bolt-grade');
      const lubEl = document.getElementById('lubrication');
      const preloadEl = document.getElementById('preload');

      function populateBoltSizes() {
        const sys = systemEl.value;
        boltSizeEl.innerHTML = '';
        boltData[sys].forEach(b => {
          const opt = document.createElement('option');
          opt.value = b.label;
          opt.textContent = b.label;
          boltSizeEl.appendChild(opt);
        });
      }

      populateBoltSizes();
      systemEl.addEventListener('change', populateBoltSizes);

      function getProofStrength(grade, system) {
        // return proof strength in Pa (N/m2)
        // metric classes approximated
        if (grade === 'metric_88') return 580e6;
        if (grade === 'metric_109') return 830e6;
        if (grade === 'metric_129') return 970e6;
        // imperial grades: use ksi -> convert to Pa
        if (grade === 'imperial_5') return 85e3 * 6894.757; // 85 ksi
        if (grade === 'imperial_8') return 120e3 * 6894.757; // 120 ksi
        // fallback
        return system === 'metric' ? 580e6 : 85e3 * 6894.757;
      }

      function getNutFactor(lub) {
        return lub === 'lubed' ? 0.15 : 0.20;
      }

      function calculateTorque() {
        const system = systemEl.value;
        const sizeLabel = boltSizeEl.value;
        const grade = gradeEl.value;
        const lub = lubEl.value;
        const preloadPercent = parseFloat(preloadEl.value) / 100;

        const nutFactor = getNutFactor(lub);
        const proof = getProofStrength(grade, system);

        let clampLoadN = 0;
        let diameter_m = 0;

        if (system === 'metric') {
          const bolt = boltData.metric.find(b => b.label === sizeLabel);
          if (!bolt) return;
          const area_m2 = bolt.area_mm2 * 1e-6;
          const proofLoad = proof * area_m2;
          clampLoadN = proofLoad * preloadPercent;
          diameter_m = bolt.d_mm / 1000;
        } else {
          const bolt = boltData.imperial.find(b => b.label === sizeLabel);
          if (!bolt) return;
          const area_m2 = bolt.area_in2 * 0.00064516;
          const proofLoad = proof * area_m2;
          clampLoadN = proofLoad * preloadPercent;
          diameter_m = bolt.d_in * 0.0254;
        }

        const torque_Nm = nutFactor * diameter_m * clampLoadN;
        const torque_ftlb = torque_Nm / 1.3558179483314;
        const torque_inlb = torque_Nm / 0.11298482902762;
        const clamp_kN = clampLoadN / 1000;

        document.getElementById('res-nm').textContent = torque_Nm > 0 ? torque_Nm.toFixed(1) : '—';
        document.getElementById('res-ftlb').textContent = torque_ftlb > 0 ? torque_ftlb.toFixed(1) : '—';
        document.getElementById('res-inlb').textContent = torque_inlb > 0 ? torque_inlb.toFixed(0) : '—';
        document.getElementById('res-clamp').textContent = clamp_kN > 0 ? clamp_kN.toFixed(2) : '—';
      }

      document.getElementById('calculate-btn').addEventListener('click', calculateTorque);
      // initial calculation
      calculateTorque();
    </script>

