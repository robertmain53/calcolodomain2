    <script>
      // Activity presets with typical MET values
      const activityPresets = [
        { label: "Sitting quietly", met: 1.0 },
        { label: "Standing, light office work", met: 1.8 },
        { label: "Slow walking (3 km/h)", met: 2.5 },
        { label: "Brisk walking (5–6 km/h)", met: 4.0 },
        { label: "Easy cycling (<16 km/h)", met: 4.0 },
        { label: "Jogging (8 km/h)", met: 8.0 },
        { label: "Running (10–11 km/h)", met: 10.0 },
        { label: "Vigorous cycling (>20 km/h)", met: 10.0 },
        { label: "Swimming, moderate", met: 6.0 },
        { label: "HIIT / vigorous circuit", met: 9.0 },
      ];

      const activitiesBody = document.getElementById("activities-body");
      const addActivityBtn = document.getElementById("add-activity");
      const totalMetMinEl = document.getElementById("total-met-min");
      const totalCaloriesEl = document.getElementById("total-calories");
      const guidelineStatusEl = document.getElementById("guideline-status");
      const intensityBadge = document.getElementById("intensity-badge");
      const whoBadge = document.getElementById("who-badge");
      const resetAllBtn = document.getElementById("reset-all");
      const weightInput = document.getElementById("weight");
      const weightUnitSelect = document.getElementById("weight-unit");

      function createActivityRow(presetIndex = 3) {
        const row = document.createElement("tr");
        row.className = "activity-row";

        const preset = activityPresets[presetIndex] || activityPresets[3];

        row.innerHTML = `
      <td class="px-3 py-2 align-top">
        <input type="text" class="activity-name w-full border-gray-300 rounded-md px-2 py-1 text-sm mb-1" value="${
          preset.label
        }">
        <select class="activity-preset w-full border-gray-200 rounded-md px-2 py-1 text-xs text-gray-600 bg-gray-50">
          ${activityPresets
            .map(
              (a, i) => `
            <option value="${i}" ${i === presetIndex ? "selected" : ""}>${
                a.label
              } (${a.met} METs)</option>
          `
            )
            .join("")}
          <option value="custom">Custom MET value</option>
        </select>
      </td>
      <td class="px-3 py-2 align-top">
        <div class="flex items-center gap-1">
          <input type="number" min="0" step="0.1" class="activity-met w-20 border-gray-300 rounded-md px-2 py-1 text-sm" value="${
            preset.met
          }">
          <span class="text-xs text-gray-500">METs</span>
        </div>
        <p class="text-[11px] text-gray-400 mt-1">Adjust if you know a more precise MET value.</p>
      </td>
      <td class="px-3 py-2 align-top">
        <div class="flex items-center gap-1">
          <input type="number" min="0" step="1" class="activity-minutes w-24 border-gray-300 rounded-md px-2 py-1 text-sm" value="30">
          <span class="text-xs text-gray-500">min/week</span>
        </div>
      </td>
      <td class="px-3 py-2 align-top">
        <span class="activity-metmin font-mono text-sm text-gray-800">0</span>
      </td>
      <td class="px-3 py-2 align-top text-right">
        <button type="button" class="remove-activity text-xs text-red-600 hover:underline">Remove</button>
      </td>
    `;

        const presetSelect = row.querySelector(".activity-preset");
        const metInput = row.querySelector(".activity-met");

        presetSelect.addEventListener("change", () => {
          const val = presetSelect.value;
          if (val === "custom") {
            metInput.focus();
          } else {
            const p = activityPresets[parseInt(val, 10)];
            if (p) {
              metInput.value = p.met;
              const nameInput = row.querySelector(".activity-name");
              nameInput.value = p.label;
            }
          }
          recalc();
        });

        row.querySelectorAll("input").forEach((input) => {
          input.addEventListener("input", recalc);
        });

        row.querySelector(".remove-activity").addEventListener("click", () => {
          row.remove();
          recalc();
        });

        activitiesBody.appendChild(row);
      }

      function getWeightKg() {
        const w = parseFloat(weightInput.value);
        if (isNaN(w) || w <= 0) return null;
        return weightUnitSelect.value === "lb" ? w * 0.45359237 : w;
      }

      function recalc() {
        let totalMetMin = 0;
        let totalCalories = 0;
        let totalMinutes = 0;
        let weightedMetSum = 0;

        const weightKg = getWeightKg();

        activitiesBody.querySelectorAll(".activity-row").forEach((row) => {
          const met = parseFloat(row.querySelector(".activity-met").value);
          const minutes = parseFloat(
            row.querySelector(".activity-minutes").value
          );

          const metVal = isNaN(met) || met < 0 ? 0 : met;
          const minVal = isNaN(minutes) || minutes < 0 ? 0 : minutes;

          const metMin = metVal * minVal;
          row.querySelector(".activity-metmin").textContent = metMin.toFixed(0);

          totalMetMin += metMin;
          totalMinutes += minVal;
          weightedMetSum += metVal * minVal;

          if (weightKg && metVal > 0 && minVal > 0) {
            const cals = (metVal * 3.5 * weightKg * minVal) / 200;
            totalCalories += cals;
          }
        });

        totalMetMinEl.textContent = totalMetMin.toFixed(0);
        totalCaloriesEl.textContent =
          totalCalories > 0 ? totalCalories.toFixed(0) + " kcal" : "–";

        // Guideline status
        if (totalMetMin === 0) {
          guidelineStatusEl.textContent =
            "Add activities to see if you meet guidelines.";
          intensityBadge.textContent = "No activity yet";
          intensityBadge.className = "badge bg-gray-100 text-gray-700";
        } else if (totalMetMin < 500) {
          guidelineStatusEl.textContent =
            "Below the commonly recommended 500 MET-min/week. Consider adding more moderate or vigorous activity if appropriate for you.";
          intensityBadge.textContent = "Below guideline range";
          intensityBadge.className = "badge bg-yellow-100 text-yellow-800";
        } else if (totalMetMin <= 1000) {
          guidelineStatusEl.textContent =
            "Within the commonly recommended 500–1,000 MET-min/week range for general health benefits.";
          intensityBadge.textContent = "Within guideline range";
          intensityBadge.className = "badge bg-green-100 text-green-800";
        } else {
          guidelineStatusEl.textContent =
            "Above 1,000 MET-min/week. This may provide additional benefits, but ensure the volume and intensity are safe for your health status.";
          intensityBadge.textContent = "Above guideline range";
          intensityBadge.className = "badge bg-blue-100 text-blue-800";
        }

        // Average intensity badge
        if (totalMinutes > 0) {
          const avgMet = weightedMetSum / totalMinutes;
          let label = `Average intensity: ${avgMet.toFixed(1)} METs`;
          whoBadge.textContent = label;
        } else {
          whoBadge.textContent = "WHO target: 500–1,000 MET-min/week";
        }
      }

      addActivityBtn.addEventListener("click", () => {
        createActivityRow();
        recalc();
      });

      resetAllBtn.addEventListener("click", () => {
        activitiesBody.innerHTML = "";
        weightInput.value = "";
        weightUnitSelect.value = "kg";
        createActivityRow(3);
        createActivityRow(5);
        recalc();
      });

      weightInput.addEventListener("input", recalc);
      weightUnitSelect.addEventListener("change", recalc);

      // Initialize with two common activities
      createActivityRow(3); // brisk walking
      createActivityRow(5); // jogging
      recalc();

      // Mobile menu script (simple toggle)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const expanded = this.getAttribute("aria-expanded") === "true";
          this.setAttribute("aria-expanded", String(!expanded));
          menu.classList.toggle("hidden");
        });
    </script>

