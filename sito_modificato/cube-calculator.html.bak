    <script>
      // Cube calculator logic
      (function () {
        const knownSelect = document.getElementById("known-quantity");
        const inputValueEl = document.getElementById("input-value");
        const unitSelect = document.getElementById("unit-select");
        const calcBtn = document.getElementById("calculate-btn");
        const resetBtn = document.getElementById("reset-btn");
        const errorEl = document.getElementById("error-message");
        const resultsCard = document.getElementById("results-card");
        const edgeEl = document.getElementById("result-edge");
        const volEl = document.getElementById("result-volume");
        const surfEl = document.getElementById("result-surface");
        const faceDiagEl = document.getElementById("result-face-diagonal");
        const spaceDiagEl = document.getElementById("result-space-diagonal");
        const stepsEl = document.getElementById("step-by-step");

        function formatNumber(x, decimals = 6) {
          if (!isFinite(x)) return "–";
          const abs = Math.abs(x);
          if (abs !== 0 && (abs < 1e-4 || abs >= 1e6)) {
            return x.toExponential(4);
          }
          return parseFloat(x.toFixed(decimals)).toString();
        }

        function unitLabels(unit) {
          const linear = unit;
          const area = unit + "\u00B2";
          const volume = unit + "\u00B3";
          return { linear, area, volume };
        }

        function calculate() {
          errorEl.textContent = "";
          stepsEl.innerHTML = "";
          resultsCard.classList.add("hidden");

          const raw = parseFloat(inputValueEl.value);
          if (isNaN(raw)) {
            errorEl.textContent = "Please enter a numeric value.";
            return;
          }
          if (raw <= 0) {
            errorEl.textContent = "Value must be greater than zero.";
            return;
          }

          const known = knownSelect.value;
          const unit = unitSelect.value;
          const labels = unitLabels(unit);

          let a; // edge length
          let stepsHtml =
            '<h4 class="font-semibold mb-2">Step-by-step calculation</h4>';

          switch (known) {
            case "edge":
              a = raw;
              stepsHtml += `<p>You entered the edge length: <strong>a = ${formatNumber(
                a
              )} ${labels.linear}</strong>.</p>`;
              break;
            case "volume":
              a = Math.cbrt(raw);
              stepsHtml += `<p>You entered the volume: <strong>V = ${formatNumber(
                raw
              )} ${labels.volume}</strong>.</p>`;
              stepsHtml += `<p>Edge length from volume: \( a = \\sqrt[3]{V} = \\sqrt[3]{${formatNumber(
                raw
              )}} = ${formatNumber(a)} \\).</p>`;
              break;
            case "surface":
              a = Math.sqrt(raw / 6);
              stepsHtml += `<p>You entered the surface area: <strong>S = ${formatNumber(
                raw
              )} ${labels.area}</strong>.</p>`;
              stepsHtml += `<p>Edge length from surface area: \( a = \\sqrt{S/6} = \\sqrt{${formatNumber(
                raw
              )}/6} = ${formatNumber(a)} \\).</p>`;
              break;
            case "faceDiagonal":
              a = raw / Math.SQRT2;
              stepsHtml += `<p>You entered the face diagonal: <strong>d<sub>face</sub> = ${formatNumber(
                raw
              )} ${labels.linear}</strong>.</p>`;
              stepsHtml += `<p>Edge length from face diagonal: \( a = d_{face} / \\sqrt{2} = ${formatNumber(
                raw
              )} / \\sqrt{2} = ${formatNumber(a)} \\).</p>`;
              break;
            case "spaceDiagonal":
              a = raw / Math.sqrt(3);
              stepsHtml += `<p>You entered the space diagonal: <strong>d<sub>space</sub> = ${formatNumber(
                raw
              )} ${labels.linear}</strong>.</p>`;
              stepsHtml += `<p>Edge length from space diagonal: \( a = d_{space} / \\sqrt{3} = ${formatNumber(
                raw
              )} / \\sqrt{3} = ${formatNumber(a)} \\).</p>`;
              break;
            default:
              errorEl.textContent = "Unknown input type.";
              return;
          }

          if (!isFinite(a) || a <= 0) {
            errorEl.textContent =
              "The input leads to an invalid cube dimension. Please check your value.";
            return;
          }

          const V = Math.pow(a, 3);
          const S = 6 * Math.pow(a, 2);
          const dFace = a * Math.SQRT2;
          const dSpace = a * Math.sqrt(3);

          edgeEl.innerHTML = `${formatNumber(a)} ${labels.linear}`;
          volEl.innerHTML = `${formatNumber(V)} ${labels.volume}`;
          surfEl.innerHTML = `${formatNumber(S)} ${labels.area}`;
          faceDiagEl.innerHTML = `${formatNumber(dFace)} ${labels.linear}`;
          spaceDiagEl.innerHTML = `${formatNumber(dSpace)} ${labels.linear}`;

          stepsHtml += '<p class="mt-2"><strong>Other properties:</strong></p>';
          stepsHtml += `<ul class="list-disc ml-5">
        <li>Volume: \( V = a^3 = ${formatNumber(a)}^3 = ${formatNumber(V)} \,${
            labels.volume
          } \)</li>
        <li>Surface area: \( S = 6a^2 = 6 \\times ${formatNumber(
          a
        )}^2 = ${formatNumber(S)} \,${labels.area} \)</li>
        <li>Face diagonal: \( d_{face} = a\\sqrt{2} = ${formatNumber(
          a
        )}\\sqrt{2} = ${formatNumber(dFace)} \,${labels.linear} \)</li>
        <li>Space diagonal: \( d_{space} = a\\sqrt{3} = ${formatNumber(
          a
        )}\\sqrt{3} = ${formatNumber(dSpace)} \,${labels.linear} \)</li>
      </ul>`;

          stepsEl.innerHTML = stepsHtml;
          resultsCard.classList.remove("hidden");

          if (window.MathJax && window.MathJax.typesetPromise) {
            window.MathJax.typesetPromise([stepsEl]).catch(function (err) {
              console.error(err);
            });
          }
        }

        function reset() {
          inputValueEl.value = "";
          errorEl.textContent = "";
          resultsCard.classList.add("hidden");
          stepsEl.innerHTML = "";
        }

        calcBtn.addEventListener("click", calculate);
        resetBtn.addEventListener("click", reset);
        inputValueEl.addEventListener("keydown", function (e) {
          if (e.key === "Enter") {
            e.preventDefault();
            calculate();
          }
        });
      })();
    </script>

    <script>
      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
