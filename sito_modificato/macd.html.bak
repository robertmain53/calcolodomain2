2024-01-18,107.4
2024-01-19,109.0
2024-01-22,110.2
2024-01-23,111.0
2024-01-24,110.6
2024-01-25,112.3
2024-01-26,113.1
2024-01-29,112.5
2024-01-30,114.0
2024-01-31,115.2
2024-02-01,116.0
2024-02-02,115.4
2024-02-05,116.8
2024-02-06,118.1
2024-02-07,117.6
2024-02-08,119.0
2024-02-09,120.3
2024-02-12,121.0`;
          priceInput.value = sample;
        }

        function clearInput() {
          priceInput.value = "";
          statusEl.textContent = "";
          tbody.innerHTML =
            '<tr><td colspan="9" class="px-3 py-3 text-center text-gray-400">No data yet. Paste prices and click “Calculate MACD”.</td></tr>';
          sumPanel.classList.add("hidden");
          clearChart();
        }

        function parsePrices(text) {
          const lines = text
            .split(/\r?\n/)
            .map((l) => l.trim())
            .filter((l) => l.length > 0);
          const data = [];
          for (let i = 0; i < lines.length; i++) {
            const line = lines[i];
            const parts = line.split(",").map((p) => p.trim());
            if (parts.length === 1) {
              const v = parseFloat(parts[0]);
              if (!isNaN(v)) {
                data.push({ label: String(i + 1), close: v });
              }
            } else {
              const label = parts[0];
              const nums = parts.slice(1).map((x) => parseFloat(x));
              if (nums.some((n) => isNaN(n))) continue;
              let open, high, low, close;
              if (nums.length === 1) {
                close = nums[0];
              } else if (nums.length >= 4) {
                [open, high, low, close] = nums;
              } else {
                close = nums[nums.length - 1];
              }
              data.push({ label, open, high, low, close });
            }
          }
          return data;
        }

        function selectPrice(row, type) {
          const o = row.open,
            h = row.high,
            l = row.low,
            c = row.close;
          switch (type) {
            case "hl2":
              if (h != null && l != null) return (h + l) / 2;
              return c;
            case "hlc3":
              if (h != null && l != null && c != null) return (h + l + c) / 3;
              return c;
            case "ohlc4":
              if (o != null && h != null && l != null && c != null)
                return (o + h + l + c) / 4;
              return c;
            default:
              return c;
          }
        }

        function ema(values, period) {
          const result = new Array(values.length).fill(null);
          if (values.length < period) return result;
          let sum = 0;
          for (let i = 0; i < period; i++) sum += values[i];
          let prevEma = sum / period;
          result[period - 1] = prevEma;
          const alpha = 2 / (period + 1);
          for (let i = period; i < values.length; i++) {
            const e = alpha * values[i] + (1 - alpha) * prevEma;
            result[i] = e;
            prevEma = e;
          }
          return result;
        }

        function computeMACD(prices, fast, slow, signalPeriod) {
          const fastEma = ema(prices, fast);
          const slowEma = ema(prices, slow);
          const macd = prices.map((_, i) => {
            if (fastEma[i] == null || slowEma[i] == null) return null;
            return fastEma[i] - slowEma[i];
          });
          const signal = ema(
            macd.map((v) => (v == null ? 0 : v)),
            signalPeriod
          ).map((v, i) => (macd[i] == null ? null : v));
          const hist = macd.map((v, i) =>
            v == null || signal[i] == null ? null : v - signal[i]
          );
          return { fastEma, slowEma, macd, signal, hist };
        }

        function detectSignalType(prevMacd, prevSignal, macd, signal) {
          if (
            prevMacd == null ||
            prevSignal == null ||
            macd == null ||
            signal == null
          )
            return { type: "neutral", text: "" };
          const prevDiff = prevMacd - prevSignal;
          const diff = macd - signal;
          if (prevDiff <= 0 && diff > 0) {
            return {
              type: "buy",
              text: "Bullish crossover (MACD crossed above signal).",
            };
          }
          if (prevDiff >= 0 && diff < 0) {
            return {
              type: "sell",
              text: "Bearish crossover (MACD crossed below signal).",
            };
          }
          return { type: "neutral", text: "" };
        }

        function renderTable(
          rows,
          prices,
          fastEma,
          slowEma,
          macd,
          signal,
          hist
        ) {
          if (!rows.length) {
            tbody.innerHTML =
              '<tr><td colspan="9" class="px-3 py-3 text-center text-gray-400">No valid data.</td></tr>';
            return;
          }
          let html = "";
          for (let i = 0; i < rows.length; i++) {
            const price = prices[i];
            const fe = fastEma[i],
              se = slowEma[i],
              m = macd[i],
              s = signal[i],
              h = hist[i];
            const prevM = i > 0 ? macd[i - 1] : null;
            const prevS = i > 0 ? signal[i - 1] : null;
            const sig = detectSignalType(prevM, prevS, m, s);
            let badge = "";
            if (sig.type === "buy")
              badge = '<span class="badge badge-buy">Bullish</span>';
            else if (sig.type === "sell")
              badge = '<span class="badge badge-sell">Bearish</span>';
            html += `<tr class="border-t">
        <td class="px-2 py-1 text-gray-500">${i + 1}</td>
        <td class="px-2 py-1">${rows[i].label}</td>
        <td class="px-2 py-1 text-right">${
          price != null ? price.toFixed(4) : ""
        }</td>
        <td class="px-2 py-1 text-right">${fe != null ? fe.toFixed(4) : ""}</td>
        <td class="px-2 py-1 text-right">${se != null ? se.toFixed(4) : ""}</td>
        <td class="px-2 py-1 text-right">${m != null ? m.toFixed(4) : ""}</td>
        <td class="px-2 py-1 text-right">${s != null ? s.toFixed(4) : ""}</td>
        <td class="px-2 py-1 text-right">${h != null ? h.toFixed(4) : ""}</td>
        <td class="px-2 py-1">${badge}</td>
      </tr>`;
          }
          tbody.innerHTML = html;
        }

        function clearChart() {
          chartCtx.clearRect(0, 0, chartCanvas.width, chartCanvas.height);
        }

        function renderChart(labels, prices, macd, signal, hist) {
          clearChart();
          if (!labels.length) return;

          const w = chartCanvas.width;
          const h = chartCanvas.height;
          const topHeight = h * 0.45;
          const bottomHeight = h * 0.45;
          const gap = h * 0.1;

          const n = labels.length;
          const xs = labels.map(
            (_, i) => (i / Math.max(1, n - 1)) * (w - 40) + 20
          );

          // Price scale
          const validPrices = prices.filter((v) => v != null);
          const pMin = Math.min.apply(null, validPrices);
          const pMax = Math.max.apply(null, validPrices);
          const pRange = pMax - pMin || 1;
          function yPrice(v) {
            return topHeight - ((v - pMin) / pRange) * (topHeight - 20) + 10;
          }

          // MACD scale
          const macdVals = macd
            .concat(signal)
            .concat(hist)
            .filter((v) => v != null);
          const mMin = Math.min.apply(null, macdVals);
          const mMax = Math.max.apply(null, macdVals);
          const mRange = mMax - mMin || 1;
          function yMacd(v) {
            return (
              topHeight +
              gap +
              bottomHeight -
              ((v - mMin) / mRange) * (bottomHeight - 20) +
              10
            );
          }

          // Price line
          chartCtx.beginPath();
          chartCtx.strokeStyle = "#0f766e";
          chartCtx.lineWidth = 1.5;
          let started = false;
          for (let i = 0; i < n; i++) {
            const v = prices[i];
            if (v == null) continue;
            const x = xs[i];
            const y = yPrice(v);
            if (!started) {
              chartCtx.moveTo(x, y);
              started = true;
            } else {
              chartCtx.lineTo(x, y);
            }
          }
          chartCtx.stroke();

          // MACD line
          chartCtx.beginPath();
          chartCtx.strokeStyle = "#2563eb";
          chartCtx.lineWidth = 1.5;
          started = false;
          for (let i = 0; i < n; i++) {
            const v = macd[i];
            if (v == null) continue;
            const x = xs[i];
            const y = yMacd(v);
            if (!started) {
              chartCtx.moveTo(x, y);
              started = true;
            } else {
              chartCtx.lineTo(x, y);
            }
          }
          chartCtx.stroke();

          // Signal line
          chartCtx.beginPath();
          chartCtx.strokeStyle = "#f97316";
          chartCtx.lineWidth = 1.2;
          started = false;
          for (let i = 0; i < n; i++) {
            const v = signal[i];
            if (v == null) continue;
            const x = xs[i];
            const y = yMacd(v);
            if (!started) {
              chartCtx.moveTo(x, y);
              started = true;
            } else {
              chartCtx.lineTo(x, y);
            }
          }
          chartCtx.stroke();

          // Histogram bars
          for (let i = 0; i < n; i++) {
            const v = hist[i];
            if (v == null) continue;
            const x = xs[i];
            const y0 = yMacd(0);
            const y1 = yMacd(v);
            chartCtx.beginPath();
            chartCtx.strokeStyle = v >= 0 ? "#16a34a" : "#dc2626";
            chartCtx.lineWidth = 3;
            chartCtx.moveTo(x, y0);
            chartCtx.lineTo(x, y1);
            chartCtx.stroke();
          }

          // Zero line
          const yZero = yMacd(0);
          chartCtx.beginPath();
          chartCtx.strokeStyle = "#9ca3af";
          chartCtx.setLineDash([4, 4]);
          chartCtx.moveTo(10, yZero);
          chartCtx.lineTo(w - 10, yZero);
          chartCtx.stroke();
          chartCtx.setLineDash([]);
        }

        function updateSummary(rows, prices, macd, signal, hist) {
          const n = rows.length;
          if (!n) {
            sumPanel.classList.add("hidden");
            return;
          }
          let lastIdx = -1;
          for (let i = n - 1; i >= 0; i--) {
            if (
              macd[i] != null &&
              signal[i] != null &&
              hist[i] != null &&
              prices[i] != null
            ) {
              lastIdx = i;
              break;
            }
          }
          if (lastIdx === -1) {
            sumPanel.classList.add("hidden");
            return;
          }
          const m = macd[lastIdx];
          const s = signal[lastIdx];
          const h = hist[lastIdx];
          const p = prices[lastIdx];
          const prevM = lastIdx > 0 ? macd[lastIdx - 1] : null;
          const prevS = lastIdx > 0 ? signal[lastIdx - 1] : null;
          const sig = detectSignalType(prevM, prevS, m, s);

          sumPrice.textContent = p.toFixed(4);
          sumMacd.textContent = m.toFixed(4);
          sumSignal.textContent = s.toFixed(4);
          sumHist.textContent = h.toFixed(4);

          sumSignalType.classList.remove(
            "badge-buy",
            "badge-sell",
            "badge-neutral"
          );
          if (sig.type === "buy") {
            sumSignalType.textContent = "Bullish";
            sumSignalType.classList.add("badge-buy");
          } else if (sig.type === "sell") {
            sumSignalType.textContent = "Bearish";
            sumSignalType.classList.add("badge-sell");
          } else {
            sumSignalType.textContent = "Neutral";
            sumSignalType.classList.add("badge-neutral");
          }
          sumSignalText.textContent =
            sig.text || "No fresh crossover on the latest bar.";
          sumPanel.classList.remove("hidden");
        }

        function calculate() {
          statusEl.textContent = "";
          const text = priceInput.value.trim();
          if (!text) {
            statusEl.textContent = "Please paste some price data first.";
            statusEl.classList.remove("text-green-600");
            statusEl.classList.add("text-red-600");
            return;
          }
          let fast = parseInt(fastInput.value, 10);
          let slow = parseInt(slowInput.value, 10);
          let signalP = parseInt(signalInput.value, 10);
          if (!(fast > 0 && slow > 0 && signalP > 0)) {
            statusEl.textContent = "Periods must be positive integers.";
            statusEl.classList.remove("text-green-600");
            statusEl.classList.add("text-red-600");
            return;
          }
          if (fast >= slow) {
            statusEl.textContent =
              "Fast EMA period should be smaller than slow EMA period.";
            statusEl.classList.remove("text-green-600");
            statusEl.classList.add("text-red-600");
            return;
          }

          const rows = parsePrices(text);
          if (!rows.length) {
            statusEl.textContent = "No valid numeric data found.";
            statusEl.classList.remove("text-green-600");
            statusEl.classList.add("text-red-600");
            return;
          }

          const type = priceTypeSelect.value;
          const prices = rows.map((r) => {
            const v = selectPrice(r, type);
            return v != null && !isNaN(v) ? v : null;
          });

          if (prices.filter((v) => v != null).length < slow + signalP) {
            statusEl.textContent =
              "Not enough data for these periods. Add more rows or use shorter periods.";
            statusEl.classList.remove("text-green-600");
            statusEl.classList.add("text-red-600");
            return;
          }

          const { fastEma, slowEma, macd, signal, hist } = computeMACD(
            prices,
            fast,
            slow,
            signalP
          );
          renderTable(rows, prices, fastEma, slowEma, macd, signal, hist);
          renderChart(
            rows.map((r) => r.label),
            prices,
            macd,
            signal,
            hist
          );
          updateSummary(rows, prices, macd, signal, hist);

          statusEl.textContent = `Calculated MACD for ${rows.length} data points.`;
          statusEl.classList.remove("text-red-600");
          statusEl.classList.add("text-green-600");
        }

        btnSample.addEventListener("click", loadSample);
        btnClear.addEventListener("click", clearInput);
        btnCalc.addEventListener("click", calculate);
      })();
    </script>
  
