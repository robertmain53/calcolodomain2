    <!-- Lightweight chart using vanilla JS + canvas -->
    <script>
      (function () {
        const priceInput = document.getElementById("price-input");
        const btnSample = document.getElementById("btn-sample");
        const btnClear = document.getElementById("btn-clear");
        const btnCalc = document.getElementById("btn-calc");
        const statusEl = document.getElementById("calc-status");
        const tbody = document.getElementById("macd-tbody");
        const chartCanvas = document.getElementById("macd-chart");
        const fastInput = document.getElementById("fast-period");
        const slowInput = document.getElementById("slow-period");
        const signalInput = document.getElementById("signal-period");
        const priceTypeSelect = document.getElementById("price-type");

        const sumPanel = document.getElementById("summary-panel");
        const sumPrice = document.getElementById("sum-price");
        const sumMacd = document.getElementById("sum-macd");
        const sumSignal = document.getElementById("sum-signal");
        const sumHist = document.getElementById("sum-hist");
        const sumSignalType = document.getElementById("sum-signal-type");
        const sumSignalText = document.getElementById("sum-signal-text");

        let chartCtx = chartCanvas.getContext("2d");

        function loadSample() {
          const sample = `Date,Close
