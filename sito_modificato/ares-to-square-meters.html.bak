    <script>
      (function () {
        const ARES_TO_SQM = 100;

        const aresInput = document.getElementById("aresInput");
        const sqmInput = document.getElementById("sqmInput");
        const liveResult = document.getElementById("liveResult");
        const swapBtn = document.getElementById("swapBtn");
        const clearBtn = document.getElementById("clearBtn");
        const quickTableBody = document.getElementById("quickTableBody");

        let aresIsSource = true;
        let isUpdating = false;

        function formatNumber(value) {
          if (!isFinite(value)) return "";
          const abs = Math.abs(value);
          if (abs === 0) return "0";
          if (abs >= 0.01 && abs < 1e6) {
            return value.toLocaleString(undefined, {
              maximumFractionDigits: 6,
            });
          }
          return value.toExponential(6);
        }

        function updateFromAres() {
          if (isUpdating) return;
          isUpdating = true;
          const val = parseFloat(aresInput.value);
          if (isNaN(val)) {
            sqmInput.value = "";
            liveResult.textContent = "";
            isUpdating = false;
            return;
          }
          const sqm = val * ARES_TO_SQM;
          sqmInput.value = formatNumber(sqm);
          liveResult.textContent = `${formatNumber(val)} a = ${formatNumber(
            sqm
          )} m²`;
          isUpdating = false;
        }

        function updateFromSqm() {
          if (isUpdating) return;
          isUpdating = true;
          const val = parseFloat(sqmInput.value);
          if (isNaN(val)) {
            aresInput.value = "";
            liveResult.textContent = "";
            isUpdating = false;
            return;
          }
          const ares = val / ARES_TO_SQM;
          aresInput.value = formatNumber(ares);
          liveResult.textContent = `${formatNumber(val)} m² = ${formatNumber(
            ares
          )} a`;
          isUpdating = false;
        }

        aresInput.addEventListener("input", function () {
          aresIsSource = true;
          updateFromAres();
        });

        sqmInput.addEventListener("input", function () {
          aresIsSource = false;
          updateFromSqm();
        });

        swapBtn.addEventListener("click", function () {
          aresIsSource = !aresIsSource;
          if (aresIsSource) {
            updateFromAres();
          } else {
            updateFromSqm();
          }
        });

        clearBtn.addEventListener("click", function () {
          aresInput.value = "";
          sqmInput.value = "";
          liveResult.textContent = "";
          aresInput.focus();
        });

        // Populate quick table (1–20 a)
        function buildQuickTable() {
          const rows = [];
          for (let a = 1; a <= 20; a++) {
            const sqm = a * ARES_TO_SQM;
            rows.push(
              `<tr>
            <td class="px-3 py-1 border-b">${a}</td>
            <td class="px-3 py-1 border-b">${sqm.toLocaleString()}</td>
          </tr>`
            );
          }
          quickTableBody.innerHTML = rows.join("");
        }

        buildQuickTable();
      })();
    </script>

