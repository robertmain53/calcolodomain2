    <script>
      // Unit helpers
      function pcfToKNm3(pcf) {
        return pcf * 0.1570875;
      }
      function kNm3ToPcf(g) {
        return g / 0.1570875;
      }
      function psfToKPa(psf) {
        return psf * 0.0478803;
      }
      function kPaToPsf(kpa) {
        return kpa / 0.0478803;
      }
      function ftToM(ft) {
        return ft * 0.3048;
      }
      function mToFt(m) {
        return m / 0.3048;
      }
      function kNToKips(kN) {
        return kN * 0.224809;
      }
      function kipsToKN(kips) {
        return kips / 0.224809;
      }

      function bearingCapacityFactors(phiDeg) {
        const phi = (phiDeg * Math.PI) / 180;
        if (phi === 0) {
          return { Nc: 5.7, Nq: 1.0, Ngamma: 0.0 };
        }
        const Nq =
          Math.exp(Math.PI * Math.tan(phi)) *
          Math.pow(Math.tan(Math.PI / 4 + phi / 2), 2);
        const Nc = (Nq - 1) / Math.tan(phi);
        const Ngamma = 2 * (Nq + 1) * Math.tan(phi);
        return { Nc, Nq, Ngamma };
      }

      function shapeDepthFactors(method, footingType, B, L, Df) {
        if (method === "terzaghi") {
          return {
            sc: 1.0,
            sq: 1.0,
            sgamma: 1.0,
            dc: 1.0,
            dq: 1.0,
            dgamma: 1.0,
          };
        }
        const BoverL = L > 0 ? B / L : 1;
        let sc = 1,
          sq = 1,
          sgamma = 1;
        if (footingType === "strip") {
          sc = 1.0;
          sq = 1.0;
          sgamma = 1.0;
        } else if (footingType === "square") {
          sc = 1 + 0.2 * BoverL;
          sq = 1 + 0.1 * BoverL;
          sgamma = Math.max(0.6, 1 - 0.4 * BoverL);
        } else if (footingType === "circular") {
          sc = 1.3;
          sq = 1.2;
          sgamma = 0.6;
        }
        const DoverB = B > 0 ? Df / B : 0;
        const dc = 1 + 0.2 * DoverB;
        const dq = 1 + 0.1 * DoverB;
        const dgamma = 1.0;
        return { sc, sq, sgamma, dc, dq, dgamma };
      }

      function computeBearingCapacity() {
        const method = document.getElementById("method").value;
        const footingType = document.getElementById("footingType").value;

        let gamma = parseFloat(document.getElementById("gamma").value);
        const gammaUnit = document.getElementById("gammaUnit").value;
        let c = parseFloat(document.getElementById("cohesion").value);
        const cUnit = document.getElementById("cohesionUnit").value;
        const phi = parseFloat(document.getElementById("phi").value);
        let B = parseFloat(document.getElementById("width").value);
        let L = parseFloat(document.getElementById("length").value);
        let Df = parseFloat(document.getElementById("depth").value);
        const lengthUnit = document.getElementById("lengthUnit").value;
        const gwOn = document.getElementById("gwToggle").checked;
        let gwDepth = parseFloat(document.getElementById("gwDepth").value);
        const FS = parseFloat(document.getElementById("fos").value);
        let Qapplied = parseFloat(document.getElementById("appliedLoad").value);
        const loadUnit = document.getElementById("loadUnit").value;

        if (
          isNaN(gamma) ||
          isNaN(c) ||
          isNaN(phi) ||
          isNaN(B) ||
          isNaN(Df) ||
          isNaN(FS) ||
          FS <= 0
        ) {
          alert(
            "Please enter valid soil parameters, footing dimensions, and factor of safety."
          );
          return;
        }

        // Convert to SI: gamma kN/m3, c kPa, B,L,Df,gwDepth m, Q kN
        if (gammaUnit === "pcf") gamma = pcfToKNm3(gamma);
        if (cUnit === "psf") c = psfToKPa(c);
        if (lengthUnit === "ft") {
          B = ftToM(B);
          L = ftToM(L);
          Df = ftToM(Df);
          gwDepth = ftToM(gwDepth);
        }
        if (!isNaN(Qapplied)) {
          if (loadUnit === "kips") Qapplied = kipsToKN(Qapplied);
        }

        // Groundwater correction (simple)
        const gammaWater = 9.81 / 9.81; // ~1 t/m3, but we work in kN/m3; use 9.81 kN/m3
        const gammaW = 9.81;
        if (gwOn && gwDepth <= Df) {
          gamma = Math.max(0, gamma - gammaW);
        }

        const { Nc, Nq, Ngamma } = bearingCapacityFactors(phi);
        const { sc, sq, sgamma, dc, dq, dgamma } = shapeDepthFactors(
          method,
          footingType,
          B,
          L,
          Df
        );

        let qult;
        if (method === "terzaghi") {
          qult = c * Nc + gamma * Df * Nq + 0.5 * gamma * B * Ngamma;
        } else {
          qult =
            c * Nc * sc * dc +
            gamma * Df * Nq * sq * dq +
            0.5 * gamma * B * Ngamma * sgamma * dgamma;
        }

        const qall = qult / FS;

        const area =
          footingType === "circular" ? Math.PI * Math.pow(B / 2, 2) : B * L;
        const Qallow = (qall * area) / 1000; // kN (since qall in kPa = kN/m2)

        // Display results
        document.getElementById("resultsSection").classList.remove("hidden");
        document.getElementById("qultMetric").textContent =
          qult.toFixed(1) + " kPa";
        document.getElementById("qallMetric").textContent =
          qall.toFixed(1) + " kPa";
        document.getElementById("qAllowMetric").textContent =
          Qallow.toFixed(1) + " kN";

        const qultPsf = kPaToPsf(qult);
        const qallPsf = kPaToPsf(qall);
        const QallowKips = kNToKips(Qallow);

        document.getElementById("qultImperial").textContent =
          qultPsf.toFixed(0) + " psf";
        document.getElementById("qallImperial").textContent =
          qallPsf.toFixed(0) + " psf";
        document.getElementById("qAllowImperial").textContent =
          QallowKips.toFixed(2) + " kips";

        document.getElementById("factorsDisplay").innerHTML =
          "N<sub>c</sub> = " +
          Nc.toFixed(2) +
          ", N<sub>q</sub> = " +
          Nq.toFixed(2) +
          ", N<sub>\u03b3</sub> = " +
          Ngamma.toFixed(2);

        document.getElementById("factorsMethodNote").textContent =
          method === "terzaghi"
            ? "Terzaghi equation for strip footing; shape and depth factors = 1.0."
            : "Meyerhof equation with approximate shape and depth factors for " +
              footingType +
              " footing.";

        const utilCard = document.getElementById("utilizationCard");
        const utilText = document.getElementById("utilizationText");
        if (!isNaN(Qapplied) && Qapplied > 0) {
          const util = Qapplied / Qallow;
          const percent = (util * 100).toFixed(1);
          let msg =
            "Applied load Q = " +
            Qapplied.toFixed(1) +
            " " +
            (loadUnit === "kips" ? "kips" : "kN") +
            " vs allowable Q_allow = " +
            (loadUnit === "kips"
              ? QallowKips.toFixed(2) + " kips"
              : Qallow.toFixed(1) + " kN") +
            " (utilization " +
            percent +
            "%). ";
          if (util <= 1) {
            msg +=
              "The footing is within the allowable bearing capacity (by shear). Settlement may still govern.";
          } else {
            msg +=
              "The applied load exceeds the allowable bearing capacity. Increase footing size or improve soil.";
          }
          utilText.textContent = msg;
          utilCard.classList.remove("hidden");
        } else {
          utilCard.classList.add("hidden");
        }
      }

      document
        .getElementById("calcBtn")
        .addEventListener("click", computeBearingCapacity);
      document
        .getElementById("resetBtn")
        .addEventListener("click", function () {
