    <script>
      function formatNumber(value) {
        if (!isFinite(value)) return "";
        const abs = Math.abs(value);
        if (abs === 0) return "0";
        if (abs >= 1e4 || abs < 1e-3) {
          return value.toExponential(2);
        }
        return value.toLocaleString(undefined, { maximumFractionDigits: 4 });
      }

      function updatePerStepDisplay() {
        const transfer = parseFloat(
          document.getElementById("transfer-vol").value
        );
        const finalV = parseFloat(document.getElementById("final-vol").value);
        const display = document.getElementById("per-step-display");
        if (transfer > 0 && finalV > 0 && finalV >= transfer) {
          const D = finalV / transfer;
          display.textContent = `1:${formatNumber(D)}`;
        } else {
          display.textContent = "—";
        }
      }

      document
        .getElementById("transfer-vol")
        .addEventListener("input", updatePerStepDisplay);
      document
        .getElementById("final-vol")
        .addEventListener("input", updatePerStepDisplay);

      document.querySelectorAll(".mode-radio").forEach((r) => {
        r.addEventListener("change", () => {
          const mode = document.querySelector(".mode-radio:checked").value;
          document
            .getElementById("mode-volumes")
            .classList.toggle("hidden", mode !== "volumes");
          document
            .getElementById("mode-factor")
            .classList.toggle("hidden", mode !== "factor");
        });
      });

      document.getElementById("reset-btn").addEventListener("click", () => {
        document.getElementById("stock-conc").value = "";
        document.getElementById("num-steps").value = 6;
        document.getElementById("transfer-vol").value = 1;
        document.getElementById("final-vol").value = 10;
        document.getElementById("step-factor").value = 10;
        document.getElementById("transfer-vol-factor").value = "";
        document.getElementById("final-vol-factor").value = "";
        document.getElementById("error-msg").textContent = "";
        document.getElementById("results-container").classList.add("hidden");
        document.getElementById("summary").classList.add("hidden");
        document.getElementById("results-body").innerHTML = "";
        updatePerStepDisplay();
      });

      document.getElementById("calc-btn").addEventListener("click", () => {
        const errorEl = document.getElementById("error-msg");
        errorEl.textContent = "";
        const mode = document.querySelector(".mode-radio:checked").value;
        const stockConc = parseFloat(
          document.getElementById("stock-conc").value
        );
        const numSteps = parseInt(
          document.getElementById("num-steps").value,
          10
        );
        const concUnit = document.getElementById("conc-unit").value;
        const volUnit = document.getElementById("vol-unit").value;

        if (!isFinite(stockConc) || stockConc <= 0) {
          errorEl.textContent = "Enter a valid positive stock concentration.";
          return;
        }
        if (!Number.isInteger(numSteps) || numSteps < 1 || numSteps > 20) {
          errorEl.textContent = "Number of steps must be between 1 and 20.";
          return;
        }

        let D, transfer, finalV;
        if (mode === "volumes") {
          transfer = parseFloat(document.getElementById("transfer-vol").value);
          finalV = parseFloat(document.getElementById("final-vol").value);
          if (!isFinite(transfer) || transfer <= 0) {
            errorEl.textContent = "Enter a valid positive transfer volume.";
            return;
          }
          if (!isFinite(finalV) || finalV <= 0 || finalV < transfer) {
            errorEl.textContent =
              "Final volume must be ≥ transfer volume and > 0.";
            return;
          }
          D = finalV / transfer;
        } else {
          D = parseFloat(document.getElementById("step-factor").value);
          transfer = parseFloat(
            document.getElementById("transfer-vol-factor").value
          );
          finalV = parseFloat(
            document.getElementById("final-vol-factor").value
          );
          if (!isFinite(D) || D <= 1) {
            errorEl.textContent = "Per-step dilution factor must be > 1.";
            return;
          }
          if (isFinite(transfer) && isFinite(finalV) && finalV < transfer) {
            errorEl.textContent = "Final volume must be ≥ transfer volume.";
            return;
          }
        }

        const tbody = document.getElementById("results-body");
        tbody.innerHTML = "";
        const rows = [];
        for (let i = 1; i <= numSteps; i++) {
          const step = i;
          const tubeLabel = `Tube ${i}`;
          const overallFactor = Math.pow(D, i);
          const conc = stockConc / overallFactor;

          let stepTransfer = "";
          let diluent = "";
          let totalVol = "";
          if (
            isFinite(transfer) &&
            transfer > 0 &&
            isFinite(finalV) &&
            finalV > 0
          ) {
            stepTransfer = `${formatNumber(transfer)} ${volUnit}`;
            totalVol = `${formatNumber(finalV)} ${volUnit}`;
            const dil = finalV - transfer;
            diluent = dil >= 0 ? `${formatNumber(dil)} ${volUnit}` : "";
          }

          const stepDilutionStr = `1:${formatNumber(D)}`;
          const overallDilutionStr = `1:${formatNumber(overallFactor)}`;
          const concStr = `${formatNumber(conc)} ${concUnit}`;

          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="px-2 py-1 border-t">${step}</td>
        <td class="px-2 py-1 border-t">${tubeLabel}</td>
        <td class="px-2 py-1 border-t">${stepTransfer}</td>
        <td class="px-2 py-1 border-t">${diluent}</td>
        <td class="px-2 py-1 border-t">${totalVol}</td>
        <td class="px-2 py-1 border-t">${stepDilutionStr}</td>
        <td class="px-2 py-1 border-t">${overallDilutionStr}</td>
        <td class="px-2 py-1 border-t">${concStr}</td>
      `;
          tbody.appendChild(tr);

          rows.push({
            step,
            tube: tubeLabel,
            transfer: stepTransfer,
            diluent,
            totalVol,
            stepDilution: stepDilutionStr,
            overallDilution: overallDilutionStr,
            concentration: concStr,
          });
        }

        const summary = document.getElementById("summary");
        const lastOverall = Math.pow(D, numSteps);
        summary.innerHTML = `
      <strong>Summary:</strong>
      ${numSteps} steps with per‑step dilution 1:${formatNumber(
          D
        )} produce a final dilution of
      1:${formatNumber(
        lastOverall
      )} relative to stock. Final concentration in tube ${numSteps}:
      ${formatNumber(stockConc / lastOverall)} ${concUnit}.
    `;
        summary.classList.remove("hidden");
        document.getElementById("results-container").classList.remove("hidden");

        // store rows for CSV export
        window.serialDilutionRows = rows;
      });

      document.getElementById("export-btn").addEventListener("click", () => {
        const rows = window.serialDilutionRows || [];
        if (!rows.length) {
          alert("Run the calculation first to export results.");
          return;
        }
        const header = [
          "Step",
          "Tube",
          "Transfer",
          "Diluent",
          "Total Volume",
          "Step Dilution",
          "Overall Dilution",
          "Concentration",
        ];
        const lines = [header.join(",")];
        rows.forEach((r) => {
          lines.push(
            [
              r.step,
              r.tube,
              r.transfer,
              r.diluent,
              r.totalVol,
              r.stepDilution,
              r.overallDilution,
              r.concentration,
            ]
              .map((v) => `"${String(v).replace(/"/g, '""')}"`)
              .join(",")
          );
        });
        const blob = new Blob([lines.join("\n")], {
          type: "text/csv;charset=utf-8;",
        });
        const url = URL.createObjectURL(blob);
        const a = document.createElement("a");
        a.href = url;
        a.download = "serial-dilution-plan.csv";
        document.body.appendChild(a);
        a.click();
        document.body.removeChild(a);
        URL.revokeObjectURL(url);
      });

      // initialize
      updatePerStepDisplay();
    </script>

