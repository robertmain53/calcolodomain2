    <script>
      // Default letter grade scales
      const defaultLetterScale = {
        "A+": 4.0,
        A: 4.0,
        "A-": 3.7,
        "B+": 3.3,
        B: 3.0,
        "B-": 2.7,
        "C+": 2.3,
        C: 2.0,
        "C-": 1.7,
        "D+": 1.3,
        D: 1.0,
        "D-": 0.7,
        F: 0.0,
      };

      let customScale = { ...defaultLetterScale };
      let mode = "highschool"; // 'highschool' or 'college';

      const coursesBody = document.getElementById("courses-body");
      const semesterGpaEl = document.getElementById("semester-gpa");
      const semesterSummaryEl = document.getElementById("semester-summary");
      const cumulativeGpaEl = document.getElementById("cumulative-gpa");
      const cumulativeSummaryEl = document.getElementById("cumulative-summary");
      const gpaLetterEl = document.getElementById("gpa-letter");
      const gpaStandingEl = document.getElementById("gpa-standing");

      function createCourseRow(index) {
        const tr = document.createElement("tr");
        tr.className = index % 2 === 0 ? "bg-white" : "bg-gray-50";
        tr.innerHTML = `
      <td class="px-3 py-2">
        <input type="text" class="w-full border rounded-md px-2 py-1 text-xs" placeholder="Course name">
      </td>
      <td class="px-3 py-2">
        <select class="grade-select w-full border rounded-md px-2 py-1 text-xs mb-1"></select>
        <input type="number" step="0.1" min="0" max="100" class="grade-percent w-full border rounded-md px-2 py-1 text-xs hidden" placeholder="%">
      </td>
      <td class="px-3 py-2">
        <input type="number" step="0.5" min="0" class="credits input-small w-20 border rounded-md px-2 py-1 text-xs" value="3">
      </td>
      <td class="px-3 py-2">
        <select class="level-select w-full border rounded-md px-2 py-1 text-xs">
          <option value="1">Regular (×1.0)</option>
          <option value="1.05">Honors (×1.05)</option>
          <option value="1.1">AP/IB (×1.10)</option>
          <option value="custom">Custom…</option>
        </select>
        <input type="number" step="0.01" min="0" class="level-custom input-small w-full border rounded-md px-2 py-1 text-xs mt-1 hidden" placeholder="Custom weight (e.g. 1.2)">
      </td>
      <td class="px-3 py-2 text-center">
        <input type="checkbox" class="passfail-toggle">
      </td>
      <td class="px-3 py-2">
        <span class="grade-points text-xs text-gray-700">0.00</span>
      </td>
      <td class="px-3 py-2 text-right">
        <button type="button" class="remove-course text-xs text-red-600 hover:underline">Remove</button>
      </td>
    `;
        initGradeSelect(tr.querySelector(".grade-select"));
        return tr;
      }

      function initGradeSelect(selectEl) {
        const scaleSelect = document.getElementById("scale-select");
        const scale = scaleSelect.value;
        selectEl.innerHTML = "";
        if (scale === "100") {
          // Percentage mode: hide select, show numeric
          selectEl.classList.add("hidden");
          const percentInput =
            selectEl.parentElement.querySelector(".grade-percent");
          percentInput.classList.remove("hidden");
        } else {
          const percentInput =
            selectEl.parentElement.querySelector(".grade-percent");
          percentInput.classList.add("hidden");
          selectEl.classList.remove("hidden");
          const letters = Object.keys(customScale);
          letters.forEach((letter) => {
            const opt = document.createElement("option");
            opt.value = letter;
            opt.textContent = letter;
            selectEl.appendChild(opt);
          });
        }
      }

      function refreshAllGradeSelects() {
        document.querySelectorAll(".grade-select").forEach(initGradeSelect);
      }

      function addCourse() {
        const index = coursesBody.children.length;
        const row = createCourseRow(index);
        coursesBody.appendChild(row);
        attachRowEvents(row);
        calculateGpa();
      }

      function attachRowEvents(row) {
        row.querySelectorAll("input, select").forEach((el) => {
          el.addEventListener("input", calculateGpa);
          el.addEventListener("change", calculateGpa);
        });
        row.querySelector(".remove-course").addEventListener("click", () => {
          row.remove();
          Array.from(coursesBody.children).forEach((tr, idx) => {
            tr.className = idx % 2 === 0 ? "bg-white" : "bg-gray-50";
          });
          calculateGpa();
        });
        const levelSelect = row.querySelector(".level-select");
        const levelCustom = row.querySelector(".level-custom");
        levelSelect.addEventListener("change", () => {
          if (levelSelect.value === "custom") {
            levelCustom.classList.remove("hidden");
          } else {
            levelCustom.classList.add("hidden");
          }
          calculateGpa();
        });
      }

      function getGradePointsFromRow(row) {
        const scaleSelect = document.getElementById("scale-select");
        const scale = scaleSelect.value;
        const passFail = row.querySelector(".passfail-toggle").checked;
        const credits = parseFloat(row.querySelector(".credits").value) || 0;
        if (credits <= 0) return { qualityPoints: 0, credits: 0 };

        let basePoints = 0;
        if (scale === "100") {
          const pct = parseFloat(row.querySelector(".grade-percent").value);
          if (isNaN(pct)) return { qualityPoints: 0, credits: 0 };
          basePoints = (pct / 100) * 4.0;
        } else {
          const letter = row.querySelector(".grade-select").value;
          basePoints = customScale[letter] ?? 0;
        }

        if (passFail) {
          // Exclude from GPA; still could count as completed credits, but we ignore for GPA math here
          row.querySelector(".grade-points").textContent = "P/F";
          return { qualityPoints: 0, credits: 0 };
        }

        const weightedToggle = document.getElementById("weighted-toggle").value;
        let weightFactor = 1;
        if (weightedToggle === "weighted") {
          const levelVal = row.querySelector(".level-select").value;
          if (levelVal === "custom") {
            const customWeight = parseFloat(
              row.querySelector(".level-custom").value
            );
            weightFactor =
              !isNaN(customWeight) && customWeight > 0 ? customWeight : 1;
          } else {
            weightFactor = parseFloat(levelVal) || 1;
          }
        }
        const effectivePoints = basePoints * weightFactor;
        const qualityPoints = effectivePoints * credits;
        row.querySelector(".grade-points").textContent =
          effectivePoints.toFixed(2);
        return { qualityPoints, credits };
      }

      function calculateGpa() {
        let totalQuality = 0;
        let totalCredits = 0;
        document.querySelectorAll("#courses-body tr").forEach((row) => {
          const { qualityPoints, credits } = getGradePointsFromRow(row);
          totalQuality += qualityPoints;
          totalCredits += credits;
        });

        const semesterGpa = totalCredits > 0 ? totalQuality / totalCredits : 0;
        semesterGpaEl.textContent = semesterGpa.toFixed(2);
        semesterSummaryEl.textContent = `${totalQuality.toFixed(
          2
        )} quality points / ${totalCredits.toFixed(2)} credits`;

        const priorGpa = parseFloat(document.getElementById("prior-gpa").value);
        const priorCredits = parseFloat(
          document.getElementById("prior-credits").value
        );
        if (!isNaN(priorGpa) && !isNaN(priorCredits) && priorCredits > 0) {
          const priorQuality = priorGpa * priorCredits;
          const cumQuality = priorQuality + totalQuality;
          const cumCredits = priorCredits + totalCredits;
          const cumGpa = cumCredits > 0 ? cumQuality / cumCredits : 0;
          cumulativeGpaEl.textContent = cumGpa.toFixed(2);
          cumulativeSummaryEl.textContent = `${cumQuality.toFixed(
            2
          )} quality points / ${cumCredits.toFixed(2)} credits`;
          updateStanding(cumGpa);
        } else {
          cumulativeGpaEl.textContent = semesterGpa.toFixed(2);
          cumulativeSummaryEl.textContent =
            "Cumulative GPA equals current term (no prior GPA entered).";
          updateStanding(semesterGpa);
        }
      }

      function updateStanding(gpa) {
        let letter = "–";
        if (gpa >= 3.85) letter = "A";
        else if (gpa >= 3.5) letter = "A− / B+";
        else if (gpa >= 3.0) letter = "B range";
        else if (gpa >= 2.0) letter = "C range";
        else if (gpa > 0) letter = "Below 2.0";

        gpaLetterEl.textContent = letter;

        let standing = "";
        if (gpa === 0) {
          standing = "Add courses to estimate your academic standing.";
        } else if (gpa >= 3.5) {
          standing =
            "This is typically in the honors or Dean’s List range at many institutions.";
        } else if (gpa >= 3.0) {
          standing = "This is a solid GPA and competitive for many programs.";
        } else if (gpa >= 2.0) {
          standing =
            "You are in good standing at most schools, but some competitive programs may require higher GPAs.";
        } else {
          standing =
            "Below 2.0 is often considered at-risk or probation territory. Check your school’s policies.";
        }
        gpaStandingEl.textContent = standing;
      }

      function initCustomScaleUI() {
        const container = document.getElementById("custom-scale-container");
        container.innerHTML = "";
        Object.keys(defaultLetterScale).forEach((letter) => {
          const div = document.createElement("div");
          div.className = "flex items-center gap-2";
          div.innerHTML = `
        <span class="w-8 text-xs font-medium">${letter}</span>
        <input type="number" step="0.1" min="0" max="10" data-letter="${letter}"
          class="input-small flex-1 border rounded-md px-2 py-1 text-xs"
          value="${customScale[letter].toFixed(1)}">
      `;
          container.appendChild(div);
        });
        container.querySelectorAll("input").forEach((input) => {
          input.addEventListener("input", () => {
            const letter = input.getAttribute("data-letter");
            const val = parseFloat(input.value);
            if (!isNaN(val)) {
              customScale[letter] = val;
              refreshAllGradeSelects();
              calculateGpa();
            }
          });
        });
      }

      function initScaleSelect() {
        const scaleSelect = document.getElementById("scale-select");
        scaleSelect.addEventListener("change", () => {
          const val = scaleSelect.value;
          if (val === "4") {
            customScale = { ...defaultLetterScale };
          } else if (val === "5") {
            // Scale up proportionally to 5.0
            customScale = {};
            Object.keys(defaultLetterScale).forEach((letter) => {
              customScale[letter] = defaultLetterScale[letter] * (5 / 4);
            });
          }
          initCustomScaleUI();
          refreshAllGradeSelects();
          calculateGpa();
        });
      }

      function initModeTabs() {
        const tabHS = document.getElementById("tab-highschool");
        const tabCollege = document.getElementById("tab-college");
        tabHS.addEventListener("click", () => {
          mode = "highschool";
          tabHS.classList.add("tab-active");
          tabCollege.classList.remove("tab-active");
          document.getElementById("weighted-toggle").value = "weighted";
          calculateGpa();
        });
        tabCollege.addEventListener("click", () => {
          mode = "college";
          tabCollege.classList.add("tab-active");
          tabHS.classList.remove("tab-active");
          document.getElementById("weighted-toggle").value = "unweighted";
          calculateGpa();
        });
      }

      function initWhatIf() {
        document.getElementById("calc-whatif").addEventListener("click", () => {
          const target = parseFloat(
            document.getElementById("target-gpa").value
          );
          const futureCredits = parseFloat(
            document.getElementById("future-credits").value
          );
          const priorGpa = parseFloat(
            document.getElementById("prior-gpa").value
          );
          const priorCredits = parseFloat(
            document.getElementById("prior-credits").value
          );
          const resultEl = document.getElementById("whatif-result");

          if (
            isNaN(target) ||
            isNaN(futureCredits) ||
            futureCredits <= 0 ||
            isNaN(priorGpa) ||
            isNaN(priorCredits) ||
            priorCredits <= 0
          ) {
            resultEl.textContent =
              "Enter your current cumulative GPA, current total credits, target GPA, and planned future credits.";
            return;
          }

          const currentQuality = priorGpa * priorCredits;
          const totalCredits = priorCredits + futureCredits;
          const requiredTotalQuality = target * totalCredits;
          const neededFutureQuality = requiredTotalQuality - currentQuality;
          const requiredFutureGpa = neededFutureQuality / futureCredits;

          if (requiredFutureGpa > 4.0) {
            resultEl.textContent = `You would need an average GPA of ${requiredFutureGpa.toFixed(
              2
            )}, which is above a typical 4.0 scale. This target may not be realistic without more credits or grade replacement.`;
          } else if (requiredFutureGpa < 0) {
            resultEl.textContent =
              "Your current GPA is already above the target; even with 0.0 in all future courses, you would remain above the target.";
          } else {
            resultEl.textContent = `You need an average GPA of approximately ${requiredFutureGpa.toFixed(
              2
            )} over the next ${futureCredits.toFixed(
              1
            )} credits to reach a cumulative GPA of ${target.toFixed(2)}.`;
          }
        });
      }

      document.addEventListener("DOMContentLoaded", () => {
        initScaleSelect();
        initCustomScaleUI();
        initModeTabs();
        initWhatIf();
        document
          .getElementById("add-course")
          .addEventListener("click", addCourse);
        document
          .getElementById("clear-courses")
          .addEventListener("click", () => {
            coursesBody.innerHTML = "";
            calculateGpa();
          });
        document
          .getElementById("weighted-toggle")
          .addEventListener("change", calculateGpa);
        document
          .getElementById("prior-gpa")
          .addEventListener("input", calculateGpa);
        document
          .getElementById("prior-credits")
          .addEventListener("input", calculateGpa);

        // Start with a few blank rows
        for (let i = 0; i < 4; i++) addCourse();
      });

      // Mobile menu script (simple)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", () => {
          const menu = document.getElementById("mobile-menu");
          const expanded = menu.classList.contains("hidden");
          menu.classList.toggle("hidden");
          document
            .getElementById("mobile-menu-toggle")
            .setAttribute("aria-expanded", expanded ? "true" : "false");
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
