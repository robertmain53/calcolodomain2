    <script>
      // Paint app logic
      (function () {
        const canvas = document.getElementById("paint-canvas");
        const ctx = canvas.getContext("2d");

        // Layers: 3 offscreen canvases
        const layerCount = 3;
        const layers = [];
        let activeLayer = 0;

        function createLayerCanvas(w, h) {
          const c = document.createElement("canvas");
          c.width = w;
          c.height = h;
          return c;
        }

        function initLayers(width, height) {
          layers.length = 0;
          for (let i = 0; i < layerCount; i++) {
            layers.push(createLayerCanvas(width, height));
          }
          redrawComposite();
          saveState();
        }

        // Tools & state
        let tool = "brush";
        let drawing = false;
        let startX = 0,
          startY = 0;
        let brushSize = 6;
        let color = "#000000";
        let isShiftDown = false;

        // Undo/redo stacks (store image data of all layers)
        const undoStack = [];
        const redoStack = [];
        const maxHistory = 30;

        function getStateSnapshot() {
          return layers.map((layer) => layer.toDataURL());
        }

        function loadStateSnapshot(snapshot) {
          snapshot.forEach((dataURL, index) => {
            const img = new Image();
            img.onload = () => {
              const lctx = layers[index].getContext("2d");
              lctx.clearRect(0, 0, layers[index].width, layers[index].height);
              lctx.drawImage(img, 0, 0);
              redrawComposite();
            };
            img.src = dataURL;
          });
        }

        function saveState() {
          const snapshot = getStateSnapshot();
          undoStack.push(snapshot);
          if (undoStack.length > maxHistory) undoStack.shift();
          redoStack.length = 0;
        }

        function undo() {
          if (undoStack.length <= 1) return;
          const current = undoStack.pop();
          redoStack.push(current);
          const prev = undoStack[undoStack.length - 1];
          loadStateSnapshot(prev);
        }

        function redo() {
          if (!redoStack.length) return;
          const snapshot = redoStack.pop();
          undoStack.push(snapshot);
          loadStateSnapshot(snapshot);
        }

        function redrawComposite() {
          ctx.clearRect(0, 0, canvas.width, canvas.height);
          layers.forEach((layer) => {
            ctx.drawImage(layer, 0, 0);
          });
        }

        function getActiveLayerCtx() {
          return layers[activeLayer].getContext("2d");
        }

        function getCanvasPos(evt) {
          const rect = canvas.getBoundingClientRect();
          const clientX = evt.touches ? evt.touches[0].clientX : evt.clientX;
          const clientY = evt.touches ? evt.touches[0].clientY : evt.clientY;
          return {
            x: (clientX - rect.left) * (canvas.width / rect.width),
            y: (clientY - rect.top) * (canvas.height / rect.height),
          };
        }

        function startDraw(evt) {
          evt.preventDefault();
          const pos = getCanvasPos(evt);
          drawing = true;
          startX = pos.x;
          startY = pos.y;

          if (tool === "brush" || tool === "eraser") {
            const lctx = getActiveLayerCtx();
            lctx.lineCap = "round";
            lctx.lineJoin = "round";
            lctx.lineWidth = brushSize;
            lctx.strokeStyle = tool === "eraser" ? "#ffffff" : color;
            lctx.beginPath();
            lctx.moveTo(startX, startY);
          }
        }

        function draw(evt) {
          if (!drawing) return;
          evt.preventDefault();
          const pos = getCanvasPos(evt);
          const lctx = getActiveLayerCtx();

          if (tool === "brush" || tool === "eraser") {
            lctx.lineTo(pos.x, pos.y);
            lctx.stroke();
          } else {
            // For shape tools, redraw from snapshot
            const snapshot = undoStack[undoStack.length - 1];
            if (!snapshot) return;
            // Temporarily restore last state for this layer only
            const img = new Image();
            img.onload = () => {
              lctx.clearRect(
                0,
                0,
                layers[activeLayer].width,
                layers[activeLayer].height
              );
              lctx.drawImage(img, 0, 0);
              lctx.lineWidth = brushSize;
              lctx.strokeStyle = color;
              lctx.fillStyle = color;
              if (tool === "line") {
                let x2 = pos.x;
                let y2 = pos.y;
                if (isShiftDown) {
                  const dx = x2 - startX;
                  const dy = y2 - startY;
                  if (Math.abs(dx) > Math.abs(dy)) {
                    y2 = startY;
                  } else {
                    x2 = startX;
                  }
                }
                lctx.beginPath();
                lctx.moveTo(startX, startY);
                lctx.lineTo(x2, y2);
                lctx.stroke();
              } else if (tool === "rect") {
                const w = pos.x - startX;
                const h = pos.y - startY;
                lctx.beginPath();
                lctx.rect(startX, startY, w, h);
                lctx.stroke();
              } else if (tool === "circle") {
                const rx = (pos.x - startX) / 2;
                const ry = (pos.y - startY) / 2;
                const cx = startX + rx;
                const cy = startY + ry;
                const radius = Math.sqrt(rx * rx + ry * ry);
                lctx.beginPath();
                lctx.arc(cx, cy, radius, 0, Math.PI * 2);
                lctx.stroke();
              }
              redrawComposite();
            };
            img.src = snapshot[activeLayer];
            return;
          }
          redrawComposite();
        }

        function endDraw(evt) {
          if (!drawing) return;
          drawing = false;
          saveState();
        }

        // Text tool
        function handleTextClick(evt) {
          if (tool !== "text") return;
          const pos = getCanvasPos(evt);
          const text = prompt("Enter text:");
          if (!text) return;
          const lctx = getActiveLayerCtx();
          lctx.fillStyle = color;
          lctx.font = `${Math.max(
            12,
            brushSize * 3
          )}px Inter, system-ui, sans-serif`;
          lctx.textBaseline = "top";
          lctx.fillText(text, pos.x, pos.y);
          redrawComposite();
          saveState();
        }

        // Event listeners
        canvas.addEventListener("mousedown", (e) => {
          if (tool === "text") {
            handleTextClick(e);
          } else {
            startDraw(e);
          }
        });
        canvas.addEventListener("mousemove", draw);
        window.addEventListener("mouseup", endDraw);

        canvas.addEventListener(
          "touchstart",
          (e) => {
            if (tool === "text") {
              handleTextClick(e);
            } else {
              startDraw(e);
            }
          },
          { passive: false }
        );
        canvas.addEventListener("touchmove", draw, { passive: false });
        window.addEventListener("touchend", endDraw);

        window.addEventListener("keydown", (e) => {
          if (e.key === "Shift") isShiftDown = true;
        });
        window.addEventListener("keyup", (e) => {
          if (e.key === "Shift") isShiftDown = false;
        });

        // Tool buttons
        const toolButtons = document.querySelectorAll(".tool-button");
        function setTool(newTool) {
          tool = newTool;
          toolButtons.forEach((btn) => {
            btn.classList.toggle("active", btn.dataset.tool === newTool);
          });
        }
        toolButtons.forEach((btn) => {
          btn.addEventListener("click", () => setTool(btn.dataset.tool));
        });
        setTool("brush");

        // Brush size
        const brushSizeInput = document.getElementById("brush-size");
        const brushSizeLabel = document.getElementById("brush-size-label");
        brushSizeInput.addEventListener("input", () => {
          brushSize = parseInt(brushSizeInput.value, 10) || 1;
          brushSizeLabel.textContent = brushSize + " px";
        });

        // Color picker & swatches
        const colorPicker = document.getElementById("color-picker");
        const swatches = document.querySelectorAll(".color-swatch");

        function setColor(newColor) {
          color = newColor;
          colorPicker.value = newColor;
          swatches.forEach((s) =>
            s.classList.toggle(
              "selected",
              s.dataset.color.toLowerCase() === newColor.toLowerCase()
            )
          );
        }

        colorPicker.addEventListener("input", () => {
          setColor(colorPicker.value);
        });

        swatches.forEach((swatch) => {
          swatch.addEventListener("click", () => {
            setColor(swatch.dataset.color);
          });
        });

        // Layer selection
        const layerRadios = document.querySelectorAll(".layer-radio");
        layerRadios.forEach((radio) => {
          radio.addEventListener("change", () => {
            activeLayer = parseInt(radio.value, 10) || 0;
          });
        });

        // Undo/redo/clear
        document.getElementById("undo-btn").addEventListener("click", undo);
        document.getElementById("redo-btn").addEventListener("click", redo);
        document.getElementById("clear-btn").addEventListener("click", () => {
          if (!confirm("Clear the entire canvas? This cannot be undone."))
            return;
          layers.forEach((layer) => {
            const lctx = layer.getContext("2d");
            lctx.clearRect(0, 0, layer.width, layer.height);
          });
          redrawComposite();
          saveState();
        });

        // Download PNG
        document
          .getElementById("download-btn")
          .addEventListener("click", () => {
            const link = document.createElement("a");
            link.download = "calcdomain-paint.png";
            link.href = canvas.toDataURL("image/png");
            link.click();
          });

        // Resize canvas
        const widthInput = document.getElementById("canvas-width");
        const heightInput = document.getElementById("canvas-height");
        document
          .getElementById("resize-canvas-btn")
          .addEventListener("click", () => {
            const newW = Math.min(
              2400,
              Math.max(200, parseInt(widthInput.value, 10) || canvas.width)
            );
            const newH = Math.min(
              1600,
              Math.max(200, parseInt(heightInput.value, 10) || canvas.height)
            );

            // Scale each layer to new size
            layers.forEach((layer, i) => {
              const tmp = createLayerCanvas(newW, newH);
              const tctx = tmp.getContext("2d");
              tctx.drawImage(layer, 0, 0, newW, newH);
              layers[i] = tmp;
            });

            canvas.width = newW;
            canvas.height = newH;
            redrawComposite();
            saveState();
          });

        // Initialize
        initLayers(canvas.width, canvas.height);
        setColor("#000000");
      })();
    </script>

