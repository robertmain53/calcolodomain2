    <script>
      // Utility: classify efficiency (US gallons heuristic)
      function efficiencyLabelFromG100mi(g100) {
        if (!isFinite(g100) || g100 <= 0) return "";
        if (g100 < 3) return "Very efficient fuel use";
        if (g100 < 4) return "Efficient for a typical car";
        if (g100 < 5) return "Average modern car";
        if (g100 < 7) return "Relatively thirsty vehicle";
        return "High fuel consumption";
      }

      function formatNumber(value, decimals) {
        if (!isFinite(value)) return "—";
        return value.toFixed(decimals);
      }

      function updateG100miToMpg() {
        const g100Input = document.getElementById("g100mi-input");
        const unitSelect = document.getElementById("unit-system-1");
        const resultEl = document.getElementById("mpg-result");
        const interpEl = document.getElementById("mpg-interpretation");

        const g100 = parseFloat(g100Input.value);
        if (!isFinite(g100) || g100 <= 0) {
          resultEl.textContent = "Please enter a positive value.";
          interpEl.textContent = "";
          return;
        }

        const mpg = 100 / g100;
        const unitLabel = unitSelect.value === "uk" ? "MPG (UK)" : "MPG (US)";
        resultEl.textContent = `${formatNumber(mpg, 2)} ${unitLabel}`;
        interpEl.textContent = efficiencyLabelFromG100mi(g100);
      }

      function updateMpgToG100mi() {
        const mpgInput = document.getElementById("mpg-input");
        const unitSelect = document.getElementById("unit-system-2");
        const resultEl = document.getElementById("g100mi-result");
        const interpEl = document.getElementById("g100mi-interpretation");

        const mpg = parseFloat(mpgInput.value);
        if (!isFinite(mpg) || mpg <= 0) {
          resultEl.textContent = "Please enter a positive value.";
          interpEl.textContent = "";
          return;
        }

        const g100 = 100 / mpg;
        const unitLabel =
          unitSelect.value === "uk"
            ? "gallons (UK) per 100 miles"
            : "gallons (US) per 100 miles";

        resultEl.textContent = `${formatNumber(g100, 2)} ${unitLabel}`;
        interpEl.textContent = efficiencyLabelFromG100mi(g100);
      }

      function compareCars() {
        const g1 = parseFloat(document.getElementById("car1-g100mi").value);
        const g2 = parseFloat(document.getElementById("car2-g100mi").value);
        const annualMiles = parseFloat(
          document.getElementById("annual-miles").value
        );
        const fuelPrice = parseFloat(
          document.getElementById("fuel-price").value
        );
        const co2PerGallon = parseFloat(
          document.getElementById("co2-per-gallon").value
        );

        const summaryEl = document.getElementById("comparison-summary");
        const detailsEl = document.getElementById("comparison-details");

        if (
          ![g1, g2, annualMiles, fuelPrice].every((v) => isFinite(v) && v > 0)
        ) {
          summaryEl.textContent =
            "Enter positive values for fuel use, annual miles, and fuel price.";
          detailsEl.classList.add("hidden");
          return;
        }

        const tripsPerYear = annualMiles / 100;
        const car1Gallons = g1 * tripsPerYear;
        const car2Gallons = g2 * tripsPerYear;
        const car1Mpg = 100 / g1;
        const car2Mpg = 100 / g2;

        const car1Cost = car1Gallons * fuelPrice;
        const car2Cost = car2Gallons * fuelPrice;

        const car1Co2 =
          isFinite(co2PerGallon) && co2PerGallon > 0
            ? car1Gallons * co2PerGallon
            : null;
        const car2Co2 =
          isFinite(co2PerGallon) && co2PerGallon > 0
            ? car2Gallons * co2PerGallon
            : null;

        const savingsGallons = car1Gallons - car2Gallons;
        const savingsCost = car1Cost - car2Cost;
        const savingsCo2 = car1Co2 !== null ? car1Co2 - car2Co2 : null;

        const better = g2 < g1 ? "Car B" : g2 > g1 ? "Car A" : "Both cars";
        summaryEl.textContent =
          `${better} uses less fuel. Annual difference: ${formatNumber(
            Math.abs(savingsGallons),
            1
          )} gallons and ` +
          `${
            savingsCost >= 0
              ? "$" + formatNumber(Math.abs(savingsCost), 0) + " saved"
              : "$" + formatNumber(Math.abs(savingsCost), 0) + " extra"
          }.`;

        document.getElementById(
          "car1-mpg"
        ).textContent = `Fuel economy: ${formatNumber(car1Mpg, 1)} MPG (US)`;
        document.getElementById(
          "car1-annual-gallons"
        ).textContent = `Fuel used: ${formatNumber(
          car1Gallons,
          1
        )} gallons/year`;
        document.getElementById(
          "car1-annual-cost"
        ).textContent = `Fuel cost: $${formatNumber(car1Cost, 0)}/year`;
        document.getElementById("car1-annual-co2").textContent =
          car1Co2 !== null
            ? `CO₂: ${formatNumber(car1Co2 / 1000, 2)} tonnes/year`
            : "CO₂: —";

        document.getElementById(
          "car2-mpg"
        ).textContent = `Fuel economy: ${formatNumber(car2Mpg, 1)} MPG (US)`;
        document.getElementById(
          "car2-annual-gallons"
        ).textContent = `Fuel used: ${formatNumber(
          car2Gallons,
          1
        )} gallons/year`;
        document.getElementById(
          "car2-annual-cost"
        ).textContent = `Fuel cost: $${formatNumber(car2Cost, 0)}/year`;
        document.getElementById("car2-annual-co2").textContent =
          car2Co2 !== null
            ? `CO₂: ${formatNumber(car2Co2 / 1000, 2)} tonnes/year`
            : "CO₂: —";

        document.getElementById(
          "savings-gallons"
        ).textContent = `Fuel saved: ${formatNumber(
          Math.abs(savingsGallons),
          1
        )} gallons/year`;
        document.getElementById(
          "savings-cost"
        ).textContent = `Money saved: $${formatNumber(
          Math.abs(savingsCost),
          0
        )}/year`;
        document.getElementById("savings-co2").textContent =
          savingsCo2 !== null
            ? `CO₂ avoided: ${formatNumber(
                Math.abs(savingsCo2) / 1000,
                2
              )} tonnes/year`
            : "CO₂ avoided: —";

        detailsEl.classList.remove("hidden");
      }

      document.addEventListener("DOMContentLoaded", function () {
        document
          .getElementById("convert-g100mi-to-mpg")
          .addEventListener("click", updateG100miToMpg);
        document
          .getElementById("convert-mpg-to-g100mi")
          .addEventListener("click", updateMpgToG100mi);

        document
          .getElementById("g100mi-input")
          .addEventListener("input", updateG100miToMpg);
        document
          .getElementById("unit-system-1")
          .addEventListener("change", updateG100miToMpg);
        document
          .getElementById("mpg-input")
          .addEventListener("input", updateMpgToG100mi);
        document
          .getElementById("unit-system-2")
          .addEventListener("change", updateMpgToG100mi);

        document
          .getElementById("compare-cars")
          .addEventListener("click", compareCars);

        // Initial calculations
        updateG100miToMpg();
        updateMpgToG100mi();
        compareCars();
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
