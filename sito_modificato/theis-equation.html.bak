    <script>
      // Utility: unit conversions to SI
      function convertQToM3s(Q, unit) {
        switch (unit) {
          case "m3s":
            return Q;
          case "m3d":
            return Q / 86400;
          case "lps":
            return Q / 1000;
          case "ft3s":
            return Q * 0.028316846592;
          case "gpm":
            return Q * 0.0000630901964; // 1 gpm = 0.00006309 m3/s
          default:
            return Q;
        }
      }
      function convertTToM2s(T, unit) {
        switch (unit) {
          case "m2s":
            return T;
          case "m2d":
            return T / 86400;
          case "ft2s":
            return T * 0.09290304;
          case "ft2d":
            return (T * 0.09290304) / 86400;
          default:
            return T;
        }
      }
      function convertRToMeters(r, unit) {
        switch (unit) {
          case "m":
            return r;
          case "ft":
            return r * 0.3048;
          default:
            return r;
        }
      }
      function convertTimeToSeconds(t, unit) {
        switch (unit) {
          case "s":
            return t;
          case "min":
            return t * 60;
          case "h":
            return t * 3600;
          case "d":
            return t * 86400;
          default:
            return t;
        }
      }

      // Exponential integral E1(x) = ∫_x^∞ e^{-t}/t dt
      // Piecewise approximation: series for small x, continued fraction for larger x
      function expEi1(x) {
        if (x <= 0) return Infinity;
        if (x < 1.0) {
          // Series: E1(x) = -γ - ln x + Σ (-1)^{n+1} x^n / (n * n!)
          const gamma = 0.5772156649015328606;
          let term = x;
          let sum = 0;
          let n = 1;
          while (Math.abs(term) > 1e-15 && n < 100) {
            sum += (Math.pow(-1, n + 1) * Math.pow(x, n)) / (n * factorial(n));
            n++;
          }
          return -gamma - Math.log(x) + sum;
        } else {
          // Continued fraction (Lentz's method)
          const maxIter = 200;
          const eps = 1e-14;
          let tiny = 1e-30;
          let b = x + 1;
          let C = 1 / tiny;
          let D = 1 / b;
          let f = D;
          for (let i = 1; i < maxIter; i++) {
            let a = -i;
            b += 2;
            D = 1 / (a * D + b);
            C = b + a / C;
            if (C === 0) C = tiny;
            if (D === 0) D = tiny;
            let delta = C * D;
            f *= delta;
            if (Math.abs(delta - 1) < eps) break;
          }
          return Math.exp(-x) * f;
        }
      }

      function factorial(n) {
        if (n < 0) return NaN;
        let res = 1;
        for (let i = 2; i <= n; i++) res *= i;
        return res;
      }

      function formatNumber(x, digits = 4) {
        if (!isFinite(x)) return "—";
        if (x === 0) return "0";
        const absx = Math.abs(x);
        if (absx >= 1e4 || absx <= 1e-3) {
          return x.toExponential(digits - 1);
        }
        return x.toFixed(digits);
      }

      let chart;

      function updateChart(Qm3s, Tm2s, S, rm, tMinSec, tMaxSec, rUnitLabel) {
        const ctx = document.getElementById("theis-chart").getContext("2d");
        const n = 60;
        const times = [];
        const sVals = [];
        const logMin = Math.log10(tMinSec);
        const logMax = Math.log10(tMaxSec);
        for (let i = 0; i < n; i++) {
          const logt = logMin + ((logMax - logMin) * i) / (n - 1);
          const t = Math.pow(10, logt);
          const u = (rm * rm * S) / (4 * Tm2s * t);
          const Wu = expEi1(u);
          const s_m = (Qm3s / (4 * Math.PI * Tm2s)) * Wu; // in meters
          // convert s to same unit as r
          let s;
          if (rUnitLabel === "ft") {
            s = s_m / 0.3048;
          } else {
            s = s_m;
          }
          times.push(t);
          sVals.push(s);
        }

        const data = {
          labels: times,
          datasets: [
            {
              label: "Theis drawdown",
              data: sVals,
              borderColor: "rgb(37, 99, 235)",
              backgroundColor: "rgba(37, 99, 235, 0.1)",
              tension: 0.15,
              pointRadius: 0,
            },
          ],
        };

        const options = {
          responsive: true,
          maintainAspectRatio: false,
          scales: {
            x: {
              type: "logarithmic",
              title: { display: true, text: "Time since pumping started (s)" },
              ticks: {
                callback: function (value, index, ticks) {
                  const v = this.getLabelForValue(value);
                  return Number(v).toExponential(1);
                },
              },
            },
            y: {
              type: "logarithmic",
              title: { display: true, text: "Drawdown s (" + rUnitLabel + ")" },
              ticks: {
                callback: function (value) {
                  return Number(value).toExponential(1);
                },
              },
            },
          },
          plugins: {
            legend: { display: true },
            tooltip: {
              callbacks: {
                label: function (context) {
                  const t = context.label;
                  const s = context.parsed.y;
                  return (
                    "t = " +
                    formatNumber(Number(t), 3) +
                    " s, s = " +
                    formatNumber(s, 3) +
                    " " +
                    rUnitLabel
                  );
                },
              },
            },
          },
        };

        if (chart) {
          chart.data = data;
          chart.options = options;
          chart.update();
        } else {
          chart = new Chart(ctx, {
            type: "line",
            data,
            options,
          });
        }
      }

      document
        .getElementById("compute-btn")
        .addEventListener("click", function () {
          const Q = parseFloat(document.getElementById("Q").value);
          const T = parseFloat(document.getElementById("T").value);
          const S = parseFloat(document.getElementById("S").value);
          const r = parseFloat(document.getElementById("r").value);
          const t = parseFloat(document.getElementById("t").value);

          const Q_unit = document.getElementById("Q_unit").value;
          const T_unit = document.getElementById("T_unit").value;
          const r_unit = document.getElementById("r_unit").value;
          const t_unit = document.getElementById("t_unit").value;

          const showCJ = document.getElementById("show_cj").checked;
          const generateCurve =
            document.getElementById("generate_curve").checked;

          const errorEl = document.getElementById("error-msg");
          errorEl.textContent = "";

          if (!(Q > 0 && T > 0 && S > 0 && r > 0 && t > 0)) {
            errorEl.textContent = "All input values must be positive.";
            return;
          }

          const Qm3s = convertQToM3s(Q, Q_unit);
          const Tm2s = convertTToM2s(T, T_unit);
          const rm = convertRToMeters(r, r_unit);
          const ts = convertTimeToSeconds(t, t_unit);

          const u = (rm * rm * S) / (4 * Tm2s * ts);
          const Wu = expEi1(u);
          const s_m = (Qm3s / (4 * Math.PI * Tm2s)) * Wu; // drawdown in meters

          // Convert drawdown to same unit as r for display
          let s_display;
          if (r_unit === "ft") {
            s_display = s_m / 0.3048;
          } else {
            s_display = s_m;
          }

          // Cooper–Jacob approximation
          const gamma = 0.5772156649015329;
          const Wu_cj = -gamma - Math.log(u);
          const s_cj_m = (Qm3s / (4 * Math.PI * Tm2s)) * Wu_cj;
          let s_cj_display = r_unit === "ft" ? s_cj_m / 0.3048 : s_cj_m;

          const relErr = Math.abs((s_cj_m - s_m) / s_m);

          // Update results UI
          document.getElementById("results-content").classList.remove("hidden");
          document.getElementById("s_theis").textContent =
            formatNumber(s_display, 4) + " " + r_unit;
          document.getElementById("u_val").textContent = formatNumber(u, 4);
          document.getElementById("Wu_val").textContent = formatNumber(Wu, 4);

          const cjCard = document.getElementById("cj-card");
          if (showCJ) {
            cjCard.classList.remove("hidden");
            document.getElementById("s_cj").textContent =
              formatNumber(s_cj_display, 4) + " " + r_unit;
            document.getElementById("rel_err").textContent =
              formatNumber(relErr * 100, 3) + " %";
          } else {
            cjCard.classList.add("hidden");
          }

          // Update chart
          if (generateCurve) {
            // choose t range around current t (two decades below and above)
            const tsMin = ts / 100;
            const tsMax = ts * 100;
            const tMinSec = Math.max(tsMin, 1e-1);
            const tMaxSec = Math.max(tsMax, tMinSec * 10);
            updateChart(Qm3s, Tm2s, S, rm, tMinSec, tMaxSec, r_unit);
          }
        });

      document
        .getElementById("reset-btn")
        .addEventListener("click", function () {
          document.getElementById("Q").value = 0.01;
          document.getElementById("Q_unit").value = "m3s";
          document.getElementById("T").value = 1e-3;
          document.getElementById("T_unit").value = "m2s";
          document.getElementById("S").value = 1e-4;
          document.getElementById("r").value = 50;
          document.getElementById("r_unit").value = "m";
          document.getElementById("t").value = 3600;
          document.getElementById("t_unit").value = "s";
          document.getElementById("show_cj").checked = true;
          document.getElementById("generate_curve").checked = true;
          document.getElementById("error-msg").textContent = "";
          document.getElementById("results-content").classList.add("hidden");
          if (chart) {
            chart.destroy();
            chart = null;
          }
        });

      // Initialize empty chart for layout stability
      window.addEventListener("load", function () {
        const ctx = document.getElementById("theis-chart").getContext("2d");
        chart = new Chart(ctx, {
          type: "line",
          data: { labels: [], datasets: [] },
          options: {
            responsive: true,
            maintainAspectRatio: false,
            scales: {
              x: { type: "logarithmic", display: true },
              y: { type: "logarithmic", display: true },
            },
            plugins: { legend: { display: false } },
          },
        });
      });

      // Mobile menu toggle
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          this.setAttribute("aria-expanded", isHidden ? "true" : "false");
        });
    </script>

