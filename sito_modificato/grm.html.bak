    <script>
      // Mobile menu toggle
      document
        .getElementById("mobile-menu-toggle")
        ?.addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          if (!menu) return;
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          this.setAttribute("aria-expanded", String(isHidden));
        });

      function formatNumber(value, decimals) {
        if (isNaN(value) || !isFinite(value)) return "–";
        return Number(value).toLocaleString(undefined, {
          minimumFractionDigits: decimals,
          maximumFractionDigits: decimals,
        });
      }

      // GRM calculator logic
      (function () {
        const modeTabs = document.querySelectorAll(".grm-mode-tab");
        const modePanels = {
          grm: document.getElementById("mode-grm"),
          price: document.getElementById("mode-price"),
          rent: document.getElementById("mode-rent"),
        };
        const rentFreqButtons = document.querySelectorAll(".rentfreq-btn");
        let rentFrequency = "annual"; // or 'monthly'
        let currentMode = "grm";

        const rentLabels = {
          grm: document.getElementById("rent-label-grm"),
          price: document.getElementById("rent-label-price"),
        };

        const currencyInput = document.getElementById("currency-symbol");
        const decimalsInput = document.getElementById("decimals");
        const resetBtn = document.getElementById("grm-reset");
        const calcBtn = document.getElementById("grm-calc-btn");
        const resultsBox = document.getElementById("grm-results");
        const resultMainLabel = document.getElementById("result-main-label");
        const resultMainValue = document.getElementById("result-main-value");
        const resultMainSub = document.getElementById("result-main-sub");
        const resultInterp = document.getElementById("result-interpretation");

        const priceGrm = document.getElementById("price-grm");
        const rentGrm = document.getElementById("rent-grm");
        const grmPrice = document.getElementById("grm-price");
        const rentPrice = document.getElementById("rent-price");
        const grmRent = document.getElementById("grm-rent");
        const priceRent = document.getElementById("price-rent");

        // Mode switching
        modeTabs.forEach((tab) => {
          tab.addEventListener("click", () => {
            const mode = tab.getAttribute("data-mode");
            if (!mode || !modePanels[mode]) return;
            currentMode = mode;

            modeTabs.forEach((t) =>
              t.classList.remove("tab-active", "text-blue-600")
            );
            modeTabs.forEach((t) => t.classList.add("text-gray-500"));
            tab.classList.add("tab-active");
            tab.classList.remove("text-gray-500");

            Object.keys(modePanels).forEach((m) => {
              modePanels[m].classList.toggle("hidden", m !== mode);
            });

            updateRentLabels();
            resultsBox.classList.add("hidden");
          });
        });

        function updateRentLabels() {
          const labelText = rentFrequency === "annual" ? "annual" : "monthly";
          Object.values(rentLabels).forEach((el) => {
            if (el) el.textContent = labelText;
          });
        }

        // Rent frequency toggle
        rentFreqButtons.forEach((btn) => {
          btn.addEventListener("click", () => {
            const freq = btn.getAttribute("data-rentfreq");
            if (!freq) return;
            rentFrequency = freq;

            rentFreqButtons.forEach((b) => {
              b.classList.remove("bg-blue-50", "text-blue-700");
              b.classList.add("bg-white", "text-gray-700");
            });
            btn.classList.add("bg-blue-50", "text-blue-700");
            btn.classList.remove("text-gray-700");

            updateRentLabels();
            resultsBox.classList.add("hidden");
          });
        });

        // Reset
        resetBtn.addEventListener("click", () => {
          [priceGrm, rentGrm, grmPrice, rentPrice, grmRent, priceRent].forEach(
            (input) => {
              if (input) input.value = "";
            }
          );
          decimalsInput.value = 2;
          currencyInput.value = "$";
          resultsBox.classList.add("hidden");
        });

        // Main calculation
        calcBtn.addEventListener("click", () => {
          const decimals = Math.min(
            4,
            Math.max(0, parseInt(decimalsInput.value, 10) || 0)
          );
          decimalsInput.value = decimals;
          const currency = currencyInput.value || "$";

          let mainValue = null;
          let mainLabel = "";
          let mainSub = "";
          let interp = "";

          const rentToAnnual = (rent) => {
            if (rentFrequency === "monthly") return rent * 12;
            return rent;
          };

          if (currentMode === "grm") {
            const price = parseFloat(priceGrm.value);
            const rentInput = parseFloat(rentGrm.value);
            if (!(price > 0) || !(rentInput > 0)) {
              alert("Please enter both price and rent greater than 0.");
              return;
            }
            const annualRent = rentToAnnual(rentInput);
            const grm = price / annualRent;
            mainValue = grm;
            mainLabel = "GRM";
            mainSub = `Price ${currency}${formatNumber(
              price,
              decimals
            )} ÷ annual rent ${currency}${formatNumber(annualRent, decimals)}`;
            if (grm < 8) {
              interp =
                "This is a relatively low GRM. If expenses and risk are reasonable, this could indicate a strong income-to-price ratio.";
            } else if (grm <= 15) {
              interp =
                "This GRM is in a typical range for many residential rentals. Compare to similar properties in the same market.";
            } else {
              interp =
                "This is a relatively high GRM. The property may be in a premium or low-risk area, or rents may be under market.";
            }
          } else if (currentMode === "price") {
            const grmVal = parseFloat(grmPrice.value);
            const rentInput = parseFloat(rentPrice.value);
            if (!(grmVal > 0) || !(rentInput > 0)) {
              alert("Please enter GRM and rent greater than 0.");
              return;
            }
            const annualRent = rentToAnnual(rentInput);
            const price = grmVal * annualRent;
            mainValue = price;
            mainLabel = "Implied property value";
            mainSub = `Price = GRM ${formatNumber(
              grmVal,
              2
            )} × annual rent ${currency}${formatNumber(annualRent, decimals)}`;
            interp =
              "This is the approximate price implied by your target GRM and rent. Always cross-check with cap rate and full underwriting.";
          } else if (currentMode === "rent") {
            const grmVal = parseFloat(grmRent.value);
            const price = parseFloat(priceRent.value);
            if (!(grmVal > 0) || !(price > 0)) {
              alert("Please enter GRM and price greater than 0.");
              return;
            }
            const annualRent = price / grmVal;
            const rentOutput =
              rentFrequency === "monthly" ? annualRent / 12 : annualRent;
            mainValue = rentOutput;
            const freqLabel =
              rentFrequency === "monthly"
                ? "Required monthly rent"
                : "Required annual rent";
            mainLabel = freqLabel;
            mainSub = `Annual rent = Price ${currency}${formatNumber(
              price,
              decimals
            )} ÷ GRM ${formatNumber(grmVal, 2)}`;
            interp =
              "This is the rent level needed to hit your target GRM. Check if this rent is realistic for the local market.";
          }

          if (mainValue === null) return;

          resultMainLabel.textContent = mainLabel;
          resultMainValue.textContent =
            currentMode === "grm"
              ? formatNumber(mainValue, 2)
              : currency + formatNumber(mainValue, decimals);
          resultMainSub.textContent = mainSub;
          resultInterp.textContent = interp;
          resultsBox.classList.remove("hidden");
        });

        // Comparison table
        const compareTbody = document.getElementById("compare-tbody");
        const addRowBtn = document.getElementById("add-row-btn");

        function createRow(index) {
          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="px-3 py-2">
          <input type="text" class="w-full border border-gray-200 rounded-md px-2 py-1 text-sm" placeholder="Property ${
            index + 1
          }">
        </td>
        <td class="px-3 py-2 text-right">
          <input type="number" min="0" step="0.01" class="w-full border border-gray-200 rounded-md px-2 py-1 text-sm text-right price-input">
        </td>
        <td class="px-3 py-2 text-right">
          <input type="number" min="0" step="0.01" class="w-full border border-gray-200 rounded-md px-2 py-1 text-sm text-right rent-input">
        </td>
        <td class="px-3 py-2 text-right">
          <span class="grm-output text-sm text-gray-800">–</span>
        </td>
        <td class="px-3 py-2 text-center">
          <span class="best-badge hidden result-badge bg-green-100 text-green-800">Lowest GRM</span>
        </td>
        <td class="px-3 py-2 text-center">
          <button type="button" class="remove-row text-xs text-red-500 hover:underline">Remove</button>
        </td>
      `;
          return tr;
        }

        function recalcTable() {
          const rows = Array.from(compareTbody.querySelectorAll("tr"));
          let bestGrm = Infinity;
          let bestRows = [];

          rows.forEach((row) => {
            const priceInput = row.querySelector(".price-input");
            const rentInput = row.querySelector(".rent-input");
            const grmSpan = row.querySelector(".grm-output");
            const bestBadge = row.querySelector(".best-badge");

            bestBadge.classList.add("hidden");
            let grmText = "–";

            const price = parseFloat(priceInput.value);
            const rent = parseFloat(rentInput.value);
            if (price > 0 && rent > 0) {
              const annualRent = rent; // assume annual in comparison table
              const grm = price / annualRent;
              grmText = formatNumber(grm, 2);
              if (grm < bestGrm) {
                bestGrm = grm;
                bestRows = [row];
              } else if (grm === bestGrm) {
                bestRows.push(row);
              }
            }
            grmSpan.textContent = grmText;
          });

          if (isFinite(bestGrm)) {
            bestRows.forEach((row) => {
              const bestBadge = row.querySelector(".best-badge");
              if (bestBadge) bestBadge.classList.remove("hidden");
            });
          }
        }

        function addRow() {
          const currentRows = compareTbody.querySelectorAll("tr").length;
          if (currentRows >= 4) return;
          const row = createRow(currentRows);
          compareTbody.appendChild(row);

          row.querySelectorAll(".price-input, .rent-input").forEach((input) => {
            input.addEventListener("input", recalcTable);
          });
          row.querySelector(".remove-row").addEventListener("click", () => {
            row.remove();
            recalcTable();
          });
        }

        addRowBtn.addEventListener("click", () => {
          addRow();
        });

        // Initialize with 2 rows
        addRow();
        addRow();
      })();

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
