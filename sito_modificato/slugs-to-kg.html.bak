  <script>
    const SLUG_TO_KG = 14.59390294;

    const slugInput = document.getElementById('slug-value');
    const kgInput = document.getElementById('kg-value');
    const tbl025 = document.getElementById('tbl-025');
    const tbl05 = document.getElementById('tbl-05');
    const tbl1 = document.getElementById('tbl-1');
    const tbl2 = document.getElementById('tbl-2');
    const tbl5 = document.getElementById('tbl-5');

    let isUpdating = false;

    function updateFromSlug() {
      if (isUpdating) return;
      isUpdating = true;
      const slugVal = parseFloat(slugInput.value || 0);
      const kgVal = slugVal * SLUG_TO_KG;
      kgInput.value = +kgVal.toFixed(6);
      isUpdating = false;
    }

    function updateFromKg() {
      if (isUpdating) return;
      isUpdating = true;
      const kgVal = parseFloat(kgInput.value || 0);
      const slugVal = kgVal / SLUG_TO_KG;
      slugInput.value = +slugVal.toFixed(6);
      isUpdating = false;
    }

    function updateTable() {
      tbl025.textContent = (0.25 * SLUG_TO_KG).toFixed(4).replace(/\.?0+$/,'');
      tbl05.textContent = (0.5 * SLUG_TO_KG).toFixed(4).replace(/\.?0+$/,'');
      tbl1.textContent = (1 * SLUG_TO_KG).toFixed(4).replace(/\.?0+$/,'');
      tbl2.textContent = (2 * SLUG_TO_KG).toFixed(4).replace(/\.?0+$/,'');
      tbl5.textContent = (5 * SLUG_TO_KG).toFixed(4).replace(/\.?0+$/,'');
    }

    slugInput.addEventListener('input', updateFromSlug);
    kgInput.addEventListener('input', updateFromKg);

    // init
    updateFromSlug();
    updateTable();
  </script>

