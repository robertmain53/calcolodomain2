    <script>
      function parseNumber(id) {
        const el = document.getElementById(id);
        if (!el) return 0;
        const v = parseFloat(el.value.replace(",", "."));
        return isNaN(v) || v < 0 ? 0 : v;
      }

      function formatGrams(value) {
        return value.toFixed(1).replace(/\.0$/, "") + " g";
      }

      function getFactor(id) {
        const el = document.getElementById(id);
        if (!el) return 0;
        const v = parseFloat(el.value);
        return isNaN(v) ? 0 : v;
      }

      function updatePresetLabel(name, description) {
        const label = document.getElementById("presetLabel");
        if (!label) return;
        if (!name) {
          label.textContent = "";
          return;
        }
        label.textContent = name + " preset: " + description;
      }

      function classifyKeto(netPerServing) {
        if (netPerServing <= 3) {
          return {
            text: "Very keto‑friendly (low net carbs per serving)",
            color: "text-green-700",
          };
        } else if (netPerServing <= 7) {
          return {
            text: "Moderate – fits many low‑carb diets",
            color: "text-amber-700",
          };
        } else {
          return {
            text: "High net carbs – may not fit strict keto",
            color: "text-red-700",
          };
        }
      }

      function calculateNetCarbs() {
        const totalCarbs = parseNumber("totalCarbs");
        const fiber = parseNumber("fiber");
        const erythritol = parseNumber("erythritol");
        const otherSa = parseNumber("otherSugarAlcohols");
        const allulose = parseNumber("allulose");
        let servings = parseNumber("servings");
        if (servings <= 0) servings = 1;

        if (totalCarbs === 0) {
          alert("Please enter total carbohydrates.");
          return;
        }

        const fiberFactor = getFactor("fiberFactor");
        const erythritolFactor = getFactor("erythritolFactor");
        const otherSaFactor = getFactor("otherSaFactor");

        const effectiveFiber = fiber * fiberFactor;
        const effectiveErythritol = erythritol * erythritolFactor;
        const effectiveOtherSa = otherSa * otherSaFactor;
        const effectiveAllulose = allulose * 0; // default: subtract all (0 impact)

        let netPerServing =
          totalCarbs -
          effectiveFiber -
          effectiveErythritol -
          effectiveOtherSa -
          effectiveAllulose;
        if (netPerServing < 0) netPerServing = 0;

        const netTotal = netPerServing * servings;

        const resultsCard = document.getElementById("resultsCard");
        const perServingEl = document.getElementById("netCarbsPerServing");
        const totalEl = document.getElementById("netCarbsTotal");
        const servingNote = document.getElementById("servingNote");
        const ketoRatingEl = document.getElementById("ketoRating");
        const breakdownEl = document.getElementById("formulaBreakdown");

        if (
          !resultsCard ||
          !perServingEl ||
          !totalEl ||
          !ketoRatingEl ||
          !breakdownEl
        )
          return;

        resultsCard.classList.remove("hidden");
        perServingEl.textContent = formatGrams(netPerServing);
        totalEl.textContent = formatGrams(netTotal);
        servingNote.textContent =
          servings > 1 ? "(" + servings + " servings)" : "(1 serving)";

        const rating = classifyKeto(netPerServing);
        ketoRatingEl.textContent = rating.text;
        ketoRatingEl.className = "text-sm font-medium " + rating.color;

        const lines = [];
        lines.push("Calculation per serving:");
        lines.push(
          "Net carbs = Total carbs − (Fiber × factor) − (Erythritol × factor) − (Other sugar alcohols × factor) − Allulose"
        );
        lines.push(
          "Net carbs = " +
            totalCarbs.toFixed(1).replace(/\.0$/, "") +
            " − " +
            effectiveFiber.toFixed(1).replace(/\.0$/, "") +
            " − " +
            effectiveErythritol.toFixed(1).replace(/\.0$/, "") +
            " − " +
            effectiveOtherSa.toFixed(1).replace(/\.0$/, "") +
            " − 0"
        );
        lines.push("= " + netPerServing.toFixed(2) + " g per serving");

        breakdownEl.textContent = lines.join(" | ");
      }

      function applyPreset(preset) {
        const fiberFactorEl = document.getElementById("fiberFactor");
        const erythritolFactorEl = document.getElementById("erythritolFactor");
        const otherSaFactorEl = document.getElementById("otherSaFactor");
        if (!fiberFactorEl || !erythritolFactorEl || !otherSaFactorEl) return;

        if (preset === "keto-strict") {
          fiberFactorEl.value = "0";
          erythritolFactorEl.value = "0";
          otherSaFactorEl.value = "0.5";
          updatePresetLabel(
            "Keto (strict)",
            "Subtract all fiber, erythritol, allulose; count 50% of other sugar alcohols."
          );
        } else if (preset === "keto-liberal") {
          fiberFactorEl.value = "0";
          erythritolFactorEl.value = "0";
          otherSaFactorEl.value = "0";
          updatePresetLabel(
            "Keto (liberal)",
            "Subtract all fiber and all sugar alcohols."
          );
        } else if (preset === "diabetes") {
          fiberFactorEl.value = "0.5";
          erythritolFactorEl.value = "1";
          otherSaFactorEl.value = "1";
          updatePresetLabel(
            "Diabetes‑cautious",
            "Count 50% of fiber and 100% of sugar alcohols."
          );
        } else {
          fiberFactorEl.value = "0";
          erythritolFactorEl.value = "0";
          otherSaFactorEl.value = "0.5";
          updatePresetLabel("", "");
        }
      }

      document.addEventListener("DOMContentLoaded", function () {
        const calcBtn = document.getElementById("calculateBtn");
        const resetBtn = document.getElementById("resetBtn");
        const form = document.getElementById("netCarbsForm");

        if (calcBtn) calcBtn.addEventListener("click", calculateNetCarbs);

        if (resetBtn && form) {
          resetBtn.addEventListener("click", function () {
            form.reset();
            applyPreset(null);
            const resultsCard = document.getElementById("resultsCard");
            if (resultsCard) resultsCard.classList.add("hidden");
          });
        }

        document
          .querySelectorAll("button[data-preset]")
          .forEach(function (btn) {
            btn.addEventListener("click", function () {
              const preset = btn.getAttribute("data-preset");
              applyPreset(preset);
              calculateNetCarbs();
            });
          });

        // Default preset: Standard (already set in HTML)
        applyPreset(null);
      });
    </script>

