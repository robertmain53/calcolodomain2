    <script>
      // Helper conversions
      function cmToMeters(cm) {
        return cm / 100;
      }
      function kgToLb(kg) {
        return kg * 2.2046226218;
      }
      function lbToKg(lb) {
        return lb / 2.2046226218;
      }
      function ftInToCm(ft, inch) {
        return (ft * 12 + inch) * 2.54;
      }

      const unitButtons = document.querySelectorAll(".unit-toggle");
      const sexButtons = document.querySelectorAll(".sex-toggle");
      const heightMetricGroup = document.getElementById("height-metric-group");
      const heightImperialGroup = document.getElementById(
        "height-imperial-group"
      );
      const weightMetricGroup = document.getElementById("weight-metric-group");
      const weightImperialGroup = document.getElementById(
        "weight-imperial-group"
      );
      const goalSelect = document.getElementById("goal");
      const customProteinGroup = document.getElementById(
        "custom-protein-group"
      );
      const calcBtn = document.getElementById("calc-btn");
      const resultsSection = document.getElementById("results-section");
      const macroSection = document.getElementById("macro-section");

      let currentUnit = "metric";
      let currentSex = "male";

      unitButtons.forEach((btn) => {
        btn.addEventListener("click", () => {
          currentUnit = btn.dataset.unit;
          unitButtons.forEach((b) => {
            const active = b.dataset.unit === currentUnit;
            b.className =
              "unit-toggle px-3 py-1.5 rounded border text-sm font-medium " +
              (active
                ? "bg-blue-600 text-white border-blue-600"
                : "bg-white text-gray-700 border-gray-300");
          });
          if (currentUnit === "metric") {
            heightMetricGroup.classList.remove("hidden");
            weightMetricGroup.classList.remove("hidden");
            heightImperialGroup.classList.add("hidden");
            weightImperialGroup.classList.add("hidden");
          } else {
            heightMetricGroup.classList.add("hidden");
            weightMetricGroup.classList.add("hidden");
            heightImperialGroup.classList.remove("hidden");
            weightImperialGroup.classList.remove("hidden");
          }
        });
      });

      sexButtons.forEach((btn) => {
        btn.addEventListener("click", () => {
          currentSex = btn.dataset.sex;
          sexButtons.forEach((b) => {
            const active = b.dataset.sex === currentSex;
            b.className =
              "sex-toggle px-3 py-1.5 rounded border text-sm font-medium " +
              (active
                ? "bg-blue-600 text-white border-blue-600"
                : "bg-white text-gray-700 border-gray-300");
          });
        });
      });

      goalSelect.addEventListener("change", () => {
        if (goalSelect.value === "custom") {
          customProteinGroup.classList.remove("hidden");
        } else {
          customProteinGroup.classList.add("hidden");
        }
      });

      function getActivityLabel(val) {
        switch (val) {
          case "sedentary":
            return "Sedentary";
          case "light":
            return "Lightly active";
          case "moderate":
            return "Moderately active";
          case "high":
            return "Very active";
          case "athlete":
            return "Athlete / twice-daily training";
          default:
            return "";
        }
      }

      function getGoalLabel(val) {
        switch (val) {
          case "maintain":
            return "Maintain weight & health";
          case "fatloss":
            return "Fat loss (cutting)";
          case "muscle":
            return "Muscle gain / recomposition";
          case "endurance":
            return "Endurance performance";
          case "custom":
            return "Custom protein target";
          default:
            return "";
        }
      }

      function getProteinFactor(goal, activity) {
        // Returns g/kg based on goal & activity
        const base = {
          maintain: {
            sedentary: 1.2,
            light: 1.3,
            moderate: 1.4,
            high: 1.5,
            athlete: 1.6,
          },
          fatloss: {
            sedentary: 1.8,
            light: 2.0,
            moderate: 2.1,
            high: 2.2,
            athlete: 2.3,
          },
          muscle: {
            sedentary: 1.6,
            light: 1.7,
            moderate: 1.8,
            high: 2.0,
            athlete: 2.2,
          },
          endurance: {
            sedentary: 1.3,
            light: 1.4,
            moderate: 1.5,
            high: 1.6,
            athlete: 1.8,
          },
        };
        if (!base[goal]) return 1.6;
        return base[goal][activity] || 1.6;
      }

      calcBtn.addEventListener("click", () => {
        const age = parseFloat(document.getElementById("age").value);
        const bodyfat = parseFloat(document.getElementById("bodyfat").value);
        const activity = document.getElementById("activity").value;
        const goal = goalSelect.value;
        const useLean = document.getElementById("use-lean-mass").checked;
        const meals = parseInt(document.getElementById("meals").value, 10);
        const calories = parseFloat(document.getElementById("calories").value);

        let weightKg;
        if (currentUnit === "metric") {
          weightKg = parseFloat(document.getElementById("weight-kg").value);
        } else {
          const weightLb = parseFloat(
            document.getElementById("weight-lb").value
          );
          weightKg = lbToKg(weightLb);
        }

        if (!weightKg || weightKg <= 0 || !age || age < 10) {
          alert("Please enter a valid age and weight.");
          return;
        }

        let basisKg = weightKg;
        let basisLabel = "Total body weight";

        if (!isNaN(bodyfat) && bodyfat > 3 && bodyfat < 60 && useLean) {
          const leanKg = weightKg * (1 - bodyfat / 100);
          if (leanKg > 0) {
            basisKg = leanKg;
            basisLabel = "Estimated lean body mass";
          }
        }

        let proteinPerKg;
        if (goal === "custom") {
          const customVal = parseFloat(
            document.getElementById("custom-protein").value
          );
          const customUnit = document.getElementById(
            "custom-protein-unit"
          ).value;
          if (!customVal || customVal <= 0) {
            alert("Please enter a valid custom protein target.");
            return;
          }
          proteinPerKg =
            customUnit === "g_per_kg" ? customVal : customVal * 2.2046226218;
        } else {
          proteinPerKg = getProteinFactor(goal, activity);
        }

        let proteinGrams = basisKg * proteinPerKg;
        // Clamp to a reasonable range to avoid extreme outputs
        proteinGrams = Math.max(40, Math.min(proteinGrams, 350));

        const proteinOunces = proteinGrams / 28.3495;
        const proteinPerKgActual = proteinGrams / weightKg;
        const proteinPerLbActual = proteinGrams / kgToLb(weightKg);
        const proteinPerMeal = proteinGrams / meals;

        document.getElementById("protein-grams").textContent =
          proteinGrams.toFixed(0);
        document.getElementById("protein-ounces").textContent =
          proteinOunces.toFixed(1);
        document.getElementById("protein-per-kg").textContent =
          proteinPerKgActual.toFixed(2);
        document.getElementById("protein-per-lb").textContent =
          proteinPerLbActual.toFixed(2);
        document.getElementById("protein-per-meal").textContent =
          proteinPerMeal.toFixed(0);
        document.getElementById("meals-label").textContent = meals.toString();

        if (!isNaN(calories) && calories > 0) {
          const proteinKcal = proteinGrams * 4;
          const proteinPct = (proteinKcal / calories) * 100;
          document.getElementById("protein-kcal").textContent =
            proteinKcal.toFixed(0);
          document.getElementById("protein-percent").textContent = Math.min(
            100,
            proteinPct
          ).toFixed(1);
          macroSection.classList.remove("hidden");
        } else {
          macroSection.classList.add("hidden");
        }

        document.getElementById("summary-goal").textContent =
          getGoalLabel(goal);
        document.getElementById("summary-activity").textContent =
          getActivityLabel(activity);
        document.getElementById("summary-basis").textContent = basisLabel;

        resultsSection.classList.remove("hidden");
        resultsSection.scrollIntoView({ behavior: "smooth", block: "start" });
      });

      // Simple FAQ toggle (in addition to global script_faq if any)
      document
        .querySelectorAll("#faq-container .faq-question")
        .forEach((btn) => {
          btn.addEventListener("click", () => {
            const answer = btn.nextElementSibling;
            const isHidden = answer.classList.contains("hidden");
            document
              .querySelectorAll("#faq-container .faq-answer")
              .forEach((a) => a.classList.add("hidden"));
            document
              .querySelectorAll("#faq-container .faq-question span:last-child")
              .forEach((icon) => (icon.textContent = "+"));
            if (isHidden) {
              answer.classList.remove("hidden");
              btn.querySelector("span:last-child").textContent = "−";
            }
          });
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
