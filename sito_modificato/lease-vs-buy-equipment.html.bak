    <script>
      // Utility functions
      function toNumber(id) {
        const v = parseFloat(document.getElementById(id).value);
        return isNaN(v) ? 0 : v;
      }
      function fmtMoney(v) {
        if (!isFinite(v)) return "-";
        const sign = v < 0 ? "-" : "";
        const abs = Math.abs(v);
        return (
          sign +
          "$" +
          abs.toLocaleString(undefined, { maximumFractionDigits: 0 })
        );
      }
      function fmtMoney2(v) {
        if (!isFinite(v)) return "-";
        const sign = v < 0 ? "-" : "";
        const abs = Math.abs(v);
        return (
          sign +
          "$" +
          abs.toLocaleString(undefined, {
            minimumFractionDigits: 2,
            maximumFractionDigits: 2,
          })
        );
      }
      function fmtPct(v) {
        if (!isFinite(v)) return "-";
        return v.toLocaleString(undefined, { maximumFractionDigits: 1 }) + "%";
      }

      function buildScheduleBuy(params) {
        const {
          cost,
          downPct,
          loanRate,
          loanYears,
          analysisYears,
          taxRate,
          discountRate,
          salvagePct,
          useDep,
        } = params;

        const down = cost * (downPct / 100);
        const loanAmount = cost - down;
        const nLoanMonths = Math.round(loanYears * 12);
        const nAnalysisMonths = Math.round(analysisYears * 12);
        const rLoan = loanRate / 100 / 12;
        const rDisc = discountRate / 100 / 12;
        const salvage = cost * (salvagePct / 100);

        let payment = 0;
        if (loanAmount > 0 && rLoan > 0 && nLoanMonths > 0) {
          payment =
            (rLoan * loanAmount) / (1 - Math.pow(1 + rLoan, -nLoanMonths));
        } else if (loanAmount > 0 && nLoanMonths > 0) {
          payment = loanAmount / nLoanMonths;
        }

        const depYears = Math.min(loanYears, analysisYears);
        const depAnnual = useDep && depYears > 0 ? cost / depYears : 0;
        const depMonthly = depAnnual / 12;

        let balance = loanAmount;
        const rows = [];
        let npv = 0;
        let totalCash = down;
        let taxPV = 0;
        let residualPV = 0;

        // t=0: down payment
        npv += down; // already at present value

        for (let m = 1; m <= nAnalysisMonths; m++) {
          const t = m / 12;
          let interest = 0;
          let principal = 0;
          let loanPayment = 0;

          if (m <= nLoanMonths && balance > 0) {
            loanPayment = payment;
            interest = balance * rLoan;
            principal = loanPayment - interest;
            balance -= principal;
            if (balance < 1e-6) balance = 0;
          }

          const depreciation = useDep && t <= depYears ? depMonthly : 0;
          const taxShield = (interest + depreciation) * (taxRate / 100);

          const cashOut = loanPayment; // ignoring non-cash depreciation
          const netCash = cashOut - taxShield;

          const df = Math.pow(1 + rDisc, m);
          const pv = netCash / df;
          npv += pv;
          totalCash += cashOut;
          taxPV += taxShield / df;

          rows.push({
            month: m,
            year: t,
            loanPayment,
            interest,
            principal,
            depreciation,
            taxShield,
            netCash,
            pv,
          });
        }

        // Residual value at end of analysis period (if you still own it)
        if (salvage > 0) {
          const mEnd = nAnalysisMonths;
          const dfEnd = Math.pow(1 + rDisc, mEnd);
          const pvRes = -salvage / dfEnd; // benefit reduces cost
          npv += pvRes;
          residualPV = -pvRes;
        }

        return {
          npv,
          rows,
          payment,
          down,
          totalCash,
          taxPV,
          residualPV,
        };
      }

      function buildScheduleLease(params) {
        const {
          cost,
          leasePayment,
          leaseYears,
          analysisYears,
          taxRate,
          discountRate,
          leaseResidualPct,
          leaseTaxDeductible,
        } = params;

        const nLeaseMonths = Math.round(leaseYears * 12);
        const nAnalysisMonths = Math.round(analysisYears * 12);
        const rDisc = discountRate / 100 / 12;
        const residual = cost * (leaseResidualPct / 100);

        let npv = 0;
        let totalCash = 0;
        let taxPV = 0;
        let residualPV = 0;
        const rows = [];

        for (let m = 1; m <= nAnalysisMonths; m++) {
          const t = m / 12;
          let leasePay = 0;
          if (m <= nLeaseMonths) {
            leasePay = leasePayment;
          }

          let taxShield = 0;
          if (leaseTaxDeductible) {
            taxShield = leasePay * (taxRate / 100);
          }

          let extraOut = 0;
          if (m === nLeaseMonths && residual > 0) {
            extraOut = residual;
          }

          const cashOut = leasePay + extraOut;
          const netCash = cashOut - taxShield;

          const df = Math.pow(1 + rDisc, m);
          const pv = netCash / df;
          npv += pv;
          totalCash += cashOut;
          taxPV += taxShield / df;
          if (extraOut > 0) {
            residualPV = extraOut / df;
          }

          rows.push({
            month: m,
            year: t,
            leasePay,
            taxShield,
            extraOut,
            netCash,
            pv,
          });
        }

        return {
          npv,
          rows,
          leasePayment,
          totalCash,
          taxPV,
          residualPV,
        };
      }

      function buildTables(buy, lease) {
        const maxMonths = Math.max(buy.rows.length, lease.rows.length);
        let html = "";

        html +=
          '<h4 class="font-semibold mb-2">Buy option cash flows (monthly)</h4>';
        html +=
          '<table class="min-w-full text-xs border mb-4"><thead><tr class="bg-gray-100">';
        html += '<th class="px-2 py-1 border">Month</th>';
        html += '<th class="px-2 py-1 border">Payment</th>';
        html += '<th class="px-2 py-1 border">Interest</th>';
        html += '<th class="px-2 py-1 border">Depreciation</th>';
        html += '<th class="px-2 py-1 border">Tax shield</th>';
        html += '<th class="px-2 py-1 border">Net cash</th>';
        html += '<th class="px-2 py-1 border">PV of cash</th>';
        html += "</tr></thead><tbody>";

        buy.rows.forEach((r) => {
          html += "<tr>";
          html += `<td class="px-2 py-1 border text-right">${r.month}</td>`;
          html += `<td class="px-2 py-1 border text-right">${fmtMoney2(
            r.loanPayment
          )}</td>`;
          html += `<td class="px-2 py-1 border text-right">${fmtMoney2(
            r.interest
          )}</td>`;
          html += `<td class="px-2 py-1 border text-right">${fmtMoney2(
            r.depreciation
          )}</td>`;
          html += `<td class="px-2 py-1 border text-right">${fmtMoney2(
            r.taxShield
          )}</td>`;
          html += `<td class="px-2 py-1 border text-right">${fmtMoney2(
            r.netCash
          )}</td>`;
          html += `<td class="px-2 py-1 border text-right">${fmtMoney2(
            r.pv
          )}</td>`;
          html += "</tr>";
        });
        html += "</tbody></table>";

        html +=
          '<h4 class="font-semibold mb-2">Lease option cash flows (monthly)</h4>';
        html +=
          '<table class="min-w-full text-xs border"><thead><tr class="bg-gray-100">';
        html += '<th class="px-2 py-1 border">Month</th>';
        html += '<th class="px-2 py-1 border">Lease payment</th>';
        html += '<th class="px-2 py-1 border">Tax shield</th>';
        html += '<th class="px-2 py-1 border">Buyout</th>';
        html += '<th class="px-2 py-1 border">Net cash</th>';
        html += '<th class="px-2 py-1 border">PV of cash</th>';
        html += "</tr></thead><tbody>";

        lease.rows.forEach((r) => {
          html += "<tr>";
          html += `<td class="px-2 py-1 border text-right">${r.month}</td>`;
          html += `<td class="px-2 py-1 border text-right">${fmtMoney2(
            r.leasePay
          )}</td>`;
          html += `<td class="px-2 py-1 border text-right">${fmtMoney2(
            r.taxShield
          )}</td>`;
          html += `<td class="px-2 py-1 border text-right">${fmtMoney2(
            r.extraOut
          )}</td>`;
          html += `<td class="px-2 py-1 border text-right">${fmtMoney2(
            r.netCash
          )}</td>`;
          html += `<td class="px-2 py-1 border text-right">${fmtMoney2(
            r.pv
          )}</td>`;
          html += "</tr>";
        });
        html += "</tbody></table>";

        return html;
      }

      function calculate() {
        const errorEl = document.getElementById("errorMsg");
        errorEl.textContent = "";

        const cost = toNumber("equipCost");
        const analysisYears = toNumber("analysisYears");
        const discountRate = toNumber("discountRate");
        const taxRate = toNumber("taxRate");

        const downPaymentPct = toNumber("downPaymentPct");
        const loanRate = toNumber("loanRate");
        const loanYears = toNumber("loanYears");
        const salvagePct = toNumber("salvagePct");
        const useDep = document.getElementById("useDepreciation").checked;

        const leasePayment = toNumber("leasePayment");
        const leaseYears = toNumber("leaseYears");
        const leaseResidualPct = toNumber("leaseResidualPct");
        const leaseTaxDeductible =
          document.getElementById("leaseTaxDeductible").checked;

        if (
          cost <= 0 ||
          analysisYears <= 0 ||
          discountRate < 0 ||
          taxRate < 0
        ) {
          errorEl.textContent =
            "Please enter valid equipment cost, analysis period, discount rate, and tax rate.";
          return;
        }
        if (loanYears <= 0 || leaseYears <= 0) {
          errorEl.textContent =
            "Please enter valid loan and lease terms in years.";
          return;
        }
        if (leasePayment < 0) {
          errorEl.textContent = "Lease payment cannot be negative.";
          return;
        }

        const buy = buildScheduleBuy({
          cost,
          downPct: downPaymentPct,
          loanRate,
          loanYears,
          analysisYears,
          taxRate,
          discountRate,
          salvagePct,
          useDep,
        });

        const lease = buildScheduleLease({
          cost,
          leasePayment,
          leaseYears,
          analysisYears,
          taxRate,
          discountRate,
          leaseResidualPct,
          leaseTaxDeductible,
        });

        const npvBuy = buy.npv;
        const npvLease = lease.npv;

        document.getElementById("npvBuy").textContent = fmtMoney2(npvBuy);
        document.getElementById("npvLease").textContent = fmtMoney2(npvLease);

        let winnerText = "";
        let savingsText = "";
        const winnerEl = document.getElementById("winnerLabel");
        const savingsEl = document.getElementById("winnerSavings");

        if (npvBuy < npvLease) {
          const diff = npvLease - npvBuy;
          winnerText = "Buying is cheaper";
          savingsText = `Buying saves about ${fmtMoney(
            diff
          )} in present-value terms over ${analysisYears} years.`;
          winnerEl.classList.remove("result-bad");
          winnerEl.classList.add("result-good");
        } else if (npvLease < npvBuy) {
          const diff = npvBuy - npvLease;
          winnerText = "Leasing is cheaper";
          savingsText = `Leasing saves about ${fmtMoney(
            diff
          )} in present-value terms over ${analysisYears} years.`;
          winnerEl.classList.remove("result-bad");
          winnerEl.classList.add("result-good");
        } else {
          winnerText = "Both options cost about the same";
          savingsText = "";
          winnerEl.classList.remove("result-good", "result-bad");
        }

        winnerEl.textContent = winnerText;
        savingsEl.textContent = savingsText;

        document.getElementById("buyUpfront").textContent = fmtMoney2(buy.down);
        document.getElementById("buyMonthly").textContent = fmtMoney2(
          buy.payment
        );
        document.getElementById("buyTotalCash").textContent = fmtMoney2(
          buy.totalCash
        );
        document.getElementById("buyTaxPV").textContent = fmtMoney2(buy.taxPV);
        document.getElementById("buyResidualPV").textContent = fmtMoney2(
          buy.residualPV
        );

        document.getElementById("leaseUpfront").textContent = fmtMoney2(0);
        document.getElementById("leaseMonthly").textContent = fmtMoney2(
          lease.leasePayment
        );
        document.getElementById("leaseTotalCash").textContent = fmtMoney2(
          lease.totalCash
        );
        document.getElementById("leaseTaxPV").textContent = fmtMoney2(
          lease.taxPV
        );
        document.getElementById("leaseResidualPV").textContent = fmtMoney2(
          lease.residualPV
        );

        document.getElementById("cashflowTables").innerHTML = buildTables(
          buy,
          lease
        );

        document.getElementById("resultsSection").classList.remove("hidden");
      }

      document.getElementById("calcBtn").addEventListener("click", calculate);

      document.getElementById("resetBtn").addEventListener("click", () => {
        document.getElementById("equipCost").value = 50000;
        document.getElementById("analysisYears").value = 5;
        document.getElementById("discountRate").value = 8;
        document.getElementById("taxRate").value = 25;
        document.getElementById("downPaymentPct").value = 20;
        document.getElementById("loanRate").value = 7;
        document.getElementById("loanYears").value = 5;
        document.getElementById("salvagePct").value = 20;
        document.getElementById("useDepreciation").checked = true;
        document.getElementById("leasePayment").value = 950;
        document.getElementById("leaseYears").value = 5;
        document.getElementById("leaseResidualPct").value = 0;
        document.getElementById("leaseTaxDeductible").checked = true;
        document.getElementById("errorMsg").textContent = "";
        document.getElementById("resultsSection").classList.add("hidden");
      });

      // Basic mobile menu toggle
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          this.setAttribute("aria-expanded", String(isHidden));
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
