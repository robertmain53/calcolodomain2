            <script>
              document.addEventListener('DOMContentLoaded', function() {
                const inputFunction = document.getElementById('input-function');
                const inputX0 = document.getElementById('input-x0');
                const calculateBtn = document.getElementById('calculate-btn');
                const resultsContainer = document.getElementById('results-container');
                const errorMessage = document.getElementById('error-message');
                
                const resultTangentEq = document.getElementById('result-tangent-eq');
                const resultSlope = document.getElementById('result-slope');
                const resultPoint = document.getElementById('result-point');
                const stepByStep = document.getElementById('step-by-step');

                // --- Helper Functions ---
                function formatNumber(num) {
                    if (Math.abs(num) < 1e-10) return '0';
                    return parseFloat(num.toFixed(6)).toString();
                }

                function showError(message) {
                    errorMessage.textContent = `Error: ${message}`;
                    resultsContainer.classList.add('hidden');
                }
                
                // --- Core Calculation Logic using Math.js ---
                calculateBtn.addEventListener('click', () => {
                    const funcStr = inputFunction.value.trim();
                    const x0 = parseFloat(inputX0.value);
                    
                    try {
                        if (funcStr === '' || isNaN(x0)) {
                            throw new Error('Please enter a valid function $f(x)$ and a valid point $x_0$.');
                        }
                        
                        errorMessage.textContent = '';

                        // 1. Parse and Differentiate
                        const f = math.parse(funcStr);
                        const fPrime = math.derivative(f, 'x');
                        
                        // 2. Find Point of Tangency (y0 = f(x0))
                        const scopeY = { x: x0 };
                        const y0 = f.evaluate(scopeY);
                        
                        // 3. Find Slope (m = f'(x0))
                        const scopeM = { x: x0 };
                        const m = fPrime.evaluate(scopeM);
                        
                        if (isNaN(y0) || isNaN(m)) {
                             throw new Error('The function or its derivative cannot be evaluated at the given point. Check for domain issues (e.g., log(0), 1/0).');
                        }

                        // 4. Construct Tangent Line Equation (y = mx + b)
                        const b = y0 - m * x0; // Solve for y-intercept
                        
                        // Format final equation components
                        const fm = formatNumber(m);
                        const fb = formatNumber(b);
                        const fX0 = formatNumber(x0);
                        const fY0 = formatNumber(y0);
                        
                        const b_sign = (b >= 0) ? '+' : '-';
                        const tangentEq = `y = ${fm}x ${b_sign} ${formatNumber(Math.abs(b))}`;
                        
                        // 5. Step-by-Step Construction
                        const fPrimeStr = fPrime.toString();
                        
                        let steps = `
                            <p>1. <strong>Find the Derivative $f'(x)$:</strong> The derivative of $f(x) = ${funcStr}$ is:</p>
                            <p class="formula-box text-center">$f'(x) = ${fPrimeStr}$</p>

                            <p>2. <strong>Find the Slope ($m$):</strong> Substitute $x_0 = ${fX0}$ into the derivative:</p>
                            <p class="formula-box text-center">$m = f'(${fX0}) = ${fm}$</p>
                            
                            <p>3. <strong>Find the Point ($y_0$):</strong> Substitute $x_0 = ${fX0}$ into the original function:</p>
                            <p class="formula-box text-center">$y_0 = f(${fX0}) = ${fY0}$. Point of tangency is $(${fX0}, ${fY0})$</p>
                            
                            <p>4. <strong>Use Point-Slope Form:</strong> $y - y_0 = m(x - x_0)$</p>
                            <p class="formula-box text-center">$y - ${fY0} = ${fm}(x - ${fX0})$</p>
                            
                            <p>5. <strong>Solve for $y = mx + b$:</strong> Rearrange the equation to find the $y$-intercept $b$.</p>
                            <p class="formula-box text-center">$y = ${fm}x ${b_sign} ${formatNumber(Math.abs(b))}$</p>
                        `;

                        // 6. Update DOM
                        resultTangentEq.textContent = tangentEq;
                        resultSlope.textContent = fm;
                        resultPoint.textContent = `(${fX0}, ${fY0})`;
                        stepByStep.innerHTML = steps;
                        resultsContainer.classList.remove('hidden');

                        // Re-render MathJax
                        if (window.MathJax) {
                            window.MathJax.typesetPromise([resultTangentEq, stepByStep]);
                        }

                    } catch (e) {
                        showError(e.message);
                    }
                });

                // Initial calculation (for example values)
                calculateBtn.click();
            </script>

