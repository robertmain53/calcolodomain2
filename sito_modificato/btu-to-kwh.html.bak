            <script>
              document.addEventListener('DOMContentLoaded', () => {

                // --- FATTORI DI CONVERSIONE ---
                const BTU_TO_KWH = 0.000293071039;
                const KWH_TO_BTU = 3412.141633;
                const BTUHR_TO_KW = 0.000293071039;

                // --- LOGICA STRUMENTO 1: CONVERTITORE DI ENERGIA ---
                const btuInput = document.getElementById('btu-input');
                const kwhInput = document.getElementById('kwh-input');

                function initSimpleConverter() {
                    btuInput.addEventListener('input', () => {
                        const btu = parseFloat(btuInput.value);
                        if (isNaN(btu) || btu === 0) {
                            kwhInput.value = '';
                            return;
                        }
                        kwhInput.value = (btu * BTU_TO_KWH).toFixed(6);
                    });

                    kwhInput.addEventListener('input', () => {
                        const kwh = parseFloat(kwhInput.value);
                        if (isNaN(kwh) || kwh === 0) {
                            btuInput.value = '';
                            return;
                        }
                        btuInput.value = (kwh * KWH_TO_BTU).toFixed(2);
                    });
                }

                // --- LOGICA STRUMENTO 2: CALCOLATORE DI COSTO ---
                const btuHrInput = document.getElementById('btu-hr-input');
                const hoursInput = document.getElementById('hours-input');
                const costInput = document.getElementById('cost-input');
                const costOutput = document.getElementById('cost-output');
                const kwhDayOutput = document.getElementById('kwh-day-output');

                function calculateCost() {
                    const btuHr = parseFloat(btuHrInput.value) || 0;
                    const hours = parseFloat(hoursInput.value) || 0;
                    const costPerKwh = parseFloat(costInput.value) || 0;

                    if (btuHr === 0 || hours === 0) {
                        costOutput.textContent = "$0.00";
                        kwhDayOutput.innerHTML = "Based on <strong>0 kWh</strong> of energy used per day.";
                        return;
                    }
                    
                    const kw = btuHr * BTUHR_TO_KW;
                    const kwhPerDay = kw * hours;
                    const costPerDay = kwhPerDay * costPerKwh;

                    // Format the currency
                    const costFormatted = new Intl.NumberFormat('en-US', {
                        style: 'currency',
                        currency: 'USD' // Puoi cambiarlo in EUR se preferisci
                    }).format(costPerDay);

                    costOutput.textContent = costFormatted;
                    kwhDayOutput.innerHTML = `Based on <strong>${kwhPerDay.toFixed(2)} kWh</strong> of energy used per day.`;
                }
                
                function initCostConverter() {
                    [btuHrInput, hoursInput, costInput].forEach(input => {
                        input.addEventListener('input', calculateCost);
                    });
                    // Calculate on load
                    calculateCost();
                }

                // Inizializza entrambi i calcolatori
                initSimpleConverter();
                initCostConverter();
              });
            </script>
            
