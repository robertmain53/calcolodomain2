    <script>
      // Tab handling
      const tabs = {
        genotype: {
          tab: document.getElementById("tab-genotype"),
          panel: document.getElementById("panel-genotype"),
        },
        allele: {
          tab: document.getElementById("tab-allele"),
          panel: document.getElementById("panel-allele"),
        },
        vaf: {
          tab: document.getElementById("tab-vaf"),
          panel: document.getElementById("panel-vaf"),
        },
      };

      function activateTab(name) {
        Object.keys(tabs).forEach((key) => {
          const { tab, panel } = tabs[key];
          if (key === name) {
            tab.classList.add("tab-active");
            tab.classList.remove("text-gray-600");
            panel.classList.remove("hidden");
            tab.setAttribute("aria-selected", "true");
          } else {
            tab.classList.remove("tab-active");
            tab.classList.add("text-gray-600");
            panel.classList.add("hidden");
            tab.setAttribute("aria-selected", "false");
          }
        });
        clearError();
      }

      tabs.genotype.tab.addEventListener("click", () =>
        activateTab("genotype")
      );
      tabs.allele.tab.addEventListener("click", () => activateTab("allele"));
      tabs.vaf.tab.addEventListener("click", () => activateTab("vaf"));

      // Error handling
      const errorBox = document.getElementById("error-box");
      const errorMessage = document.getElementById("error-message");

      function showError(msg) {
        errorMessage.textContent = msg;
        errorBox.classList.remove("hidden");
      }

      function clearError() {
        errorBox.classList.add("hidden");
        errorMessage.textContent = "";
      }

      // Utility
      function toNumber(value) {
        const n = Number(value);
        return Number.isFinite(n) ? n : NaN;
      }

      function round(value, decimals) {
        const factor = Math.pow(10, decimals);
        return Math.round(value * factor) / factor;
      }

      // Genotype calculations
      const nAAInput = document.getElementById("nAA");
      const nAaInput = document.getElementById("nAa");
      const naaInput = document.getElementById("naa");
      const btnCalcGenotype = document.getElementById("btn-calc-genotype");
      const btnResetGenotype = document.getElementById("btn-reset-genotype");
      const chkHweTest = document.getElementById("chk-hwe-test");
      const genotypeResults = document.getElementById("genotype-results");
      const genotypeAlleleSummary = document.getElementById(
        "genotype-allele-summary"
      );
      const genotypeGenotypeSummary = document.getElementById(
        "genotype-genotype-summary"
      );
      const hweSection = document.getElementById("hwe-section");
      const hweFreq = document.getElementById("hwe-freq");
      const hweCounts = document.getElementById("hwe-counts");
      const hweChi2 = document.getElementById("hwe-chi2");
      const hweInterpretation = document.getElementById("hwe-interpretation");

      btnCalcGenotype.addEventListener("click", () => {
        clearError();
        const nAA = toNumber(nAAInput.value);
        const nAa = toNumber(nAaInput.value);
        const naa = toNumber(naaInput.value);

        if ([nAA, nAa, naa].some((v) => isNaN(v) || v < 0)) {
          showError("Genotype counts must be non-negative numbers.");
          return;
        }

        const N = nAA + nAa + naa;
        if (N === 0) {
          showError("Total number of individuals cannot be zero.");
          return;
        }

        const totalAlleles = 2 * N;
        const Acount = 2 * nAA + nAa;
        const acount = 2 * naa + nAa;

        const p = Acount / totalAlleles;
        const q = acount / totalAlleles;

        genotypeAlleleSummary.innerHTML = `
      <p>Total individuals (N): <strong>${N}</strong></p>
      <p>Total alleles (2N): <strong>${totalAlleles}</strong></p>
      <p>A alleles: <strong>${Acount}</strong> &nbsp;→&nbsp; p = ${round(
          p,
          4
        )}</p>
      <p>a alleles: <strong>${acount}</strong> &nbsp;→&nbsp; q = ${round(
          q,
          4
        )}</p>
      <p>Check: p + q = ${round(p + q, 4)}</p>
    `;

        const fAA = nAA / N;
        const fAa = nAa / N;
        const faa = naa / N;

        genotypeGenotypeSummary.innerHTML = `
      <p>f(AA) = n<sub>AA</sub> / N = ${nAA} / ${N} = <strong>${round(
          fAA,
          4
        )}</strong></p>
      <p>f(Aa) = n<sub>Aa</sub> / N = ${nAa} / ${N} = <strong>${round(
          fAa,
          4
        )}</strong></p>
      <p>f(aa) = n<sub>aa</sub> / N = ${naa} / ${N} = <strong>${round(
          faa,
          4
        )}</strong></p>
    `;

        genotypeResults.classList.remove("hidden");

        if (chkHweTest.checked) {
          const expAA = p * p * N;
          const expAa = 2 * p * q * N;
          const expaa = q * q * N;

          hweFreq.innerHTML = `
        Expected genotype frequencies under HWE:<br>
        f(AA) = p² = ${round(p * p, 4)}, &nbsp;
        f(Aa) = 2pq = ${round(2 * p * q, 4)}, &nbsp;
        f(aa) = q² = ${round(q * q, 4)}
      `;
          hweCounts.innerHTML = `
        Expected counts (N = ${N}):<br>
        E[AA] = ${round(expAA, 3)}, &nbsp;
        E[Aa] = ${round(expAa, 3)}, &nbsp;
        E[aa] = ${round(expaa, 3)}
      `;

          // Chi-square
          function chiTerm(obs, exp) {
            return exp > 0 ? Math.pow(obs - exp, 2) / exp : 0;
          }
          const chi2 =
            chiTerm(nAA, expAA) + chiTerm(nAa, expAa) + chiTerm(naa, expaa);
          hweChi2.innerHTML = `Chi-square statistic: <strong>χ² = ${round(
            chi2,
            4
          )}</strong> (df ≈ 1)`;

          const critical = 3.841; // p ~ 0.05, df=1
          if (chi2 > critical) {
            hweInterpretation.textContent =
              "Interpretation: χ² exceeds 3.84 (p < 0.05). The sample shows a statistically significant deviation from Hardy–Weinberg expectations.";
          } else {
            hweInterpretation.textContent =
              "Interpretation: χ² does not exceed 3.84 (p ≥ 0.05). There is no strong evidence of deviation from Hardy–Weinberg equilibrium.";
          }

          hweSection.classList.remove("hidden");
        } else {
          hweSection.classList.add("hidden");
        }
      });

      btnResetGenotype.addEventListener("click", () => {
        nAAInput.value = 30;
        nAaInput.value = 40;
        naaInput.value = 30;
        chkHweTest.checked = false;
        genotypeResults.classList.add("hidden");
        hweSection.classList.add("hidden");
        clearError();
      });

      // Allele count calculations
      const countAInput = document.getElementById("countA");
      const countaInput = document.getElementById("counta");
      const btnCalcAllele = document.getElementById("btn-calc-allele");
      const btnResetAllele = document.getElementById("btn-reset-allele");
      const alleleResults = document.getElementById("allele-results");
      const alleleSummary = document.getElementById("allele-summary");

      btnCalcAllele.addEventListener("click", () => {
        clearError();
        const nA = toNumber(countAInput.value);
        const na = toNumber(countaInput.value);

        if ([nA, na].some((v) => isNaN(v) || v < 0)) {
          showError("Allele counts must be non-negative numbers.");
          return;
        }

        const total = nA + na;
        if (total === 0) {
          showError("Total allele count cannot be zero.");
          return;
        }

        const p = nA / total;
        const q = na / total;

        alleleSummary.innerHTML = `
      <p>Total alleles: <strong>${total}</strong></p>
      <p>A alleles: <strong>${nA}</strong> &nbsp;→&nbsp; p = ${round(p, 4)}</p>
      <p>a alleles: <strong>${na}</strong> &nbsp;→&nbsp; q = ${round(q, 4)}</p>
      <p>Check: p + q = ${round(p + q, 4)}</p>
    `;
        alleleResults.classList.remove("hidden");
      });

      btnResetAllele.addEventListener("click", () => {
        countAInput.value = 100;
        countaInput.value = 100;
        alleleResults.classList.add("hidden");
        clearError();
      });

      // VAF calculations
      const readsRefInput = document.getElementById("readsRef");
      const readsAltInput = document.getElementById("readsAlt");
      const vafDecimalsInput = document.getElementById("vafDecimals");
      const btnCalcVaf = document.getElementById("btn-calc-vaf");
      const btnResetVaf = document.getElementById("btn-reset-vaf");
      const vafResults = document.getElementById("vaf-results");
      const vafSummary = document.getElementById("vaf-summary");

      btnCalcVaf.addEventListener("click", () => {
        clearError();
        const ref = toNumber(readsRefInput.value);
        const alt = toNumber(readsAltInput.value);
        let decimals = toNumber(vafDecimalsInput.value);

        if ([ref, alt].some((v) => isNaN(v) || v < 0)) {
          showError("Read counts must be non-negative numbers.");
          return;
        }

        if (isNaN(decimals) || decimals < 0) decimals = 3;
        if (decimals > 6) decimals = 6;

        const total = ref + alt;
        if (total === 0) {
          showError("Total read count cannot be zero.");
          return;
        }

        const vaf = alt / total;
        const vafPct = vaf * 100;

        vafSummary.innerHTML = `
      <p>Total reads: <strong>${total}</strong></p>
      <p>Variant reads: <strong>${alt}</strong></p>
      <p>VAF = variant / total = ${alt} / ${total} = <strong>${vaf.toFixed(
          decimals
        )}</strong> (${vafPct.toFixed(decimals)}%)</p>
    `;
        vafResults.classList.remove("hidden");
      });

      btnResetVaf.addEventListener("click", () => {
        readsRefInput.value = 60;
        readsAltInput.value = 40;
        vafDecimalsInput.value = 3;
        vafResults.classList.add("hidden");
        clearError();
      });

      // Initialize default tab
      activateTab("genotype");

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
