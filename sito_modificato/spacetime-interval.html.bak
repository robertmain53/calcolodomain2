    <script>
      (function () {
        const modeNaturalBtn = document.getElementById("mode-natural");
        const modeSiBtn = document.getElementById("mode-si");
        const metricSignature = document.getElementById("metric-signature");

        const t1 = document.getElementById("t1");
        const x1 = document.getElementById("x1");
        const y1 = document.getElementById("y1");
        const z1 = document.getElementById("z1");
        const t2 = document.getElementById("t2");
        const x2 = document.getElementById("x2");
        const y2 = document.getElementById("y2");
        const z2 = document.getElementById("z2");
        const vInput = document.getElementById("v");
        const cInput = document.getElementById("c");

        const labels = {
          t1: document.getElementById("label-t1-unit"),
          t2: document.getElementById("label-t2-unit"),
          x1: document.getElementById("label-x1-unit"),
          y1: document.getElementById("label-y1-unit"),
          z1: document.getElementById("label-z1-unit"),
          x2: document.getElementById("label-x2-unit"),
          y2: document.getElementById("label-y2-unit"),
          z2: document.getElementById("label-z2-unit"),
          v: document.getElementById("label-v-unit"),
          c: document.getElementById("label-c-unit"),
        };

        const btnCalc = document.getElementById("btn-calc");
        const btnReset = document.getElementById("btn-reset");
        const btnBoost = document.getElementById("btn-boost");
        const boostWarning = document.getElementById("boost-warning");
        const calcError = document.getElementById("calc-error");

        const resultsContainer = document.getElementById("results-container");
        const resDt = document.getElementById("res-dt");
        const resDx = document.getElementById("res-dx");
        const resDy = document.getElementById("res-dy");
        const resDz = document.getElementById("res-dz");
        const resCdt = document.getElementById("res-cdt");
        const resS2 = document.getElementById("res-s2");
        const resSabs = document.getElementById("res-sabs");
        const resType = document.getElementById("res-type");
        const resInterpretation = document.getElementById("res-interpretation");

        const resDtUnit = document.getElementById("res-dt-unit");
        const resDxUnit = document.getElementById("res-dx-unit");
        const resDyUnit = document.getElementById("res-dy-unit");
        const resDzUnit = document.getElementById("res-dz-unit");
        const resCdtUnit = document.getElementById("res-cdt-unit");
        const resS2Unit = document.getElementById("res-s2-unit");
        const resSabsUnit = document.getElementById("res-sabs-unit");

        let mode = "natural"; // 'natural' or 'si'

        function setMode(newMode) {
          mode = newMode;
          if (mode === "natural") {
            modeNaturalBtn.classList.add(
              "bg-blue-600",
              "text-white",
              "font-medium"
            );
            modeSiBtn.classList.remove(
              "bg-blue-600",
              "text-white",
              "font-medium"
            );
            labels.t1.textContent = "(time units)";
            labels.t2.textContent = "(time units)";
            labels.x1.textContent = "(space units)";
            labels.y1.textContent = "(space units)";
            labels.z1.textContent = "(space units)";
            labels.x2.textContent = "(space units)";
            labels.y2.textContent = "(space units)";
            labels.z2.textContent = "(space units)";
            labels.v.textContent = "(fraction of c)";
            labels.c.textContent = "(dimensionless, c=1)";
            cInput.value = 1;
            metricSignature.textContent =
              "Metric signature: (+, −, −, −), natural units c = 1";
          } else {
            modeSiBtn.classList.add("bg-blue-600", "text-white", "font-medium");
            modeNaturalBtn.classList.remove(
              "bg-blue-600",
              "text-white",
              "font-medium"
            );
            labels.t1.textContent = "(s)";
            labels.t2.textContent = "(s)";
            labels.x1.textContent = "(m)";
            labels.y1.textContent = "(m)";
            labels.z1.textContent = "(m)";
            labels.x2.textContent = "(m)";
            labels.y2.textContent = "(m)";
            labels.z2.textContent = "(m)";
            labels.v.textContent = "(m/s)";
            labels.c.textContent = "(m/s)";
            cInput.value = 299792458;
            metricSignature.textContent =
              "Metric signature: (+, −, −, −), SI units";
          }
          resultsContainer.classList.add("hidden");
          calcError.classList.add("hidden");
        }

        modeNaturalBtn.addEventListener("click", () => setMode("natural"));
        modeSiBtn.addEventListener("click", () => setMode("si"));

        function parse(val) {
          const n = parseFloat(val);
          return isNaN(n) ? null : n;
        }

        function classifyInterval(s2, eps = 1e-12) {
          if (Math.abs(s2) < eps) return "lightlike";
          return s2 > 0 ? "timelike" : "spacelike";
        }

        function formatNumber(n) {
          if (!isFinite(n)) return "NaN";
          const abs = Math.abs(n);
          if (abs === 0) return "0";
          if (abs >= 1e4 || abs < 1e-3) return n.toExponential(4);
          return n.toFixed(6).replace(/\.?0+$/, "");
        }

        function computeInterval() {
          calcError.classList.add("hidden");
          boostWarning.classList.add("hidden");

          const t1v = parse(t1.value);
          const t2v = parse(t2.value);
          const x1v = parse(x1.value);
          const x2v = parse(x2.value);
          const y1v = parse(y1.value);
          const y2v = parse(y2.value);
          const z1v = parse(z1.value);
          const z2v = parse(z2.value);
          const cv = parse(cInput.value);

          if (
            [t1v, t2v, x1v, x2v, y1v, y2v, z1v, z2v, cv].some((v) => v === null)
          ) {
            calcError.textContent =
              "Please enter valid numeric values for all fields.";
            calcError.classList.remove("hidden");
            resultsContainer.classList.add("hidden");
            return;
          }
          if (cv <= 0) {
            calcError.textContent = "Speed of light c must be positive.";
            calcError.classList.remove("hidden");
            resultsContainer.classList.add("hidden");
            return;
          }

          const dt = t2v - t1v;
          const dx = x2v - x1v;
          const dy = y2v - y1v;
          const dz = z2v - z1v;

          let cdt, s2;
          if (mode === "natural") {
            cdt = dt; // c = 1
            s2 = dt * dt - dx * dx - dy * dy - dz * dz;
          } else {
            cdt = cv * dt;
            s2 = cv * dt * (cv * dt) - dx * dx - dy * dy - dz * dz;
          }

          const intervalType = classifyInterval(s2);
          const sabs = Math.sqrt(Math.abs(s2));

          resDt.textContent = formatNumber(dt);
          resDx.textContent = formatNumber(dx);
          resDy.textContent = formatNumber(dy);
          resDz.textContent = formatNumber(dz);
          resCdt.textContent = formatNumber(cdt);
          resS2.textContent = formatNumber(s2);
          resSabs.textContent = formatNumber(sabs);

          if (mode === "natural") {
            resDtUnit.textContent = " (time units)";
            resDxUnit.textContent = " (space units)";
            resDyUnit.textContent = " (space units)";
            resDzUnit.textContent = " (space units)";
            resCdtUnit.textContent = " (space units)";
            resS2Unit.textContent = " (space units)²";
            resSabsUnit.textContent = " (space units)";
          } else {
            resDtUnit.textContent = " s";
            resDxUnit.textContent = " m";
            resDyUnit.textContent = " m";
            resDzUnit.textContent = " m";
            resCdtUnit.textContent = " m";
            resS2Unit.textContent = " m²";
            resSabsUnit.textContent = " m";
          }

          if (intervalType === "timelike") {
            resType.textContent = "Timelike";
            resType.className = "badge bg-green-100 text-green-800";
            resInterpretation.textContent =
              "A causal influence is possible; there exists a frame where the events occur at the same spatial point but different times.";
          } else if (intervalType === "spacelike") {
            resType.textContent = "Spacelike";
            resType.className = "badge bg-red-100 text-red-800";
            resInterpretation.textContent =
              "No causal influence is possible; there exists a frame where the events are simultaneous but separated in space.";
          } else {
            resType.textContent = "Lightlike (null)";
            resType.className = "badge bg-yellow-100 text-yellow-800";
            resInterpretation.textContent =
              "The events can be connected by a light signal; they lie on each other’s light cone.";
          }

          resultsContainer.classList.remove("hidden");
        }

        function applyBoost() {
          boostWarning.classList.add("hidden");
          const vVal = parse(vInput.value);
          const cv = parse(cInput.value);

          if (vVal === null || cv === null) {
            boostWarning.textContent = "Enter valid values for v and c.";
            boostWarning.classList.remove("hidden");
            return;
          }

          let beta;
          if (mode === "natural") {
            beta = vVal; // v is fraction of c
          } else {
            beta = vVal / cv;
          }

          if (!isFinite(beta) || Math.abs(beta) >= 1) {
            boostWarning.textContent = "|v| must be less than c.";
            boostWarning.classList.remove("hidden");
            return;
          }

          const gamma = 1 / Math.sqrt(1 - beta * beta);

          const t1v = parse(t1.value);
          const t2v = parse(t2.value);
          const x1v = parse(x1.value);
          const x2v = parse(x2.value);
          const y1v = parse(y1.value);
          const y2v = parse(y2.value);
          const z1v = parse(z1.value);
          const z2v = parse(z2.value);

          if (
            [t1v, t2v, x1v, x2v, y1v, y2v, z1v, z2v].some((v) => v === null)
          ) {
            boostWarning.textContent =
              "Please enter valid numeric event coordinates before applying a boost.";
            boostWarning.classList.remove("hidden");
            return;
          }

          function transform(t, x) {
            if (mode === "natural") {
              const tPrime = gamma * (t - beta * x);
              const xPrime = gamma * (x - beta * t);
              return { t: tPrime, x: xPrime };
            } else {
              const v = vVal;
              const tPrime = gamma * (t - (v * x) / (cv * cv));
              const xPrime = gamma * (x - v * t);
              return { t: tPrime, x: xPrime };
            }
          }

          const e1 = transform(t1v, x1v);
          const e2 = transform(t2v, x2v);

          t1.value = e1.t;
          x1.value = e1.x;
          t2.value = e2.t;
          x2.value = e2.x;

          // y and z unchanged
          y1.value = y1v;
          y2.value = y2v;
          z1.value = z1v;
          z2.value = z2v;

          computeInterval();
        }

        function resetForm() {
          t1.value = 0;
          x1.value = 0;
          y1.value = 0;
          z1.value = 0;
          t2.value = 1;
          x2.value = 0;
          y2.value = 0;
          z2.value = 0;
          vInput.value = 0;
          cInput.value = mode === "natural" ? 1 : 299792458;
          resultsContainer.classList.add("hidden");
          calcError.classList.add("hidden");
          boostWarning.classList.add("hidden");
        }

        btnCalc.addEventListener("click", computeInterval);
        btnBoost.addEventListener("click", applyBoost);
        btnReset.addEventListener("click", resetForm);

        // Initialize
        setMode("natural");
      })();
    </script>
  
