    <script>
      // Utility helpers
      function parseNum(id) {
        const v = document.getElementById(id).value;
        const n = parseFloat(v);
        return isNaN(n) ? null : n;
      }

      function showError(msg) {
        const el = document.getElementById("error-msg");
        el.textContent = msg;
        el.classList.remove("hidden");
      }

      function clearError() {
        const el = document.getElementById("error-msg");
        el.textContent = "";
        el.classList.add("hidden");
      }

      function setResults(mlhr, gttmin, contextText) {
        const resBox = document.getElementById("results");
        const stepsBox = document.getElementById("steps");
        const resMl = document.getElementById("res-mlhr");
        const resGtt = document.getElementById("res-gttmin");
        const resGttRounded = document.getElementById("res-gttmin-rounded");
        const resExtra = document.getElementById("res-extra");

        if (mlhr != null) {
          resMl.textContent = mlhr.toFixed(2) + " mL/hr";
        } else {
          resMl.textContent = "– mL/hr";
        }

        if (gttmin != null) {
          resGtt.textContent = gttmin.toFixed(2) + " gtt/min";
          resGttRounded.textContent =
            "Rounded to nearest whole drop: " + Math.round(gttmin) + " gtt/min";
        } else {
          resGtt.textContent = "– gtt/min";
          resGttRounded.textContent = "Rounded to nearest whole drop: –";
        }

        resExtra.innerHTML = contextText || "";
        resBox.classList.remove("hidden");
        stepsBox.classList.remove("hidden");
      }

      function clearAll() {
        document.getElementById("iv-form").reset();
        document.getElementById("results").classList.add("hidden");
        document.getElementById("steps").classList.add("hidden");
        document.getElementById("steps-content").innerHTML = "";
        clearError();
        // Reset defaults
        document.getElementById("dropFactor").value = "15";
        document.getElementById("dropFactorW").value = "15";
        document.getElementById("weightUnit").value = "kg";
        document.getElementById("drugAmountUnit").value = "mg";
        document.getElementById("weightOutput").value = "both";
      }

      // Mode switching
      const tabSimple = document.getElementById("tab-simple");
      const tabWeight = document.getElementById("tab-weight");
      const sectionSimple = document.getElementById("section-simple");
      const sectionWeight = document.getElementById("section-weight");

      function setMode(mode) {
        if (mode === "simple") {
          sectionSimple.classList.remove("hidden");
          sectionWeight.classList.add("hidden");
          tabSimple.classList.add("bg-blue-600", "text-white");
          tabSimple.classList.remove("bg-white", "text-gray-700");
          tabWeight.classList.remove("bg-blue-600", "text-white");
          tabWeight.classList.add("bg-white", "text-gray-700");
          tabSimple.setAttribute("aria-selected", "true");
          tabWeight.setAttribute("aria-selected", "false");
        } else {
          sectionSimple.classList.add("hidden");
          sectionWeight.classList.remove("hidden");
          tabWeight.classList.add("bg-blue-600", "text-white");
          tabWeight.classList.remove("bg-white", "text-gray-700");
          tabSimple.classList.remove("bg-blue-600", "text-white");
          tabSimple.classList.add("bg-white", "text-gray-700");
          tabWeight.setAttribute("aria-selected", "true");
          tabSimple.setAttribute("aria-selected", "false");
        }
        clearError();
        document.getElementById("results").classList.add("hidden");
        document.getElementById("steps").classList.add("hidden");
      }

      tabSimple.addEventListener("click", () => setMode("simple"));
      tabWeight.addEventListener("click", () => setMode("weight"));

      // Known value toggle (simple mode)
      let knownType = "mlhr";
      const btnKnownMlhr = document.getElementById("known-mlhr");
      const btnKnownGtt = document.getElementById("known-gttmin");

      function updateKnownButtons(type) {
        knownType = type;
        if (type === "mlhr") {
          btnKnownMlhr.classList.remove(
            "border-gray-300",
            "bg-white",
            "text-gray-700"
          );
          btnKnownMlhr.classList.add(
            "border-blue-600",
            "bg-blue-50",
            "text-blue-700"
          );
          btnKnownGtt.classList.remove(
            "border-blue-600",
            "bg-blue-50",
            "text-blue-700"
          );
          btnKnownGtt.classList.add(
            "border-gray-300",
            "bg-white",
            "text-gray-700"
          );
        } else {
          btnKnownGtt.classList.remove(
            "border-gray-300",
            "bg-white",
            "text-gray-700"
          );
          btnKnownGtt.classList.add(
            "border-blue-600",
            "bg-blue-50",
            "text-blue-700"
          );
          btnKnownMlhr.classList.remove(
            "border-blue-600",
            "bg-blue-50",
            "text-blue-700"
          );
          btnKnownMlhr.classList.add(
            "border-gray-300",
            "bg-white",
            "text-gray-700"
          );
        }
      }

      btnKnownMlhr.addEventListener("click", () => updateKnownButtons("mlhr"));
      btnKnownGtt.addEventListener("click", () => updateKnownButtons("gttmin"));

      // Main calculate handler
      document.getElementById("btn-calc").addEventListener("click", () => {
        clearError();
        const mode = sectionSimple.classList.contains("hidden")
          ? "weight"
          : "simple";
        const steps = [];
        let mlhr = null;
        let gttmin = null;

        if (mode === "simple") {
          const volume = parseNum("volumeMl");
          const timeH = parseNum("timeHours");
          const df = parseNum("dropFactor");
          const mlhrInput = parseNum("rateMlHr");
          const gttInput = parseNum("rateGttMin");

          if (!df || df <= 0) {
            showError("Please enter a valid drop factor.");
            return;
          }

          // Case 1: volume & time known -> compute mL/hr and gtt/min
          if (volume != null && volume > 0 && timeH != null && timeH > 0) {
            const timeMin = timeH * 60;
            mlhr = volume / timeH;
            gttmin = (volume * df) / timeMin;

            steps.push(
              `Volume = ${volume.toFixed(2)} mL, Time = ${timeH.toFixed(
                2
              )} h (${timeMin.toFixed(2)} min), Drop factor = ${df.toFixed(
                0
              )} gtt/mL.`
            );
            steps.push(
              `mL/hr = Volume ÷ Time = ${volume.toFixed(2)} ÷ ${timeH.toFixed(
                2
              )} = ${mlhr.toFixed(2)} mL/hr.`
            );
            steps.push(
              `gtt/min = (Volume × Drop factor) ÷ Time(min) = (${volume.toFixed(
                2
              )} × ${df.toFixed(0)}) ÷ ${timeMin.toFixed(2)} = ${gttmin.toFixed(
                2
              )} gtt/min.`
            );
          }
          // Case 2: mL/hr known -> compute gtt/min
          else if (mlhrInput != null && mlhrInput > 0) {
            mlhr = mlhrInput;
            gttmin = (mlhr * df) / 60;
            steps.push(
              `mL/hr is given as ${mlhr.toFixed(
                2
              )} mL/hr, Drop factor = ${df.toFixed(0)} gtt/mL.`
            );
            steps.push(
              `gtt/min = (mL/hr × Drop factor) ÷ 60 = (${mlhr.toFixed(
                2
              )} × ${df.toFixed(0)}) ÷ 60 = ${gttmin.toFixed(2)} gtt/min.`
            );
          }
          // Case 3: gtt/min known -> compute mL/hr
          else if (gttInput != null && gttInput > 0) {
            gttmin = gttInput;
            mlhr = (gttmin * 60) / df;
            steps.push(
              `gtt/min is given as ${gttmin.toFixed(
                2
              )} gtt/min, Drop factor = ${df.toFixed(0)} gtt/mL.`
            );
            steps.push(
              `mL/hr = (gtt/min × 60) ÷ Drop factor = (${gttmin.toFixed(
                2
              )} × 60) ÷ ${df.toFixed(0)} = ${mlhr.toFixed(2)} mL/hr.`
            );
          } else {
            showError(
              "Enter either volume & time, or a known mL/hr or gtt/min value."
            );
            return;
          }

          const ctx =
            "<p>This calculation assumes a constant drip rate with a drop factor of <strong>" +
            df.toFixed(0) +
            " gtt/mL</strong>.</p>";
          setResults(mlhr, gttmin, ctx);
        } else {
          // Weight-based mode
          const weight = parseNum("weight");
          const weightUnit = document.getElementById("weightUnit").value;
          const dose = parseNum("dose");
          const drugAmt = parseNum("drugAmount");
          const drugUnit = document.getElementById("drugAmountUnit").value;
          const bagVol = parseNum("bagVolume");
          const df = parseNum("dropFactorW");
          const outPref = document.getElementById("weightOutput").value;

          if (
            !weight ||
            weight <= 0 ||
            !dose ||
            dose <= 0 ||
            !drugAmt ||
            drugAmt <= 0 ||
            !bagVol ||
            bagVol <= 0 ||
            !df ||
            df <= 0
          ) {
            showError(
              "Please enter weight, dose, drug amount, bag volume, and drop factor."
            );
            return;
          }

          let weightKg = weight;
          if (weightUnit === "lb") {
            weightKg = weight * 0.45359237;
            steps.push(
              `Weight = ${weight.toFixed(2)} lb = ${weightKg.toFixed(2)} kg.`
            );
          } else {
            steps.push(`Weight = ${weightKg.toFixed(2)} kg.`);
          }

          let totalMcg = drugAmt;
          if (drugUnit === "mg") {
            totalMcg = drugAmt * 1000;
            steps.push(
              `Drug in bag = ${drugAmt.toFixed(2)} mg = ${totalMcg.toFixed(
                2
              )} mcg.`
            );
          } else {
            steps.push(`Drug in bag = ${totalMcg.toFixed(2)} mcg.`);
          }

          const conc = totalMcg / bagVol; // mcg/mL
          steps.push(
            `Bag volume = ${bagVol.toFixed(
              2
            )} mL, so concentration = Total drug ÷ Volume = ${totalMcg.toFixed(
              2
            )} ÷ ${bagVol.toFixed(2)} = ${conc.toFixed(2)} mcg/mL.`
          );

          // mL/hr = (dose * weightKg * 60) / conc
          mlhr = (dose * weightKg * 60) / conc;
          steps.push(`Dose = ${dose.toFixed(2)} mcg/kg/min.`);
          steps.push(
            `mL/hr = (Dose × Weight × 60) ÷ Concentration = (${dose.toFixed(
              2
            )} × ${weightKg.toFixed(2)} × 60) ÷ ${conc.toFixed(
              2
            )} = ${mlhr.toFixed(2)} mL/hr.`
          );

          gttmin = (mlhr * df) / 60;
          steps.push(`Drop factor = ${df.toFixed(0)} gtt/mL.`);
          steps.push(
            `gtt/min = (mL/hr × Drop factor) ÷ 60 = (${mlhr.toFixed(
              2
            )} × ${df.toFixed(0)}) ÷ 60 = ${gttmin.toFixed(2)} gtt/min.`
          );

          let ctx = `<p>Weight-based infusion at <strong>${dose.toFixed(
            2
          )} mcg/kg/min</strong> for a ${weightKg.toFixed(
            1
          )} kg patient, with a bag concentration of <strong>${conc.toFixed(
            1
          )} mcg/mL</strong> and drop factor <strong>${df.toFixed(
            0
          )} gtt/mL</strong>.</p>`;
          if (outPref === "mlhr") {
            ctx +=
              "<p>Only the mL/hr value is typically used when programming an infusion pump.</p>";
          } else if (outPref === "gttmin") {
            ctx +=
              "<p>Use the gtt/min value when running the infusion by gravity drip.</p>";
          }

          // Respect output preference
          if (outPref === "mlhr") {
            setResults(mlhr, null, ctx);
          } else if (outPref === "gttmin") {
            setResults(null, gttmin, ctx);
          } else {
            setResults(mlhr, gttmin, ctx);
          }
        }

        document.getElementById("steps-content").innerHTML = steps
          .map((s) => "<p>" + s + "</p>")
          .join("");
      });

      document.getElementById("btn-clear").addEventListener("click", clearAll);

      // Initialize defaults
      updateKnownButtons("mlhr");
    </script>

