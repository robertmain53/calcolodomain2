<script="">
        function calculateValue() {
            const pickNumberInput = document.getElementById('pick-number');
            const pickValueOutput = document.getElementById('pick-value');
            const pickError = document.getElementById('pickError');

            const pickNumber = parseInt(pickNumberInput.value, 10);

            if (isNaN(pickNumber) || pickNumber < 1) {
                pickError.textContent = 'Please enter a valid pick number greater than 0.';
                pickValueOutput.textContent = '0';
                return;
            }

            pickError.textContent = '';
            const pickValue = (600 - pickNumber) * 10;
            pickValueOutput.textContent = pickValue.toString();
        }
    </script>


