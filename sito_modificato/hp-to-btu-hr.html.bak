    <script>
      // Calculator logic
      (function () {
        const modeHpToBtuBtn = document.getElementById("mode-hp-to-btu");
        const modeBtuToHpBtn = document.getElementById("mode-btu-to-hp");
        const hpTypeSelect = document.getElementById("hp-type");
        const btuTypeSelect = document.getElementById("btu-type");
        const hpInputGroup = document.getElementById("input-group-hp");
        const btuInputGroup = document.getElementById("input-group-btu");
        const hpInput = document.getElementById("hp-input");
        const btuInput = document.getElementById("btu-input");
        const convertBtn = document.getElementById("convert-btn");
        const resultMain = document.getElementById("result-main");
        const resultSub = document.getElementById("result-sub");
        const factorLine = document.getElementById("factor-line");
        const wattLine = document.getElementById("watt-line");
        const hpUnitLabel = document.getElementById("hp-unit-label");

        let mode = "hp-to-btu";

        const hpToW = {
          mechanical: 745.7,
          metric: 735.49875,
          boiler: 9809.5,
        };

        const btuToW = {
          it: 1055.056 / 3600, // ≈ 0.29307107
          th: 1054.35 / 3600, // ≈ 0.292875
        };

        function formatNumber(value, decimals = 2) {
          if (!isFinite(value)) return "—";
          if (
            Math.abs(value) >= 1e6 ||
            (Math.abs(value) < 1e-2 && value !== 0)
          ) {
            return value.toExponential(3);
          }
          return value.toLocaleString(undefined, {
            maximumFractionDigits: decimals,
            minimumFractionDigits: 0,
          });
        }

        function updateLabels() {
          const hpType = hpTypeSelect.value;
          hpUnitLabel.textContent = hpType === "boiler" ? "BHP" : "hp";
        }

        function updateInfoLines(factor) {
          const hpType = hpTypeSelect.value;
          const btuType = btuTypeSelect.value;
          const hpLabel =
            hpType === "mechanical"
              ? "mechanical hp"
              : hpType === "metric"
              ? "metric hp"
              : "boiler hp";
          const btuLabel = btuType === "it" ? "BTU (IT)" : "BTU (th)";

          factorLine.textContent = `Using: 1 ${hpLabel} = ${formatNumber(
            factor,
            2
          )} ${btuLabel}/hr`;
          wattLine.textContent = `Internal base: 1 ${hpLabel} = ${
            hpToW[hpType]
          } W, 1 ${btuLabel}/hr = ${btuToW[btuType].toFixed(8)} W`;
        }

        function convert() {
          const hpType = hpTypeSelect.value;
          const btuType = btuTypeSelect.value;
          const hpW = hpToW[hpType];
          const btuW = btuToW[btuType];

          if (!hpW || !btuW) return;

          const factorHpToBtu = hpW / btuW;

          if (mode === "hp-to-btu") {
            const hpVal = parseFloat(hpInput.value);
            if (isNaN(hpVal)) {
              resultMain.textContent = "Please enter a horsepower value.";
              resultSub.textContent = "";
              return;
            }
            const btu = hpVal * factorHpToBtu;
            resultMain.textContent = `${formatNumber(hpVal)} ${
              hpUnitLabel.textContent
            } = ${formatNumber(btu)} BTU/hr`;
            resultSub.textContent = `BTU/hr = hp × ${factorHpToBtu.toFixed(2)}`;
          } else {
            const btuVal = parseFloat(btuInput.value);
            if (isNaN(btuVal)) {
              resultMain.textContent = "Please enter a BTU/hr value.";
              resultSub.textContent = "";
              return;
            }
            const hp = (btuVal * btuW) / hpW;
            resultMain.textContent = `${formatNumber(
              btuVal
            )} BTU/hr = ${formatNumber(hp)} ${hpUnitLabel.textContent}`;
            resultSub.textContent = `hp = BTU/hr ÷ ${factorHpToBtu.toFixed(2)}`;
          }

          updateInfoLines(factorHpToBtu);
        }

        function setMode(newMode) {
          mode = newMode;
          if (mode === "hp-to-btu") {
            modeHpToBtuBtn.classList.add("toggle-btn-active");
            modeHpToBtuBtn.classList.remove("toggle-btn-inactive");
            modeBtuToHpBtn.classList.add("toggle-btn-inactive");
            modeBtuToHpBtn.classList.remove("toggle-btn-active");
            hpInputGroup.classList.remove("hidden");
            btuInputGroup.classList.add("hidden");
          } else {
            modeBtuToHpBtn.classList.add("toggle-btn-active");
            modeBtuToHpBtn.classList.remove("toggle-btn-inactive");
            modeHpToBtuBtn.classList.add("toggle-btn-inactive");
            modeHpToBtuBtn.classList.remove("toggle-btn-active");
            btuInputGroup.classList.remove("hidden");
            hpInputGroup.classList.add("hidden");
          }
          convert();
        }

        modeHpToBtuBtn.addEventListener("click", () => setMode("hp-to-btu"));
        modeBtuToHpBtn.addEventListener("click", () => setMode("btu-to-hp"));
        convertBtn.addEventListener("click", convert);

        hpTypeSelect.addEventListener("change", () => {
          updateLabels();
          convert();
        });
        btuTypeSelect.addEventListener("change", convert);

        hpInput.addEventListener("keyup", (e) => {
          if (e.key === "Enter") convert();
        });
        btuInput.addEventListener("keyup", (e) => {
          if (e.key === "Enter") convert();
        });

        // Initialize
        updateLabels();
        updateInfoLines(hpToW["mechanical"] / btuToW["it"]);
      })();

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
