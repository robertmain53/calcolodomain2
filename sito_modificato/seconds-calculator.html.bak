    <script>
      // Utility: normalize to number (seconds)
      function toNumber(value) {
        const n = parseFloat(value);
        return isNaN(n) ? 0 : n;
      }

      function hmsToSeconds(h, m, s) {
        return h * 3600 + m * 60 + s;
      }

      function secondsToHms(totalSeconds) {
        const sign = totalSeconds < 0 ? -1 : 1;
        let s = Math.abs(totalSeconds);
        const h = Math.floor(s / 3600);
        s = s % 3600;
        const m = Math.floor(s / 60);
        s = s % 60;
        return {
          sign,
          h,
          m,
          s,
        };
      }

      function formatHms(sign, h, m, s) {
        const prefix = sign < 0 ? "-" : "";
        const hh = String(h).padStart(2, "0");
        const mm = String(m).padStart(2, "0");
        const ss = String(s).padStart(2, "0");
        return prefix + hh + ":" + mm + ":" + ss;
      }

      document.addEventListener("DOMContentLoaded", () => {
        // Panel 1
        document
          .getElementById("btn-to-seconds")
          .addEventListener("click", () => {
            const h = toNumber(document.getElementById("h1").value);
            const m = toNumber(document.getElementById("m1").value);
            const s = toNumber(document.getElementById("s1").value);
            const total = hmsToSeconds(h, m, s);
            document.getElementById("total-seconds-output").value =
              total.toString();
          });

        // Panel 2
        document
          .getElementById("btn-from-seconds")
          .addEventListener("click", () => {
            const sec = toNumber(document.getElementById("seconds2").value);
            const { sign, h, m, s } = secondsToHms(sec);
            document.getElementById("h2").value = (sign < 0 ? "-" : "") + h;
            document.getElementById("m2").value = m;
            document.getElementById("s2").value = s;
          });

        // Panel 3
        document.getElementById("btn-add-sub").addEventListener("click", () => {
          const ah = toNumber(document.getElementById("a-h").value);
          const am = toNumber(document.getElementById("a-m").value);
          const as = toNumber(document.getElementById("a-s").value);
          const bh = toNumber(document.getElementById("b-h").value);
          const bm = toNumber(document.getElementById("b-m").value);
          const bs = toNumber(document.getElementById("b-s").value);

          const aSec = hmsToSeconds(ah, am, as);
          const bSec = hmsToSeconds(bh, bm, bs);

          const op = document.querySelector("input[name='op']:checked").value;
          const resultSec = op === "add" ? aSec + bSec : aSec - bSec;

          const { sign, h, m, s } = secondsToHms(resultSec);
          document.getElementById("res-hms").value = formatHms(sign, h, m, s);
          document.getElementById("res-seconds").value = resultSec.toString();
        });

        // Presets
        document.querySelectorAll(".preset-btn").forEach((btn) => {
          btn.addEventListener("click", () => {
            const sec = parseInt(btn.getAttribute("data-preset"), 10);
            document.getElementById("seconds2").value = sec;
            const { sign, h, m, s } = secondsToHms(sec);
            document.getElementById("h2").value = (sign < 0 ? "-" : "") + h;
            document.getElementById("m2").value = m;
            document.getElementById("s2").value = s;
            document.getElementById("total-seconds-output").value =
              sec.toString();
          });
        });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
