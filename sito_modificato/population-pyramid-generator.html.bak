    <script>
      // Population Pyramid Generator Logic
      (function () {
        const tbody = document.getElementById("data-tbody");
        const addRowBtn = document.getElementById("add-row-btn");
        const resetBtn = document.getElementById("reset-data-btn");
        const normalizeBtn = document.getElementById("normalize-btn");
        const asPercentageCheckbox = document.getElementById("as-percentage");
        const mirrorAxisCheckbox = document.getElementById("mirror-axis");
        const maleColorInput = document.getElementById("male-color");
        const femaleColorInput = document.getElementById("female-color");
        const chartTitleInput = document.getElementById("chart-title");
        const chartTitleDisplay = document.getElementById(
          "chart-title-display"
        );
        const chartSubtitle = document.getElementById("chart-subtitle");
        const chartNote = document.getElementById("chart-note");
        const csvFileInput = document.getElementById("csv-file");
        const downloadCsvBtn = document.getElementById("download-csv-btn");
        const downloadPngBtn = document.getElementById("download-png-btn");
        const copySvgBtn = document.getElementById("copy-svg-btn");
        const svg = d3.select("#pyramid-svg");

        let tooltip;

        function createTooltip() {
          if (!tooltip) {
            tooltip = document.createElement("div");
            tooltip.className = "tooltip hidden";
            document.body.appendChild(tooltip);
          }
        }

        function showTooltip(html, x, y) {
          createTooltip();
          tooltip.innerHTML = html;
          tooltip.style.left = x + 10 + "px";
          tooltip.style.top = y + 10 + "px";
          tooltip.classList.remove("hidden");
        }

        function hideTooltip() {
          if (tooltip) tooltip.classList.add("hidden");
        }

        function addRow(age = "", male = "", female = "") {
          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="px-2 py-1 border-t border-gray-100">
          <input type="text" class="w-full px-2 py-1 border border-gray-200 rounded text-xs age-input" value="${age}">
        </td>
        <td class="px-2 py-1 border-t border-gray-100 text-right">
          <input type="number" min="0" step="any" class="w-full px-2 py-1 border border-gray-200 rounded text-xs text-right male-input" value="${male}">
        </td>
        <td class="px-2 py-1 border-t border-gray-100 text-right">
          <input type="number" min="0" step="any" class="w-full px-2 py-1 border border-gray-200 rounded text-xs text-right female-input" value="${female}">
        </td>
        <td class="px-2 py-1 border-t border-gray-100 text-center">
          <button type="button" class="text-xs text-red-600 hover:underline remove-row-btn">Remove</button>
        </td>
      `;
          tbody.appendChild(tr);
        }

        function loadExampleData() {
          tbody.innerHTML = "";
          const example = [
            ["0-4", 1200, 1150],
            ["5-9", 1300, 1250],
            ["10-14", 1400, 1350],
            ["15-19", 1500, 1450],
            ["20-24", 1600, 1550],
            ["25-29", 1550, 1500],
            ["30-34", 1500, 1480],
            ["35-39", 1450, 1470],
            ["40-44", 1400, 1450],
            ["45-49", 1300, 1400],
            ["50-54", 1200, 1300],
            ["55-59", 1000, 1150],
            ["60-64", 800, 950],
            ["65-69", 600, 800],
            ["70-74", 400, 600],
            ["75-79", 250, 400],
            ["80+", 150, 300],
          ];
          example.forEach((row) => addRow(row[0], row[1], row[2]));
          updateChart();
        }

        function getData() {
          const rows = [];
          tbody.querySelectorAll("tr").forEach((tr) => {
            const age = tr.querySelector(".age-input").value.trim();
            const male = parseFloat(tr.querySelector(".male-input").value);
            const female = parseFloat(tr.querySelector(".female-input").value);
            if (age && !isNaN(male) && !isNaN(female)) {
              rows.push({ age, male, female });
            }
          });
          return rows;
        }

        function normalizeToPercent() {
          const data = getData();
          let total = 0;
          data.forEach((d) => {
            total += d.male + d.female;
          });
          if (!total) return;
          tbody.querySelectorAll("tr").forEach((tr, i) => {
            const d = data[i];
            if (!d) return;
            const malePct = (d.male / total) * 100;
            const femalePct = (d.female / total) * 100;
            tr.querySelector(".male-input").value = malePct.toFixed(3);
            tr.querySelector(".female-input").value = femalePct.toFixed(3);
          });
          asPercentageCheckbox.checked = true;
          updateChart();
        }

        function updateChart() {
          const data = getData();
          const asPercent = asPercentageCheckbox.checked;
          const mirror = mirrorAxisCheckbox.checked;
          const maleColor = maleColorInput.value || "#3b82f6";
          const femaleColor = femaleColorInput.value || "#ec4899";

          chartTitleDisplay.textContent =
            chartTitleInput.value || "Population Pyramid";
          chartSubtitle.textContent = asPercent
            ? "Age–sex distribution (percent of total population)"
            : "Age–sex distribution (absolute counts)";
          chartNote.textContent = asPercent
            ? "Hover over a bar to see exact percentages. Values are assumed to already be in percent."
            : "Hover over a bar to see exact counts. Use “Normalize to %” to convert counts to percentages.";

          const containerWidth =
            document.getElementById("chart-container").clientWidth || 600;
          const height = window.innerWidth < 768 ? 320 : 380;
          const margin = { top: 20, right: 40, bottom: 30, left: 40 };
          const width = containerWidth - margin.left - margin.right;

          svg.attr("width", containerWidth).attr("height", height);
          svg.selectAll("*").remove();

          if (!data.length) return;

          // Sort by age group if possible (simple heuristic: numeric start of range)
          data.sort((a, b) => {
            const aStart = parseInt(a.age);
            const bStart = parseInt(b.age);
            if (isNaN(aStart) || isNaN(bStart)) return 0;
            return aStart - bStart;
          });

          const ages = data.map((d) => d.age);
          const maxVal = d3.max(data, (d) => Math.max(d.male, d.female)) || 1;

          const xMax = mirror ? maxVal : maxVal;
          const x = mirror
            ? d3.scaleLinear().domain([-xMax, xMax]).range([0, width])
            : d3
                .scaleLinear()
                .domain([0, xMax])
                .range([0, width / 2 - 10]);

          const y = d3
            .scaleBand()
            .domain(ages)
            .range([height - margin.bottom, margin.top])
            .padding(0.1);

          const g = svg
            .append("g")
            .attr("transform", `translate(${margin.left},0)`);

          // Axes
          const xAxis = d3
            .axisBottom(x)
            .ticks(6)
            .tickFormat((d) =>
              asPercent
                ? Math.abs(d).toFixed(0) + "%"
                : Math.abs(d).toLocaleString()
            );

          const yAxis = d3.axisLeft(y);

          // Male bars
          g.selectAll(".bar-male")
            .data(data)
            .enter()
            .append("rect")
            .attr("class", "bar-male")
            .attr("y", (d) => y(d.age))
            .attr("height", y.bandwidth())
            .attr("x", (d) => (mirror ? x(-d.male) : x(0) - x(d.male)))
            .attr("width", (d) => (mirror ? x(0) - x(-d.male) : x(d.male)))
            .attr("fill", maleColor)
            .on("mousemove", function (event, d) {
              const val = d.male;
              const label = asPercent
                ? val.toFixed(2) + "%"
                : val.toLocaleString();
              showTooltip(
                `<strong>${d.age}</strong><br>Male: ${label}`,
                event.pageX,
                event.pageY
              );
              d3.select(this).classed("bar-hover", true);
            })
            .on("mouseout", function () {
              hideTooltip();
              d3.select(this).classed("bar-hover", false);
            });

          // Female bars
          g.selectAll(".bar-female")
            .data(data)
            .enter()
            .append("rect")
            .attr("class", "bar-female")
            .attr("y", (d) => y(d.age))
            .attr("height", y.bandwidth())
            .attr("x", (d) => (mirror ? x(0) : width / 2 + 10))
            .attr("width", (d) => (mirror ? x(d.female) - x(0) : x(d.female)))
            .attr("fill", femaleColor)
            .on("mousemove", function (event, d) {
              const val = d.female;
              const label = asPercent
                ? val.toFixed(2) + "%"
                : val.toLocaleString();
              showTooltip(
                `<strong>${d.age}</strong><br>Female: ${label}`,
                event.pageX,
                event.pageY
              );
              d3.select(this).classed("bar-hover", true);
            })
            .on("mouseout", function () {
              hideTooltip();
              d3.select(this).classed("bar-hover", false);
            });

          // X axis
          g.append("g")
            .attr("class", "axis")
            .attr("transform", `translate(0,${height - margin.bottom})`)
            .call(xAxis);

          // Y axis
          g.append("g").attr("class", "axis").call(yAxis);

          // Center line (for mirrored chart)
          if (mirror) {
            g.append("line")
              .attr("x1", x(0))
              .attr("x2", x(0))
              .attr("y1", margin.top - 5)
              .attr("y2", height - margin.bottom)
              .attr("stroke", "#9ca3af")
              .attr("stroke-width", 1);
          }

          // Legend
          const legend = svg
            .append("g")
            .attr("transform", `translate(${margin.left},${margin.top})`);

          legend
            .append("rect")
            .attr("x", 0)
            .attr("y", -15)
            .attr("width", 10)
            .attr("height", 10)
            .attr("fill", maleColor);

          legend
            .append("text")
            .attr("x", 15)
            .attr("y", -6)
            .attr("font-size", 11)
            .attr("fill", "#374151")
            .text("Male");

          legend
            .append("rect")
            .attr("x", 60)
            .attr("y", -15)
            .attr("width", 10)
            .attr("height", 10)
            .attr("fill", femaleColor);

          legend
            .append("text")
            .attr("x", 75)
            .attr("y", -6)
            .attr("font-size", 11)
            .attr("fill", "#374151")
            .text("Female");
        }

        function handleTableChange(e) {
          if (e.target.matches(".age-input, .male-input, .female-input")) {
            updateChart();
          } else if (e.target.matches(".remove-row-btn")) {
            e.target.closest("tr").remove();
            updateChart();
          }
        }

        function handleCsvUpload(file) {
          if (!file) return;
          const reader = new FileReader();
          reader.onload = function (e) {
            const text = e.target.result;
            const lines = text.split(/\r?\n/).filter((l) => l.trim().length);
            if (!lines.length) return;
            tbody.innerHTML = "";
            let startIndex = 0;
            const header = lines[0].toLowerCase();
            if (
              header.includes("age") &&
              header.includes("male") &&
              header.includes("female")
            ) {
              startIndex = 1;
            }
            for (let i = startIndex; i < lines.length; i++) {
              const parts = lines[i].split(",");
              if (parts.length < 3) continue;
              const age = parts[0].trim();
              const male = parts[1].trim();
              const female = parts[2].trim();
              if (age) addRow(age, male, female);
            }
            updateChart();
          };
          reader.readAsText(file);
        }

        function downloadCsvTemplate() {
          const header = "age_group,male,female\n";
          const data = getData();
          const rows = data
            .map((d) => `${d.age},${d.male},${d.female}`)
            .join("\n");
          const blob = new Blob([header + rows], {
            type: "text/csv;charset=utf-8;",
          });
          const url = URL.createObjectURL(blob);
          const a = document.createElement("a");
          a.href = url;
          a.download = "population_pyramid_template.csv";
          document.body.appendChild(a);
          a.click();
          document.body.removeChild(a);
          URL.revokeObjectURL(url);
        }

        function downloadPng() {
          const svgNode = document.getElementById("pyramid-svg");
          const serializer = new XMLSerializer();
          const svgString = serializer.serializeToString(svgNode);
          const canvas = document.createElement("canvas");
          const bbox = svgNode.getBoundingClientRect();
          canvas.width = bbox.width * 2;
          canvas.height = bbox.height * 2;
          const ctx = canvas.getContext("2d");
          const img = new Image();
          const svgBlob = new Blob([svgString], {
            type: "image/svg+xml;charset=utf-8",
          });
          const url = URL.createObjectURL(svgBlob);
          img.onload = function () {
            ctx.setTransform(2, 0, 0, 2, 0, 0);
            ctx.fillStyle = "#f9fafb";
            ctx.fillRect(0, 0, bbox.width, bbox.height);
            ctx.drawImage(img, 0, 0);
            URL.revokeObjectURL(url);
            canvas.toBlob(function (blob) {
              const a = document.createElement("a");
              a.href = URL.createObjectURL(blob);
              a.download = "population_pyramid.png";
              document.body.appendChild(a);
              a.click();
              document.body.removeChild(a);
            }, "image/png");
          };
          img.src = url;
        }

        function copySvgToClipboard() {
          const svgNode = document.getElementById("pyramid-svg");
          const serializer = new XMLSerializer();
          const svgString = serializer.serializeToString(svgNode);
          if (navigator.clipboard && navigator.clipboard.writeText) {
            navigator.clipboard
              .writeText(svgString)
              .then(() => {
                copySvgBtn.textContent = "Copied!";
                setTimeout(() => {
                  copySvgBtn.textContent = "Copy SVG";
                }, 1500);
              })
              .catch(() => {
                alert("Could not copy SVG to clipboard.");
              });
          } else {
            alert("Clipboard API not supported in this browser.");
          }
        }

        // Event listeners
        addRowBtn.addEventListener("click", () => {
          addRow("", "", "");
        });

        resetBtn.addEventListener("click", loadExampleData);
        normalizeBtn.addEventListener("click", normalizeToPercent);
        asPercentageCheckbox.addEventListener("change", updateChart);
        mirrorAxisCheckbox.addEventListener("change", updateChart);
        maleColorInput.addEventListener("input", updateChart);
        femaleColorInput.addEventListener("input", updateChart);
        chartTitleInput.addEventListener("input", updateChart);
        tbody.addEventListener("input", handleTableChange);
        tbody.addEventListener("click", handleTableChange);
        csvFileInput.addEventListener("change", (e) =>
          handleCsvUpload(e.target.files[0])
        );
        downloadCsvBtn.addEventListener("click", downloadCsvTemplate);
        downloadPngBtn.addEventListener("click", downloadPng);
        copySvgBtn.addEventListener("click", copySvgToClipboard);
        window.addEventListener("resize", () => {
          updateChart();
        });

        // Initialize
        loadExampleData();
      })();
    </script>

    <script>
      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
