    <script>
      // Tab handling
      const tabs = {
        simple: {
          tab: document.getElementById("tab-simple"),
          panel: document.getElementById("panel-simple"),
        },
        advanced: {
          tab: document.getElementById("tab-advanced"),
          panel: document.getElementById("panel-advanced"),
        },
        compare: {
          tab: document.getElementById("tab-compare"),
          panel: document.getElementById("panel-compare"),
        },
      };

      function activateTab(name) {
        Object.keys(tabs).forEach((key) => {
          const { tab, panel } = tabs[key];
          if (key === name) {
            tab.classList.remove("tab-inactive");
            tab.classList.add("tab-active");
            panel.classList.remove("hidden");
          } else {
            tab.classList.remove("tab-active");
            tab.classList.add("tab-inactive");
            panel.classList.add("hidden");
          }
        });
      }

      tabs.simple.tab.addEventListener("click", () => activateTab("simple"));
      tabs.advanced.tab.addEventListener("click", () =>
        activateTab("advanced")
      );
      tabs.compare.tab.addEventListener("click", () => activateTab("compare"));

      // Utility functions
      function parseNumber(id) {
        const el = document.getElementById(id);
        if (!el) return NaN;
        const v = parseFloat(el.value.replace(",", "."));
        return isNaN(v) ? NaN : v;
      }

      function formatCurrency(value) {
        if (!isFinite(value)) return "–";
        return value.toLocaleString(undefined, {
          minimumFractionDigits: 2,
          maximumFractionDigits: 2,
        });
      }

      function formatPercent(value) {
        if (!isFinite(value)) return "–";
        return (
          (value * 100).toLocaleString(undefined, {
            minimumFractionDigits: 2,
            maximumFractionDigits: 2,
          }) + "%"
        );
      }

      function formatRatio(value) {
        if (!isFinite(value)) return "–";
        return (
          value.toLocaleString(undefined, {
            minimumFractionDigits: 2,
            maximumFractionDigits: 2,
          }) + "×"
        );
      }

      function periodToYears(value, unit) {
        if (!isFinite(value) || value <= 0) return NaN;
        switch (unit) {
          case "years":
            return value;
          case "months":
            return value / 12;
          case "days":
            return value / 365;
          default:
            return value;
        }
      }

      // SIMPLE ROI CALC
      document
        .getElementById("btn-calc-simple")
        .addEventListener("click", () => {
          const initial = parseNumber("initialInvestment");
          const finalVal = parseNumber("finalValue");
          const fees = parseNumber("fees");
          const period = parseNumber("holdingPeriod");
          const unit = document.getElementById("holdingPeriodUnit").value;

          if (!isFinite(initial) || !isFinite(finalVal) || initial <= 0) {
            alert(
              "Please enter at least a positive initial investment and final value."
            );
            return;
          }

          const safeFees = isFinite(fees) ? fees : 0;
          const grossProfit = finalVal - initial;
          const netProfit = finalVal - initial - safeFees;

          const grossROI = grossProfit / initial;
          const netROI = netProfit / initial;

          document.getElementById("simple-net-profit").textContent =
            formatCurrency(netProfit);
          document.getElementById("simple-roi-percent").textContent =
            formatPercent(grossROI);
          document.getElementById("simple-roi-net-percent").textContent =
            formatPercent(netROI);

          const years = periodToYears(period, unit);
          if (isFinite(years) && years > 0) {
            const grossFactor = finalVal / initial;
            const netFinal = finalVal - safeFees;
            const netFactor = netFinal / initial;

            const annualGross = Math.pow(grossFactor, 1 / years) - 1;
            const annualNet =
              netFactor > 0 ? Math.pow(netFactor, 1 / years) - 1 : NaN;

            document.getElementById("simple-annualized-roi").textContent =
              formatPercent(annualGross);
            document.getElementById("simple-annualized-roi-net").textContent =
              formatPercent(annualNet);
          } else {
            document.getElementById("simple-annualized-roi").textContent = "–";
            document.getElementById("simple-annualized-roi-net").textContent =
              "–";
          }
        });

      document
        .getElementById("btn-reset-simple")
        .addEventListener("click", () => {
          ["initialInvestment", "finalValue", "fees", "holdingPeriod"].forEach(
            (id) => {
              const el = document.getElementById(id);
              if (el) el.value = "";
            }
          );
          document.getElementById("holdingPeriodUnit").value = "years";
          [
            "simple-net-profit",
            "simple-roi-percent",
            "simple-roi-net-percent",
            "simple-annualized-roi",
            "simple-annualized-roi-net",
          ].forEach((id) => (document.getElementById(id).textContent = "–"));
        });

      // ADVANCED / MARKETING ROI
      document
        .getElementById("btn-calc-advanced")
        .addEventListener("click", () => {
          const revenue = parseNumber("advRevenue");
          const cost = parseNumber("advCost");
          const cogs = parseNumber("advCOGS");
          const period = parseNumber("advPeriod");
          const unit = document.getElementById("advPeriodUnit").value;
          const basis =
            document.querySelector('input[name="advBasis"]:checked')?.value ||
            "revenue";

          if (!isFinite(revenue) || !isFinite(cost) || cost <= 0) {
            alert("Please enter at least revenue and a positive total cost.");
            return;
          }

          const safeCOGS = isFinite(cogs) ? cogs : 0;
          const roas = revenue / cost;

          let profit;
          if (basis === "profit") {
            profit = revenue - safeCOGS - cost;
          } else {
            profit = revenue - cost;
          }

          const roi = profit / cost;

          document.getElementById("adv-profit").textContent =
            formatCurrency(profit);
          document.getElementById("adv-roi-percent").textContent =
            formatPercent(roi);
          document.getElementById("adv-roas").textContent = formatRatio(roas);

          const years = periodToYears(period, unit);
          if (isFinite(years) && years > 0) {
            const factor = 1 + roi;
            const annual = factor > 0 ? Math.pow(factor, 1 / years) - 1 : NaN;
            document.getElementById("adv-annualized-roi").textContent =
              formatPercent(annual);
          } else {
            document.getElementById("adv-annualized-roi").textContent = "–";
          }

          const paybackMultiple = revenue / cost;
          document.getElementById("adv-payback-multiple").textContent =
            formatRatio(paybackMultiple);
        });

      document
        .getElementById("btn-reset-advanced")
        .addEventListener("click", () => {
          ["advRevenue", "advCost", "advCOGS", "advPeriod"].forEach((id) => {
            const el = document.getElementById(id);
            if (el) el.value = "";
          });
          document.getElementById("advPeriodUnit").value = "months";
          document.querySelector(
            'input[name="advBasis"][value="revenue"]'
          ).checked = true;
          [
            "adv-profit",
            "adv-roi-percent",
            "adv-roas",
            "adv-annualized-roi",
            "adv-payback-multiple",
          ].forEach((id) => (document.getElementById(id).textContent = "–"));
        });

      // COMPARE SCENARIOS
      function computeScenarioROI(cost, finalVal, years) {
        if (!isFinite(cost) || !isFinite(finalVal) || cost <= 0)
          return { roi: NaN, annual: NaN };
        const roi = (finalVal - cost) / cost;
        let annual = NaN;
        if (isFinite(years) && years > 0) {
          const factor = finalVal / cost;
          annual = Math.pow(factor, 1 / years) - 1;
        }
        return { roi, annual };
      }

      document
        .getElementById("btn-calc-compare")
        .addEventListener("click", () => {
          const scenarios = ["a", "b", "c"].map((label) => {
            return {
              label: label.toUpperCase(),
              cost: parseNumber(`cmp-cost-${label}`),
              finalVal: parseNumber(`cmp-final-${label}`),
              years: parseNumber(`cmp-years-${label}`),
            };
          });

          const results = scenarios.map((s) =>
            computeScenarioROI(s.cost, s.finalVal, s.years)
          );

          document.getElementById("cmp-roi-a").textContent = formatPercent(
            results[0].roi
          );
          document.getElementById("cmp-annual-a").textContent = formatPercent(
            results[0].annual
          );
          document.getElementById("cmp-roi-b").textContent = formatPercent(
            results[1].roi
          );
          document.getElementById("cmp-annual-b").textContent = formatPercent(
            results[1].annual
          );
          document.getElementById("cmp-roi-c").textContent = formatPercent(
            results[2].roi
          );
          document.getElementById("cmp-annual-c").textContent = formatPercent(
            results[2].annual
          );

          // Determine best scenario by annualized ROI, then by ROI
          let bestIndex = -1;
          let bestAnnual = -Infinity;
          let bestROI = -Infinity;
          results.forEach((r, idx) => {
            if (!isFinite(r.annual) && !isFinite(r.roi)) return;
            const annual = isFinite(r.annual) ? r.annual : -Infinity;
            const roi = isFinite(r.roi) ? r.roi : -Infinity;
            if (
              annual > bestAnnual ||
              (annual === bestAnnual && roi > bestROI)
            ) {
              bestAnnual = annual;
              bestROI = roi;
              bestIndex = idx;
            }
          });

          const bestBox = document.getElementById("compare-best");
          const bestText = document.getElementById("compare-best-text");
          if (bestIndex >= 0) {
            const label = scenarios[bestIndex].label;
            bestText.textContent = `Scenario ${label} has the highest time‑adjusted return based on the data provided.`;
            bestBox.classList.remove("hidden");
          } else {
            bestBox.classList.add("hidden");
            bestText.textContent = "";
          }
        });

      document
        .getElementById("btn-reset-compare")
        .addEventListener("click", () => {
          ["a", "b", "c"].forEach((label) => {
            ["cost", "final", "years"].forEach((field) => {
              const el = document.getElementById(`cmp-${field}-${label}`);
              if (el) el.value = "";
            });
          });
          [
            "cmp-roi-a",
            "cmp-annual-a",
            "cmp-roi-b",
            "cmp-annual-b",
            "cmp-roi-c",
            "cmp-annual-c",
          ].forEach((id) => (document.getElementById(id).textContent = "–"));
          document.getElementById("compare-best").classList.add("hidden");
          document.getElementById("compare-best-text").textContent = "";
        });

      // Mobile menu script (simple)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", () => {
          const menu = document.getElementById("mobile-menu");
          const expanded = menu.classList.contains("hidden") ? "true" : "false";
          menu.classList.toggle("hidden");
          document
            .getElementById("mobile-menu-toggle")
            .setAttribute("aria-expanded", expanded);
        });
    </script>

