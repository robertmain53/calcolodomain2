            <script>
              document.addEventListener('DOMContentLoaded', () => {
                const rootInput = document.getElementById('nth-root-input');
                const numberInput = document.getElementById('number-input');
                const calculateBtn = document.getElementById('calculate-btn');
                const sqrtOutput = document.getElementById('sqrt-output');
                const nthOutput = document.getElementById('nth-output');
                const stepsBox = document.getElementById('calculation-steps');

                function formatNum(num) {
                    if (isNaN(num)) return '-';
                    return parseFloat(num.toFixed(6)).toString();
                }

                function getPerfectSquareFactor(n) {
                    let largestFactor = 1;
                    const max = Math.floor(Math.sqrt(n));
                    for (let i = max; i >= 2; i--) {
                        if (n % (i * i) === 0) {
                            largestFactor = i * i;
                            break;
                        }
                    }
                    return largestFactor;
                }

                function simplifyRadical(radicand) {
                    const factor = getPerfectSquareFactor(radicand);
                    const outside = Math.sqrt(factor);
                    const inside = radicand / factor;
                    
                    if (factor === 1) {
                        return `\\sqrt{${radicand}}`;
                    } else if (inside === 1) {
                        return outside.toString();
                    } else {
                        return `${outside}\\sqrt{${inside}}`;
                    }
                }

                calculateBtn.addEventListener('click', () => {
                    const N = parseFloat(rootInput.value) || 2;
                    const X = parseFloat(numberInput.value);

                    if (isNaN(X) || X === 0) {
                        sqrtOutput.textContent = '-';
                        nthOutput.textContent = '-';
                        stepsBox.hidden = true;
                        return;
                    }

                    if (X < 0 && N % 2 === 0) {
                        sqrtOutput.textContent = 'Error';
                        nthOutput.textContent = 'Imaginary';
                        stepsBox.innerHTML = '<h3 class="text-lg font-semibold text-red-700">Error: Imaginary Result</h3><p class="text-red-700">You cannot take an even root (like a square root) of a negative number in the real number system.</p>';
                        stepsBox.hidden = false;
                        return;
                    }
                    
                    // Calculations
                    const sqrtX = Math.sqrt(X);
                    const nthRoot = Math.pow(Math.abs(X), 1 / N);
                    
                    // Adjust sign for odd roots of negative numbers
                    const finalNth = X < 0 && N % 2 !== 0 ? -nthRoot : nthRoot;
                    
                    // Aggiorna Output Principale
                    sqrtOutput.textContent = formatNum(sqrtX);
                    nthOutput.textContent = formatNum(finalNth);

                    // Aggiorna Passaggi
                    let stepsHtml = `<h3 class="text-lg font-semibold text-gray-800 mb-2">Calculation Steps:</h3>`;

                    // Passaggi Radice Quadrata
                    if (N !== 2) {
                        stepsHtml += `<p><strong>Square Root ($\sqrt{${X}}$):</strong> <code>${X}^{1/2} = ${formatNum(sqrtX)}</code></p>`;
                    }
                    
                    stepsHtml += `<p><strong>Nth Root ($\sqrt[${N}]{${X}}$):</strong> <code>${X}^{1/${N}} = ${formatNum(finalNth)}</code></p>`;
                    
                    // Semplificazione (solo per radice quadrata e non negativa)
                    if (N === 2 && X >= 0) {
                        const simplified = simplifyRadical(X);
                        if (simplified !== `\\sqrt{${X}}`) {
                            stepsHtml += `<hr class="my-4">`;
                            stepsHtml += `<p><strong>Simplified Radical:</strong></p>`;
                            stepsHtml += `<p class="formula-box">$$ \\sqrt{${X}} = ${simplified} $$</p>`;
                        } else {
                            stepsHtml += `<p><strong>Simplified Radical:</strong> <span class="text-gray-600">Cannot be simplified further.</span></p>`;
                        }
                    }
                    
                    stepsBox.innerHTML = stepsHtml;
                    stepsBox.hidden = false;
                    // Rende le formule LaTeX
                    if (window.MathJax) {
                       MathJax.typesetPromise([stepsBox]);
                    }
                });
              });
            </script>
            
