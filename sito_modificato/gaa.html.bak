    <script>
      // Helper to get numeric value safely
      function num(id) {
        const v = parseFloat(document.getElementById(id).value);
        return isNaN(v) ? 0 : v;
      }

      function getRegulationMinutes() {
        const select = document.getElementById("gameLength");
        if (select.value === "custom") {
          const custom = parseFloat(
            document.getElementById("customGameLength").value
          );
          return isNaN(custom) || custom <= 0 ? 60 : custom;
        }
        return parseFloat(select.value);
      }

      function calculateMainGAA() {
        const goals = num("goalsAgainst");
        const modeMinutes = !document
          .getElementById("minutes-wrapper")
          .classList.contains("hidden");
        let minutesPlayed = 0;

        if (modeMinutes) {
          minutesPlayed = num("minutesPlayed");
        } else {
          const games = num("gamesPlayed");
          const reg = getRegulationMinutes();
          minutesPlayed = games * reg;
        }

        const regMinutes = getRegulationMinutes();

        if (minutesPlayed <= 0 || goals < 0) {
          document.getElementById("gaaPer60").textContent = "0.00";
          document.getElementById("gaaPerGame").textContent = "0.00";
          document.getElementById("summaryGoals").textContent = "0";
          document.getElementById("summaryMinutes").textContent = "0";
          document.getElementById("summaryGames").textContent = "0.0";
          return;
        }

        const gaaPer60 = (goals / minutesPlayed) * 60;
        const gaaPerGame = (goals / minutesPlayed) * regMinutes;
        const estGames = minutesPlayed / regMinutes;

        document.getElementById("gaaPer60").textContent = gaaPer60.toFixed(2);
        document.getElementById("gaaPerGame").textContent =
          gaaPerGame.toFixed(2);
        document.getElementById("summaryGoals").textContent = goals.toString();
        document.getElementById("summaryMinutes").textContent =
          minutesPlayed.toFixed(0);
        document.getElementById("summaryGames").textContent =
          estGames.toFixed(1);
      }

      function calculateSplit(idGoals, idMinutes) {
        const g = num(idGoals);
        const m = num(idMinutes);
        if (m <= 0 || g < 0) return 0;
        return (g / m) * 60;
      }

      function updateSplitDisplay() {
        const gaaA = calculateSplit("aGoals", "aMinutes");
        const gaaB = calculateSplit("bGoals", "bMinutes");
        document.getElementById("gaaA").textContent = gaaA.toFixed(2);
        document.getElementById("gaaB").textContent = gaaB.toFixed(2);
      }

      document.addEventListener("DOMContentLoaded", function () {
        const tabMinutes = document.getElementById("tab-minutes");
        const tabGames = document.getElementById("tab-games");
        const minutesWrapper = document.getElementById("minutes-wrapper");
        const gamesWrapper = document.getElementById("games-wrapper");
        const gameLengthSelect = document.getElementById("gameLength");
        const customGameLengthInput =
          document.getElementById("customGameLength");

        tabMinutes.addEventListener("click", function () {
          tabMinutes.classList.add("tab-active");
          tabMinutes.classList.remove("text-gray-500");
          tabGames.classList.remove("tab-active");
          tabGames.classList.add("text-gray-500");
          minutesWrapper.classList.remove("hidden");
          gamesWrapper.classList.add("hidden");
        });

        tabGames.addEventListener("click", function () {
          tabGames.classList.add("tab-active");
          tabGames.classList.remove("text-gray-500");
          tabMinutes.classList.remove("tab-active");
          tabMinutes.classList.add("text-gray-500");
          gamesWrapper.classList.remove("hidden");
          minutesWrapper.classList.add("hidden");
        });

        gameLengthSelect.addEventListener("change", function () {
          if (this.value === "custom") {
            customGameLengthInput.classList.remove("hidden");
          } else {
            customGameLengthInput.classList.add("hidden");
          }
        });

        document
          .getElementById("calculateBtn")
          .addEventListener("click", calculateMainGAA);

        document
          .getElementById("resetBtn")
          .addEventListener("click", function () {
            document.getElementById("goalsAgainst").value = 0;
            document.getElementById("minutesPlayed").value = 60;
            document.getElementById("gamesPlayed").value = 1;
            document.getElementById("gameLength").value = "60";
            customGameLengthInput.value = 60;
            customGameLengthInput.classList.add("hidden");
            calculateMainGAA();
          });

        document
          .getElementById("compareBtn")
          .addEventListener("click", function () {
            const gaaA = calculateSplit("aGoals", "aMinutes");
            const gaaB = calculateSplit("bGoals", "bMinutes");
            const summary = document.getElementById("compareSummary");

            if (
              !isFinite(gaaA) ||
              !isFinite(gaaB) ||
              (gaaA === 0 && gaaB === 0)
            ) {
              summary.textContent =
                "Enter goals against and minutes for both goalies to compare.";
              return;
            }

            if (gaaA === gaaB) {
              summary.textContent = "Both goalies have the same GAA.";
            } else if (gaaA < gaaB) {
              summary.textContent = `Goalie A has the better GAA (${gaaA.toFixed(
                2
              )} vs ${gaaB.toFixed(2)}).`;
            } else {
              summary.textContent = `Goalie B has the better GAA (${gaaB.toFixed(
                2
              )} vs ${gaaA.toFixed(2)}).`;
            }
          });

        // Live update for split comparison
        ["aGoals", "aMinutes", "bGoals", "bMinutes"].forEach((id) => {
          document
            .getElementById(id)
            .addEventListener("input", updateSplitDisplay);
        });

        // Initial calculations
        calculateMainGAA();
        updateSplitDisplay();
      });
    </script>

