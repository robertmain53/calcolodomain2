    <script>
      // Utah income tax calculator logic
      const FLAT_RATE = 0.0465;

      const incomeSlider = document.getElementById("income");
      const incomeInput = document.getElementById("incomeInput");
      const incomeDisplay = document.getElementById("incomeDisplay");
      const deductionsInput = document.getElementById("deductions");
      const creditsInput = document.getElementById("credits");
      const withholdingInput = document.getElementById("withholding");
      const otherTaxesInput = document.getElementById("otherTaxes");
      const calcBtn = document.getElementById("calculateBtn");
      const resetBtn = document.getElementById("resetBtn");
      const errorEl = document.getElementById("calcError");

      const utTaxEl = document.getElementById("utTax");
      const utEffectiveRateEl = document.getElementById("utEffectiveRate");
      const afterTaxEl = document.getElementById("afterTax");
      const marginalRateEl = document.getElementById("marginalRate");
      const taxableIncomeEl = document.getElementById("taxableIncome");
      const creditsAppliedEl = document.getElementById("creditsApplied");
      const refundDueEl = document.getElementById("refundDue");

      function formatMoney(value) {
        return value.toLocaleString("en-US", {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 0,
        });
      }

      function formatPercent(value) {
        return (value * 100).toFixed(2) + "%";
      }

      function syncIncomeFromSlider() {
        const val = Number(incomeSlider.value) || 0;
        incomeInput.value = val;
        incomeDisplay.textContent = formatMoney(val);
      }

      function syncIncomeFromInput() {
        let val = Number(incomeInput.value);
        if (isNaN(val) || val < 0) val = 0;
        if (val > Number(incomeSlider.max)) {
          incomeSlider.max = Math.ceil(val / 1000) * 1000;
        }
        incomeSlider.value = val;
        incomeDisplay.textContent = formatMoney(val);
      }

      incomeSlider.addEventListener("input", syncIncomeFromSlider);
      incomeInput.addEventListener("input", syncIncomeFromInput);

      function calculateUtahTax() {
        errorEl.classList.add("hidden");

        const income = Number(incomeInput.value);
        const deductions = Number(deductionsInput.value);
        const credits = Number(creditsInput.value);
        const withholding = Number(withholdingInput.value);
        const otherTaxes = Number(otherTaxesInput.value);

        if (
          [income, deductions, credits, withholding, otherTaxes].some(
            (v) => isNaN(v) || v < 0
          )
        ) {
          errorEl.classList.remove("hidden");
          return;
        }

        const taxableAfterDeductions = Math.max(0, income - deductions);
        const preCreditTax = taxableAfterDeductions * FLAT_RATE;
        const creditsApplied = Math.min(preCreditTax, credits);
        const taxAfterCredits = Math.max(0, preCreditTax - creditsApplied);
        const totalUtahTax = taxAfterCredits + otherTaxes;
        const afterTaxIncome = income - totalUtahTax;
        const effectiveRate = income > 0 ? totalUtahTax / income : 0;
        const refund = withholding - totalUtahTax;

        utTaxEl.textContent = formatMoney(Math.round(totalUtahTax));
        utEffectiveRateEl.textContent =
          "Effective Utah tax rate: " + formatPercent(effectiveRate);
        afterTaxEl.textContent = formatMoney(Math.round(afterTaxIncome));
        marginalRateEl.textContent =
          "Marginal Utah rate: " + formatPercent(FLAT_RATE) + " (flat)";

        taxableIncomeEl.textContent = formatMoney(
          Math.round(taxableAfterDeductions)
        );
        creditsAppliedEl.textContent = formatMoney(Math.round(creditsApplied));

        let refundText;
        if (Math.abs(refund) < 1) {
          refundText = "$0 (even)";
        } else if (refund > 0) {
          refundText = formatMoney(Math.round(refund)) + " refund";
        } else {
          refundText = formatMoney(Math.round(-refund)) + " due";
        }
        refundDueEl.textContent = refundText;
      }

      function resetCalculator() {
        document.getElementById("filingStatus").value = "single";
        document.getElementById("taxYear").value = "2025";
        incomeSlider.value = 60000;
        incomeSlider.max = 300000;
        syncIncomeFromSlider();
        deductionsInput.value = 0;
        creditsInput.value = 0;
        withholdingInput.value = 0;
        otherTaxesInput.value = 0;
        errorEl.classList.add("hidden");
        calculateUtahTax();
      }

      calcBtn.addEventListener("click", calculateUtahTax);
      resetBtn.addEventListener("click", resetCalculator);

      // Initial sync and calculation
      syncIncomeFromSlider();
      calculateUtahTax();
    </script>

