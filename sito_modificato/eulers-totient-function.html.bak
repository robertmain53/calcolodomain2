    <script>
      (function () {
        function $(id) {
          return document.getElementById(id);
        }
        function show(el) {
          el && el.classList.remove("hidden");
        }
        function hide(el) {
          el && el.classList.add("hidden");
        }

        // Mobile menu
        var toggle = document.getElementById("mobile-menu-toggle");
        var mobileMenu = document.getElementById("mobile-menu");
        if (toggle && mobileMenu) {
          toggle.addEventListener("click", function () {
            var isHidden = mobileMenu.classList.contains("hidden");
            if (isHidden) {
              mobileMenu.classList.remove("hidden");
              toggle.setAttribute("aria-expanded", "true");
            } else {
              mobileMenu.classList.add("hidden");
              toggle.setAttribute("aria-expanded", "false");
            }
          });
        }

        function toInt(value) {
          if (typeof value === "number") return Math.floor(value);
          var v = String(value || "").trim();
          if (!v) return NaN;
          var n = Number(v);
          if (!Number.isFinite(n)) return NaN;
          return Math.floor(n);
        }

        // GCD
        function gcd(a, b) {
          a = Math.abs(a);
          b = Math.abs(b);
          if (!a) return b;
          if (!b) return a;
          while (b) {
            var t = b;
            b = a % b;
            a = t;
          }
          return a;
        }

        // Prime factorization by trial division
        function factorize(n) {
          var factors = [];
          var d = 2;
          var count;
          while (d * d <= n) {
            count = 0;
            while (n % d === 0) {
              n = n / d;
              count++;
            }
            if (count > 0) {
              factors.push({ p: d, e: count });
            }
            d = d === 2 ? 3 : d + 2; // skip even numbers after 2
          }
          if (n > 1) {
            factors.push({ p: n, e: 1 });
          }
          return factors;
        }

        function computePhiFromFactors(n, factors) {
          if (n === 1) return 1;
          var result = n;
          for (var i = 0; i < factors.length; i++) {
            var p = factors[i].p;
            result = (result / p) * (p - 1);
          }
          return Math.round(result);
        }

        function formatFactorization(factors) {
          if (!factors || !factors.length) return "1";
          return factors
            .map(function (fe) {
              if (fe.e === 1) return fe.p.toString();
              return fe.p + "<sup>" + fe.e + "</sup>";
            })
            .join(" · ");
        }

        function formatPhiFormula(n, factors, phi) {
          if (n === 1) {
            return "By convention, \\(\\varphi(1) = 1\\).";
          }
          if (!factors.length) {
            return "No prime factors found (n = 1), so \\(\\varphi(1) = 1\\).";
          }

          var distinctPrimes = factors.map(function (fe) {
            return fe.p;
          });
          var productParts = distinctPrimes.map(function (p) {
            return "\\left(1 - \\frac{1}{" + p + "}\\right)";
          });

          var latex =
            "\\[\\varphi(" +
            n +
            ") = " +
            n +
            " \\cdot " +
            productParts.join("\\,") +
            " = " +
            phi +
            ".\\]";

          var partsHtml = distinctPrimes
            .map(function (p) {
              return "(1 − 1/" + p + ")";
            })
            .join(" · ");

          return (
            "Using the prime factorization and the identity " +
            "\\(\\varphi(n) = n \\prod_{p \\mid n} (1 - 1/p)\\), we get:" +
            "<br />" +
            latex +
            "<br />In words: φ(" +
            n +
            ") = " +
            n +
            " × " +
            partsHtml +
            " = <strong>" +
            phi +
            "</strong>."
          );
        }

        function classifyN(n, factors, phi) {
          if (n === 1) {
            return "φ(1) = 1 by convention. Every integer ≤ 1 (namely 1 itself) is coprime to 1.";
          }
          if (factors.length === 1 && factors[0].e === 1) {
            return (
              "n is prime, so all integers 1, …, n−1 are coprime with n and φ(n) = n − 1 = " +
              phi +
              "."
            );
          }
          if (factors.length === 1 && factors[0].e > 1) {
            var p = factors[0].p;
            return (
              "n = " +
              p +
              "<sup>" +
              factors[0].e +
              "</sup> is a prime power. The general formula gives φ(n) = " +
              p +
              "<sup>" +
              factors[0].e +
              "</sup> − " +
              p +
              "<sup>" +
              (factors[0].e - 1) +
              "</sup> = " +
              phi +
              "."
            );
          }
          var msg =
            "n has at least two distinct prime factors, so φ(n) captures how many integers are simultaneously coprime to all of them.";
          msg +=
            " Here φ(" +
            n +
            ") = " +
            phi +
            " integers in {1, …, " +
            n +
            "} are coprime to n.";
          return msg;
        }

        function listCoprimes(n, phi, coprimes) {
          if (!coprimes.length) {
            return (
              "No integers in {1, …, " +
              n +
              "} are coprime to " +
              n +
              " (this only happens for n = 0, which we do not consider)."
            );
          }
          var html = '<div class="mt-1 flex flex-wrap gap-1">';
          for (var i = 0; i < coprimes.length; i++) {
            html += '<span class="vector-pill">' + coprimes[i] + "</span>";
          }
          html += "</div>";
          return html;
        }

        var form = $("phi-form");
        var nInput = $("phi-n");
        var showCoprimesCheckbox = $("phi-show-coprimes");
        var errorBox = $("phi-error");
        var resultsBox = $("phi-results");
        var mainOut = $("phi-result-main");
        var stateOut = $("phi-result-state");
        var factOut = $("phi-factorization");
        var formulaOut = $("phi-formula");
        var specialOut = $("phi-special");
        var coprimesBlock = $("phi-coprimes-block");
        var coprimesSummary = $("phi-coprimes-summary");
        var coprimesList = $("phi-coprimes-list");
        var resetBtn = $("phi-reset");

        function handleSubmit(e) {
          e.preventDefault();
          hide(errorBox);
          hide(coprimesBlock);
          mainOut.textContent = "";
          stateOut.textContent = "";
          factOut.textContent = "";
          formulaOut.textContent = "";
          specialOut.textContent = "";
          coprimesSummary.textContent = "";
          coprimesList.textContent = "";
          hide(resultsBox);

          var n = toInt(nInput.value);
          if (!Number.isFinite(n) || n < 1) {
            errorBox.textContent = "Please enter a positive integer n ≥ 1.";
            show(errorBox);
            return;
          }

          if (n > 1e13) {
            errorBox.textContent =
              "For stability and performance, please use n ≤ 1e13.";
            show(errorBox);
            return;
          }

          var factors = factorize(n);
          var phi = computePhiFromFactors(n, factors);

          mainOut.innerHTML =
            '<span class="vector-pill">n = ' +
            n +
            "</span>" +
            '<span class="vector-pill">φ(n) = ' +
            phi +
            "</span>";

          if (n === 1) {
            stateOut.innerHTML =
              '<span class="pill-state">Special case: φ(1) is defined to be 1.</span>';
          } else if (factors.length === 1 && factors[0].e === 1) {
            stateOut.innerHTML =
              '<span class="pill-state">n is prime ⇒ φ(n) = n − 1.</span>';
          } else {
            stateOut.innerHTML =
              '<span class="pill-state">Composite n with ' +
              factors.length +
              " distinct prime factor(s).</span>";
          }

          if (factors.length) {
            factOut.innerHTML =
              "<strong>Prime factorization:</strong> n = " +
              formatFactorization(factors) +
              ".";
          } else {
            factOut.innerHTML =
              "<strong>Prime factorization:</strong> n = 1 (empty product).";
          }

          formulaOut.innerHTML =
            "<strong>Totient formula:</strong><br />" +
            formatPhiFormula(n, factors, phi);

          specialOut.textContent = classifyN(n, factors, phi);

          // Coprime listing
          if (showCoprimesCheckbox.checked) {
            if (n <= 2000) {
              var coprimes = [];
              for (var k = 1; k <= n; k++) {
                if (gcd(k, n) === 1) coprimes.push(k);
              }
              coprimesSummary.textContent =
                "There are " +
                coprimes.length +
                " integers ≤ " +
                n +
                " that are coprime to " +
                n +
                ".";
              coprimesList.innerHTML = listCoprimes(n, phi, coprimes);
              show(coprimesBlock);
            } else {
              coprimesSummary.innerHTML =
                '<span class="pill-state bad">Listing disabled:</span> n is too large to list all ' +
                phi +
                " coprime integers efficiently. Try a smaller n or uncheck the option.";
              coprimesList.innerHTML = "";
              show(coprimesBlock);
            }
          }

          show(resultsBox);

          if (window.MathJax && window.MathJax.typesetPromise) {
            window.MathJax.typesetPromise();
          }
        }

        function handleReset() {
          nInput.value = "36";
          showCoprimesCheckbox.checked = false;
          hide(errorBox);
          hide(resultsBox);
          hide(coprimesBlock);
          mainOut.textContent = "";
          stateOut.textContent = "";
          factOut.textContent = "";
          formulaOut.textContent = "";
          specialOut.textContent = "";
          coprimesSummary.textContent = "";
          coprimesList.textContent = "";
        }

        // Example chips
        document
          .querySelectorAll(".chip-button[data-phi-example]")
          .forEach(function (chip) {
            chip.addEventListener("click", function () {
              var val = chip.getAttribute("data-phi-example");
              nInput.value = val;
              form.dispatchEvent(new Event("submit"));
            });
          });

        if (form) form.addEventListener("submit", handleSubmit);
        if (resetBtn) resetBtn.addEventListener("click", handleReset);
      })();
    </script>
  
