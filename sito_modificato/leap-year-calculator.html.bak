    <script>
      // Core leap year logic
      function isLeapYear(year) {
        if (!Number.isInteger(year)) return false;
        if (year % 4 !== 0) return false;
        if (year % 100 !== 0) return true;
        if (year % 400 !== 0) return false;
        return true;
      }

      function explainLeapRule(year) {
        if (!Number.isInteger(year)) return "";
        const parts = [];
        parts.push(
          `Year ${year} ÷ 4 = ${year / 4}${
            year % 4 === 0
              ? " (no remainder)"
              : " (remainder " + (year % 4) + ")"
          }.`
        );
        if (year % 4 !== 0) {
          parts.push("Because it is not divisible by 4, it is a common year.");
          return parts.join(" ");
        }
        parts.push(
          `Year ${year} ÷ 100 = ${year / 100}${
            year % 100 === 0
              ? " (no remainder)"
              : " (remainder " + (year % 100) + ")"
          }.`
        );
        if (year % 100 !== 0) {
          parts.push(
            "It is divisible by 4 but not by 100, so it is a leap year."
          );
          return parts.join(" ");
        }
        parts.push(
          `Year ${year} ÷ 400 = ${year / 400}${
            year % 400 === 0
              ? " (no remainder)"
              : " (remainder " + (year % 400) + ")"
          }.`
        );
        if (year % 400 !== 0) {
          parts.push(
            "It is divisible by 100 but not by 400, so it is a common year."
          );
        } else {
          parts.push("It is divisible by 400, so it is a leap year.");
        }
        return parts.join(" ");
      }

      document.addEventListener("DOMContentLoaded", () => {
        // Single year checker
        const yearInput = document.getElementById("single-year-input");
        const checkYearBtn = document.getElementById("check-year-btn");
        const yearResult = document.getElementById("single-year-result");
        const yearRule = document.getElementById("single-year-rule");

        function handleYearCheck() {
          const val = yearInput.value.trim();
          if (val === "") {
            yearResult.textContent = "Please enter a year.";
            yearResult.classList.remove("text-green-700", "text-red-700");
            yearResult.classList.add("text-gray-800");
            yearRule.textContent = "";
            return;
          }
          const year = Number(val);
          if (!Number.isFinite(year) || !Number.isInteger(year)) {
            yearResult.textContent =
              "Year must be an integer (e.g. 2024, 1900, 2000).";
            yearResult.classList.remove("text-green-700");
            yearResult.classList.add("text-red-700");
            yearRule.textContent = "";
            return;
          }
          const leap = isLeapYear(year);
          yearResult.textContent = leap
            ? `${year} is a leap year.`
            : `${year} is a common year (not a leap year).`;
          yearResult.classList.remove(
            "text-gray-800",
            "text-red-700",
            "text-green-700"
          );
          yearResult.classList.add(leap ? "text-green-700" : "text-red-700");
          yearRule.textContent = explainLeapRule(year);
        }

        checkYearBtn.addEventListener("click", handleYearCheck);
        yearInput.addEventListener("keydown", (e) => {
          if (e.key === "Enter") {
            e.preventDefault();
            handleYearCheck();
          }
        });

        // Date checker
        const dateInput = document.getElementById("date-input");
        const checkDateBtn = document.getElementById("check-date-btn");
        const dateResult = document.getElementById("date-result");
        const dateDetail = document.getElementById("date-detail");

        function getFeb29Adjustment() {
          const radios = document.querySelectorAll(
            'input[name="feb29-adjust"]'
          );
          for (const r of radios) {
            if (r.checked) return r.value;
          }
          return "feb28";
        }

        function handleDateCheck() {
          const val = dateInput.value;
          if (!val) {
            dateResult.textContent = "Please select a date.";
            dateResult.classList.remove("text-green-700", "text-red-700");
            dateResult.classList.add("text-gray-800");
            dateDetail.textContent = "";
            return;
          }
          const [yearStr, monthStr, dayStr] = val.split("-");
          const year = Number(yearStr);
          const month = Number(monthStr);
          const day = Number(dayStr);

          const leap = isLeapYear(year);
          const monthNames = [
            "January",
            "February",
            "March",
            "April",
            "May",
            "June",
            "July",
            "August",
            "September",
            "October",
            "November",
            "December",
          ];
          const dateLabel = `${monthNames[month - 1]} ${day}, ${year}`;

          if (month === 2 && day === 29) {
            if (leap) {
              dateResult.textContent = `${dateLabel} is valid because ${year} is a leap year.`;
              dateResult.classList.remove("text-red-700", "text-gray-800");
              dateResult.classList.add("text-green-700");
              dateDetail.textContent = explainLeapRule(year);
            } else {
              const adj = getFeb29Adjustment();
              const adjustedDate =
                adj === "feb28" ? `February 28, ${year}` : `March 1, ${year}`;
              dateResult.textContent = `${dateLabel} does not exist because ${year} is not a leap year.`;
              dateResult.classList.remove("text-green-700", "text-gray-800");
              dateResult.classList.add("text-red-700");
              dateDetail.textContent =
                `Using your chosen convention, February 29 is adjusted to ${adjustedDate}. ` +
                explainLeapRule(year);
            }
          } else {
            dateResult.textContent = `${dateLabel} exists in ${
              leap ? "a leap year" : "a common year"
            }.`;
            dateResult.classList.remove("text-red-700", "text-gray-800");
            dateResult.classList.add("text-green-700");
            dateDetail.textContent = leap
              ? `${year} is a leap year, so February has 29 days.`
              : `${year} is a common year, so February has 28 days.`;
          }
        }

        checkDateBtn.addEventListener("click", handleDateCheck);

        // Range listing
        const startYearInput = document.getElementById("start-year-input");
        const endYearInput = document.getElementById("end-year-input");
        const listRangeBtn = document.getElementById("list-range-btn");
        const rangeSummary = document.getElementById("range-summary");
        const rangeCount = document.getElementById("range-count");
        const rangeList = document.getElementById("range-list");

        function handleRangeList() {
          const startVal = startYearInput.value.trim();
          const endVal = endYearInput.value.trim();
          if (startVal === "" || endVal === "") {
            rangeSummary.textContent =
              "Please enter both a start and an end year.";
            rangeSummary.classList.remove("text-green-700");
            rangeSummary.classList.add("text-red-700");
            rangeCount.textContent = "";
            rangeList.textContent = "";
            return;
          }
          const startYear = Number(startVal);
          const endYear = Number(endVal);
          if (!Number.isInteger(startYear) || !Number.isInteger(endYear)) {
            rangeSummary.textContent =
              "Both start and end years must be integers.";
            rangeSummary.classList.remove("text-green-700");
            rangeSummary.classList.add("text-red-700");
            rangeCount.textContent = "";
            rangeList.textContent = "";
            return;
          }

          const step = startYear <= endYear ? 1 : -1;
          const leapYears = [];
          for (
            let y = startYear;
            step > 0 ? y <= endYear : y >= endYear;
            y += step
          ) {
            if (isLeapYear(y)) leapYears.push(y);
            // simple guard to avoid UI freeze on absurd ranges
            if (leapYears.length > 20000) break;
          }

          rangeSummary.classList.remove("text-red-700");
          rangeSummary.classList.add("text-gray-800");
          rangeSummary.textContent = `Leap years between ${startYear} and ${endYear} (inclusive):`;
          rangeCount.textContent = `Found ${leapYears.length} leap year${
            leapYears.length === 1 ? "" : "s"
          }.`;

          if (leapYears.length === 0) {
            rangeList.textContent = "None in this range.";
          } else {
            // Format in rows for readability
            const perRow = 15;
            const rows = [];
            for (let i = 0; i < leapYears.length; i += perRow) {
              rows.push(leapYears.slice(i, i + perRow).join(", "));
            }
            rangeList.textContent = rows.join("\n");
          }
        }

        listRangeBtn.addEventListener("click", handleRangeList);
      });

      // Mobile menu script (from template)
      document.addEventListener("DOMContentLoaded", () => {
        const toggle = document.getElementById("mobile-menu-toggle");
        const menu = document.getElementById("mobile-menu");
        if (toggle && menu) {
          toggle.addEventListener("click", () => {
            const isHidden = menu.classList.contains("hidden");
            menu.classList.toggle("hidden", !isHidden);
            toggle.setAttribute("aria-expanded", String(isHidden));
          });
        }
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
