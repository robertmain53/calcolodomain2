    <script>
      // Utility: parse flexible time strings like "9", "9:30", "9am", "9:30pm"
      function parseTime(str) {
        if (!str) return null;
        str = str.trim().toLowerCase();
        if (!str) return null;

        let isPM = false;
        let isAM = false;
        if (str.endsWith("am")) {
          isAM = true;
          str = str.replace("am", "").trim();
        }
        if (str.endsWith("pm")) {
          isPM = true;
          str = str.replace("pm", "").trim();
        }

        let parts = str.split(":");
        let h = parseInt(parts[0], 10);
        let m = parts.length > 1 ? parseInt(parts[1], 10) : 0;
        if (isNaN(h) || isNaN(m) || h < 0 || h > 23 || m < 0 || m > 59)
          return null;

        if (isPM && h < 12) h += 12;
        if (isAM && h === 12) h = 0;

        return h * 60 + m;
      }

      function formatHours(h) {
        if (!isFinite(h)) return "0.00";
        return h.toFixed(2);
      }

      function roundMinutes(mins, step) {
        if (!step || step <= 1) return mins;
        return Math.round(mins / step) * step;
      }

      const dayNames = [
        "Sunday",
        "Monday",
        "Tuesday",
        "Wednesday",
        "Thursday",
        "Friday",
        "Saturday",
      ];

      function buildRows() {
        const tbody = document.getElementById("timecard-body");
        tbody.innerHTML = "";
        // Default: 7 days, starting Monday
        const today = new Date();
        const base = new Date(today);
        base.setDate(base.getDate() - base.getDay() + 1); // Monday of current week
        for (let i = 0; i < 7; i++) {
          const d = new Date(base);
          d.setDate(base.getDate() + i);
          const dayName = dayNames[d.getDay()];
          const dateStr = d.toISOString().slice(0, 10);
          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="px-2 py-2 border-t border-gray-100 text-gray-700">${dayName}</td>
        <td class="px-2 py-2 border-t border-gray-100">
          <input type="date" class="w-full border-gray-300 rounded-md text-xs px-1 py-1" value="${dateStr}">
        </td>
        <td class="px-2 py-2 border-t border-gray-100">
          <input type="text" class="time-input border-gray-300 rounded-md text-xs px-1 py-1" placeholder="e.g. 9:00">
        </td>
        <td class="px-2 py-2 border-t border-gray-100">
          <input type="text" class="time-input border-gray-300 rounded-md text-xs px-1 py-1" placeholder="e.g. 17:30">
        </td>
        <td class="px-2 py-2 border-t border-gray-100">
          <div class="flex gap-1">
            <input type="number" class="border-gray-300 rounded-md text-xs px-1 py-1 w-16" placeholder="min" min="0" step="1">
          </div>
        </td>
        <td class="px-2 py-2 border-t border-gray-100 text-right text-gray-700">
          <span class="daily-hours">0.00</span>
        </td>
        <td class="px-2 py-2 border-t border-gray-100">
          <input type="text" class="w-full border-gray-300 rounded-md text-xs px-1 py-1" placeholder="Optional note">
        </td>
      `;
          tbody.appendChild(tr);
        }
      }

      function calculate() {
        const tbody = document.getElementById("timecard-body");
        const rows = Array.from(tbody.querySelectorAll("tr"));
        const roundingStep =
          parseInt(document.getElementById("rounding").value, 10) || 1;
        const otMode = document.getElementById("ot-mode").value;
        const otThreshold =
          parseFloat(document.getElementById("ot-threshold").value) || 0;
        const otMultiplier =
          parseFloat(document.getElementById("ot-multiplier").value) || 1;
        const hourlyRate =
          parseFloat(document.getElementById("hourly-rate").value) || 0;

        let dailyHours = [];
        let totalMinutes = 0;
        let daysWorked = 0;

        rows.forEach((tr, idx) => {
          const inputs = tr.querySelectorAll("input");
          const dateInput = inputs[0];
          const startInput = inputs[1];
          const endInput = inputs[2];
          const breakInput = inputs[3];

          const startMin = parseTime(startInput.value);
          const endMin = parseTime(endInput.value);
          const breakMin = parseInt(breakInput.value, 10) || 0;

          let minutesWorked = 0;
          if (startMin != null && endMin != null) {
            let endAdj = endMin;
            if (endMin < startMin) {
              endAdj += 24 * 60; // crosses midnight
            }
            minutesWorked = endAdj - startMin - breakMin;
            if (minutesWorked < 0) minutesWorked = 0;
            minutesWorked = roundMinutes(minutesWorked, roundingStep);
            const hours = minutesWorked / 60;
            tr.querySelector(".daily-hours").textContent = formatHours(hours);
            if (hours > 0) {
              daysWorked++;
            }
            dailyHours[idx] = hours;
            totalMinutes += minutesWorked;
          } else {
            tr.querySelector(".daily-hours").textContent = "0.00";
            dailyHours[idx] = 0;
          }
        });

        const totalHours = totalMinutes / 60;
        let regularHours = totalHours;
        let overtimeHours = 0;

        if (otMode === "daily" && otThreshold > 0) {
          regularHours = 0;
          overtimeHours = 0;
          dailyHours.forEach((h) => {
            const reg = Math.min(h, otThreshold);
            const ot = Math.max(0, h - otThreshold);
            regularHours += reg;
            overtimeHours += ot;
          });
        } else if (otMode === "weekly" && otThreshold > 0) {
          regularHours = Math.min(totalHours, otThreshold);
          overtimeHours = Math.max(0, totalHours - otThreshold);
        }

        const regularPay = hourlyRate * regularHours;
        const overtimePay = hourlyRate * overtimeHours * otMultiplier;
        const totalPay = regularPay + overtimePay;

        document.getElementById("total-hours").textContent =
          formatHours(totalHours);
        document.getElementById("regular-hours").textContent =
          formatHours(regularHours);
        document.getElementById("overtime-hours").textContent =
          formatHours(overtimeHours);
        document.getElementById("days-worked").textContent =
          daysWorked.toString();

        const currency = document.getElementById("currency").value || "";
        const prefix = currency ? currency + " " : "";
        document.getElementById("regular-pay").textContent =
          prefix + regularPay.toFixed(2);
        document.getElementById("overtime-pay").textContent =
          prefix + overtimePay.toFixed(2);
        document.getElementById("total-pay").textContent =
          prefix + totalPay.toFixed(2);
      }

      function clearAll() {
        buildRows();
        document.getElementById("total-hours").textContent = "0.00";
        document.getElementById("regular-hours").textContent = "0.00";
        document.getElementById("overtime-hours").textContent = "0.00";
        document.getElementById("days-worked").textContent = "0";
        document.getElementById("regular-pay").textContent = "0.00";
        document.getElementById("overtime-pay").textContent = "0.00";
        document.getElementById("total-pay").textContent = "0.00";
      }

      function buildSummaryText() {
        const totalHours = document.getElementById("total-hours").textContent;
        const regularHours =
          document.getElementById("regular-hours").textContent;
        const overtimeHours =
          document.getElementById("overtime-hours").textContent;
        const daysWorked = document.getElementById("days-worked").textContent;
        const regularPay = document.getElementById("regular-pay").textContent;
        const overtimePay = document.getElementById("overtime-pay").textContent;
        const totalPay = document.getElementById("total-pay").textContent;

        return [
          "Working Hours Summary",
          "----------------------",
          `Days worked: ${daysWorked}`,
          `Total hours: ${totalHours}`,
          `Regular hours: ${regularHours}`,
          `Overtime hours: ${overtimeHours}`,
          "",
          `Regular pay: ${regularPay}`,
          `Overtime pay: ${overtimePay}`,
          `Total pay: ${totalPay}`,
        ].join("\n");
      }

      function copySummary() {
        const text = buildSummaryText();
        if (navigator.clipboard && navigator.clipboard.writeText) {
          navigator.clipboard.writeText(text).catch(() => {
            const textarea = document.createElement("textarea");
            textarea.value = text;
            document.body.appendChild(textarea);
            textarea.select();
            document.execCommand("copy");
            document.body.removeChild(textarea);
          });
        } else {
          const textarea = document.createElement("textarea");
          textarea.value = text;
          document.body.appendChild(textarea);
          textarea.select();
          document.execCommand("copy");
          document.body.removeChild(textarea);
        }
      }

      function downloadCSV() {
        const tbody = document.getElementById("timecard-body");
        const rows = Array.from(tbody.querySelectorAll("tr"));
        const header = [
          "Day",
          "Date",
          "Start",
          "End",
          "Unpaid break (min)",
          "Hours",
          "Notes",
        ];
        const lines = [header.join(",")];

        rows.forEach((tr) => {
          const tds = tr.querySelectorAll("td");
          const day = tds[0].textContent.trim();
          const inputs = tr.querySelectorAll("input");
          const date = inputs[0].value || "";
          const start = inputs[1].value || "";
          const end = inputs[2].value || "";
          const brk = inputs[3].value || "";
          const hours = tr.querySelector(".daily-hours").textContent || "0.00";
          const notes = inputs[4].value || "";
          const row = [day, date, start, end, brk, hours, notes].map(
            (v) => `"${(v + "").replace(/"/g, '""')}"`
          );
          lines.push(row.join(","));
        });

        const csv = lines.join("\n");
        const blob = new Blob([csv], { type: "text/csv;charset=utf-8;" });
        const url = URL.createObjectURL(blob);
        const a = document.createElement("a");
        a.href = url;
        a.download = "working-hours-timesheet.csv";
        document.body.appendChild(a);
        a.click();
        document.body.removeChild(a);
        URL.revokeObjectURL(url);
      }

      function printPage() {
        window.print();
      }

      document.addEventListener("DOMContentLoaded", () => {
        buildRows();
        document
          .getElementById("calculate-btn")
          .addEventListener("click", calculate);
        document
          .getElementById("clear-btn")
          .addEventListener("click", clearAll);
        document.getElementById("copy-btn").addEventListener("click", () => {
          calculate();
          copySummary();
        });
        document.getElementById("csv-btn").addEventListener("click", () => {
          calculate();
          downloadCSV();
        });
        document.getElementById("print-btn").addEventListener("click", () => {
          calculate();
          printPage();
        });
      });

      // Mobile menu script (simple)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const expanded = this.getAttribute("aria-expanded") === "true";
          this.setAttribute("aria-expanded", String(!expanded));
          menu.classList.toggle("hidden");
        });
    </script>

