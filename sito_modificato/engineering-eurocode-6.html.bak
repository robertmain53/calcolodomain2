    <script>
      // Tab switching
      document.querySelectorAll(".ec6-tab").forEach(function (btn) {
        btn.addEventListener("click", function () {
          const target = this.getAttribute("data-tab");
          document.querySelectorAll(".ec6-tab").forEach((b) => {
            b.classList.remove("border-blue-600", "text-blue-600");
            b.classList.add("border-transparent", "text-gray-600");
          });
          this.classList.add("border-blue-600", "text-blue-600");
          this.classList.remove("border-transparent", "text-gray-600");

          document
            .querySelectorAll(".ec6-tab-panel")
            .forEach((p) => p.classList.add("hidden"));
          const panel = document.getElementById(target);
          if (panel) panel.classList.remove("hidden");
        });
      });

      // Helper: format number with thousand separators
      function fmt(num, decimals) {
        if (!isFinite(num)) return "-";
        return num.toLocaleString(undefined, {
          minimumFractionDigits: decimals,
          maximumFractionDigits: decimals,
        });
      }

      // 1. Axial capacity calculator
      function calcCapacity() {
        const fk = parseFloat(document.getElementById("fk").value) || 0;
        const gammaM = parseFloat(document.getElementById("gammaM").value) || 1;
        const t = parseFloat(document.getElementById("t").value) || 0;
        const leff = parseFloat(document.getElementById("leff").value) || 0;
        const phi = parseFloat(document.getElementById("phi").value) || 1;
        const gammaF = parseFloat(document.getElementById("gammaF").value) || 1;

        const fd = fk / gammaM; // N/mm²
        // Convert t and leff from mm to m, area in m², then multiply by fd (N/mm²) -> kN
        const t_m = t / 1000;
        const leff_m = leff / 1000;
        const area_m2 = t_m * leff_m;
        const fd_N_per_m2 = fd * 1e6; // 1 N/mm² = 1e6 N/m²
        let NRd_kN = (phi * fd_N_per_m2 * area_m2) / 1000; // kN
        NRd_kN = NRd_kN / gammaF;

        document.getElementById("fd-display").textContent = fmt(fd, 2);
        document.getElementById("nrd-display").textContent = fmt(NRd_kN, 0);
      }

      document
        .getElementById("capacity-calc-btn")
        .addEventListener("click", calcCapacity);
      ["fk", "gammaM", "t", "leff", "phi", "gammaF"].forEach((id) => {
        document.getElementById(id).addEventListener("input", calcCapacity);
      });

      // 2. Slenderness calculator
      function calcSlenderness() {
        const heff = parseFloat(document.getElementById("heff").value) || 0;
        const t2 = parseFloat(document.getElementById("t2").value) || 1;
        const lambdaLimit =
          parseFloat(document.getElementById("lambdaLimit").value) || 0;

        const lambda = heff / t2;
        document.getElementById("lambda-display").textContent = fmt(lambda, 2);

        const checkEl = document.getElementById("lambda-check");
        if (!isFinite(lambda) || lambda <= 0) {
          checkEl.textContent = "Invalid input";
          checkEl.className = "font-semibold text-gray-600";
          return;
        }
        if (lambda <= lambdaLimit) {
          checkEl.textContent = "OK (within limit)";
          checkEl.className = "font-semibold text-green-700";
        } else {
          checkEl.textContent = "Not OK (exceeds limit)";
          checkEl.className = "font-semibold text-red-700";
        }
      }

      document
        .getElementById("slenderness-calc-btn")
        .addEventListener("click", calcSlenderness);
      ["heff", "t2", "lambdaLimit"].forEach((id) => {
        document.getElementById(id).addEventListener("input", calcSlenderness);
      });

      // 3. Partial factor calculator
      function calcPartial() {
        const fk3 = parseFloat(document.getElementById("fk3").value) || 0;
        const gammaM3 =
          parseFloat(document.getElementById("gammaM3").value) || 1;
        const phi3 = parseFloat(document.getElementById("phi3").value) || 1;

        const fd3 = fk3 / gammaM3;
        const fd3red = fd3 * phi3;

        document.getElementById("fd3-display").textContent = fmt(fd3, 2);
        document.getElementById("fd3red-display").textContent = fmt(fd3red, 2);
      }

      document
        .getElementById("partial-calc-btn")
        .addEventListener("click", calcPartial);
      ["fk3", "gammaM3", "phi3"].forEach((id) => {
        document.getElementById(id).addEventListener("input", calcPartial);
      });

      // Initial calculations on load
      calcCapacity();
      calcSlenderness();
      calcPartial();

      // Mobile menu script (simple)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          this.setAttribute("aria-expanded", isHidden ? "true" : "false");
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
