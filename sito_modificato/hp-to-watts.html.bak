    <script>
      // HP to Watts calculator logic
      (function () {
        const modeHpToWBtn = document.getElementById("mode-hp-to-w");
        const modeWToHpBtn = document.getElementById("mode-w-to-hp");
        const hpTypeSelect = document.getElementById("hp-type");
        const hpInput = document.getElementById("hp-input");
        const wInput = document.getElementById("w-input");
        const inputHpGroup = document.getElementById("input-hp-group");
        const inputWGroup = document.getElementById("input-w-group");
        const resultMain = document.getElementById("result-main");
        const resultKw = document.getElementById("result-kw");
        const resultHp = document.getElementById("result-hp");
        const resultLabelMain = document.getElementById("result-label-main");
        const resultNote = document.getElementById("result-note");
        const swapBtn = document.getElementById("swap-btn");
        const quickTableBody = document.getElementById("quick-table-body");

        let mode = "hp-to-w"; // or 'w-to-hp'

        function getFactor() {
          const type = hpTypeSelect.value;
          if (type === "metric") return 735.49875;
          if (type === "electrical") return 746;
          return 745.699872; // mechanical default
        }

        function formatNumber(value, decimals = 2) {
          if (!isFinite(value)) return "–";
          const abs = Math.abs(value);
          if (abs !== 0 && (abs < 0.001 || abs >= 1e6)) {
            return value.toExponential(3);
          }
          return value.toLocaleString(undefined, {
            maximumFractionDigits: decimals,
            minimumFractionDigits: 0,
          });
        }

        function calculate() {
          const factor = getFactor();
          let hpVal = parseFloat(hpInput.value);
          let wVal = parseFloat(wInput.value);

          if (mode === "hp-to-w") {
            if (isNaN(hpVal)) {
              resultMain.textContent = "–";
              resultKw.textContent = "–";
              resultHp.textContent = "–";
              resultNote.textContent =
                "Enter horsepower to see the conversion.";
              return;
            }
            wVal = hpVal * factor;
            const kW = wVal / 1000;
            resultMain.textContent = formatNumber(wVal, 2) + " W";
            resultKw.textContent = formatNumber(kW, 4) + " kW";
            resultHp.textContent = formatNumber(hpVal, 4) + " hp";
            resultNote.textContent =
              "Using 1 hp = " + factor + " W for the selected horsepower type.";
          } else {
            if (isNaN(wVal)) {
              resultMain.textContent = "–";
              resultKw.textContent = "–";
              resultHp.textContent = "–";
              resultNote.textContent = "Enter watts to see the conversion.";
              return;
            }
            hpVal = wVal / factor;
            const kW = wVal / 1000;
            resultMain.textContent = formatNumber(hpVal, 4) + " hp";
            resultKw.textContent = formatNumber(kW, 4) + " kW";
            resultHp.textContent = formatNumber(wVal, 2) + " W";
            resultNote.textContent =
              "Using 1 hp = " + factor + " W for the selected horsepower type.";
          }
        }

        function setMode(newMode) {
          mode = newMode;
          if (mode === "hp-to-w") {
            modeHpToWBtn.classList.add("bg-blue-600", "text-white");
            modeHpToWBtn.classList.remove("bg-white", "text-gray-700");
            modeWToHpBtn.classList.remove("bg-blue-600", "text-white");
            modeWToHpBtn.classList.add("bg-white", "text-gray-700");
            inputHpGroup.classList.remove("hidden");
            inputWGroup.classList.add("hidden");
            resultLabelMain.textContent = "Power in watts (W)";
            resultNote.textContent = "Enter horsepower to see the conversion.";
          } else {
            modeWToHpBtn.classList.add("bg-blue-600", "text-white");
            modeWToHpBtn.classList.remove("bg-white", "text-gray-700");
            modeHpToWBtn.classList.remove("bg-blue-600", "text-white");
            modeHpToWBtn.classList.add("bg-white", "text-gray-700");
            inputHpGroup.classList.add("hidden");
            inputWGroup.classList.remove("hidden");
            resultLabelMain.textContent = "Power in horsepower (hp)";
            resultNote.textContent = "Enter watts to see the conversion.";
          }
          calculate();
        }

        function swapValues() {
          if (mode === "hp-to-w") {
            // If we already have a computed W, move it to W input and switch mode
            const factor = getFactor();
            const hpVal = parseFloat(hpInput.value);
            if (!isNaN(hpVal)) {
              const wVal = hpVal * factor;
              wInput.value = wVal;
            }
            setMode("w-to-hp");
          } else {
            const factor = getFactor();
            const wVal = parseFloat(wInput.value);
            if (!isNaN(wVal)) {
              const hpVal = wVal / factor;
              hpInput.value = hpVal;
            }
            setMode("hp-to-w");
          }
          calculate();
        }

        function buildQuickTable() {
          const factor = 745.699872; // mechanical hp
          const rows = [0.5, 1, 2, 3, 5, 7.5, 10, 15, 20];
          quickTableBody.innerHTML = "";
          rows.forEach((hp) => {
            const w = hp * factor;
            const kW = w / 1000;
            const tr = document.createElement("tr");
            tr.innerHTML = `
          <td class="px-3 py-1.5">${formatNumber(hp, 2)} hp</td>
          <td class="px-3 py-1.5">${formatNumber(w, 1)} W</td>
          <td class="px-3 py-1.5">${formatNumber(kW, 3)} kW</td>
        `;
            quickTableBody.appendChild(tr);
          });
        }

        modeHpToWBtn.addEventListener("click", () => setMode("hp-to-w"));
        modeWToHpBtn.addEventListener("click", () => setMode("w-to-hp"));
        hpTypeSelect.addEventListener("change", calculate);
        hpInput.addEventListener("input", calculate);
        wInput.addEventListener("input", calculate);
        swapBtn.addEventListener("click", swapValues);

        buildQuickTable();
        calculate();
      })();
    </script>

