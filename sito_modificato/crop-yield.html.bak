    <script>
      // Utility functions
      function toNumber(value) {
        const n = parseFloat(value);
        return isNaN(n) ? null : n;
      }

      function getBushelWeight(crop, customValue) {
        const defaults = {
          corn: 56,
          soybean: 60,
          wheat: 60,
          barley: 48,
          canola: 50,
        };
        if (crop === "custom") {
          const v = toNumber(customValue);
          return v && v > 0 ? v : null;
        }
        return defaults[crop] || 56;
      }

      function correctForMoisture(weight, moisture, targetMoisture) {
        const m = toNumber(moisture);
        const t = toNumber(targetMoisture);
        if (m === null || t === null)
          return {
            weightCorrected: weight,
            note: "No moisture correction applied (assuming sample is at target moisture).",
          };
        if (m <= 0 || t <= 0 || m >= 100 || t >= 100 || m === t) {
          return {
            weightCorrected: weight,
            note: "Moisture values invalid or equal; no correction applied.",
          };
        }
        const corrected = (weight * (100 - m)) / (100 - t);
        const note =
          "Weight corrected from " +
          m.toFixed(1) +
          "% to " +
          t.toFixed(1) +
          "% moisture.";
        return { weightCorrected: corrected, note: note };
      }

      function formatNumber(value, decimals) {
        return value.toLocaleString(undefined, {
          maximumFractionDigits: decimals,
          minimumFractionDigits: 0,
        });
      }

      function convertWeightToLb(weight, unit) {
        if (unit === "lb") return weight;
        if (unit === "kg") return weight * 2.2046226218;
        if (unit === "g") return weight * 0.0022046226;
        return weight;
      }

      function convertLengthToFt(length, unit) {
        if (unit === "ft") return length;
        if (unit === "m") return length * 3.280839895;
        return length;
      }

      function convertSpacingToFt(spacing, unit) {
        if (unit === "in") return spacing / 12;
        if (unit === "cm") return spacing / 30.48;
        return spacing;
      }

      function convertLengthToM(length, unit) {
        if (unit === "m") return length;
        if (unit === "ft") return length * 0.3048;
        return length;
      }

      function computeYield(
        weightCorrectedLb,
        sampleAreaFt2,
        outputUnit,
        bushelWeightLb
      ) {
        const ACRE_FT2 = 43560;
        const HA_M2 = 10000;
        const FT2_PER_M2 = 10.7639104167;

        const scaleToAcre = ACRE_FT2 / sampleAreaFt2;
        const yieldLbPerAcre = weightCorrectedLb * scaleToAcre;

        if (outputUnit === "lb_ac") {
          return {
            main: yieldLbPerAcre,
            mainLabel: "lb/ac",
            alt:
              (yieldLbPerAcre / 2.2046226218 / (ACRE_FT2 / FT2_PER_M2)) * HA_M2,
            altLabel: "kg/ha",
          };
        }

        if (outputUnit === "bu_ac") {
          const buPerAcre = yieldLbPerAcre / bushelWeightLb;
          const kgPerHa =
            (yieldLbPerAcre / 2.2046226218 / (ACRE_FT2 / FT2_PER_M2)) * HA_M2;
          return {
            main: buPerAcre,
            mainLabel: "bu/ac",
            alt: kgPerHa,
            altLabel: "kg/ha",
          };
        }

        // Convert via kg/ha as base
        const kgPerHa =
          (yieldLbPerAcre / 2.2046226218 / (ACRE_FT2 / FT2_PER_M2)) * HA_M2;

        if (outputUnit === "kg_ha") {
          return {
            main: kgPerHa,
            mainLabel: "kg/ha",
            alt: kgPerHa / 1000,
            altLabel: "t/ha",
          };
        }

        if (outputUnit === "t_ha") {
          return {
            main: kgPerHa / 1000,
            mainLabel: "t/ha",
            alt: kgPerHa,
            altLabel: "kg/ha",
          };
        }

        return {
          main: yieldLbPerAcre,
          mainLabel: "lb/ac",
          alt: kgPerHa,
          altLabel: "kg/ha",
        };
      }

      // Tab switching
      const tabRow = document.getElementById("tab-row");
      const tabArea = document.getElementById("tab-area");
      const panelRow = document.getElementById("panel-row");
      const panelArea = document.getElementById("panel-area");

      function activateTab(tab) {
        if (tab === "row") {
          tabRow.classList.add("tab-active");
          tabArea.classList.remove("tab-active");
          panelRow.classList.remove("hidden");
          panelArea.classList.add("hidden");
        } else {
          tabArea.classList.add("tab-active");
          tabRow.classList.remove("tab-active");
          panelArea.classList.remove("hidden");
          panelRow.classList.add("hidden");
        }
      }

      tabRow.addEventListener("click", () => activateTab("row"));
      tabArea.addEventListener("click", () => activateTab("area"));

      // Show/hide custom bushel weight
      const rowCrop = document.getElementById("row-crop");
      const rowCustomBushelWrapper = document.getElementById(
        "row-custom-bushel-wrapper"
      );
      const areaCrop = document.getElementById("area-crop");
      const areaCustomBushelWrapper = document.getElementById(
        "area-custom-bushel-wrapper"
      );

      rowCrop.addEventListener("change", () => {
        if (rowCrop.value === "custom") {
          rowCustomBushelWrapper.classList.remove("hidden");
        } else {
          rowCustomBushelWrapper.classList.add("hidden");
        }
      });

      areaCrop.addEventListener("change", () => {
        if (areaCrop.value === "custom") {
          areaCustomBushelWrapper.classList.remove("hidden");
        } else {
          areaCustomBushelWrapper.classList.add("hidden");
        }
      });

      // Row-based calculation
      document.getElementById("row-calc-btn").addEventListener("click", () => {
        const spacing = toNumber(document.getElementById("row-spacing").value);
        const spacingUnit = document.getElementById("row-spacing-unit").value;
        const length = toNumber(document.getElementById("row-length").value);
        const lengthUnit = document.getElementById("row-length-unit").value;
        const weight = toNumber(document.getElementById("row-weight").value);
        const weightUnit = document.getElementById("row-weight-unit").value;
        const moisture = document.getElementById("row-moisture").value;
        const targetMoisture = document.getElementById(
          "row-target-moisture"
        ).value;
        const outputUnit = document.getElementById("row-output-unit").value;
        const crop = document.getElementById("row-crop").value;
        const customBushel = document.getElementById("row-custom-bushel").value;
        const errorEl = document.getElementById("row-error");
        const resultsEl = document.getElementById("row-results");

        errorEl.textContent = "";
        resultsEl.classList.add("hidden");

        if (
          spacing === null ||
          spacing <= 0 ||
          length === null ||
          length <= 0 ||
          weight === null ||
          weight <= 0
        ) {
          errorEl.textContent =
            "Please enter positive values for row spacing, row length, and harvest weight.";
          return;
        }

        const spacingFt = convertSpacingToFt(spacing, spacingUnit);
        const lengthFt = convertLengthToFt(length, lengthUnit);
        const sampleAreaFt2 = spacingFt * lengthFt;

        if (!sampleAreaFt2 || sampleAreaFt2 <= 0) {
          errorEl.textContent =
            "Sample area is invalid. Check your row spacing and length.";
          return;
        }

        const weightLb = convertWeightToLb(weight, weightUnit);
        const { weightCorrected, note } = correctForMoisture(
          weightLb,
          moisture,
          targetMoisture
        );

        const bushelWeight = getBushelWeight(crop, customBushel);
        if (outputUnit === "bu_ac" && (!bushelWeight || bushelWeight <= 0)) {
          errorEl.textContent =
            "Please provide a valid bushel weight for the selected crop.";
          return;
        }

        const yieldData = computeYield(
          weightCorrected,
          sampleAreaFt2,
          outputUnit,
          bushelWeight || 56
        );

        document.getElementById("row-yield-main").textContent =
          formatNumber(yieldData.main, 2) + " " + yieldData.mainLabel;
        document.getElementById("row-yield-alt").textContent =
          formatNumber(yieldData.alt, 2) + " " + yieldData.altLabel;
        document.getElementById("row-moisture-note").textContent = note;

        document.getElementById("row-sample-area").textContent =
          formatNumber(sampleAreaFt2, 2) + " ft²";
        const scaleFactor = 43560 / sampleAreaFt2;
        document.getElementById("row-scale-factor").textContent =
          "× " + formatNumber(scaleFactor, 1) + " (to reach 1 acre)";

        resultsEl.classList.remove("hidden");
      });

      document.getElementById("row-reset-btn").addEventListener("click", () => {
        [
          "row-spacing",
          "row-length",
          "row-weight",
          "row-moisture",
          "row-custom-bushel",
        ].forEach((id) => {
          document.getElementById(id).value = "";
        });
        document.getElementById("row-target-moisture").value = 15;
        document.getElementById("row-spacing-unit").value = "in";
        document.getElementById("row-length-unit").value = "ft";
        document.getElementById("row-weight-unit").value = "lb";
        document.getElementById("row-output-unit").value = "bu_ac";
        document.getElementById("row-crop").value = "corn";
        rowCustomBushelWrapper.classList.add("hidden");
        document.getElementById("row-error").textContent = "";
        document.getElementById("row-results").classList.add("hidden");
      });

      // Area-based calculation
      document.getElementById("area-calc-btn").addEventListener("click", () => {
        const width = toNumber(document.getElementById("area-width").value);
        const widthUnit = document.getElementById("area-width-unit").value;
        const length = toNumber(document.getElementById("area-length").value);
        const lengthUnit = document.getElementById("area-length-unit").value;
        const weight = toNumber(document.getElementById("area-weight").value);
        const weightUnit = document.getElementById("area-weight-unit").value;
        const moisture = document.getElementById("area-moisture").value;
        const targetMoisture = document.getElementById(
          "area-target-moisture"
        ).value;
        const outputUnit = document.getElementById("area-output-unit").value;
        const crop = document.getElementById("area-crop").value;
        const customBushel =
          document.getElementById("area-custom-bushel").value;
        const errorEl = document.getElementById("area-error");
        const resultsEl = document.getElementById("area-results");

        errorEl.textContent = "";
        resultsEl.classList.add("hidden");

        if (
          width === null ||
          width <= 0 ||
          length === null ||
          length <= 0 ||
          weight === null ||
          weight <= 0
        ) {
          errorEl.textContent =
            "Please enter positive values for sample width, length, and harvest weight.";
          return;
        }

        // Compute area in ft² for scaling to acre
        const widthFt = convertLengthToFt(width, widthUnit);
        const lengthFt = convertLengthToFt(length, lengthUnit);
        const sampleAreaFt2 = widthFt * lengthFt;

        if (!sampleAreaFt2 || sampleAreaFt2 <= 0) {
          errorEl.textContent =
            "Sample area is invalid. Check your dimensions.";
          return;
        }

        const weightLb = convertWeightToLb(weight, weightUnit);
        const { weightCorrected, note } = correctForMoisture(
          weightLb,
          moisture,
          targetMoisture
        );

        const bushelWeight = getBushelWeight(crop, customBushel);
        if (outputUnit === "bu_ac" && (!bushelWeight || bushelWeight <= 0)) {
          errorEl.textContent =
            "Please provide a valid bushel weight for the selected crop.";
          return;
        }

        const yieldData = computeYield(
          weightCorrected,
          sampleAreaFt2,
          outputUnit,
          bushelWeight || 56
        );

        document.getElementById("area-yield-main").textContent =
          formatNumber(yieldData.main, 2) + " " + yieldData.mainLabel;
        document.getElementById("area-yield-alt").textContent =
          formatNumber(yieldData.alt, 2) + " " + yieldData.altLabel;
        document.getElementById("area-moisture-note").textContent = note;

        document.getElementById("area-sample-area").textContent =
          formatNumber(sampleAreaFt2, 2) + " ft²";
        const scaleFactor = 43560 / sampleAreaFt2;
        document.getElementById("area-scale-factor").textContent =
          "× " + formatNumber(scaleFactor, 1) + " (to reach 1 acre)";

        resultsEl.classList.remove("hidden");
      });

      document
        .getElementById("area-reset-btn")
        .addEventListener("click", () => {
          [
            "area-width",
            "area-length",
            "area-weight",
            "area-moisture",
            "area-custom-bushel",
          ].forEach((id) => {
            document.getElementById(id).value = "";
          });
          document.getElementById("area-target-moisture").value = 15;
          document.getElementById("area-width-unit").value = "ft";
          document.getElementById("area-length-unit").value = "ft";
          document.getElementById("area-weight-unit").value = "lb";
          document.getElementById("area-output-unit").value = "bu_ac";
          document.getElementById("area-crop").value = "corn";
          areaCustomBushelWrapper.classList.add("hidden");
          document.getElementById("area-error").textContent = "";
          document.getElementById("area-results").classList.add("hidden");
        });

      // Initialize default tab
      activateTab("row");

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
