        document.addEventListener('DOMContentLoaded', function() {
            const form = document.getElementById('conversion-form');
            const roodInput = document.getElementById('roodInput');
            const resultValue = document.getElementById('resultValue');

            form.addEventListener('submit', function(event) {
                event.preventDefault();
                const roods = parseFloat(roodInput.value);
                if (!isNaN(roods) && roods >= 0) {
                    const acres = roods * 0.25;
                    resultValue.textContent = acres.toFixed(4);
                } else {
                    resultValue.textContent = 'Invalid input';
                }
            });
        });
    </script>
