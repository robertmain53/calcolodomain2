    <script>
      // Utility: format currency
      function formatCurrency(value, currencyCode) {
        if (!isFinite(value)) return "–";
        try {
          return new Intl.NumberFormat(undefined, {
            style: "currency",
            currency: currencyCode || "USD",
            maximumFractionDigits: 0,
          }).format(value);
        } catch (e) {
          return value.toLocaleString(undefined, { maximumFractionDigits: 0 });
        }
      }

      function getAreaUnitLabel() {
        const unit = document.getElementById("areaUnit").value;
        return unit === "sqm" ? "sq m" : "sq ft";
      }

      function getQualityFactor(quality) {
        switch (quality) {
          case "economy":
            return 0.85;
          case "standard":
            return 1.0;
          case "premium":
            return 1.2;
          case "luxury":
            return 1.4;
          default:
            return 1.0;
        }
      }

      function suggestBaseCost() {
        const projectType = document.getElementById("projectType").value;
        const quality = document.getElementById("quality").value;
        const unit = document.getElementById("areaUnit").value;
        const baseCostInput = document.getElementById("baseCost");
        const hint = document.getElementById("baseCostHint");

        let low, high, text;

        if (projectType === "residential") {
          if (quality === "economy") {
            low = 120;
            high = 180;
          } else if (quality === "standard") {
            low = 180;
            high = 260;
          } else if (quality === "premium") {
            low = 260;
            high = 350;
          } else {
            low = 350;
            high = 500;
          }
          text = `Suggested: $${low.toLocaleString()}–$${high.toLocaleString()} / sq ft for ${quality} residential.`;
        } else if (projectType === "commercial") {
          if (quality === "economy") {
            low = 140;
            high = 200;
          } else if (quality === "standard") {
            low = 180;
            high = 260;
          } else if (quality === "premium") {
            low = 240;
            high = 340;
          } else {
            low = 320;
            high = 450;
          }
          text = `Suggested: $${low.toLocaleString()}–$${high.toLocaleString()} / sq ft for ${quality} commercial.`;
        } else {
          // renovation
          if (quality === "economy") {
            low = 60;
            high = 120;
          } else if (quality === "standard") {
            low = 100;
            high = 180;
          } else if (quality === "premium") {
            low = 160;
            high = 260;
          } else {
            low = 220;
            high = 350;
          }
          text = `Suggested: $${low.toLocaleString()}–$${high.toLocaleString()} / sq ft for ${quality} renovation.`;
        }

        // Convert to per sq m if needed (1 sq m ≈ 10.7639 sq ft)
        if (unit === "sqm") {
          const factor = 10.7639;
          low = Math.round(low * factor);
          high = Math.round(high * factor);
          text = text.replace(
            /\/ sq ft.*/,
            `/ sq m for ${quality} ${
              projectType === "residential" ? "residential" : projectType
            }.`
          );
          text = `Suggested: $${low.toLocaleString()}–$${high.toLocaleString()} / sq m for ${quality} ${
            projectType === "residential" ? "residential" : projectType
          }.`;
        }

        hint.textContent = text;

        // If base cost is empty, prefill with mid-range
        if (!baseCostInput.value) {
          baseCostInput.value = Math.round((low + high) / 2);
        }
      }

      function normalizeMaterialLabor() {
        const materialsSlider = document.getElementById("materialsPct");
        const laborSlider = document.getElementById("laborPct");
        const mLabel = document.getElementById("materialsPctLabel");
        const lLabel = document.getElementById("laborPctLabel");

        let m = parseFloat(materialsSlider.value);
        let l = parseFloat(laborSlider.value);
        if (!isFinite(m)) m = 50;
        if (!isFinite(l)) l = 50;

        const sum = m + l;
        if (sum === 0) {
          m = 50;
          l = 50;
        } else {
          m = (m / sum) * 100;
          l = 100 - m;
        }

        materialsSlider.value = Math.round(m);
        laborSlider.value = Math.round(l);
        mLabel.textContent = `${Math.round(m)}%`;
        lLabel.textContent = `${Math.round(l)}%`;
      }

      function estimateCost() {
        const area = parseFloat(document.getElementById("area").value);
        const unit = document.getElementById("areaUnit").value;
        const baseCost = parseFloat(document.getElementById("baseCost").value);
        const locationFactor =
          parseFloat(document.getElementById("locationFactor").value) || 1;
        const quality = document.getElementById("quality").value;
        const currency = document.getElementById("currency").value;

        const overheadPct =
          parseFloat(document.getElementById("overheadPct").value) || 0;
        const profitPct =
          parseFloat(document.getElementById("profitPct").value) || 0;
        const contingencyPct =
          parseFloat(document.getElementById("contingencyPct").value) || 0;

        const materialsPct =
          parseFloat(document.getElementById("materialsPct").value) || 50;
        const laborPct =
          parseFloat(document.getElementById("laborPct").value) || 50;

        const errorMsg = document.getElementById("errorMsg");

        if (
          !isFinite(area) ||
          area <= 0 ||
          !isFinite(baseCost) ||
          baseCost <= 0
        ) {
          errorMsg.classList.remove("hidden");
          return;
        }
        errorMsg.classList.add("hidden");

        const qualityFactor = getQualityFactor(quality);

        // Direct construction cost
        const directCost = area * baseCost * locationFactor * qualityFactor;

        const materialsShare = materialsPct / (materialsPct + laborPct);
        const laborShare = 1 - materialsShare;

        const materialsCost = directCost * materialsShare;
        const laborCost = directCost * laborShare;

        const overhead = directCost * (overheadPct / 100);
        const profit = directCost * (profitPct / 100);
        const contingency = directCost * (contingencyPct / 100);

        const totalCost = directCost + overhead + profit + contingency;

        const areaLabel = getAreaUnitLabel();

        // Update summary
        document.getElementById("results-section").classList.remove("hidden");
        document.getElementById("directCost").textContent = formatCurrency(
          directCost,
          currency
        );
        document.getElementById(
          "directCostPerUnit"
        ).textContent = `${formatCurrency(
          directCost / area,
          currency
        )} per ${areaLabel}`;

        document.getElementById("totalCost").textContent = formatCurrency(
          totalCost,
          currency
        );
        document.getElementById(
          "totalCostPerUnit"
        ).textContent = `${formatCurrency(
          totalCost / area,
          currency
        )} per ${areaLabel}`;

        // Breakdown table
        const tbody = document.getElementById("breakdownTableBody");
        tbody.innerHTML = "";

        const rows = [
          { label: "Materials", value: materialsCost },
          { label: "Labor", value: laborCost },
          { label: "Overhead", value: overhead },
          { label: "Profit", value: profit },
          { label: "Contingency", value: contingency },
        ];

        rows.forEach((row) => {
          const tr = document.createElement("tr");
          const share = totalCost > 0 ? (row.value / totalCost) * 100 : 0;

          tr.innerHTML = `
        <td class="px-3 py-2 text-gray-800">${row.label}</td>
        <td class="px-3 py-2 text-right text-gray-800">${formatCurrency(
          row.value,
          currency
        )}</td>
        <td class="px-3 py-2 text-right text-gray-600">${share.toFixed(1)}%</td>
      `;
          tbody.appendChild(tr);
        });

        // Calculation details
        const details = document.getElementById("calcDetails");
        details.innerHTML = `
      <p><strong>Area:</strong> ${area.toLocaleString()} ${areaLabel}</p>
      <p><strong>Base cost per ${areaLabel}:</strong> ${formatCurrency(
          baseCost,
          currency
        )}</p>
      <p><strong>Location factor:</strong> ${locationFactor.toFixed(2)}</p>
      <p><strong>Quality factor:</strong> ${qualityFactor.toFixed(2)}</p>
      <p><strong>Direct construction cost:</strong> ${formatCurrency(
        directCost,
        currency
      )}</p>
      <p><strong>Materials vs. labor:</strong> ${Math.round(
        materialsShare * 100
      )}% materials, ${Math.round(laborShare * 100)}% labor</p>
      <p><strong>Overhead:</strong> ${overheadPct.toFixed(
        1
      )}% of direct cost</p>
      <p><strong>Profit:</strong> ${profitPct.toFixed(1)}% of direct cost</p>
      <p><strong>Contingency:</strong> ${contingencyPct.toFixed(
        1
      )}% of direct cost</p>
    `;
      }

      function resetForm() {
        document.getElementById("projectType").value = "residential";
        document.getElementById("quality").value = "standard";
        document.getElementById("area").value = 2000;
        document.getElementById("areaUnit").value = "sqft";
        document.getElementById("locationFactor").value = 1.0;
        document.getElementById("currency").value = "USD";

        document.getElementById("materialsPct").value = 50;
        document.getElementById("laborPct").value = 50;
        document.getElementById("overheadPct").value = 10;
        document.getElementById("profitPct").value = 10;
        document.getElementById("contingencyPct").value = 10;

        document.getElementById("baseCost").value = "";
        suggestBaseCost();
        normalizeMaterialLabor();
        document.getElementById("results-section").classList.add("hidden");
        document.getElementById("errorMsg").classList.add("hidden");
      }

      document.addEventListener("DOMContentLoaded", () => {
        // Initialize suggestions and sliders
        suggestBaseCost();
        normalizeMaterialLabor();

        document
          .getElementById("projectType")
          .addEventListener("change", () => {
            document.getElementById("baseCost").value = "";
            suggestBaseCost();
          });
        document.getElementById("quality").addEventListener("change", () => {
          document.getElementById("baseCost").value = "";
          suggestBaseCost();
        });
        document.getElementById("areaUnit").addEventListener("change", () => {
          document.getElementById("baseCost").value = "";
          suggestBaseCost();
        });

        document
          .getElementById("materialsPct")
          .addEventListener("input", normalizeMaterialLabor);
        document
          .getElementById("laborPct")
          .addEventListener("input", normalizeMaterialLabor);

        document
          .getElementById("estimateBtn")
          .addEventListener("click", estimateCost);
        document
          .getElementById("resetBtn")
          .addEventListener("click", resetForm);

        // Allow Enter key to trigger estimate
        document.getElementById("cost-form").addEventListener("submit", (e) => {
          e.preventDefault();
          estimateCost();
        });
      });
    </script>

