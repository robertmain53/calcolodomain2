    <script>
      // Constants
      const G0 = 9.80665; // m/s^2

      const bodySelect = document.getElementById("body-select");
      const muInput = document.getElementById("mu-input");
      const r1Input = document.getElementById("r1-input");
      const r2Input = document.getElementById("r2-input");
      const r1Unit = document.getElementById("r1-unit");
      const r2Unit = document.getElementById("r2-unit");
      const dirSelect = document.getElementById("direction-select");
      const ispInput = document.getElementById("isp-input");
      const computeBtn = document.getElementById("compute-btn");
      const swapBtn = document.getElementById("swap-btn");
      const errorMsg = document.getElementById("error-msg");

      const outputs = {
        dv1: document.getElementById("dv1-output"),
        dv2: document.getElementById("dv2-output"),
        dvTotal: document.getElementById("dv-total-output"),
        tof: document.getElementById("tof-output"),
        period: document.getElementById("period-output"),
        v1: document.getElementById("v1-output"),
        v2: document.getElementById("v2-output"),
        vt1: document.getElementById("vt1-output"),
        vt2: document.getElementById("vt2-output"),
        aT: document.getElementById("aT-output"),
        eT: document.getElementById("eT-output"),
        rp: document.getElementById("rp-output"),
        ra: document.getElementById("ra-output"),
        massRatio: document.getElementById("mass-ratio-output"),
        propFrac: document.getElementById("prop-frac-output"),
      };
      const massResults = document.getElementById("mass-results");

      const svg = {
        orbit1: document.getElementById("svg-orbit1"),
        orbit2: document.getElementById("svg-orbit2"),
        transfer: document.getElementById("svg-transfer"),
        burn1: document.getElementById("svg-burn1"),
        burn2: document.getElementById("svg-burn2"),
        arrow: document.getElementById("svg-arrow"),
      };

      const presetMu = {
        earth: 3.986004418e14,
        sun: 1.32712440018e20,
        mars: 4.282837e13,
        moon: 4.9048695e12,
      };

      bodySelect.addEventListener("change", () => {
        const val = bodySelect.value;
        if (presetMu[val]) {
          muInput.value = presetMu[val];
        }
      });

      swapBtn.addEventListener("click", () => {
        const r1 = r1Input.value;
        const r2 = r2Input.value;
        const u1 = r1Unit.value;
        const u2 = r2Unit.value;
        r1Input.value = r2;
        r2Input.value = r1;
        r1Unit.value = u2;
        r2Unit.value = u1;
        errorMsg.textContent = "";
      });

      function toMeters(value, unit) {
        const v = Number(value);
        if (!isFinite(v)) return NaN;
        switch (unit) {
          case "m":
            return v;
          case "km":
            return v * 1e3;
          case "au":
            return v * 1.495978707e11;
          default:
            return NaN;
        }
      }

      function formatVelocity(v) {
        if (!isFinite(v)) return "–";
        const abs = Math.abs(v);
        const sign = v >= 0 ? "" : "−";
        if (abs >= 1000) {
          return sign + (abs / 1000).toFixed(3) + " km/s";
        }
        return sign + abs.toFixed(1) + " m/s";
      }

      function formatDistance(r) {
        if (!isFinite(r)) return "–";
        if (r >= 1e9) return (r / 1e9).toFixed(3) + " Gm";
        if (r >= 1e6) return (r / 1e6).toFixed(3) + " Mm";
        if (r >= 1e3) return (r / 1e3).toFixed(3) + " km";
        return r.toFixed(0) + " m";
      }

      function formatTime(t) {
        if (!isFinite(t)) return "–";
        if (t < 60) return t.toFixed(1) + " s";
        const minutes = t / 60;
        if (minutes < 60) return minutes.toFixed(2) + " min";
        const hours = minutes / 60;
        if (hours < 48) return hours.toFixed(2) + " h";
        const days = hours / 24;
        return days.toFixed(2) + " days";
      }

      function updateSvg(r1, r2) {
        // Simple qualitative scaling: map min/max radii to fixed pixel radii
        const minR = Math.min(r1, r2);
        const maxR = Math.max(r1, r2);
        if (!isFinite(minR) || !isFinite(maxR) || minR <= 0) return;

        const minPix = 40;
        const maxPix = 130;
        const scale = (val) => {
          if (maxR === minR) return (minPix + maxPix) / 2;
          const t = (val - minR) / (maxR - minR);
          return minPix + t * (maxPix - minPix);
        };

        const r1Pix = scale(r1);
        const r2Pix = scale(r2);
        svg.orbit1.setAttribute("r", r1Pix);
        svg.orbit2.setAttribute("r", r2Pix);

        const aT = (r1 + r2) / 2;
        const eT = Math.abs(r2 - r1) / (r1 + r2);
        const aPix = scale(aT);
        const bPix = aPix * Math.sqrt(1 - eT * eT);
        svg.transfer.setAttribute("rx", aPix);
        svg.transfer.setAttribute("ry", bPix);

        // Place burns at right (periapsis) and left (apoapsis)
        const cx = 160,
          cy = 160;
        const focusOffset = eT * aPix;
        const periX =
          cx +
          focusOffset +
          (r1 < r2 ? aPix - focusOffset : -(aPix - focusOffset));
        const apoX =
          cx +
          focusOffset +
          (r1 < r2 ? -(aPix + focusOffset) : aPix + focusOffset);

        svg.burn1.setAttribute("cx", periX);
        svg.burn1.setAttribute("cy", cy);
        svg.burn2.setAttribute("cx", apoX);
        svg.burn2.setAttribute("cy", cy);

        svg.arrow.setAttribute("x1", periX);
        svg.arrow.setAttribute("y1", cy);
        svg.arrow.setAttribute("x2", periX);
        svg.arrow.setAttribute("y2", cy - 30);
      }

      function compute() {
        errorMsg.textContent = "";
        const mu = Number(muInput.value);
        const r1 = toMeters(r1Input.value, r1Unit.value);
        const r2 = toMeters(r2Input.value, r2Unit.value);

        if (!isFinite(mu) || mu <= 0) {
          errorMsg.textContent =
            "Please enter a valid positive gravitational parameter μ.";
          return;
        }
        if (!isFinite(r1) || r1 <= 0 || !isFinite(r2) || r2 <= 0) {
          errorMsg.textContent = "Please enter valid positive orbit radii.";
          return;
        }
        if (Math.abs(r1 - r2) < 1e-6 * Math.max(r1, r2)) {
          errorMsg.textContent =
            "r₁ and r₂ must be different for a Hohmann transfer.";
          return;
        }

        // Direction check (mainly for user clarity)
        const dir = dirSelect.value;
        if (dir === "raise" && r2 <= r1) {
          errorMsg.textContent =
            'Direction set to "raise" but r₂ ≤ r₁. Adjust radii or direction.';
          return;
        }
        if (dir === "lower" && r2 >= r1) {
          errorMsg.textContent =
            'Direction set to "lower" but r₂ ≥ r₁. Adjust radii or direction.';
          return;
        }

        // Circular velocities
        const v1 = Math.sqrt(mu / r1);
        const v2 = Math.sqrt(mu / r2);

        // Transfer orbit
        const aT = (r1 + r2) / 2;
        const eT = Math.abs(r2 - r1) / (r1 + r2);
        const vT1 = Math.sqrt(mu * (2 / r1 - 1 / aT));
        const vT2 = Math.sqrt(mu * (2 / r2 - 1 / aT));

        const dv1 = vT1 - v1;
        const dv2 = v2 - vT2;
        const dvTotal = Math.abs(dv1) + Math.abs(dv2);

        const periodT = 2 * Math.PI * Math.sqrt((aT * aT * aT) / mu);
        const tof = periodT / 2;

        outputs.v1.textContent = formatVelocity(v1);
        outputs.v2.textContent = formatVelocity(v2);
        outputs.vt1.textContent = formatVelocity(vT1);
        outputs.vt2.textContent = formatVelocity(vT2);
        outputs.dv1.textContent = formatVelocity(dv1);
        outputs.dv2.textContent = formatVelocity(dv2);
        outputs.dvTotal.textContent = formatVelocity(dvTotal);
        outputs.aT.textContent = formatDistance(aT);
        outputs.eT.textContent = isFinite(eT) ? eT.toFixed(6) : "–";
        outputs.rp.textContent = formatDistance(Math.min(r1, r2));
        outputs.ra.textContent = formatDistance(Math.max(r1, r2));
        outputs.period.textContent = formatTime(periodT);
        outputs.tof.textContent = formatTime(tof);

        const isp = Number(ispInput.value);
        if (isFinite(isp) && isp > 0) {
          const massRatio = Math.exp(dvTotal / (isp * G0));
          const propFrac = 1 - 1 / massRatio;
          outputs.massRatio.textContent = massRatio.toFixed(3);
          outputs.propFrac.textContent = (propFrac * 100).toFixed(2) + " %";
          massResults.classList.remove("hidden");
        } else {
          massResults.classList.add("hidden");
        }

        updateSvg(r1, r2);
      }

      computeBtn.addEventListener("click", compute);

      // Initial SVG setup with default values
      window.addEventListener("load", () => {
        try {
          compute();
        } catch (e) {}
      });

      // MathJax config
      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
