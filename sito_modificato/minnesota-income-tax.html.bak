    <script>
      // Simplified Minnesota brackets (illustrative only, not official).
      // Structure: { status: [ {limit, rate}, ... ] } where limit is upper bound of bracket.
      const MN_BRACKETS = {
        single: [
          { limit: 30000, rate: 0.0535 },
          { limit: 100000, rate: 0.068 },
          { limit: 200000, rate: 0.0785 },
          { limit: Infinity, rate: 0.0985 },
        ],
        mfj: [
          { limit: 50000, rate: 0.0535 },
          { limit: 180000, rate: 0.068 },
          { limit: 300000, rate: 0.0785 },
          { limit: Infinity, rate: 0.0985 },
        ],
        mfs: [
          { limit: 25000, rate: 0.0535 },
          { limit: 90000, rate: 0.068 },
          { limit: 150000, rate: 0.0785 },
          { limit: Infinity, rate: 0.0985 },
        ],
        hoh: [
          { limit: 40000, rate: 0.0535 },
          { limit: 130000, rate: 0.068 },
          { limit: 220000, rate: 0.0785 },
          { limit: Infinity, rate: 0.0985 },
        ],
      };

      // Rough standard deduction estimates (illustrative only)
      const MN_STD_DED = {
        single: 14000,
        mfj: 28000,
        mfs: 14000,
        hoh: 21000,
      };

      function formatMoney(value) {
        return value.toLocaleString("en-US", {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 0,
        });
      }

      function formatPercent(value) {
        if (!isFinite(value)) return "0%";
        return (value * 100).toFixed(1) + "%";
      }

      function computeMnTax(taxable, status) {
        const brackets = MN_BRACKETS[status] || MN_BRACKETS.single;
        let remaining = taxable;
        let lastLimit = 0;
        let totalTax = 0;
        const breakdown = [];

        for (const bracket of brackets) {
          if (remaining <= 0) break;
          const bracketIncome = Math.min(remaining, bracket.limit - lastLimit);
          const taxHere = bracketIncome * bracket.rate;
          breakdown.push({
            from: lastLimit,
            to: bracket.limit,
            income: bracketIncome,
            rate: bracket.rate,
            tax: taxHere,
          });
          totalTax += taxHere;
          remaining -= bracketIncome;
          lastLimit = bracket.limit;
        }

        const marginalRate =
          taxable <= 0
            ? 0
            : (
                brackets.find((b) => taxable <= b.limit) ||
                brackets[brackets.length - 1]
              ).rate;

        return { totalTax, breakdown, marginalRate };
      }

      document.addEventListener("DOMContentLoaded", () => {
        const form = document.getElementById("mnTaxForm");
        const itemizedRow = document.getElementById("itemizedRow");
        const resultsSection = document.getElementById("resultsSection");

        const resTax = document.getElementById("resTax");
        const resTakeHome = document.getElementById("resTakeHome");
        const resTaxable = document.getElementById("resTaxable");
        const resAvgRate = document.getElementById("resAvgRate");
        const resMarginalRate = document.getElementById("resMarginalRate");
        const resCredits = document.getElementById("resCredits");
        const resRefund = document.getElementById("resRefund");
        const bracketBreakdown = document.getElementById("bracketBreakdown");

        // Toggle itemized deduction input
        form.deductionType.forEach((radio) => {
          radio.addEventListener("change", () => {
            itemizedRow.classList.toggle(
              "hidden",
              form.deductionType.value !== "itemized"
            );
          });
        });

        document.getElementById("calcBtn").addEventListener("click", () => {
          const income =
            parseFloat(document.getElementById("mnIncome").value) || 0;
          const status = form.filingStatus.value;
          const deductionType = form.deductionType.value;
          const dependents = Math.max(
            0,
            parseInt(document.getElementById("mnDependents").value, 10) || 0
          );
          const withholding =
            parseFloat(document.getElementById("mnWithholding").value) || 0;

          let deductions;
          if (deductionType === "standard") {
            deductions = MN_STD_DED[status] || MN_STD_DED.single;
          } else {
            deductions =
              parseFloat(document.getElementById("mnDeductions").value) || 0;
          }

          let taxable = Math.max(0, income - deductions);
          const {
            totalTax: grossTax,
            breakdown,
            marginalRate,
          } = computeMnTax(taxable, status);

          // Simple dependent credit estimate
          const estCreditPerDependent = 150;
          let credits = dependents * estCreditPerDependent;
          credits = Math.min(credits, grossTax); // tax can't go below zero

          const netTax = Math.max(0, grossTax - credits);
          const avgRate = taxable > 0 ? netTax / taxable : 0;
          const takeHome = income - netTax;
          const refundOrOwed = withholding - netTax; // positive = refund, negative = owed

          // Update UI
          resultsSection.classList.remove("hidden");
          resTax.textContent = formatMoney(netTax);
          resTakeHome.textContent = formatMoney(takeHome);
          resTaxable.textContent = formatMoney(taxable);
          resAvgRate.textContent = formatPercent(avgRate);
          resMarginalRate.textContent = formatPercent(marginalRate);
          resCredits.textContent = formatMoney(credits);

          let refundLabel = "";
          if (Math.abs(refundOrOwed) < 1) {
            refundLabel = "$0 (about even with withholding)";
          } else if (refundOrOwed > 0) {
            refundLabel = formatMoney(refundOrOwed) + " refund (over-withheld)";
          } else {
            refundLabel = formatMoney(-refundOrOwed) + " owed (under-withheld)";
          }
          resRefund.textContent = refundLabel;

          // Bracket breakdown
          bracketBreakdown.innerHTML = "";
          if (breakdown.length === 0 || taxable === 0) {
            bracketBreakdown.textContent =
              "No Minnesota income tax is due at this income level.";
          } else {
            breakdown.forEach((b, idx) => {
              if (b.income <= 0) return;
              const div = document.createElement("div");
              div.innerHTML = `
            <div class="flex justify-between">
              <span>Bracket ${idx + 1}: ${formatMoney(b.from)} – ${
                b.to === Infinity ? "and above" : formatMoney(b.to)
              }</span>
              <span>${formatPercent(b.rate)} on ${formatMoney(
                b.income
              )} = ${formatMoney(b.tax)}</span>
            </div>
          `;
              bracketBreakdown.appendChild(div);
            });
            const totalLine = document.createElement("div");
            totalLine.className = "mt-2 font-medium";
            totalLine.textContent = `Total MN tax before credits: ${formatMoney(
              grossTax
            )}`;
            bracketBreakdown.appendChild(totalLine);
          }
        });
      });

      // MathJax config
      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
