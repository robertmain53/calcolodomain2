    <script>
      // Utility: format with units
      function formatValue(val, unit) {
        if (!isFinite(val)) return "–";
        const abs = Math.abs(val);
        let digits = 2;
        if (abs > 1000) digits = 1;
        if (abs < 1) digits = 3;
        return val.toFixed(digits) + " " + unit;
      }

      function getUnitSystem() {
        return document.querySelector('input[name="unitSystem"]:checked').value;
      }

      function updateUnitLabels() {
        const sys = getUnitSystem();
        document.getElementById("shaftDiameterUnit").textContent =
          sys === "si" ? "m" : "ft";
        document.getElementById("shaftLengthUnit").textContent =
          sys === "si" ? "m" : "ft";
        document.getElementById("rockSocketLengthUnit").textContent =
          sys === "si" ? "m" : "ft";
        document.getElementById("suUnit").textContent =
          sys === "si" ? "kPa" : "psf";
        document.getElementById("gammaEffUnit").textContent =
          sys === "si" ? "kN/m³" : "pcf";
        document.getElementById("quUnit").textContent =
          sys === "si" ? "MPa" : "ksi";
        document.getElementById("factoredCompressionLoadUnit").textContent =
          sys === "si" ? "kN" : "kips";
        document.getElementById("factoredUpliftLoadUnit").textContent =
          sys === "si" ? "kN" : "kips";
      }

      document.querySelectorAll('input[name="unitSystem"]').forEach((r) => {
        r.addEventListener("change", updateUnitLabels);
      });

      // Soil type visibility
      function updateSoilParamsVisibility() {
        const type = document.getElementById("soilType").value;
        document
          .getElementById("clayParams")
          .classList.toggle("hidden", type !== "clay");
        document
          .getElementById("sandParams")
          .classList.toggle("hidden", type !== "sand");
        document
          .getElementById("rockParams")
          .classList.toggle("hidden", type !== "rock");
      }
      document
        .getElementById("soilType")
        .addEventListener("change", updateSoilParamsVisibility);

      // Main calculation
      document.getElementById("calculateBtn").addEventListener("click", () => {
        const sys = getUnitSystem();
        const D = parseFloat(document.getElementById("shaftDiameter").value);
        const L = parseFloat(document.getElementById("shaftLength").value);
        const Lrock =
          parseFloat(document.getElementById("rockSocketLength").value) || 0;
        const FS = parseFloat(document.getElementById("fsGlobal").value);

        if (!D || !L || !FS || FS <= 0) {
          alert(
            "Please enter valid shaft diameter, length, and factor of safety."
          );
          return;
        }

        const soilType = document.getElementById("soilType").value;

        // Geometry
        const perimeter = Math.PI * D;
        const area = (Math.PI * D * D) / 4;

        let Qs = 0; // compression side
        let Qb = 0; // compression tip
        let QsUplift = 0;

        if (soilType === "clay") {
          let su = parseFloat(document.getElementById("su").value);
          let alpha = parseFloat(document.getElementById("alpha").value);
          if (!su || !alpha) {
            alert("Please enter valid clay parameters (su, alpha).");
            return;
          }

          // Units: su in kPa or psf, convert to kN/m² or kips/ft² internally
          if (sys === "si") {
            const fs = alpha * su; // kPa
            Qs = (fs * perimeter * L) / 1000; // kN (kPa * m² /1000)
            const Nc = 9;
            const qb = Nc * su; // kPa
            Qb = (qb * area) / 1000; // kN
            QsUplift = Qs; // same magnitude
          } else {
            // US: su in psf, D,L in ft, area in ft², perimeter in ft
            const fs = alpha * su; // psf
            Qs = (fs * perimeter * L) / 1000; // kips (psf*ft²/1000)
            const Nc = 9;
            const qb = Nc * su; // psf
            Qb = (qb * area) / 1000; // kips
            QsUplift = Qs;
          }
        } else if (soilType === "sand") {
          let phi = parseFloat(document.getElementById("phi").value);
          let gammaEff = parseFloat(document.getElementById("gammaEff").value);
          let beta = parseFloat(document.getElementById("beta").value);
          let Nq = parseFloat(document.getElementById("Nq").value);
          if (!phi || !gammaEff || !beta || !Nq) {
            alert(
              "Please enter valid sand parameters (phi, gamma', beta, Nq)."
            );
            return;
          }

          if (sys === "si") {
            // gammaEff in kN/m³, D,L in m
            const sigmaVavg = (gammaEff * L) / 2; // kPa (kN/m²)
            const fs = beta * sigmaVavg; // kPa
            Qs = (fs * perimeter * L) / 1000; // kN
            const sigmaVtip = gammaEff * L; // kPa
            const qb = sigmaVtip * Nq; // kPa
            Qb = (qb * area) / 1000; // kN
            QsUplift = Qs;
          } else {
            // US: gammaEff in pcf, D,L in ft
            const sigmaVavg = (gammaEff * L) / 2; // psf
            const fs = beta * sigmaVavg; // psf
            Qs = (fs * perimeter * L) / 1000; // kips
            const sigmaVtip = gammaEff * L; // psf
            const qb = sigmaVtip * Nq; // psf
            Qb = (qb * area) / 1000; // kips
            QsUplift = Qs;
          }
        } else if (soilType === "rock") {
          let qu = parseFloat(document.getElementById("qu").value);
          let alphaRock = parseFloat(
            document.getElementById("rockSideFactor").value
          );
          let Nbr = parseFloat(document.getElementById("rockTipFactor").value);
          const socketOnly =
            document.getElementById("socketOnlySide").value === "yes";
          if (!qu || !alphaRock || !Nbr) {
            alert(
              "Please enter valid rock parameters (qu, side factor, tip factor)."
            );
            return;
          }

          const Ls = Math.min(Lrock, L); // socket length cannot exceed total
          const Lside = socketOnly ? Ls : L;

          if (sys === "si") {
            // qu in MPa, convert to kPa
            const qu_kPa = qu * 1000;
            const fs = alphaRock * qu_kPa; // kPa
            Qs = (fs * perimeter * Lside) / 1000; // kN
            const qb = Nbr * qu_kPa; // kPa
            Qb = (qb * area) / 1000; // kN
            QsUplift = Qs;
          } else {
            // qu in ksi, convert to psf: 1 ksi = 1000 psi = 1000*144 psf
            const qu_psf = qu * 1000 * 144;
            const fs = alphaRock * qu_psf; // psf
            Qs = (fs * perimeter * Lside) / 1000; // kips
            const qb = Nbr * qu_psf; // psf
            Qb = (qb * area) / 1000; // kips
            QsUplift = Qs;
          }
        }

        const QultComp = Qs + Qb;
        const QallComp = QultComp / FS;

        // Self-weight of shaft (approx)
        let selfWeight = 0;
        if (sys === "si") {
          const gammaConc = 24; // kN/m³
          selfWeight = gammaConc * area * L; // kN
        } else {
          const gammaConc = 150; // pcf
          selfWeight = (gammaConc * area * L) / 1000; // kips
        }

        const QultUplift = QsUplift + selfWeight;
        const QallUplift = QultUplift / FS;

        const unitForce = sys === "si" ? "kN" : "kips";

        // Write results
        document.getElementById("QsCompText").textContent = formatValue(
          Qs,
          unitForce
        );
        document.getElementById("QbCompText").textContent = formatValue(
          Qb,
          unitForce
        );
        document.getElementById("QultCompText").textContent = formatValue(
          QultComp,
          unitForce
        );
        document.getElementById("QallCompText").textContent = formatValue(
          QallComp,
          unitForce
        );

        document.getElementById("QsUpliftText").textContent = formatValue(
          QsUplift,
          unitForce
        );
        document.getElementById("selfWeightText").textContent = formatValue(
          selfWeight,
          unitForce
        );
        document.getElementById("QultUpliftText").textContent = formatValue(
          QultUplift,
          unitForce
        );
        document.getElementById("QallUpliftText").textContent = formatValue(
          QallUplift,
          unitForce
        );

        // Utilization checks
        const factComp = parseFloat(
          document.getElementById("factoredCompressionLoad").value
        );
        const factUplift = parseFloat(
          document.getElementById("factoredUpliftLoad").value
        );

        const utilCompEl = document.getElementById("utilCompText");
        const utilUpliftEl = document.getElementById("utilUpliftText");

        if (factComp && QallComp > 0) {
          const util = factComp / QallComp;
          utilCompEl.textContent =
            "Compression utilization (factored / allowable): " +
            util.toFixed(2);
        } else {
          utilCompEl.textContent = "";
        }

        if (factUplift && QallUplift > 0) {
          const util = factUplift / QallUplift;
          utilUpliftEl.textContent =
            "Uplift utilization (factored / allowable): " + util.toFixed(2);
        } else {
          utilUpliftEl.textContent = "";
        }

        document.getElementById("resultsSection").classList.remove("hidden");
      });

      document.getElementById("resetBtn").addEventListener("click", () => {
        document.getElementById("shaftDiameter").value = 1.2;
        document.getElementById("shaftLength").value = 15;
        document.getElementById("rockSocketLength").value = 0;
        document.getElementById("fsGlobal").value = 2.5;
        document.getElementById("soilType").value = "clay";
        document.getElementById("su").value = 75;
        document.getElementById("alpha").value = 0.6;
        document.getElementById("phi").value = 35;
        document.getElementById("gammaEff").value = 10;
        document.getElementById("beta").value = 0.35;
        document.getElementById("Nq").value = 40;
        document.getElementById("qu").value = 5;
        document.getElementById("rockSideFactor").value = 0.3;
        document.getElementById("rockTipFactor").value = 0.4;
        document.getElementById("socketOnlySide").value = "yes";
        document.getElementById("factoredCompressionLoad").value = "";
        document.getElementById("factoredUpliftLoad").value = "";
        document.getElementById("resultsSection").classList.add("hidden");
        updateSoilParamsVisibility();
      });

      // Initial setup
      updateUnitLabels();
      updateSoilParamsVisibility();
    </script>

