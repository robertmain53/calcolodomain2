    <script>
      // --- Data model for room types and scopes (cost per ft², mid-range baseline) ---
      const ROOM_COSTS = {
        kitchen: {
          label: "Kitchen",
          light: { low: 60, mid: 80, high: 100 },
          mid: { low: 90, mid: 130, high: 170 },
          gut: { low: 150, mid: 225, high: 300 },
        },
        bathroom: {
          label: "Bathroom",
          light: { low: 70, mid: 90, high: 110 },
          mid: { low: 120, mid: 170, high: 220 },
          gut: { low: 180, mid: 260, high: 340 },
        },
        living: {
          label: "Living / Dining / Bedroom",
          light: { low: 15, mid: 25, high: 35 },
          mid: { low: 30, mid: 50, high: 70 },
          gut: { low: 60, mid: 90, high: 130 },
        },
        basement: {
          label: "Basement / Attic Finish",
          light: { low: 25, mid: 35, high: 45 },
          mid: { low: 40, mid: 60, high: 80 },
          gut: { low: 70, mid: 100, high: 140 },
        },
        exterior: {
          label: "Exterior / Whole‑house envelope",
          light: { low: 15, mid: 25, high: 35 },
          mid: { low: 30, mid: 45, high: 60 },
          gut: { low: 50, mid: 75, high: 110 },
        },
        custom: {
          label: "Custom / Other",
          light: { low: 15, mid: 25, high: 35 },
          mid: { low: 30, mid: 50, high: 70 },
          gut: { low: 60, mid: 90, high: 130 },
        },
      };

      const SCOPE_LABELS = {
        light: "Light refresh",
        mid: "Mid‑range remodel",
        gut: "Full gut / high‑end",
      };

      let roomIdCounter = 0;

      const currencySymbols = {
        USD: "$",
        EUR: "€",
        GBP: "£",
        AUD: "A$",
        CAD: "C$",
      };

      function getCurrencySymbol() {
        const cur = document.getElementById("currency-select").value || "USD";
        return currencySymbols[cur] || cur + " ";
      }

      function formatMoney(value) {
        const symbol = getCurrencySymbol();
        if (!isFinite(value)) return symbol + "0";
        return (
          symbol + value.toLocaleString(undefined, { maximumFractionDigits: 0 })
        );
      }

      function formatArea(area, unit) {
        if (!isFinite(area)) return "0";
        const suffix = unit === "metric" ? "m²" : "ft²";
        return (
          area.toLocaleString(undefined, { maximumFractionDigits: 0 }) +
          " " +
          suffix
        );
      }

      function qualityMultiplier() {
        const q = document.getElementById("quality-select").value;
        if (q === "basic") return 0.85;
        if (q === "high") return 1.25;
        return 1.0;
      }

      function unitMultiplierToFt2() {
        const unit = document.getElementById("unit-select").value;
        // If metric, user enters m²; convert to ft² for cost tables
        return unit === "metric" ? 10.7639 : 1.0;
      }

      function createRoomRow(defaults = {}) {
        const tbody = document.getElementById("rooms-tbody");
        const id = roomIdCounter++;
        const tr = document.createElement("tr");
        tr.dataset.roomId = id;

        const name = defaults.name || `Room ${id + 1}`;
        const type = defaults.type || "kitchen";
        const scope = defaults.scope || "mid";
        const size = defaults.size || "";

        tr.innerHTML = `
      <td class="px-2 py-2 border-t align-top">
        <input type="text" class="w-full border rounded-md px-2 py-1 text-sm room-name" value="${name}">
      </td>
      <td class="px-2 py-2 border-t align-top">
        <select class="w-full border rounded-md px-2 py-1 text-sm room-type">
          ${Object.entries(ROOM_COSTS)
            .map(
              ([key, cfg]) =>
                `<option value="${key}" ${key === type ? "selected" : ""}>${
                  cfg.label
                }</option>`
            )
            .join("")}
        </select>
      </td>
      <td class="px-2 py-2 border-t align-top text-right">
        <input type="number" min="0" step="1" class="w-24 border rounded-md px-2 py-1 text-sm inline-block text-right room-size" value="${size}">
      </td>
      <td class="px-2 py-2 border-t align-top">
        <select class="w-full border rounded-md px-2 py-1 text-sm room-scope">
          ${Object.entries(SCOPE_LABELS)
            .map(
              ([key, label]) =>
                `<option value="${key}" ${
                  key === scope ? "selected" : ""
                }>${label}</option>`
            )
            .join("")}
        </select>
      </td>
      <td class="px-2 py-2 border-t align-top text-right text-xs text-gray-600 room-base-display">–</td>
      <td class="px-2 py-2 border-t align-top text-center">
        <button type="button" class="text-red-600 text-xs hover:underline room-remove-btn">Remove</button>
      </td>
    `;
        tbody.appendChild(tr);
      }

      function resetRooms() {
        const tbody = document.getElementById("rooms-tbody");
        tbody.innerHTML = "";
        roomIdCounter = 0;
        createRoomRow({
          name: "Kitchen",
          type: "kitchen",
          scope: "mid",
          size: "",
        });
        createRoomRow({
          name: "Bathroom",
          type: "bathroom",
          scope: "mid",
          size: "",
        });
        createRoomRow({
          name: "Living Room",
          type: "living",
          scope: "light",
          size: "",
        });
      }

      function calculateEstimate() {
        const errorEl = document.getElementById("error-msg");
        errorEl.textContent = "";

        const laborShare =
          parseFloat(document.getElementById("labor-share-input").value) || 0;
        const contingencyPct =
          parseFloat(document.getElementById("contingency-input").value) || 0;
        const overheadPct =
          parseFloat(document.getElementById("overhead-input").value) || 0;
        const otherFixed =
          parseFloat(document.getElementById("custom-extra-input").value) || 0;
        const unit = document.getElementById("unit-select").value;

        const tbody = document.getElementById("rooms-tbody");
        const rows = Array.from(tbody.querySelectorAll("tr"));
        if (!rows.length) {
          errorEl.textContent = "Add at least one room or area.";
          return;
        }

        const qMult = qualityMultiplier();
        const toFt2 = unitMultiplierToFt2();

        let baseTotal = 0;
        let totalAreaUserUnit = 0;
        const roomSummaries = [];

        for (const row of rows) {
          const sizeInput = row.querySelector(".room-size");
          const typeSelect = row.querySelector(".room-type");
          const scopeSelect = row.querySelector(".room-scope");
          const baseDisplay = row.querySelector(".room-base-display");
          const nameInput = row.querySelector(".room-name");

          const areaUser = parseFloat(sizeInput.value);
          if (!areaUser || areaUser <= 0) {
            baseDisplay.textContent = "Enter size";
            continue;
          }

          const areaFt2 = areaUser * toFt2;
          const type = typeSelect.value;
          const scope = scopeSelect.value;

          const cfg = ROOM_COSTS[type] && ROOM_COSTS[type][scope];
          if (!cfg) continue;

          const midCostPerFt2 = cfg.mid * qMult;
          const lowCostPerFt2 = cfg.low * qMult;
          const highCostPerFt2 = cfg.high * qMult;

          const baseMid = areaFt2 * midCostPerFt2;
          const baseLow = areaFt2 * lowCostPerFt2;
          const baseHigh = areaFt2 * highCostPerFt2;

          baseTotal += baseMid;
          totalAreaUserUnit += areaUser;

          baseDisplay.textContent = `${formatMoney(baseLow)} – ${formatMoney(
            baseHigh
          )}`;

          roomSummaries.push({
            name: nameInput.value || ROOM_COSTS[type].label,
            areaUser,
            baseLow,
            baseMid,
            baseHigh,
          });
        }

        if (baseTotal <= 0) {
          errorEl.textContent =
            "Please enter a valid size for at least one room.";
          return;
        }

        // Range around baseTotal
        const lowTotal = baseTotal * 0.85;
        const midTotal = baseTotal;
        const highTotal = baseTotal * 1.2;

        // Materials vs labor (typical)
        const labor = midTotal * (laborShare / 100);
        const materials = midTotal - labor;

        const contingency = midTotal * (contingencyPct / 100);
        const overhead = midTotal * (overheadPct / 100);
        const totalAllIn = midTotal + contingency + overhead + otherFixed;

        // Per-unit costs
        const basePerUnit =
          totalAreaUserUnit > 0 ? midTotal / totalAreaUserUnit : 0;
        const allInPerUnit =
          totalAreaUserUnit > 0 ? totalAllIn / totalAreaUserUnit : 0;

        // Update UI
        document.getElementById("results-section").classList.remove("hidden");
        document.getElementById("total-low").textContent =
          formatMoney(lowTotal);
        document.getElementById("total-mid").textContent =
          formatMoney(totalAllIn); // show all-in as main
        document.getElementById("total-high").textContent = formatMoney(
          highTotal * (1 + (contingencyPct + overheadPct) / 100) + otherFixed
        );

        document.getElementById("materials-mid").textContent =
          formatMoney(materials);
        document.getElementById("labor-mid").textContent = formatMoney(labor);
        document.getElementById("contingency-mid").textContent =
          formatMoney(contingency);
        document.getElementById("overhead-mid").textContent =
          formatMoney(overhead);
        document.getElementById("total-mid-small").textContent =
          formatMoney(totalAllIn);

        document.getElementById("base-per-unit").textContent =
          formatMoney(basePerUnit) + " / " + (unit === "metric" ? "m²" : "ft²");
        document.getElementById("allin-per-unit").textContent =
          formatMoney(allInPerUnit) +
          " / " +
          (unit === "metric" ? "m²" : "ft²");
        document.getElementById("total-area-display").textContent = formatArea(
          totalAreaUserUnit,
          unit
        );

        // Range bar width: relative spread (low to high)
        const spread = highTotal - lowTotal;
        const maxSpread = highTotal; // simple normalization
        const widthPct =
          maxSpread > 0 ? Math.min(100, (spread / maxSpread) * 100) : 0;
        document.getElementById("range-bar-fill").style.width = widthPct + "%";

        // Room summary
        const summaryEl = document.getElementById("room-summary");
        summaryEl.innerHTML = "";
        roomSummaries.forEach((r) => {
          const div = document.createElement("div");
          div.textContent = `${r.name}: ${formatArea(
            r.areaUser,
            unit
          )} → ${formatMoney(r.baseLow)} – ${formatMoney(
            r.baseHigh
          )} (base range)`;
          summaryEl.appendChild(div);
        });
      }

      document.addEventListener("DOMContentLoaded", () => {
        resetRooms();

        document
          .getElementById("add-room-btn")
          .addEventListener("click", () => {
            createRoomRow();
          });

        document
          .getElementById("rooms-tbody")
          .addEventListener("click", (e) => {
            if (e.target.classList.contains("room-remove-btn")) {
              const row = e.target.closest("tr");
              row?.remove();
            }
          });

        document
          .getElementById("calculate-btn")
          .addEventListener("click", calculateEstimate);

        document.getElementById("reset-btn").addEventListener("click", () => {
          document.getElementById("currency-select").value = "USD";
          document.getElementById("unit-select").value = "imperial";
          document.getElementById("quality-select").value = "mid";
          document.getElementById("labor-share-input").value = 55;
          document.getElementById("contingency-input").value = 15;
          document.getElementById("overhead-input").value = 20;
          document.getElementById("custom-extra-input").value = 0;
          document.getElementById("error-msg").textContent = "";
          document.getElementById("results-section").classList.add("hidden");
          resetRooms();
        });
      });

      // MathJax config
      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
