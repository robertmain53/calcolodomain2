  <script>
    // mobile menu toggle
    const mobileToggle = document.getElementById('mobile-menu-toggle');
    const mobileMenu = document.getElementById('mobile-menu');
    if (mobileToggle && mobileMenu) {
      mobileToggle.addEventListener('click', () => {
        const isHidden = mobileMenu.classList.contains('hidden');
        mobileMenu.classList.toggle('hidden', !isHidden);
        mobileToggle.setAttribute('aria-expanded', String(isHidden));
      });
    }

    // Roman numeral logic
    const numberInput = document.getElementById('number-input');
    const numberBtn = document.getElementById('number-to-roman-btn');
    const numberError = document.getElementById('number-error');
    const numberResult = document.getElementById('number-to-roman-result');

    const romanInput = document.getElementById('roman-input');
    const romanBtn = document.getElementById('roman-to-number-btn');
    const romanError = document.getElementById('roman-error');
    const romanResult = document.getElementById('roman-to-number-result');

    const dateInput = document.getElementById('date-input');
    const dateBtn = document.getElementById('date-to-roman-btn');
    const dateResult = document.getElementById('date-roman-result');

    function numberToRoman(num) {
      const vals = [
        {v:1000, r:"M"},
        {v:900, r:"CM"},
        {v:500, r:"D"},
        {v:400, r:"CD"},
        {v:100, r:"C"},
        {v:90, r:"XC"},
        {v:50, r:"L"},
        {v:40, r:"XL"},
        {v:10, r:"X"},
        {v:9, r:"IX"},
        {v:5, r:"V"},
        {v:4, r:"IV"},
        {v:1, r:"I"}
      ];
      let res = "";
      for (let i = 0; i < vals.length; i++) {
        while (num >= vals[i].v) {
          res += vals[i].r;
          num -= vals[i].v;
        }
      }
      return res;
    }

    function romanToNumber(str) {
      const map = {I:1, V:5, X:10, L:50, C:100, D:500, M:1000};
      let total = 0;
      let prev = 0;
      for (let i = str.length - 1; i >= 0; i--) {
        const curr = map[str[i]];
        if (!curr) return NaN;
        if (curr < prev) {
          total -= curr;
        } else {
          total += curr;
        }
        prev = curr;
      }
      return total;
    }

    function isValidRoman(roman) {
      const re = /^(M{0,3})(CM|CD|D?C{0,3})(XC|XL|L?X{0,3})(IX|IV|V?I{0,3})$/;
      return re.test(roman);
    }

    numberBtn.addEventListener('click', () => {
      numberError.textContent = "";
      const val = parseInt(numberInput.value, 10);
      if (isNaN(val) || val < 1 || val > 3999) {
        numberError.textContent = "Please enter an integer between 1 and 3999.";
        numberResult.textContent = "—";
        return;
      }
      const roman = numberToRoman(val);
      numberResult.textContent = roman;
    });

    romanBtn.addEventListener('click', () => {
      romanError.textContent = "";
      const raw = romanInput.value.trim().toUpperCase();
      if (!raw) {
        romanError.textContent = "Please enter a Roman numeral.";
        romanResult.textContent = "—";
        return;
      }
      if (!isValidRoman(raw)) {
        romanError.textContent = "Invalid or non-standard Roman numeral.";
        romanResult.textContent = "—";
        return;
      }
      const num = romanToNumber(raw);
      romanResult.textContent = num;
    });

    dateBtn.addEventListener('click', () => {
      const val = dateInput.value;
      if (!val) {
        dateResult.textContent = "Please select a date.";
        return;
      }
      const parts = val.split("-");
      if (parts.length !== 3) {
        dateResult.textContent = "Invalid date.";
        return;
      }
      const year = parseInt(parts[0], 10);
      const month = parseInt(parts[1], 10);
      const day = parseInt(parts[2], 10);
      const rDay = numberToRoman(day);
      const rMonth = numberToRoman(month);
      const rYear = year <= 3999 ? numberToRoman(year) : "—";
      dateResult.textContent = rDay + " • " + rMonth + " • " + rYear;
    });

    // init with defaults
    numberBtn.click();
    romanBtn.click();
    dateBtn.click();
  </script>

