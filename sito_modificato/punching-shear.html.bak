    <script>
      function getUnits() {
        const system = document.getElementById("unit-system").value;
        if (system === "metric") {
          return {
            length: "mm",
            stress: "MPa",
            shear: "kN",
          };
        }
        return {
          length: "in",
          stress: "ksi",
          shear: "kips",
        };
      }

      function updateUnitLabels() {
        const u = getUnits();
        document.getElementById("slab-thickness-unit").textContent = u.length;
        document.getElementById("effective-depth-unit").textContent = u.length;
        document.getElementById("col-width-x-unit").textContent = u.length;
        document.getElementById("col-width-y-unit").textContent = u.length;
        document.getElementById("fc-unit").textContent = u.stress;
        document.getElementById("vu-unit").textContent = u.shear;
      }

      document
        .getElementById("unit-system")
        .addEventListener("change", function () {
          const prev = this.getAttribute("data-prev") || "us";
          const current = this.value;
          if (prev === current) {
            updateUnitLabels();
            return;
          }

          // Simple numeric conversion of existing values
          const fields = [
            "slab-thickness",
            "effective-depth",
            "col-width-x",
            "col-width-y",
          ];
          const fcField = "fc";
          const vuField = "vu";

          if (prev === "us" && current === "metric") {
            // in -> mm, ksi -> MPa, kips -> kN
            fields.forEach((id) => {
              const el = document.getElementById(id);
              const v = parseFloat(el.value);
              if (!isNaN(v)) el.value = (v * 25.4).toFixed(1);
            });
            const fcEl = document.getElementById(fcField);
            const fcVal = parseFloat(fcEl.value);
            if (!isNaN(fcVal)) fcEl.value = (fcVal * 6.89476).toFixed(2);

            const vuEl = document.getElementById(vuField);
            const vuVal = parseFloat(vuEl.value);
            if (!isNaN(vuVal)) vuEl.value = (vuVal * 4.44822).toFixed(2);
          } else if (prev === "metric" && current === "us") {
            // mm -> in, MPa -> ksi, kN -> kips
            fields.forEach((id) => {
              const el = document.getElementById(id);
              const v = parseFloat(el.value);
              if (!isNaN(v)) el.value = (v / 25.4).toFixed(2);
            });
            const fcEl = document.getElementById(fcField);
            const fcVal = parseFloat(fcEl.value);
            if (!isNaN(fcVal)) fcEl.value = (fcVal / 6.89476).toFixed(2);

            const vuEl = document.getElementById(vuField);
            const vuVal = parseFloat(vuEl.value);
            if (!isNaN(vuVal)) vuEl.value = (vuVal / 4.44822).toFixed(2);
          }

          this.setAttribute("data-prev", current);
          updateUnitLabels();
        });

      function calculatePunchingShear() {
        const errEl = document.getElementById("calc-error");
        errEl.textContent = "";
        const results = document.getElementById("results-container");
        const system = document.getElementById("unit-system").value;
        const loc = document.getElementById("column-location").value;

        function val(id) {
          return parseFloat(document.getElementById(id).value);
        }

        const h = val("slab-thickness");
        const d = val("effective-depth");
        const bcx = val("col-width-x");
        const bcy = val("col-width-y");
        const fc = val("fc");
        const phi = val("phi");
        const Vu = val("vu");
        const beta = val("beta");

        if ([h, d, bcx, bcy, fc, phi, Vu].some((v) => isNaN(v) || v <= 0)) {
          errEl.textContent =
            "Please enter positive numeric values for all required fields.";
          return;
        }
        if (d >= h) {
          errEl.textContent =
            "Effective depth d should be less than slab thickness h.";
          return;
        }

        // Critical perimeter for interior column (rectangular)
        let b0_interior = 2 * (bcx + bcy) + 4 * d;

        // Location factor for reduced perimeter
        let locFactor = 1.0;
        if (loc === "edge") locFactor = 2.0 / 3.0;
        if (loc === "corner") locFactor = 0.5;

        const b0 = b0_interior * locFactor;

        // Shear stress coefficient alpha depending on location (simplified)
        let alpha = 4.0; // interior baseline
        if (loc === "edge") alpha = 3.0;
        if (loc === "corner") alpha = 2.5;

        // Convert units to consistent base: use ksi & in for US, MPa & mm for metric
        let vc; // nominal shear stress
        let Vc; // nominal capacity
        let phiVc;
        let Vu_over_phiVc;

        if (system === "us") {
          // fc in ksi, result vc in ksi
          vc = alpha * Math.sqrt(fc); // simplified illustrative
          // Vc in kips: vc(ksi)*b0(in)*d(in)/1000
          Vc = (vc * b0 * d) / 1000.0;
          // apply beta for unbalanced moment
          Vc = Vc / beta;
          phiVc = phi * Vc;
          Vu_over_phiVc = Vu / phiVc;
        } else {
          // metric: fc in MPa, vc in MPa
          vc = alpha * Math.sqrt(fc); // simplified illustrative
          // Vc in kN: vc(MPa)*b0(mm)*d(mm)/1e6 * 1000 kN/MPa? Better: 1 MPa = 1 N/mm2
          // So vc (N/mm2) * b0(mm)*d(mm) = N; divide by 1000 to get kN
          Vc = (vc * b0 * d) / 1000.0;
          Vc = Vc / beta;
          phiVc = phi * Vc;
          Vu_over_phiVc = Vu / phiVc;
        }

        const ok = Vu <= phiVc;
        const u = getUnits();

        const html = `
      <div class="p-4 rounded-md ${
        ok
          ? "bg-green-50 border border-green-200"
          : "bg-red-50 border border-red-200"
      }">
        <div class="flex items-center justify-between mb-2">
          <h3 class="font-semibold text-lg">Design Check</h3>
          <span class="px-3 py-1 rounded-full text-xs font-semibold ${
            ok ? "result-badge-ok" : "result-badge-ng"
          }">
            ${
              ok
                ? "OK – Punching shear capacity adequate"
                : "NG – Punching shear capacity exceeded"
            }
          </span>
        </div>
        <p class="text-sm mb-1">
          <strong>Utilization:</strong>
          <span class="${ok ? "text-green-700" : "text-red-700"}">
            ${Vu_over_phiVc.toFixed(2)}
          </span>
          (V<sub>u</sub> / ϕV<sub>c</sub>)
        </p>
        <p class="text-sm mb-1">
          <strong>Factored demand V<sub>u</sub>:</strong> ${Vu.toFixed(2)} ${
          u.shear
        }
        </p>
        <p class="text-sm mb-1">
          <strong>Design capacity ϕV<sub>c</sub>:</strong> ${phiVc.toFixed(
            2
          )} ${u.shear}
        </p>
      </div>

      <div class="grid md:grid-cols-2 gap-4 mt-4">
        <div class="border rounded-md p-3">
          <h4 class="font-semibold text-sm mb-2">Geometry</h4>
          <ul class="text-xs space-y-1">
            <li>Location: <strong>${
              loc.charAt(0).toUpperCase() + loc.slice(1)
            }</strong></li>
            <li>Slab thickness h: <strong>${h.toFixed(2)} ${
          u.length
        }</strong></li>
            <li>Effective depth d: <strong>${d.toFixed(2)} ${
          u.length
        }</strong></li>
            <li>Column size: <strong>${bcx.toFixed(2)} × ${bcy.toFixed(2)} ${
          u.length
        }</strong></li>
            <li>Interior critical perimeter b<sub>0,int</sub>: <strong>${b0_interior.toFixed(
              2
            )} ${u.length}</strong></li>
            <li>Location factor: <strong>${locFactor.toFixed(2)}</strong></li>
            <li>Effective perimeter b<sub>0</sub>: <strong>${b0.toFixed(2)} ${
          u.length
        }</strong></li>
          </ul>
        </div>
        <div class="border rounded-md p-3">
          <h4 class="font-semibold text-sm mb-2">Shear Strength</h4>
          <ul class="text-xs space-y-1">
            <li>Concrete strength f'<sub>c</sub>: <strong>${fc.toFixed(2)} ${
          u.stress
        }</strong></li>
            <li>Location coefficient α: <strong>${alpha.toFixed(
              2
            )}</strong></li>
            <li>Unbalanced moment factor β<sub>c</sub>: <strong>${beta.toFixed(
              2
            )}</strong></li>
            <li>Nominal shear stress v<sub>c</sub>: <strong>${vc.toFixed(2)} ${
          u.stress
        }</strong></li>
            <li>Nominal capacity V<sub>c</sub>: <strong>${Vc.toFixed(2)} ${
          u.shear
        }</strong></li>
            <li>Strength reduction factor ϕ: <strong>${phi.toFixed(
              2
            )}</strong></li>
            <li>Design capacity ϕV<sub>c</sub>: <strong>${phiVc.toFixed(2)} ${
          u.shear
        }</strong></li>
          </ul>
        </div>
      </div>

      <p class="text-xs text-gray-500 mt-3">
        Note: This calculator uses a simplified ACI 318-style approach for educational and preliminary design purposes.
        Always verify results with the latest code provisions and detailed design checks.
      </p>
    `;

        results.innerHTML = html;
      }

      document
        .getElementById("calculate-btn")
        .addEventListener("click", calculatePunchingShear);

      document
        .getElementById("reset-btn")
        .addEventListener("click", function () {
          document.getElementById("unit-system").value = "us";
          document
            .getElementById("unit-system")
            .setAttribute("data-prev", "us");
          document.getElementById("column-location").value = "interior";
          document.getElementById("slab-thickness").value = 8;
          document.getElementById("effective-depth").value = 6.5;
          document.getElementById("col-width-x").value = 16;
          document.getElementById("col-width-y").value = 16;
          document.getElementById("fc").value = 4;
          document.getElementById("phi").value = 0.75;
          document.getElementById("vu").value = 150;
          document.getElementById("beta").value = 1.0;
          document.getElementById("calc-error").textContent = "";
          updateUnitLabels();
          document.getElementById("results-container").innerHTML = `
      <div class="p-4 rounded-md border border-dashed border-gray-300 text-gray-500 text-sm">
        Enter inputs and click <span class="font-semibold">Calculate</span> to see punching shear capacity, utilization ratio, and governing check.
      </div>
    `;
        });

      // Initialize
      document.getElementById("unit-system").setAttribute("data-prev", "us");
      updateUnitLabels();
    </script>

