    <script>
      // --- Simplified pricing model (per provider, in USD) ---
      const basePricing = {
        aws: {
          vcpuHour: 0.045, // medium-ish general purpose
          storageBlockGb: 0.1 / 30, // $0.10/GB-month -> per day approx, but we use month directly
          storageObjectGb: 0.023, // S3 standard
          egressGb: 0.09,
          dbVcpuHour: 0.06,
          dbStorageGb: 0.115 / 30,
        },
        azure: {
          vcpuHour: 0.047,
          storageBlockGb: 0.12 / 30,
          storageObjectGb: 0.02,
          egressGb: 0.087,
          dbVcpuHour: 0.065,
          dbStorageGb: 0.12 / 30,
        },
        gcp: {
          vcpuHour: 0.043,
          storageBlockGb: 0.11 / 30,
          storageObjectGb: 0.02,
          egressGb: 0.085,
          dbVcpuHour: 0.058,
          dbStorageGb: 0.115 / 30,
        },
      };

      const currencyRates = {
        USD: 1,
        EUR: 0.92,
        GBP: 0.8,
      };

      const currencySymbols = {
        USD: "$",
        EUR: "€",
        GBP: "£",
      };

      let currentProvider = "aws";

      function getInstanceVcpus(size) {
        switch (size) {
          case "small":
            return 2;
          case "medium":
            return 4;
          case "large":
            return 8;
          default:
            return 4;
        }
      }

      function getCommitmentDiscount(commitment) {
        switch (commitment) {
          case "reserved1y":
            return 0.3;
          case "reserved3y":
            return 0.5;
          case "spot":
            return 0.7;
          default:
            return 0;
        }
      }

      function getStorageTierMultiplier(tier) {
        switch (tier) {
          case "infrequent":
            return 0.6;
          case "archive":
            return 0.2;
          default:
            return 1;
        }
      }

      function getEgressMultiplier(region) {
        switch (region) {
          case "crossRegion":
            return 1.2;
          case "cdn":
            return 0.6;
          default:
            return 1;
        }
      }

      function getDbHaMultiplier(ha) {
        return ha === "multi" ? 2 : 1;
      }

      function formatCurrency(value, currency) {
        const symbol = currencySymbols[currency] || "$";
        return symbol + value.toFixed(2);
      }

      function readInputs() {
        const instancesCount =
          parseFloat(document.getElementById("instancesCount").value) || 0;
        const instanceType = document.getElementById("instanceType").value;
        const instanceHours = Math.min(
          744,
          Math.max(
            0,
            parseFloat(document.getElementById("instanceHours").value) || 0
          )
        );
        const instanceCommitment =
          document.getElementById("instanceCommitment").value;

        const blockStorageGb = Math.max(
          0,
          parseFloat(document.getElementById("blockStorageGb").value) || 0
        );
        const objectStorageGb = Math.max(
          0,
          parseFloat(document.getElementById("objectStorageGb").value) || 0
        );
        const storageRedundancy =
          document.getElementById("storageRedundancy").value;

        const egressGb = Math.max(
          0,
          parseFloat(document.getElementById("egressGb").value) || 0
        );
        const egressRegion = document.getElementById("egressRegion").value;

        const dbInstances = Math.max(
          0,
          parseFloat(document.getElementById("dbInstances").value) || 0
        );
        const dbSize = document.getElementById("dbSize").value;
        const dbStorageGb = Math.max(
          0,
          parseFloat(document.getElementById("dbStorageGb").value) || 0
        );
        const dbHa = document.getElementById("dbHa").value;

        const currency = document.getElementById("currency").value || "USD";

        return {
          instancesCount,
          instanceType,
          instanceHours,
          instanceCommitment,
          blockStorageGb,
          objectStorageGb,
          storageRedundancy,
          egressGb,
          egressRegion,
          dbInstances,
          dbSize,
          dbStorageGb,
          dbHa,
          currency,
        };
      }

      function estimateForProvider(providerKey, inputs) {
        const p = basePricing[providerKey];
        const vcpus = getInstanceVcpus(inputs.instanceType);
        const discount = getCommitmentDiscount(inputs.instanceCommitment);
        const tierMult = getStorageTierMultiplier(inputs.storageRedundancy);
        const egressMult = getEgressMultiplier(inputs.egressRegion);
        const dbVcpus = getInstanceVcpus(inputs.dbSize);
        const dbHaMult = getDbHaMultiplier(inputs.dbHa);

        const compute =
          inputs.instancesCount *
          vcpus *
          inputs.instanceHours *
          p.vcpuHour *
          (1 - discount);

        const storageBlock = inputs.blockStorageGb * p.storageBlockGb * 30; // convert back to month
        const storageObject = inputs.objectStorageGb * p.storageObjectGb;
        const storage = (storageBlock + storageObject) * tierMult;

        const network = inputs.egressGb * p.egressGb * egressMult;

        const dbCompute =
          inputs.dbInstances *
          dbHaMult *
          dbVcpus *
          inputs.instanceHours *
          p.dbVcpuHour;
        const dbStorage = inputs.dbStorageGb * p.dbStorageGb * 30;
        const db = dbCompute + dbStorage;

        const total = compute + storage + network + db;

        return {
          compute,
          storage,
          network,
          db,
          total,
          vcpus,
          dbVcpus,
          dbHaMult,
        };
      }

      function updateCalculator() {
        const inputs = readInputs();
        const rate = currencyRates[inputs.currency] || 1;

        const result = estimateForProvider(currentProvider, inputs);

        const totalConv = result.total * rate;
        const computeConv = result.compute * rate;
        const storageConv = result.storage * rate;
        const networkConv = result.network * rate;
        const dbConv = result.db * rate;

        document.getElementById("totalCost").textContent = formatCurrency(
          totalConv,
          inputs.currency
        );
        document.getElementById(
          "totalCostProvider"
        ).textContent = `Provider: ${currentProvider.toUpperCase()}, Currency: ${
          inputs.currency
        }`;

        document.getElementById("computeCost").textContent = formatCurrency(
          computeConv,
          inputs.currency
        );
        document.getElementById("storageCost").textContent = formatCurrency(
          storageConv,
          inputs.currency
        );
        document.getElementById("networkCost").textContent = formatCurrency(
          networkConv,
          inputs.currency
        );
        document.getElementById("dbCost").textContent = formatCurrency(
          dbConv,
          inputs.currency
        );

        // Effective unit costs
        const totalVcpuHours =
          inputs.instancesCount * result.vcpus * inputs.instanceHours;
        const unitCompute =
          totalVcpuHours > 0 ? computeConv / totalVcpuHours : 0;
        document.getElementById("unitCompute").textContent =
          totalVcpuHours > 0
            ? `Per vCPU‑hr: ${formatCurrency(unitCompute, inputs.currency)}`
            : "Per vCPU‑hr: –";

        const totalStorageGb = inputs.blockStorageGb + inputs.objectStorageGb;
        const unitStorage =
          totalStorageGb > 0 ? storageConv / totalStorageGb : 0;
        document.getElementById("unitStorage").textContent =
          totalStorageGb > 0
            ? `Per GB storage: ${formatCurrency(unitStorage, inputs.currency)}`
            : "Per GB storage: –";

        const unitNetwork =
          inputs.egressGb > 0 ? networkConv / inputs.egressGb : 0;
        document.getElementById("unitNetwork").textContent =
          inputs.egressGb > 0
            ? `Per GB egress: ${formatCurrency(unitNetwork, inputs.currency)}`
            : "Per GB egress: –";

        const unitDb = inputs.dbInstances > 0 ? dbConv / inputs.dbInstances : 0;
        document.getElementById("unitDb").textContent =
          inputs.dbInstances > 0
            ? `Per DB instance: ${formatCurrency(unitDb, inputs.currency)}`
            : "Per DB instance: –";

        // Comparison table
        const tbody = document.getElementById("comparisonTableBody");
        tbody.innerHTML = "";
        ["aws", "azure", "gcp"].forEach((pk) => {
          const r = estimateForProvider(pk, inputs);
          const tr = document.createElement("tr");
          tr.className = pk === currentProvider ? "bg-blue-50" : "";
          const conv = (x) => formatCurrency(x * rate, inputs.currency);
          tr.innerHTML = `
        <td class="px-3 py-2 border font-semibold">${pk.toUpperCase()}</td>
        <td class="px-3 py-2 border text-right">${conv(r.compute)}</td>
        <td class="px-3 py-2 border text-right">${conv(r.storage)}</td>
        <td class="px-3 py-2 border text-right">${conv(r.network)}</td>
        <td class="px-3 py-2 border text-right">${conv(r.db)}</td>
        <td class="px-3 py-2 border text-right font-semibold">${conv(
          r.total
        )}</td>
      `;
          tbody.appendChild(tr);
        });
      }

      function setProvider(providerKey) {
        currentProvider = providerKey;
        document.querySelectorAll(".provider-tab").forEach((btn) => {
          if (btn.dataset.provider === providerKey) {
            btn.classList.add("tab-active");
          } else {
            btn.classList.remove("tab-active");
          }
        });
        updateCalculator();
      }

      function resetDefaults() {
        document.getElementById("instancesCount").value = 3;
        document.getElementById("instanceType").value = "medium";
        document.getElementById("instanceHours").value = 730;
        document.getElementById("instanceCommitment").value = "ondemand";

        document.getElementById("blockStorageGb").value = 500;
        document.getElementById("objectStorageGb").value = 1000;
        document.getElementById("storageRedundancy").value = "standard";

        document.getElementById("egressGb").value = 2000;
        document.getElementById("egressRegion").value = "sameRegion";

        document.getElementById("dbInstances").value = 1;
        document.getElementById("dbSize").value = "medium";
        document.getElementById("dbStorageGb").value = 200;
        document.getElementById("dbHa").value = "single";

        document.getElementById("currency").value = "USD";
        setProvider("aws");
      }

      document.addEventListener("DOMContentLoaded", () => {
        document.querySelectorAll(".provider-tab").forEach((btn) => {
          btn.addEventListener("click", () =>
            setProvider(btn.dataset.provider)
          );
        });

        [
          "instancesCount",
          "instanceType",
          "instanceHours",
          "instanceCommitment",
          "blockStorageGb",
          "objectStorageGb",
          "storageRedundancy",
          "egressGb",
          "egressRegion",
          "dbInstances",
          "dbSize",
          "dbStorageGb",
          "dbHa",
          "currency",
        ].forEach((id) => {
          const el = document.getElementById(id);
          if (el) {
            el.addEventListener("input", updateCalculator);
            el.addEventListener("change", updateCalculator);
          }
        });

        document.getElementById("resetBtn").addEventListener("click", () => {
          resetDefaults();
        });

        resetDefaults();
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
