    <script>
      // Delaware tax brackets (approximate, single-like baseline; used for all statuses as DE brackets are not heavily status-dependent)
      // Each bracket: { upTo: number | null, rate: decimal }
      const DE_BRACKETS = [
        { upTo: 2000, rate: 0.0 },
        { upTo: 5000, rate: 0.022 },
        { upTo: 10000, rate: 0.039 },
        { upTo: 20000, rate: 0.048 },
        { upTo: 25000, rate: 0.052 },
        { upTo: 60000, rate: 0.0555 },
        { upTo: null, rate: 0.066 }, // top bracket
      ];

      // Approximate Wilmington wage tax rate (can be adjusted)
      const WILMINGTON_RATE = 0.015; // 1.5% placeholder

      function formatCurrency(value) {
        return (
          "$" + value.toLocaleString("en-US", { maximumFractionDigits: 0 })
        );
      }

      function formatPercent(value) {
        return (value * 100).toFixed(1) + "%";
      }

      function getStandardDeduction(status) {
        // Simple heuristic; Delaware has its own amounts & personal credits.
        switch (status) {
          case "married_joint":
            return 6500;
          case "hoh":
            return 5000;
          case "married_separate":
            return 3250;
          case "single":
          default:
            return 3250;
        }
      }

      function calculateDelawareTax(taxableIncome) {
        let remaining = Math.max(0, taxableIncome);
        let lastLimit = 0;
        let totalTax = 0;
        const breakdown = [];

        for (const bracket of DE_BRACKETS) {
          if (remaining <= 0) break;
          const upper = bracket.upTo === null ? Infinity : bracket.upTo;
          const amountInBracket = Math.max(
            0,
            Math.min(remaining, upper - lastLimit)
          );
          const taxInBracket = amountInBracket * bracket.rate;
          breakdown.push({
            range:
              bracket.upTo === null
                ? `$${lastLimit.toLocaleString()}+`
                : `$${lastLimit.toLocaleString()} – $${upper.toLocaleString()}`,
            taxable: amountInBracket,
            rate: bracket.rate,
            tax: taxInBracket,
          });
          totalTax += taxInBracket;
          remaining -= amountInBracket;
          lastLimit = upper;
        }

        return { totalTax, breakdown };
      }

      function updateBracketTable(breakdown) {
        const tbody = document.getElementById("bracket-breakdown");
        tbody.innerHTML = "";
        if (!breakdown.length) {
          const row = document.createElement("tr");
          row.innerHTML =
            '<td colspan="4" class="px-3 py-2 text-center text-gray-500">No taxable income.</td>';
          tbody.appendChild(row);
          return;
        }
        breakdown.forEach((b) => {
          const row = document.createElement("tr");
          row.className = "border-t border-gray-100";
          row.innerHTML = `
        <td class="px-3 py-2 text-gray-700">${b.range}</td>
        <td class="px-3 py-2 text-right text-gray-700">${formatCurrency(
          b.taxable
        )}</td>
        <td class="px-3 py-2 text-right text-gray-700">${formatPercent(
          b.rate
        )}</td>
        <td class="px-3 py-2 text-right text-gray-700">${formatCurrency(
          b.tax
        )}</td>
      `;
          tbody.appendChild(row);
        });
      }

      function recalc() {
        const income = Number(document.getElementById("income").value) || 0;
        const otherIncome =
          Number(document.getElementById("other-income").value) || 0;
        const preTax = Number(document.getElementById("pre-tax").value) || 0;
        const status = document.getElementById("filing-status").value;
        const wilmington = document.getElementById("wilmington-tax").checked;

        const deductionType =
          document.querySelector('input[name="deduction-type"]:checked')
            ?.value || "standard";
        let deductionsInput =
          Number(document.getElementById("deductions").value) || 0;

        if (deductionType === "standard") {
          deductionsInput = getStandardDeduction(status);
          document.getElementById("deductions").value = deductionsInput;
        }

        const grossTotal = income + otherIncome;
        const adjustedIncome = Math.max(0, grossTotal - preTax);
        const taxableIncome = Math.max(0, adjustedIncome - deductionsInput);

        const { totalTax: stateTax, breakdown } =
          calculateDelawareTax(taxableIncome);

        // Wilmington local tax approximated on earned income (assume main income field is earned)
        let localTax = 0;
        let localRateApplied = 0;
        if (wilmington) {
          localTax = Math.max(0, income) * WILMINGTON_RATE;
          localRateApplied = WILMINGTON_RATE;
        }

        const totalTax = stateTax + localTax;
        const takeHome = Math.max(0, grossTotal - totalTax);

        const stateEffectiveRate = grossTotal > 0 ? stateTax / grossTotal : 0;
        const totalEffectiveRate = grossTotal > 0 ? totalTax / grossTotal : 0;

        document.getElementById("taxable-income").textContent =
          formatCurrency(taxableIncome);
        document.getElementById("state-tax").textContent =
          formatCurrency(stateTax);
        document.getElementById("take-home").textContent =
          formatCurrency(takeHome);
        document.getElementById("local-tax").textContent =
          formatCurrency(localTax);

        document.getElementById(
          "state-effective-rate"
        ).textContent = `Effective rate: ${formatPercent(stateEffectiveRate)}`;
        document.getElementById(
          "total-effective-rate"
        ).textContent = `Total effective rate: ${formatPercent(
          totalEffectiveRate
        )}`;
        document.getElementById(
          "local-rate"
        ).textContent = `Rate applied: ${formatPercent(localRateApplied)}`;

        document.getElementById(
          "deduction-summary"
        ).textContent = `After $${deductionsInput.toLocaleString()} in deductions and $${preTax.toLocaleString()} in pre‑tax contributions`;

        updateBracketTable(breakdown);
      }

      document.addEventListener("DOMContentLoaded", () => {
        document
          .getElementById("calculate-btn")
          .addEventListener("click", recalc);
        document.getElementById("reset-btn").addEventListener("click", () => {
          document.getElementById("income").value = 75000;
          document.getElementById("filing-status").value = "single";
          document.querySelector(
            'input[name="deduction-type"][value="standard"]'
          ).checked = true;
          document.getElementById("deductions").value =
            getStandardDeduction("single");
          document.getElementById("other-income").value = 0;
          document.getElementById("pre-tax").value = 0;
          document.getElementById("wilmington-tax").checked = false;
          recalc();
        });

        // Recalculate when key inputs change for a responsive feel
        [
          "income",
          "other-income",
          "pre-tax",
          "deductions",
          "filing-status",
          "wilmington-tax",
        ].forEach((id) => {
          const el = document.getElementById(id);
          if (el) {
            el.addEventListener("input", () => {
              // avoid overriding manual deductions when user is typing itemized
              if (id === "deductions") {
                const itemizedRadio = document.querySelector(
                  'input[name="deduction-type"][value="itemized"]'
                );
                if (itemizedRadio) itemizedRadio.checked = true;
              }
              recalc();
            });
            el.addEventListener("change", recalc);
          }
        });

        document
          .querySelectorAll('input[name="deduction-type"]')
          .forEach((r) => {
            r.addEventListener("change", () => {
              if (r.value === "standard") {
                const status = document.getElementById("filing-status").value;
                document.getElementById("deductions").value =
                  getStandardDeduction(status);
              }
              recalc();
            });
          });

        // Initial calculation
        document.getElementById("deductions").value =
          getStandardDeduction("single");
        recalc();
      });

      // Mobile menu script (simple)
      document.addEventListener("DOMContentLoaded", () => {
        const toggle = document.getElementById("mobile-menu-toggle");
        const menu = document.getElementById("mobile-menu");
        if (toggle && menu) {
          toggle.addEventListener("click", () => {
            const isHidden = menu.classList.contains("hidden");
            menu.classList.toggle("hidden", !isHidden);
            toggle.setAttribute("aria-expanded", String(isHidden));
          });
        }
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
