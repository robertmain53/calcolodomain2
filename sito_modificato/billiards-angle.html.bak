    <script>
      // Coordinate system:
      // Canvas 600x300, logical table coordinates 0-100 (x), 0-50 (y)
      // We'll map logical -> canvas with padding for rails.
      (function () {
        const canvas = document.getElementById("tableCanvas");
        const ctx = canvas.getContext("2d");

        const table = {
          width: 100,
          height: 50,
          padding: 20, // pixels around playfield
        };

        let tableFeet = 8;
        let ballDiameterMm = 57.15;

        // Positions in logical units
        const state = {
          cue: { x: 25, y: 25 },
          obj: { x: 60, y: 25 },
          pocket: { x: 98, y: 25 },
          dragging: null,
        };

        const ui = {
          cutAngleDisplay: document.getElementById("cutAngleDisplay"),
          coDistDisplay: document.getElementById("coDistDisplay"),
          opDistDisplay: document.getElementById("opDistDisplay"),
          tableSizeSelect: document.getElementById("tableSizeSelect"),
          ballDiameterInput: document.getElementById("ballDiameterInput"),
          showGhostBall: document.getElementById("showGhostBall"),
          showContactPoint: document.getElementById("showContactPoint"),
          show90Rule: document.getElementById("show90Rule"),
          cueX: document.getElementById("cueX"),
          cueY: document.getElementById("cueY"),
          objX: document.getElementById("objX"),
          objY: document.getElementById("objY"),
          pocketX: document.getElementById("pocketX"),
          pocketY: document.getElementById("pocketY"),
          cueXVal: document.getElementById("cueXVal"),
          cueYVal: document.getElementById("cueYVal"),
          objXVal: document.getElementById("objXVal"),
          objYVal: document.getElementById("objYVal"),
          pocketXVal: document.getElementById("pocketXVal"),
          pocketYVal: document.getElementById("pocketYVal"),
          resetLayout: document.getElementById("resetLayout"),
        };

        function logicalToCanvas(p) {
          const w = canvas.width - 2 * table.padding;
          const h = canvas.height - 2 * table.padding;
          return {
            x: table.padding + (p.x / table.width) * w,
            y: table.padding + (1 - p.y / table.height) * h,
          };
        }

        function canvasToLogical(p) {
          const w = canvas.width - 2 * table.padding;
          const h = canvas.height - 2 * table.padding;
          let x = ((p.x - table.padding) / w) * table.width;
          let y = (1 - (p.y - table.padding) / h) * table.height;
          x = Math.max(0, Math.min(table.width, x));
          y = Math.max(0, Math.min(table.height, y));
          return { x, y };
        }

        function distance(a, b) {
          const dx = a.x - b.x;
          const dy = a.y - b.y;
          return Math.sqrt(dx * dx + dy * dy);
        }

        function normalize(v) {
          const len = Math.sqrt(v.x * v.x + v.y * v.y);
          if (!len) return { x: 0, y: 0 };
          return { x: v.x / len, y: v.y / len };
        }

        function dot(a, b) {
          return a.x * b.x + a.y * b.y;
        }

        function draw() {
          ctx.clearRect(0, 0, canvas.width, canvas.height);

          // Table background
          ctx.fillStyle = "#064e3b";
          ctx.fillRect(0, 0, canvas.width, canvas.height);

          // Playfield
          ctx.fillStyle = "#047857";
          ctx.fillRect(
            table.padding,
            table.padding,
            canvas.width - 2 * table.padding,
            canvas.height - 2 * table.padding
          );

          // Pockets (simple circles at corners and middles)
          ctx.fillStyle = "#111827";
          const pf = [
            { x: 0, y: 0 },
            { x: 50, y: 0 },
            { x: 100, y: 0 },
            { x: 0, y: 50 },
            { x: 50, y: 50 },
            { x: 100, y: 50 },
          ];
          pf.forEach((p) => {
            const c = logicalToCanvas(p);
            ctx.beginPath();
            ctx.arc(c.x, c.y, 10, 0, Math.PI * 2);
            ctx.fill();
          });

          // Compute geometry
          const cue = state.cue;
          const obj = state.obj;
          const pocket = state.pocket;

          const vOP = { x: pocket.x - obj.x, y: pocket.y - obj.y };
          const vCO = { x: obj.x - cue.x, y: obj.y - cue.y };

          const lenOP = distance(obj, pocket);
          const lenCO = distance(cue, obj);

          let cutAngleDeg = null;
          if (lenOP > 0 && lenCO > 0) {
            const cosTheta = dot(vOP, vCO) / (lenOP * lenCO);
            const clamped = Math.max(-1, Math.min(1, cosTheta));
            const theta = Math.acos(clamped);
            cutAngleDeg = (theta * 180) / Math.PI;
          }

          // Ball radius in logical units: we scale so that table width ~ actual feet
          // but for visualization we just keep a reasonable size.
          const ballRadiusLogical = 2; // ~2% of table width
          const ballRadiusPx =
            (canvas.width - 2 * table.padding) *
            (ballRadiusLogical / table.width);

          // Ghost ball
          let ghost = null;
          if (lenOP > 0) {
            const dirOP = normalize(vOP);
            ghost = {
              x: obj.x - ballRadiusLogical * 2 * dirOP.x,
              y: obj.y - ballRadiusLogical * 2 * dirOP.y,
            };
          }

          // Contact point on object ball
          let contactPoint = null;
          if (lenCO > 0) {
            const dirCO = normalize(vCO);
            contactPoint = {
              x: obj.x - ballRadiusLogical * dirCO.x,
              y: obj.y - ballRadiusLogical * dirCO.y,
            };
          }

          // Lines: object to pocket
          ctx.lineWidth = 2;
          ctx.setLineDash([6, 4]);
          ctx.strokeStyle = "rgba(255,255,255,0.7)";
          const objC = logicalToCanvas(obj);
          const pocketC = logicalToCanvas(pocket);
          ctx.beginPath();
          ctx.moveTo(objC.x, objC.y);
          ctx.lineTo(pocketC.x, pocketC.y);
          ctx.stroke();

          // Cue to ghost ball line
          if (ghost) {
            ctx.setLineDash([]);
            ctx.strokeStyle = "rgba(59,130,246,0.9)";
            const cueC = logicalToCanvas(cue);
            const ghostC = logicalToCanvas(ghost);
            ctx.beginPath();
            ctx.moveTo(cueC.x, cueC.y);
            ctx.lineTo(ghostC.x, ghostC.y);
            ctx.stroke();
          }

          // 90° tangent line at object ball
          if (ui.show90Rule.checked && lenOP > 0) {
            const dirOPn = normalize(vOP);
            // Perpendicular vector
            const perp = { x: -dirOPn.y, y: dirOPn.x };
            const scale = 15; // logical units
            const p1 = { x: obj.x - perp.x * scale, y: obj.y - perp.y * scale };
            const p2 = { x: obj.x + perp.x * scale, y: obj.y + perp.y * scale };
            const p1C = logicalToCanvas(p1);
            const p2C = logicalToCanvas(p2);
            ctx.setLineDash([4, 4]);
            ctx.strokeStyle = "rgba(248,250,252,0.7)";
            ctx.beginPath();
            ctx.moveTo(p1C.x, p1C.y);
            ctx.lineTo(p2C.x, p2C.y);
            ctx.stroke();
          }

          ctx.setLineDash([]);

          // Draw ghost ball
          if (ghost && ui.showGhostBall.checked) {
            const gC = logicalToCanvas(ghost);
            ctx.beginPath();
            ctx.fillStyle = "rgba(59,130,246,0.15)";
            ctx.strokeStyle = "rgba(59,130,246,0.9)";
            ctx.lineWidth = 2;
            ctx.arc(gC.x, gC.y, ballRadiusPx, 0, Math.PI * 2);
            ctx.fill();
            ctx.stroke();
          }

          // Draw contact point
          if (contactPoint && ui.showContactPoint.checked) {
            const cpC = logicalToCanvas(contactPoint);
            ctx.beginPath();
            ctx.fillStyle = "#fbbf24";
            ctx.arc(cpC.x, cpC.y, 4, 0, Math.PI * 2);
            ctx.fill();
          }

          // Draw balls
          function drawBall(pos, colorFill, colorStroke) {
            const c = logicalToCanvas(pos);
            ctx.beginPath();
            ctx.fillStyle = colorFill;
            ctx.strokeStyle = colorStroke;
            ctx.lineWidth = 2;
            ctx.arc(c.x, c.y, ballRadiusPx, 0, Math.PI * 2);
            ctx.fill();
            ctx.stroke();
          }

          drawBall(state.cue, "#f9fafb", "#e5e7eb"); // cue ball
          drawBall(state.obj, "#f97316", "#7c2d12"); // object ball

          // Draw pocket handle
          const pocketCanvas = logicalToCanvas(state.pocket);
          ctx.beginPath();
          ctx.strokeStyle = "#22c55e";
          ctx.lineWidth = 3;
          ctx.arc(
            pocketCanvas.x,
            pocketCanvas.y,
            ballRadiusPx * 0.8,
            0,
            Math.PI * 2
          );
          ctx.stroke();

          // Update metrics
          if (cutAngleDeg != null && isFinite(cutAngleDeg)) {
            ui.cutAngleDisplay.textContent = cutAngleDeg.toFixed(1) + "°";
          } else {
            ui.cutAngleDisplay.textContent = "–";
          }
          ui.coDistDisplay.textContent = lenCO.toFixed(1) + " units";
          ui.opDistDisplay.textContent = lenOP.toFixed(1) + " units";
        }

        // Drag handling
        function getHandleAtCanvasPoint(p) {
          const ballRadiusLogical = 2;
          const ballRadiusPx =
            (canvas.width - 2 * table.padding) *
            (ballRadiusLogical / table.width);
          const threshold = ballRadiusPx * 1.3;

          const cueC = logicalToCanvas(state.cue);
          const objC = logicalToCanvas(state.obj);
          const pocketC = logicalToCanvas(state.pocket);

          function hit(c) {
            const dx = p.x - c.x;
            const dy = p.y - c.y;
            return Math.sqrt(dx * dx + dy * dy) <= threshold;
          }

          if (hit(cueC)) return "cue";
          if (hit(objC)) return "obj";
          if (hit(pocketC)) return "pocket";
          return null;
        }

        function onPointerDown(e) {
          const rect = canvas.getBoundingClientRect();
          const p = {
            x: e.clientX - rect.left,
            y: e.clientY - rect.top,
          };
          const handle = getHandleAtCanvasPoint(p);
          if (handle) {
            state.dragging = handle;
            canvas.setPointerCapture(e.pointerId);
          }
        }

        function onPointerMove(e) {
          if (!state.dragging) return;
          const rect = canvas.getBoundingClientRect();
          const p = {
            x: e.clientX - rect.left,
            y: e.clientY - rect.top,
          };
          const logical = canvasToLogical(p);
          state[state.dragging].x = logical.x;
          state[state.dragging].y = logical.y;
          syncSlidersFromState();
          draw();
        }

        function onPointerUp(e) {
          if (state.dragging) {
            canvas.releasePointerCapture(e.pointerId);
            state.dragging = null;
          }
        }

        canvas.addEventListener("pointerdown", onPointerDown);
        canvas.addEventListener("pointermove", onPointerMove);
        canvas.addEventListener("pointerup", onPointerUp);
        canvas.addEventListener("pointercancel", onPointerUp);

        // Slider sync
        function syncSlidersFromState() {
          ui.cueX.value = state.cue.x.toFixed(0);
          ui.cueY.value = state.cue.y.toFixed(0);
          ui.objX.value = state.obj.x.toFixed(0);
          ui.objY.value = state.obj.y.toFixed(0);
          ui.pocketX.value = state.pocket.x.toFixed(0);
          ui.pocketY.value = state.pocket.y.toFixed(0);

          ui.cueXVal.textContent = ui.cueX.value;
          ui.cueYVal.textContent = ui.cueY.value;
          ui.objXVal.textContent = ui.objX.value;
          ui.objYVal.textContent = ui.objY.value;
          ui.pocketXVal.textContent = ui.pocketX.value;
          ui.pocketYVal.textContent = ui.pocketY.value;
        }

        function syncStateFromSliders() {
          state.cue.x = parseFloat(ui.cueX.value);
          state.cue.y = parseFloat(ui.cueY.value);
          state.obj.x = parseFloat(ui.objX.value);
          state.obj.y = parseFloat(ui.objY.value);
          state.pocket.x = parseFloat(ui.pocketX.value);
          state.pocket.y = parseFloat(ui.pocketY.value);

          ui.cueXVal.textContent = ui.cueX.value;
          ui.cueYVal.textContent = ui.cueY.value;
          ui.objXVal.textContent = ui.objX.value;
          ui.objYVal.textContent = ui.objY.value;
          ui.pocketXVal.textContent = ui.pocketX.value;
          ui.pocketYVal.textContent = ui.pocketY.value;

          draw();
        }

        ["cueX", "cueY", "objX", "objY", "pocketX", "pocketY"].forEach((id) => {
          ui[id].addEventListener("input", syncStateFromSliders);
        });

        // Other controls
        ui.tableSizeSelect.addEventListener("change", () => {
          tableFeet = parseFloat(ui.tableSizeSelect.value);
          draw();
        });

        ui.ballDiameterInput.addEventListener("input", () => {
          const v = parseFloat(ui.ballDiameterInput.value);
          if (!isNaN(v) && v > 0) {
            ballDiameterMm = v;
            draw();
          }
        });

        ui.showGhostBall.addEventListener("change", draw);
        ui.showContactPoint.addEventListener("change", draw);
        ui.show90Rule.addEventListener("change", draw);

        ui.resetLayout.addEventListener("click", () => {
          state.cue = { x: 25, y: 25 };
          state.obj = { x: 60, y: 25 };
          state.pocket = { x: 98, y: 25 };
          syncSlidersFromState();
          draw();
        });

        // Initial
        syncSlidersFromState();
        draw();

        // Handle resize for crisp canvas
        function resizeCanvas() {
          const rect = canvas.getBoundingClientRect();
          const ratio = window.devicePixelRatio || 1;
          canvas.width = rect.width * ratio;
          canvas.height = rect.height * ratio;
          ctx.setTransform(ratio, 0, 0, ratio, 0, 0);
          draw();
        }
        window.addEventListener("resize", resizeCanvas);
        // Initial resize after layout
        setTimeout(resizeCanvas, 50);
      })();
    </script>

