    <script>
      // --- Simplified internal "tables" ---
      // For production you would replace these with full WMA tables loaded from JSON.
      // Here we approximate with simple factors so the tool is fully functional.
      const baseStandards = {
        // base = open-class (age ~25) world-best-ish times in seconds
        M: {
          "5k": 12 * 60 + 37, // 12:37
          "10k": 26 * 60 + 17, // 26:17
          hm: 58 * 60 + 1, // 58:01
          marathon: 2 * 3600 + 1 * 60 + 9, // 2:01:09
          1500: 3 * 60 + 26, // 3:26
          3000: 7 * 60 + 20, // 7:20
          5000: 12 * 60 + 37,
          10000: 26 * 60 + 17,
        },
        F: {
          "5k": 14 * 60 + 11, // 14:11
          "10k": 29 * 60 + 1, // 29:01
          hm: 62 * 60 + 52, // 1:02:52
          marathon: 2 * 3600 + 11 * 60 + 53, // 2:11:53
          1500: 3 * 60 + 50, // 3:50
          3000: 8 * 60 + 6, // 8:06
          5000: 14 * 60 + 11,
          10000: 29 * 60 + 1,
        },
      };

      // Age factor: simple parabolic slowdown around age 25
      function ageFactor(age) {
        const a = Math.max(5, Math.min(100, age));
        const diff = a - 25;
        // 0% at 25, ~15% slower by 60, ~25% by 75, ~35% by 85
        const extra = 0.0009 * diff * diff;
        return 1 + extra;
      }

      function getStandardTime(sex, eventKey, age, distanceKm) {
        const s = sex === "F" ? "F" : "M";
        if (eventKey !== "custom") {
          const base = baseStandards[s][eventKey];
          return base * ageFactor(age);
        }
        // For custom distance, scale from 10K using Riegel exponent 1.06
        const base10k = baseStandards[s]["10k"];
        const d10 = 10.0;
        const exponent = 1.06;
        const scale = Math.pow(distanceKm / d10, exponent);
        return base10k * scale * ageFactor(age);
      }

      function secondsFromHMS(h, m, s) {
        return (
          (Number(h) || 0) * 3600 + (Number(m) || 0) * 60 + (Number(s) || 0)
        );
      }

      function formatTime(sec) {
        if (!isFinite(sec) || sec <= 0) return "–";
        const total = Math.round(sec);
        const h = Math.floor(total / 3600);
        const m = Math.floor((total % 3600) / 60);
        const s = total % 60;
        if (h > 0) {
          return `${h}:${String(m).padStart(2, "0")}:${String(s).padStart(
            2,
            "0"
          )}`;
        }
        return `${m}:${String(s).padStart(2, "0")}`;
      }

      function formatPace(secPerKm) {
        if (!isFinite(secPerKm) || secPerKm <= 0) return "–";
        const perKm = formatTime(secPerKm);
        const perMi = formatTime(secPerKm * 1.609344);
        return `${perKm} /km · ${perMi} /mile`;
      }

      function interpretAgeGrade(pct) {
        if (!isFinite(pct)) return "";
        if (pct >= 90) return "World class performance for your age group.";
        if (pct >= 80)
          return "National class performance – outstanding running.";
        if (pct >= 70)
          return "Regional class performance – very strong club runner.";
        if (pct >= 60) return "Good club‑level performance.";
        if (pct >= 50)
          return "Solid recreational performance with room to grow.";
        return "Recreational level – a great baseline to improve from.";
      }

      function equivalentTimeAtSamePercent(targetStd, percent) {
        if (!isFinite(targetStd) || !isFinite(percent) || percent <= 0)
          return NaN;
        return targetStd / (percent / 100);
      }

      function eventToDistanceKm(eventKey, customKm) {
        switch (eventKey) {
          case "5k":
            return 5;
          case "10k":
            return 10;
          case "hm":
            return 21.0975;
          case "marathon":
            return 42.195;
          case "1500":
            return 1.5;
          case "3000":
            return 3;
          case "5000":
            return 5;
          case "10000":
            return 10;
          case "custom":
            return customKm;
          default:
            return customKm || 5;
        }
      }

      document.addEventListener("DOMContentLoaded", () => {
        const eventSelect = document.getElementById("event");
        const customRow = document.getElementById("custom-distance-row");
        const calcBtn = document.getElementById("calc-btn");
        const resetBtn = document.getElementById("reset-btn");

        eventSelect.addEventListener("change", () => {
          customRow.classList.toggle("hidden", eventSelect.value !== "custom");
        });

        resetBtn.addEventListener("click", () => {
          document.getElementById("age").value = 40;
          document.querySelector('input[name="sex"][value="M"]').checked = true;
          eventSelect.value = "5k";
          customRow.classList.add("hidden");
          document.getElementById("custom-distance").value = 5;
          document.getElementById("custom-unit").value = "km";
          document.getElementById("time-h").value = 0;
          document.getElementById("time-m").value = 20;
          document.getElementById("time-s").value = 0;
          document.getElementById("results-panel").classList.add("hidden");
          document.getElementById("results-empty").classList.remove("hidden");
        });

        calcBtn.addEventListener("click", () => {
          const age = Number(document.getElementById("age").value);
          const sex =
            document.querySelector('input[name="sex"]:checked')?.value || "M";
          const eventKey = eventSelect.value;
          const h = document.getElementById("time-h").value;
          const m = document.getElementById("time-m").value;
          const s = document.getElementById("time-s").value;
          const tYou = secondsFromHMS(h, m, s);

          if (!age || age < 5 || age > 100 || !isFinite(tYou) || tYou <= 0) {
            alert(
              "Please enter a valid age (5–100) and a positive finish time."
            );
            return;
          }

          let customKm = 5;
          if (eventKey === "custom") {
            const d = Number(document.getElementById("custom-distance").value);
            const unit = document.getElementById("custom-unit").value;
            if (!d || d <= 0) {
              alert("Please enter a positive custom distance.");
              return;
            }
            customKm = unit === "mi" ? d * 1.609344 : d;
          }

          const distKm = eventToDistanceKm(eventKey, customKm);
          const stdAge = getStandardTime(sex, eventKey, age, distKm);
          const pct = (stdAge / tYou) * 100;

          // Open-class equivalent: use age 25 standard
          const stdOpen = getStandardTime(sex, eventKey, 25, distKm);
          const tOpenEq = equivalentTimeAtSamePercent(stdOpen, pct);

          // Equivalent 5K & 10K at same percent, same age
          const std5k = getStandardTime(sex, "5k", age, 5);
          const std10k = getStandardTime(sex, "10k", age, 10);
          const t5kEq = equivalentTimeAtSamePercent(std5k, pct);
          const t10kEq = equivalentTimeAtSamePercent(std10k, pct);

          // Paces
          const actualPace = tYou / distKm;
          const openPace = tOpenEq / distKm;

          // Update UI
          document.getElementById("results-empty").classList.add("hidden");
          document.getElementById("results-panel").classList.remove("hidden");

          const pctRounded = Math.round(pct * 10) / 10;
          document.getElementById("age-grade-badge").textContent = isFinite(
            pctRounded
          )
            ? pctRounded.toFixed(1) + "%"
            : "–";
          document.getElementById("age-grade-interpretation").textContent =
            interpretAgeGrade(pctRounded);

          document.getElementById("open-equivalent").textContent =
            formatTime(tOpenEq);
          document.getElementById("age-standard-time").textContent =
            formatTime(stdAge);
          document.getElementById("equiv-5k").textContent = formatTime(t5kEq);
          document.getElementById("equiv-10k").textContent = formatTime(t10kEq);

          document.getElementById("actual-pace").textContent =
            formatPace(actualPace);
          document.getElementById("open-pace").textContent =
            formatPace(openPace);
        });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
