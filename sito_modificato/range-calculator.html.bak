            <script>
              document.addEventListener('DOMContentLoaded', function() {
                const dataInput = document.getElementById('data-set');
                const calculateBtn = document.getElementById('calculate-btn');
                const resultsContainer = document.getElementById('results-container');
                const errorMessage = document.getElementById('error-message');
                
                const resultRange = document.getElementById('result-range');
                const resultMax = document.getElementById('result-max');
                const resultMin = document.getElementById('result-min');
                const resultCount = document.getElementById('result-count');
                const stepByStep = document.getElementById('step-by-step');

                calculateBtn.addEventListener('click', function() {
                  const rawText = dataInput.value;
                  
                  // Parse numbers
                  const numbers = rawText
                    .split(/[\s, \n]+/) // Split by comma, space, or newline
                    .filter(s => s.trim() !== '') // Remove empty strings
                    .map(s => parseFloat(s))     // Convert to number
                    .filter(n => !isNaN(n));      // Filter out non-numeric values
                    
                  if (numbers.length === 0) {
                    errorMessage.textContent = 'Please enter a valid set of numbers.';
                    resultsContainer.classList.add('hidden');
                    return;
                  }
                  
                  // Calculations
                  const min = Math.min(...numbers);
                  const max = Math.max(...numbers);
                  const range = max - min;
                  const count = numbers.length;
                  const sortedNumbers = [...numbers].sort((a, b) => a - b);
                  
                  // Display results
                  errorMessage.textContent = '';
                  resultsContainer.classList.remove('hidden');
                  
                  resultRange.textContent = formatNumber(range);
                  resultMax.textContent = formatNumber(max);
                  resultMin.textContent = formatNumber(min);
                  resultCount.textContent = count;
                  
                  // Display steps
                  stepByStep.innerHTML = `
                    <p>1. <strong>Original Data:</strong> {${numbers.join(', ')}}</p>
                    <p>2. <strong>Sorted Data:</strong> {${sortedNumbers.join(', ')}}</p>
                    <p>3. <strong>Find Maximum (Max):</strong> ${formatNumber(max)}</p>
                    <p>4. <strong>Find Minimum (Min):</strong> ${formatNumber(min)}</p>
                    <p>5. <strong>Calculate Range:</strong> ${formatNumber(max)} - ${formatNumber(min)} = <strong>${formatNumber(range)}</strong></p>
                  `;
                });
                
                function formatNumber(num) {
                    // Remove trailing zeros after decimal
                    return parseFloat(num.toFixed(10)).toString();
                }
              });
            </script>

