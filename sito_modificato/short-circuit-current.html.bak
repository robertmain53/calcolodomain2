    <script>
      // Mobile menu toggle
      document
        .getElementById("mobile-menu-toggle")
        ?.addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          if (!menu) return;
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          this.setAttribute("aria-expanded", String(isHidden));
        });

      const calcModeEl = document.getElementById("calc-mode");
      const modeKvaEl = document.getElementById("mode-kva");
      const modeIscEl = document.getElementById("mode-isc-base");

      function updateModeVisibility() {
        const mode = calcModeEl.value;
        if (mode === "kva") {
          modeKvaEl.classList.remove("hidden");
          modeIscEl.classList.add("hidden");
        } else {
          modeKvaEl.classList.add("hidden");
          modeIscEl.classList.remove("hidden");
        }
      }
      calcModeEl.addEventListener("change", updateModeVisibility);

      function formatAmp(value) {
        if (!isFinite(value)) return "–";
        if (value >= 1e3) {
          return (
            value.toLocaleString(undefined, { maximumFractionDigits: 1 }) + " A"
          );
        }
        return (
          value.toLocaleString(undefined, { maximumFractionDigits: 2 }) + " A"
        );
      }

      function formatkA(value) {
        if (!isFinite(value)) return "–";
        const kA = value / 1000;
        return (
          kA.toLocaleString(undefined, { maximumFractionDigits: 2 }) + " kA"
        );
      }

      document
        .getElementById("calc-btn")
        .addEventListener("click", function () {
          const systemType = document.getElementById("system-type").value;
          const mode = calcModeEl.value;
          const errorEl = document.getElementById("error-msg");
          const resultsEl = document.getElementById("results");
          errorEl.classList.add("hidden");
          errorEl.textContent = "";

          let Isym = NaN;
          let Ibase = NaN;
          let xr = null;

          try {
            if (mode === "kva") {
              const kva = parseFloat(document.getElementById("kva").value);
              const V = parseFloat(document.getElementById("voltage").value);
              const Zpct = parseFloat(
                document.getElementById("impedance").value
              );
              xr = parseFloat(document.getElementById("xr").value);

              if (!(kva > 0) || !(V > 0) || !(Zpct > 0)) {
                throw new Error(
                  "Please enter positive values for kVA, voltage, and impedance."
                );
              }

              const Sbase = kva * 1000; // VA
              if (systemType === "3ph") {
                Ibase = Sbase / (Math.sqrt(3) * V);
              } else {
                Ibase = Sbase / V;
              }

              const Zpu = Zpct / 100;
              Isym = Ibase / Zpu;
            } else {
              const IscKnown = parseFloat(
                document.getElementById("isc-known").value
              );
              xr = parseFloat(document.getElementById("xr-known").value);
              if (!(IscKnown > 0)) {
                throw new Error(
                  "Please enter a positive known short circuit current."
                );
              }
              Isym = IscKnown;
              Ibase = NaN;
            }

            if (!isFinite(Isym) || Isym <= 0) {
              throw new Error(
                "Unable to compute short circuit current with the given inputs."
              );
            }

            // Asymmetrical factor approximation based on X/R
            let asymFactor = 1.0;
            let asymNote =
              "X/R not provided – asymmetrical current assumed equal to symmetrical current.";
            if (!isNaN(xr) && xr > 0) {
              asymFactor = 1 + 0.02 * xr;
              // Cap to a reasonable range (typical first-cycle factors)
              if (asymFactor < 1.1) asymFactor = 1.1;
              if (asymFactor > 1.6) asymFactor = 1.6;
              asymNote =
                "Estimated using a simplified factor based on X/R. For standards work, use IEEE/IEC tables.";
            }

            const Iasym = Isym * asymFactor;
            const Ipeak = Isym * 2.5; // rough typical factor

            document.getElementById("isc-sym").textContent = formatAmp(Isym);
            document.getElementById("isc-sym-kA").textContent = formatkA(Isym);
            document.getElementById("isc-asym").textContent = formatAmp(Iasym);
            document.getElementById("isc-asym-kA").textContent =
              formatkA(Iasym);
            document.getElementById("isc-asym-note").textContent = asymNote;
            document.getElementById("isc-peak").textContent = formatAmp(Ipeak);
            document.getElementById("ibase").textContent = isFinite(Ibase)
              ? formatAmp(Ibase)
              : "–";

            resultsEl.classList.remove("hidden");
          } catch (e) {
            errorEl.textContent = e.message;
            errorEl.classList.remove("hidden");
            resultsEl.classList.add("hidden");
          }
        });

      document
        .getElementById("reset-btn")
        .addEventListener("click", function () {
          document.getElementById("system-type").value = "3ph";
          document.getElementById("calc-mode").value = "kva";
          document.getElementById("kva").value = "1000";
          document.getElementById("voltage").value = "480";
          document.getElementById("impedance").value = "5";
          document.getElementById("xr").value = "";
          document.getElementById("isc-known").value = "25000";
          document.getElementById("xr-known").value = "";
          updateModeVisibility();
          document.getElementById("results").classList.add("hidden");
          const errorEl = document.getElementById("error-msg");
          errorEl.classList.add("hidden");
          errorEl.textContent = "";
        });

      // Initialize mode visibility on load
      updateModeVisibility();
    </script>

