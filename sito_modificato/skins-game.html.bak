    <script>
      // Core state
      const state = {
        numPlayers: 4,
        numHoles: 18,
        players: [],
        scores: [], // [holeIndex][playerIndex] = {gross, netOrPoints}
        skinsPerPlayer: [],
        moneyPerPlayer: [],
        carryoverCount: 0,
        lastSkinWinnerIndex: null,
      };

      function initState() {
        state.numPlayers = parseInt(
          document.getElementById("num-players").value,
          10
        );
        state.numHoles = parseInt(
          document.getElementById("num-holes").value,
          10
        );
        state.players = [];
        for (let i = 0; i < state.numPlayers; i++) {
          state.players.push({
            name: "Player " + (i + 1),
            handicap: 0,
          });
        }
        state.scores = Array.from({ length: state.numHoles }, () =>
          Array.from({ length: state.numPlayers }, () => ({
            gross: null,
            netOrPoints: null,
          }))
        );
        state.skinsPerPlayer = Array(state.numPlayers).fill(0);
        state.moneyPerPlayer = Array(state.numPlayers).fill(0);
        state.carryoverCount = 0;
        state.lastSkinWinnerIndex = null;
      }

      function renderPlayers() {
        const container = document.getElementById("players-container");
        container.innerHTML = "";
        state.players.forEach((p, idx) => {
          const div = document.createElement("div");
          div.className = "flex items-center gap-2";
          div.innerHTML = `
        <input type="text" data-player-index="${idx}" data-field="name"
          class="flex-1 border border-gray-300 rounded-md px-2 py-1.5 text-sm"
          value="${p.name}">
        <div class="flex items-center gap-1 text-xs text-gray-600">
          <span>HCP</span>
          <input type="number" min="0" max="54" step="1"
            data-player-index="${idx}" data-field="handicap"
            class="w-14 border border-gray-300 rounded-md px-1.5 py-1 text-xs"
            value="${p.handicap}">
        </div>
      `;
          container.appendChild(div);
        });
      }

      function renderHoleSelector() {
        const select = document.getElementById("current-hole");
        select.innerHTML = "";
        for (let i = 1; i <= state.numHoles; i++) {
          const opt = document.createElement("option");
          opt.value = i;
          opt.textContent = "Hole " + i;
          select.appendChild(opt);
        }
      }

      function renderScoreRows() {
        const tbody = document.getElementById("hole-score-rows");
        tbody.innerHTML = "";
        const currentHoleIndex =
          parseInt(document.getElementById("current-hole").value, 10) - 1;
        const useStableford = document.getElementById("use-stableford").checked;
        const netSkins = document.getElementById("net-skins").checked;

        state.players.forEach((p, idx) => {
          const scoreObj = state.scores[currentHoleIndex][idx];
          const tr = document.createElement("tr");
          tr.className = "bg-white";
          tr.innerHTML = `
        <td class="px-3 py-2 border-t border-gray-200">${p.name}</td>
        <td class="px-3 py-2 border-t border-gray-200 text-center">
          <input type="number" min="1" max="20"
            class="table-input border border-gray-300 rounded-md px-1 py-1 text-sm"
            data-score-type="gross" data-player-index="${idx}"
            value="${scoreObj.gross !== null ? scoreObj.gross : ""}">
        </td>
        <td class="px-3 py-2 border-t border-gray-200 text-center">
          <input type="number"
            class="table-input border border-gray-300 rounded-md px-1 py-1 text-sm"
            data-score-type="netOrPoints" data-player-index="${idx}"
            placeholder="${useStableford ? "Pts" : netSkins ? "Net" : "—"}"
            ${useStableford || netSkins ? "" : "disabled"}
            value="${
              scoreObj.netOrPoints !== null ? scoreObj.netOrPoints : ""
            }">
        </td>
      `;
          tbody.appendChild(tr);
        });

        document
          .getElementById("carryover-indicator")
          .classList.toggle("hidden", state.carryoverCount === 0);
      }

      function syncPlayerInputs() {
        const container = document.getElementById("players-container");
        container.addEventListener("input", (e) => {
          const target = e.target;
          const idx = parseInt(target.getAttribute("data-player-index"), 10);
          const field = target.getAttribute("data-field");
          if (Number.isNaN(idx) || !field) return;
          if (field === "name") {
            state.players[idx].name = target.value || "Player " + (idx + 1);
          } else if (field === "handicap") {
            state.players[idx].handicap = parseInt(target.value || "0", 10);
          }
          renderScoreRows();
          renderSummary();
        });
      }

      function handleScoreInput() {
        document
          .getElementById("hole-score-rows")
          .addEventListener("input", (e) => {
            const target = e.target;
            const type = target.getAttribute("data-score-type");
            const idx = parseInt(target.getAttribute("data-player-index"), 10);
            if (!type || Number.isNaN(idx)) return;
            const holeIndex =
              parseInt(document.getElementById("current-hole").value, 10) - 1;
            const val = target.value === "" ? null : Number(target.value);
            if (type === "gross") {
              state.scores[holeIndex][idx].gross = val;
            } else {
              state.scores[holeIndex][idx].netOrPoints = val;
            }
          });
      }

      function recalcSkins() {
        // Reset skins and money, then recompute from scratch based on scores and settings
        state.skinsPerPlayer = Array(state.numPlayers).fill(0);
        state.moneyPerPlayer = Array(state.numPlayers).fill(0);
        state.carryoverCount = 0;
        state.lastSkinWinnerIndex = null;

        const carryoverEnabled = document.getElementById("carryover").checked;
        const useStableford = document.getElementById("use-stableford").checked;
        const netSkins = document.getElementById("net-skins").checked;
        const skinValue = Number(
          document.getElementById("skin-value").value || 0
        );

        for (let h = 0; h < state.numHoles; h++) {
          const holeScores = state.scores[h];
          const values = holeScores.map((s) => {
            if (useStableford || netSkins) {
              return s.netOrPoints;
            }
            return s.gross;
          });

          if (values.some((v) => v === null || Number.isNaN(v))) {
            continue; // skip incomplete holes
          }

          let winnerIndex = null;
          if (useStableford) {
            // Highest points wins
            const max = Math.max(...values);
            const winners = values
              .map((v, i) => (v === max ? i : -1))
              .filter((i) => i !== -1);
            if (winners.length === 1) {
              winnerIndex = winners[0];
            }
          } else {
            // Lowest strokes / net wins
            const min = Math.min(...values);
            const winners = values
              .map((v, i) => (v === min ? i : -1))
              .filter((i) => i !== -1);
            if (winners.length === 1) {
              winnerIndex = winners[0];
            }
          }

          if (winnerIndex === null) {
            // tie
            if (carryoverEnabled) {
              state.carryoverCount += 1;
            }
          } else {
            const skinsWon = 1 + state.carryoverCount;
            state.skinsPerPlayer[winnerIndex] += skinsWon;
            state.carryoverCount = 0;
            state.lastSkinWinnerIndex = winnerIndex;
          }
        }

        const totalSkins = state.skinsPerPlayer.reduce((a, b) => a + b, 0);
        if (skinValue > 0 && totalSkins > 0) {
          const pot = totalSkins * skinValue;
          const perPlayerContribution = pot / state.numPlayers;
          state.moneyPerPlayer = state.skinsPerPlayer.map(
            (skins) => skins * skinValue - perPlayerContribution
          );
        }
      }

      function renderSummary() {
        recalcSkins();
        const skinsList = document.getElementById("skins-summary-list");
        const payoutList = document.getElementById("payout-summary-list");
        const totalSkinsEl = document.getElementById("total-skins");
        const potValueEl = document.getElementById("pot-value");
        const skinValue = Number(
          document.getElementById("skin-value").value || 0
        );

        skinsList.innerHTML = "";
        payoutList.innerHTML = "";

        const totalSkins = state.skinsPerPlayer.reduce((a, b) => a + b, 0);
        totalSkinsEl.textContent = totalSkins;

        const pot = totalSkins * skinValue;
        potValueEl.textContent = "$" + pot.toFixed(2);

        state.players.forEach((p, idx) => {
          const li = document.createElement("li");
          li.textContent = `${p.name}: ${state.skinsPerPlayer[idx]} skin${
            state.skinsPerPlayer[idx] === 1 ? "" : "s"
          }`;
          skinsList.appendChild(li);

          if (skinValue > 0 && totalSkins > 0) {
            const money = state.moneyPerPlayer[idx];
            const li2 = document.createElement("li");
            const sign = money >= 0 ? "+" : "−";
            const absVal = Math.abs(money).toFixed(2);
            li2.textContent = `${p.name}: ${sign}$${absVal}`;
            payoutList.appendChild(li2);
          }
        });

        document
          .getElementById("carryover-indicator")
          .classList.toggle("hidden", state.carryoverCount === 0);
      }

      function updateCurrentHoleSkinsMessage() {
        const msgEl = document.getElementById("hole-result-message");
        const holeIndex =
          parseInt(document.getElementById("current-hole").value, 10) - 1;
        const carryoverEnabled = document.getElementById("carryover").checked;
        const useStableford = document.getElementById("use-stableford").checked;
        const netSkins = document.getElementById("net-skins").checked;

        const holeScores = state.scores[holeIndex];
        const values = holeScores.map((s) => {
          if (useStableford || netSkins) return s.netOrPoints;
          return s.gross;
        });

        if (values.some((v) => v === null || Number.isNaN(v))) {
          msgEl.textContent = "";
          return;
        }

        let winnerIndex = null;
        if (useStableford) {
          const max = Math.max(...values);
          const winners = values
            .map((v, i) => (v === max ? i : -1))
            .filter((i) => i !== -1);
          if (winners.length === 1) winnerIndex = winners[0];
        } else {
          const min = Math.min(...values);
          const winners = values
            .map((v, i) => (v === min ? i : -1))
            .filter((i) => i !== -1);
          if (winners.length === 1) winnerIndex = winners[0];
        }

        if (winnerIndex === null) {
          msgEl.textContent = carryoverEnabled
            ? "Hole tied – skins carry over to the next hole."
            : "Hole tied – no skin awarded on this hole.";
        } else {
          const skinsOnHole = 1 + state.carryoverCount;
          msgEl.textContent = `${
            state.players[winnerIndex].name
          } currently wins ${skinsOnHole} skin${
            skinsOnHole === 1 ? "" : "s"
          } on this hole (based on entered scores).`;
        }
      }

      function attachControls() {
        document
          .getElementById("num-players")
          .addEventListener("change", () => {
            initState();
            renderPlayers();
            renderHoleSelector();
            renderScoreRows();
            renderSummary();
          });

        document.getElementById("num-holes").addEventListener("change", () => {
          initState();
          renderPlayers();
          renderHoleSelector();
          renderScoreRows();
          renderSummary();
        });

        document.getElementById("net-skins").addEventListener("change", () => {
          renderScoreRows();
        });

        document
          .getElementById("use-stableford")
          .addEventListener("change", () => {
            renderScoreRows();
          });

        document
          .getElementById("current-hole")
          .addEventListener("change", () => {
            renderScoreRows();
            updateCurrentHoleSkinsMessage();
          });

        document
          .getElementById("prev-hole-btn")
          .addEventListener("click", () => {
            const select = document.getElementById("current-hole");
            const val = parseInt(select.value, 10);
            if (val > 1) {
              select.value = String(val - 1);
              renderScoreRows();
              updateCurrentHoleSkinsMessage();
            }
          });

        document
          .getElementById("next-hole-btn")
          .addEventListener("click", () => {
            const select = document.getElementById("current-hole");
            const val = parseInt(select.value, 10);
            if (val < state.numHoles) {
              select.value = String(val + 1);
              renderScoreRows();
              updateCurrentHoleSkinsMessage();
            }
          });

        document
          .getElementById("update-skins-btn")
          .addEventListener("click", () => {
            // Recalculate and show message for current hole
            renderSummary();
            updateCurrentHoleSkinsMessage();
          });

        document
          .getElementById("clear-hole-btn")
          .addEventListener("click", () => {
            const holeIndex =
              parseInt(document.getElementById("current-hole").value, 10) - 1;
            state.scores[holeIndex] = Array.from(
              { length: state.numPlayers },
              () => ({ gross: null, netOrPoints: null })
            );
            renderScoreRows();
            renderSummary();
            document.getElementById("hole-result-message").textContent =
              "Scores cleared for this hole.";
          });

        document
          .getElementById("reset-all-btn")
          .addEventListener("click", () => {
            if (!confirm("Reset all scores and skins for this round?")) return;
            initState();
            renderPlayers();
            renderHoleSelector();
            renderScoreRows();
            renderSummary();
            document.getElementById("hole-result-message").textContent =
              "Round reset.";
          });

        document.getElementById("skin-value").addEventListener("input", () => {
          renderSummary();
        });

        document.getElementById("carryover").addEventListener("change", () => {
          renderSummary();
        });

        document
          .getElementById("copy-summary-btn")
          .addEventListener("click", () => {
            const skinValue = Number(
              document.getElementById("skin-value").value || 0
            );
            const totalSkins = state.skinsPerPlayer.reduce((a, b) => a + b, 0);
            const pot = totalSkins * skinValue;
            let text = "Skins Game Summary\n\n";
            text += `Players: ${state.players.map((p) => p.name).join(", ")}\n`;
            text += `Holes: ${state.numHoles}\n`;
            text += `Value per skin: ${
              skinValue > 0 ? "$" + skinValue.toFixed(2) : "points only"
            }\n\n`;
            text += "Skins won:\n";
            state.players.forEach((p, idx) => {
              text += `- ${p.name}: ${state.skinsPerPlayer[idx]} skin${
                state.skinsPerPlayer[idx] === 1 ? "" : "s"
              }\n`;
            });
            if (skinValue > 0 && totalSkins > 0) {
              text += `\nTotal pot: $${pot.toFixed(2)}\nPayouts:\n`;
              state.players.forEach((p, idx) => {
                const money = state.moneyPerPlayer[idx];
                const sign = money >= 0 ? "+" : "-";
                const absVal = Math.abs(money).toFixed(2);
                text += `- ${p.name}: ${sign}$${absVal}\n`;
              });
            }

            navigator.clipboard
              .writeText(text)
              .then(() => {
                const fb = document.getElementById("copy-feedback");
                fb.classList.remove("hidden");
                setTimeout(() => fb.classList.add("hidden"), 2000);
              })
              .catch(() => {
                alert("Could not copy to clipboard. Please copy manually.");
              });
          });
      }

      document.addEventListener("DOMContentLoaded", () => {
        initState();
        renderPlayers();
        renderHoleSelector();
        renderScoreRows();
        renderSummary();
        syncPlayerInputs();
        handleScoreInput();
        attachControls();
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
