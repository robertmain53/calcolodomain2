    <script>
      // Utility conversions
      function cfmToM3s(cfm) {
        return cfm * 0.00047194745;
      }
      function lpsToM3s(lps) {
        return lps / 1000;
      }
      function cmhToM3s(cmh) {
        return cmh / 3600;
      }
      function m3sToCfm(m3s) {
        return m3s / 0.00047194745;
      }

      function inWgPer100ftToPaPerM(val) {
        // 1 in.wg = 249.0889 Pa, 100 ft = 30.48 m
        return (val * 249.0889) / 30.48;
      }
      function paPerMToInWgPer100ft(val) {
        return (val * 30.48) / 249.0889;
      }

      function ftToM(ft) {
        return ft * 0.3048;
      }
      function mToFt(m) {
        return m / 0.3048;
      }
      function inToM(inch) {
        return inch * 0.0254;
      }
      function mToIn(m) {
        return m / 0.0254;
      }
      function mmToM(mm) {
        return mm / 1000;
      }
      function mToMm(m) {
        return m * 1000;
      }

      document.addEventListener("DOMContentLoaded", function () {
        const ductTypeEl = document.getElementById("ductType");
        const rectOptionsEl = document.getElementById("rectOptions");
        const calcBtn = document.getElementById("calcBtn");
        const resetBtn = document.getElementById("resetBtn");

        ductTypeEl.addEventListener("change", () => {
          rectOptionsEl.classList.toggle("hidden", ductTypeEl.value !== "rect");
        });

        resetBtn.addEventListener("click", () => {
          document.getElementById("airflow").value = "";
          document.getElementById("friction").value = "";
          document.getElementById("length").value = "";
          document.getElementById("eqLength").value = "";
          document.getElementById("rectKnownValue").value = "";
          document.getElementById("airflowUnit").value = "cfm";
          document.getElementById("frictionUnit").value = "in100";
          document.getElementById("lengthUnit").value = "ft";
          document.getElementById("eqLengthUnit").value = "ft";
          document.getElementById("ductType").value = "round";
          rectOptionsEl.classList.add("hidden");
          document.getElementById("roughness").value = "galv";
          updateResults(null);
        });

        calcBtn.addEventListener("click", () => {
          try {
            const result = performDuctSizing();
            updateResults(result);
          } catch (e) {
            alert(e.message);
          }
        });
      });

      function performDuctSizing() {
        const airflow = parseFloat(document.getElementById("airflow").value);
        const airflowUnit = document.getElementById("airflowUnit").value;
        const friction = parseFloat(document.getElementById("friction").value);
        const frictionUnit = document.getElementById("frictionUnit").value;
        const length = parseFloat(
          document.getElementById("length").value || "0"
        );
        const lengthUnit = document.getElementById("lengthUnit").value;
        const eqLength = parseFloat(
          document.getElementById("eqLength").value || "0"
        );
        const eqLengthUnit = document.getElementById("eqLengthUnit").value;
        const ductType = document.getElementById("ductType").value;
        const roughness = document.getElementById("roughness").value;
        const rectKnownSide = document.getElementById("rectKnownSide").value;
        const rectKnownValue = parseFloat(
          document.getElementById("rectKnownValue").value || "0"
        );
        const rectKnownUnit = document.getElementById("rectKnownUnit").value;

        if (isNaN(airflow) || airflow <= 0) {
          throw new Error("Please enter a valid airflow.");
        }
        if (isNaN(friction) || friction <= 0) {
          throw new Error("Please enter a valid friction rate.");
        }

        // Convert airflow to m3/s
        let Q_m3s;
        if (airflowUnit === "cfm") Q_m3s = cfmToM3s(airflow);
        else if (airflowUnit === "lps") Q_m3s = lpsToM3s(airflow);
        else Q_m3s = cmhToM3s(airflow);

        // Convert friction rate to Pa/m
        let fr_Pa_m =
          frictionUnit === "in100" ? inWgPer100ftToPaPerM(friction) : friction;

        // Adjust friction for material (effective factor)
        let materialFactor = 1.0;
        if (roughness === "flex") materialFactor = 1.3; // flex duct more loss
        else if (roughness === "lined") materialFactor = 1.1;
        fr_Pa_m = fr_Pa_m / materialFactor; // we will size slightly larger to hit target effective rate

        // Convert lengths to meters
        const L_m = lengthUnit === "ft" ? ftToM(length) : length;
        const Leq_m = eqLengthUnit === "ft" ? ftToM(eqLength) : eqLength;
        const Ltot_m = L_m + Leq_m;

        // Empirical sizing: use a power-law relation between friction, flow and diameter.
        // Based on typical duct charts, approximate:
        // fr ~ K * Q^1.9 / D^5.02  => D = (K * Q^1.9 / fr)^(1/5.02)
        // Choose K so that for Q=1000 cfm (~0.472 m3/s), fr=0.08 in/100ft (~0.65 Pa/m), D ≈ 0.35 m (~14 in)
        const Q_ref = cfmToM3s(1000); // 1000 cfm in m3/s
        const fr_ref = inWgPer100ftToPaPerM(0.08);
        const D_ref = 0.3556; // 14 in in meters
        const nQ = 1.9;
        const nD = 5.02;
        const K = (fr_ref * Math.pow(D_ref, nD)) / Math.pow(Q_ref, nQ);

        const D_m = Math.pow((K * Math.pow(Q_m3s, nQ)) / fr_Pa_m, 1 / nD); // round duct diameter in meters

        // Velocity
        const area_m2 = (Math.PI * D_m * D_m) / 4;
        const V_ms = Q_m3s / area_m2;

        // Total pressure drop using friction rate (effective, including material factor)
        const fr_effective_Pa_m = fr_Pa_m * materialFactor;
        const dP_total_Pa = fr_effective_Pa_m * Ltot_m;

        // Rectangular equivalent
        let rectWidth_m = null;
        let rectHeight_m = null;
        const Dh = D_m;

        if (ductType === "rect") {
          if (
            rectKnownSide === "none" ||
            isNaN(rectKnownValue) ||
            rectKnownValue <= 0
          ) {
            // square equivalent: a = b, Dh = a
            rectWidth_m = Dh;
            rectHeight_m = Dh;
          } else {
            let known_m =
              rectKnownUnit === "in"
                ? inToM(rectKnownValue)
                : mmToM(rectKnownValue);
            if (known_m <= 0) known_m = Dh;
            if (rectKnownSide === "width") {
              const a = known_m;
              // Dh = 2ab / (a + b) => solve for b
              // Dh(a + b) = 2ab => Dh*a + Dh*b = 2ab => Dh*b - 2ab = -Dh*a => b(Dh - 2a) = -Dh*a
              let b = (Dh * a) / (2 * a - Dh);
              if (!isFinite(b) || b <= 0) b = Dh;
              rectWidth_m = a;
              rectHeight_m = b;
            } else {
              const b = known_m;
              let a = (Dh * b) / (2 * b - Dh);
              if (!isFinite(a) || a <= 0) a = Dh;
              rectWidth_m = a;
              rectHeight_m = b;
            }
          }
        } else {
          // still compute an equivalent square for reference
          rectWidth_m = Dh;
          rectHeight_m = Dh;
        }

        return {
          D_m,
          V_ms,
          dP_total_Pa,
          fr_effective_Pa_m,
          Ltot_m,
          rectWidth_m,
          rectHeight_m,
          Q_m3s,
        };
      }

      function updateResults(res) {
        const resDiameter = document.getElementById("resDiameter");
        const resRect = document.getElementById("resRect");
        const resVelocity = document.getElementById("resVelocity");
        const resPressure = document.getElementById("resPressure");
        const resFriction = document.getElementById("resFriction");
        const velocityBadge = document.getElementById("velocityBadge");
        const designNotes = document.getElementById("designNotes");

        if (!res) {
          resDiameter.textContent = "–";
          resRect.textContent = "–";
          resVelocity.textContent = "–";
          resPressure.textContent = "–";
          resFriction.textContent = "–";
          velocityBadge.innerHTML = "";
          designNotes.innerHTML =
            "<li>• Velocity and pressure drop checks will appear here after calculation.</li>";
          return;
        }

        const D_in = mToIn(res.D_m);
        const D_mm = mToMm(res.D_m);
        const V_fpm = res.V_ms * 196.8504;
        const dP_inWg = res.dP_total_Pa / 249.0889;
        const fr_in100 = paPerMToInWgPer100ft(res.fr_effective_Pa_m);

        resDiameter.textContent = `${D_in.toFixed(1)} in  (${D_mm.toFixed(
          0
        )} mm)`;

        if (res.rectWidth_m && res.rectHeight_m) {
          const w_in = mToIn(res.rectWidth_m);
          const h_in = mToIn(res.rectHeight_m);
          const w_mm = mToMm(res.rectWidth_m);
          const h_mm = mToMm(res.rectHeight_m);
          resRect.textContent = `${w_in.toFixed(1)} × ${h_in.toFixed(
            1
          )} in  (${w_mm.toFixed(0)} × ${h_mm.toFixed(0)} mm)`;
        } else {
          resRect.textContent = "–";
        }

        resVelocity.textContent = `${V_fpm.toFixed(0)} fpm  (${res.V_ms.toFixed(
          2
        )} m/s)`;
        resPressure.textContent = `${dP_inWg.toFixed(
          3
        )} in.wg  (${res.dP_total_Pa.toFixed(1)} Pa)`;
        resFriction.textContent = `${fr_in100.toFixed(
          3
        )} in.wg/100 ft  (${res.fr_effective_Pa_m.toFixed(2)} Pa/m)`;

        // Velocity badge
        let badgeText = "";
        let badgeColor = "";
        if (V_fpm < 300) {
          badgeText = "Velocity is low – ducts may be oversized.";
          badgeColor = "bg-yellow-100 text-yellow-800";
        } else if (V_fpm <= 900) {
          badgeText = "Velocity is in a typical range for supply ducts.";
          badgeColor = "bg-green-100 text-green-800";
        } else if (V_fpm <= 1200) {
          badgeText = "Velocity is high – check noise and pressure drop.";
          badgeColor = "bg-orange-100 text-orange-800";
        } else {
          badgeText = "Velocity is very high – consider increasing duct size.";
          badgeColor = "bg-red-100 text-red-800";
        }
        velocityBadge.innerHTML = `<span class="inline-flex items-center px-2.5 py-0.5 rounded-full text-xs font-medium ${badgeColor}">${badgeText}</span>`;

        // Design notes
        const notes = [];
        notes.push(
          `• Total equivalent length: ${mToFt(res.Ltot_m).toFixed(
            1
          )} ft (${res.Ltot_m.toFixed(1)} m).`
        );
        notes.push(
          `• Airflow: ${m3sToCfm(res.Q_m3s).toFixed(
            0
          )} CFM (${res.Q_m3s.toFixed(3)} m³/s).`
        );
        if (V_fpm > 900) {
          notes.push(
            "• Consider lowering the friction rate or increasing duct size to reduce velocity and noise."
          );
        } else if (V_fpm < 400) {
          notes.push(
            "• Velocity is relatively low; you may be able to reduce duct size or increase friction rate to save material."
          );
        } else {
          notes.push(
            "• Velocity is within a comfortable range for most main supply ducts."
          );
        }
        notes.push(
          "• Always verify final design against local codes and manufacturer recommendations."
        );

        designNotes.innerHTML = notes.map((n) => `<li>${n}</li>`).join("");
      }

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
