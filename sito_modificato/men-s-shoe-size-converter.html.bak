    <script>
      // Data table for men's sizes (approximate, blended from common charts)
      const sizeData = [
        { us: 6, uk: 5.5, eu: 39, cm: 24.5 },
        { us: 6.5, uk: 6, eu: 39.5, cm: 25.0 },
        { us: 7, uk: 6.5, eu: 40, cm: 25.4 },
        { us: 7.5, uk: 7, eu: 40.5, cm: 25.8 },
        { us: 8, uk: 7.5, eu: 41, cm: 26.2 },
        { us: 8.5, uk: 8, eu: 42, cm: 26.7 },
        { us: 9, uk: 8.5, eu: 42.5, cm: 27.1 },
        { us: 9.5, uk: 9, eu: 43, cm: 27.5 },
        { us: 10, uk: 9.5, eu: 44, cm: 28.0 },
        { us: 10.5, uk: 10, eu: 44.5, cm: 28.4 },
        { us: 11, uk: 10.5, eu: 45, cm: 28.8 },
        { us: 11.5, uk: 11, eu: 45.5, cm: 29.2 },
        { us: 12, uk: 11.5, eu: 46, cm: 29.6 },
        { us: 13, uk: 12.5, eu: 47.5, cm: 30.5 },
        { us: 14, uk: 13.5, eu: 49, cm: 31.3 },
      ];

      const tbody = document.getElementById("size-table-body");
      const inputSystemEl = document.getElementById("input-system");
      const inputValueEl = document.getElementById("input-value");
      const widthSelectEl = document.getElementById("width-select");
      const summaryCard = document.getElementById("summary-card");
      const summaryText = document.getElementById("summary-text");

      function cmToIn(cm) {
        return cm / 2.54;
      }

      function describeWidth(width) {
        if (width === "narrow") return "Narrow width (B/C)";
        if (width === "wide") return "Wide width (E/EE)";
        return "Standard width (D)";
      }

      function buildTable(highlightIndex = null) {
        tbody.innerHTML = "";
        const widthDesc = describeWidth(widthSelectEl.value);

        sizeData.forEach((row, index) => {
          const tr = document.createElement("tr");
          tr.className = "cursor-pointer hover:bg-blue-50 transition-colors";
          if (index === highlightIndex) {
            tr.classList.add("bg-blue-100");
          }

          const inches = cmToIn(row.cm);

          tr.innerHTML = `
        <td class="px-3 py-2 border-b">${row.us.toFixed(
          row.us % 1 ? 1 : 0
        )}</td>
        <td class="px-3 py-2 border-b">${row.uk.toFixed(
          row.uk % 1 ? 1 : 0
        )}</td>
        <td class="px-3 py-2 border-b">${row.eu}</td>
        <td class="px-3 py-2 border-b">${row.cm.toFixed(1)}</td>
        <td class="px-3 py-2 border-b">${inches.toFixed(2)}</td>
        <td class="px-3 py-2 border-b text-xs text-gray-700">${widthDesc}</td>
      `;

          tr.addEventListener("click", () => {
            // When clicking a row, set it as the reference
            inputSystemEl.value = "us";
            inputValueEl.value = row.us.toFixed(row.us % 1 ? 1 : 0);
            updateHighlight();
          });

          tbody.appendChild(tr);
        });
      }

      function findClosestIndex(system, value) {
        if (isNaN(value)) return null;
        let bestIndex = null;
        let bestDiff = Infinity;

        sizeData.forEach((row, index) => {
          let target;
          if (system === "us") target = row.us;
          else if (system === "uk") target = row.uk;
          else if (system === "eu") target = row.eu;
          else if (system === "cm") target = row.cm;
          else if (system === "in") target = cmToIn(row.cm);
          else target = null;

          if (target == null) return;

          const diff = Math.abs(target - value);
          if (diff < bestDiff) {
            bestDiff = diff;
            bestIndex = index;
          }
        });

        return bestIndex;
      }

      function updateHighlight() {
        const system = inputSystemEl.value;
        const rawValue = parseFloat(inputValueEl.value);
        const index = findClosestIndex(system, rawValue);

        buildTable(index);

        if (index != null) {
          const row = sizeData[index];
          const inches = cmToIn(row.cm);
          const widthDesc = describeWidth(widthSelectEl.value);

          summaryText.textContent =
            `Approximate match: US ${row.us.toFixed(row.us % 1 ? 1 : 0)}, ` +
            `UK ${row.uk.toFixed(row.uk % 1 ? 1 : 0)}, EU ${row.eu}, ` +
            `foot length ≈ ${row.cm.toFixed(1)} cm (${inches.toFixed(
              2
            )} in), ` +
            `${widthDesc}.`;
          summaryCard.classList.remove("hidden");
        } else {
          summaryCard.classList.add("hidden");
        }
      }

      inputSystemEl.addEventListener("change", updateHighlight);
      inputValueEl.addEventListener("input", updateHighlight);
      widthSelectEl.addEventListener("change", updateHighlight);

      // Initial render
      buildTable();

      document
        .querySelectorAll("#faq-container .faq-question")
        .forEach((btn) => {
          btn.addEventListener("click", () => {
            const answer = btn.nextElementSibling;
            const icon = btn.querySelector("span:last-child");
            const isHidden = answer.classList.contains("hidden");
            document
              .querySelectorAll("#faq-container .faq-question + div")
              .forEach((div) => div.classList.add("hidden"));
            document
              .querySelectorAll("#faq-container .faq-question span:last-child")
              .forEach((i) => (i.textContent = "+"));
            if (isHidden) {
              answer.classList.remove("hidden");
              icon.textContent = "−";
            }
          });
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
