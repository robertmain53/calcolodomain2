      // Normalize word list to ensure length property is correct
      for (const entry of WORD_LIST) {
        entry.length = entry.word.replace(/\s+/g, "").length;
      }

      /**
       * Utility: normalize text for comparison.
       */
      function normalizeText(str) {
        return (str || "").toString().trim().toUpperCase();
      }

      /**
       * Convert a pattern with ? and * into a RegExp.
       * ? -> single letter [A-Z]
       * * -> any number of letters [A-Z]*
       */
      function patternToRegex(pattern) {
        if (!pattern) return null;
        const esc = pattern.toUpperCase().replace(/[^A-Z?*]/g, (ch) => {
          // allow spaces in pattern to match spaces literally
          if (ch === " ") return "\\s";
          return ch.replace(/[.*+?^${}()|[\]\\]/g, "\\$&");
        });
        let regexStr = "^";
        for (let i = 0; i < esc.length; i++) {
          const ch = esc[i];
          if (ch === "?") {
            regexStr += "[A-Z]";
          } else if (ch === "*") {
            regexStr += "[A-Z]*";
          } else if (ch === "\\s") {
            regexStr += "\\s";
          } else {
            regexStr += ch;
          }
        }
        regexStr += "$";
        return new RegExp(regexStr, "i");
      }

      /**
       * Basic clue relevance scoring:
       * - simple keyword overlap between clue and definition/tags
       */
      function scoreByClue(entry, clue) {
        if (!clue) return 0;
        const clueWords = clue
          .toLowerCase()
          .split(/[^a-z]+/)
          .filter(Boolean);
        if (!clueWords.length) return 0;
        const def = (entry.def || "").toLowerCase();
        const tags = (entry.tags || []).join(" ").toLowerCase();
        let score = 0;
        for (const w of clueWords) {
          if (def.includes(w)) score += 3;
          if (tags.includes(w)) score += 2;
        }
        return score;
      }

      /**
       * Highlight matched pattern characters in result word.
       */
      function highlightPattern(word, pattern) {
        if (!pattern) return word;
        const upWord = word.toUpperCase();
        const upPattern = pattern.toUpperCase();
        // Simple heuristic: highlight fixed letters from pattern
        let result = "";
        for (let i = 0; i < upWord.length; i++) {
          const pCh = upPattern[i] || "";
          const wCh = upWord[i];
          if (pCh && pCh >= "A" && pCh <= "Z" && pCh === wCh) {
            result += `<span class="result-highlight">${word[i]}</span>`;
          } else {
            result += word[i];
          }
        }
        return result;
      }

      /**
       * Main crossword solver.
       */
      function runSolver() {
        const clue = document.getElementById("clue").value;
        const patternRaw = document.getElementById("pattern").value;
        const lengthVal = document.getElementById("length").value;
        const startsWithVal = document.getElementById("starts-with").value;
        const containsVal = document.getElementById("contains").value;
        const allowMultiword =
          document.getElementById("allow-multiword").checked;
        const showDefinitions =
          document.getElementById("show-definitions").checked;
        const statusEl = document.getElementById("status-text");
        const resultsEl = document.getElementById("results-container");
        const resultCountEl = document.getElementById("result-count");

        statusEl.textContent = "Searching…";
        let pattern = patternRaw.trim();
        const clueNorm = normalizeText(clue);
        const startsWith = normalizeText(startsWithVal);
        const contains = normalizeText(containsVal);

        let targetLength = null;
        if (lengthVal) {
          const n = parseInt(lengthVal, 10);
          if (!isNaN(n) && n > 0) targetLength = n;
        } else if (pattern) {
          // Infer length from pattern when no * present
          if (!pattern.includes("*")) {
            targetLength = pattern.replace(/[^A-Za-z?]/g, "").length;
          }
        }

        const regex = pattern ? patternToRegex(pattern) : null;

        let candidates = WORD_LIST.filter((entry) => {
          const wordRaw = entry.word;
          const wordNoSpace = wordRaw.replace(/\s+/g, "");
          const wordUpper = wordRaw.toUpperCase();

          if (!allowMultiword && /\s/.test(wordRaw)) return false;

          if (targetLength !== null && wordNoSpace.length !== targetLength)
            return false;

          if (regex && !regex.test(wordRaw)) return false;

          if (startsWith && !wordUpper.startsWith(startsWith)) return false;

          if (contains && !wordUpper.includes(contains)) return false;

          return true;
        });

        // Score and sort
        candidates = candidates.map((entry) => {
          const baseScore = 10; // base
          const clueScore = scoreByClue(entry, clue);
          const lengthBonus =
            targetLength && entry.length === targetLength ? 2 : 0;
          const patternBonus = pattern ? 1 : 0;
          return {
            ...entry,
            _score: baseScore + clueScore + lengthBonus + patternBonus,
          };
        });

        const sortMode = document.getElementById("sort-mode").value;
        if (sortMode === "alpha") {
          candidates.sort((a, b) => a.word.localeCompare(b.word));
        } else if (sortMode === "length") {
          candidates.sort(
            (a, b) => a.length - b.length || a.word.localeCompare(b.word)
          );
        } else {
          candidates.sort(
            (a, b) => b._score - a._score || a.word.localeCompare(b.word)
          );
        }

        // Render
        if (!candidates.length) {
          resultsEl.innerHTML =
            '<p class="text-sm text-gray-500">No matches found. Try relaxing the pattern or removing some filters.</p>';
          resultCountEl.textContent = "0 results";
          statusEl.textContent = "No matches.";
          return;
        }

        const maxToShow = 100;
        const limited = candidates.slice(0, maxToShow);

        const html = limited
          .map((entry) => {
            const wordDisplay = highlightPattern(entry.word, pattern);
            const def =
              showDefinitions && entry.def
                ? `<div class="text-xs text-gray-600 mt-0.5">${entry.def}</div>`
                : "";
            const meta = `<span class="badge bg-gray-100 text-gray-600 mr-1">${entry.length} letters</span>`;
            return `
      <div class="py-1.5 border-b border-gray-100 last:border-b-0">
        <div class="flex items-baseline justify-between">
          <div class="font-semibold text-sm">${wordDisplay}</div>
          <div class="text-[0.7rem] text-gray-400 ml-2">${meta}</div>
        </div>
        ${def}
      </div>
    `;
          })
          .join("");

        resultsEl.innerHTML = html;
        resultCountEl.textContent = `${candidates.length} result${
          candidates.length !== 1 ? "s" : ""
        }${
          candidates.length > maxToShow
            ? " (showing first " + maxToShow + ")"
            : ""
        }`;
        statusEl.textContent = "Done.";
      }

      /**
       * Anagram solver.
       */
      function runAnagramSolver() {
        const lettersRaw =
          document.getElementById("anagram-letters").value || "";
        const lengthVal = document.getElementById("anagram-length").value;
        const startsWithVal = document.getElementById(
          "anagram-starts-with"
        ).value;
        const containsVal = document.getElementById("anagram-contains").value;
        const statusEl = document.getElementById("anagram-status-text");
        const resultsEl = document.getElementById("results-container");
        const resultCountEl = document.getElementById("result-count");
        const showDefinitions =
          document.getElementById("show-definitions").checked;

        const letters = normalizeText(lettersRaw).replace(/[^A-Z]/g, "");
        if (!letters) {
          statusEl.textContent = "Enter some letters first.";
          return;
        }

        statusEl.textContent = "Searching…";

        let targetLength = null;
        if (lengthVal) {
          const n = parseInt(lengthVal, 10);
          if (!isNaN(n) && n > 0) targetLength = n;
        }

        const startsWith = normalizeText(startsWithVal);
        const contains = normalizeText(containsVal);

        // Build frequency map of letters
        const freq = {};
        for (const ch of letters) {
          freq[ch] = (freq[ch] || 0) + 1;
        }

        function canForm(word) {
          const w = word.replace(/\s+/g, "").toUpperCase();
          if (targetLength && w.length !== targetLength) return false;
          const f = {};
          for (const ch of w) {
            f[ch] = (f[ch] || 0) + 1;
            if (!freq[ch] || f[ch] > freq[ch]) return false;
          }
          return true;
        }

        let candidates = WORD_LIST.filter((entry) => {
          if (!canForm(entry.word)) return false;
          const w = entry.word.toUpperCase();
          if (startsWith && !w.startsWith(startsWith)) return false;
          if (contains && !w.includes(contains)) return false;
          return true;
        });

        candidates = candidates.map((entry) => ({
          ...entry,
          _score: entry.length, // simple: longer words first
        }));

        const sortMode = document.getElementById("sort-mode").value;
        if (sortMode === "alpha") {
          candidates.sort((a, b) => a.word.localeCompare(b.word));
        } else if (sortMode === "length") {
          candidates.sort(
            (a, b) => a.length - b.length || a.word.localeCompare(b.word)
          );
        } else {
          candidates.sort(
            (a, b) => b._score - a._score || a.word.localeCompare(b.word)
          );
        }

        if (!candidates.length) {
          resultsEl.innerHTML =
            '<p class="text-sm text-gray-500">No anagrams found. Try allowing any length or removing filters.</p>';
          resultCountEl.textContent = "0 results";
          statusEl.textContent = "No matches.";
          return;
        }

        const maxToShow = 100;
        const limited = candidates.slice(0, maxToShow);

        const html = limited
          .map((entry) => {
            const def =
              showDefinitions && entry.def
                ? `<div class="text-xs text-gray-600 mt-0.5">${entry.def}</div>`
                : "";
            const meta = `<span class="badge bg-gray-100 text-gray-600 mr-1">${entry.length} letters</span>`;
            return `
      <div class="py-1.5 border-b border-gray-100 last:border-b-0">
        <div class="flex items-baseline justify-between">
          <div class="font-semibold text-sm">${entry.word}</div>
          <div class="text-[0.7rem] text-gray-400 ml-2">${meta}</div>
        </div>
        ${def}
      </div>
    `;
          })
          .join("");

        resultsEl.innerHTML = html;
        resultCountEl.textContent = `${candidates.length} anagram${
          candidates.length !== 1 ? "s" : ""
        }${
          candidates.length > maxToShow
            ? " (showing first " + maxToShow + ")"
            : ""
        }`;
        statusEl.textContent = "Done.";
      }

      /**
       * Reset functions.
       */
      function resetSolver() {
        document.getElementById("clue").value = "";
        document.getElementById("pattern").value = "";
        document.getElementById("length").value = "";
        document.getElementById("starts-with").value = "";
        document.getElementById("contains").value = "";
        document.getElementById("allow-multiword").checked = false;
        document.getElementById("show-definitions").checked = true;
        document.getElementById("status-text").textContent = "";
        document.getElementById("results-container").innerHTML =
          '<p class="text-sm text-gray-500">Enter a clue and/or pattern above, then click <span class="font-semibold">Solve crossword</span>. Results will appear here.</p>';
        document.getElementById("result-count").textContent = "No search yet.";
      }

      function resetAnagramSolver() {
        document.getElementById("anagram-letters").value = "";
        document.getElementById("anagram-length").value = "";
        document.getElementById("anagram-starts-with").value = "";
        document.getElementById("anagram-contains").value = "";
        document.getElementById("anagram-status-text").textContent = "";
        resetSolver();
      }

      /**
       * Mode switching.
       */
      document
        .getElementById("mode-crossword")
        .addEventListener("click", () => {
          document
            .getElementById("mode-crossword")
            .classList.add(
              "bg-white",
              "shadow",
              "text-blue-600",
              "border",
              "border-gray-200"
            );
          document
            .getElementById("mode-anagram")
            .classList.remove(
              "bg-white",
              "shadow",
              "text-blue-600",
              "border",
              "border-gray-200"
            );
          document
            .getElementById("mode-anagram")
            .classList.add("text-gray-600");
          document.getElementById("crossword-form").classList.remove("hidden");
          document.getElementById("anagram-form").classList.add("hidden");
        });

      document.getElementById("mode-anagram").addEventListener("click", () => {
        document
          .getElementById("mode-anagram")
          .classList.add(
            "bg-white",
            "shadow",
            "text-blue-600",
            "border",
            "border-gray-200"
          );
        document
          .getElementById("mode-crossword")
          .classList.remove(
            "bg-white",
            "shadow",
            "text-blue-600",
            "border",
            "border-gray-200"
          );
        document
          .getElementById("mode-crossword")
          .classList.add("text-gray-600");
        document.getElementById("crossword-form").classList.add("hidden");
        document.getElementById("anagram-form").classList.remove("hidden");
      });

      /**
       * Re-run sort when sort mode changes (if we already have results).
       * For simplicity, we just re-run the current mode search.
       */
      document.getElementById("sort-mode").addEventListener("change", () => {
        const anagramVisible = !document
          .getElementById("anagram-form")
          .classList.contains("hidden");
        if (anagramVisible) {
          runAnagramSolver();
        } else {
          runSolver();
        }
      });
    </script>

