    <script>
      // Tab handling
      const tabButtons = document.querySelectorAll(".tab-btn");
      const tabPanels = document.querySelectorAll(".tab-panel");

      function activateTab(id) {
        tabPanels.forEach((p) => p.classList.add("hidden"));
        tabButtons.forEach((b) => b.classList.remove("tab-active"));
        const panel = document.getElementById(id);
        const btn = Array.from(tabButtons).find((b) => b.dataset.tab === id);
        if (panel) panel.classList.remove("hidden");
        if (btn) btn.classList.add("tab-active");
      }

      tabButtons.forEach((btn) => {
        btn.addEventListener("click", () => activateTab(btn.dataset.tab));
      });

      // Default tab
      activateTab("tab-power");

      // POWER CONVERSIONS
      const powerInput = document.getElementById("power-watts");
      const powerUnit = document.getElementById("power-watts-unit");
      const outW = document.getElementById("power-out-w");
      const outKW = document.getElementById("power-out-kw");
      const outBTUhr = document.getElementById("power-out-btuhr");
      const outHP = document.getElementById("power-out-hp");

      function formatVal(v) {
        if (!isFinite(v)) return "–";
        if (Math.abs(v) >= 1000 || (Math.abs(v) < 0.01 && v !== 0)) {
          return v.toExponential(3);
        }
        return v.toFixed(3).replace(/\.?0+$/, "");
      }

      function updatePower() {
        const val = parseFloat(powerInput.value);
        if (isNaN(val)) {
          outW.textContent =
            outKW.textContent =
            outBTUhr.textContent =
            outHP.textContent =
              "–";
          return;
        }
        let watts;
        switch (powerUnit.value) {
          case "W":
            watts = val;
            break;
          case "kW":
            watts = val * 1000;
            break;
          case "HP":
            watts = val * 745.7;
            break;
          case "BTUhr":
            watts = val / 3.412141633;
            break;
          default:
            watts = val;
        }
        const kw = watts / 1000;
        const btuhr = watts * 3.412141633;
        const hp = watts / 745.7;

        outW.textContent = formatVal(watts);
        outKW.textContent = formatVal(kw);
        outBTUhr.textContent = formatVal(btuhr);
        outHP.textContent = formatVal(hp);
      }

      powerInput.addEventListener("input", updatePower);
      powerUnit.addEventListener("change", updatePower);

      // ENERGY & COST
      const ePower = document.getElementById("energy-power");
      const ePowerUnit = document.getElementById("energy-power-unit");
      const eHours = document.getElementById("energy-hours");
      const eRate = document.getElementById("energy-rate");
      const eOutKWh = document.getElementById("energy-out-kwh");
      const eOutMJ = document.getElementById("energy-out-mj");
      const eOutBTU = document.getElementById("energy-out-btu");
      const eOutCost = document.getElementById("energy-out-cost");

      function updateEnergy() {
        const pVal = parseFloat(ePower.value);
        const tVal = parseFloat(eHours.value);
        const rVal = parseFloat(eRate.value);

        if (isNaN(pVal) || isNaN(tVal)) {
          eOutKWh.textContent =
            eOutMJ.textContent =
            eOutBTU.textContent =
            eOutCost.textContent =
              "–";
          return;
        }

        let kW = ePowerUnit.value === "W" ? pVal / 1000 : pVal;
        const kWh = kW * tVal;
        const MJ = kWh * 3.6;
        const BTU = kWh * 3412.141633;
        const cost = isNaN(rVal) ? null : kWh * rVal;

        eOutKWh.textContent = formatVal(kWh);
        eOutMJ.textContent = formatVal(MJ);
        eOutBTU.textContent = formatVal(BTU);
        eOutCost.textContent = cost === null ? "–" : "$" + formatVal(cost);
      }

      [ePower, ePowerUnit, eHours, eRate].forEach((el) =>
        el.addEventListener("input", updateEnergy)
      );

      // LIGHTING
      const lightMode = document.getElementById("light-mode");
      const lightLumens = document.getElementById("light-lumens");
      const lightArea = document.getElementById("light-area");
      const lightAreaUnit = document.getElementById("light-area-unit");
      const lightLux = document.getElementById("light-lux");
      const lightOut = document.getElementById("light-out");

      function toM2(area, unit) {
        if (unit === "ft2") return area * 0.09290304;
        return area;
      }

      function fromM2(areaM2, unit) {
        if (unit === "ft2") return areaM2 / 0.09290304;
        return areaM2;
      }

      function updateLighting() {
        const mode = lightMode.value;
        const lumens = parseFloat(lightLumens.value);
        const area = parseFloat(lightArea.value);
        const lux = parseFloat(lightLux.value);
        const unit = lightAreaUnit.value;

        let resultText = "–";

        if (mode === "lux") {
          if (isNaN(lumens) || isNaN(area) || area <= 0) {
            resultText = "Enter lumens and area to compute lux.";
          } else {
            const areaM2 = toM2(area, unit);
            const luxVal = lumens / areaM2;
            resultText = `Illuminance ≈ ${formatVal(luxVal)} lux`;
          }
        } else if (mode === "lumens") {
          if (isNaN(lux) || isNaN(area) || area <= 0) {
            resultText = "Enter target lux and area to compute lumens.";
          } else {
            const areaM2 = toM2(area, unit);
            const lumensVal = lux * areaM2;
            resultText = `Required lumens ≈ ${formatVal(lumensVal)} lm`;
          }
        } else if (mode === "area") {
          if (isNaN(lux) || isNaN(lumens) || lux <= 0) {
            resultText = "Enter lumens and target lux to compute area.";
          } else {
            const areaM2 = lumens / lux;
            const areaOut = fromM2(areaM2, unit);
            resultText = `Lit area ≈ ${formatVal(areaOut)} ${
              unit === "m2" ? "m²" : "ft²"
            }`;
          }
        }

        lightOut.textContent = resultText;
      }

      [lightMode, lightLumens, lightArea, lightAreaUnit, lightLux].forEach(
        (el) => el.addEventListener("input", updateLighting)
      );

      // EFFICACY
      const effLumens = document.getElementById("eff-lumens");
      const effWatts = document.getElementById("eff-watts");
      const effOut = document.getElementById("eff-out");

      function updateEfficacy() {
        const lm = parseFloat(effLumens.value);
        const w = parseFloat(effWatts.value);
        if (isNaN(lm) || isNaN(w) || w <= 0) {
          effOut.textContent = "Enter lumens and watts to compute lm/W.";
          return;
        }
        const eff = lm / w;
        effOut.textContent = `Luminous efficacy ≈ ${formatVal(eff)} lm/W`;
      }

      [effLumens, effWatts].forEach((el) =>
        el.addEventListener("input", updateEfficacy)
      );
    </script>

