            <script>
              document.addEventListener('DOMContentLoaded', function() {
                const input = document.getElementById('decimal-input');
                const placesSelect = document.getElementById('rounding-places');
                const calculateBtn = document.getElementById('calculate-btn');
                const resultsContainer = document.getElementById('results-container');
                const errorMessage = document.getElementById('error-message');
                const resultRounded = document.getElementById('result-rounded');
                const resultFraction = document.getElementById('result-fraction');
                const resultPercentage = document.getElementById('result-percentage');
                const stepByStep = document.getElementById('step-by-step');

                // --- Helper: GCD for Fraction Simplification ---
                function gcd(a, b) {
                    return b ? gcd(b, a % b) : a;
                }
                
                // --- Decimal to Fraction Conversion Logic ---
                function convertToFraction(decimal) {
                    if (isNaN(decimal)) return { fraction: 'Invalid', steps: '' };

                    const wholePart = Math.floor(Math.abs(decimal));
                    const decimalPart = Math.abs(decimal) - wholePart;
                    
                    if (decimalPart === 0) {
                        return { fraction: wholePart.toString(), steps: '' };
                    }
                    
                    const sign = decimal < 0 ? '-' : '';
                    const tolerance = 1e-10; // Tolerance for floating point error

                    // Find power of 10 for denominator
                    let numerator = decimalPart;
                    let denominator = 1;
                    while (Math.abs(numerator - Math.round(numerator)) > tolerance) {
                        numerator *= 10;
                        denominator *= 10;
                    }

                    numerator = Math.round(numerator);
                    const commonDivisor = gcd(numerator, denominator);
                    
                    const simplifiedNumerator = numerator / commonDivisor;
                    const simplifiedDenominator = denominator / commonDivisor;

                    let fractionOutput;
                    if (wholePart > 0) {
                        fractionOutput = `${sign}${wholePart} ${simplifiedNumerator}/${simplifiedDenominator}`;
                    } else {
                        fractionOutput = `${sign}${simplifiedNumerator}/${simplifiedDenominator}`;
                    }
                    
                    return {
                        fraction: fractionOutput,
                        steps: simplifiedNumerator,
                        // This simple logic is sufficient for the tool display
                    };
                }

                // --- Main Calculation Logic ---
                calculateBtn.addEventListener('click', () => {
                    const rawDecimal = input.value;
                    const decimal = parseFloat(rawDecimal);
                    const places = parseInt(placesSelect.value, 10);

                    try {
                        if (rawDecimal.trim() === '' || isNaN(decimal)) {
                            throw new Error('Please enter a valid number.');
                        }
                        
                        errorMessage.textContent = '';
                        
                        // 1. Rounding Calculation
                        const factor = Math.pow(10, places);
                        const roundedValue = Math.round(decimal * factor) / factor;
                        
                        // 2. Determine Rounding Steps
                        const [whole, dec] = rawDecimal.split('.');
                        let stepText = '';
                        
                        if (dec && places >= 0) {
                            const targetIndex = places;
                            const nextDigit = dec[targetIndex];
                            
                            stepText += `<p>1. The original number is <strong>${rawDecimal}</strong>.</p>`;
                            stepText += `<p>2. We are rounding to <strong>${places} decimal place(s)</strong>, or the ${places === 1 ? 'tenths' : (places === 2 ? 'hundredths' : places + '-th decimal')} place.</p>`;
                            
                            if (nextDigit === undefined) {
                                stepText += `<p>The number has fewer than ${places} decimal places, so no rounding is needed. Result: <strong>${rawDecimal}</strong></p>`;
                            } else {
                                const targetDigit = dec[targetIndex - 1] || whole[whole.length - 1]; // Handle 0 places separately
                                
                                if (places === 0) {
                                    stepText += `<p>3. The target digit is the last digit of the whole number: <strong>${targetDigit}</strong>.</p>`;
                                } else {
                                    stepText += `<p>3. The target digit (in the ${places}-th place) is highlighted in <strong>${whole}.${dec.substring(0, targetIndex-1)}<span class="text-blue-600 font-bold">${dec[targetIndex-1]}</span>${dec.substring(targetIndex)}</strong>.</p>`;
                                }
                                
                                stepText += `<p>4. The digit to the right of the target is <strong>${nextDigit}</strong>.</p>`;

                                if (parseInt(nextDigit) >= 5) {
                                    stepText += `<p>5. Since ${nextDigit} is 5 or greater, we <strong>round up</strong> the target digit.</p>`;
                                } else {
                                    stepText += `<p>5. Since ${nextDigit} is 4 or less, we <strong>keep the target digit the same</strong>.</p>`;
                                }
                                stepText += `<p>6. The final rounded result is: <strong>${formatNumber(roundedValue)}</strong></p>`;
                            }
                        } else if (places === 0 && decimal !== Math.floor(decimal)) {
                            const nextDigit = dec ? dec[0] : 0;
                            if(nextDigit) {
                                stepText += `<p>1. The number is <strong>${rawDecimal}</strong>.</p>`;
                                stepText += `<p>2. We are rounding to the nearest whole number.</p>`;
                                stepText += `<p>3. The digit in the tenths place (the next digit) is <strong>${nextDigit}</strong>.</p>`;
                                if (parseInt(nextDigit) >= 5) {
                                    stepText += `<p>4. Since ${nextDigit} is 5 or greater, we round up the whole number part.</p>`;
                                } else {
                                    stepText += `<p>4. Since ${nextDigit} is 4 or less, we keep the whole number part as is.</p>`;
                                }
                                stepText += `<p>5. The final rounded result is: <strong>${formatNumber(roundedValue)}</strong></p>`;
                            }
                        } else {
                             stepText += `<p>The number is an integer or has fewer decimal places than requested. Result: <strong>${formatNumber(roundedValue)}</strong></p>`;
                        }
                        
                        // 3. Conversion Results
                        const fractionData = convertToFraction(decimal);
                        const percentageValue = formatNumber(decimal * 100);
                        
                        // 4. Update DOM
                        resultRounded.textContent = formatNumber(roundedValue);
                        resultFraction.textContent = fractionData.fraction;
                        resultPercentage.textContent = `${percentageValue}%`;
                        stepByStep.innerHTML = stepText;
                        resultsContainer.classList.remove('hidden');

                    } catch (e) {
                        showError(e.message);
                    }
                });
                
                // Initial calculation (for example values)
                calculateBtn.click(); 
                
                function formatNumber(num) {
                    return parseFloat(num.toFixed(10)).toString();
                }
              });
            </script>

