    <script>
      // Utility functions
      function toSeconds(h, m, s) {
        h = Number(h) || 0;
        m = Number(m) || 0;
        s = Number(s) || 0;
        return h * 3600 + m * 60 + s;
      }

      function fromSeconds(totalSeconds) {
        const sign = totalSeconds < 0 ? -1 : 1;
        let secs = Math.abs(totalSeconds);
        const hours = Math.trunc(secs / 3600) * sign;
        secs = secs % 3600;
        const minutes = Math.trunc(secs / 60) * sign;
        const seconds = (secs % 60) * sign;
        return { hours, minutes, seconds };
      }

      function formatHMS(obj) {
        const { hours, minutes, seconds } = obj;
        const sign = hours < 0 || minutes < 0 || seconds < 0 ? "-" : "";
        const ah = Math.abs(hours);
        const am = Math.abs(minutes);
        const as = Math.abs(seconds);
        const sRounded = Math.round(as * 1000) / 1000;
        return sign + ah + " h " + am + " min " + sRounded + " s";
      }

      function formatNumber(n, decimals = 0) {
        if (!isFinite(n)) return "0";
        return n.toLocaleString(undefined, { maximumFractionDigits: decimals });
      }

      // Tabs
      document.querySelectorAll(".tab-btn").forEach((btn) => {
        btn.addEventListener("click", () => {
          const tab = btn.getAttribute("data-tab");
          document.querySelectorAll(".tab-btn").forEach((b) => {
            b.classList.remove("tab-active");
            b.setAttribute("aria-selected", "false");
          });
          btn.classList.add("tab-active");
          btn.setAttribute("aria-selected", "true");
          document.querySelectorAll(".tab-panel").forEach((panel) => {
            panel.classList.add("hidden");
          });
          document.getElementById("tab-" + tab).classList.remove("hidden");
        });
      });
      // Activate first tab by default
      document
        .querySelector('.tab-btn[data-tab="sum"]')
        .classList.add("tab-active");

      // Sum tab logic
      const sumRowsContainer = document.getElementById("sum-rows");
      const sumResultHms = document.getElementById("sum-result-hms");
      const sumResultMins = document.getElementById("sum-result-mins");
      const sumResultSecs = document.getElementById("sum-result-secs");

      function createRow(initial = { h: 0, m: 0, s: 0 }) {
        const row = document.createElement("div");
        row.className = "grid grid-cols-12 gap-2 items-end";
        row.innerHTML = `
      <div class="col-span-4">
        <label class="block text-xs text-gray-600 mb-1">Hours</label>
        <input type="number" step="any" value="${initial.h}" class="sum-h w-full border border-gray-300 rounded px-2 py-1.5 text-sm focus:outline-none focus:ring-2 focus:ring-blue-500">
      </div>
      <div class="col-span-4">
        <label class="block text-xs text-gray-600 mb-1">Minutes</label>
        <input type="number" step="any" value="${initial.m}" class="sum-m w-full border border-gray-300 rounded px-2 py-1.5 text-sm focus:outline-none focus:ring-2 focus:ring-blue-500">
      </div>
      <div class="col-span-3">
        <label class="block text-xs text-gray-600 mb-1">Seconds</label>
        <input type="number" step="any" value="${initial.s}" class="sum-s w-full border border-gray-300 rounded px-2 py-1.5 text-sm focus:outline-none focus:ring-2 focus:ring-blue-500">
      </div>
      <div class="col-span-1 flex justify-end">
        <button type="button" class="remove-row text-gray-400 hover:text-red-500" title="Remove row">&times;</button>
      </div>
    `;
        sumRowsContainer.appendChild(row);
      }

      function recalcSum() {
        let totalSeconds = 0;
        sumRowsContainer.querySelectorAll(".grid").forEach((row) => {
          const h = row.querySelector(".sum-h").value;
          const m = row.querySelector(".sum-m").value;
          const s = row.querySelector(".sum-s").value;
          totalSeconds += toSeconds(h, m, s);
        });
        const hms = fromSeconds(totalSeconds);
        sumResultHms.textContent = formatHMS(hms);
        const totalMinutes = totalSeconds / 60;
        sumResultMins.textContent =
          "= " + formatNumber(totalMinutes, 4) + " minutes";
        sumResultSecs.textContent =
          "= " + formatNumber(totalSeconds, 4) + " seconds";
      }

      document.getElementById("add-row-btn").addEventListener("click", () => {
        createRow();
        recalcSum();
      });

      document
        .getElementById("clear-rows-btn")
        .addEventListener("click", () => {
          sumRowsContainer.innerHTML = "";
          createRow();
          recalcSum();
        });

      sumRowsContainer.addEventListener("input", (e) => {
        if (e.target.matches(".sum-h, .sum-m, .sum-s")) {
          recalcSum();
        }
      });

      sumRowsContainer.addEventListener("click", (e) => {
        if (e.target.classList.contains("remove-row")) {
          const row = e.target.closest(".grid");
          row.remove();
          if (sumRowsContainer.children.length === 0) {
            createRow();
          }
          recalcSum();
        }
      });

      // Initialize with two example rows
      createRow({ h: 1, m: 30, s: 0 });
      createRow({ h: 0, m: 45, s: 0 });
      recalcSum();

      // Difference tab logic
      const startTimeInput = document.getElementById("start-time");
      const endTimeInput = document.getElementById("end-time");
      const diffResultHms = document.getElementById("diff-result-hms");
      const diffResultMins = document.getElementById("diff-result-mins");
      const diffResultSecs = document.getElementById("diff-result-secs");

      function timeToSeconds(t) {
        if (!t) return 0;
        const parts = t.split(":").map(Number);
        const h = parts[0] || 0;
        const m = parts[1] || 0;
        const s = parts[2] || 0;
        return toSeconds(h, m, s);
      }

      function recalcDiff() {
        let startSec = timeToSeconds(startTimeInput.value);
        let endSec = timeToSeconds(endTimeInput.value);
        let diff = endSec - startSec;

        // If crossing midnight and user likely expects positive duration, also show that?
        // We'll keep raw diff; user can swap if needed.
        const hms = fromSeconds(diff);
        diffResultHms.textContent = formatHMS(hms);
        const mins = diff / 60;
        diffResultMins.textContent = "= " + formatNumber(mins, 4) + " minutes";
        diffResultSecs.textContent = "= " + formatNumber(diff, 4) + " seconds";
      }

      startTimeInput.addEventListener("input", recalcDiff);
      endTimeInput.addEventListener("input", recalcDiff);

      document
        .getElementById("swap-times-btn")
        .addEventListener("click", () => {
          const tmp = startTimeInput.value;
          startTimeInput.value = endTimeInput.value;
          endTimeInput.value = tmp;
          recalcDiff();
        });

      function setNowToInput(input) {
        const now = new Date();
        const hh = String(now.getHours()).padStart(2, "0");
        const mm = String(now.getMinutes()).padStart(2, "0");
        const ss = String(now.getSeconds()).padStart(2, "0");
        input.value = `${hh}:${mm}:${ss}`;
      }

      document
        .getElementById("now-as-start-btn")
        .addEventListener("click", () => {
          setNowToInput(startTimeInput);
          recalcDiff();
        });

      document
        .getElementById("now-as-end-btn")
        .addEventListener("click", () => {
          setNowToInput(endTimeInput);
          recalcDiff();
        });

      recalcDiff();

      // Convert tab logic
      const convHours = document.getElementById("conv-hours");
      const convMins = document.getElementById("conv-mins");
      const convSecs = document.getElementById("conv-secs");
      const convResultHms = document.getElementById("conv-result-hms");
      const convResultMins = document.getElementById("conv-result-mins");
      const convResultSecs = document.getElementById("conv-result-secs");

      let convUpdating = false;

      function updateFromSeconds(totalSeconds, source) {
        convUpdating = true;
        if (source !== "hours") {
          convHours.value = totalSeconds / 3600;
        }
        if (source !== "mins") {
          convMins.value = totalSeconds / 60;
        }
        if (source !== "secs") {
          convSecs.value = totalSeconds;
        }
        const hms = fromSeconds(totalSeconds);
        convResultHms.textContent = formatHMS(hms);
        convResultMins.textContent =
          "= " + formatNumber(totalSeconds / 60, 6) + " minutes";
        convResultSecs.textContent =
          "= " + formatNumber(totalSeconds, 6) + " seconds";
        convUpdating = false;
      }

      convHours.addEventListener("input", () => {
        if (convUpdating) return;
        const totalSeconds = (Number(convHours.value) || 0) * 3600;
        updateFromSeconds(totalSeconds, "hours");
      });

      convMins.addEventListener("input", () => {
        if (convUpdating) return;
        const totalSeconds = (Number(convMins.value) || 0) * 60;
        updateFromSeconds(totalSeconds, "mins");
      });

      convSecs.addEventListener("input", () => {
        if (convUpdating) return;
        const totalSeconds = Number(convSecs.value) || 0;
        updateFromSeconds(totalSeconds, "secs");
      });

      // Initialize convert tab
      updateFromSeconds(3600, "hours");

      // Copy buttons
      document.querySelectorAll(".copy-btn").forEach((btn) => {
        btn.addEventListener("click", () => {
          const id = btn.getAttribute("data-copy-source");
          const el = document.getElementById(id);
          if (!el) return;
          const text = el.textContent.replace(/^=\s*/, "");
          navigator.clipboard
            .writeText(text)
            .then(() => {
              btn.textContent = "Copied!";
              setTimeout(() => {
                btn.textContent = "Copy minutes";
              }, 1200);
            })
            .catch(() => {});
        });
      });

      // MathJax config
      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
