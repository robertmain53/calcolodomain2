    const cordOutput = document.getElementById('cord-output');
    const cordLabel = document.getElementById('cord-label');

    let currentCordSize = 128;
    let isUpdating = false;

    function updateFromCords() {
      if (isUpdating) return;
      isUpdating = true;
      const cords = parseFloat(cordInput.value || 0);
      const ft3 = cords * currentCordSize;
      cubicFtOutput.value = +ft3.toFixed(2);
