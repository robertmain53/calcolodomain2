    }

    function updateFromFt3() {
      if (isUpdating) return;
      isUpdating = true;
      const ft3 = parseFloat(cubicFtInput.value || 0);
      const cords = currentCordSize ? ft3 / currentCordSize : 0;
      cordOutput.value = +cords.toFixed(4);
      isUpdating = false;
    }

    cordButtons.forEach(btn => {
      btn.addEventListener('click', () => {
        cordButtons.forEach(b => b.classList.remove('mode-pill-active'));
        btn.classList.add('mode-pill-active');
        currentCordSize = parseFloat(btn.dataset.volume);
        cordLabel.textContent = currentCordSize.toFixed(2) + ' ft³ per cord';
        updateFromCords();
        updateFromFt3();
      });
    });

    cordInput.addEventListener('input', updateFromCords);
    cubicFtInput.addEventListener('input', updateFromFt3);

    // init
    updateFromCords();
    updateFromFt3();

    // fill quick table (full cord = 128, face = 42.6667)
    document.getElementById('tbl-0-25-full').textContent = (0.25 * 128).toFixed(2);
    document.getElementById('tbl-0-25-face').textContent = (0.25 * 42.6667).toFixed(2);
    document.getElementById('tbl-0-5-full').textContent = (0.5 * 128).toFixed(2);
    document.getElementById('tbl-0-5-face').textContent = (0.5 * 42.6667).toFixed(2);
    document.getElementById('tbl-1-full').textContent = (1 * 128).toFixed(2);
    document.getElementById('tbl-1-face').textContent = (1 * 42.6667).toFixed(2);
    document.getElementById('tbl-1-5-full').textContent = (1.5 * 128).toFixed(2);
    document.getElementById('tbl-1-5-face').textContent = (1.5 * 42.6667).toFixed(2);
    document.getElementById('tbl-2-full').textContent = (2 * 128).toFixed(2);
    document.getElementById('tbl-2-face').textContent = (2 * 42.6667).toFixed(2);
  </script>

