            <script>
              document.addEventListener('DOMContentLoaded', function() {
                const tabContainer = document.getElementById('tab-container');
                const tabs = document.querySelectorAll('.tab-button');
                const panels = document.querySelectorAll('.tab-panel');
                const calculateBtn = document.getElementById('calculate-btn');
                const resultsContainer = document.getElementById('results-container');
                const errorMessage = document.getElementById('error-message');
                const resultVolume = document.getElementById('result-volume');
                const resultLateralArea = document.getElementById('result-lateral-area');
                const resultTotalArea = document.getElementById('result-total-area');
                const stepByStep = document.getElementById('step-by-step');

                let activeCase = 'rectangular'; 
                
                // --- Helper Functions ---
                function parseInput(val) {
                    const num = parseFloat(val);
                    return (num > 0 && !isNaN(num)) ? num : NaN;
                }
                
                function formatNumber(num) {
                    if (Math.abs(num) < 1e-10) return '0';
                    return parseFloat(num.toFixed(6)).toString();
                }

                function showError(message) {
                    errorMessage.textContent = `Error: ${message}`;
                    resultsContainer.classList.add('hidden');
                }

                // --- Tab Switching ---
                tabs.forEach(tab => {
                  tab.addEventListener('click', () => {
                    tabs.forEach(t => t.classList.remove('active'));
                    panels.forEach(p => p.classList.remove('active'));
                    
                    tab.classList.add('active');
                    activeCase = tab.getAttribute('data-tab');
                    document.getElementById(activeCase).classList.add('active');
                    
                    resultsContainer.classList.add('hidden');
                    errorMessage.textContent = '';
                  });
                });
                
                // --- Main Solver Logic ---
                calculateBtn.addEventListener('click', () => {
                    try {
                        errorMessage.textContent = '';
                        let V, B, P, LSA, TSA, L;
                        let steps = '';
                        
                        if (activeCase === 'rectangular') {
                            const l = parseInput(document.getElementById('rect-l').value);
                            const w = parseInput(document.getElementById('rect-w').value);
                            const h = parseInput(document.getElementById('rect-h').value);

                            if (isNaN(l) || isNaN(w) || isNaN(h)) throw new Error('Please enter valid, positive numbers for Length, Width, and Height.');
                            L = h;

                            // Base Area (B)
                            B = l * w;
                            // Base Perimeter (P)
                            P = 2 * l + 2 * w;
                            // Volume (V)
                            V = B * L;
                            // Lateral Surface Area (LSA)
                            LSA = P * L;
                            // Total Surface Area (TSA)
                            TSA = LSA + 2 * B;

                            steps = `
                                <h4 class="font-semibold text-lg">1. Calculate Base Area ($B$):</h4>
                                <p>Formula: $B = l \\cdot w$</p>
                                <p class="formula-box text-center">$B = ${formatNumber(l)} \\times ${formatNumber(w)} = ${formatNumber(B)}$</p>

                                <h4 class="font-semibold text-lg mt-4">2. Calculate Base Perimeter ($P$):</h4>
                                <p>Formula: $P = 2l + 2w$</p>
                                <p class="formula-box text-center">$P = 2 \\times ${formatNumber(l)} + 2 \\times ${formatNumber(w)} = ${formatNumber(P)}$</p>
                                
                                <h4 class="font-semibold text-lg mt-4">3. Calculate Volume ($V$):</h4>
                                <p>Formula: $V = B \\cdot h$</p>
                                <p class="formula-box text-center">$V = ${formatNumber(B)} \\times ${formatNumber(h)} = ${formatNumber(V)}$</p>
                                
                                <h4 class="font-semibold text-lg mt-4">4. Calculate Total Surface Area ($TSA$):</h4>
                                <p>Formula: $TSA = P \\cdot h + 2B$</p>
                                <p>Lateral Area ($LSA$): $LSA = ${formatNumber(P)} \\times ${formatNumber(h)} = ${formatNumber(LSA)}$</p>
                                <p class="formula-box text-center">$TSA = ${formatNumber(LSA)} + 2 \\times ${formatNumber(B)} = ${formatNumber(TSA)}$</p>
                            `;
                            
                        } else if (activeCase === 'triangular') {
                            const b_tri = parseInput(document.getElementById('tri-b').value);
                            const h_tri = parseInput(document.getElementById('tri-hb').value);
                            const L_prism = parseInput(document.getElementById('tri-L').value);
                            const s1 = parseInput(document.getElementById('tri-s1').value);
                            const s2 = parseInput(document.getElementById('tri-s2').value);

                            if (isNaN(b_tri) || isNaN(h_tri) || isNaN(L_prism)) throw new Error('Please enter valid, positive numbers for Base Side, Base Height, and Prism Length.');
                            if (isNaN(s1) || isNaN(s2)) throw new Error('Please enter valid, positive numbers for all three sides of the triangular base (b, s1, s2) to calculate Total Area.');

                            L = L_prism;

                            // Base Area (B)
                            B = 0.5 * b_tri * h_tri;
                            // Base Perimeter (P)
                            P = b_tri + s1 + s2;
                            // Volume (V)
                            V = B * L;
                            // Lateral Surface Area (LSA)
                            LSA = P * L;
                            // Total Surface Area (TSA)
                            TSA = LSA + 2 * B;

                             steps = `
                                <h4 class="font-semibold text-lg">1. Calculate Base Area ($B$) of the Triangle:</h4>
                                <p>Formula: $B = \\frac{1}{2} \\cdot b_{tri} \\cdot h_{tri}$</p>
                                <p class="formula-box text-center">$B = \\frac{1}{2} \\times ${formatNumber(b_tri)} \\times ${formatNumber(h_tri)} = ${formatNumber(B)}$</p>

                                <h4 class="font-semibold text-lg mt-4">2. Calculate Base Perimeter ($P$):</h4>
                                <p>Formula: $P = b_{tri} + s_1 + s_2$</p>
                                <p class="formula-box text-center">$P = ${formatNumber(b_tri)} + ${formatNumber(s1)} + ${formatNumber(s2)} = ${formatNumber(P)}$</p>
                                
                                <h4 class="font-semibold text-lg mt-4">3. Calculate Volume ($V$):</h4>
                                <p>Formula: $V = B \\cdot L$</p>
                                <p class="formula-box text-center">$V = ${formatNumber(B)} \\times ${formatNumber(L)} = ${formatNumber(V)}$</p>
                                
                                <h4 class="font-semibold text-lg mt-4">4. Calculate Total Surface Area ($TSA$):</h4>
                                <p>Formula: $TSA = P \\cdot L + 2B$</p>
                                <p>Lateral Area ($LSA$): $LSA = ${formatNumber(P)} \\times ${formatNumber(L)} = ${formatNumber(LSA)}$</p>
                                <p class="formula-box text-center">$TSA = ${formatNumber(LSA)} + 2 \\times ${formatNumber(B)} = ${formatNumber(TSA)}$</p>
                            `;
                        } else {
                            throw new Error('Internal error: Unknown prism case.');
                        }
                        
                        // 5. Update DOM
                        resultVolume.textContent = formatNumber(V);
                        resultLateralArea.textContent = formatNumber(LSA);
                        resultTotalArea.textContent = formatNumber(TSA);
                        stepByStep.innerHTML = steps;
                        resultsContainer.classList.remove('hidden');

                        // Re-render MathJax
                        if (window.MathJax) {
                            window.MathJax.typesetPromise([stepByStep]);
                        }

                    } catch (e) {
                        showError(e.message);
                    }
                });

                // Initial calculation (for example values)
                document.getElementById('rect-l').value = 5;
                document.getElementById('rect-w').value = 4;
                document.getElementById('rect-h').value = 3;
                document.getElementById('tri-b').value = 6;
                document.getElementById('tri-hb').value = 4;
                document.getElementById('tri-L').value = 10;
                document.getElementById('tri-s1').value = 5;
                document.getElementById('tri-s2').value = 5;
                
                // Function to set the active tab (used to re-click the tab after initial load)
