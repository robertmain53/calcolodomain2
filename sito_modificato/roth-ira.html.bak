    <script>
      // Utility: format currency
      function formatCurrency(value) {
        if (!isFinite(value)) return "$0";
        const abs = Math.abs(value);
        const sign = value < 0 ? "-" : "";
        return (
          sign +
          abs.toLocaleString(undefined, {
            style: "currency",
            currency: "USD",
            maximumFractionDigits: 0,
          })
        );
      }

      function calculateRothIRA() {
        const currentAge =
          parseFloat(document.getElementById("current-age").value) || 0;
        const retirementAge =
          parseFloat(document.getElementById("retirement-age").value) || 0;
        const years = Math.max(0, retirementAge - currentAge);

        let balance = Math.max(
          0,
          parseFloat(document.getElementById("current-balance").value) || 0
        );
        let annualContribution = Math.max(
          0,
          parseFloat(document.getElementById("annual-contribution").value) || 0
        );
        const contribIncrease =
          (parseFloat(document.getElementById("annual-increase").value) || 0) /
          100;
        const rate =
          (parseFloat(document.getElementById("rate-of-return").value) || 0) /
          100;
        const inflation =
          (parseFloat(document.getElementById("inflation-rate").value) || 0) /
          100;

        const currentTax =
          (parseFloat(document.getElementById("current-tax-rate").value) || 0) /
          100;
        const retirementTax =
          (parseFloat(document.getElementById("retirement-tax-rate").value) ||
            0) / 100;

        let totalContributions = 0;
        let totalGrowth = 0;

        // For traditional IRA comparison: assume same pre-tax dollars as Roth contribution / (1 - currentTax)
        const rothAfterTaxContribution = annualContribution;
        const preTaxContribution =
          currentTax < 1
            ? rothAfterTaxContribution / (1 - currentTax)
            : rothAfterTaxContribution;
        let tradBalance = balance; // assume same starting balance for comparison

        for (let t = 0; t < years; t++) {
          // Grow existing balances
          balance *= 1 + rate;
          tradBalance *= 1 + rate;

          // Contribution for this year
          const contribThisYear =
            annualContribution * Math.pow(1 + contribIncrease, t);
          balance += contribThisYear;
          tradBalance += preTaxContribution * Math.pow(1 + contribIncrease, t);

          totalContributions += contribThisYear;
        }

        const futureBalance = balance;
        totalGrowth = Math.max(
          0,
          futureBalance -
            (parseFloat(document.getElementById("current-balance").value) ||
              0) -
            totalContributions
        );

        const realBalance =
          inflation > -1 && years > 0
            ? futureBalance / Math.pow(1 + inflation, years)
            : futureBalance;

        // Traditional IRA after-tax value
        const tradFuture = tradBalance;
        const tradAfterTax = tradFuture * (1 - retirementTax);

        // Roth after-tax is just the balance (assuming qualified)
        const rothAfterTax = futureBalance;

        // Update DOM
        document.getElementById("result-future-balance").textContent =
          formatCurrency(futureBalance);
        document.getElementById("result-real-balance").textContent =
          formatCurrency(realBalance);
        document.getElementById("result-total-contributions").textContent =
          formatCurrency(totalContributions);
        document.getElementById("result-total-growth").textContent =
          formatCurrency(totalGrowth);
        document.getElementById("result-roth-after-tax").textContent =
          formatCurrency(rothAfterTax);
        document.getElementById("result-trad-after-tax").textContent =
          formatCurrency(tradAfterTax);

        const betterLabelEl = document.getElementById("result-better-label");
        const betterAmountEl = document.getElementById("result-better-amount");
        const diff = rothAfterTax - tradAfterTax;

        if (Math.abs(diff) < 1) {
          betterLabelEl.textContent =
            "Roth and traditional are roughly equivalent under these assumptions.";
          betterAmountEl.textContent = "";
        } else if (diff > 0) {
          betterLabelEl.textContent = "Roth advantage (after tax):";
          betterAmountEl.textContent = formatCurrency(diff);
        } else {
          betterLabelEl.textContent = "Traditional advantage (after tax):";
          betterAmountEl.textContent = formatCurrency(-diff);
        }
      }

      document.addEventListener("DOMContentLoaded", function () {
        const annualIncreaseInput = document.getElementById("annual-increase");
        const annualIncreaseValue = document.getElementById(
          "annual-increase-value"
        );
        const rateInput = document.getElementById("rate-of-return");
        const rateValue = document.getElementById("rate-of-return-value");
        const inflationInput = document.getElementById("inflation-rate");
        const inflationValue = document.getElementById("inflation-rate-value");
        const maxContributionBtn = document.getElementById(
          "max-contribution-btn"
        );

        function syncRange(input, label, suffix = "%") {
          label.textContent = (parseFloat(input.value) || 0) + suffix;
        }

        syncRange(annualIncreaseInput, annualIncreaseValue);
        syncRange(rateInput, rateValue);
        syncRange(inflationInput, inflationValue);

        annualIncreaseInput.addEventListener("input", () =>
          syncRange(annualIncreaseInput, annualIncreaseValue)
        );
        rateInput.addEventListener("input", () =>
          syncRange(rateInput, rateValue)
        );
        inflationInput.addEventListener("input", () =>
          syncRange(inflationInput, inflationValue)
        );

        document
          .getElementById("calculate-btn")
          .addEventListener("click", calculateRothIRA);

        // Simple helper: set a typical IRS max contribution (user can adjust)
        maxContributionBtn.addEventListener("click", function () {
          const age =
            parseFloat(document.getElementById("current-age").value) || 0;
          // Example: base limit 7000, catch-up 1000 if 50+
          const baseLimit = 7000;
          const catchUp = age >= 50 ? 1000 : 0;
          document.getElementById("annual-contribution").value =
            baseLimit + catchUp;
          calculateRothIRA();
        });

        // Auto-calc on load
        calculateRothIRA();
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
