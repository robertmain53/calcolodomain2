<script>
  // Utility: clamp
  function clamp(val, min, max) {
    return Math.min(Math.max(val, min), max);
  }

  // Map age to bracket index (0..8)
  function getAgeBracket(age) {
    if (age <= 20) return 0;
    if (age <= 25) return 1;
    if (age <= 30) return 2;
    if (age <= 35) return 3;
    if (age <= 40) return 4;
    if (age <= 45) return 5;
    if (age <= 50) return 6;
    if (age <= 55) return 7;
    return 8;
  }

  // Approximate thresholds per gender & age bracket
  const thresholds = {
    male: {
      pullups: [
        { min: 3, max: 23 }, { min: 3, max: 22 }, { min: 3, max: 21 },
        { min: 3, max: 20 }, { min: 3, max: 19 }, { min: 3, max: 18 },
        { min: 3, max: 17 }, { min: 3, max: 16 }, { min: 3, max: 15 }
      ],
      pushups: [
        { min: 40, max: 82 }, { min: 38, max: 80 }, { min: 36, max: 78 },
        { min: 34, max: 76 }, { min: 32, max: 74 }, { min: 30, max: 72 },
        { min: 28, max: 70 }, { min: 26, max: 68 }, { min: 24, max: 66 }
      ],
      crunches: [
        { min: 40, max: 115 }, { min: 40, max: 110 }, { min: 40, max: 105 },
        { min: 40, max: 100 }, { min: 40, max: 95 },  { min: 40, max: 90 },
        { min: 40, max: 85 },  { min: 40, max: 80 },  { min: 40, max: 75 }
      ],
      plankSec: [
        { min: 45, max: 225 }, { min: 45, max: 220 }, { min: 45, max: 215 },
        { min: 45, max: 210 }, { min: 45, max: 205 }, { min: 45, max: 200 },
        { min: 45, max: 195 }, { min: 45, max: 190 }, { min: 45, max: 185 }
      ],
      runSec: [
        { min: 28*60, max: 18*60 }, { min: 28*60, max: 18*60+30 },
        { min: 28*60+30, max: 19*60 }, { min: 29*60, max: 19*60+30 },
        { min: 29*60+30, max: 20*60 }, { min: 30*60, max: 20*60+30 },
        { min: 30*60+30, max: 21*60 }, { min: 31*60, max: 21*60+30 },
        { min: 31*60+30, max: 22*60 }
      ]
    },
    female: {
      pullups: [
        { min: 1, max: 12 }, { min: 1, max: 11 }, { min: 1, max: 10 },
        { min: 1, max: 9 },  { min: 1, max: 8 },  { min: 1, max: 7 },
        { min: 1, max: 6 },  { min: 1, max: 5 },  { min: 1, max: 4 }
      ],
      pushups: [
        { min: 20, max: 60 }, { min: 20, max: 58 }, { min: 18, max: 56 },
        { min: 18, max: 54 }, { min: 16, max: 52 }, { min: 16, max: 50 },
        { min: 14, max: 48 }, { min: 14, max: 46 }, { min: 12, max: 44 }
      ],
      crunches: [
        { min: 40, max: 110 }, { min: 40, max: 105 }, { min: 40, max: 100 },
        { min: 40, max: 95 },  { min: 40, max: 90 },  { min: 40, max: 85 },
        { min: 40, max: 80 },  { min: 40, max: 75 },  { min: 40, max: 70 }
      ],
      plankSec: [
        { min: 45, max: 210 }, { min: 45, max: 205 }, { min: 45, max: 200 },
        { min: 45, max: 195 }, { min: 45, max: 190 }, { min: 45, max: 185 },
        { min: 45, max: 180 }, { min: 45, max: 175 }, { min: 45, max: 170 }
      ],
      runSec: [
        { min: 31*60, max: 21*60 }, { min: 31*60, max: 21*60+30 },
        { min: 31*60+30, max: 22*60 }, { min: 32*60, max: 22*60+30 },
        { min: 32*60+30, max: 23*60 }, { min: 33*60, max: 23*60+30 },
        { min: 33*60+30, max: 24*60 }, { min: 34*60, max: 24*60+30 },
        { min: 34*60+30, max: 25*60 }
      ]
    }
  };

  function scoreLinear(value, minVal, maxVal, reverse = false) {
    if (!isFinite(value)) return 0;
    if (!reverse) {
      if (value <= minVal) return 0;
      if (value >= maxVal) return 100;
      return ((value - minVal) / (maxVal - minVal)) * 100;
    } else {
      if (value >= minVal) return 0;
      if (value <= maxVal) return 100;
      return ((minVal - value) / (minVal - maxVal)) * 100;
    }
  }

  function classifyScore(total) {
    if (!isFinite(total) || total <= 0) return { cls: "Fail", desc: "Incomplete or invalid score." };
    if (total >= 235) return { cls: "1st Class", desc: "Excellent – meets top-tier Marine Corps fitness standards." };
    if (total >= 200) return { cls: "2nd Class", desc: "Good – meets standards with room for improvement." };
    if (total >= 150) return { cls: "3rd Class", desc: "Minimum passing – significant improvement recommended." };
    return { cls: "Fail", desc: "Below minimum standard – does not meet PFT requirements." };
  }

  function updateCoreInputs() {
    const coreEvent = document.querySelector('input[name="coreEvent"]:checked').value;
    document.getElementById('core-input-crunches').classList.toggle('hidden', coreEvent !== 'crunches');
    document.getElementById('core-input-plank').classList.toggle('hidden', coreEvent !== 'plank');
  }

  document.querySelectorAll('input[name="coreEvent"]').forEach(r => {
    r.addEventListener('change', updateCoreInputs);
  });

  document.getElementById('resetBtn').addEventListener('click', () => {
    document.getElementById('gender').value = 'male';
    document.getElementById('age').value = 22;
    document.querySelector('input[name="upperEvent"][value="pullups"]').checked = true;
    document.getElementById('upperReps').value = 15;
    document.querySelector('input[name="coreEvent"][value="crunches"]').checked = true;
    updateCoreInputs();
    document.getElementById('crunchReps').value = 80;
    document.getElementById('plankMin').value = 3;
    document.getElementById('plankSec').value = 0;
    document.getElementById('runMin').value = 22;
    document.getElementById('runSec').value = 0;
    document.getElementById('validationMsg').textContent = '';
    document.getElementById('totalScore').textContent = '—';
    document.getElementById('classBadge').textContent = '—';
    document.getElementById('classBadge').className = 'badge bg-gray-200 text-gray-700';
    document.getElementById('classDescription').textContent = '';
    document.getElementById('upperDetail').textContent = '—';
    document.getElementById('upperScore').textContent = 'Score: —';
    document.getElementById('coreDetail').textContent = '—';
    document.getElementById('coreScore').textContent = 'Score: —';
    document.getElementById('runDetail').textContent = '—';
    document.getElementById('runScore').textContent = 'Score: —';
  });

  document.getElementById('calculateBtn').addEventListener('click', () => {
    const gender = document.getElementById('gender').value === 'female' ? 'female' : 'male';
    const age = parseInt(document.getElementById('age').value, 10);
    const validationMsg = document.getElementById('validationMsg');
    validationMsg.textContent = '';

    if (!age || age < 17 || age > 60) {
      validationMsg.textContent = 'Please enter an age between 17 and 60.';
      return;
    }

    const bracket = getAgeBracket(age);
    const t = thresholds[gender];

    // Upper body
    const upperEvent = document.querySelector('input[name="upperEvent"]:checked').value;
    const upperReps = parseInt(document.getElementById('upperReps').value, 10);
    if (!Number.isFinite(upperReps) || upperReps < 0) {
      validationMsg.textContent = 'Please enter a valid number of repetitions for the upper-body event.';
      return;
    }
    let upperScoreVal = 0;
    if (upperEvent === 'pullups') {
      const th = t.pullups[bracket];
      upperScoreVal = scoreLinear(upperReps, th.min, th.max, false);
    } else {
      const th = t.pushups[bracket];
      upperScoreVal = scoreLinear(upperReps, th.min, th.max, false);
    }
    upperScoreVal = clamp(Math.round(upperScoreVal), 0, 100);

    // Core
    const coreEvent = document.querySelector('input[name="coreEvent"]:checked').value;
    let coreScoreVal = 0;
    let coreDetailText = '';
    if (coreEvent === 'crunches') {
      const crunchReps = parseInt(document.getElementById('crunchReps').value, 10);
      if (!Number.isFinite(crunchReps) || crunchReps < 0) {
        validationMsg.textContent = 'Please enter a valid number of crunches.';
        return;
      }
      const th = t.crunches[bracket];
      coreScoreVal = scoreLinear(crunchReps, th.min, th.max, false);
      coreDetailText = `${crunchReps} crunches (2 min)`;
    } else {
      const min = parseInt(document.getElementById('plankMin').value, 10);
      const sec = parseInt(document.getElementById('plankSec').value, 10);
      if (!Number.isFinite(min) || !Number.isFinite(sec) || min < 0 || sec < 0 || sec > 59) {
        validationMsg.textContent = 'Please enter a valid plank time.';
        return;
      }
      const totalSec = min * 60 + sec;
      const th = t.plankSec[bracket];
      coreScoreVal = scoreLinear(totalSec, th.min, th.max, false);
      coreDetailText = `Plank: ${min}m ${sec}s`;
    }
    coreScoreVal = clamp(Math.round(coreScoreVal), 0, 100);

    // Run
    const runMin = parseInt(document.getElementById('runMin').value, 10);
    const runSec = parseInt(document.getElementById('runSec').value, 10);
    if (!Number.isFinite(runMin) || !Number.isFinite(runSec) || runMin <= 0 || runSec < 0 || runSec > 59) {
      validationMsg.textContent = 'Please enter a valid 3-mile run time.';
      return;
    }
    const runTotalSec = runMin * 60 + runSec;
    const runTh = t.runSec[bracket];
    let runScoreVal = scoreLinear(runTotalSec, runTh.min, runTh.max, true);
    runScoreVal = clamp(Math.round(runScoreVal), 0, 100);

    const total = upperScoreVal + coreScoreVal + runScoreVal;
    const cls = classifyScore(total);

    // Update UI
    document.getElementById('totalScore').textContent = `${total} / 300`;

    const badge = document.getElementById('classBadge');
    badge.textContent = cls.cls;
    badge.className = 'badge text-white ' +
      (cls.cls === '1st Class' ? 'bg-green-600' :
       cls.cls === '2nd Class' ? 'bg-blue-600' :
       cls.cls === '3rd Class' ? 'bg-yellow-500' :
       'bg-red-600');
    document.getElementById('classDescription').textContent = cls.desc;

    const upperDetail = upperEvent === 'pullups'
      ? `${upperReps} pull-ups`
      : `${upperReps} push-ups`;
    document.getElementById('upperDetail').textContent = upperDetail;
    document.getElementById('upperScore').textContent = `Score: ${upperScoreVal} / 100`;

    document.getElementById('coreDetail').textContent = coreDetailText;
    document.getElementById('coreScore').textContent = `Score: ${coreScoreVal} / 100`;

    document.getElementById('runDetail').textContent = `3-mile run: ${runMin}m ${runSec}s`;
    document.getElementById('runScore').textContent = `Score: ${runScoreVal} / 100`;
  });

  // Initialize core inputs on load
  updateCoreInputs();

  window.MathJax = {
    tex: { inlineMath: [['\\(','\\)'], ['$', '$']], displayMath: [['$','$'], ['\\[','\\]']] },
    svg: { fontCache: 'global' }
  };
</script>
