    <script>
      // Dynamic groups / resistors
      const groupsContainer = document.getElementById("groupsContainer");
      const addGroupBtn = document.getElementById("addGroupBtn");
      const resetBtn = document.getElementById("resetBtn");
      const calculateBtn = document.getElementById("calculateBtn");
      const maxGroupsInput = document.getElementById("maxGroups");
      const resUnitSelect = document.getElementById("resUnit");
      const supplyVoltageInput = document.getElementById("supplyVoltage");

      let groupCount = 0;

      function createGroupElement(index) {
        const groupId = `group-${index}`;
        const wrapper = document.createElement("div");
        wrapper.className = "border rounded-lg p-3 bg-gray-50";
        wrapper.dataset.groupIndex = index;

        wrapper.innerHTML = `
      <div class="flex justify-between items-center mb-2">
        <div class="flex items-center gap-2">
          <span class="font-semibold">Group ${index + 1}</span>
          <span class="badge bg-blue-100 text-blue-700" data-group-type-label>Series</span>
        </div>
        <div class="flex items-center gap-2">
          <label class="text-xs text-gray-600">
            Type:
            <select class="ml-1 text-xs border border-gray-300 rounded-md px-1 py-0.5 focus:ring-1 focus:ring-blue-500 focus:outline-none" data-group-type>
              <option value="series">Series</option>
              <option value="parallel">Parallel</option>
            </select>
          </label>
          <button type="button" class="text-xs text-red-600 hover:underline" data-remove-group>Remove</button>
        </div>
      </div>
      <div class="space-y-2" data-resistor-list></div>
      <button type="button" class="mt-2 inline-flex items-center px-3 py-1 bg-gray-200 text-xs rounded-md hover:bg-gray-300" data-add-resistor>
        + Add resistor
      </button>
    `;

        const typeSelect = wrapper.querySelector("[data-group-type]");
        const typeLabel = wrapper.querySelector("[data-group-type-label]");
        typeSelect.addEventListener("change", () => {
          typeLabel.textContent =
            typeSelect.value === "series" ? "Series" : "Parallel";
          typeLabel.className =
            "badge " +
            (typeSelect.value === "series"
              ? "bg-blue-100 text-blue-700"
              : "bg-purple-100 text-purple-700");
        });

        wrapper
          .querySelector("[data-add-resistor]")
          .addEventListener("click", () => {
            addResistorRow(wrapper);
          });

        wrapper
          .querySelector("[data-remove-group]")
          .addEventListener("click", () => {
            wrapper.remove();
            updateGroupLabels();
            groupCount = document.querySelectorAll("[data-group-index]").length;
          });

        // Add first resistor row by default
        addResistorRow(wrapper);

        return wrapper;
      }

      function addResistorRow(groupElement) {
        const list = groupElement.querySelector("[data-resistor-list]");
        const row = document.createElement("div");
        row.className = "flex items-center gap-2";

        row.innerHTML = `
      <label class="text-xs text-gray-600 w-16">R<span data-res-index></span></label>
      <div class="flex-1 flex">
        <input type="number" min="0" step="0.0001" class="flex-1 border border-gray-300 rounded-l-md px-2 py-1 text-sm focus:ring-2 focus:ring-blue-500 focus:outline-none" placeholder="e.g. 1">
        <span class="inline-flex items-center px-2 border border-l-0 border-gray-300 bg-gray-100 rounded-r-md text-xs text-gray-600">unit</span>
      </div>
      <button type="button" class="text-xs text-red-600 hover:underline" data-remove-resistor>Remove</button>
    `;

        row
          .querySelector("[data-remove-resistor]")
          .addEventListener("click", () => {
            row.remove();
            updateResistorLabels(groupElement);
          });

        list.appendChild(row);
        updateResistorLabels(groupElement);
      }

      function updateGroupLabels() {
        document.querySelectorAll("[data-group-index]").forEach((el, idx) => {
          el.dataset.groupIndex = idx;
          const label = el.querySelector("span.font-semibold");
          if (label) label.textContent = `Group ${idx + 1}`;
        });
      }

      function updateResistorLabels(groupElement) {
        const rows = groupElement.querySelectorAll(
          "[data-resistor-list] > div"
        );
        rows.forEach((row, idx) => {
          const labelSpan = row.querySelector("[data-res-index]");
          if (labelSpan) labelSpan.textContent = idx + 1;
        });
      }

      function addGroup() {
        const maxGroups = parseInt(maxGroupsInput.value, 10) || 1;
        if (groupCount >= maxGroups) return;
        const groupEl = createGroupElement(groupCount);
        groupsContainer.appendChild(groupEl);
        groupCount++;
      }

      addGroupBtn.addEventListener("click", addGroup);

      resetBtn.addEventListener("click", () => {
        groupsContainer.innerHTML = "";
        groupCount = 0;
        addGroup();
        supplyVoltageInput.value = "";
        document.getElementById("eqResistanceDisplay").textContent = "–";
        document.getElementById("eqResistanceAltDisplay").textContent = "";
        document.getElementById("totalCurrentDisplay").textContent = "–";
        document.getElementById("totalPowerDisplay").textContent = "–";
        document.getElementById("detailedResults").classList.add("hidden");
        document.getElementById("groupDetailsContainer").innerHTML = "";
      });

      // Initialize with one group
      addGroup();

      function parseCircuit() {
        const groups = [];
        const unitFactor = parseFloat(resUnitSelect.value) || 1;

        document.querySelectorAll("[data-group-index]").forEach((groupEl) => {
          const type = groupEl.querySelector("[data-group-type]").value;
          const resistors = [];
          groupEl
            .querySelectorAll('[data-resistor-list] input[type="number"]')
            .forEach((input) => {
              const val = parseFloat(input.value);
              if (!isNaN(val) && val > 0) {
                resistors.push(val * unitFactor); // convert to ohms
              }
            });
          if (resistors.length > 0) {
            groups.push({ type, resistors });
          }
        });

        return groups;
      }

      function calcSeries(resistors) {
        return resistors.reduce((sum, r) => sum + r, 0);
      }

      function calcParallel(resistors) {
        let inv = 0;
        for (const r of resistors) {
          if (r <= 0) return NaN;
          inv += 1 / r;
        }
        return inv === 0 ? NaN : 1 / inv;
      }

      function formatOhms(R) {
        if (!isFinite(R)) return "–";
        if (R >= 1e6) return (R / 1e6).toFixed(3).replace(/\.?0+$/, "") + " MΩ";
        if (R >= 1e3) return (R / 1e3).toFixed(3).replace(/\.?0+$/, "") + " kΩ";
        return R.toFixed(3).replace(/\.?0+$/, "") + " Ω";
      }

      function formatCurrent(I) {
        if (!isFinite(I)) return "–";
        const abs = Math.abs(I);
        if (abs >= 1) return I.toFixed(3).replace(/\.?0+$/, "") + " A";
        if (abs >= 1e-3)
          return (I * 1e3).toFixed(3).replace(/\.?0+$/, "") + " mA";
        return (I * 1e6).toFixed(3).replace(/\.?0+$/, "") + " µA";
      }

      function formatVoltage(V) {
        if (!isFinite(V)) return "–";
        return V.toFixed(3).replace(/\.?0+$/, "") + " V";
      }

      function formatPower(P) {
        if (!isFinite(P)) return "–";
        const abs = Math.abs(P);
        if (abs >= 1) return P.toFixed(3).replace(/\.?0+$/, "") + " W";
        if (abs >= 1e-3)
          return (P * 1e3).toFixed(3).replace(/\.?0+$/, "") + " mW";
        return (P * 1e6).toFixed(3).replace(/\.?0+$/, "") + " µW";
      }

      calculateBtn.addEventListener("click", () => {
        const groups = parseCircuit();
        const eqDisplay = document.getElementById("eqResistanceDisplay");
        const eqAltDisplay = document.getElementById("eqResistanceAltDisplay");
        const totalCurrentDisplay = document.getElementById(
          "totalCurrentDisplay"
        );
        const totalPowerDisplay = document.getElementById("totalPowerDisplay");
        const detailsContainer = document.getElementById(
          "groupDetailsContainer"
        );
        const detailedSection = document.getElementById("detailedResults");

        if (groups.length === 0) {
          eqDisplay.textContent = "–";
          eqAltDisplay.textContent = "Enter at least one resistor.";
          totalCurrentDisplay.textContent = "–";
          totalPowerDisplay.textContent = "–";
          detailedSection.classList.add("hidden");
          detailsContainer.innerHTML = "";
          return;
        }

        // Compute equivalent of each group
        const groupResults = [];
        let totalReq = 0;
        for (let i = 0; i < groups.length; i++) {
          const g = groups[i];
          const Req =
            g.type === "series"
              ? calcSeries(g.resistors)
              : calcParallel(g.resistors);
          if (!isFinite(Req) || Req <= 0) {
            eqDisplay.textContent = "–";
            eqAltDisplay.textContent =
              "Invalid resistor values in one of the groups.";
            totalCurrentDisplay.textContent = "–";
            totalPowerDisplay.textContent = "–";
            detailedSection.classList.add("hidden");
            detailsContainer.innerHTML = "";
            return;
          }
          groupResults.push({ ...g, Req });
          totalReq += Req;
        }

        eqDisplay.textContent = formatOhms(totalReq);
        eqAltDisplay.textContent = `= ${totalReq
          .toFixed(3)
          .replace(/\.?0+$/, "")} Ω`;

        const V = parseFloat(supplyVoltageInput.value);
        let Itotal = NaN;
        let Ptotal = NaN;

        detailsContainer.innerHTML = "";

        if (!isNaN(V) && V !== 0) {
          Itotal = V / totalReq;
          Ptotal = V * Itotal;
          totalCurrentDisplay.textContent = formatCurrent(Itotal);
          totalPowerDisplay.textContent = formatPower(Ptotal);

          // Build details
          groupResults.forEach((g, idx) => {
            const groupVoltage = V * (g.Req / totalReq);
            const groupDiv = document.createElement("div");
            groupDiv.className = "border rounded-md p-3 bg-white";

            const typeLabel = g.type === "series" ? "Series" : "Parallel";
            groupDiv.innerHTML = `
          <div class="flex justify-between items-center mb-1">
            <div class="font-semibold">Group ${idx + 1} (${typeLabel})</div>
            <div class="text-xs text-gray-500">R<sub>eq</sub> = ${formatOhms(
              g.Req
            )}</div>
          </div>
          <div class="text-xs text-gray-600 mb-2">
            V<sub>group</sub> = ${formatVoltage(groupVoltage)}
          </div>
        `;

            const table = document.createElement("table");
            table.className = "w-full text-xs border-t";
            const thead = document.createElement("thead");
            thead.innerHTML = `
          <tr class="bg-gray-50">
            <th class="py-1 px-1 text-left font-medium">Resistor</th>
            <th class="py-1 px-1 text-right font-medium">R</th>
            <th class="py-1 px-1 text-right font-medium">I</th>
            <th class="py-1 px-1 text-right font-medium">V</th>
            <th class="py-1 px-1 text-right font-medium">P</th>
          </tr>
        `;
            table.appendChild(thead);
            const tbody = document.createElement("tbody");

            g.resistors.forEach((R, rIdx) => {
              let I, Vr;
              if (g.type === "series") {
                I = Itotal;
                Vr = I * R;
              } else {
                Vr = groupVoltage;
                I = Vr / R;
              }
              const P = Vr * I;
              const tr = document.createElement("tr");
              tr.innerHTML = `
            <td class="py-1 px-1">R${idx + 1}.${rIdx + 1}</td>
            <td class="py-1 px-1 text-right">${formatOhms(R)}</td>
            <td class="py-1 px-1 text-right">${formatCurrent(I)}</td>
            <td class="py-1 px-1 text-right">${formatVoltage(Vr)}</td>
            <td class="py-1 px-1 text-right">${formatPower(P)}</td>
          `;
              tbody.appendChild(tr);
            });

            table.appendChild(tbody);
            groupDiv.appendChild(table);
            detailsContainer.appendChild(groupDiv);
          });

          detailedSection.classList.remove("hidden");
        } else {
          totalCurrentDisplay.textContent = "Enter V";
          totalPowerDisplay.textContent = "Enter V";
          detailedSection.classList.add("hidden");
          detailsContainer.innerHTML = "";
        }
      });

      // Simple FAQ toggle (in addition to global script_faq if present)
      document
        .querySelectorAll("#faq-container .faq-question")
        .forEach((btn) => {
          btn.addEventListener("click", () => {
            const answer = btn.parentElement.querySelector(".faq-answer");
            const symbol = btn.querySelector("span.ml-2");
            const isHidden = answer.classList.contains("hidden");
            document
              .querySelectorAll("#faq-container .faq-answer")
              .forEach((a) => a.classList.add("hidden"));
            document
              .querySelectorAll("#faq-container .faq-question span.ml-2")
              .forEach((s) => (s.textContent = "+"));
            if (isHidden) {
              answer.classList.remove("hidden");
              symbol.textContent = "−";
            }
          });
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
