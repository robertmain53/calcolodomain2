    <script>
      // Utility: angle unit handling
      function toRad(angle, unit) {
        return unit === "deg" ? (angle * Math.PI) / 180 : angle;
      }
      function fromRad(rad, unit) {
        return unit === "deg" ? (rad * 180) / Math.PI : rad;
      }

      function getAngleUnit() {
        return document.querySelector('input[name="angleUnit"]:checked').value;
      }

      // Medium presets
      function bindMediumPreset(selectId, inputId) {
        const sel = document.getElementById(selectId);
        const inp = document.getElementById(inputId);
        sel.addEventListener("change", () => {
          if (sel.value !== "custom") {
            inp.value = sel.value;
          }
          inp.focus();
        });
      }

      bindMediumPreset("medium1Preset", "n1");
      bindMediumPreset("medium2Preset", "n2");

      // Sync sliders and inputs for angles (degrees only visually)
      function bindAnglePair(inputId, sliderId) {
        const input = document.getElementById(inputId);
        const slider = document.getElementById(sliderId);
        input.addEventListener("input", () => {
          const unit = getAngleUnit();
          let val = parseFloat(input.value);
          if (isNaN(val)) return;
          if (unit === "rad") {
            val = fromRad(val, "deg");
          }
          val = Math.max(0, Math.min(89, val));
          slider.value = val;
        });
        slider.addEventListener("input", () => {
          const unit = getAngleUnit();
          let deg = parseFloat(slider.value);
          if (unit === "deg") {
            input.value = deg.toFixed(2);
          } else {
            input.value = toRad(deg, "deg").toFixed(4);
          }
          drawDiagram();
        });
      }

      bindAnglePair("theta1", "theta1Slider");
      bindAnglePair("theta2", "theta2Slider");

      // Re-sync when unit changes
      document.querySelectorAll('input[name="angleUnit"]').forEach((r) => {
        r.addEventListener("change", () => {
          ["theta1", "theta2"].forEach((id) => {
            const input = document.getElementById(id);
            const slider = document.getElementById(id + "Slider");
            let deg = parseFloat(slider.value);
            if (getAngleUnit() === "deg") {
              input.value = isNaN(deg) ? "" : deg.toFixed(2);
            } else {
              input.value = isNaN(deg) ? "" : toRad(deg, "deg").toFixed(4);
            }
          });
          drawDiagram();
        });
      });

      // Main calculation
      function calculateSnell() {
        const solveFor = document.getElementById("solveFor").value;
        const unit = getAngleUnit();
        const n1 = parseFloat(document.getElementById("n1").value);
        const n2 = parseFloat(document.getElementById("n2").value);
        const theta1Input = parseFloat(document.getElementById("theta1").value);
        const theta2Input = parseFloat(document.getElementById("theta2").value);
        const resultText = document.getElementById("resultText");
        const tirBadge = document.getElementById("tirBadge");

        tirBadge.classList.add("hidden");

        if (!(n1 > 0) || !(n2 > 0)) {
          resultText.textContent =
            "Refractive indices n₁ and n₂ must be positive numbers.";
          return;
        }

        let theta1Rad = toRad(theta1Input || 0, unit);
        let theta2Rad = toRad(theta2Input || 0, unit);
        let result;

        try {
          if (solveFor === "theta2") {
            if (isNaN(theta1Input)) {
              resultText.textContent = "Enter a valid angle of incidence θ₁.";
              return;
            }
            const arg = (n1 / n2) * Math.sin(theta1Rad);
            if (Math.abs(arg) > 1) {
              tirBadge.classList.remove("hidden");
              resultText.textContent =
                "Total internal reflection: no real refraction angle θ₂ exists for these values.";
              // For diagram: reflect ray
              drawDiagram(true);
              return;
            }
            theta2Rad = Math.asin(arg);
            const theta2Out = fromRad(theta2Rad, unit);
            document.getElementById("theta2").value = theta2Out.toFixed(4);
            document.getElementById("theta2Slider").value = fromRad(
              theta2Rad,
              "deg"
            );
            result = `Angle of refraction θ₂ = ${theta2Out.toFixed(2)} ${
              unit === "deg" ? "°" : "rad"
            }.`;
          } else if (solveFor === "theta1") {
            if (isNaN(theta2Input)) {
              resultText.textContent = "Enter a valid angle of refraction θ₂.";
              return;
            }
            const arg = (n2 / n1) * Math.sin(theta2Rad);
            if (Math.abs(arg) > 1) {
              tirBadge.classList.remove("hidden");
              resultText.textContent =
                "No real incidence angle θ₁ satisfies these values (check n₁, n₂, and θ₂).";
              drawDiagram();
              return;
            }
            theta1Rad = Math.asin(arg);
            const theta1Out = fromRad(theta1Rad, unit);
            document.getElementById("theta1").value = theta1Out.toFixed(4);
            document.getElementById("theta1Slider").value = fromRad(
              theta1Rad,
              "deg"
            );
            result = `Angle of incidence θ₁ = ${theta1Out.toFixed(2)} ${
              unit === "deg" ? "°" : "rad"
            }.`;
          } else if (solveFor === "n2") {
            if (isNaN(theta1Input) || isNaN(theta2Input)) {
              resultText.textContent = "Enter both θ₁ and θ₂ to solve for n₂.";
              return;
            }
            const sin1 = Math.sin(theta1Rad);
            const sin2 = Math.sin(theta2Rad);
            if (Math.abs(sin2) < 1e-12) {
              resultText.textContent =
                "θ₂ is too close to 0; cannot reliably compute n₂.";
              return;
            }
            const n2Calc = (n1 * sin1) / sin2;
            document.getElementById("n2").value = n2Calc.toFixed(5);
            result = `Refractive index n₂ = ${n2Calc.toFixed(5)}.`;
          } else if (solveFor === "n1") {
            if (isNaN(theta1Input) || isNaN(theta2Input)) {
              resultText.textContent = "Enter both θ₁ and θ₂ to solve for n₁.";
              return;
            }
            const sin1 = Math.sin(theta1Rad);
            const sin2 = Math.sin(theta2Rad);
            if (Math.abs(sin1) < 1e-12) {
              resultText.textContent =
                "θ₁ is too close to 0; cannot reliably compute n₁.";
              return;
            }
            const n1Calc = (n2 * sin2) / sin1;
            document.getElementById("n1").value = n1Calc.toFixed(5);
            result = `Refractive index n₁ = ${n1Calc.toFixed(5)}.`;
          } else if (solveFor === "critical") {
            if (n1 <= n2) {
              resultText.textContent =
                "Critical angle exists only when n₁ > n₂ (light going from denser to rarer medium).";
              return;
            }
            const ratio = n2 / n1;
            if (ratio > 1) {
              resultText.textContent = "No critical angle because n₂ > n₁.";
              return;
            }
            const thetaC = Math.asin(ratio);
            const thetaCOut = fromRad(thetaC, unit);
            result = `Critical angle θc = ${thetaCOut.toFixed(2)} ${
              unit === "deg" ? "°" : "rad"
            }.`;
            // For diagram, set θ₁ to θc and θ₂ to 90°
            document.getElementById("theta1Slider").value = fromRad(
              thetaC,
              "deg"
            );
            document.getElementById("theta1").value = thetaCOut.toFixed(4);
            document.getElementById("theta2Slider").value = 89;
            document.getElementById("theta2").value =
              unit === "deg"
                ? "90.0000"
                : fromRad(Math.PI / 2, "rad").toFixed(4);
          }

          resultText.textContent = result || "Calculation complete.";
          drawDiagram();
        } catch (e) {
          console.error(e);
          resultText.textContent =
            "An error occurred while calculating. Please check your inputs.";
        }
      }

      document
        .getElementById("calculateBtn")
        .addEventListener("click", calculateSnell);

      document.getElementById("resetBtn").addEventListener("click", () => {
        document.getElementById("solveFor").value = "theta2";
        document.querySelector(
          'input[name="angleUnit"][value="deg"]'
        ).checked = true;
        document.getElementById("medium1Preset").value = "1.000293";
        document.getElementById("medium2Preset").value = "1.333";
        document.getElementById("n1").value = "1.0003";
        document.getElementById("n2").value = "1.3330";
        document.getElementById("theta1Slider").value = 30;
        document.getElementById("theta2Slider").value = 0;
        document.getElementById("theta1").value = "30.00";
        document.getElementById("theta2").value = "";
        document.getElementById("resultText").textContent =
          "Enter values and click Calculate.";
        document.getElementById("tirBadge").classList.add("hidden");
        drawDiagram();
      });

      // Simple FAQ toggle (in addition to global script_faq if any)
      document.querySelectorAll(".faq-question").forEach((btn) => {
        btn.addEventListener("click", () => {
          const ans = btn.parentElement.querySelector(".faq-answer");
          const icon = btn.querySelector("span:last-child");
          const isHidden = ans.classList.contains("hidden");
          document
            .querySelectorAll(".faq-answer")
            .forEach((a) => a.classList.add("hidden"));
          document
            .querySelectorAll(".faq-question span:last-child")
            .forEach((i) => (i.textContent = "+"));
          if (isHidden) {
            ans.classList.remove("hidden");
            icon.textContent = "−";
          }
        });
      });

      // Ray diagram drawing
      function drawDiagram(forceTIR) {
        const canvas = document.getElementById("snellCanvas");
        if (!canvas || !canvas.getContext) return;
        const ctx = canvas.getContext("2d");
        const w = canvas.width;
        const h = canvas.height;
        ctx.clearRect(0, 0, w, h);

        const cx = w / 2;
        const cy = h / 2;

        // Interface line
        ctx.strokeStyle = "#9ca3af";
        ctx.lineWidth = 1;
        ctx.beginPath();
        ctx.moveTo(0, cy);
        ctx.lineTo(w, cy);
        ctx.stroke();

        // Normal
        ctx.setLineDash([4, 4]);
        ctx.beginPath();
        ctx.moveTo(cx, 0);
        ctx.lineTo(cx, h);
        ctx.stroke();
        ctx.setLineDash([]);

        const unit = getAngleUnit();
        let theta1 = parseFloat(document.getElementById("theta1").value);
        let theta2 = parseFloat(document.getElementById("theta2").value);
        if (isNaN(theta1)) theta1 = 0;
        if (isNaN(theta2)) theta2 = 0;
        const theta1Rad = toRad(theta1, unit);
        const theta2Rad = toRad(theta2, unit);

        const len = Math.min(w, h) * 0.45;

        // Incident ray (above interface)
        const incAngleCanvas = -theta1Rad; // above, measured from normal
        const x1 = cx - len * Math.sin(incAngleCanvas);
        const y1 = cy - len * Math.cos(incAngleCanvas);

        ctx.strokeStyle = "#2563eb";
        ctx.lineWidth = 2;
        ctx.beginPath();
        ctx.moveTo(x1, y1);
        ctx.lineTo(cx, cy);
        ctx.stroke();

        // Arrowhead for incident
        drawArrowhead(ctx, cx, cy, x1, y1, "#2563eb");

        // Decide if TIR
        const n1 = parseFloat(document.getElementById("n1").value);
        const n2 = parseFloat(document.getElementById("n2").value);
        let tir = false;
        if (forceTIR) {
          tir = true;
        } else if (n1 > n2) {
          const arg = (n1 / n2) * Math.sin(theta1Rad);
          if (Math.abs(arg) > 1) tir = true;
        }

        if (tir) {
          // Reflected ray (same medium, symmetric angle)
          const reflAngleCanvas = theta1Rad; // below normal mirrored above interface
          const xr = cx - len * Math.sin(reflAngleCanvas);
          const yr = cy - len * Math.cos(reflAngleCanvas);

          ctx.strokeStyle = "#dc2626";
          ctx.beginPath();
          ctx.moveTo(cx, cy);
          ctx.lineTo(xr, yr);
          ctx.stroke();
          drawArrowhead(ctx, xr, yr, cx, cy, "#dc2626");

          // Label
          ctx.fillStyle = "#dc2626";
          ctx.font = "12px Inter, sans-serif";
          ctx.fillText("Total internal reflection", 10, 15);
        } else {
          // Refracted ray (below interface)
          const refrAngleCanvas = theta2Rad; // below, measured from normal
          const x2 = cx + len * Math.sin(refrAngleCanvas);
          const y2 = cy + len * Math.cos(refrAngleCanvas);

          ctx.strokeStyle = "#16a34a";
          ctx.beginPath();
          ctx.moveTo(cx, cy);
          ctx.lineTo(x2, y2);
          ctx.stroke();
          drawArrowhead(ctx, x2, y2, cx, cy, "#16a34a");
        }

        // Labels for media
        ctx.fillStyle = "#4b5563";
        ctx.font = "12px Inter, sans-serif";
        ctx.fillText("Medium 1 (n₁)", 10, cy - 8);
        ctx.fillText("Medium 2 (n₂)", 10, cy + 20);
      }

      function drawArrowhead(ctx, x0, y0, x1, y1, color) {
        const angle = Math.atan2(y1 - y0, x1 - x0);
        const size = 8;
        ctx.fillStyle = color;
        ctx.beginPath();
        ctx.moveTo(x0, y0);
        ctx.lineTo(
          x0 - size * Math.cos(angle - Math.PI / 6),
          y0 - size * Math.sin(angle - Math.PI / 6)
        );
        ctx.lineTo(
          x0 - size * Math.cos(angle + Math.PI / 6),
          y0 - size * Math.sin(angle + Math.PI / 6)
        );
        ctx.closePath();
        ctx.fill();
      }

      // Initial diagram
      window.addEventListener("load", () => {
        drawDiagram();
      });
    </script>

