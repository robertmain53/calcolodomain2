    <script>
      const inputEl = document.getElementById('numbers-input');
      const btnCalc = document.getElementById('btn-calc');
      const btnSortAsc = document.getElementById('btn-sort-asc');
      const btnSortDesc = document.getElementById('btn-sort-desc');
      const btnClear = document.getElementById('btn-clear');

      const summaryBox = document.getElementById('result-summary');
      const tableWrap = document.getElementById('result-table-wrap');
      const tableBody = document.querySelector('#result-table tbody');

      const resMedian = document.getElementById('res-median');
      const resMean = document.getElementById('res-mean');
      const resMode = document.getElementById('res-mode');
      const resMinMax = document.getElementById('res-minmax');
      const resRange = document.getElementById('res-range');
      const resQQQ = document.getElementById('res-qqq');

      function parseNumbers(text) {
        // split on comma, semicolon, space, newline, tab
        const raw = text.split(/[\s,;]+/);
        const nums = [];
        for (const r of raw) {
          if (!r.trim()) continue;
          const val = parseFloat(r);
          if (!isNaN(val)) nums.push(val);
        }
        return nums;
      }

      function medianOfSorted(sorted) {
        const n = sorted.length;
        if (n === 0) return NaN;
        if (n % 2 === 1) {
          return sorted[(n - 1) / 2];
        } else {
          const mid1 = sorted[n/2 - 1];
          const mid2 = sorted[n/2];
          return (mid1 + mid2) / 2;
        }
      }

      function meanOf(numbers) {
        if (numbers.length === 0) return NaN;
        return numbers.reduce((a,b) => a + b, 0) / numbers.length;
      }

      function modeOf(numbers) {
        if (numbers.length === 0) return null;
        const freq = {};
        let max = 0;
        for (const n of numbers) {
          const key = n.toString();
          freq[key] = (freq[key] || 0) + 1;
          if (freq[key] > max) max = freq[key];
        }
        const modes = Object.keys(freq).filter(k => freq[k] === max);
        if (modes.length === numbers.length) return null; // all unique
        return modes.map(m => parseFloat(m));
      }

      function quartiles(sorted) {
        const n = sorted.length;
        if (n === 0) return {q1: NaN, q3: NaN};
        const midIndex = Math.floor(n/2);
        let lower, upper;
        if (n % 2 === 0) {
          lower = sorted.slice(0, midIndex);
          upper = sorted.slice(midIndex);
        } else {
          lower = sorted.slice(0, midIndex);
          upper = sorted.slice(midIndex + 1);
        }
        return {
          q1: medianOfSorted(lower),
          q3: medianOfSorted(upper)
        };
      }

      function renderTable(sorted, medianVal) {
        tableBody.innerHTML = '';
        const n = sorted.length;
        let medianIndexes = [];
        if (n % 2 === 1) {
          medianIndexes = [(n - 1)/2];
        } else {
          medianIndexes = [n/2 - 1, n/2];
        }
        sorted.forEach((val, idx) => {
          const tr = document.createElement('tr');
          const tdIndex = document.createElement('td');
          const tdVal = document.createElement('td');
          tdIndex.textContent = idx + 1;
          if (medianIndexes.includes(idx)) {
            tdVal.innerHTML = val + ' <span class="badge">median</span>';
          } else {
            tdVal.textContent = val;
          }
          tr.appendChild(tdIndex);
          tr.appendChild(tdVal);
          tableBody.appendChild(tr);
        });
      }

      function calculate() {
        const nums = parseNumbers(inputEl.value);
        if (nums.length === 0) {
          summaryBox.classList.add('hidden');
          tableWrap.classList.add('hidden');
          return;
        }
        const sorted = nums.slice().sort((a,b) => a - b);
        const median = medianOfSorted(sorted);
        const mean = meanOf(nums);
        const mode = modeOf(nums);
        const min = sorted[0];
        const max = sorted[sorted.length - 1];
        const range = max - min;
        const {q1, q3} = quartiles(sorted);
        const iqr = q3 - q1;

        resMedian.textContent = isNaN(median) ? '—' : median;
        resMean.textContent = isNaN(mean) ? '—' : mean.toFixed(4).replace(/\.?0+$/, '');
        resMode.textContent = mode ? mode.join(', ') : 'No unique mode';
        resMinMax.textContent = min + ' / ' + max;
        resRange.textContent = range;
        resQQQ.textContent = (isNaN(q1) ? '—' : q1) + ' / ' + (isNaN(q3) ? '—' : q3) + ' / ' + (isNaN(iqr) ? '—' : iqr);

        renderTable(sorted, median);

        summaryBox.classList.remove('hidden');
        tableWrap.classList.remove('hidden');
      }

      btnCalc.addEventListener('click', calculate);

      btnSortAsc.addEventListener('click', function() {
        const nums = parseNumbers(inputEl.value);
        if (!nums.length) return;
        nums.sort((a,b) => a - b);
        inputEl.value = nums.join(', ');
        calculate();
      });

      btnSortDesc.addEventListener('click', function() {
        const nums = parseNumbers(inputEl.value);
        if (!nums.length) return;
        nums.sort((a,b) => b - a);
        inputEl.value = nums.join(', ');
        calculate();
      });

      btnClear.addEventListener('click', function() {
        inputEl.value = '';
        summaryBox.classList.add('hidden');
        tableWrap.classList.add('hidden');
      });

      // preload example
      inputEl.value = '12, 7, 9, 21, 18, 9, 13';
      calculate();
    </script>

