        function calculateEvolution() {
            const mass = parseFloat(document.getElementById('mass-input').value);
            const luminosity = parseFloat(document.getElementById('luminosity-input').value);
            const age = parseFloat(document.getElementById('age-input').value);

            if (isNaN(mass) || isNaN(luminosity) || isNaN(age)) {
                alert("Please enter valid numbers for all fields.");
                return;
            }

            // Example logic for current stage determination based on mass, luminosity, and age
            let currentStage = "Main Sequence";
            let nextStage = "Red Giant";

            if (age > 5) {
                currentStage = "Red Giant";
                nextStage = "White Dwarf";
            }

            document.getElementById('current-stage').textContent = currentStage;
            document.getElementById('next-stage').textContent = nextStage;
        }
    </script>


