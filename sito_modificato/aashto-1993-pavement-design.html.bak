    <script>
      // Utility: normal deviate from reliability
      function zFromReliability(R) {
        const p = R / 100;
        if (p <= 0 || p >= 1) return -1.645;
        // Approximate inverse normal CDF (Beasley-Springer/Moro)
        const a = [
          2.50662823884, -18.61500062529, 41.39119773534, -25.44106049637,
        ];
        const b = [
          -8.4735109309, 23.08336743743, -21.06224101826, 3.13082909833,
        ];
        const c = [
          0.3374754822726147, 0.9761690190917186, 0.1607979714918209,
          0.0276438810333863, 0.0038405729373609, 0.0003951896511919,
          0.0000321767881768, 0.0000002888167364, 0.0000003960315187,
        ];
        let x = p - 0.5;
        if (Math.abs(x) < 0.42) {
          const r = x * x;
          const num = ((a[3] * r + a[2]) * r + a[1]) * r + a[0];
          const den = (((b[3] * r + b[2]) * r + b[1]) * r + b[0]) * r + 1;
          return (x * num) / den;
        }
        let r = p;
        if (x > 0) r = 1 - p;
        r = Math.log(-Math.log(r));
        let z = c[0];
        for (let i = 1; i < c.length; i++) z += c[i] * Math.pow(r, i);
        return x < 0 ? -z : z;
      }

      function formatNumber(x, decimals) {
        if (!isFinite(x)) return "-";
        return x.toLocaleString(undefined, {
          maximumFractionDigits: decimals,
          minimumFractionDigits: decimals,
        });
      }

      // Toggle sections
      const btnFlexible = document.getElementById("btn-flexible");
      const btnRigid = document.getElementById("btn-rigid");
      const flexibleSection = document.getElementById("flexible-section");
      const rigidSection = document.getElementById("rigid-section");

      btnFlexible.addEventListener("click", () => {
        btnFlexible.classList.add("bg-blue-600", "text-white");
        btnFlexible.classList.remove("bg-white", "text-gray-700");
        btnRigid.classList.remove("bg-blue-600", "text-white");
        btnRigid.classList.add("bg-white", "text-gray-700");
        flexibleSection.classList.remove("hidden");
        rigidSection.classList.add("hidden");
      });

      btnRigid.addEventListener("click", () => {
        btnRigid.classList.add("bg-blue-600", "text-white");
        btnRigid.classList.remove("bg-white", "text-gray-700");
        btnFlexible.classList.remove("bg-blue-600", "text-white");
        btnFlexible.classList.add("bg-white", "text-gray-700");
        rigidSection.classList.remove("hidden");
        flexibleSection.classList.add("hidden");
      });

      // FLEXIBLE CALC
      document.getElementById("flex-calc").addEventListener("click", () => {
        const logW18 = parseFloat(document.getElementById("flex-logW18").value);
        const R = parseFloat(document.getElementById("flex-R").value);
        let ZR = parseFloat(document.getElementById("flex-ZR").value);
        const S0 = parseFloat(document.getElementById("flex-S0").value);
        const P0 = parseFloat(document.getElementById("flex-PSI0").value);
        const Pt = parseFloat(document.getElementById("flex-PSI").value);
        const MR = parseFloat(document.getElementById("flex-MR").value);
        const Dtot = parseFloat(document.getElementById("flex-D").value);

        const a1 = parseFloat(document.getElementById("flex-a1").value);
        const a2 = parseFloat(document.getElementById("flex-a2").value);
        const a3 = parseFloat(document.getElementById("flex-a3").value);
        const m2 = parseFloat(document.getElementById("flex-m2").value);
        const m3 = parseFloat(document.getElementById("flex-m3").value);

        if (
          isNaN(logW18) ||
          isNaN(R) ||
          isNaN(S0) ||
          isNaN(P0) ||
          isNaN(Pt) ||
          isNaN(MR)
        ) {
          alert("Please fill in all required flexible pavement inputs.");
          return;
        }
        if (!ZR && ZR !== 0) {
          ZR = zFromReliability(R);
        }

        const dPSI = P0 - Pt;
        const logMR = Math.log10(MR);

        // Solve for SN using simple iteration (Newton-like)
        let SN = 3.0; // initial guess
        const target = logW18;
        for (let i = 0; i < 40; i++) {
          const term1 = ZR * S0;
          const term2 = 9.36 * Math.log10(SN + 1) - 0.2;
          const term3 = Math.log10(dPSI / (4.2 - 1.5));
          const denom = 0.4 + 1094 / Math.pow(SN + 1, 5.19);
          const term4 = term3 / denom;
          const term5 = 2.32 * logMR - 8.07;
          const f = term1 + term2 + term4 + term5 - target;

          // Numerical derivative d(logW18)/dSN
          const dSN = 0.01;
          const SN2 = SN + dSN;
          const term2b = 9.36 * Math.log10(SN2 + 1) - 0.2;
          const denomb = 0.4 + 1094 / Math.pow(SN2 + 1, 5.19);
          const term4b = term3 / denomb;
          const fb = term1 + term2b + term4b + term5 - target;
          const df = (fb - f) / dSN || 0.1;

          SN = SN - f / df;
          if (SN < 0.1) SN = 0.1;
          if (Math.abs(f) < 1e-4) break;
        }

        const W18 = Math.pow(10, logW18);

        // Example layer thickness distribution: D1:D2:D3 = 1:1.5:2
        const ratio1 = 1,
          ratio2 = 1.5,
          ratio3 = 2;
        const sumRatio = ratio1 + ratio2 + ratio3;
        const D1 = (Dtot * ratio1) / sumRatio;
        const D2 = (Dtot * ratio2) / sumRatio;
        const D3 = (Dtot * ratio3) / sumRatio;

        const SNstructure = a1 * D1 + a2 * D2 * m2 + a3 * D3 * m3;

        document.getElementById("flex-sn").textContent = formatNumber(SN, 2);
        document.getElementById("flex-W18").textContent = formatNumber(W18, 0);
        document.getElementById("flex-D1").textContent = formatNumber(D1, 2);
        document.getElementById("flex-D2").textContent = formatNumber(D2, 2);
        document.getElementById("flex-D3").textContent = formatNumber(D3, 2);
        document.getElementById("flex-sn-structure").textContent = formatNumber(
          SNstructure,
          2
        );
        document.getElementById("flex-dPSI").textContent = formatNumber(
          dPSI,
          2
        );
        document.getElementById("flex-logMR").textContent = formatNumber(
          logMR,
          3
        );
        document.getElementById("flex-ZRS0").textContent = formatNumber(
          ZR * S0,
          3
        );

        document.getElementById("flex-results").classList.remove("hidden");
      });

      document.getElementById("flex-reset").addEventListener("click", () => {
        document.getElementById("flex-logW18").value = 6;
        document.getElementById("flex-R").value = 95;
        document.getElementById("flex-ZR").value = "";
        document.getElementById("flex-S0").value = 0.45;
        document.getElementById("flex-PSI0").value = 4.2;
        document.getElementById("flex-PSI").value = 2.5;
        document.getElementById("flex-MR").value = 8000;
        document.getElementById("flex-D").value = 10;
        document.getElementById("flex-a1").value = 0.44;
        document.getElementById("flex-a2").value = 0.14;
        document.getElementById("flex-a3").value = 0.11;
        document.getElementById("flex-m2").value = 1.0;
        document.getElementById("flex-m3").value = 1.0;
        document.getElementById("flex-results").classList.add("hidden");
      });

      // RIGID CALC (simplified iterative)
      document.getElementById("rigid-calc").addEventListener("click", () => {
        const logW18 = parseFloat(
          document.getElementById("rigid-logW18").value
        );
        const R = parseFloat(document.getElementById("rigid-R").value);
        let ZR = parseFloat(document.getElementById("rigid-ZR").value);
        const S0 = parseFloat(document.getElementById("rigid-S0").value);
        const P0 = parseFloat(document.getElementById("rigid-PSI0").value);
        const Pt = parseFloat(document.getElementById("rigid-PSI").value);
        const k = parseFloat(document.getElementById("rigid-MR").value);
        const Sc = parseFloat(document.getElementById("rigid-SC").value);
        const Ec = parseFloat(document.getElementById("rigid-Ec").value);
        const J = parseFloat(document.getElementById("rigid-J").value);
        const Cd = parseFloat(document.getElementById("rigid-Cd").value);
        const L = parseFloat(document.getElementById("rigid-L").value);

        if (
          isNaN(logW18) ||
          isNaN(R) ||
          isNaN(S0) ||
          isNaN(P0) ||
          isNaN(Pt) ||
          isNaN(k) ||
          isNaN(Sc) ||
          isNaN(Ec) ||
          isNaN(J) ||
          isNaN(Cd) ||
          isNaN(L)
        ) {
          alert("Please fill in all required rigid pavement inputs.");
          return;
        }
        if (!ZR && ZR !== 0) {
          ZR = zFromReliability(R);
        }

        const dPSI = P0 - Pt;
        const W18 = Math.pow(10, logW18);

        // Very simplified iterative model:
        // Assume fatigue capacity proportional to (Sc / stress)^12 and erosion capacity proportional to (k * h)^m.
        // We adjust h until combined damage ~ 1. This is NOT a replacement for the full AASHTO equation but preserves trends.
        let h = 8; // initial thickness in inches
        for (let iter = 0; iter < 60; iter++) {
          const h_ft = h / 12;
          const l = Math.pow(
            (Ec * Math.pow(h, 3)) / (12 * (1 - 0.15 * 0.15) * k),
            0.25
          ); // radius of relative stiffness (in), v≈0.15
          const stressEdge =
            ((3 * 18) / (2 * Math.PI * h * h)) * (1 + 0.6 * (h / l)); // very rough edge stress (ksi) scaled
          const stressPsi = (stressEdge * 1000 * J) / Cd;
          const SR = stressPsi / Sc;

          // Fatigue capacity (approx): Nf ~ (1/SR)^12
          const Nf = Math.pow(1 / Math.max(SR, 0.1), 12);
          const fatigueDamage = W18 / (Nf + 1e-9);

          // Erosion damage proxy: higher k and thinner slabs → more damage
          const erosionIndex = (k / 150) * (10 / h);
          const erosionDamage = erosionIndex * (W18 / 1e6);

          const totalDamage =
            fatigueDamage + erosionDamage + Math.max(0, (dPSI - 1.5) / 5);

          if (Math.abs(totalDamage - 1) < 0.05) break;
          if (totalDamage > 1) {
            h += 0.2; // increase thickness
          } else {
            h -= 0.2; // decrease thickness
          }
          if (h < 6) h = 6;
          if (h > 20) {
            h = 20;
            break;
          }
        }

        const hRounded = Math.round(h * 4) / 4; // round to nearest 0.25 in

        document.getElementById("rigid-h").textContent =
          formatNumber(hRounded, 2) + " in";
        document.getElementById("rigid-W18").textContent = formatNumber(W18, 0);
        document.getElementById("rigid-dPSI").textContent = formatNumber(
          dPSI,
          2
        );

        const l = Math.pow(
          (Ec * Math.pow(hRounded, 3)) / (12 * (1 - 0.15 * 0.15) * k),
          0.25
        );
        document.getElementById("rigid-l").textContent = formatNumber(l, 1);
        document.getElementById("rigid-ZRS0").textContent = formatNumber(
          ZR * S0,
          3
        );

        document.getElementById("rigid-results").classList.remove("hidden");
      });

      document.getElementById("rigid-reset").addEventListener("click", () => {
        document.getElementById("rigid-logW18").value = 6;
        document.getElementById("rigid-R").value = 95;
        document.getElementById("rigid-ZR").value = "";
        document.getElementById("rigid-S0").value = 0.35;
        document.getElementById("rigid-PSI0").value = 4.5;
        document.getElementById("rigid-PSI").value = 2.5;
        document.getElementById("rigid-MR").value = 150;
        document.getElementById("rigid-SC").value = 650;
        document.getElementById("rigid-Ec").value = 4000000;
        document.getElementById("rigid-J").value = 3.2;
        document.getElementById("rigid-Cd").value = 1.0;
        document.getElementById("rigid-L").value = 15;
        document.getElementById("rigid-results").classList.add("hidden");
      });

      window.MathJax = {
        tex: {
