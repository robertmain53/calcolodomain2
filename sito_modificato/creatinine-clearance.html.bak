    <script>
      // Creatinine clearance calculator logic
      (function () {
        const sexEl = document.getElementById("sex");
        const ageEl = document.getElementById("age");
        const scrEl = document.getElementById("scr");
        const scrUnitEl = document.getElementById("scr-unit");
        const heightEl = document.getElementById("height");
        const heightUnitEl = document.getElementById("height-unit");
        const weightEl = document.getElementById("weight");
        const weightUnitEl = document.getElementById("weight-unit");
        const weightStrategyEl = document.getElementById("weight-strategy");

        const calcBtn = document.getElementById("crcl-calc-btn");
        const resetBtn = document.getElementById("crcl-reset-btn");
        const errorEl = document.getElementById("crcl-error");

        const crclResultMainEl = document.getElementById("crcl-result-main");
        const crclResultWeightNoteEl = document.getElementById(
          "crcl-result-weight-note"
        );
        const crclCategoryEl = document.getElementById("crcl-category");

        const bmiDisplayEl = document.getElementById("bmi-display");
        const ibwDisplayEl = document.getElementById("ibw-display");
        const adjbwDisplayEl = document.getElementById("adjbw-display");
        const usedWeightDisplayEl = document.getElementById(
          "used-weight-display"
        );

        function toNumber(el) {
          const v = parseFloat(el.value);
          return isNaN(v) ? null : v;
        }

        function convertHeightToCm(value, unit) {
          if (unit === "cm") return value;
          // inches to cm
          return value * 2.54;
        }

        function convertWeightToKg(value, unit) {
          if (unit === "kg") return value;
          // lb to kg
          return value * 0.45359237;
        }

        function convertScrToMgDl(value, unit) {
          if (unit === "mgdl") return value;
          // µmol/L to mg/dL
          return value / 88.4;
        }

        function cmToInches(cm) {
          return cm / 2.54;
        }

        function round(value, decimals) {
          const factor = Math.pow(10, decimals);
          return Math.round(value * factor) / factor;
        }

        function determineWeightUsed(strategy, abw, ibw, adjbw, bmi) {
          let used = abw;
          let note = "";

          if (strategy === "actual") {
            used = abw;
            note = "Using actual body weight (ABW).";
          } else if (strategy === "ibw") {
            used = ibw;
            note = "Using ideal body weight (IBW).";
          } else if (strategy === "adj") {
            used = adjbw || ibw;
            note = "Using adjusted body weight (AdjBW).";
          } else {
            // auto
            if (bmi !== null) {
              if (bmi < 18.5) {
                used = abw;
                note = "Auto: underweight (BMI < 18.5), using ABW.";
              } else if (bmi >= 30) {
                used = adjbw || ibw;
                note = "Auto: obese (BMI ≥ 30), using AdjBW.";
              } else {
                used = ibw;
                note = "Auto: normal/overweight (18.5 ≤ BMI < 30), using IBW.";
              }
            } else {
              used = ibw;
              note = "Auto: BMI unavailable, defaulting to IBW.";
            }
          }

          return { used, note };
        }

        function categorizeCrCl(crcl) {
          if (crcl === null || isNaN(crcl)) return "–";
          if (crcl >= 90)
            return "Normal or high kidney function (CrCl ≥ 90 mL/min).";
          if (crcl >= 60) return "Mild renal impairment (CrCl 60–89 mL/min).";
          if (crcl >= 30)
            return "Moderate renal impairment (CrCl 30–59 mL/min).";
          if (crcl >= 15) return "Severe renal impairment (CrCl 15–29 mL/min).";
          return "Kidney failure (CrCl < 15 mL/min).";
        }

        function calculate() {
          errorEl.classList.add("hidden");
          errorEl.textContent = "";

          const sex = sexEl.value;
          const age = toNumber(ageEl);
          const scr = toNumber(scrEl);
          const scrUnit = scrUnitEl.value;
          const height = toNumber(heightEl);
          const heightUnit = heightUnitEl.value;
          const weight = toNumber(weightEl);
          const weightUnit = weightUnitEl.value;
          const strategy = weightStrategyEl.value;

          if (age === null || age <= 0 || age > 120) {
            return showError(
              "Please enter a valid age between 18 and 120 years."
            );
          }
          if (age < 18) {
            return showError(
              "Cockcroft–Gault is not validated in children. Please enter age ≥ 18 years."
            );
          }
          if (scr === null || scr <= 0) {
            return showError(
              "Please enter a valid serum creatinine value greater than 0."
            );
          }
          if (height === null || height <= 0) {
            return showError("Please enter a valid height.");
          }
          if (weight === null || weight <= 0) {
            return showError("Please enter a valid weight.");
          }

          const heightCm = convertHeightToCm(height, heightUnit);
          const weightKg = convertWeightToKg(weight, weightUnit);
          const scrMgDl = convertScrToMgDl(scr, scrUnit);

          const heightIn = cmToInches(heightCm);
          const baseIbw = sex === "male" ? 50 : 45.5;
          const ibw = baseIbw + 2.3 * Math.max(0, heightIn - 60);
          const adjbw = weightKg > ibw ? ibw + 0.4 * (weightKg - ibw) : ibw;

          const heightM = heightCm / 100;
          const bmi = weightKg / (heightM * heightM);

          const { used, note } = determineWeightUsed(
            strategy,
            weightKg,
            ibw,
            adjbw,
            bmi
          );

          const sexFactor = sex === "female" ? 0.85 : 1.0;
          const crcl = ((140 - age) * used * sexFactor) / (72 * scrMgDl);

          // Update displays
          crclResultMainEl.textContent = isFinite(crcl)
            ? `${round(crcl, 1)} mL/min`
            : "–";
          crclCategoryEl.textContent = categorizeCrCl(crcl);
          crclResultWeightNoteEl.textContent = note;

          bmiDisplayEl.textContent = isFinite(bmi)
            ? `${round(bmi, 1)} kg/m²`
            : "–";
          ibwDisplayEl.textContent = isFinite(ibw)
            ? `${round(ibw, 1)} kg`
            : "–";
          adjbwDisplayEl.textContent = isFinite(adjbw)
            ? `${round(adjbw, 1)} kg`
            : "–";
          usedWeightDisplayEl.textContent = isFinite(used)
            ? `${round(used, 1)} kg`
            : "–";
        }

        function showError(msg) {
          errorEl.textContent = msg;
          errorEl.classList.remove("hidden");
          crclResultMainEl.textContent = "–";
          crclCategoryEl.textContent = "–";
          crclResultWeightNoteEl.textContent =
            "Enter valid inputs and recalculate.";
          return;
        }

        function reset() {
          sexEl.value = "male";
          ageEl.value = 65;
          scrEl.value = 1;
          scrUnitEl.value = "mgdl";
          heightEl.value = 170;
          heightUnitEl.value = "cm";
          weightEl.value = 70;
          weightUnitEl.value = "kg";
          weightStrategyEl.value = "auto";

          errorEl.classList.add("hidden");
          errorEl.textContent = "";
          crclResultMainEl.textContent = "–";
          crclCategoryEl.textContent = "–";
          crclResultWeightNoteEl.textContent =
            "Enter values and click “Calculate”.";
          bmiDisplayEl.textContent = "–";
          ibwDisplayEl.textContent = "–";
          adjbwDisplayEl.textContent = "–";
          usedWeightDisplayEl.textContent = "–";
        }

        calcBtn.addEventListener("click", function (e) {
          e.preventDefault();
          calculate();
        });

        resetBtn.addEventListener("click", function (e) {
          e.preventDefault();
          reset();
        });
      })();
    </script>

