    <script>
      // --- Graph & Prim's Algorithm Implementation ---

      const edgesInputEl = document.getElementById("edges-input");
      const vertexCountEl = document.getElementById("vertex-count");
      const startVertexEl = document.getElementById("start-vertex");
      const inputErrorEl = document.getElementById("input-error");

      const buildGraphBtn = document.getElementById("build-graph-btn");
      const randomGraphBtn = document.getElementById("random-graph-btn");
      const loadExampleBtn = document.getElementById("load-example-btn");
      const clearEdgesBtn = document.getElementById("clear-edges-btn");

      const runPrimBtn = document.getElementById("run-prim-btn");
      const stepPrimBtn = document.getElementById("step-prim-btn");
      const resetPrimBtn = document.getElementById("reset-prim-btn");
      const stepIndicatorEl = document.getElementById("step-indicator");
      const mstWeightLabelEl = document.getElementById("mst-weight-label");

      const graphSvg = document.getElementById("graph-svg");
      const mstEdgesBody = document.getElementById("mst-edges-body");
      const stepLogEl = document.getElementById("step-log");

      let graph = {
        n: 0,
        adjMatrix: [],
        edges: [],
        positions: [],
      };

      let primState = null;

      function showError(msg) {
        inputErrorEl.textContent = msg;
        inputErrorEl.classList.remove("hidden");
      }

      function clearError() {
        inputErrorEl.textContent = "";
        inputErrorEl.classList.add("hidden");
      }

      function parseEdges(n, text) {
        const lines = text
          .split(/\r?\n/)
          .map((l) => l.trim())
          .filter((l) => l.length > 0);
        const edges = [];
        for (let line of lines) {
          const parts = line.split(/[\s,;]+/).filter(Boolean);
          if (parts.length !== 3) {
            throw new Error(`Invalid line "${line}". Use format: u v w`);
          }
          let u = parseInt(parts[0], 10);
          let v = parseInt(parts[1], 10);
          let w = parseFloat(parts[2]);
          if (!Number.isInteger(u) || !Number.isInteger(v) || isNaN(w)) {
            throw new Error(`Invalid numbers in line "${line}".`);
          }
          if (u < 1 || u > n || v < 1 || v > n) {
            throw new Error(
              `Vertex index out of range in line "${line}". Vertices must be between 1 and ${n}.`
            );
          }
          if (u === v) {
            throw new Error(`Self-loops are not allowed: "${line}".`);
          }
          edges.push({ u: u - 1, v: v - 1, w });
        }
        if (edges.length === 0) {
          throw new Error("Please enter at least one edge.");
        }
        return edges;
      }

      function buildAdjMatrix(n, edges) {
        const INF = Infinity;
        const m = Array.from({ length: n }, () => Array(n).fill(INF));
        for (let i = 0; i < n; i++) m[i][i] = 0;
        edges.forEach((e) => {
          if (e.w < m[e.u][e.v]) {
            m[e.u][e.v] = e.w;
            m[e.v][e.u] = e.w;
          }
        });
        return m;
      }

      function computePositions(n) {
        const cx = 200,
          cy = 150,
          r = 100;
        const positions = [];
        for (let i = 0; i < n; i++) {
          const angle = (2 * Math.PI * i) / n - Math.PI / 2;
          positions.push({
            x: cx + r * Math.cos(angle),
            y: cy + r * Math.sin(angle),
          });
        }
        return positions;
      }

      function drawGraph() {
        while (graphSvg.firstChild) graphSvg.removeChild(graphSvg.firstChild);
        const n = graph.n;
        if (n <= 0) return;

        // Draw edges
        graph.edges.forEach((e, idx) => {
          const p1 = graph.positions[e.u];
          const p2 = graph.positions[e.v];
          const line = document.createElementNS(
            "http://www.w3.org/2000/svg",
            "line"
          );
          line.setAttribute("x1", p1.x);
          line.setAttribute("y1", p1.y);
          line.setAttribute("x2", p2.x);
          line.setAttribute("y2", p2.y);
          line.setAttribute("stroke", "#cbd5f5");
          line.setAttribute("stroke-width", "2");
          line.classList.add("edge-line");
          line.dataset.edgeIndex = idx;
          graphSvg.appendChild(line);

          const midX = (p1.x + p2.x) / 2;
          const midY = (p1.y + p2.y) / 2;
          const text = document.createElementNS(
            "http://www.w3.org/2000/svg",
            "text"
          );
          text.setAttribute("x", midX);
          text.setAttribute("y", midY - 4);
          text.setAttribute("text-anchor", "middle");
          text.setAttribute("font-size", "10");
          text.setAttribute("fill", "#4b5563");
          text.textContent = e.w;
          graphSvg.appendChild(text);
        });

        // Draw nodes
        for (let i = 0; i < n; i++) {
          const pos = graph.positions[i];
          const g = document.createElementNS("http://www.w3.org/2000/svg", "g");
          g.classList.add("node");

          const circle = document.createElementNS(
            "http://www.w3.org/2000/svg",
            "circle"
          );
          circle.setAttribute("cx", pos.x);
          circle.setAttribute("cy", pos.y);
          circle.setAttribute("r", 16);
          circle.setAttribute("fill", "#ffffff");
          circle.setAttribute("stroke", "#2563eb");
          circle.setAttribute("stroke-width", "2");

          const label = document.createElementNS(
            "http://www.w3.org/2000/svg",
            "text"
          );
          label.setAttribute("x", pos.x);
          label.setAttribute("y", pos.y + 4);
          label.setAttribute("text-anchor", "middle");
          label.setAttribute("font-size", "12");
          label.setAttribute("fill", "#111827");
          label.textContent = (i + 1).toString();

          g.appendChild(circle);
          g.appendChild(label);
          graphSvg.appendChild(g);
        }
      }

      function initPrimState() {
        const n = graph.n;
        const start = parseInt(startVertexEl.value, 10) - 1;
        if (!Number.isInteger(start) || start < 0 || start >= n) {
          showError(`Start vertex must be between 1 and ${n}.`);
          return null;
        }
        const visited = Array(n).fill(false);
        visited[start] = true;
        primState = {
          n,
          start,
          visited,
          mstEdges: [],
          totalWeight: 0,
          step: 0,
          finished: false,
        };
        mstEdgesBody.innerHTML = "";
        stepLogEl.innerHTML = "";
        mstWeightLabelEl.textContent = "MST weight: –";
        stepIndicatorEl.textContent = "Step: 0";
        highlightMSTEdges();
        logStep(`Start at vertex ${start + 1}.`);
        return primState;
      }

      function logStep(text) {
        const li = document.createElement("li");
        li.textContent = text;
        stepLogEl.appendChild(li);
        stepLogEl.scrollTop = stepLogEl.scrollHeight;
      }

      function highlightMSTEdges(currentEdgeIndex = null) {
        const lines = graphSvg.querySelectorAll(".edge-line");
        lines.forEach((line) => {
          const idx = parseInt(line.dataset.edgeIndex, 10);
          const e = graph.edges[idx];
          const inMST =
            primState &&
            primState.mstEdges.some(
              (me) =>
                (me.u === e.u && me.v === e.v) || (me.u === e.v && me.v === e.u)
            );
          if (inMST) {
            line.setAttribute("stroke", "#2563eb");
            line.setAttribute("stroke-width", "3");
          } else {
            line.setAttribute("stroke", "#d1d5db");
            line.setAttribute("stroke-width", "2");
          }
          line.classList.remove("highlight");
          if (currentEdgeIndex !== null && idx === currentEdgeIndex) {
            line.setAttribute("stroke", "#f97316");
            line.classList.add("highlight");
          }
        });
      }

      function primStep() {
        if (!primState || primState.finished) return;

        const { n, visited } = primState;
        const INF = Infinity;
        let bestU = -1,
          bestV = -1,
          bestW = INF,
          bestEdgeIndex = null;

        for (let u = 0; u < n; u++) {
          if (!visited[u]) continue;
          for (let v = 0; v < n; v++) {
            if (visited[v]) continue;
            const w = graph.adjMatrix[u][v];
            if (w !== Infinity && w < bestW) {
              bestW = w;
              bestU = u;
              bestV = v;
            }
          }
        }

        if (bestU === -1 || bestV === -1) {
          primState.finished = true;
          stepIndicatorEl.textContent = `Step: ${primState.step} (done)`;
          mstWeightLabelEl.textContent = `MST weight: ${primState.totalWeight}`;
          const allVisited = visited.every((v) => v);
          if (!allVisited) {
            logStep(
              "No more reachable vertices. Graph appears to be disconnected."
            );
          } else {
            logStep("All vertices included. Prim’s algorithm finished.");
          }
          highlightMSTEdges();
          return;
        }

        let edgeIndex = null;
        for (let i = 0; i < graph.edges.length; i++) {
          const e = graph.edges[i];
          if (
            (e.u === bestU && e.v === bestV) ||
            (e.u === bestV && e.v === bestU)
          ) {
            if (edgeIndex === null || e.w < graph.edges[edgeIndex].w) {
              edgeIndex = i;
            }
          }
        }

        primState.step += 1;
        primState.visited[bestV] = true;
        primState.mstEdges.push({ u: bestU, v: bestV, w: bestW });
        primState.totalWeight += bestW;

        const row = document.createElement("tr");
        row.innerHTML = `
      <td class="px-2 py-1">${primState.step}</td>
      <td class="px-2 py-1">${bestU + 1} – ${bestV + 1}</td>
      <td class="px-2 py-1">${bestW}</td>
    `;
        mstEdgesBody.appendChild(row);

        stepIndicatorEl.textContent = `Step: ${primState.step}`;
        mstWeightLabelEl.textContent = `MST weight: ${primState.totalWeight}`;
        logStep(
          `Step ${primState.step}: choose edge (${bestU + 1}, ${
            bestV + 1
          }) with weight ${bestW}.`
        );

        highlightMSTEdges(edgeIndex);

        const allVisited = primState.visited.every((v) => v);
        if (allVisited) {
          primState.finished = true;
          logStep("All vertices included. Prim’s algorithm finished.");
          stepIndicatorEl.textContent = `Step: ${primState.step} (done)`;
          highlightMSTEdges();
        }
      }

      function runPrimToEnd() {
        if (!primState) {
          if (!initPrimState()) return;
        }
        let safety = 0;
        while (primState && !primState.finished && safety < 1000) {
          primStep();
          safety++;
        }
      }

      function buildGraphFromInput() {
        clearError();
        const n = parseInt(vertexCountEl.value, 10);
        if (!Number.isInteger(n) || n < 2 || n > 15) {
          showError("Number of vertices must be between 2 and 15.");
          return;
        }
        let edges;
        try {
          edges = parseEdges(n, edgesInputEl.value);
        } catch (e) {
          showError(e.message);
          return;
        }
        graph.n = n;
        graph.edges = edges;
        graph.adjMatrix = buildAdjMatrix(n, edges);
        graph.positions = computePositions(n);
        drawGraph();
        primState = null;
        mstEdgesBody.innerHTML = "";
        stepLogEl.innerHTML = "";
        stepIndicatorEl.textContent = "Step: –";
        mstWeightLabelEl.textContent = "MST weight: –";
        logStep(
          'Graph built. Click "Run to completion" or "Step" to start Prim’s algorithm.'
        );
      }

      function generateRandomConnectedGraph() {
        clearError();
        const n = parseInt(vertexCountEl.value, 10);
        if (!Number.isInteger(n) || n < 2 || n > 15) {
          showError("Number of vertices must be between 2 and 15.");
          return;
        }
        const edges = [];
        const used = new Set();
        for (let v = 1; v < n; v++) {
          const u = Math.floor(Math.random() * v);
          const w = Math.floor(Math.random() * 9) + 1;
          edges.push({ u, v, w });
          used.add(`${u}-${v}`);
          used.add(`${v}-${u}`);
        }
        const extraEdges = Math.floor(Math.random() * n);
        for (let k = 0; k < extraEdges; k++) {
          const u = Math.floor(Math.random() * n);
          let v = Math.floor(Math.random() * n);
          if (u === v) continue;
          const key = `${u}-${v}`;
          if (used.has(key)) continue;
          const w = Math.floor(Math.random() * 9) + 1;
          edges.push({ u, v, w });
          used.add(`${u}-${v}`);
          used.add(`${v}-${u}`);
        }
        const lines = edges.map((e) => `${e.u + 1} ${e.v + 1} ${e.w}`);
        edgesInputEl.value = lines.join("\n");
        buildGraphFromInput();
      }

      loadExampleBtn.addEventListener("click", () => {
        vertexCountEl.value = 5;
        startVertexEl.value = 1;
        edgesInputEl.value = [
          "1 2 2",
          "1 3 3",
          "2 3 1",
          "2 4 4",
          "3 5 5",
          "4 5 7",
        ].join("\n");
        buildGraphFromInput();
      });

      clearEdgesBtn.addEventListener("click", () => {
        edgesInputEl.value = "";
        mstEdgesBody.innerHTML = "";
        stepLogEl.innerHTML = "";
        stepIndicatorEl.textContent = "Step: –";
        mstWeightLabelEl.textContent = "MST weight: –";
        while (graphSvg.firstChild) graphSvg.removeChild(graphSvg.firstChild);
        primState = null;
        clearError();
      });

      buildGraphBtn.addEventListener("click", buildGraphFromInput);
      randomGraphBtn.addEventListener("click", generateRandomConnectedGraph);

      runPrimBtn.addEventListener("click", () => {
        if (!graph || graph.n === 0) {
          showError("Please build a graph first.");
          return;
        }
        if (!primState) {
          if (!initPrimState()) return;
        }
        runPrimToEnd();
      });

      stepPrimBtn.addEventListener("click", () => {
        if (!graph || graph.n === 0) {
          showError("Please build a graph first.");
          return;
        }
        if (!primState) {
          if (!initPrimState()) return;
        }
        primStep();
      });

      resetPrimBtn.addEventListener("click", () => {
        if (!graph || graph.n === 0) {
          showError("Please build a graph first.");
          return;
        }
        initPrimState();
      });

      // Initialize with a small example
      window.addEventListener("DOMContentLoaded", () => {
        loadExampleBtn.click();
      });

      // Mobile menu script (from template)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const expanded = this.getAttribute("aria-expanded") === "true";
          this.setAttribute("aria-expanded", String(!expanded));
          menu.classList.toggle("hidden");
        });
    </script>

