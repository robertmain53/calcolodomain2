    <script>
      // Utility functions
      function roundTo(value, decimals) {
        if (value === null || isNaN(value)) return null;
        const factor = Math.pow(10, decimals);
        return Math.round(value * factor) / factor;
      }

      function classifyLevel(total) {
        if (total === null)
          return { label: "N/A", badgeClass: "bg-gray-100 text-gray-700" };
        if (total < 10)
          return {
            label: "Subtherapeutic",
            badgeClass: "bg-blue-100 text-blue-800",
          };
        if (total <= 20)
          return {
            label: "Within usual range",
            badgeClass: "bg-green-100 text-green-800",
          };
        return {
          label: "Above usual range",
          badgeClass: "bg-red-100 text-red-800",
        };
      }

      function mcgToUmol(total) {
        // 1 mcg/mL ≈ 3.96 µmol/L for phenytoin
        return total * 3.96;
      }

      document.addEventListener("DOMContentLoaded", function () {
        const totalInput = document.getElementById("totalPhenytoin");
        const totalUnit = document.getElementById("totalUnit");
        const albuminInput = document.getElementById("albumin");
        const albuminUnit = document.getElementById("albuminUnit");
        const crclInput = document.getElementById("crcl");
        const renalRadios = document.querySelectorAll(
          'input[name="renalStatus"]'
        );
        const showFree = document.getElementById("showFree");
        const showAllFormulas = document.getElementById("showAllFormulas");
        const calculateBtn = document.getElementById("calculateBtn");
        const resetBtn = document.getElementById("resetBtn");
        const inputError = document.getElementById("inputError");

        const resultsSection = document.getElementById("results-section");
        const primaryResult = document.getElementById("primaryResult");
        const primaryRange = document.getElementById("primaryRange");
        const renalFlag = document.getElementById("renalFlag");
        const warningText = document.getElementById("warningText");
        const resultsBody = document.getElementById("resultsBody");
        const inputSummary = document.getElementById("inputSummary");

        function getRenalStatus(crcl, override) {
          if (override === "normal")
            return { status: "normal", reason: "Set by user override." };
          if (override === "failure")
            return { status: "failure", reason: "Set by user override." };

          if (!crcl || isNaN(crcl)) {
            return {
              status: "unknown",
              reason:
                "Renal function not provided; using normal-function equation as default.",
            };
          }
          if (crcl < 20) {
            return {
              status: "failure",
              reason: "CrCl/eGFR < 20 mL/min: using renal failure equation.",
            };
          }
          if (crcl < 25) {
            return {
              status: "borderline",
              reason:
                "CrCl/eGFR 20–25 mL/min: consider renal failure equation; clinical judgment required.",
            };
          }
          return {
            status: "normal",
            reason:
              "CrCl/eGFR ≥ 25 mL/min: using normal renal function equation.",
          };
        }

        function buildRow(name, equation, corrected, freeEst, isPrimary) {
          const cls = classifyLevel(corrected);
          const correctedDisplay =
            corrected !== null ? `${roundTo(corrected, 1)} mcg/mL` : "—";
          const freeDisplay =
            freeEst !== null ? `${roundTo(freeEst, 2)} mcg/mL` : "—";

          return `
        <tr class="${isPrimary ? "bg-blue-50" : ""}">
          <td class="px-3 py-2 align-top">
            <div class="flex items-center gap-1">
              <span class="font-medium">${name}</span>
              ${
                isPrimary
                  ? '<span class="badge bg-blue-600 text-white">Primary</span>'
                  : ""
              }
            </div>
          </td>
          <td class="px-3 py-2 align-top text-xs text-gray-600 font-mono">${equation}</td>
          <td class="px-3 py-2 align-top text-right">${correctedDisplay}</td>
          <td class="px-3 py-2 align-top text-right">${freeDisplay}</td>
          <td class="px-3 py-2 align-top text-right">
            <span class="badge ${cls.badgeClass}">${cls.label}</span>
          </td>
        </tr>
      `;
        }

        calculateBtn.addEventListener("click", function () {
          inputError.textContent = "";
          resultsSection.classList.add("hidden");
          resultsBody.innerHTML = "";
          primaryResult.textContent = "";
          primaryRange.textContent = "";
          renalFlag.textContent = "";
          warningText.textContent = "";
          inputSummary.innerHTML = "";

          const totalVal = parseFloat(totalInput.value);
          const albuminVal = parseFloat(albuminInput.value);
          const crclVal = crclInput.value ? parseFloat(crclInput.value) : null;
          const totalUnitVal = totalUnit.value;
          const albuminUnitVal = albuminUnit.value;
          const override =
            Array.from(renalRadios).find((r) => r.checked)?.value || "auto";

          if (isNaN(totalVal) || totalVal <= 0) {
            inputError.textContent =
              "Please enter a valid measured total phenytoin level.";
            return;
          }
          if (isNaN(albuminVal) || albuminVal <= 0) {
            inputError.textContent =
              "Please enter a valid serum albumin value.";
            return;
          }

          // Convert total to mcg/mL if needed
          let totalMcg = totalVal;
          if (totalUnitVal === "umol") {
            // reverse of mcgToUmol: mcg = umol / 3.96
            totalMcg = totalVal / 3.96;
          }

          // Convert albumin to g/dL if needed
          let albuminGdl = albuminVal;
          if (albuminUnitVal === "gL") {
            albuminGdl = albuminVal / 10;
          }

          const renal = getRenalStatus(crclVal, override);

          // Classic Sheiner–Tozer (normal renal)
          const denomClassic = 0.2 * albuminGdl + 0.1;
          const corrClassic = denomClassic > 0 ? totalMcg / denomClassic : null;

          // Renal failure Sheiner–Tozer
          const denomRenal = 0.1 * albuminGdl + 0.1;
          const corrRenal = denomRenal > 0 ? totalMcg / denomRenal : null;

          // Revised variant (example: 0.25*Alb + 0.1)
          const denomRevised = 0.25 * albuminGdl + 0.1;
          const corrRevised = denomRevised > 0 ? totalMcg / denomRevised : null;

          // Choose primary result
          let primary = corrClassic;
          let primaryLabel = "Classic Sheiner–Tozer (normal renal function)";
          if (renal.status === "failure") {
            primary = corrRenal;
            primaryLabel = "Sheiner–Tozer (renal failure)";
          }

          // Estimate free level if requested (approx 10% of total at normal binding)
          const includeFree = showFree.checked;
          const freePrimary =
            includeFree && primary !== null ? primary * 0.1 : null;
          const freeClassic =
            includeFree && corrClassic !== null ? corrClassic * 0.1 : null;
          const freeRenal =
            includeFree && corrRenal !== null ? corrRenal * 0.1 : null;
          const freeRevised =
            includeFree && corrRevised !== null ? corrRevised * 0.1 : null;

          // Populate primary summary
          if (primary !== null) {
            const cls = classifyLevel(primary);
            primaryResult.textContent = `${primaryLabel}: ${roundTo(
              primary,
              1
            )} mcg/mL`;
            primaryRange.textContent =
              "Usual total therapeutic range: 10–20 mcg/mL (approx. 40–80 µmol/L).";
            if (renal.status === "failure" || renal.status === "borderline") {
              renalFlag.textContent = renal.reason;
            } else if (renal.status === "unknown") {
              renalFlag.textContent = renal.reason;
            }
            if (cls.label === "Above usual range") {
              warningText.textContent =
                "Corrected level is above the usual total therapeutic range. Assess for toxicity and consider measuring a free phenytoin level.";
            } else if (cls.label === "Subtherapeutic") {
              warningText.textContent =
                "Corrected level is below the usual total therapeutic range. Correlate with seizure control before adjusting dose.";
            }
          }

          // Build results table
          let rows = "";
          const showAll = showAllFormulas.checked;

          rows += buildRow(
            "Classic Sheiner–Tozer",
            "Ccorr = Cmeasured / (0.2 × Alb + 0.1)",
            corrClassic,
            includeFree ? freeClassic : null,
            primaryLabel.startsWith("Classic")
          );

          if (showAll) {
            rows += buildRow(
              "Sheiner–Tozer (renal failure)",
              "Ccorr = Cmeasured / (0.1 × Alb + 0.1)",
              corrRenal,
              includeFree ? freeRenal : null,
              primaryLabel.startsWith("Sheiner–Tozer (renal failure)")
            );

            rows += buildRow(
              "Revised coefficients (example)",
              "Ccorr = Cmeasured / (0.25 × Alb + 0.1)",
              corrRevised,
              includeFree ? freeRevised : null,
              false
            );
          }

          resultsBody.innerHTML = rows;

          // Input summary
          const totalDisplay =
            totalUnitVal === "mcg"
              ? `${totalVal} mcg/mL`
              : `${totalVal} µmol/L (≈ ${roundTo(totalMcg, 1)} mcg/mL)`;

          const albuminDisplay =
            albuminUnitVal === "gdl"
              ? `${albuminVal} g/dL`
              : `${albuminVal} g/L (≈ ${roundTo(albuminGdl, 2)} g/dL)`;

          inputSummary.innerHTML = `
        <p><strong>Measured total phenytoin:</strong> ${totalDisplay}</p>
        <p><strong>Albumin:</strong> ${albuminDisplay}</p>
        <p><strong>Renal function (CrCl/eGFR):</strong> ${
          crclVal !== null && !isNaN(crclVal)
            ? crclVal + " mL/min"
            : "Not provided"
        }</p>
        <p><strong>Renal status used:</strong> ${renal.status} (${
            renal.reason
          })</p>
        <p><strong>Primary formula:</strong> ${primaryLabel}</p>
      `;

          resultsSection.classList.remove("hidden");
        });

        resetBtn.addEventListener("click", function () {
          document.getElementById("phenytoin-form").reset();
          inputError.textContent = "";
          resultsSection.classList.add("hidden");
          resultsBody.innerHTML = "";
          primaryResult.textContent = "";
          primaryRange.textContent = "";
          renalFlag.textContent = "";
          warningText.textContent = "";
          inputSummary.innerHTML = "";
          // reset defaults
          totalUnit.value = "mcg";
          albuminUnit.value = "gdl";
          showAllFormulas.checked = true;
        });
      });
    </script>

