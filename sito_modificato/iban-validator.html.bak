    <script>
      // IBAN country lengths and simple BBAN structure hints (partial, extendable)
      const IBAN_COUNTRY_DATA = {
        AD: { length: 24, bbanHint: { bank: [4, 8], account: [12, 24] } },
        AE: { length: 23 },
        AL: { length: 28 },
        AT: { length: 20, bbanHint: { bank: [4, 9], account: [9, 20] } },
        AZ: { length: 28 },
        BA: { length: 20 },
        BE: { length: 16, bbanHint: { bank: [4, 7], account: [7, 16] } },
        BG: {
          length: 22,
          bbanHint: { bank: [4, 8], branch: [8, 12], account: [12, 22] },
        },
        BH: { length: 22 },
        BR: { length: 29 },
        BY: { length: 28 },
        CH: { length: 21, bbanHint: { bank: [4, 9], account: [9, 21] } },
        CR: { length: 22 },
        CY: { length: 28, bbanHint: { bank: [4, 7], account: [11, 28] } },
        CZ: { length: 24, bbanHint: { bank: [4, 8], account: [8, 24] } },
        DE: { length: 22, bbanHint: { bank: [4, 12], account: [12, 22] } },
        DK: { length: 18, bbanHint: { bank: [4, 8], account: [8, 18] } },
        DO: { length: 28 },
        EE: { length: 20 },
        EG: { length: 29 },
        ES: {
          length: 24,
          bbanHint: { bank: [4, 8], branch: [8, 12], account: [14, 24] },
        },
        FI: { length: 18 },
        FO: { length: 18 },
        FR: {
          length: 27,
          bbanHint: { bank: [4, 9], branch: [9, 14], account: [14, 25] },
        },
        GB: {
          length: 22,
          bbanHint: { bank: [4, 8], branch: [8, 14], account: [14, 22] },
        },
        GE: { length: 22 },
        GI: { length: 23 },
        GL: { length: 18 },
        GR: {
          length: 27,
          bbanHint: { bank: [4, 7], branch: [7, 11], account: [11, 27] },
        },
        GT: { length: 28 },
        HR: { length: 21 },
        HU: { length: 28 },
        IE: {
          length: 22,
          bbanHint: { bank: [4, 8], branch: [8, 14], account: [14, 22] },
        },
        IL: { length: 23 },
        IQ: { length: 23 },
        IS: { length: 26 },
        IT: {
          length: 27,
          bbanHint: { bank: [5, 10], branch: [10, 15], account: [15, 27] },
        },
        JO: { length: 30 },
        KW: { length: 30 },
        KZ: { length: 20 },
        LB: { length: 28 },
        LC: { length: 32 },
        LI: { length: 21 },
        LT: { length: 20 },
        LU: { length: 20 },
        LV: { length: 21, bbanHint: { bank: [4, 8], account: [8, 21] } },
        MC: { length: 27 },
        MD: { length: 24 },
        ME: { length: 22 },
        MK: { length: 19 },
        MR: { length: 27 },
        MT: {
          length: 31,
          bbanHint: { bank: [4, 8], branch: [8, 13], account: [13, 31] },
        },
        MU: { length: 30 },
        NL: { length: 18, bbanHint: { bank: [4, 8], account: [8, 18] } },
        NO: { length: 15, bbanHint: { bank: [4, 8], account: [8, 15] } },
        PK: { length: 24 },
        PL: { length: 28, bbanHint: { bank: [4, 12], account: [12, 28] } },
        PS: { length: 29 },
        PT: { length: 25 },
        QA: { length: 29 },
        RO: { length: 24, bbanHint: { bank: [4, 8], account: [8, 24] } },
        RS: { length: 22 },
        SA: { length: 24 },
        SC: { length: 31 },
        SE: { length: 24 },
        SI: { length: 19 },
        SK: { length: 24 },
        SM: { length: 27 },
        ST: { length: 25 },
        SV: { length: 28 },
        TL: { length: 23 },
        TN: { length: 24 },
        TR: { length: 26 },
        UA: { length: 29 },
        VA: { length: 22 },
        VG: { length: 24 },
        XK: { length: 20 },
      };

      function cleanIban(input) {
        return (input || "").toUpperCase().replace(/[^A-Z0-9]/g, "");
      }

      function ibanMod97(iban) {
        // Move first 4 chars to the end
        const rearranged = iban.slice(4) + iban.slice(0, 4);
        // Replace letters with numbers
        let numeric = "";
        for (const ch of rearranged) {
          if (ch >= "A" && ch <= "Z") {
            numeric += (ch.charCodeAt(0) - 55).toString(); // A=10 ... Z=35
          } else {
            numeric += ch;
          }
        }
        // Compute mod 97 in chunks to avoid overflow
        let remainder = 0;
        const chunkSize = 9;
        for (let i = 0; i < numeric.length; i += chunkSize) {
          const part = remainder.toString() + numeric.slice(i, i + chunkSize);
          remainder = parseInt(part, 10) % 97;
        }
        return remainder;
      }

      function validateIbanCore(raw) {
        const cleaned = cleanIban(raw);
        const result = {
          raw,
          cleaned,
          isEmpty: cleaned.length === 0,
          isValid: false,
          countryCode: null,
          expectedLength: null,
          actualLength: cleaned.length,
          errors: [],
          warnings: [],
          mod97: null,
        };

        if (result.isEmpty) {
          result.errors.push("No IBAN provided.");
          return result;
        }

        if (!/^[A-Z0-9]+$/.test(cleaned)) {
          result.errors.push(
            "IBAN contains invalid characters. Only A–Z and 0–9 are allowed."
          );
        }

        if (cleaned.length < 4) {
          result.errors.push(
            "IBAN is too short to contain country code and check digits."
          );
          return result;
        }

        const country = cleaned.slice(0, 2);
        const checkDigits = cleaned.slice(2, 4);
        result.countryCode = country;
        result.checkDigits = checkDigits;

        const countryData = IBAN_COUNTRY_DATA[country];
        if (!countryData) {
          result.errors.push(
            `Country code "${country}" is not a recognized IBAN country.`
          );
        } else {
          result.expectedLength = countryData.length;
          if (cleaned.length !== countryData.length) {
            result.errors.push(
              `Invalid length for ${country}. Expected ${countryData.length} characters, got ${cleaned.length}.`
            );
          }
        }

        if (!/^[0-9]{2}$/.test(checkDigits)) {
          result.errors.push("Check digits (positions 3–4) must be numeric.");
        }

        if (result.errors.length === 0) {
          const remainder = ibanMod97(cleaned);
          result.mod97 = remainder;
          if (remainder !== 1) {
            result.errors.push(
              "Checksum failed: MOD 97 remainder is " +
                remainder +
                " (should be 1)."
            );
          } else {
            result.isValid = true;
          }
        }

        return result;
      }

      function describeCountryLength(countryCode, expected, actual) {
        if (!countryCode) return "Unknown country.";
        if (!expected)
          return `Country ${countryCode} is not in the IBAN registry used by this tool.`;
        if (expected === actual) {
          return `${countryCode} – length ${actual}/${expected} characters (OK).`;
        }
        return `${countryCode} – length ${actual}/${expected} characters (invalid).`;
      }

      function fillStructure(result) {
        const container = document.getElementById("iban-structure-container");
        const cEl = document.getElementById("iban-struct-country");
        const chkEl = document.getElementById("iban-struct-check");
        const bankEl = document.getElementById("iban-struct-bank");
        const branchEl = document.getElementById("iban-struct-branch");
        const accEl = document.getElementById("iban-struct-account");

        if (!result.countryCode || !IBAN_COUNTRY_DATA[result.countryCode]) {
          container.classList.add("hidden");
          return;
        }

        const data = IBAN_COUNTRY_DATA[result.countryCode];
        const iban = result.cleaned;
        const bban = iban.slice(4);

        cEl.textContent = result.countryCode;
        chkEl.textContent = result.checkDigits || "";

        let bank = "",
          branch = "",
          account = "",
          other = "";
        if (data.bbanHint) {
          const hint = data.bbanHint;
          if (hint.bank) bank = iban.slice(hint.bank[0], hint.bank[1]);
          if (hint.branch) branch = iban.slice(hint.branch[0], hint.branch[1]);
          if (hint.account)
            account = iban.slice(hint.account[0], hint.account[1]);
          // Fallback for any remaining BBAN part
          const used = new Set();
          if (hint.bank)
            for (let i = hint.bank[0]; i < hint.bank[1]; i++) used.add(i);
          if (hint.branch)
            for (let i = hint.branch[0]; i < hint.branch[1]; i++) used.add(i);
          if (hint.account)
            for (let i = hint.account[0]; i < hint.account[1]; i++) used.add(i);
          let otherChars = "";
          for (let i = 4; i < iban.length; i++) {
            if (!used.has(i)) otherChars += iban[i];
          }
          other = otherChars;
        } else {
          account = bban;
        }

        bankEl.textContent = bank || "–";
        branchEl.textContent = branch || other || "–";
        accEl.textContent = account || "–";
        container.classList.remove("hidden");
      }

      function updateSingleIbanUI(result) {
        const input = document.getElementById("iban-input");
        const statusPill = document.getElementById("iban-status-pill");
        const countryLen = document.getElementById("iban-country-length");
        const details = document.getElementById("iban-details");

        input.classList.remove("iban-valid", "iban-invalid");

        // Status pill
        if (result.isEmpty) {
          statusPill.textContent = "No IBAN";
          statusPill.className =
            "inline-flex items-center px-3 py-1 rounded-full text-xs font-semibold bg-gray-100 text-gray-600";
          countryLen.textContent = "–";
          details.innerHTML = "<li>Please enter an IBAN to validate.</li>";
          document
            .getElementById("iban-structure-container")
            .classList.add("hidden");
          return;
        }

        if (result.isValid) {
          statusPill.textContent = "Valid IBAN";
          statusPill.className =
            "inline-flex items-center px-3 py-1 rounded-full text-xs font-semibold bg-green-100 text-green-700";
          input.classList.add("iban-valid");
        } else {
          statusPill.textContent = "Invalid IBAN";
          statusPill.className =
            "inline-flex items-center px-3 py-1 rounded-full text-xs font-semibold bg-red-100 text-red-700";
          input.classList.add("iban-invalid");
        }

        countryLen.textContent = describeCountryLength(
          result.countryCode,
          result.expectedLength,
          result.actualLength
        );

        const items = [];
        if (result.isValid) {
          items.push("Structure and checksum are valid.");
          if (result.mod97 !== null) {
            items.push("MOD 97 remainder = " + result.mod97 + " (correct).");
          }
        } else {
          if (result.errors.length) {
            for (const e of result.errors) items.push(e);
          }
          if (result.mod97 !== null && result.mod97 !== 1) {
            items.push(
              "MOD 97 remainder = " + result.mod97 + " (should be 1)."
            );
          }
        }
        if (!items.length) items.push("No issues detected.");

        details.innerHTML = items.map((t) => `<li>${t}</li>`).join("");

        if (result.isValid) {
          fillStructure(result);
        } else {
          document
            .getElementById("iban-structure-container")
            .classList.add("hidden");
        }
      }

      function handleSingleValidation() {
        const raw = document.getElementById("iban-input").value;
        const result = validateIbanCore(raw);
        updateSingleIbanUI(result);
      }

      function handleBulkValidation() {
        const raw = document.getElementById("iban-bulk-input").value || "";
        const lines = raw
          .split(/\r?\n/)
          .map((l) => l.trim())
          .filter((l) => l.length > 0);
        const tbody = document.getElementById("iban-bulk-tbody");
        const summary = document.getElementById("iban-bulk-summary");
        const tableWrapper = document.getElementById("iban-bulk-results");

        if (!lines.length) {
          summary.textContent = "No IBANs to validate.";
          tbody.innerHTML = "";
          tableWrapper.classList.add("hidden");
          return;
        }

        let validCount = 0;
        let invalidCount = 0;
        const rows = [];

        lines.forEach((line, idx) => {
          const res = validateIbanCore(line);
          if (res.isValid) validCount++;
          else invalidCount++;
          const statusText = res.isValid ? "Valid" : "Invalid";
          const statusClass = res.isValid ? "text-green-700" : "text-red-700";
          const msg = res.isValid
            ? "Structure and checksum OK."
            : res.errors[0] || "Invalid IBAN.";

          rows.push(`
        <tr>
          <td class="px-3 py-1 text-gray-600">${idx + 1}</td>
          <td class="px-3 py-1 mono">${res.cleaned || line}</td>
          <td class="px-3 py-1">${res.countryCode || "–"}</td>
          <td class="px-3 py-1 font-semibold ${statusClass}">${statusText}</td>
          <td class="px-3 py-1 text-gray-700">${msg}</td>
        </tr>
      `);
        });

        tbody.innerHTML = rows.join("");
        tableWrapper.classList.remove("hidden");
        summary.textContent = `${lines.length} IBAN(s) checked – ${validCount} valid, ${invalidCount} invalid.`;
      }

      document.addEventListener("DOMContentLoaded", () => {
        document
          .getElementById("iban-validate-btn")
          .addEventListener("click", handleSingleValidation);
        document
          .getElementById("iban-input")
          .addEventListener("keydown", (e) => {
            if (e.key === "Enter") {
              e.preventDefault();
              handleSingleValidation();
            }
          });
        document
          .getElementById("iban-bulk-validate-btn")
          .addEventListener("click", handleBulkValidation);
      });

