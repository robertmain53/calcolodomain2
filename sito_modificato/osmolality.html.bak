    <script>
      // Mobile menu toggle
      document
        .getElementById("mobile-menu-toggle")
        ?.addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          if (!menu) return;
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          this.setAttribute("aria-expanded", String(isHidden));
        });

      // Osmolality calculator logic
      (function () {
        const unitsMgdlBtn = document.getElementById("units-mgdl-btn");
        const unitsMmolBtn = document.getElementById("units-mmol-btn");
        const glucoseUnitSpan = document.getElementById("glucose-unit");
        const ureaUnitSpan = document.getElementById("urea-unit");
        const ureaLabelExtra = document.getElementById("urea-label-extra");
        const ethanolUnitSpan = document.getElementById("ethanol-unit");
        const errorSpan = document.getElementById("calc-error");
        const resultsSection = document.getElementById("results-section");
        const calcOsmEl = document.getElementById("calc-osm");
        const calcOsmInterpretEl =
          document.getElementById("calc-osm-interpret");
        const osmGapEl = document.getElementById("osm-gap");
        const osmGapInterpretEl = document.getElementById("osm-gap-interpret");
        const summaryTextEl = document.getElementById("summary-text");

        let useConventional = true; // true = mg/dL + BUN, false = mmol/L + urea

        function setUnits(conventional) {
          useConventional = conventional;
          if (conventional) {
            unitsMgdlBtn.classList.add("bg-blue-600", "text-white");
            unitsMgdlBtn.classList.remove("bg-white", "text-gray-700");
            unitsMmolBtn.classList.remove("bg-blue-600", "text-white");
            unitsMmolBtn.classList.add("bg-white", "text-gray-700");

            glucoseUnitSpan.textContent = "mg/dL";
            ureaUnitSpan.textContent = "mg/dL";
            ureaLabelExtra.textContent = "(BUN)";
            ethanolUnitSpan.textContent = "mg/dL";
          } else {
            unitsMmolBtn.classList.add("bg-blue-600", "text-white");
            unitsMmolBtn.classList.remove("bg-white", "text-gray-700");
            unitsMgdlBtn.classList.remove("bg-blue-600", "text-white");
            unitsMgdlBtn.classList.add("bg-white", "text-gray-700");

            glucoseUnitSpan.textContent = "mmol/L";
            ureaUnitSpan.textContent = "mmol/L";
            ureaLabelExtra.textContent = "(urea)";
            ethanolUnitSpan.textContent = "mmol/L";
          }
          errorSpan.textContent = "";
          resultsSection.classList.add("hidden");
        }

        unitsMgdlBtn?.addEventListener("click", () => setUnits(true));
        unitsMmolBtn?.addEventListener("click", () => setUnits(false));

        document.getElementById("reset-btn")?.addEventListener("click", () => {
          errorSpan.textContent = "";
          resultsSection.classList.add("hidden");
        });

        document
          .getElementById("calculate-btn")
          ?.addEventListener("click", () => {
            errorSpan.textContent = "";

            const na = parseFloat(document.getElementById("na").value);
            const glucose = parseFloat(
              document.getElementById("glucose").value
            );
            const urea = parseFloat(document.getElementById("urea").value);
            const ethanol = parseFloat(
              document.getElementById("ethanol").value
            );
            const measured = parseFloat(
              document.getElementById("measured").value
            );

            if (isNaN(na) || isNaN(glucose) || isNaN(urea)) {
              errorSpan.textContent =
                "Please enter sodium, glucose, and urea/BUN.";
              resultsSection.classList.add("hidden");
              return;
            }

            if (na <= 0 || glucose < 0 || urea < 0) {
              errorSpan.textContent =
                "Values must be non‑negative, and sodium must be > 0.";
              resultsSection.classList.add("hidden");
              return;
            }

            let calcOsm = 0;
            if (useConventional) {
              // mg/dL + BUN
              calcOsm = 2 * na + glucose / 18 + urea / 2.8;
              if (!isNaN(ethanol) && ethanol > 0) {
                calcOsm += ethanol / 3.7;
              }
            } else {
              // mmol/L + urea
              calcOsm = 2 * na + glucose + urea;
              if (!isNaN(ethanol) && ethanol > 0) {
                calcOsm += ethanol;
              }
            }

            const calcOsmRounded = Math.round(calcOsm * 10) / 10;
            calcOsmEl.textContent = calcOsmRounded.toFixed(1);

            // Interpretation for calculated osmolality
            let osmInterp = "";
            if (calcOsmRounded < 275) {
              osmInterp =
                "Below typical serum osmolality range (≈275–295 mOsm/kg). Consider hypotonic hyponatremia or lab variation.";
            } else if (calcOsmRounded <= 295) {
              osmInterp =
                "Within typical serum osmolality range (≈275–295 mOsm/kg).";
            } else {
              osmInterp =
                "Above typical serum osmolality range (≈275–295 mOsm/kg). Consider hypernatremia, marked hyperglycemia, or other causes of hyperosmolality.";
            }
            calcOsmInterpretEl.textContent = osmInterp;

            // Osmolar gap if measured value provided
            let gapText = "–";
            let gapInterp =
              "Enter a measured serum osmolality to compute the osmolar gap.";
            let summary = "";

            if (!isNaN(measured) && measured > 0) {
              const gap = measured - calcOsmRounded;
              const gapRounded = Math.round(gap * 10) / 10;
              gapText = gapRounded.toFixed(1);

              if (gapRounded < -10) {
                gapInterp =
                  "Osmolar gap is below the usual range (−10 to +10 mOsm/kg). This is uncommon and may reflect analytical variation or formula differences.";
              } else if (gapRounded <= 10) {
                gapInterp =
                  "Osmolar gap is within the usual range (−10 to +10 mOsm/kg). No strong evidence of unmeasured osmoles.";
              } else if (gapRounded <= 20) {
                gapInterp =
                  "Mildly elevated osmolar gap (>10 mOsm/kg). Could reflect unmeasured osmoles, lab variation, or formula choice; interpret with clinical context.";
              } else {
                gapInterp =
                  "Markedly elevated osmolar gap (>20 mOsm/kg). Consider unmeasured osmoles such as toxic alcohols, mannitol, or contrast agents; correlate with anion gap and toxicology testing.";
              }

              summary =
                "Calculated osmolality is " +
                calcOsmRounded.toFixed(1) +
                " mOsm/kg; measured osmolality is " +
                measured.toFixed(1) +
                " mOsm/kg, giving an osmolar gap of " +
                gapRounded.toFixed(1) +
                " mOsm/kg. Always interpret these values together with the clinical picture, acid–base status, and local reference ranges.";
            } else {
              summary =
                "Calculated osmolality is " +
                calcOsmRounded.toFixed(1) +
                " mOsm/kg. To assess the osmolar gap, enter a directly measured serum osmolality from the laboratory.";
            }

            osmGapEl.textContent = gapText;
            osmGapInterpretEl.textContent = gapInterp;
            summaryTextEl.textContent = summary;

            resultsSection.classList.remove("hidden");
          });
      })();

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
