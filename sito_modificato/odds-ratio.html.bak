    <script>
      // Utility: standard normal CDF using Abramowitz-Stegun approximation
      function normalCdf(z) {
        var sign = z < 0 ? -1 : 1;
        z = Math.abs(z) / Math.sqrt(2);
        var t = 1 / (1 + 0.3275911 * z);
        var a1 = 0.254829592,
          a2 = -0.284496736,
          a3 = 1.421413741,
          a4 = -1.453152027,
          a5 = 1.061405429;
        var erf =
          1 -
          ((((a5 * t + a4) * t + a3) * t + a2) * t + a1) * t * Math.exp(-z * z);
        return 0.5 * (1 + sign * erf);
      }

      function formatNumber(x, decimals) {
        if (!isFinite(x)) return "NA";
        return x.toFixed(decimals);
      }

      function updateTotals() {
        const a = Number(document.getElementById("cell-a").value) || 0;
        const b = Number(document.getElementById("cell-b").value) || 0;
        const c = Number(document.getElementById("cell-c").value) || 0;
        const d = Number(document.getElementById("cell-d").value) || 0;

        const row1 = a + b;
        const row2 = c + d;
        const col1 = a + c;
        const col2 = b + d;
        const grand = row1 + row2;

        document.getElementById("row1-total").textContent = row1;
        document.getElementById("row2-total").textContent = row2;
        document.getElementById("col1-total").textContent = col1;
        document.getElementById("col2-total").textContent = col2;
        document.getElementById("grand-total").textContent = grand;
      }

      function syncLabels() {
        document.getElementById("th-exposed").textContent =
          document.getElementById("label-exposed").value || "Exposed";
        document.getElementById("th-unexposed").textContent =
          document.getElementById("label-unexposed").value || "Unexposed";
        document.getElementById("th-outcome").textContent =
          document.getElementById("label-outcome").value || "Outcome +";
        document.getElementById("th-nooutcome").textContent =
          document.getElementById("label-nooutcome").value || "Outcome −";
      }

      function interpretOR(or, lower, upper, alpha, labels) {
        if (!isFinite(or) || or <= 0)
          return "Odds ratio could not be interpreted due to invalid or zero counts.";

        const level = (1 - alpha) * 100;
        const expLabel = labels.exposed;
        const unexpLabel = labels.unexposed;
        const outcomeLabel = labels.outcome;

        let direction, magnitude;
        if (or > 1) {
          direction = "higher";
          if (or < 1.5) magnitude = "slightly";
          else if (or < 3) magnitude = "moderately";
          else magnitude = "substantially";
        } else if (or < 1) {
          direction = "lower";
          const inv = 1 / or;
          if (inv < 1.5) magnitude = "slightly";
          else if (inv < 3) magnitude = "moderately";
          else magnitude = "substantially";
        } else {
          return `The odds ratio is 1.00, indicating no difference in the odds of ${outcomeLabel} between ${expLabel} and ${unexpLabel}.`;
        }

        const ciIncludes1 = lower <= 1 && upper >= 1;
        const sigText = ciIncludes1
          ? `The ${level.toFixed(
              0
            )}% confidence interval includes 1, so this association is not statistically significant at the ${alpha.toFixed(
              2
            )} level.`
          : `The ${level.toFixed(
              0
            )}% confidence interval does not include 1, so this association is statistically significant at the ${alpha.toFixed(
              2
            )} level.`;

        return `In this sample, the odds of ${outcomeLabel} in ${expLabel} are ${magnitude} ${direction} than in ${unexpLabel} (OR ≈ ${or.toFixed(
          2
        )}). ${sigText}`;
      }

      function calculateOR() {
        const a = Number(document.getElementById("cell-a").value);
        const b = Number(document.getElementById("cell-b").value);
        const c = Number(document.getElementById("cell-c").value);
        const d = Number(document.getElementById("cell-d").value);

        if ([a, b, c, d].some((v) => isNaN(v) || v < 0)) {
          document.getElementById("results-container").innerHTML =
            '<p class="text-sm text-red-600">Please enter non‑negative numeric counts in all four cells.</p>';
          return;
        }

        let aa = a,
          bb = b,
          cc = c,
          dd = d;
        const applyCorrection =
          document.getElementById("apply-correction").checked;
        let correctionApplied = false;

        if (applyCorrection && (aa === 0 || bb === 0 || cc === 0 || dd === 0)) {
          aa += 0.5;
          bb += 0.5;
          cc += 0.5;
          dd += 0.5;
          correctionApplied = true;
        }

        if (aa === 0 || bb === 0 || cc === 0 || dd === 0) {
          document.getElementById("results-container").innerHTML =
            '<p class="text-sm text-red-600">At least one cell is zero and continuity correction is disabled. Odds ratio is undefined. Enable the correction or adjust counts.</p>';
          return;
        }

        const or = (aa * dd) / (bb * cc);
        const logOR = Math.log(or);
        const seLogOR = Math.sqrt(1 / aa + 1 / bb + 1 / cc + 1 / dd);

        const alpha = Number(document.getElementById("alpha-level").value);
        const zMap = {
          0.1: 1.6448536269514722,
          0.05: 1.959963984540054,
          0.01: 2.5758293035489004,
        };
        const z = zMap[alpha] || 1.959963984540054;

        const lowerLog = logOR - z * seLogOR;
        const upperLog = logOR + z * seLogOR;
        const lowerOR = Math.exp(lowerLog);
        const upperOR = Math.exp(upperLog);

        const zStat = logOR / seLogOR;
        const pTwoSided = 2 * (1 - normalCdf(Math.abs(zStat)));

        const labels = {
          exposed: document.getElementById("label-exposed").value || "Exposed",
          unexposed:
            document.getElementById("label-unexposed").value || "Unexposed",
          outcome:
            document.getElementById("label-outcome").value || "the outcome",
        };

        const interpretation = interpretOR(or, lowerOR, upperOR, alpha, labels);

        const level = (1 - alpha) * 100;

        const resultsHtml = `
      <div class="grid md:grid-cols-2 gap-4">
        <div class="border border-gray-200 rounded-md p-3">
          <h3 class="text-sm font-semibold mb-2">Effect estimate</h3>
          <p class="text-sm flex items-center justify-between">
            <span>Odds ratio (OR)</span>
            <span class="font-mono">${formatNumber(or, 3)}</span>
          </p>
          <p class="text-xs text-gray-500 mt-1">
            Computed as (a × d) / (b × c) using ${
              correctionApplied ? "Haldane–Anscombe corrected" : "raw"
            } counts.
          </p>
          <p class="text-sm mt-3 flex items-center justify-between">
            <span>log(OR)</span>
            <span class="font-mono">${formatNumber(logOR, 3)}</span>
          </p>
          <p class="text-sm flex items-center justify-between">
            <span>SE[log(OR)]</span>
            <span class="font-mono">${formatNumber(seLogOR, 3)}</span>
          </p>
        </div>
        <div class="border border-gray-200 rounded-md p-3">
          <h3 class="text-sm font-semibold mb-2">Inference</h3>
          <p class="text-sm flex items-center justify-between">
            <span>${level.toFixed(0)}% CI for OR</span>
            <span class="font-mono">[${formatNumber(
              lowerOR,
              3
            )}, ${formatNumber(upperOR, 3)}]</span>
          </p>
          <p class="text-sm flex items-center justify-between mt-2">
            <span>z statistic</span>
            <span class="font-mono">${formatNumber(zStat, 3)}</span>
          </p>
          <p class="text-sm flex items-center justify-between">
            <span>p‑value (two‑sided)</span>
            <span class="font-mono">${
              pTwoSided < 0.0001 ? "&lt; 0.0001" : formatNumber(pTwoSided, 4)
            }</span>
          </p>
          <div class="mt-3">
            <span class="result-badge ${
              pTwoSided < alpha
                ? "bg-green-100 text-green-800"
                : "bg-yellow-100 text-yellow-800"
            }">
              ${
                pTwoSided < alpha
                  ? "Statistically significant"
                  : "Not statistically significant"
              }
            </span>
          </div>
        </div>
      </div>
      <div class="mt-4 border border-blue-100 bg-blue-50 rounded-md p-3 text-sm text-blue-900">
        <h3 class="font-semibold mb-1">Interpretation</h3>
        <p>${interpretation}</p>
      </div>
      ${
        correctionApplied
          ? `
      <p class="mt-3 text-xs text-gray-500">
        Note: At least one cell was zero. A continuity correction of +0.5 was added to all four cells (Haldane–Anscombe) to allow
        computation of the odds ratio and its confidence interval.
      </p>`
          : ""
      }
    `;

        document.getElementById("results-container").innerHTML = resultsHtml;
      }

      function resetForm() {
        document.getElementById("label-exposed").value = "Exposed";
        document.getElementById("label-unexposed").value = "Unexposed";
        document.getElementById("label-outcome").value = "Outcome +";
        document.getElementById("label-nooutcome").value = "Outcome −";

        document.getElementById("cell-a").value = 30;
        document.getElementById("cell-b").value = 70;
        document.getElementById("cell-c").value = 10;
        document.getElementById("cell-d").value = 90;

        document.getElementById("apply-correction").checked = true;
        document.getElementById("alpha-level").value = "0.05";

        syncLabels();
        updateTotals();

        document.getElementById("results-container").innerHTML =
          '<p class="text-sm text-gray-600">Enter your data and click “Calculate odds ratio” to see results here.</p>';
      }

      document.addEventListener("DOMContentLoaded", function () {
        ["cell-a", "cell-b", "cell-c", "cell-d"].forEach((id) => {
          document.getElementById(id).addEventListener("input", updateTotals);
        });

        [
          "label-exposed",
          "label-unexposed",
          "label-outcome",
          "label-nooutcome",
        ].forEach((id) => {
          document.getElementById(id).addEventListener("input", syncLabels);
        });

        document
          .getElementById("calculate-btn")
          .addEventListener("click", calculateOR);
        document
          .getElementById("reset-btn")
          .addEventListener("click", resetForm);

        syncLabels();
        updateTotals();
      });
    </script>

