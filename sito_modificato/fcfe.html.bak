    <script>
      // Tab switching
      const tabNetIncome = document.getElementById("tab-netincome");
      const tabFCFF = document.getElementById("tab-fcff");
      const modeNetIncome = document.getElementById("mode-netincome");
      const modeFCFF = document.getElementById("mode-fcff");

      function setMode(mode) {
        if (mode === "netincome") {
          modeNetIncome.classList.remove("hidden");
          modeFCFF.classList.add("hidden");
          tabNetIncome.classList.add("bg-blue-600", "text-white");
          tabNetIncome.classList.remove("bg-white", "text-gray-700");
          tabFCFF.classList.remove("bg-blue-600", "text-white");
          tabFCFF.classList.add("bg-white", "text-gray-700");
        } else {
          modeNetIncome.classList.add("hidden");
          modeFCFF.classList.remove("hidden");
          tabFCFF.classList.add("bg-blue-600", "text-white");
          tabFCFF.classList.remove("bg-white", "text-gray-700");
          tabNetIncome.classList.remove("bg-blue-600", "text-white");
          tabNetIncome.classList.add("bg-white", "text-gray-700");
        }
        document.getElementById("results").classList.add("hidden");
        document.getElementById("errorMsg").textContent = "";
      }

      tabNetIncome.addEventListener("click", () => setMode("netincome"));
      tabFCFF.addEventListener("click", () => setMode("fcff"));

      // Number formatting
      function formatCurrency(value) {
        if (!isFinite(value)) return "–";
        const abs = Math.abs(value);
        let suffix = "";
        let num = value;
        if (abs >= 1e9) {
          num = value / 1e9;
          suffix = "B";
        } else if (abs >= 1e6) {
          num = value / 1e6;
          suffix = "M";
        } else if (abs >= 1e3) {
          num = value / 1e3;
          suffix = "K";
        }
        return (
          (value < 0 ? "-$" : "$") +
          Math.abs(num).toLocaleString(undefined, {
            maximumFractionDigits: 2,
          }) +
          suffix
        );
      }

      function formatNumber(value) {
        if (!isFinite(value)) return "–";
        return value.toLocaleString(undefined, { maximumFractionDigits: 4 });
      }

      // Calculation
      document.getElementById("calculateBtn").addEventListener("click", () => {
        const isNetIncomeMode = !modeNetIncome.classList.contains("hidden");
        const errorEl = document.getElementById("errorMsg");
        const resultsEl = document.getElementById("results");
        const breakdownEl = document.getElementById("breakdown");
        errorEl.textContent = "";
        breakdownEl.innerHTML = "";

        let fcfe = null;
        let perShare = null;
        let breakdownLines = [];

        if (isNetIncomeMode) {
          const ni = parseFloat(document.getElementById("ni").value);
          const da = parseFloat(document.getElementById("da").value);
          const capex = parseFloat(document.getElementById("capex").value);
          const wc = parseFloat(document.getElementById("wc").value);
          const netBorrowing = parseFloat(
            document.getElementById("netBorrowing").value
          );
          const shares = parseFloat(document.getElementById("shares").value);

          if (
            isNaN(ni) ||
            isNaN(da) ||
            isNaN(capex) ||
            isNaN(wc) ||
            isNaN(netBorrowing)
          ) {
            errorEl.textContent =
              "Please fill in Net Income, D&A, Capex, ΔWorking Capital, and Net Borrowing.";
            resultsEl.classList.add("hidden");
            return;
          }

          fcfe = ni + da - capex - wc + netBorrowing;

          breakdownLines.push(
            `FCFE = Net Income + Depreciation & Amortization − Capex − ΔWorking Capital + Net Borrowing`
          );
          breakdownLines.push(
            `FCFE = ${formatCurrency(ni)} + ${formatCurrency(
              da
            )} − ${formatCurrency(capex)} − ${formatCurrency(
              wc
            )} + ${formatCurrency(netBorrowing)}`
          );
          breakdownLines.push(`FCFE = ${formatCurrency(fcfe)}`);

          if (!isNaN(shares) && shares > 0) {
            perShare = fcfe / shares;
            breakdownLines.push(
              `FCFE per share = FCFE / Shares = ${formatCurrency(
                fcfe
              )} / ${formatNumber(shares)} = ${formatCurrency(perShare)}`
            );
          }
        } else {
          const fcff = parseFloat(document.getElementById("fcff").value);
          const interest = parseFloat(
            document.getElementById("interest").value
          );
          const taxRate = parseFloat(document.getElementById("taxRate").value);
          const netBorrowing2 = parseFloat(
            document.getElementById("netBorrowing2").value
          );
          const shares2 = parseFloat(document.getElementById("shares2").value);

          if (
            isNaN(fcff) ||
            isNaN(interest) ||
            isNaN(taxRate) ||
            isNaN(netBorrowing2)
          ) {
            errorEl.textContent =
              "Please fill in FCFF, Interest, Tax Rate, and Net Borrowing.";
            resultsEl.classList.add("hidden");
            return;
          }

          const t = taxRate / 100;
          const afterTaxInterest = interest * (1 - t);
          fcfe = fcff - afterTaxInterest + netBorrowing2;

          breakdownLines.push(
            `FCFE = FCFF − Interest × (1 − Tax Rate) + Net Borrowing`
          );
          breakdownLines.push(
            `After‑tax interest = ${formatCurrency(
              interest
            )} × (1 − ${formatNumber(t)}) = ${formatCurrency(afterTaxInterest)}`
          );
          breakdownLines.push(
            `FCFE = ${formatCurrency(fcff)} − ${formatCurrency(
              afterTaxInterest
            )} + ${formatCurrency(netBorrowing2)} = ${formatCurrency(fcfe)}`
          );

          if (!isNaN(shares2) && shares2 > 0) {
            perShare = fcfe / shares2;
            breakdownLines.push(
              `FCFE per share = FCFE / Shares = ${formatCurrency(
                fcfe
              )} / ${formatNumber(shares2)} = ${formatCurrency(perShare)}`
            );
          }
        }

        document.getElementById("fcfeValue").textContent = formatCurrency(fcfe);
        document.getElementById("fcfePerShare").textContent =
          perShare === null ? "–" : formatCurrency(perShare);

        breakdownLines.forEach((line) => {
          const p = document.createElement("p");
          p.textContent = line;
          breakdownEl.appendChild(p);
        });

        resultsEl.classList.remove("hidden");
      });

      document.getElementById("resetBtn").addEventListener("click", () => {
        document.getElementById("fcfe-form").reset();
        document.getElementById("results").classList.add("hidden");
        document.getElementById("errorMsg").textContent = "";
        document.getElementById("breakdown").innerHTML = "";
      });

      // Mobile menu script (simple)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", () => {
          const menu = document.getElementById("mobile-menu");
          const expanded = menu.classList.contains("hidden") ? "true" : "false";
          menu.classList.toggle("hidden");
          document
            .getElementById("mobile-menu-toggle")
            .setAttribute("aria-expanded", expanded);
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
