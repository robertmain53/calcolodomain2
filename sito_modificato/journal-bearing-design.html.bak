    <script>
      // Unit helpers
      function mmToM(v) {
        return v / 1000;
      }
      function inToM(v) {
        return v * 0.0254;
      }
      function umToM(v) {
        return v / 1e6;
      }
      function rpmToRps(v) {
        return v / 60;
      }
      function kNToN(v) {
        return v * 1000;
      }
      function kgfToN(v) {
        return v * 9.80665;
      }
      function cPToPas(v) {
        return v / 1000;
      }
      function umToMm(v) {
        return v / 1000;
      }

      function formatVal(v, unit, digits = 3) {
        if (!isFinite(v)) return "–";
        return v.toExponential
          ? v.toExponential(digits).replace(/e\+?(-?\d+)/, "×10^$1") +
              " " +
              unit
          : v.toFixed(digits) + " " + unit;
      }

      function formatFixed(v, unit, digits = 3) {
        if (!isFinite(v)) return "–";
        return v.toFixed(digits) + " " + unit;
      }

      function updatePreset(preset) {
        const shaft = document.getElementById("shaftDiameter");
        const length = document.getElementById("bearingLength");
        const clear = document.getElementById("clearance");
        const speed = document.getElementById("speed");
        const load = document.getElementById("load");
        const visc = document.getElementById("viscosity");

        if (preset === "light") {
          shaft.value = 40;
          length.value = 40;
          clear.value = 0.04;
          speed.value = 3000;
          load.value = 3000;
          visc.value = 0.01;
        } else if (preset === "medium") {
          shaft.value = 50;
          length.value = 50;
          clear.value = 0.05;
          speed.value = 1500;
          load.value = 5000;
          visc.value = 0.02;
        } else if (preset === "heavy") {
          shaft.value = 80;
          length.value = 80;
          clear.value = 0.08;
          speed.value = 600;
          load.value = 15000;
          visc.value = 0.04;
        }
        document.getElementById("statusMessage").textContent =
          "Preset applied. Adjust as needed, then calculate.";
      }

      document.querySelectorAll(".preset-btn").forEach((btn) => {
        btn.addEventListener("click", () => updatePreset(btn.dataset.preset));
      });

      document.getElementById("resetBtn").addEventListener("click", () => {
        document.getElementById("bearing-form").reset();
        document.getElementById("shaftDiameter").value = 50;
        document.getElementById("bearingLength").value = 50;
        document.getElementById("clearance").value = 0.05;
        document.getElementById("speed").value = 1500;
        document.getElementById("load").value = 5000;
        document.getElementById("viscosity").value = 0.02;
        document.getElementById("roughness").value = 0.5;
        document.getElementById("pvLimit").value = 1.5;
        document.getElementById("shortBearing").checked = true;
        document.getElementById("fullFilm").checked = true;
        [
          "unitLoad",
          "slidingSpeed",
          "pvFactor",
          "sommerfeld",
          "hmin",
          "filmRatio",
          "frictionCoeff",
          "powerLoss",
        ].forEach((id) => {
          document.getElementById(id).textContent = "–";
        });
        document.getElementById("safetyBadge").textContent = "Not evaluated";
        document.getElementById("safetyBadge").className =
          "result-badge px-2 py-1 rounded-full bg-gray-200 text-gray-700";
        document.getElementById("designNote").textContent =
          "Enter parameters and click “Calculate bearing performance” to see results and qualitative safety assessment.";
        document.getElementById("statusMessage").textContent = "";
      });

      document.getElementById("calculateBtn").addEventListener("click", () => {
        const status = document.getElementById("statusMessage");
        status.textContent = "";

        // Read & convert inputs
        let D = parseFloat(document.getElementById("shaftDiameter").value);
        const Dunit = document.getElementById("shaftDiameterUnit").value;
        let L = parseFloat(document.getElementById("bearingLength").value);
        const Lunit = document.getElementById("bearingLengthUnit").value;
        let C = parseFloat(document.getElementById("clearance").value);
        const Cunit = document.getElementById("clearanceUnit").value;
        let n = parseFloat(document.getElementById("speed").value);
        const nUnit = document.getElementById("speedUnit").value;
        let W = parseFloat(document.getElementById("load").value);
        const Wunit = document.getElementById("loadUnit").value;
        let mu = parseFloat(document.getElementById("viscosity").value);
        const muUnit = document.getElementById("viscosityUnit").value;
        let Rq = parseFloat(document.getElementById("roughness").value);
        const RqUnit = document.getElementById("roughnessUnit").value;
        let pvLimit = parseFloat(document.getElementById("pvLimit").value);

        if ([D, L, C, n, W, mu, Rq].some((v) => !isFinite(v) || v <= 0)) {
          status.textContent =
            "Please enter positive numeric values for all required inputs.";
          return;
        }

        // Convert to SI
        if (Dunit === "mm") D = mmToM(D);
        else if (Dunit === "in") D = inToM(D);
        if (Lunit === "mm") L = mmToM(L);
        else if (Lunit === "in") L = inToM(L);
        if (Cunit === "mm") C = mmToM(C);
        else if (Cunit === "um") C = umToM(C);
        else if (Cunit === "in") C = inToM(C);
        if (nUnit === "rpm") n = rpmToRps(n);
        if (Wunit === "kN") W = kNToN(W);
        else if (Wunit === "kgf") W = kgfToN(W);
        if (muUnit === "cP") mu = cPToPas(mu);
        if (RqUnit === "um") Rq = umToM(Rq);
        else if (RqUnit === "mm") Rq = mmToM(Rq);
        if (!isFinite(pvLimit) || pvLimit <= 0) pvLimit = 1.5; // default MPa·m/s

        const R = D / 2;

        // Unit load p
        const p = W / (L * D); // Pa
        // Sliding speed U
        const U = Math.PI * D * n; // m/s
        // PV factor (Pa * m/s -> MPa·m/s)
        const PV = (p * U) / 1e6;

        // Sommerfeld number (one common definition)
        const S = (mu * n * Math.pow(R / C, 2)) / p;

        // Eccentricity ratio heuristic
        // Choose k so that for S ~ 1, epsilon ~ 0.7; for S ~ 0.1, epsilon ~ 0.9
        const k = 0.6;
        let eps = 1 / (1 + k * S);
        if (eps > 0.99) eps = 0.99;
        if (eps < 0.1) eps = 0.1;

        const hmin = C * (1 - eps);

        // Friction coefficient heuristic: f ≈ kf * sqrt(S)
        const kf = 0.02;
        let f = kf * Math.sqrt(S);
        if (f < 0.0005) f = 0.0005;

        const Pf = f * W * U; // W

        const filmRatio = hmin / Rq;

        // Update UI
        document.getElementById("unitLoad").textContent = formatFixed(
          p / 1e6,
          "MPa",
          3
        );
        document.getElementById("slidingSpeed").textContent = formatFixed(
          U,
          "m/s",
          3
        );
        document.getElementById("pvFactor").textContent = formatFixed(
          PV,
          "MPa·m/s",
          3
        );
        document.getElementById("sommerfeld").textContent = formatFixed(
          S,
          "–",
          3
        );
        document.getElementById("hmin").textContent = formatFixed(
          hmin * 1e6,
          "µm",
          2
        );
        document.getElementById("filmRatio").textContent = isFinite(filmRatio)
          ? filmRatio.toFixed(2) + " –"
          : "–";
        document.getElementById("frictionCoeff").textContent = formatFixed(
          f,
          "–",
          4
        );
        document.getElementById("powerLoss").textContent = formatFixed(
          Pf / 1000,
          "kW",
          3
        );

        // Safety badge
        const badge = document.getElementById("safetyBadge");
        let note = [];

        let safeFilm = filmRatio >= 3;
        let safePV = PV <= pvLimit;

        if (safeFilm && safePV) {
          badge.textContent = "Likely OK (preliminary)";
          badge.className =
            "result-badge px-2 py-1 rounded-full bg-green-100 text-green-700";
          note.push(
            "Minimum film thickness is comfortably above combined surface roughness, and PV is below the specified limit."
          );
        } else if (!safeFilm && safePV) {
          badge.textContent = "Film risk";
          badge.className =
            "result-badge px-2 py-1 rounded-full bg-yellow-100 text-yellow-700";
          note.push(
            "Minimum film thickness is low relative to surface roughness. Mixed or boundary lubrication is likely."
          );
        } else if (safeFilm && !safePV) {
          badge.textContent = "PV overload risk";
          badge.className =
            "result-badge px-2 py-1 rounded-full bg-yellow-100 text-yellow-700";
          note.push(
            "PV factor exceeds the specified material limit. Overheating and wear are more likely."
          );
        } else {
          badge.textContent = "High risk";
          badge.className =
            "result-badge px-2 py-1 rounded-full bg-red-100 text-red-700";
          note.push(
            "Both film thickness and PV factor are outside typical safe ranges. Reconsider geometry, load, speed, or lubricant."
          );
        }

        note.push(
          "These checks are approximate and intended for preliminary design only. Validate critical bearings with detailed analysis and manufacturer data."
        );
        document.getElementById("designNote").textContent = note.join(" ");
        status.textContent = "Calculation completed.";
      });
    </script>

