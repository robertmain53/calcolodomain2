    <script>
      // Utility: format with engineering prefixes
      function formatWithPrefix(value, unit) {
        if (!isFinite(value)) return "—";
        const abs = Math.abs(value);
        let scaled = value,
          suffix = unit;
        if (abs === 0) return "0 " + unit;
        if (abs >= 1e9) {
          scaled = value / 1e9;
          suffix = "G" + unit;
        } else if (abs >= 1e6) {
          scaled = value / 1e6;
          suffix = "M" + unit;
        } else if (abs >= 1e3) {
          scaled = value / 1e3;
          suffix = "k" + unit;
        } else if (abs >= 1) {
          scaled = value;
          suffix = unit;
        } else if (abs >= 1e-3) {
          scaled = value * 1e3;
          suffix = "m" + unit;
        } else if (abs >= 1e-6) {
          scaled = value * 1e6;
          suffix = "µ" + unit;
        } else if (abs >= 1e-9) {
          scaled = value * 1e9;
          suffix = "n" + unit;
        } else {
          scaled = value * 1e12;
          suffix = "p" + unit;
        }
        return scaled.toFixed(3).replace(/\.?0+$/, "") + " " + suffix;
      }

      function formatNumber(value, decimals = 3) {
        if (!isFinite(value)) return "—";
        if (Math.abs(value) >= 1e4 || (Math.abs(value) < 1e-2 && value !== 0)) {
          return value.toExponential(2);
        }
        return value.toFixed(decimals).replace(/\.?0+$/, "");
      }

      document.addEventListener("DOMContentLoaded", () => {
        const filterButtons = document.querySelectorAll(".filter-type-btn");
        let filterType = "lowpass";

        filterButtons.forEach((btn) => {
          btn.addEventListener("click", () => {
            filterType = btn.dataset.filterType;
            filterButtons.forEach((b) => {
              if (b === btn) {
                b.classList.remove(
                  "border-gray-300",
                  "bg-white",
                  "text-gray-700"
                );
                b.classList.add("border-blue-600", "bg-blue-600", "text-white");
              } else {
                b.classList.remove(
                  "border-blue-600",
                  "bg-blue-600",
                  "text-white"
                );
                b.classList.add("border-gray-300", "bg-white", "text-gray-700");
              }
            });
            calculateRcFilter(); // recalc on type change
          });
        });

        const rInput = document.getElementById("r-value");
        const rUnit = document.getElementById("r-unit");
        const cInput = document.getElementById("c-value");
        const cUnit = document.getElementById("c-unit");
        const fInput = document.getElementById("freq-value");
        const fUnit = document.getElementById("freq-unit");
        const slider = document.getElementById("freq-slider");
        const sliderLabel = document.getElementById("freq-slider-label");

        const fcOut = document.getElementById("fc-output");
        const tauOut = document.getElementById("tau-output");
        const omegaOut = document.getElementById("omega-output");
        const magOut = document.getElementById("mag-output");
        const magDbOut = document.getElementById("magdb-output");
        const phaseOut = document.getElementById("phase-output");

        function sliderToFrequency(x) {
          // x in [0,4] -> 10^x Hz (10^0=1 Hz to 10^4=10 kHz)
          return Math.pow(10, x);
        }

        function updateSliderLabel(freqHz) {
          sliderLabel.textContent = formatWithPrefix(freqHz, "Hz");
        }

        slider.addEventListener("input", () => {
          const fHz = sliderToFrequency(parseFloat(slider.value));
          updateSliderLabel(fHz);
          // also update numeric frequency input (in Hz, kHz, MHz depending on magnitude)
          let unitFactor = 1,
            unitSelect = "1";
          if (fHz >= 1e6) {
            unitFactor = 1e6;
            unitSelect = "1e6";
          } else if (fHz >= 1e3) {
            unitFactor = 1e3;
            unitSelect = "1e3";
          } else {
            unitFactor = 1;
            unitSelect = "1";
          }
          fUnit.value = unitSelect;
          fInput.value = (fHz / unitFactor).toFixed(3).replace(/\.?0+$/, "");
          calculateRcFilter();
        });

        document
          .getElementById("rc-calc-btn")
          .addEventListener("click", calculateRcFilter);
        [rInput, rUnit, cInput, cUnit, fInput, fUnit].forEach((el) => {
          el.addEventListener("change", calculateRcFilter);
        });

        function calculateRcFilter() {
          const R = parseFloat(rInput.value) * parseFloat(rUnit.value);
          const C = parseFloat(cInput.value) * parseFloat(cUnit.value);
          const f = parseFloat(fInput.value) * parseFloat(fUnit.value);

          if (!isFinite(R) || !isFinite(C) || R <= 0 || C <= 0) {
            fcOut.textContent = "—";
            tauOut.textContent = "—";
            omegaOut.textContent = "—";
            magOut.textContent = "—";
            magDbOut.textContent = "—";
            phaseOut.textContent = "—";
            return;
          }

          const tau = R * C;
          const fc = 1 / (2 * Math.PI * R * C);
          const omega = 2 * Math.PI * f;

          fcOut.textContent = formatWithPrefix(fc, "Hz");
          tauOut.textContent = formatWithPrefix(tau, "s");
          omegaOut.textContent = formatWithPrefix(omega, "rad/s");

          if (!isFinite(f) || f <= 0) {
            magOut.textContent = "—";
            magDbOut.textContent = "—";
            phaseOut.textContent = "—";
            return;
          }

          const x = omega * R * C;
          let mag, phaseRad;

          if (filterType === "lowpass") {
            mag = 1 / Math.sqrt(1 + x * x);
            phaseRad = -Math.atan(x);
          } else {
            mag = x / Math.sqrt(1 + x * x);
            phaseRad = Math.PI / 2 - Math.atan(x);
          }

          const magDb = 20 * Math.log10(mag);
          const phaseDeg = (phaseRad * 180) / Math.PI;

          magOut.textContent = formatNumber(mag, 4);
          magDbOut.textContent = formatNumber(magDb, 2) + " dB";
          phaseOut.textContent = formatNumber(phaseDeg, 1) + "°";
        }

        // Initialize slider label and outputs
        const initialF = sliderToFrequency(parseFloat(slider.value));
        updateSliderLabel(initialF);
        calculateRcFilter();
      });
    </script>

