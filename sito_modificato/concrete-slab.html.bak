    <script>
      // Unit toggle
      const unitImperialBtn = document.getElementById("unit-imperial");
      const unitMetricBtn = document.getElementById("unit-metric");
      const inputsImperial = document.getElementById("inputs-imperial");
      const inputsMetric = document.getElementById("inputs-metric");

      function setUnitSystem(system) {
        if (system === "imperial") {
          unitImperialBtn.classList.add("bg-blue-600", "text-white");
          unitImperialBtn.classList.remove("bg-white", "text-gray-700");
          unitMetricBtn.classList.add("bg-white", "text-gray-700");
          unitMetricBtn.classList.remove("bg-blue-600", "text-white");
          inputsImperial.classList.remove("hidden");
          inputsMetric.classList.add("hidden");
        } else {
          unitMetricBtn.classList.add("bg-blue-600", "text-white");
          unitMetricBtn.classList.remove("bg-white", "text-gray-700");
          unitImperialBtn.classList.add("bg-white", "text-gray-700");
          unitImperialBtn.classList.remove("bg-blue-600", "text-white");
          inputsMetric.classList.remove("hidden");
          inputsImperial.classList.add("hidden");
        }
      }

      unitImperialBtn.addEventListener("click", () =>
        setUnitSystem("imperial")
      );
      unitMetricBtn.addEventListener("click", () => setUnitSystem("metric"));

      // Thickness custom fields
      const thicknessInSelect = document.getElementById("thickness-in");
      const thicknessInCustom = document.getElementById("thickness-in-custom");
      const thicknessCmSelect = document.getElementById("thickness-cm");
      const thicknessCmCustom = document.getElementById("thickness-cm-custom");

      thicknessInSelect.addEventListener("change", () => {
        if (thicknessInSelect.value === "custom") {
          thicknessInCustom.classList.remove("hidden");
        } else {
          thicknessInCustom.classList.add("hidden");
        }
      });

      thicknessCmSelect.addEventListener("change", () => {
        if (thicknessCmSelect.value === "custom") {
          thicknessCmCustom.classList.remove("hidden");
        } else {
          thicknessCmCustom.classList.add("hidden");
        }
      });

      // Calculation
      const calculateBtn = document.getElementById("calculate-btn");
      const resetBtn = document.getElementById("reset-btn");
      const resultsDiv = document.getElementById("results");

      function parseNumber(id) {
        const el = document.getElementById(id);
        if (!el) return 0;
        const v = parseFloat(el.value);
        return isNaN(v) ? 0 : v;
      }

      function formatNumber(val, decimals) {
        if (!isFinite(val)) return "–";
        return val.toFixed(decimals);
      }

      calculateBtn.addEventListener("click", () => {
        const usingImperial = !inputsImperial.classList.contains("hidden");
        const wastePct = Math.max(0, parseNumber("waste"));

        let lengthFt = 0,
          widthFt = 0,
          thicknessFt = 0;
        let lengthM = 0,
          widthM = 0,
          thicknessM = 0;

        if (usingImperial) {
          const Lft = parseNumber("length-ft");
          const Lin = parseNumber("length-in");
          const Wft = parseNumber("width-ft");
          const Win = parseNumber("width-in");
          let Tin;
          if (thicknessInSelect.value === "custom") {
            Tin = parseNumber("thickness-in-custom");
          } else {
            Tin = parseFloat(thicknessInSelect.value);
          }
          if (Tin <= 0) {
            alert("Please enter a valid slab thickness in inches.");
            return;
          }
          lengthFt = Lft + Lin / 12;
          widthFt = Wft + Win / 12;
          thicknessFt = Tin / 12;
          if (lengthFt <= 0 || widthFt <= 0) {
            alert("Please enter positive length and width.");
            return;
          }
        } else {
          lengthM = parseNumber("length-m");
          widthM = parseNumber("width-m");
          let Tcm;
          if (thicknessCmSelect.value === "custom") {
            Tcm = parseNumber("thickness-cm-custom");
          } else {
            Tcm = parseFloat(thicknessCmSelect.value);
          }
          if (Tcm <= 0) {
            alert("Please enter a valid slab thickness in centimeters.");
            return;
          }
          thicknessM = Tcm / 100;
          if (lengthM <= 0 || widthM <= 0) {
            alert("Please enter positive length and width.");
            return;
          }
          // Convert to imperial internally for bag/rebar estimates
          lengthFt = lengthM * 3.28084;
          widthFt = widthM * 3.28084;
          thicknessFt = thicknessM * 3.28084;
        }

        // Volume calculations
        let volumeFt3 = lengthFt * widthFt * thicknessFt;
        let volumeM3 = volumeFt3 * 0.0283168;
        let volumeYd3 = volumeFt3 / 27;

        const factor = 1 + wastePct / 100;
        volumeFt3 *= factor;
        volumeM3 *= factor;
        volumeYd3 *= factor;

        // Bag estimation
        const bagSize = parseInt(document.getElementById("bag-size").value, 10);
        const yieldPerBag = bagSize === 60 ? 0.45 : 0.6; // ft³
        const bags = volumeFt3 > 0 ? Math.ceil(volumeFt3 / yieldPerBag) : 0;

        // Rebar estimation
        let rebarLengthFt = 0;
        if (usingImperial) {
          const spacingIn = document.getElementById("rebar-grid").value;
          if (spacingIn !== "none") {
            const sFt = parseFloat(spacingIn) / 12;
            const barsAlongLength = Math.floor(widthFt / sFt) + 1;
            const barsAlongWidth = Math.floor(lengthFt / sFt) + 1;
            rebarLengthFt =
              barsAlongLength * lengthFt + barsAlongWidth * widthFt;
          }
        } else {
          const spacingM = document.getElementById("rebar-grid-metric").value;
          if (spacingM !== "none") {
            const sM = parseFloat(spacingM);
            const widthMCalc = widthFt / 3.28084;
            const lengthMCalc = lengthFt / 3.28084;
            const barsAlongLength = Math.floor(widthMCalc / sM) + 1;
            const barsAlongWidth = Math.floor(lengthMCalc / sM) + 1;
            const totalM =
              barsAlongLength * lengthMCalc + barsAlongWidth * widthMCalc;
            rebarLengthFt = totalM * 3.28084;
          }
        }

        // Cost estimation
        let costText = "–";
        if (usingImperial) {
          const pricePerYd = parseNumber("price-per-yard");
          if (pricePerYd > 0) {
            const cost = volumeYd3 * pricePerYd;
            costText = "$" + formatNumber(cost, 2);
          }
        } else {
          const pricePerM3 = parseNumber("price-per-m3");
          if (pricePerM3 > 0) {
            const cost = volumeM3 * pricePerM3;
            costText = "$" + formatNumber(cost, 2);
          }
        }

        // Populate results
        document.getElementById("res-volume-ft3").textContent = formatNumber(
          volumeFt3,
          2
        );
        document.getElementById("res-volume-yd3").textContent = formatNumber(
          volumeYd3,
          2
        );
        document.getElementById("res-volume-m3").textContent = formatNumber(
          volumeM3,
          3
        );
        document.getElementById("res-waste-pct").textContent =
          wastePct.toString();
        document.getElementById("res-bags").textContent =
          bags > 0 ? bags.toString() : "–";
        document.getElementById("res-bag-size").textContent = bagSize
          ? bagSize.toString()
          : "–";
        document.getElementById("res-rebar-length").textContent =
          rebarLengthFt > 0 ? formatNumber(rebarLengthFt, 1) : "–";
        document.getElementById("res-cost-concrete").textContent = costText;

        resultsDiv.classList.remove("hidden");
      });

      resetBtn.addEventListener("click", () => {
        document.querySelectorAll("input").forEach((el) => {
          if (el.type === "number") el.value = "";
        });
        document.getElementById("waste").value = "10";
        document.getElementById("price-per-yard").value = "";
        document.getElementById("price-per-m3").value = "";
        thicknessInSelect.value = "4";
        thicknessInCustom.classList.add("hidden");
        thicknessCmSelect.value = "10";
        thicknessCmCustom.classList.add("hidden");
        document.getElementById("bag-size").value = "80";
        document.getElementById("rebar-grid").value = "none";
        document.getElementById("rebar-grid-metric").value = "none";
        resultsDiv.classList.add("hidden");
      });

      // Initialize default unit system
      setUnitSystem("imperial");

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
