<script>
  // Utility conversions
  function toMeters(value, unit) {
    const v = Number(value);
    if (unit === 'm') return v;
    if (unit === 'mm') return v / 1000;
    if (unit === 'ft') return v * 0.3048;
    if (unit === 'in') return v * 0.0254;
    return v;
  }
  function toMetersFromDepth(value, unit) {
    return toMeters(value, unit);
  }
  function toMetersFromLength(value, unit) {
    return unit === 'ft' ? value * 0.3048 : value;
  }
  function toCMS(value, unit) {
    return unit === 'cfs' ? value * 0.0283168466 : value;
  }
  function formatNumber(x, digits = 3) {
    if (!isFinite(x)) return '–';
    return Number(x).toFixed(digits);
  }

  function getManningN() {
    const material = document.getElementById('material').value;
    if (material === 'custom') {
      return Number(document.getElementById('manningN').value) || 0.015;
    }
    return Number(material);
  }

  function computeCulvert() {
    const Q = toCMS(document.getElementById('q').value, document.getElementById('qUnit').value);
    const S = Number(document.getElementById('slope').value);
    const L = toMetersFromLength(Number(document.getElementById('length').value), document.getElementById('lengthUnit').value);
    const TW = toMetersFromDepth(Number(document.getElementById('tailwater').value), document.getElementById('tailwaterUnit').value);
    const HWallow = toMetersFromDepth(Number(document.getElementById('hwAllow').value), document.getElementById('hwAllowUnit').value);
    const D = toMeters(document.getElementById('diameter').value, document.getElementById('diameterUnit').value);
    const Cd = Number(document.getElementById('entrance').value);
    const n = getManningN();
    const unitsSystem = document.getElementById('unitsSystem').value;

    if (!(Q > 0) || !(S > 0) || !(D > 0) || !(L > 0)) {
      alert('Please enter positive values for Q, slope, diameter and length.');
      return null;
    }

    const g = 9.81;
    const A = Math.PI * D * D / 4;
    const P = Math.PI * D;
    const R = A / P;

    // Inlet control: orifice equation, solve for H above centerline
    const H_inlet = Math.pow(Q / (Cd * A), 2) / (2 * g);
    const HW_inlet = H_inlet + D / 2; // depth above invert
    const Qcap_inlet = Cd * A * Math.sqrt(2 * g * (HWallow - D / 2 > 0 ? HWallow - D / 2 : 0));

    // Outlet control: full-flow Manning capacity
    const Q_manning = (1 / n) * A * Math.pow(R, 2 / 3) * Math.pow(S, 0.5);

    // Simple energy balance to estimate HW under outlet control:
    // HW = TW + headloss (entrance + friction)
    const V = Q / A;
    const ke = 0.5; // entrance loss coefficient (approx)
    const hf = (n * n * L * Math.pow(Q, 2)) / (Math.pow(A, 2) * Math.pow(R, 4 / 3)); // Manning-based friction loss
    const HW_outlet = TW + ke * V * V / (2 * g) + hf;
    const Qcap_outlet = Q_manning; // capacity limited by full-flow Manning

    // Determine controlling condition
    const HW_control = Math.max(HW_inlet, HW_outlet);
    const condition = HW_inlet >= HW_outlet ? 'Inlet control' : 'Outlet control';

    // Convert outputs to requested units
    const depthFactor = unitsSystem === 'us' ? 3.2808399 : 1;
    const qFactor = unitsSystem === 'us' ? 35.3146667 : 1;
    const velFactor = unitsSystem === 'us' ? 3.2808399 : 1;

    const result = {
      Q_inlet: Qcap_inlet * qFactor,
      Q_outlet: Qcap_outlet * qFactor,
      HW_inlet: HW_inlet * depthFactor,
      HW_outlet: HW_outlet * depthFactor,
      HW_control: HW_control * depthFactor,
      HW_allow: HWallow * depthFactor,
      HW_inlet_ratio: HW_inlet / D,
      HW_outlet_ratio: HW_outlet / D,
      Q_manning: Q_manning * qFactor,
      V: V * velFactor,
      condition,
      units: unitsSystem === 'us'
        ? { depth: 'ft', flow: 'cfs', velocity: 'ft/s' }
        : { depth: 'm', flow: 'm³/s', velocity: 'm/s' },
      Q_design: Q * qFactor
    };

    return result;
  }

  function updateResults() {
    const res = computeCulvert();
    if (!res) return;

    document.getElementById('noResultsMessage').classList.add('hidden');
    document.getElementById('resultsSummary').classList.remove('hidden');

    const depthUnit = res.units.depth;
    const flowUnit = res.units.flow;
    const velUnit = res.units.velocity;

    document.getElementById('controllingCondition').textContent = res.condition;
    const passes = res.HW_control <= res.HW_allow;
    document.getElementById('capacityStatus').textContent = passes
      ? 'Design discharge passes without exceeding allowable headwater.'
      : 'Design discharge exceeds allowable headwater for this diameter.';

    document.getElementById('hwResult').textContent = `${formatNumber(res.HW_control, 3)} ${depthUnit}`;
    document.getElementById('hwRatio').textContent = `HW/D (controlling) ≈ ${formatNumber(
      res.condition === 'Inlet control' ? res.HW_inlet_ratio : res.HW_outlet_ratio,
      2
    )}`;

    document.getElementById('velocityResult').textContent = `${formatNumber(res.V, 2)} ${velUnit}`;
    let vNote = 'Typical recommended outlet velocities are often 1–3 m/s (3–10 ft/s), depending on channel protection.';
    document.getElementById('velocityNote').textContent = vNote;

    document.getElementById('qInlet').textContent = `${formatNumber(res.Q_inlet, 3)} ${flowUnit}`;
    document.getElementById('qOutlet').textContent = `${formatNumber(res.Q_outlet, 3)} ${flowUnit}`;
    document.getElementById('hwInlet').textContent = `${formatNumber(res.HW_inlet, 3)} ${depthUnit}`;
    document.getElementById('hwOutlet').textContent = `${formatNumber(res.HW_outlet, 3)} ${depthUnit}`;
    document.getElementById('hwInletRatio').textContent = formatNumber(res.HW_inlet_ratio, 2);
    document.getElementById('hwOutletRatio').textContent = formatNumber(res.HW_outlet_ratio, 2);
    document.getElementById('qManning').textContent = `${formatNumber(res.Q_manning, 3)} ${flowUnit}`;

    const designMsg = [];
    designMsg.push(`Design discharge: <strong>${formatNumber(res.Q_design, 3)} ${flowUnit}</strong>.`);
    designMsg.push(`Controlling condition: <strong>${res.condition}</strong>.`);
    designMsg.push(
      `Resulting headwater: <strong>${formatNumber(res.HW_control, 3)} ${depthUnit}</strong> vs allowable <strong>${formatNumber(
        res.HW_allow,
        3
      )} ${depthUnit}</strong>.`
    );
    if (res.HW_control > res.HW_allow) {
      designMsg.push('Consider increasing diameter, steepening slope, improving entrance, or using multiple barrels.');
    } else {
      designMsg.push('Headwater is within the specified limit for this preliminary design.');
    }
    document.getElementById('designMessage').innerHTML = designMsg.join(' ');
  }

  function suggestSizes() {
    const base = computeCulvert();
    if (!base) return;

    const diameters_m = [0.6, 0.75, 0.9, 1.0, 1.2, 1.5, 1.8, 2.1];
    const tbody = document.getElementById('suggestedSizesBody');
    tbody.innerHTML = '';

    const depthUnit = base.units.depth;
    const flowUnit = base.units.flow;

    const Q_si = base.Q_design / (base.units.flow === 'cfs' ? 35.3146667 : 1); // back to m³/s

    diameters_m.forEach(D => {
      const g = 9.81;
      const A = Math.PI * D * D / 4;
      const P = Math.PI * D;
      const R = A / P;
      const n = getManningN();
      const Cd = Number(document.getElementById('entrance').value);
      const S = Number(document.getElementById('slope').value);
      const L = toMetersFromLength(Number(document.getElementById('length').value), document.getElementById('lengthUnit').value);
      const TW = toMetersFromDepth(Number(document.getElementById('tailwater').value), document.getElementById('tailwaterUnit').value);
      const HWallow = toMetersFromDepth(Number(document.getElementById('hwAllow').value), document.getElementById('hwAllowUnit').value);

      const H_inlet = Math.pow(Q_si / (Cd * A), 2) / (2 * g);
      const HW_inlet = H_inlet + D / 2;

      const Q_manning = (1 / n) * A * Math.pow(R, 2 / 3) * Math.pow(S, 0.5);
      const V = Q_si / A;
      const ke = 0.5;
      const hf = (n * n * L * Math.pow(Q_si, 2)) / (Math.pow(A, 2) * Math.pow(R, 4 / 3));
      const HW_outlet = TW + ke * V * V / (2 * g) + hf;

      const HW_control = Math.max(HW_inlet, HW_outlet);
      const condition = HW_inlet >= HW_outlet ? 'Inlet' : 'Outlet';
      const Qcap_control = Q_manning; // simplified

      const depthFactor = base.units.depth === 'ft' ? 3.2808399 : 1;
      const qFactor = base.units.flow === 'cfs' ? 35.3146667 : 1;

      const row = document.createElement('tr');
      const ok = HW_control <= HWallow;

      row.className = ok ? 'bg-green-50' : 'bg-red-50';

      row.innerHTML = `
        <td class="border px-2 py-1">${formatNumber(D * depthFactor, 2)} ${depthUnit}</td>
        <td class="border px-2 py-1 text-right">${formatNumber(Qcap_control * qFactor, 2)} ${flowUnit}</td>
        <td class="border px-2 py-1 text-right">${formatNumber(HW_control * depthFactor, 2)} ${depthUnit} (${condition})</td>
        <td class="border px-2 py-1 text-right">${formatNumber(HW_control / D, 2)}</td>
      `;
      tbody.appendChild(row);
    });

    document.getElementById('suggestedSizesWrapper').classList.remove('hidden');
  }

  document.getElementById('btnRun').addEventListener('click', updateResults);
  document.getElementById('btnTrySizes').addEventListener('click', suggestSizes);
  document.getElementById('btnReset').addEventListener('click', () => {
    document.getElementById('q').value = 5;
    document.getElementById('qUnit').value = 'cms';
    document.getElementById('slope').value = 0.01;
    document.getElementById('length').value = 20;
    document.getElementById('lengthUnit').value = 'm';
    document.getElementById('tailwater').value = 0.5;
    document.getElementById('tailwaterUnit').value = 'm';
    document.getElementById('hwAllow').value = 1.5;
    document.getElementById('hwAllowUnit').value = 'm';
    document.getElementById('diameter').value = 1.0;
    document.getElementById('diameterUnit').value = 'm';
    document.getElementById('material').value = '0.012';
    document.getElementById('customNWrapper').classList.add('hidden');
    document.getElementById('entrance').value = '0.6';
    document.getElementById('unitsSystem').value = 'si';
    document.getElementById('resultsSummary').classList.add('hidden');
    document.getElementById('noResultsMessage').classList.remove('hidden');
    document.getElementById('suggestedSizesWrapper').classList.add('hidden');
  });

  document.getElementById('material').addEventListener('change', () => {
    const mat = document.getElementById('material').value;
    const wrapper = document.getElementById('customNWrapper');
    if (mat === 'custom') {
      wrapper.classList.remove('hidden');
    } else {
      wrapper.classList.add('hidden');
    }
  });

  window.MathJax = {
    tex: { inlineMath: [['\\(','\\)'], ['$', '$']], displayMath: [['$','$'], ['\\[','\\]']] },
    svg: { fontCache: 'global' }
  };
</script>
