    <script>
      // Parse numbers, allowing fractions and mixed numbers like "1 1/2"
      function parseFlexibleNumber(value) {
        if (!value) return NaN;
        value = value.trim();
        if (!value) return NaN;

        // If it's a plain number, parse directly
        if (/^[+-]?\d+(\.\d+)?$/.test(value)) {
          return parseFloat(value);
        }

        // Mixed number: "a b/c"
        const mixedMatch = /^([+-]?\d+)\s+(\d+)\/(\d+)$/.exec(value);
        if (mixedMatch) {
          const whole = parseFloat(mixedMatch[1]);
          const num = parseFloat(mixedMatch[2]);
          const den = parseFloat(mixedMatch[3]);
          if (den === 0) return NaN;
          const frac = num / den;
          return whole >= 0 ? whole + frac : whole - frac;
        }

        // Simple fraction: "a/b"
        const fracMatch = /^([+-]?\d+)\/(\d+)$/.exec(value);
        if (fracMatch) {
          const num = parseFloat(fracMatch[1]);
          const den = parseFloat(fracMatch[2]);
          if (den === 0) return NaN;
          return num / den;
        }

        return NaN;
      }

      function formatNumber(val, decimalsDefault = 4) {
        if (!isFinite(val)) return "";
        const abs = Math.abs(val);
        let decimals = decimalsDefault;
        if (abs === 0) decimals = 0;
        else if (abs >= 1000) decimals = 2;
        else if (abs >= 100) decimals = 3;
        else if (abs >= 10) decimals = 4;
        else decimals = decimalsDefault;

        const rounded = parseFloat(val.toFixed(decimals));
        return rounded.toString();
      }

      document.addEventListener("DOMContentLoaded", function () {
        const ozInput = document.getElementById("ozInput");
        const lbResult = document.getElementById("lbResult");
        const ozToLbFormulaText = document.getElementById("ozToLbFormulaText");

        const lbInput = document.getElementById("lbInput");
        const ozResult = document.getElementById("ozResult");
        const lbToOzFormulaText = document.getElementById("lbToOzFormulaText");

        const errorEl = document.getElementById("ozLbError");
        const swapToLbBtn = document.getElementById("swapToLbBtn");
        const swapToOzBtn = document.getElementById("swapToOzBtn");

        function showError(show) {
          if (show) {
            errorEl.classList.remove("hidden");
          } else {
            errorEl.classList.add("hidden");
          }
        }

        function updateOzToLb() {
          const raw = ozInput.value;
          if (!raw.trim()) {
            lbResult.textContent = "";
            ozToLbFormulaText.textContent = "";
            showError(false);
            return;
          }
          const oz = parseFlexibleNumber(raw);
          if (isNaN(oz)) {
            showError(true);
            return;
          }
          showError(false);
          const lb = oz / 16;
          lbResult.textContent = formatNumber(lb, 4) + " lb";
          ozToLbFormulaText.textContent = `${formatNumber(
            oz,
            4
          )} oz ÷ 16 = ${formatNumber(lb, 4)} lb`;
        }

        function updateLbToOz() {
          const raw = lbInput.value;
          if (!raw.trim()) {
            ozResult.textContent = "";
            lbToOzFormulaText.textContent = "";
            showError(false);
            return;
          }
          const lb = parseFlexibleNumber(raw);
          if (isNaN(lb)) {
            showError(true);
            return;
          }
          showError(false);
          const oz = lb * 16;
          ozResult.textContent = formatNumber(oz, 4) + " oz";
          lbToOzFormulaText.textContent = `${formatNumber(
            lb,
            4
          )} lb × 16 = ${formatNumber(oz, 4)} oz`;
        }

        ozInput.addEventListener("input", function () {
          updateOzToLb();
        });

        lbInput.addEventListener("input", function () {
          updateLbToOz();
        });

        swapToLbBtn.addEventListener("click", function () {
          const raw = ozInput.value;
          const val = parseFlexibleNumber(raw);
          if (!isNaN(val)) {
            const lb = val / 16;
            lbInput.value = formatNumber(lb, 4);
            updateLbToOz();
          }
        });

        swapToOzBtn.addEventListener("click", function () {
          const raw = lbInput.value;
          const val = parseFlexibleNumber(raw);
          if (!isNaN(val)) {
            const oz = val * 16;
            ozInput.value = formatNumber(oz, 4);
            updateOzToLb();
          }
        });

        // Quick buttons
        document.querySelectorAll("[data-quick-oz]").forEach((btn) => {
          btn.addEventListener("click", () => {
            const v = btn.getAttribute("data-quick-oz");
            ozInput.value = v;
            updateOzToLb();
          });
        });
        document.querySelectorAll("[data-quick-lb]").forEach((btn) => {
          btn.addEventListener("click", () => {
            const v = btn.getAttribute("data-quick-lb");
            lbInput.value = v;
            updateLbToOz();
          });
        });

        // Initialize with some defaults
        ozInput.value = "12";
        lbInput.value = "2";
        updateOzToLb();
        updateLbToOz();
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
