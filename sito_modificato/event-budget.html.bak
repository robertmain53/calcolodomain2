    <script>
      // Initial default rows
      const defaultRows = [
        {
          category: "Venue & rentals",
          item: "Venue rental",
          qty: 1,
          unitCost: 3000,
        },
        {
          category: "Venue & rentals",
          item: "Furniture / décor",
          qty: 1,
          unitCost: 800,
        },
        {
          category: "Catering & F&B",
          item: "Food & beverage",
          qty: 100,
          unitCost: 45,
        },
        {
          category: "Catering & F&B",
          item: "Service charges & gratuity",
          qty: 1,
          unitCost: 900,
        },
        {
          category: "AV & production",
          item: "Audio / visual package",
          qty: 1,
          unitCost: 2500,
        },
        {
          category: "Marketing & registration",
          item: "Marketing & ads",
          qty: 1,
          unitCost: 1200,
        },
        {
          category: "Speakers & entertainment",
          item: "Speaker fees",
          qty: 1,
          unitCost: 1500,
        },
        {
          category: "Staff & operations",
          item: "Event staff",
          qty: 1,
          unitCost: 1000,
        },
        {
          category: "Travel & lodging",
          item: "Travel & hotel",
          qty: 1,
          unitCost: 2000,
        },
        {
          category: "Contingency & misc.",
          item: "Contingency",
          qty: 1,
          unitCost: 1500,
        },
      ];

      const tbody = document.getElementById("budget-tbody");
      const attendeesInput = document.getElementById("attendees");

      function formatCurrency(value) {
        if (!isFinite(value)) return "$0";
        return (
          "$" +
          value.toLocaleString(undefined, {
            maximumFractionDigits: 2,
            minimumFractionDigits: 0,
          })
        );
      }

      function formatPercent(value) {
        if (!isFinite(value)) return "0%";
        return value.toFixed(1).replace(/\.0$/, "") + "%";
      }

      function createRow(data = {}) {
        const tr = document.createElement("tr");

        tr.innerHTML = `
      <td class="px-2 py-1.5">
        <input type="text" class="w-full border rounded px-1 py-1 text-xs md:text-sm category-input" value="${
          data.category || ""
        }">
      </td>
      <td class="px-2 py-1.5">
        <input type="text" class="w-full border rounded px-1 py-1 text-xs md:text-sm item-input" value="${
          data.item || ""
        }">
      </td>
      <td class="px-2 py-1.5 text-right">
        <input type="number" min="0" step="1" class="w-20 border rounded px-1 py-1 text-right text-xs md:text-sm table-input qty-input" value="${
          data.qty != null ? data.qty : ""
        }">
      </td>
      <td class="px-2 py-1.5 text-right">
        <input type="number" min="0" step="0.01" class="w-24 border rounded px-1 py-1 text-right text-xs md:text-sm table-input unit-cost-input" value="${
          data.unitCost != null ? data.unitCost : ""
        }">
      </td>
      <td class="px-2 py-1.5 text-right">
        <input type="number" min="0" step="0.01" class="w-24 border rounded px-1 py-1 text-right text-xs md:text-sm table-input est-total-input">
      </td>
      <td class="px-2 py-1.5 text-right">
        <input type="number" min="0" step="0.01" class="w-24 border rounded px-1 py-1 text-right text-xs md:text-sm table-input act-total-input">
      </td>
      <td class="px-2 py-1.5 text-right text-xs md:text-sm variance-cell text-gray-700">$0</td>
      <td class="px-2 py-1.5 text-center text-xs md:text-sm variance-percent-cell text-gray-700">0%</td>
      <td class="px-2 py-1.5 text-center">
        <button type="button" class="text-red-500 text-xs md:text-sm remove-row-btn">✕</button>
      </td>
    `;

        tbody.appendChild(tr);
        return tr;
      }

      function recalcRow(tr) {
        const qty = parseFloat(tr.querySelector(".qty-input").value) || 0;
        const unitCost =
          parseFloat(tr.querySelector(".unit-cost-input").value) || 0;
        const estInput = tr.querySelector(".est-total-input");
        const actInput = tr.querySelector(".act-total-input");
        let est = parseFloat(estInput.value);

        // If user hasn't manually set est, auto-calc from qty * unitCost
        if (!estInput.dataset.manual || estInput.value === "") {
          est = qty * unitCost;
          estInput.value = est ? est.toFixed(2) : "";
          estInput.dataset.manual = ""; // mark as auto unless user edits
        }

        const act = parseFloat(actInput.value) || 0;
        const variance = isNaN(est) ? 0 : act - est;
        const varianceCell = tr.querySelector(".variance-cell");
        const variancePercentCell = tr.querySelector(".variance-percent-cell");

        varianceCell.textContent = formatCurrency(variance);
        varianceCell.classList.toggle("text-red-600", variance < 0);
        varianceCell.classList.toggle("text-green-600", variance > 0);

        let variancePercent = 0;
        if (est > 0) {
          variancePercent = (variance / est) * 100;
        }
        variancePercentCell.textContent = formatPercent(variancePercent);
        variancePercentCell.classList.toggle("text-red-600", variance < 0);
        variancePercentCell.classList.toggle("text-green-600", variance > 0);
      }

      function recalcTotals() {
        let estTotal = 0;
        let actTotal = 0;

        tbody.querySelectorAll("tr").forEach((tr) => {
          const est =
            parseFloat(tr.querySelector(".est-total-input").value) || 0;
          const act =
            parseFloat(tr.querySelector(".act-total-input").value) || 0;
          estTotal += est;
          actTotal += act;
        });

        const varianceTotal = actTotal - estTotal;
        const variancePercentTotal =
          estTotal > 0 ? (varianceTotal / estTotal) * 100 : 0;

        document.getElementById("est-total-cell").textContent =
          formatCurrency(estTotal);
        document.getElementById("act-total-cell").textContent =
          formatCurrency(actTotal);
        const varCell = document.getElementById("variance-total-cell");
        varCell.textContent = formatCurrency(varianceTotal);
        varCell.classList.toggle("text-red-600", varianceTotal < 0);
        varCell.classList.toggle("text-green-600", varianceTotal > 0);
        document.getElementById("variance-percent-total-cell").textContent =
          formatPercent(variancePercentTotal);

        // Summary
        document.getElementById("summary-est-total").textContent =
          formatCurrency(estTotal);
        document.getElementById("summary-act-total").textContent =
          formatCurrency(actTotal);
        const summaryVar = document.getElementById("summary-variance");
        summaryVar.textContent = formatCurrency(varianceTotal);
        summaryVar.classList.toggle("text-red-600", varianceTotal < 0);
        summaryVar.classList.toggle("text-green-600", varianceTotal > 0);
        document.getElementById("summary-variance-percent").textContent =
          formatPercent(variancePercentTotal);

        const attendees = parseFloat(attendeesInput.value) || 0;
        const estPer = attendees > 0 ? estTotal / attendees : 0;
        const actPer = attendees > 0 ? actTotal / attendees : 0;
        document.getElementById("summary-est-per-attendee").textContent =
          formatCurrency(estPer);
        document.getElementById("summary-act-per-attendee").textContent =
          formatCurrency(actPer);
        document.getElementById("summary-break-even").textContent =
          formatCurrency(estPer);
      }

      function recalcAll() {
        tbody.querySelectorAll("tr").forEach((tr) => recalcRow(tr));
        recalcTotals();
      }

      function initTable() {
        tbody.innerHTML = "";
        defaultRows.forEach((row) => {
          const tr = createRow(row);
          recalcRow(tr);
        });
        recalcTotals();
      }

      document.getElementById("add-row-btn").addEventListener("click", () => {
        const tr = createRow({ category: "Custom", item: "" });
        recalcRow(tr);
        recalcTotals();
      });

      document
        .getElementById("reset-table-btn")
        .addEventListener("click", () => {
          if (confirm("Reset table to default event budget lines?")) {
            initTable();
          }
        });

      tbody.addEventListener("input", (e) => {
        const tr = e.target.closest("tr");
        if (!tr) return;

        if (e.target.classList.contains("est-total-input")) {
          // Mark as manually overridden
          e.target.dataset.manual = "true";
        }

        recalcRow(tr);
        recalcTotals();
      });

      tbody.addEventListener("click", (e) => {
        if (e.target.classList.contains("remove-row-btn")) {
          const tr = e.target.closest("tr");
          tr.remove();
          recalcTotals();
        }
      });

      attendeesInput.addEventListener("input", recalcTotals);

      // FAQ toggle (simple, in addition to global script)
      document
        .querySelectorAll("#faq-container .faq-question")
        .forEach((btn) => {
          btn.addEventListener("click", () => {
            const answer = btn.parentElement.querySelector(".faq-answer");
            const icon = btn.querySelector("span:last-child");
            const isHidden = answer.classList.contains("hidden");
            answer.classList.toggle("hidden", !isHidden);
            icon.textContent = isHidden ? "−" : "+";
          });
        });

      // Initialize
      initTable();

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
