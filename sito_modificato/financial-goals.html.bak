    <script>
      // Financial Goals Planner Logic
      (function () {
        const goals = [];
        const tbody = document.getElementById("goals-tbody");
        const addBtn = document.getElementById("add-goal-btn");
        const resetBtn = document.getElementById("reset-goals-btn");
        const totalMonthlyInput = document.getElementById(
          "total-monthly-saving"
        );
        const requiredMonthlyEl = document.getElementById(
          "required-monthly-saving"
        );
        const savingGapEl = document.getElementById("saving-gap");
        const allocationNote = document.getElementById("allocation-note");

        function parseNumber(input) {
          const v = parseFloat(input);
          return isNaN(v) ? 0 : v;
        }

        function monthsBetween(today, targetDate) {
          const msPerDay = 1000 * 60 * 60 * 24;
          const diffDays = (targetDate - today) / msPerDay;
          const months = Math.round(diffDays / 30);
          return Math.max(1, months);
        }

        function computeGoalMetrics(goal) {
          const today = new Date();
          let monthsLeft = 12;
          if (goal.deadline) {
            const d = new Date(goal.deadline);
            if (!isNaN(d.getTime())) {
              monthsLeft = monthsBetween(today, d);
            }
          }
          const remaining = Math.max(0, goal.target - goal.current);
          let requiredMonthly;

          if (goal.annualReturn > 0 && monthsLeft > 1) {
            const r = goal.annualReturn / 100;
            const i = Math.pow(1 + r, 1 / 12) - 1;
            if (i > 0) {
              const factor = (Math.pow(1 + i, monthsLeft) - 1) / i;
              requiredMonthly = remaining / factor;
            } else {
              requiredMonthly = remaining / monthsLeft;
            }
          } else {
            requiredMonthly = remaining / monthsLeft;
          }

          return {
            monthsLeft,
            remaining,
            requiredMonthly: isFinite(requiredMonthly) ? requiredMonthly : 0,
          };
        }

        function priorityWeight(p) {
          if (p === 1 || p === "1" || p === "High") return 1;
          if (p === 2 || p === "2" || p === "Medium") return 2;
          return 3;
        }

        function render() {
          tbody.innerHTML = "";
          if (goals.length === 0) {
            tbody.innerHTML =
              '<tr><td colspan="8" class="px-3 py-4 text-center text-gray-500 text-sm">No goals yet. Add your first goal above to get started.</td></tr>';
            requiredMonthlyEl.textContent = "$0";
            savingGapEl.textContent = "$0";
            allocationNote.classList.add("hidden");
            return;
          }

          // Compute metrics for each goal
          goals.forEach((g) => {
            const m = computeGoalMetrics(g);
            g.monthsLeft = m.monthsLeft;
            g.remaining = m.remaining;
            g.requiredMonthly = m.requiredMonthly;
          });

          // Sort by priority then deadline
          goals.sort((a, b) => {
            const pa = priorityWeight(a.priority);
            const pb = priorityWeight(b.priority);
            if (pa !== pb) return pa - pb;
            if (a.deadline && b.deadline) {
              return new Date(a.deadline) - new Date(b.deadline);
            }
            if (a.deadline && !b.deadline) return -1;
            if (!a.deadline && b.deadline) return 1;
            return 0;
          });

          const totalRequired = goals.reduce(
            (sum, g) => sum + g.requiredMonthly,
            0
          );
          const budget = parseNumber(totalMonthlyInput.value);
          let remainingBudget = budget;

          requiredMonthlyEl.textContent = "$" + totalRequired.toFixed(2);
          const gap = budget - totalRequired;
          const gapStr =
            (gap >= 0 ? "+" : "−") + "$" + Math.abs(gap).toFixed(2);
          savingGapEl.textContent = gapStr;

          allocationNote.classList.remove("hidden");

          goals.forEach((g, index) => {
            const needed = g.requiredMonthly;
            let allocated = 0;
            let status = "Off track";
            let statusClass = "bg-red-100 text-red-700 border-red-200";

            if (remainingBudget > 0 && needed > 0) {
              if (remainingBudget >= needed) {
                allocated = needed;
                remainingBudget -= needed;
                status = "On track";
                statusClass = "bg-green-100 text-green-700 border-green-200";
              } else {
                allocated = remainingBudget;
                remainingBudget = 0;
                status = "Tight";
                statusClass = "bg-yellow-100 text-yellow-700 border-yellow-200";
              }
            }

            g.allocatedMonthly = allocated;
            g.status = status;

            const tr = document.createElement("tr");
            tr.className = index % 2 === 0 ? "bg-white" : "bg-gray-50";

            const priorityLabel =
              g.priority === 1 || g.priority === "1"
                ? "High"
                : g.priority === 2 || g.priority === "2"
                ? "Medium"
                : "Low";

            const typeLabel =
              g.type === "short"
                ? "Short"
                : g.type === "medium"
                ? "Medium"
                : "Long";

            tr.innerHTML = `
          <td class="px-3 py-2 align-top">
            <div class="font-medium text-gray-800">${
              g.name || "Untitled goal"
            }</div>
            <div class="text-[11px] text-gray-500">
              ${g.category ? `<span>${g.category} · </span>` : ""}
              <span>Priority: ${priorityLabel}</span>
              <span> · ${typeLabel}-term</span>
              ${
                g.deadline
                  ? ` · Target: ${new Date(g.deadline).toLocaleDateString()}`
                  : ""
              }
            </div>
          </td>
          <td class="px-3 py-2 text-right align-top">$${g.target.toFixed(
            2
          )}</td>
          <td class="px-3 py-2 text-right align-top">$${g.current.toFixed(
            2
          )}</td>
          <td class="px-3 py-2 text-right align-top">${g.monthsLeft}</td>
          <td class="px-3 py-2 text-right align-top">$${g.requiredMonthly.toFixed(
            2
          )}</td>
          <td class="px-3 py-2 text-right align-top">$${allocated.toFixed(
            2
          )}</td>
          <td class="px-3 py-2 text-center align-top">
            <span class="badge border ${statusClass}">${status}</span>
          </td>
          <td class="px-3 py-2 text-center align-top">
            <button type="button" data-index="${index}" class="text-xs text-red-600 hover:underline remove-goal-btn">Remove</button>
          </td>
        `;
            tbody.appendChild(tr);
          });

          // Attach remove handlers
          tbody.querySelectorAll(".remove-goal-btn").forEach((btn) => {
            btn.addEventListener("click", () => {
              const idx = parseInt(btn.getAttribute("data-index"), 10);
              if (!isNaN(idx)) {
                goals.splice(idx, 1);
                render();
              }
            });
          });
        }

        addBtn.addEventListener("click", () => {
          const name = document.getElementById("goal-name").value.trim();
          const category = document.getElementById("goal-category").value;
          const target = parseNumber(
            document.getElementById("goal-target").value
          );
          const current = parseNumber(
            document.getElementById("goal-current").value
          );
          const deadline = document.getElementById("goal-deadline").value;
          const priority = document.getElementById("goal-priority").value;
          const type = document.getElementById("goal-type").value;
          const annualReturn = parseNumber(
            document.getElementById("goal-return").value
          );

          if (!target || target <= 0) {
            alert("Please enter a positive target amount for your goal.");
            return;
          }
          if (current < 0) {
            alert("Current saved cannot be negative.");
            return;
          }
          if (current > target) {
            alert(
              "Current saved is greater than the target amount. Adjust the values and try again."
            );
            return;
          }

          goals.push({
            name: name || "Untitled goal",
            category,
            target,
            current,
            deadline: deadline || null,
            priority,
            type,
            annualReturn,
          });

          // Clear minimal fields for next entry
          document.getElementById("goal-name").value = "";
          document.getElementById("goal-target").value = "";
          document.getElementById("goal-current").value = "";
          document.getElementById("goal-deadline").value = "";

          render();
        });

        resetBtn.addEventListener("click", () => {
          if (goals.length === 0) return;
          if (confirm("Remove all goals from the planner?")) {
            goals.splice(0, goals.length);
            render();
          }
        });

        totalMonthlyInput.addEventListener("input", render);
      })();
    </script>

    <script>
      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
