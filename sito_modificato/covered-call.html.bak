    <script>
      // Core covered call calculations
      function fmt(num, decimals = 2, prefix = "$") {
        if (!isFinite(num)) return prefix + "0.00";
        const sign = num < 0 ? "-" : "";
        const abs = Math.abs(num);
        return sign + prefix + abs.toFixed(decimals);
      }

      function pct(num, decimals = 2) {
        if (!isFinite(num)) return "0%";
        const sign = num < 0 ? "-" : "";
        const abs = Math.abs(num) * 100;
        return sign + abs.toFixed(decimals) + "%";
      }

      function getInputs() {
        const stockPrice =
          parseFloat(document.getElementById("stockPrice").value) || 0;
        const entryPrice =
          parseFloat(document.getElementById("entryPrice").value) || 0;
        const strikePrice =
          parseFloat(document.getElementById("strikePrice").value) || 0;
        const premium =
          parseFloat(document.getElementById("premium").value) || 0;
        const contracts = Math.max(
          1,
          parseInt(document.getElementById("contracts").value) || 1
        );
        const daysToExpiry = Math.max(
          1,
          parseInt(document.getElementById("daysToExpiry").value) || 1
        );
        const expiryPrice =
          parseFloat(document.getElementById("expiryPrice").value) ||
          stockPrice;

        const shares = contracts * 100;
        const costBasisPerShare = entryPrice > 0 ? entryPrice : stockPrice;
        const totalCost = shares * costBasisPerShare;
        const totalPremium = shares * premium;
        const effectiveCostPerShare = costBasisPerShare - premium;
        const maxProfitTotal =
          (strikePrice - costBasisPerShare) * shares + totalPremium;
        const maxLossTotal = -effectiveCostPerShare * shares;

        return {
          stockPrice,
          entryPrice: costBasisPerShare,
          strikePrice,
          premium,
          contracts,
          daysToExpiry,
          expiryPrice,
          shares,
          totalCost,
          totalPremium,
          effectiveCostPerShare,
          maxProfitTotal,
          maxLossTotal,
        };
      }

      function coveredCallPL(inputs, ST) {
        const { entryPrice, strikePrice, premium, shares } = inputs;
        const stockPL = (ST - entryPrice) * shares;
        const totalPremium = premium * shares;
        let ccPL;
        if (ST <= strikePrice) {
          ccPL = stockPL + totalPremium;
        } else {
          ccPL = (strikePrice - entryPrice) * shares + totalPremium;
        }
        return { stockPL, ccPL };
      }

      function annualizeROI(roi, days) {
        if (days <= 0) return 0;
        return Math.pow(1 + roi, 365 / days) - 1;
      }

      function updateCalculator() {
        const inputs = getInputs();

        // Update slider label
        const expiryPriceInput = document.getElementById("expiryPrice");
        document.getElementById("expiryPriceLabel").textContent = fmt(
          inputs.expiryPrice
        );

        // Premium & breakeven
        document.getElementById("totalPremium").textContent = fmt(
          inputs.totalPremium
        );
        const premiumYield =
          inputs.totalCost > 0 ? inputs.totalPremium / inputs.totalCost : 0;
        document.getElementById("premiumYield").textContent =
          pct(premiumYield) + " over cost basis";

        document.getElementById("breakeven").textContent = fmt(
          inputs.effectiveCostPerShare
        );

        const maxProfitRoi =
          inputs.totalCost > 0 ? inputs.maxProfitTotal / inputs.totalCost : 0;
        document.getElementById("maxProfit").textContent = fmt(
          inputs.maxProfitTotal
        );
        document.getElementById("maxProfitRoi").textContent =
          pct(maxProfitRoi) + " over cost basis";

        // Key metrics
        document.getElementById("sharesOwned").textContent =
          inputs.shares.toString();
        document.getElementById("totalCost").textContent = fmt(
          inputs.totalCost
        );
        document.getElementById("effectiveCost").textContent = fmt(
          inputs.effectiveCostPerShare * inputs.shares
        );
        document.getElementById("maxLoss").textContent = fmt(
          inputs.maxLossTotal
        );

        // Profit at chosen expiry price
        const { stockPL, ccPL } = coveredCallPL(inputs, inputs.expiryPrice);
        const roi = inputs.totalCost > 0 ? ccPL / inputs.totalCost : 0;
        const annualized = annualizeROI(roi, inputs.daysToExpiry);

        document.getElementById("profitAtExpiry").textContent = fmt(ccPL);
        document.getElementById("roiAtExpiry").textContent =
          pct(roi) + " return over holding period";
        document.getElementById("annualizedRoiAtExpiry").textContent =
          "Annualized: " + pct(annualized);

        const scenarioLabel = document.getElementById("scenarioLabel");
        if (inputs.expiryPrice >= inputs.strikePrice) {
          scenarioLabel.textContent = "Assigned";
          scenarioLabel.className =
            "text-xs pill px-2 py-0.5 bg-amber-100 text-amber-800";
        } else {
          scenarioLabel.textContent = "Unassigned";
          scenarioLabel.className =
            "text-xs pill px-2 py-0.5 bg-green-100 text-green-800";
        }

        // Scenario table
        const scenarios = [
          { label: "Stock −20%", price: inputs.entryPrice * 0.8 },
          { label: "Stock −10%", price: inputs.entryPrice * 0.9 },
          { label: "At breakeven", price: inputs.effectiveCostPerShare },
          { label: "At current price", price: inputs.stockPrice },
          { label: "At strike", price: inputs.strikePrice },
          { label: "Stock +10%", price: inputs.entryPrice * 1.1 },
          { label: "Stock +20%", price: inputs.entryPrice * 1.2 },
        ];

        const tbody = document.getElementById("scenarioTableBody");
        tbody.innerHTML = "";
        scenarios.forEach((s) => {
          const { stockPL: sStockPL, ccPL: sCcPL } = coveredCallPL(
            inputs,
            s.price
          );
          const diff = sCcPL - sStockPL;
          const tr = document.createElement("tr");

          tr.innerHTML = `
        <td class="whitespace-nowrap">${s.label}</td>
        <td class="text-right whitespace-nowrap">${fmt(s.price)}</td>
        <td class="text-right whitespace-nowrap ${
          sCcPL >= 0 ? "text-green-700" : "text-red-600"
        }">${fmt(sCcPL)}</td>
        <td class="text-right whitespace-nowrap ${
          sStockPL >= 0 ? "text-green-700" : "text-red-600"
        }">${fmt(sStockPL)}</td>
        <td class="text-right whitespace-nowrap ${
          diff >= 0 ? "text-green-700" : "text-red-600"
        }">${fmt(diff)}</td>
      `;
          tbody.appendChild(tr);
        });
      }

      [
        "stockPrice",
        "entryPrice",
        "strikePrice",
        "premium",
        "contracts",
        "daysToExpiry",
        "expiryPrice",
      ].forEach((id) => {
        document.getElementById(id).addEventListener("input", updateCalculator);
        document
          .getElementById(id)
          .addEventListener("change", updateCalculator);
      });

      document.addEventListener("DOMContentLoaded", updateCalculator);
    </script>

