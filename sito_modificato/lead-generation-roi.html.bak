    <script>
      function formatCurrency(value) {
        if (!isFinite(value)) return "$0.00";
        const abs = Math.abs(value);
        if (abs >= 1e9) return (value / 1e9).toFixed(2) + "B";
        if (abs >= 1e6) return (value / 1e6).toFixed(2) + "M";
        return (
          "$" +
          value.toLocaleString(undefined, {
            minimumFractionDigits: 2,
            maximumFractionDigits: 2,
          })
        );
      }

      function formatPercent(value) {
        if (!isFinite(value)) return "0%";
        return (
          value.toLocaleString(undefined, {
            minimumFractionDigits: 0,
            maximumFractionDigits: 1,
          }) + "%"
        );
      }

      function formatMultiplier(value) {
        if (!isFinite(value)) return "0×";
        return (
          value.toLocaleString(undefined, {
            minimumFractionDigits: 1,
            maximumFractionDigits: 1,
          }) + "×"
        );
      }

      function recalcLeadGenROI() {
        const cost =
          parseFloat(document.getElementById("total-cost").value) || 0;
        const leads = parseFloat(document.getElementById("leads").value) || 0;
        const l2o =
          parseFloat(document.getElementById("lead-to-opportunity").value) || 0;
        const closeRate =
          parseFloat(document.getElementById("close-rate").value) || 0;
        const valuePerCustomer =
          parseFloat(document.getElementById("value-per-customer").value) || 0;
        const valueType = document.getElementById("value-type").value;

        const opportunities = leads * (l2o / 100);
        const customers = opportunities * (closeRate / 100);

        const revenue = customers * valuePerCustomer;
        const profit = revenue - cost;
        const cpl = leads > 0 ? cost / leads : 0;
        const cpa = customers > 0 ? cost / customers : 0;
        const roi = cost > 0 ? (profit / cost) * 100 : 0;
        const roas = cost > 0 ? revenue / cost : 0;

        document.getElementById("out-opportunities").textContent =
          opportunities.toLocaleString(undefined, { maximumFractionDigits: 1 });
        document.getElementById("out-customers").textContent =
          customers.toLocaleString(undefined, { maximumFractionDigits: 1 });
        document.getElementById("out-cpl").textContent = formatCurrency(cpl);
        document.getElementById("out-cpa").textContent = formatCurrency(cpa);
        document.getElementById("out-revenue").textContent =
          formatCurrency(revenue);
        document.getElementById("out-profit").textContent =
          formatCurrency(profit);
        document.getElementById("out-roi").textContent = formatPercent(roi);
        document.getElementById("out-roas").textContent =
          formatMultiplier(roas);

        const perDollarRevenue = cost > 0 ? revenue / cost : 0;
        const perDollarProfit = cost > 0 ? profit / cost : 0;
        const label = valueType === "profit" ? "profit" : "revenue";
        const labelProfit =
          valueType === "profit"
            ? "net profit"
            : "profit (before delivery costs)";

        document.getElementById("out-interpretation").textContent =
          cost <= 0 || revenue <= 0
            ? "Enter your campaign cost, leads, conversion rates, and value per customer to see ROI and unit economics."
            : `For every $1.00 you invest in this campaign, you generate ${perDollarRevenue.toFixed(
                2
              )} in ${label} and ${perDollarProfit.toFixed(
                2
              )} in ${labelProfit}.`;
      }

      document.addEventListener("DOMContentLoaded", function () {
        const inputs = [
          "total-cost",
          "leads",
          "lead-to-opportunity",
          "close-rate",
          "value-per-customer",
          "value-type",
          "time-horizon",
        ];

        inputs.forEach((id) => {
          const el = document.getElementById(id);
          if (el) {
            el.addEventListener("input", recalcLeadGenROI);
            el.addEventListener("change", recalcLeadGenROI);
          }
        });

        document
          .getElementById("value-type")
          .addEventListener("change", function () {
            const labelSpan = document.getElementById("value-label");
            labelSpan.textContent =
              this.value === "profit" ? "profit" : "revenue";
            recalcLeadGenROI();
          });

        document
          .getElementById("reset-btn")
          .addEventListener("click", function () {
            document.getElementById("total-cost").value = 5000;
            document.getElementById("leads").value = 200;
            document.getElementById("lead-to-opportunity").value = 30;
            document.getElementById("close-rate").value = 25;
            document.getElementById("value-type").value = "revenue";
            document.getElementById("value-label").textContent = "revenue";
            document.getElementById("value-per-customer").value = 3000;
            document.getElementById("time-horizon").value = "campaign";
            recalcLeadGenROI();
          });

        recalcLeadGenROI();
      });
    </script>

