    <script>
      // Unit system handling
      const state = {
        system: "si", // 'si' or 'us'
      };

      const els = {
        btnSi: document.getElementById("btn-si"),
        btnUs: document.getElementById("btn-us"),
        bedLengthUnit: document.getElementById("bed-length-unit"),
        particleDiameterUnit: document.getElementById("particle-diameter-unit"),
        areaUnit: document.getElementById("area-unit"),
        densityUnit: document.getElementById("density-unit"),
        viscosityUnit: document.getElementById("viscosity-unit"),
        flowRateUnit: document.getElementById("flow-rate-unit"),
        velocityUnit: document.getElementById("velocity-unit"),
        bedLength: document.getElementById("bed-length"),
        particleDiameter: document.getElementById("particle-diameter"),
        area: document.getElementById("area"),
        density: document.getElementById("density"),
        viscosity: document.getElementById("viscosity"),
        flowRate: document.getElementById("flow-rate"),
        velocity: document.getElementById("velocity"),
        btnCalc: document.getElementById("btn-calc"),
        btnReset: document.getElementById("btn-reset"),
        warning: document.getElementById("input-warning"),
        resDp: document.getElementById("res-dp"),
        resDpAlt: document.getElementById("res-dp-alt"),
        resDpDx: document.getElementById("res-dpdx"),
        resV: document.getElementById("res-v"),
        resRe: document.getElementById("res-re"),
        resRegime: document.getElementById("res-regime"),
        resF: document.getElementById("res-f"),
      };

      function setUnitSystem(system) {
        state.system = system;
        if (system === "si") {
          els.btnSi.classList.add("active");
          els.btnUs.classList.remove("active");
          els.bedLengthUnit.textContent = "m";
          els.particleDiameterUnit.textContent = "m";
          els.areaUnit.textContent = "m²";
          els.densityUnit.textContent = "kg/m³";
          els.viscosityUnit.textContent = "Pa·s";
          els.flowRateUnit.textContent = "m³/s";
          els.velocityUnit.textContent = "m/s";
          // Set typical SI defaults
          if (!els.bedLength.value) els.bedLength.value = 1;
          if (!els.particleDiameter.value) els.particleDiameter.value = 0.01;
          if (!els.area.value) els.area.value = 0.1;
          if (!els.density.value) els.density.value = 1000;
          if (!els.viscosity.value) els.viscosity.value = 0.001;
          if (!els.flowRate.value) els.flowRate.value = 0.01;
        } else {
          els.btnUs.classList.add("active");
          els.btnSi.classList.remove("active");
          els.bedLengthUnit.textContent = "ft";
          els.particleDiameterUnit.textContent = "ft";
          els.areaUnit.textContent = "ft²";
          els.densityUnit.textContent = "lb/ft³";
          els.viscosityUnit.textContent = "cP";
          els.flowRateUnit.textContent = "ft³/s";
          els.velocityUnit.textContent = "ft/s";
          // Set typical US defaults
          if (!els.bedLength.value) els.bedLength.value = 3;
          if (!els.particleDiameter.value) els.particleDiameter.value = 0.033; // ~1 inch
          if (!els.area.value) els.area.value = 1;
          if (!els.density.value) els.density.value = 62.4; // water
          if (!els.viscosity.value) els.viscosity.value = 1; // 1 cP
          if (!els.flowRate.value) els.flowRate.value = 0.5;
        }
      }

      els.btnSi.addEventListener("click", () => setUnitSystem("si"));
      els.btnUs.addEventListener("click", () => setUnitSystem("us"));

      // Initial setup
      setUnitSystem("si");

      function resetResults() {
        els.resDp.textContent = "–";
        els.resDpAlt.textContent = "";
        els.resDpDx.textContent = "–";
        els.resV.textContent = "–";
        els.resRe.textContent = "–";
        els.resRegime.textContent = "–";
        els.resF.textContent = "–";
      }

      function resetForm() {
        document.getElementById("ergun-form").reset();
        els.porosity.value = 0.4;
        resetResults();
        els.warning.classList.add("hidden");
        setUnitSystem(state.system);
      }

      els.btnReset.addEventListener("click", resetForm);

      function calculateErgun() {
        els.warning.classList.add("hidden");
        resetResults();

        const L = parseFloat(els.bedLength.value);
        const dp = parseFloat(els.particleDiameter.value);
        const eps = parseFloat(els.porosity.value);
        const A = parseFloat(els.area.value);
        let rho = parseFloat(els.density.value);
        let mu = parseFloat(els.viscosity.value);
        let Q = parseFloat(els.flowRate.value);
        let v = parseFloat(els.velocity.value);

        if (
          !(L > 0 && dp > 0 && eps > 0 && eps < 1 && A > 0 && rho > 0 && mu > 0)
        ) {
          els.warning.textContent =
            "Please provide positive values for all required inputs and porosity between 0 and 1.";
          els.warning.classList.remove("hidden");
          return;
        }

        // Convert US inputs to SI for core calculation
        if (state.system === "us") {
          // Length ft -> m
          const ftToM = 0.3048;
          const ft2ToM2 = ftToM * ftToM;
          const ft3ToM3 = ftToM * ftToM * ftToM;
          const lbft3ToKgm3 = 16.018463;
          const cPToPas = 0.001;

          // Convert
          const L_si = L * ftToM;
          const dp_si = dp * ftToM;
          const A_si = A * ft2ToM2;
          const rho_si = rho * lbft3ToKgm3;
          const mu_si = mu * cPToPas;
          let Q_si = isNaN(Q) ? NaN : Q * ft3ToM3;
          let v_si = isNaN(v) ? NaN : v * ftToM;

          // Compute missing v or Q
          if (isNaN(v_si) && !isNaN(Q_si) && Q_si > 0 && A_si > 0) {
            v_si = Q_si / A_si;
            els.velocity.value = (v_si / ftToM).toPrecision(4);
          } else if (isNaN(Q_si) && !isNaN(v_si) && v_si > 0 && A_si > 0) {
            Q_si = v_si * A_si;
            els.flowRate.value = (Q_si / ft3ToM3).toPrecision(4);
          }

          if (!(v_si > 0)) {
            els.warning.textContent =
              "Please enter a positive volumetric flow rate or superficial velocity.";
            els.warning.classList.remove("hidden");
            return;
          }

          // Ergun equation in SI
          const term1 =
            ((150 * Math.pow(1 - eps, 2)) / Math.pow(eps, 3)) *
            ((mu_si * v_si) / (dp_si * dp_si));
          const term2 =
            ((1.75 * (1 - eps)) / Math.pow(eps, 3)) *
            ((rho_si * v_si * v_si) / dp_si);
          const dPdx = term1 + term2; // Pa/m
          const dP = dPdx * L_si; // Pa

          // Reynolds number (particle-based)
          const Re_p = (rho_si * v_si * dp_si) / (mu_si * (1 - eps));
          const f = 0.5 * (150 / Re_p + 1.75);

          // Convert results back to US
          const PaToPsi = 0.0001450377;
          const mToFt = 1 / ftToM;
          const dP_psi = dP * PaToPsi;
          const dPdx_psiPerFt = (dPdx * PaToPsi) / mToFt;

          els.resDp.textContent = dP.toExponential(3) + " Pa";
          els.resDpAlt.textContent = dP_psi.toFixed(4) + " psi";
          els.resDpDx.textContent =
            dPdx.toExponential(3) +
            " Pa/m  (" +
            dPdx_psiPerFt.toFixed(4) +
            " psi/ft)";
          els.resV.textContent =
            v_si.toPrecision(4) +
            " m/s  (" +
            (v_si * mToFt).toPrecision(4) +
            " ft/s)";
          els.resRe.textContent = Re_p.toPrecision(4);
          let regime = "Transitional";
          if (Re_p < 10) regime = "Laminar-dominated";
          else if (Re_p > 1000) regime = "Turbulent-dominated";
          els.resRegime.textContent = regime;
          els.resF.textContent = f.toPrecision(4);
        } else {
          // SI system directly
          // Compute missing v or Q
          if (!(v > 0) && Q > 0 && A > 0) {
            v = Q / A;
            els.velocity.value = v.toPrecision(4);
          } else if (!(Q > 0) && v > 0 && A > 0) {
            Q = v * A;
            els.flowRate.value = Q.toPrecision(4);
          }

          if (!(v > 0)) {
            els.warning.textContent =
              "Please enter a positive volumetric flow rate or superficial velocity.";
            els.warning.classList.remove("hidden");
            return;
          }

          const term1 =
            ((150 * Math.pow(1 - eps, 2)) / Math.pow(eps, 3)) *
            ((mu * v) / (dp * dp));
          const term2 =
            ((1.75 * (1 - eps)) / Math.pow(eps, 3)) * ((rho * v * v) / dp);
          const dPdx = term1 + term2; // Pa/m
          const dP = dPdx * L; // Pa

          const Re_p = (rho * v * dp) / (mu * (1 - eps));
          const f = 0.5 * (150 / Re_p + 1.75);

          const PaToBar = 1e-5;
          const PaToPsi = 0.0001450377;

          els.resDp.textContent = dP.toExponential(3) + " Pa";
          els.resDpAlt.textContent =
            (dP * PaToBar).toPrecision(3) +
            " bar  (" +
            (dP * PaToPsi).toPrecision(3) +
            " psi)";
          els.resDpDx.textContent = dPdx.toExponential(3) + " Pa/m";
          els.resV.textContent = v.toPrecision(4) + " m/s";
          els.resRe.textContent = Re_p.toPrecision(4);
          let regime = "Transitional";
          if (Re_p < 10) regime = "Laminar-dominated";
          else if (Re_p > 1000) regime = "Turbulent-dominated";
          els.resRegime.textContent = regime;
          els.resF.textContent = f.toPrecision(4);
        }
      }

      els.btnCalc.addEventListener("click", calculateErgun);

      // Auto-update v when Q or A changes (and vice versa) for better UX
      function syncVelocityFromQ() {
        const Q = parseFloat(els.flowRate.value);
        const A = parseFloat(els.area.value);
        if (Q > 0 && A > 0) {
          const v = Q / A;
          els.velocity.value = v.toPrecision(4);
        }
      }

      function syncQFromVelocity() {
        const v = parseFloat(els.velocity.value);
        const A = parseFloat(els.area.value);
        if (v > 0 && A > 0) {
          const Q = v * A;
          els.flowRate.value = Q.toPrecision(4);
        }
      }

      els.flowRate.addEventListener("change", syncVelocityFromQ);
      els.area.addEventListener("change", () => {
        // Recompute whichever of Q or v is filled
        if (els.flowRate.value) syncVelocityFromQ();
        else if (els.velocity.value) syncQFromVelocity();
      });
      els.velocity.addEventListener("change", syncQFromVelocity);
    </script>

