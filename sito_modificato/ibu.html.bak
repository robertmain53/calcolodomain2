<script>
  // IBU calculator logic
  (function() {
    const hopTableBody = document.getElementById('hopTableBody');
    const addHopRowBtn = document.getElementById('addHopRowBtn');
    const resetBtn = document.getElementById('resetIbuBtn');
    const formulaSelect = document.getElementById('formulaSelect');
    const batchVolumeInput = document.getElementById('batchVolume');
    const volumeUnitSelect = document.getElementById('volumeUnit');
    const wortGravityInput = document.getElementById('wortGravity');
    const totalIbuEl = document.getElementById('totalIbu');
    const avgUtilEl = document.getElementById('avgUtilization');
    const buGuEl = document.getElementById('buGu');

    function createHopRow(defaults = {}) {
      const tr = document.createElement('tr');
      tr.className = 'border-t';

      tr.innerHTML = `
        <td class="px-2 py-2">
          <input type="text" class="w-full border border-gray-300 rounded-md px-2 py-1 text-xs" placeholder="e.g. Citra" value="${defaults.name || ''}">
        </td>
        <td class="px-2 py-2">
          <input type="number" min="0" step="0.1" class="input-small w-full border border-gray-300 rounded-md px-2 py-1 text-xs ibu-aa" value="${defaults.aa || 10}">
        </td>
        <td class="px-2 py-2">
          <div class="flex">
            <input type="number" min="0" step="0.1" class="input-small w-full border border-gray-300 rounded-l-md px-2 py-1 text-xs ibu-weight" value="${defaults.weight || 20}">
            <select class="border border-l-0 border-gray-300 rounded-r-md px-1 text-xs bg-gray-50 ibu-weight-unit">
              <option value="g"${defaults.unit === 'g' || !defaults.unit ? ' selected' : ''}>g</option>
              <option value="oz"${defaults.unit === 'oz' ? ' selected' : ''}>oz</option>
            </select>
          </div>
        </td>
        <td class="px-2 py-2">
          <input type="number" min="0" step="1" class="input-small w-full border border-gray-300 rounded-md px-2 py-1 text-xs ibu-time" value="${defaults.time || 60}">
        </td>
        <td class="px-2 py-2">
          <select class="w-full border border-gray-300 rounded-md px-2 py-1 text-xs ibu-type">
            <option value="boil"${defaults.type === 'boil' || !defaults.type ? ' selected' : ''}>Boil</option>
            <option value="whirlpool"${defaults.type === 'whirlpool' ? ' selected' : ''}>Whirlpool</option>
            <option value="dry"${defaults.type === 'dry' ? ' selected' : ''}>Dry hop (0 IBU)</option>
          </select>
        </td>
        <td class="px-2 py-2 text-right text-xs text-gray-700 ibu-cell">0.0</td>
        <td class="px-2 py-2 text-center">
          <button type="button" class="text-xs text-red-600 hover:underline ibu-remove">Remove</button>
        </td>
      `;

      hopTableBody.appendChild(tr);
    }

    function getVolumeLiters() {
      const vol = parseFloat(batchVolumeInput.value) || 0;
      const unit = volumeUnitSelect.value;
      return unit === 'gal' ? vol * 3.78541 : vol;
    }

    function tinsethUtilization(timeMin, gravity) {
      if (timeMin <= 0) return 0;
      const bigness = 1.65 * Math.pow(0.000125, (gravity - 1));
      const boilFactor = (1 - Math.exp(-0.04 * timeMin)) / 4.15;
      return bigness * boilFactor; // fraction
    }

    function ragerUtilization(timeMin) {
      if (timeMin <= 0) return 0;
      return 18.11 + 13.86 * Math.tanh((timeMin - 31.32) / 18.27); // percent
    }

    function recalc() {
      const formula = formulaSelect.value;
      const gravity = parseFloat(wortGravityInput.value) || 1.050;
      const volL = getVolumeLiters();
      const volGal = volL / 3.78541;

      let totalIbu = 0;
      let totalIsoMass = 0; // for avg utilization weighting
      let totalPotentialIsoMass = 0;

      hopTableBody.querySelectorAll('tr').forEach(row => {
        const aa = parseFloat(row.querySelector('.ibu-aa').value) || 0;
        const weight = parseFloat(row.querySelector('.ibu-weight').value) || 0;
        const unit = row.querySelector('.ibu-weight-unit').value;
        const time = parseFloat(row.querySelector('.ibu-time').value) || 0;
        const type = row.querySelector('.ibu-type').value;
        const ibuCell = row.querySelector('.ibu-cell');

        let ibu = 0;

        if (type === 'dry') {
          ibu = 0;
        } else if (formula === 'tinseth') {
          // Convert weight to grams
          const weightG = unit === 'oz' ? weight * 28.3495 : weight;
          const utilization = tinsethUtilization(time, gravity);
          const ibuContribution = (weightG * (aa / 100) * utilization * 1000) / (volL || 1);
          ibu = ibuContribution;
          const isoMass = weightG * (aa / 100) * utilization;
          totalIsoMass += isoMass;
          totalPotentialIsoMass += weightG * (aa / 100); // if U=1
        } else {
          // Rager
          const weightOz = unit === 'g' ? weight / 28.3495 : weight;
          let utilizationPercent = ragerUtilization(time);
          let gravityFactor = 0;
          if (gravity > 1.050) {
            gravityFactor = (gravity - 1.050) / 0.2;
          }
          const ibuContribution = (weightOz * (aa / 100) * (utilizationPercent / 100) * 7462) / ((volGal || 1) * (1 + gravityFactor));
          ibu = ibuContribution;
          const utilization = utilizationPercent / 100;
          const isoMass = weightOz * 28.3495 * (aa / 100) * utilization;
          totalIsoMass += isoMass;
          totalPotentialIsoMass += weightOz * 28.3495 * (aa / 100);
        }

        if (!isFinite(ibu)) ibu = 0;
        ibuCell.textContent = ibu.toFixed(1);
        totalIbu += ibu;
      });

      if (!isFinite(totalIbu)) totalIbu = 0;
      totalIbuEl.textContent = totalIbu.toFixed(1);

      let avgUtil = 0;
      if (totalPotentialIsoMass > 0) {
        avgUtil = (totalIsoMass / totalPotentialIsoMass) * 100;
      }
      if (!isFinite(avgUtil)) avgUtil = 0;
      avgUtilEl.textContent = avgUtil.toFixed(1) + '%';

      const gravityUnits = (gravity - 1) * 1000;
      let buGu = 0;
      if (gravityUnits > 0) {
        buGu = totalIbu / gravityUnits;
      }
      if (!isFinite(buGu)) buGu = 0;
      buGuEl.textContent = buGu.toFixed(2);
    }

    function attachEvents() {
      addHopRowBtn.addEventListener('click', () => {
        createHopRow();
        recalc();
      });

      resetBtn.addEventListener('click', () => {
        batchVolumeInput.value = 20;
        volumeUnitSelect.value = 'l';
        wortGravityInput.value = 1.050;
        formulaSelect.value = 'tinseth';
        hopTableBody.innerHTML = '';
        seedDefaultRows();
        recalc();
      });

      [formulaSelect, batchVolumeInput, volumeUnitSelect, wortGravityInput].forEach(el => {
        el.addEventListener('input', recalc);
        el.addEventListener('change', recalc);
      });

      hopTableBody.addEventListener('input', event => {
        if (event.target.matches('input, select')) {
          recalc();
        }
      });

      hopTableBody.addEventListener('click', event => {
        if (event.target.classList.contains('ibu-remove')) {
          const row = event.target.closest('tr');
          if (row) {
            row.remove();
            recalc();
          }
        }
      });
    }

    function seedDefaultRows() {
      createHopRow({ name: 'Bittering', aa: 12, weight: 20, unit: 'g', time: 60, type: 'boil' });
      createHopRow({ name: 'Flavor', aa: 8, weight: 25, unit: 'g', time: 15, type: 'boil' });
      createHopRow({ name: 'Aroma', aa: 8, weight: 30, unit: 'g', time: 0, type: 'whirlpool' });
    }

    seedDefaultRows();
    attachEvents();
    recalc();
  })();

  window.MathJax = {
    tex: { inlineMath: [['\\(','\\)'], ['$', '$']], displayMath: [['$','$'], ['\\[','\\]']] },
    svg: { fontCache: 'global' }
  };
</script>
<script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>

