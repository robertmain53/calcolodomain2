    <script>
      // Utility functions
      function parseNum(val) {
        const n = parseFloat(val);
        return isNaN(n) ? null : n;
      }

      function formatMoney(n) {
        if (n === null || n === undefined || isNaN(n)) return "–";
        const abs = Math.abs(n);
        let suffix = "";
        let value = n;
        if (abs >= 1e9) {
          value = n / 1e9;
          suffix = "B";
        } else if (abs >= 1e6) {
          value = n / 1e6;
          suffix = "M";
        } else if (abs >= 1e3) {
          value = n / 1e3;
          suffix = "K";
        }
        return "$" + value.toFixed(2) + suffix;
      }

      function formatMultiple(n) {
        if (n === null || n === undefined || isNaN(n)) return "–";
        return n.toFixed(2) + "x";
      }

      function percentile(arr, p) {
        if (!arr.length) return null;
        const sorted = [...arr].sort((a, b) => a - b);
        const idx = (p / 100) * (sorted.length - 1);
        const lower = Math.floor(idx);
        const upper = Math.ceil(idx);
        if (lower === upper) return sorted[lower];
        const weight = idx - lower;
        return sorted[lower] * (1 - weight) + sorted[upper] * weight;
      }

      // Deal row management
      const dealsTbody = document.getElementById("deals-tbody");
      const addRowBtn = document.getElementById("add-row-btn");
      const clearRowsBtn = document.getElementById("clear-rows-btn");

      function createDealRow(initial = {}) {
        const tr = document.createElement("tr");
        tr.className = "border-t";

        tr.innerHTML = `
      <td class="px-2 py-1">
        <input type="text" class="w-40 md:w-48 border-gray-200 rounded-md table-input text-xs md:text-sm" placeholder="Acquirer / Target" value="${
          initial.name || ""
        }">
      </td>
      <td class="px-2 py-1 text-right">
        <input type="number" step="0.01" class="w-28 md:w-32 border-gray-200 rounded-md table-input text-right" placeholder="EV" value="${
          initial.ev || ""
        }">
      </td>
      <td class="px-2 py-1 text-right">
        <input type="number" step="0.01" class="w-24 md:w-28 border-gray-200 rounded-md table-input text-right" placeholder="Rev" value="${
          initial.revenue || ""
        }">
      </td>
      <td class="px-2 py-1 text-right">
        <input type="number" step="0.01" class="w-24 md:w-28 border-gray-200 rounded-md table-input text-right" placeholder="EBITDA" value="${
          initial.ebitda || ""
        }">
      </td>
      <td class="px-2 py-1 text-right">
        <input type="number" step="0.01" class="w-24 md:w-28 border-gray-200 rounded-md table-input text-right" placeholder="Net inc." value="${
          initial.netIncome || ""
        }">
      </td>
      <td class="px-2 py-1 text-right text-gray-700" data-multiple="ev_revenue">–</td>
      <td class="px-2 py-1 text-right text-gray-700" data-multiple="ev_ebitda">–</td>
      <td class="px-2 py-1 text-right text-gray-700" data-multiple="pe">–</td>
      <td class="px-2 py-1 text-center">
        <input type="checkbox" class="deal-use-checkbox" checked>
      </td>
      <td class="px-2 py-1 text-center">
        <button type="button" class="text-red-500 text-xs hover:underline remove-row-btn">Delete</button>
      </td>
    `;

        dealsTbody.appendChild(tr);
      }

      function initDefaultRows() {
        const sampleDeals = [
          {
            name: "Deal A",
            ev: 500000000,
            revenue: 200000000,
            ebitda: 60000000,
            netIncome: 30000000,
          },
          {
            name: "Deal B",
            ev: 750000000,
            revenue: 250000000,
            ebitda: 90000000,
            netIncome: 45000000,
          },
          {
            name: "Deal C",
            ev: 400000000,
            revenue: 150000000,
            ebitda: 50000000,
            netIncome: 22000000,
          },
        ];
        sampleDeals.forEach((d) => createDealRow(d));
        recomputeAll();
      }

      addRowBtn.addEventListener("click", () => {
        createDealRow();
      });

      clearRowsBtn.addEventListener("click", () => {
        dealsTbody.innerHTML = "";
        recomputeAll();
      });

      dealsTbody.addEventListener("click", (e) => {
        if (e.target.classList.contains("remove-row-btn")) {
          e.target.closest("tr").remove();
          recomputeAll();
        }
      });

      dealsTbody.addEventListener("input", (e) => {
        if (e.target.matches("input")) {
          recomputeAll();
        }
      });

      dealsTbody.addEventListener("change", (e) => {
        if (e.target.classList.contains("deal-use-checkbox")) {
          recomputeAll();
        }
      });

      // Computation of multiples and stats
      function getDealsData() {
        const rows = Array.from(dealsTbody.querySelectorAll("tr"));
        const deals = [];

        rows.forEach((row) => {
          const inputs = row.querySelectorAll("input");
          const name = inputs[0].value.trim();
          const ev = parseNum(inputs[1].value);
          const revenue = parseNum(inputs[2].value);
          const ebitda = parseNum(inputs[3].value);
          const netIncome = parseNum(inputs[4].value);
          const use = row.querySelector(".deal-use-checkbox").checked;

          const evRevenueCell = row.querySelector(
            '[data-multiple="ev_revenue"]'
          );
          const evEbitdaCell = row.querySelector('[data-multiple="ev_ebitda"]');
          const peCell = row.querySelector('[data-multiple="pe"]');

          let evRev = null,
            evEbitda = null,
            pe = null;

          if (ev !== null && revenue !== null && revenue !== 0) {
            evRev = ev / revenue;
            evRevenueCell.textContent = formatMultiple(evRev);
          } else {
            evRevenueCell.textContent = "–";
          }

          if (ev !== null && ebitda !== null && ebitda !== 0) {
            evEbitda = ev / ebitda;
            evEbitdaCell.textContent = formatMultiple(evEbitda);
          } else {
            evEbitdaCell.textContent = "–";
          }

          if (ev !== null && netIncome !== null && netIncome !== 0) {
            pe = ev / netIncome;
            peCell.textContent = formatMultiple(pe);
          } else {
            peCell.textContent = "–";
          }

          deals.push({
            name,
            ev,
            revenue,
            ebitda,
            netIncome,
            evRev,
            evEbitda,
            pe,
            use,
          });
        });

        return deals;
      }

      function updateSummaryStats() {
        const deals = getDealsData().filter((d) => d.use);

        const evEbitdaArr = deals
          .map((d) => d.evEbitda)
          .filter((v) => v !== null && isFinite(v));
        const evRevenueArr = deals
          .map((d) => d.evRev)
          .filter((v) => v !== null && isFinite(v));
        const peArr = deals
          .map((d) => d.pe)
          .filter((v) => v !== null && isFinite(v));

        const sets = [
          { arr: evEbitdaArr, prefix: "ev-ebitda", countId: "count-ev-ebitda" },
          {
            arr: evRevenueArr,
            prefix: "ev-revenue",
            countId: "count-ev-revenue",
          },
          { arr: peArr, prefix: "pe", countId: "count-pe" },
        ];

        sets.forEach((set) => {
          document.getElementById(set.countId).textContent =
            "n=" + set.arr.length;
          const min = set.arr.length ? Math.min(...set.arr) : null;
          const max = set.arr.length ? Math.max(...set.arr) : null;
          const p25 = percentile(set.arr, 25);
          const med = percentile(set.arr, 50);
          const p75 = percentile(set.arr, 75);

          document.getElementById("min-" + set.prefix).textContent =
            formatMultiple(min);
          document.getElementById("p25-" + set.prefix).textContent =
            formatMultiple(p25);
          document.getElementById("median-" + set.prefix).textContent =
            formatMultiple(med);
          document.getElementById("p75-" + set.prefix).textContent =
            formatMultiple(p75);
          document.getElementById("max-" + set.prefix).textContent =
            formatMultiple(max);
        });
      }

      function recomputeAll() {
        getDealsData(); // updates row multiples
        updateSummaryStats();
        updateAssumptionPanel();
      }

      // Target & valuation logic
      const targetMetricTypeEl = document.getElementById("target-metric-type");
      const targetMetricValueEl = document.getElementById(
        "target-metric-value"
      );
      const targetNetDebtEl = document.getElementById("target-net-debt");

      const valuationMetricLabelEl = document.getElementById(
        "valuation-metric-label"
      );
      const valuationMetricValueEl = document.getElementById(
        "valuation-metric-value"
      );
      const valuationNetDebtEl = document.getElementById("valuation-net-debt");

      function updateAssumptionPanel() {
        const metricType = targetMetricTypeEl.value;
        const metricValue = parseNum(targetMetricValueEl.value);
        const netDebt = parseNum(targetNetDebtEl.value);

        const labelMap = {
          ebitda: "EBITDA",
          revenue: "Revenue",
          netIncome: "Net Income",
        };

        valuationMetricLabelEl.textContent = labelMap[metricType] || "Metric";
        valuationMetricValueEl.textContent =
          metricValue !== null ? formatMoney(metricValue) : "–";
        valuationNetDebtEl.textContent =
          netDebt !== null ? formatMoney(netDebt) : "–";
      }

      targetMetricTypeEl.addEventListener("change", updateAssumptionPanel);
      targetMetricValueEl.addEventListener("input", updateAssumptionPanel);
      targetNetDebtEl.addEventListener("input", updateAssumptionPanel);

      // Median range autofill
      const selectedMultipleTypeEl = document.getElementById(
        "selected-multiple-type"
      );
      const lowMultipleEl = document.getElementById("low-multiple");
      const highMultipleEl = document.getElementById("high-multiple");
      const useMedianRangeBtn = document.getElementById("use-median-range-btn");

      useMedianRangeBtn.addEventListener("click", () => {
        const type = selectedMultipleTypeEl.value;
        let p25Id, p75Id;

        if (type === "ev_ebitda") {
          p25Id = "p25-ev-ebitda";
          p75Id = "p75-ev-ebitda";
        } else if (type === "ev_revenue") {
          p25Id = "p25-ev-revenue";
          p75Id = "p75-ev-revenue";
        } else {
          p25Id = "p25-pe";
          p75Id = "p75-pe";
        }

        function parseMultipleText(id) {
          const txt = document
            .getElementById(id)
            .textContent.replace("x", "")
            .trim();
          const n = parseFloat(txt);
          return isNaN(n) ? null : n;
        }

        const low = parseMultipleText(p25Id);
        const high = parseMultipleText(p75Id);

        if (low === null || high === null) {
          alert(
            "Not enough data to compute a median range for this multiple. Please add more deals or enter the range manually."
          );
          return;
        }

        lowMultipleEl.value = low.toFixed(2);
        highMultipleEl.value = high.toFixed(2);
      });

      // Valuation calculation
      const calculateValuationBtn = document.getElementById(
        "calculate-valuation-btn"
      );
      const evLowEl = document.getElementById("ev-low");
      const evMidEl = document.getElementById("ev-mid");
      const evHighEl = document.getElementById("ev-high");
      const eqLowEl = document.getElementById("eq-low");
      const eqMidEl = document.getElementById("eq-mid");
      const eqHighEl = document.getElementById("eq-high");

      calculateValuationBtn.addEventListener("click", () => {
        const metricType = targetMetricTypeEl.value;
        const metricValue = parseNum(targetMetricValueEl.value);
        const netDebt = parseNum(targetNetDebtEl.value) || 0;

        if (metricValue === null || metricValue === 0) {
          alert(
            "Please enter a valid target metric value (e.g., EBITDA, Revenue, or Net Income)."
          );
          return;
        }

        const lowMult = parseNum(lowMultipleEl.value);
        const highMult = parseNum(highMultipleEl.value);

        if (
          lowMult === null ||
          highMult === null ||
          lowMult <= 0 ||
          highMult <= 0
        ) {
          alert(
            "Please enter a valid low and high multiple greater than zero."
          );
          return;
        }

        const midMult = (lowMult + highMult) / 2;

        const evLow = lowMult * metricValue;
        const evMid = midMult * metricValue;
        const evHigh = highMult * metricValue;

        const eqLow = evLow - netDebt;
        const eqMid = evMid - netDebt;
        const eqHigh = evHigh - netDebt;

        evLowEl.textContent = formatMoney(evLow);
        evMidEl.textContent = formatMoney(evMid);
        evHighEl.textContent = formatMoney(evHigh);

        eqLowEl.textContent = formatMoney(eqLow);
        eqMidEl.textContent = formatMoney(eqMid);
        eqHighEl.textContent = formatMoney(eqHigh);
      });

      // Initialize defaults
      document.addEventListener("DOMContentLoaded", () => {
        initDefaultRows();
        updateAssumptionPanel();
      });

      // Mobile menu script (simple)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          this.setAttribute("aria-expanded", isHidden ? "true" : "false");
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
