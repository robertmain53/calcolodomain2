    <script>
      // Mobile menu toggle
      document
        .getElementById("mobile-menu-toggle")
        ?.addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          if (!menu) return;
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          this.setAttribute("aria-expanded", String(isHidden));
        });

      // Period preset sync
      const periodLengthInput = document.getElementById("period-length");
      const periodPresetSelect = document.getElementById("period-preset");
      periodPresetSelect.addEventListener("change", () => {
        const val = periodPresetSelect.value;
        if (val !== "custom") {
          periodLengthInput.value = val;
        }
      });
      periodLengthInput.addEventListener("input", () => {
        periodPresetSelect.value = "custom";
      });

      // AR method toggle
      const arMethodSelect = document.getElementById("ar-method");
      const arEndingWrapper = document.getElementById("ar-ending-wrapper");
      const arBeginWrapper = document.getElementById("ar-begin-wrapper");
      arMethodSelect.addEventListener("change", () => {
        if (arMethodSelect.value === "average") {
          arBeginWrapper.classList.remove("hidden");
        } else {
          arBeginWrapper.classList.add("hidden");
        }
      });

      // AR turnover toggle
      const useArTurnoverCheckbox = document.getElementById("use-ar-turnover");
      const arTurnoverWrapper = document.getElementById("ar-turnover-wrapper");
      const creditSalesInput = document.getElementById("credit-sales");
      const arEndingInput = document.getElementById("ar-ending");
      const arBeginInput = document.getElementById("ar-begin");

      useArTurnoverCheckbox.addEventListener("change", () => {
        const useTurnover = useArTurnoverCheckbox.checked;
        arTurnoverWrapper.classList.toggle("hidden", !useTurnover);
        // Disable/enable AR & sales inputs to avoid confusion
        [creditSalesInput, arEndingInput, arBeginInput, arMethodSelect].forEach(
          (el) => {
            el.disabled = useTurnover;
            if (useTurnover) {
              el.classList.add("bg-gray-100");
            } else {
              el.classList.remove("bg-gray-100");
            }
          }
        );
      });

      function formatCurrencyShort(value) {
        if (!isFinite(value)) return "–";
        const abs = Math.abs(value);
        if (abs >= 1e9) return (value / 1e9).toFixed(2) + "B";
        if (abs >= 1e6) return (value / 1e6).toFixed(2) + "M";
        if (abs >= 1e3) return (value / 1e3).toFixed(2) + "k";
        return value.toFixed(2);
      }

      function classifyDSO(dso, termsDays) {
        if (!isFinite(dso))
          return {
            label: "–",
            color: "bg-gray-200 text-gray-700",
            text: "Enter your data to see an interpretation.",
          };

        let label = "Average";
        let color = "bg-yellow-100 text-yellow-800";
        let text =
          "Your DSO is within a typical range. Compare to your own history and industry benchmarks.";

        if (!termsDays || !isFinite(termsDays) || termsDays <= 0) {
          if (dso <= 30) {
            label = "Strong";
            color = "bg-emerald-100 text-emerald-800";
            text = "Collections are fast relative to many B2B terms.";
          } else if (dso <= 60) {
            label = "Watch";
            color = "bg-yellow-100 text-yellow-800";
            text =
              "Collections are moderate. Monitor trends and aging reports.";
          } else {
            label = "High";
            color = "bg-red-100 text-red-800";
            text =
              "Collections appear slow. Review credit policies and invoicing processes.";
          }
          return { label, color, text };
        }

        if (dso <= termsDays * 1.1) {
          label = "On target";
          color = "bg-emerald-100 text-emerald-800";
          text =
            "DSO is close to your stated payment terms. Collections performance looks healthy.";
        } else if (dso <= termsDays * 1.5) {
          label = "Slightly high";
          color = "bg-yellow-100 text-yellow-800";
          text =
            "Customers are paying somewhat later than terms. Consider tightening follow-up and resolving disputes faster.";
        } else {
          label = "Elevated";
          color = "bg-red-100 text-red-800";
          text =
            "DSO is significantly above your terms. Cash is likely tied up in receivables; review credit, billing, and collections processes.";
        }
        return { label, color, text };
      }

      document.getElementById("calculate-btn").addEventListener("click", () => {
        const days = parseFloat(periodLengthInput.value);
        const useTurnover = useArTurnoverCheckbox.checked;
        const dsoResultEl = document.getElementById("dso-result");
        const dsoQualitativeEl = document.getElementById("dso-qualitative");
        const dsoBadgeEl = document.getElementById("dso-badge");
        const avgArDisplayEl = document.getElementById("avg-ar-display");
        const formulaDisplayEl = document.getElementById("formula-display");
        const cashImpactTextEl = document.getElementById("cash-impact-text");

        if (!isFinite(days) || days <= 0) {
          alert("Please enter a valid period length in days.");
          return;
        }

        let dso;
        let avgAR = null;
        let formulaText = "";

        if (useTurnover) {
          const turnover = parseFloat(
            document.getElementById("ar-turnover").value
          );
          if (!isFinite(turnover) || turnover <= 0) {
            alert("Please enter a valid AR turnover value greater than 0.");
            return;
          }
          dso = days / turnover;
          formulaText = `DSO = ${days} ÷ ${turnover.toFixed(2)} = ${dso.toFixed(
            2
          )} days`;
          avgAR = null;
        } else {
          const arEnding = parseFloat(arEndingInput.value);
          const arBegin = parseFloat(arBeginInput.value);
          const sales = parseFloat(creditSalesInput.value);

          if (!isFinite(arEnding) || arEnding < 0) {
            alert("Please enter a valid ending accounts receivable balance.");
            return;
          }
          if (!isFinite(sales) || sales <= 0) {
            alert(
              "Please enter valid credit sales or revenue for the period (greater than 0)."
            );
            return;
          }

          if (arMethodSelect.value === "average") {
            if (!isFinite(arBegin) || arBegin < 0) {
              alert(
                "Please enter a valid beginning accounts receivable balance."
              );
              return;
            }
            avgAR = (arBegin + arEnding) / 2;
          } else {
            avgAR = arEnding;
          }

          dso = (avgAR / sales) * days;
          formulaText = `DSO = (${avgAR.toFixed(2)} ÷ ${sales.toFixed(
            2
          )}) × ${days} = ${dso.toFixed(2)} days`;
        }

        if (!isFinite(dso) || dso < 0) {
          alert(
            "Unable to calculate DSO with the provided inputs. Please check your values."
          );
          return;
        }

        dsoResultEl.textContent = dso.toFixed(2);
        formulaDisplayEl.textContent = formulaText;
        avgArDisplayEl.textContent =
          avgAR !== null
            ? formatCurrencyShort(avgAR)
            : "n/a (using AR turnover)";

        // Simple interpretation (no explicit terms input, so we classify generically)
        const interpretation = classifyDSO(dso, NaN);
        dsoBadgeEl.textContent = interpretation.label;
        dsoBadgeEl.className = "badge " + interpretation.color;
        dsoQualitativeEl.textContent = interpretation.text;

        // Cash impact: approximate daily credit sales and show 1-day DSO change effect
        if (!useTurnover && avgAR !== null) {
          const sales = parseFloat(creditSalesInput.value);
          const dailySales = sales / days;
          const oneDayCash = dailySales;
          cashImpactTextEl.textContent = `Approximate impact: each 1-day change in DSO represents about ${formatCurrencyShort(
            oneDayCash
          )} of cash tied up in receivables for this period.`;
        } else {
          cashImpactTextEl.textContent =
            "Approximate impact: once you calculate DSO using AR and sales, we will estimate how much cash is tied up per day of DSO.";
        }
      });
    </script>

