  <script>
    // mobile menu toggle
    const mobileToggle = document.getElementById('mobile-menu-toggle');
    const mobileMenu = document.getElementById('mobile-menu');
    if (mobileToggle && mobileMenu) {
      mobileToggle.addEventListener('click', () => {
        const isHidden = mobileMenu.classList.contains('hidden');
        mobileMenu.classList.toggle('hidden', !isHidden);
        mobileToggle.setAttribute('aria-expanded', String(isHidden));
      });
    }

    const G_TO_CT = 5;  // 1 g = 5 ct
    const CT_TO_G = 0.2;

    function formatNumber(num, decimals) {
      if (isNaN(num)) return '–';
      return num.toFixed(decimals);
    }

    document.getElementById('convert-btn').addEventListener('click', () => {
      const grams = parseFloat(document.getElementById('grams').value);
      const decimals = parseInt(document.getElementById('decimals').value, 10);
      if (isNaN(grams)) {
        alert('Please enter a valid weight in grams.');
        return;
      }
      const carats = grams * G_TO_CT;
      document.getElementById('res-ct').textContent = formatNumber(carats, decimals) + ' ct';
    });

    document.querySelectorAll('.preset-btn').forEach(btn => {
      btn.addEventListener('click', () => {
        const val = parseFloat(btn.dataset.val);
        document.getElementById('grams').value = val;
        document.getElementById('convert-btn').click();
      });
    });

    document.getElementById('rev-btn').addEventListener('click', () => {
      const ct = parseFloat(document.getElementById('ct-rev').value);
      if (isNaN(ct)) {
        alert('Enter a carats value to convert back.');
        return;
      }
      const grams = ct * CT_TO_G;
      document.getElementById('rev-out').textContent = ct + ' ct = ' + grams.toFixed(3) + ' g';
    });

    // initial run
    document.getElementById('convert-btn').click();
  </script>

