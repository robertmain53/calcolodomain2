    <script>
      function fmtMoneyMillions(value) {
        if (!isFinite(value)) return "$–";
        return (
          "$" +
          value.toLocaleString(undefined, { maximumFractionDigits: 1 }) +
          "M"
        );
      }
      function fmtMoney(value) {
        if (!isFinite(value)) return "$–";
        return (
          "$" + value.toLocaleString(undefined, { maximumFractionDigits: 2 })
        );
      }
      function fmtPercent(value) {
        if (!isFinite(value)) return "–";
        return (
          value.toLocaleString(undefined, { maximumFractionDigits: 2 }) + "%"
        );
      }

      function calculateMA() {
        const get = (id) => parseFloat(document.getElementById(id).value);

        const acqPrice = get("acqPrice");
        const acqShares = get("acqShares");
        const acqNetIncome = get("acqNetIncome");

        const tgtPrice = get("tgtPrice");
        const tgtShares = get("tgtShares");
        const tgtNetIncome = get("tgtNetIncome");

        const offerPremium = get("offerPremium") / 100;
        const cashPortion = get("cashPortion") / 100;
        const interestRate = get("interestRate") / 100;
        const taxRate = get("taxRate") / 100;

        const synergies = get("synergies");
        const integrationCost = get("integrationCost");
        const integrationYears = get("integrationYears");

        const errorEl = document.getElementById("errorMsg");
        errorEl.textContent = "";

        if (
          [
            acqPrice,
            acqShares,
            acqNetIncome,
            tgtPrice,
            tgtShares,
            tgtNetIncome,
          ].some((v) => !isFinite(v) || v <= 0)
        ) {
          errorEl.textContent =
            "Please enter positive values for prices, shares, and net income.";
          return;
        }
        if (!isFinite(offerPremium) || offerPremium < -0.5) {
          errorEl.textContent =
            "Offer premium looks invalid. Use a reasonable percentage.";
          return;
        }
        if (!isFinite(cashPortion) || cashPortion < 0 || cashPortion > 1) {
          errorEl.textContent = "Cash portion must be between 0% and 100%.";
          return;
        }
        if (!isFinite(integrationYears) || integrationYears <= 0) {
          errorEl.textContent = "Integration years must be greater than 0.";
          return;
        }

        // Market caps (millions)
        const acqMktCap = acqPrice * acqShares;
        const tgtMktCap = tgtPrice * tgtShares;

        // Offer and deal value
        const offerPrice = tgtPrice * (1 + offerPremium);
        const equityValue = offerPrice * tgtShares; // millions

        const cashConsideration = equityValue * cashPortion;
        const stockValue = equityValue - cashConsideration;
        const newShares = stockValue / acqPrice;

        const totalShares = acqShares + newShares;
        const acqOwnershipPct = (acqShares / totalShares) * 100;
        const tgtOwnershipPct = (newShares / totalShares) * 100;

        // Synergies & integration (millions)
        const afterTaxSynergies = synergies * (1 - taxRate);
        const annualIntegration = integrationCost / integrationYears;
        const afterTaxIntegration = annualIntegration * (1 - taxRate);

        // Interest on new debt (assume all cash is debt-financed)
        const interestExpense = cashConsideration * interestRate;
        const afterTaxInterest = interestExpense * (1 - taxRate);

        // EPS
        const acqEPS = acqNetIncome / acqShares;
        const combinedNI =
          acqNetIncome +
          tgtNetIncome +
          afterTaxSynergies -
          afterTaxIntegration -
          afterTaxInterest;
        const combinedEPS = combinedNI / totalShares;
        const epsAccretionPct = ((combinedEPS - acqEPS) / acqEPS) * 100;

        // Update UI
        document.getElementById("resTgtMktCap").textContent =
          fmtMoneyMillions(tgtMktCap);
        document.getElementById("resOfferPrice").textContent =
          fmtMoney(offerPrice);
        document.getElementById("resEquityValue").textContent =
          fmtMoneyMillions(equityValue);
        document.getElementById("resCash").textContent =
          fmtMoneyMillions(cashConsideration);
        document.getElementById("resStockValue").textContent =
          fmtMoneyMillions(stockValue);
        document.getElementById("resNewShares").textContent = isFinite(
          newShares
        )
          ? newShares.toLocaleString(undefined, { maximumFractionDigits: 2 })
          : "–";
        document.getElementById("resOwnership").textContent = isFinite(
          acqOwnershipPct
        )
          ? `${fmtPercent(acqOwnershipPct)} / ${fmtPercent(tgtOwnershipPct)}`
          : "–";

        document.getElementById("resAcqEPS").textContent = isFinite(acqEPS)
          ? "$" + acqEPS.toFixed(2)
          : "$–";
        document.getElementById("resCombinedEPS").textContent = isFinite(
          combinedEPS
        )
          ? "$" + combinedEPS.toFixed(2)
          : "$–";
        document.getElementById("resAccretion").textContent =
          fmtPercent(epsAccretionPct);

        document.getElementById("resAfterTaxSynergies").textContent =
          fmtMoneyMillions(afterTaxSynergies);
        document.getElementById("resAfterTaxIntegration").textContent =
          fmtMoneyMillions(afterTaxIntegration);
        document.getElementById("resAfterTaxInterest").textContent =
          fmtMoneyMillions(afterTaxInterest);
      }

      document
        .getElementById("calcButton")
        .addEventListener("click", calculateMA);

      document.getElementById("resetButton").addEventListener("click", () => {
        document.getElementById("acqPrice").value = 50;
        document.getElementById("acqShares").value = 500;
        document.getElementById("acqNetIncome").value = 2500;
        document.getElementById("tgtPrice").value = 30;
        document.getElementById("tgtShares").value = 200;
        document.getElementById("tgtNetIncome").value = 600;
        document.getElementById("offerPremium").value = 30;
        document.getElementById("cashPortion").value = 50;
        document.getElementById("interestRate").value = 5;
        document.getElementById("taxRate").value = 25;
        document.getElementById("synergies").value = 400;
        document.getElementById("integrationCost").value = 500;
        document.getElementById("integrationYears").value = 5;
        document.getElementById("errorMsg").textContent = "";
        // Clear results
        [
          "resTgtMktCap",
          "resOfferPrice",
          "resEquityValue",
          "resCash",
          "resStockValue",
          "resNewShares",
          "resOwnership",
          "resAcqEPS",
          "resCombinedEPS",
          "resAccretion",
          "resAfterTaxSynergies",
          "resAfterTaxIntegration",
          "resAfterTaxInterest",
        ].forEach(
          (id) =>
            (document.getElementById(id).textContent =
              id.includes("resAccretion") || id.includes("Ownership")
                ? "–"
                : "$–")
        );
      });

      // Auto-calc on load with defaults
      window.addEventListener("DOMContentLoaded", calculateMA);

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
