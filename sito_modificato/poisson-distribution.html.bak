    <script src="https://cdn.jsdelivr.net/npm/chart.js@4.4.0/dist/chart.umd.min.js"></script>
    <script>
      // Utility functions
      function factorial(k) {
        if (k < 0) return NaN;
        if (k === 0 || k === 1) return 1;
        let res = 1;
        for (let i = 2; i <= k; i++) res *= i;
        return res;
      }

      function poissonPMF(lambda, k) {
        if (lambda < 0 || k < 0) return 0;
        return (Math.exp(-lambda) * Math.pow(lambda, k)) / factorial(k);
      }

      function poissonCDF(lambda, k) {
        if (k < 0) return 0;
        let sum = 0;
        for (let i = 0; i <= k; i++) {
          sum += poissonPMF(lambda, i);
        }
        return sum;
      }

      function clampRangeK(k1, k2) {
        k1 = Math.max(0, Math.floor(k1));
        k2 = Math.max(0, Math.floor(k2));
        if (k2 < k1) [k1, k2] = [k2, k1];
        return [k1, k2];
      }

      // DOM elements
      const lambdaInput = document.getElementById("lambda");
      const kInput = document.getElementById("k");
      const probTypeSelect = document.getElementById("prob-type");
      const rangeInputs = document.getElementById("range-inputs");
      const k1Input = document.getElementById("k1");
      const k2Input = document.getElementById("k2");
      const calcBtn = document.getElementById("calc-btn");
      const resetBtn = document.getElementById("reset-btn");
      const resultText = document.getElementById("result-text");
      const resultExtra = document.getElementById("result-extra");
      const meanVal = document.getElementById("mean-val");
      const varVal = document.getElementById("var-val");
      const sdVal = document.getElementById("sd-val");
      const modeVal = document.getElementById("mode-val");
      const maxKSlider = document.getElementById("max-k");
      const maxKOutput = document.getElementById("max-k-output");

      // Charts
      const pmfCtx = document.getElementById("pmfChart").getContext("2d");
      const cdfCtx = document.getElementById("cdfChart").getContext("2d");
      let pmfChart, cdfChart;

      function updateSummary(lambda) {
        if (lambda < 0) {
          meanVal.textContent = "—";
          varVal.textContent = "—";
          sdVal.textContent = "—";
          modeVal.textContent = "—";
          return;
        }
        meanVal.textContent = lambda.toFixed(3);
        varVal.textContent = lambda.toFixed(3);
        sdVal.textContent = Math.sqrt(lambda).toFixed(3);

        if (lambda === 0) {
          modeVal.textContent = "0";
        } else if (Number.isInteger(lambda)) {
          modeVal.textContent = `${lambda - 1} or ${lambda}`;
        } else {
          modeVal.textContent = Math.floor(lambda).toString();
        }
      }

      function calculate() {
        const lambda = parseFloat(lambdaInput.value);
        const k = parseInt(kInput.value, 10);

        if (!isFinite(lambda) || lambda < 0) {
          alert("Please enter a non-negative value for λ.");
          return;
        }
        if (!Number.isInteger(k) || k < 0) {
          alert("k must be an integer ≥ 0.");
          return;
        }

        updateSummary(lambda);

        const type = probTypeSelect.value;
        let prob = 0;
        let description = "";

        if (type === "eq") {
          prob = poissonPMF(lambda, k);
          description = `P(X = ${k})`;
        } else if (type === "leq") {
          prob = poissonCDF(lambda, k);
          description = `P(X \u2264 ${k})`;
        } else if (type === "geq") {
          prob = 1 - poissonCDF(lambda, k - 1);
          description = `P(X \u2265 ${k})`;
        } else if (type === "range") {
          let [k1, k2] = clampRangeK(
            parseInt(k1Input.value, 10),
            parseInt(k2Input.value, 10)
          );
          k1Input.value = k1;
          k2Input.value = k2;
          let sum = 0;
          for (let i = k1; i <= k2; i++) {
            sum += poissonPMF(lambda, i);
          }
          prob = sum;
          description = `P(${k1} \u2264 X \u2264 ${k2})`;
        }

        prob = Math.max(0, Math.min(1, prob)); // clamp due to rounding

        resultText.textContent = `${description} \u2248 ${prob.toFixed(6)}`;
        resultExtra.textContent = `As a percentage: ${(prob * 100).toFixed(
          4
        )}%`;

        updateCharts(lambda);
      }

      function resetCalculator() {
        lambdaInput.value = 3;
        kInput.value = 2;
        probTypeSelect.value = "eq";
        k1Input.value = 0;
        k2Input.value = 5;
        rangeInputs.classList.add("hidden");
        maxKSlider.value = 15;
        maxKOutput.textContent = "15";
        updateSummary(3);
        resultText.textContent = "P(X = 2) \u2248 0.2240";
        resultExtra.textContent = "As a percentage: 22.40%";
        updateCharts(3);
      }

      probTypeSelect.addEventListener("change", () => {
        if (probTypeSelect.value === "range") {
          rangeInputs.classList.remove("hidden");
        } else {
          rangeInputs.classList.add("hidden");
        }
      });

      maxKSlider.addEventListener("input", () => {
        maxKOutput.textContent = maxKSlider.value;
        const lambda = parseFloat(lambdaInput.value);
        if (isFinite(lambda) && lambda >= 0) {
          updateCharts(lambda);
        }
      });

      calcBtn.addEventListener("click", calculate);
      resetBtn.addEventListener("click", resetCalculator);

      function buildData(lambda) {
        const maxK = parseInt(maxKSlider.value, 10);
        const labels = [];
        const pmfData = [];
        const cdfData = [];
        let cdf = 0;
        for (let k = 0; k <= maxK; k++) {
          const p = poissonPMF(lambda, k);
          cdf += p;
          labels.push(k);
          pmfData.push(p);
          cdfData.push(Math.min(1, cdf));
        }
        return { labels, pmfData, cdfData };
      }

      function updateCharts(lambda) {
        const { labels, pmfData, cdfData } = buildData(lambda);

        if (!pmfChart) {
          pmfChart = new Chart(pmfCtx, {
            type: "bar",
            data: {
              labels,
              datasets: [
                {
                  label: "P(X = k)",
                  data: pmfData,
                  backgroundColor: "rgba(37, 99, 235, 0.6)",
                  borderColor: "rgba(37, 99, 235, 1)",
                  borderWidth: 1,
                },
              ],
            },
            options: {
              responsive: true,
              scales: {
                x: { title: { display: true, text: "k" } },
                y: { title: { display: true, text: "PMF" }, beginAtZero: true },
              },
              plugins: {
                legend: { display: false },
                tooltip: {
                  callbacks: {
                    label: (ctx) =>
                      `P(X = ${ctx.label}) = ${ctx.raw.toFixed(6)}`,
                  },
                },
              },
            },
          });
        } else {
          pmfChart.data.labels = labels;
          pmfChart.data.datasets[0].data = pmfData;
          pmfChart.update();
        }

        if (!cdfChart) {
          cdfChart = new Chart(cdfCtx, {
            type: "line",
            data: {
              labels,
              datasets: [
                {
                  label: "P(X ≤ k)",
                  data: cdfData,
                  fill: false,
                  borderColor: "rgba(16, 185, 129, 1)",
                  backgroundColor: "rgba(16, 185, 129, 0.2)",
                  tension: 0.1,
                  pointRadius: 2,
                },
              ],
            },
            options: {
              responsive: true,
              scales: {
                x: { title: { display: true, text: "k" } },
                y: {
                  title: { display: true, text: "CDF" },
                  beginAtZero: true,
                  max: 1,
                },
              },
              plugins: {
                legend: { display: false },
                tooltip: {
                  callbacks: {
                    label: (ctx) =>
                      `P(X ≤ ${ctx.label}) = ${ctx.raw.toFixed(6)}`,
                  },
                },
              },
            },
          });
        } else {
          cdfChart.data.labels = labels;
          cdfChart.data.datasets[0].data = cdfData;
          cdfChart.update();
        }
      }

      // Initial setup
      document.addEventListener("DOMContentLoaded", () => {
        updateSummary(3);
        updateCharts(3);
      });
    </script>

    <script>
      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
