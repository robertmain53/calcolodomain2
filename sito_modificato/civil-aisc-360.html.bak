    <script>
      // Basic W-shape database (approximate values, for demo only)
      const shapes = {
        W12x26: {
          A: 7.65,
          Sx: 54.0,
          Zx: 61.0,
          Ix: 324.0,
          rx: 6.5,
          ry: 2.1,
          tw: 0.26,
          bf: 8.0,
          d: 12.2,
        },
        W18x35: {
          A: 10.3,
          Sx: 103.0,
          Zx: 116.0,
          Ix: 927.0,
          rx: 9.5,
          ry: 3.0,
          tw: 0.3,
          bf: 7.5,
          d: 17.7,
        },
        W21x44: {
          A: 12.9,
          Sx: 152.0,
          Zx: 173.0,
          Ix: 1600.0,
          rx: 11.1,
          ry: 3.3,
          tw: 0.32,
          bf: 8.0,
          d: 20.7,
        },
        W24x55: {
          A: 16.2,
          Sx: 214.0,
          Zx: 243.0,
          Ix: 2570.0,
          rx: 12.6,
          ry: 3.7,
          tw: 0.35,
          bf: 8.0,
          d: 23.7,
        },
        W30x90: {
          A: 26.5,
          Sx: 420.0,
          Zx: 470.0,
          Ix: 6300.0,
          rx: 15.4,
          ry: 4.7,
          tw: 0.47,
          bf: 10.0,
          d: 29.6,
        },
      };

      const E = 29000; // ksi

      function updateProps() {
        const shape = shapes[document.getElementById("shapeSelect").value];
        if (!shape) return;
        document.getElementById("propA").textContent = shape.A.toFixed(2);
        document.getElementById("propSx").textContent = shape.Sx.toFixed(1);
        document.getElementById("propZx").textContent = shape.Zx.toFixed(1);
        document.getElementById("propIx").textContent = shape.Ix.toFixed(0);
        document.getElementById("propRx").textContent = shape.rx.toFixed(2);
        document.getElementById("propRy").textContent = shape.ry.toFixed(2);
        document.getElementById("propTw").textContent = shape.tw.toFixed(2);
        document.getElementById("propBf").textContent = shape.bf.toFixed(1);
      }

      document
        .getElementById("shapeSelect")
        .addEventListener("change", updateProps);
      updateProps();

      document.getElementById("runCheck").addEventListener("click", () => {
        const method = document.getElementById("designMethod").value;
        const Fy = parseFloat(document.getElementById("Fy").value || "0"); // ksi
        const L = parseFloat(document.getElementById("L").value || "0"); // ft
        const Mu = parseFloat(document.getElementById("Mu").value || "0"); // kip-ft
        const Vu = parseFloat(document.getElementById("Vu").value || "0"); // kips
        const Pu = parseFloat(document.getElementById("Pu").value || "0"); // kips
        const Kx = parseFloat(document.getElementById("Kx").value || "1");
        const Ky = parseFloat(document.getElementById("Ky").value || "1");

        const shape = shapes[document.getElementById("shapeSelect").value];
        if (!shape || Fy <= 0 || L <= 0) return;

        // Factors
        const phi_b = 0.9,
          phi_v = 1.0,
          phi_c = 0.9,
          phi_t = 0.9;
        const Omega_b = 1.67,
          Omega_v = 1.67,
          Omega_c = 1.67,
          Omega_t = 1.67;

        // Flexure
        const Mn = Fy * shape.Zx; // kip-in
        const Mu_in = Mu * 12.0; // kip-in
        let Mcap;
        if (method === "LRFD") {
          Mcap = phi_b * Mn;
        } else {
          Mcap = Mn / Omega_b;
        }
        const flexRatio = Mcap > 0 ? Mu_in / Mcap : 0;

        // Shear
        const Aw = shape.d * shape.tw; // in^2
        const Vn = (0.6 * Fy * Aw) / 1000.0; // kips
        let Vcap;
        if (method === "LRFD") {
          Vcap = phi_v * Vn;
        } else {
          Vcap = Vn / Omega_v;
        }
        const shearRatio = Vcap > 0 ? Vu / Vcap : 0;

        // Axial
        let Pcap = 0;
        let axialMode = "none";
        if (Math.abs(Pu) > 1e-6) {
          if (Pu >= 0) {
            // Compression: use minor axis as governing
            const L_in = L * 12.0;
            const KLx = Kx * L_in;
            const KLy = Ky * L_in;
            const slenderX = KLx / shape.rx;
            const slenderY = KLy / shape.ry;
            const slender = Math.max(slenderX, slenderY);
            const Fe = (Math.PI * Math.PI * E) / (slender * slender); // ksi
            const Fcr = 0.877 * Fe; // approximate inelastic/euler
            const Pn = (Fcr * shape.A) / 1000.0; // kips
            if (method === "LRFD") {
              Pcap = phi_c * Pn;
            } else {
              Pcap = Pn / Omega_c;
            }
            axialMode = "compression";
          } else {
            // Tension
            const Pn = (Fy * shape.A) / 1000.0; // kips
            if (method === "LRFD") {
              Pcap = phi_t * Pn;
            } else {
              Pcap = Pn / Omega_t;
            }
            axialMode = "tension";
          }
        }
        const axialRatio = Pcap > 0 ? Math.abs(Pu) / Pcap : 0;

        // Interaction (simplified linear)
        const flexRatioPos = Mcap > 0 ? Mu_in / Mcap : 0;
        const axialRatioPos = Pcap > 0 ? Math.abs(Pu) / Pcap : 0;
        const interactionRatio = flexRatioPos + axialRatioPos;

        // Overall status
        const maxRatio = Math.max(
          flexRatio,
          shearRatio,
          axialRatio,
          interactionRatio
        );
        const overall = document.getElementById("overallStatus");
        if (maxRatio <= 1.0) {
          overall.textContent = `Overall status: OK (max ratio = ${maxRatio.toFixed(
            2
          )} ≤ 1.00)`;
          overall.className = "mb-3 text-sm font-semibold text-emerald-700";
        } else {
          overall.textContent = `Overall status: NOT OK (max ratio = ${maxRatio.toFixed(
            2
          )} > 1.00)`;
          overall.className = "mb-3 text-sm font-semibold text-red-700";
        }

        // Fill summaries
        document.getElementById("flexSummary").textContent = `Mu = ${Mu.toFixed(
          2
        )} kip-ft, capacity = ${(Mcap / 12.0).toFixed(1)} kip-ft (approx.).`;
        document.getElementById(
          "flexRatio"
        ).textContent = `Flexural demand/capacity ratio = ${flexRatio.toFixed(
          2
        )}.`;

        document.getElementById(
          "shearSummary"
        ).textContent = `Vu = ${Vu.toFixed(2)} kips, capacity = ${Vcap.toFixed(
          1
        )} kips (approx.).`;
        document.getElementById(
          "shearRatio"
        ).textContent = `Shear demand/capacity ratio = ${shearRatio.toFixed(
          2
        )}.`;

        if (axialMode === "compression") {
          document.getElementById(
            "axialSummary"
          ).textContent = `Pu = ${Pu.toFixed(
            2
          )} kips (compression), capacity = ${Pcap.toFixed(1)} kips (approx.).`;
        } else if (axialMode === "tension") {
          document.getElementById(
            "axialSummary"
          ).textContent = `Pu = ${Pu.toFixed(
            2
          )} kips (tension), capacity = ${Pcap.toFixed(1)} kips (approx.).`;
        } else {
          document.getElementById("axialSummary").textContent =
            "No axial load specified.";
        }
        document.getElementById(
          "axialRatio"
        ).textContent = `Axial demand/capacity ratio = ${axialRatio.toFixed(
          2
        )}.`;

        document.getElementById(
          "interactionSummary"
        ).textContent = `Simplified interaction: (Pu/Pcap) + (Mu/Mcap) = ${interactionRatio.toFixed(
          2
        )}.`;
        document.getElementById(
          "interactionRatio"
        ).textContent = `Interaction ratio ≤ 1.0 is required for this simplified check.`;

        document.getElementById("resultsCard").classList.remove("hidden");
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
