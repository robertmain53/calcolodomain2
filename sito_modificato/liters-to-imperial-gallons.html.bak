    <script>
      (function () {
        const L_PER_UK_GAL = 4.54609;

        const litersInput = document.getElementById("litersInput");
        const gallonsInput = document.getElementById("gallonsInput");
        const precisionSelect = document.getElementById("precisionSelect");
        const swapButton = document.getElementById("swapButton");
        const clearButton = document.getElementById("clearButton");
        const tableBody = document.getElementById("conversionTableBody");

        function formatNumber(value, decimals) {
          if (!isFinite(value)) return "";
          const factor = Math.pow(10, decimals);
          return (Math.round(value * factor) / factor).toFixed(decimals);
        }

        function getPrecision() {
          const p = parseInt(precisionSelect.value, 10);
          return isNaN(p) ? 2 : p;
        }

        function convertFromLiters() {
          const liters = parseFloat(litersInput.value);
          if (isNaN(liters)) {
            gallonsInput.value = "";
            return;
          }
          const gallons = liters / L_PER_UK_GAL;
          gallonsInput.value = formatNumber(gallons, getPrecision());
        }

        function convertFromGallons() {
          const gallons = parseFloat(gallonsInput.value);
          if (isNaN(gallons)) {
            litersInput.value = "";
            return;
          }
          const liters = gallons * L_PER_UK_GAL;
          litersInput.value = formatNumber(liters, getPrecision());
        }

        litersInput.addEventListener("input", function () {
          convertFromLiters();
        });

        gallonsInput.addEventListener("input", function () {
          convertFromGallons();
        });

        precisionSelect.addEventListener("change", function () {
          if (litersInput.value !== "") {
            convertFromLiters();
          } else if (gallonsInput.value !== "") {
            convertFromGallons();
          }
          buildTable();
        });

        swapButton.addEventListener("click", function () {
          const lVal = litersInput.value;
          const gVal = gallonsInput.value;
          litersInput.value = gVal;
          gallonsInput.value = lVal;

          if (litersInput.value !== "") {
            convertFromLiters();
          } else if (gallonsInput.value !== "") {
            convertFromGallons();
          }
        });

        clearButton.addEventListener("click", function () {
          litersInput.value = "";
          gallonsInput.value = "";
        });

        function buildTable() {
          if (!tableBody) return;
          tableBody.innerHTML = "";
          const litersValues = [
            1, 2, 5, 10, 20, 25, 30, 40, 50, 60, 75, 100, 150, 200, 250,
          ];
          const decimals = 3;

          litersValues.forEach(function (l) {
            const g = l / L_PER_UK_GAL;
            const tr = document.createElement("tr");
            const tdL = document.createElement("td");
            const tdG = document.createElement("td");

            tdL.className = "px-3 py-1.5 text-gray-800";
            tdG.className = "px-3 py-1.5 text-gray-800";

            tdL.textContent = formatNumber(l, 0);
            tdG.textContent = formatNumber(g, decimals);

            tr.appendChild(tdL);
            tr.appendChild(tdG);
            tableBody.appendChild(tr);
          });
        }

        buildTable();
      })();
    </script>

