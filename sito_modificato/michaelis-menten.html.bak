    <script>
      // Mobile menu
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const expanded = this.getAttribute("aria-expanded") === "true";
          this.setAttribute("aria-expanded", String(!expanded));
          menu.classList.toggle("hidden");
        });

      // Mode toggle
      const modeSimpleBtn = document.getElementById("mode-simple");
      const modeFitBtn = document.getElementById("mode-fit");
      const simpleSection = document.getElementById("simple-section");
      const fitSection = document.getElementById("fit-section");

      modeSimpleBtn.addEventListener("click", () => {
        modeSimpleBtn.classList.add("bg-blue-600", "text-white");
        modeSimpleBtn.classList.remove("bg-white", "text-gray-700");
        modeFitBtn.classList.remove("bg-blue-600", "text-white");
        modeFitBtn.classList.add("bg-white", "text-gray-700");
        simpleSection.classList.remove("hidden");
        fitSection.classList.add("hidden");
      });

      modeFitBtn.addEventListener("click", () => {
        modeFitBtn.classList.add("bg-blue-600", "text-white");
        modeFitBtn.classList.remove("bg-white", "text-gray-700");
        modeSimpleBtn.classList.remove("bg-blue-600", "text-white");
        modeSimpleBtn.classList.add("bg-white", "text-gray-700");
        simpleSection.classList.add("hidden");
        fitSection.classList.remove("hidden");
      });

      // Simple calculator
      function formatNumber(x, digits = 3) {
        if (!isFinite(x)) return "—";
        if (x === 0) return "0";
        const abs = Math.abs(x);
        if (abs >= 0.01 && abs < 1e4) {
          return x.toFixed(digits);
        }
        return x.toExponential(digits);
      }

      document.getElementById("calc-v-btn").addEventListener("click", () => {
        const vmax = parseFloat(document.getElementById("vmax-input").value);
        const km = parseFloat(document.getElementById("km-input").value);
        const s = parseFloat(document.getElementById("s-input").value);

        if (
          !isFinite(vmax) ||
          !isFinite(km) ||
          !isFinite(s) ||
          vmax <= 0 ||
          km < 0 ||
          s < 0
        ) {
          alert("Please enter valid positive values for Vmax, Km and [S].");
          return;
        }

        const v = (vmax * s) / (km + s);
        const fraction = v / vmax;

        const vmaxUnitText =
          document.getElementById("vmax-unit").selectedOptions[0].textContent;
        document.getElementById("v-result").textContent = `${formatNumber(
          v
        )} ${vmaxUnitText}`;
        document.getElementById("fraction-result").textContent = formatNumber(
          fraction,
          3
        );
        document.getElementById("simple-results").classList.remove("hidden");
      });

      // Data table setup
      const dataBody = document.getElementById("data-table-body");
      function addRow(sVal = "", vVal = "") {
        const rowIndex = dataBody.children.length + 1;
        const tr = document.createElement("tr");
        tr.innerHTML = `
      <td class="px-3 py-1.5 border-b text-gray-500">${rowIndex}</td>
      <td class="px-3 py-1.5 border-b text-right"><input type="number" step="any" class="table-input" value="${sVal}"></td>
      <td class="px-3 py-1.5 border-b text-right"><input type="number" step="any" class="table-input" value="${vVal}"></td>
    `;
        dataBody.appendChild(tr);
      }

      // Initialize with 6 rows and example pattern
      [
        [1, 0.5],
        [2, 0.8],
        [5, 1.3],
        [10, 1.7],
        [20, 1.9],
        [40, 2.0],
      ].forEach((p) => addRow(p[0], p[1]));

      document
        .getElementById("add-row-btn")
        .addEventListener("click", () => addRow());

      // Nonlinear fit for Km and Vmax using simple gradient descent / LM-style
      function fitMichaelisMenten(data, maxIter = 500, learningRate = 1e-4) {
        // Initial guesses from simple heuristics
        const sVals = data.map((d) => d.s);
        const vVals = data.map((d) => d.v);
        const vmaxGuess = Math.max(...vVals);
        const kmGuess = sVals.reduce((a, b) => a + b, 0) / sVals.length;

        let Vmax = vmaxGuess || 1;
        let Km = kmGuess || 1;

        function model(s) {
          return (Vmax * s) / (Km + s);
        }

        function sse() {
          return data.reduce((sum, d) => {
            const r = d.v - model(d.s);
            return sum + r * r;
          }, 0);
        }

        let prevError = sse();

        for (let iter = 0; iter < maxIter; iter++) {
          let dV = 0;
          let dK = 0;
          for (const d of data) {
            const s = d.s;
            const vPred = model(s);
            const r = vPred - d.v;
            const denom = Km + s;
            const dv_dV = s / denom;
            const dv_dK = (-Vmax * s) / (denom * denom);
            dV += 2 * r * dv_dV;
            dK += 2 * r * dv_dK;
          }
          Vmax -= learningRate * dV;
          Km -= learningRate * dK;

          if (Vmax <= 0) Vmax = 1e-6;
          if (Km <= 0) Km = 1e-6;

          const err = sse();
          if (Math.abs(prevError - err) / (prevError + 1e-12) < 1e-6) break;
          prevError = err;
        }

        // R^2
        const meanV = vVals.reduce((a, b) => a + b, 0) / vVals.length;
        const ssTot = vVals.reduce((sum, v) => sum + (v - meanV) ** 2, 0);
        const ssRes = data.reduce((sum, d) => {
          const r = d.v - model(d.s);
          return sum + r * r;
        }, 0);
        const r2 = ssTot === 0 ? 1 : 1 - ssRes / ssTot;

        return { Km, Vmax, r2, model };
      }

      let mmChart, lbChart, ehChart;

      function destroyCharts() {
        [mmChart, lbChart, ehChart].forEach((ch) => {
          if (ch) ch.destroy();
        });
        mmChart = lbChart = ehChart = null;
      }

      document.getElementById("fit-btn").addEventListener("click", () => {
        const rows = Array.from(dataBody.querySelectorAll("tr"));
        const data = [];
        rows.forEach((tr) => {
          const inputs = tr.querySelectorAll("input");
          const s = parseFloat(inputs[0].value);
          const v = parseFloat(inputs[1].value);
          if (isFinite(s) && isFinite(v) && s > 0 && v > 0) {
            data.push({ s, v });
          }
        });

        const errorSpan = document.getElementById("fit-error");
        errorSpan.textContent = "";

        if (data.length < 3) {
          errorSpan.textContent =
            "Enter at least 3 valid data points with positive [S] and v.";
          return;
        }

        const { Km, Vmax, r2, model } = fitMichaelisMenten(data);

        document.getElementById("fit-km").textContent =
          formatNumber(Km) + " (same units as [S])";
        document.getElementById("fit-vmax").textContent =
          formatNumber(Vmax) + " (same units as v)";
        document.getElementById("fit-r2").textContent = formatNumber(r2, 4);
        document.getElementById("fit-results").classList.remove("hidden");

        // Build smooth curve
        const sMin = 0;
        const sMax = Math.max(...data.map((d) => d.s)) * 1.1;
        const curveS = [];
        const curveV = [];
        const nPts = 100;
        for (let i = 0; i <= nPts; i++) {
          const s = sMin + ((sMax - sMin) * i) / nPts;
          curveS.push(s);
          curveV.push(model(s));
        }

        destroyCharts();

        const commonOptions = {
          responsive: true,
          maintainAspectRatio: false,
          plugins: { legend: { display: true } },
          scales: {
            x: { title: { display: true, text: "" } },
            y: { title: { display: true, text: "" } },
          },
        };

        // Michaelis-Menten chart
        mmChart = new Chart(document.getElementById("mm-chart"), {
          type: "scatter",
          data: {
            datasets: [
              {
                label: "Data",
                data: data.map((d) => ({ x: d.s, y: d.v })),
                backgroundColor: "rgba(37,99,235,0.9)",
                pointRadius: 4,
              },
              {
                label: "Fitted curve",
                data: curveS.map((s, i) => ({ x: s, y: curveV[i] })),
                type: "line",
                borderColor: "rgba(16,185,129,1)",
                borderWidth: 2,
                pointRadius: 0,
              },
            ],
          },
          options: JSON.parse(JSON.stringify(commonOptions)),
        });
        mmChart.options.scales.x.title.text = "[S]";
        mmChart.options.scales.y.title.text = "v";

        // Lineweaver-Burk
        const lbData = data
          .filter((d) => d.s > 0 && d.v > 0)
          .map((d) => ({ x: 1 / d.s, y: 1 / d.v }));
        lbChart = new Chart(document.getElementById("lb-chart"), {
          type: "scatter",
          data: {
            datasets: [
              {
                label: "Data (1/v vs 1/[S])",
                data: lbData,
                backgroundColor: "rgba(239,68,68,0.9)",
                pointRadius: 4,
              },
            ],
          },
          options: JSON.parse(JSON.stringify(commonOptions)),
        });
        lbChart.options.scales.x.title.text = "1/[S]";
        lbChart.options.scales.y.title.text = "1/v";

        // Eadie-Hofstee
        const ehData = data
          .filter((d) => d.s > 0)
          .map((d) => ({ x: d.v / d.s, y: d.v }));
        ehChart = new Chart(document.getElementById("eh-chart"), {
          type: "scatter",
          data: {
            datasets: [
              {
                label: "Data (v vs v/[S])",
                data: ehData,
                backgroundColor: "rgba(234,179,8,0.9)",
                pointRadius: 4,
              },
            ],
          },
          options: JSON.parse(JSON.stringify(commonOptions)),
        });
        ehChart.options.scales.x.title.text = "v/[S]";
        ehChart.options.scales.y.title.text = "v";
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
      if (window.__mathjaxLoading) return;
      window.__mathjaxLoading = true;
      var script = document.createElement("script");
      script.id = "MathJax-script";
      script.async = true;
      script.src = "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js";
      document.head.appendChild(script);
    };

    var target = document.querySelector(
      ".formula-box, .mathjax, .math, .formula, [data-mathjax]"
    );

    if (!target) return;

    if ("IntersectionObserver" in window) {
      var observer = new IntersectionObserver(
        function (entries) {
          if (entries.some(function (entry) { return entry.isIntersecting; })) {
            observer.disconnect();
            loadMathJax();
          }
        },
        { rootMargin: "200px" }
      );
      observer.observe(target);
    } else {
      window.addEventListener(
        "load",
        function () {
          setTimeout(loadMathJax, 1000);
        },
        { once: true }
      );
    }
  })();
</script>
  
