    <script>
      // Utility functions
      function fmtMoney(v) {
        if (!isFinite(v)) return "—";
        return (
          "$" +
          v.toLocaleString(undefined, {
            maximumFractionDigits: 2,
            minimumFractionDigits: 2,
          })
        );
      }
      function fmtYearsMonths(months) {
        if (!isFinite(months) || months <= 0) return "0 months";
        const y = Math.floor(months / 12);
        const m = Math.round(months % 12);
        const parts = [];
        if (y > 0) parts.push(y + " year" + (y !== 1 ? "s" : ""));
        if (m > 0) parts.push(m + " month" + (m !== 1 ? "s" : ""));
        return parts.join(" ");
      }

      // Amortization for fixed payment
      function amortize(balance, rateAnnual, payment, maxMonths = 1000 * 12) {
        const i = rateAnnual / 100 / 12;
        let b = balance;
        let month = 0;
        let totalInterest = 0;
        const schedule = [];
        while (b > 0 && month < maxMonths) {
          const interest = b * i;
          let principal = payment - interest;
          if (principal <= 0) {
            // Payment too small; avoid infinite loop
            break;
          }
          if (principal > b) {
            principal = b;
          }
          b -= principal;
          totalInterest += interest;
          month++;
          schedule.push({ month, interest, principal, balance: b < 0 ? 0 : b });
          if (b <= 0.01) {
            b = 0;
            break;
          }
        }
        return { schedule, totalInterest, months: month };
      }

      // Solve payment for given term
      function solvePayment(balance, rateAnnual, months) {
        const i = rateAnnual / 100 / 12;
        if (months <= 0) return NaN;
        if (i === 0) return balance / months;
        const pow = Math.pow(1 + i, months);
        return (balance * i * pow) / (pow - 1);
      }

      // Strategy handling
      const strategyPills = document.querySelectorAll(".strategy-pill");
      const strategyPanels = document.querySelectorAll(".strategy-panel");
      let currentMode = "extra-monthly";

      strategyPills.forEach((pill) => {
        pill.addEventListener("click", () => {
          currentMode = pill.dataset.mode;
          strategyPills.forEach((p) => {
            p.classList.remove(
              "border-blue-500",
              "text-blue-600",
              "bg-blue-50"
            );
            p.classList.add("border-gray-300", "text-gray-700", "bg-white");
          });
          pill.classList.remove("border-gray-300", "text-gray-700", "bg-white");
          pill.classList.add("border-blue-500", "text-blue-600", "bg-blue-50");
          strategyPanels.forEach((panel) => panel.classList.add("hidden"));
          const activePanel = document.getElementById("panel-" + currentMode);
          if (activePanel) activePanel.classList.remove("hidden");
        });
      });

      const loanBalanceEl = document.getElementById("loanBalance");
      const interestRateEl = document.getElementById("interestRate");
      const originalTermEl = document.getElementById("originalTerm");
      const yearsElapsedEl = document.getElementById("yearsElapsed");
      const monthlyPaymentEl = document.getElementById("monthlyPayment");
      const remainingTermLabel = document.getElementById("remainingTermLabel");
      const errorMsgEl = document.getElementById("errorMsg");

      function updateRemainingTermLabel() {
        const orig = parseFloat(originalTermEl.value);
        const elapsed = parseFloat(yearsElapsedEl.value);
        if (!isFinite(orig) || !isFinite(elapsed)) {
          remainingTermLabel.textContent = "—";
          return;
        }
        const remainingYears = Math.max(orig - elapsed, 0);
        const remainingMonths = Math.round(remainingYears * 12);
        remainingTermLabel.textContent =
          fmtYearsMonths(remainingMonths) + " (" + remainingMonths + " months)";
      }

      originalTermEl.addEventListener("input", updateRemainingTermLabel);
      yearsElapsedEl.addEventListener("input", updateRemainingTermLabel);
      updateRemainingTermLabel();

      document.getElementById("resetBtn").addEventListener("click", () => {
        loanBalanceEl.value = 300000;
        interestRateEl.value = 6;
        originalTermEl.value = 30;
        yearsElapsedEl.value = 5;
        monthlyPaymentEl.value = "";
        document.getElementById("extraMonthly").value = 200;
        document.getElementById("lumpSum").value = 10000;
        document.getElementById("lumpSumMonth").value = 1;
        document.getElementById("targetYears").value = 15;
        errorMsgEl.textContent = "";
        document.getElementById("resultsSection").classList.add("hidden");
        updateRemainingTermLabel();
      });

      document.getElementById("calculateBtn").addEventListener("click", () => {
        errorMsgEl.textContent = "";
        const balance = parseFloat(loanBalanceEl.value);
        const rate = parseFloat(interestRateEl.value);
        const origYears = parseFloat(originalTermEl.value);
        const elapsedYears = parseFloat(yearsElapsedEl.value || 0);
        let payment = parseFloat(monthlyPaymentEl.value);

        if (!isFinite(balance) || balance <= 0) {
          errorMsgEl.textContent = "Enter a valid current loan balance.";
          return;
        }
        if (!isFinite(rate) || rate < 0) {
          errorMsgEl.textContent = "Enter a valid interest rate.";
          return;
        }
        if (!isFinite(origYears) || origYears <= 0) {
          errorMsgEl.textContent = "Enter a valid original term in years.";
          return;
        }
        const remainingMonths = Math.max(
          Math.round((origYears - elapsedYears) * 12),
          1
        );
        const i = rate / 100 / 12;

        if (!isFinite(payment) || payment <= 0) {
          // Auto-calc payment from remaining term
          if (i === 0) {
            payment = balance / remainingMonths;
          } else {
            payment = solvePayment(balance, rate, remainingMonths);
          }
          monthlyPaymentEl.value = payment.toFixed(2);
        }

        // Original schedule
        const orig = amortize(balance, rate, payment);
        const origMonths = orig.months;
        const origInterest = orig.totalInterest;

        // New schedule based on strategy
        let newSchedule,
          newInterest,
          newMonths,
          effectiveMonthly,
          extraTotal = 0;

        if (currentMode === "extra-monthly") {
          const extra = Math.max(
            parseFloat(document.getElementById("extraMonthly").value || 0),
            0
          );
          effectiveMonthly = payment + extra;
          const res = amortize(balance, rate, effectiveMonthly);
          newSchedule = res.schedule;
          newInterest = res.totalInterest;
          newMonths = res.months;
          // Extra paid = extra * number of months actually paid
          extraTotal = extra * newMonths;
        } else if (currentMode === "lump-sum") {
          const lump = Math.max(
            parseFloat(document.getElementById("lumpSum").value || 0),
            0
          );
          const when = Math.max(
            parseInt(document.getElementById("lumpSumMonth").value || "1", 10),
            1
          );
          // First, pay standard schedule until lump sum month
          const pre = amortize(balance, rate, payment, when);
          let b =
            pre.schedule.length > 0
              ? pre.schedule[pre.schedule.length - 1].balance
              : balance;
          if (lump > 0) {
            b = Math.max(b - lump, 0);
          }
          const post = amortize(b, rate, payment);
          newSchedule = pre.schedule.concat(
            post.schedule.map((p, idx) => ({
              month: pre.schedule.length + idx + 1,
              interest: p.interest,
              principal: p.principal,
              balance: p.balance,
            }))
          );
          newInterest = pre.totalInterest + post.totalInterest;
          newMonths = newSchedule.length;
          effectiveMonthly = payment; // monthly stays same
          extraTotal = lump;
        } else if (currentMode === "biweekly") {
          // Biweekly: 13 payments per year instead of 12 => effective extra payment per year
          const annualPayment = payment * 12;
          const biweeklyPayment = payment / 2;
          const biweeklyAnnual = biweeklyPayment * 26;
          const extraPerYear = biweeklyAnnual - annualPayment;
          const extraPerMonthApprox = extraPerYear / 12;
          effectiveMonthly = payment + extraPerMonthApprox;
          const res = amortize(balance, rate, effectiveMonthly);
          newSchedule = res.schedule;
          newInterest = res.totalInterest;
          newMonths = res.months;
          extraTotal = extraPerMonthApprox * newMonths;
        } else if (currentMode === "target-date") {
          const targetYears = parseFloat(
            document.getElementById("targetYears").value || 0
          );
          if (!isFinite(targetYears) || targetYears <= 0) {
            errorMsgEl.textContent = "Enter a valid target payoff timeframe.";
            return;
          }
          const targetMonthsFromStart = Math.round(targetYears * 12);
          const monthsAlreadyPaid = Math.round(elapsedYears * 12);
          const remainingTargetMonths = Math.max(
            targetMonthsFromStart - monthsAlreadyPaid,
            1
          );
          effectiveMonthly = solvePayment(balance, rate, remainingTargetMonths);
          if (!isFinite(effectiveMonthly) || effectiveMonthly <= payment) {
            errorMsgEl.textContent =
              "Target payoff is not earlier than your current schedule, or rate/payment combination is invalid.";
            return;
          }
          const res = amortize(balance, rate, effectiveMonthly);
          newSchedule = res.schedule;
          newInterest = res.totalInterest;
          newMonths = res.months;
          extraTotal = (effectiveMonthly - payment) * newMonths;
        }

        // Populate results
        document.getElementById("origMonthly").textContent = fmtMoney(payment);
        document.getElementById("origTerm").textContent =
          fmtYearsMonths(origMonths) + " (" + origMonths + " months)";
        document.getElementById("origInterest").textContent =
          fmtMoney(origInterest);

        document.getElementById("newMonthly").textContent =
          fmtMoney(effectiveMonthly);
        document.getElementById("newTerm").textContent =
          fmtYearsMonths(newMonths) + " (" + newMonths + " months)";
        document.getElementById("newInterest").textContent =
          fmtMoney(newInterest);

        const timeSavedMonths = Math.max(origMonths - newMonths, 0);
        const interestSaved = Math.max(origInterest - newInterest, 0);

        document.getElementById("timeSaved").textContent =
          timeSavedMonths > 0 ? fmtYearsMonths(timeSavedMonths) : "0";
        document.getElementById("interestSaved").textContent =
          fmtMoney(interestSaved);
        document.getElementById("extraPaid").textContent = fmtMoney(extraTotal);

        // Amortization snapshot by year (up to 10 years)
        const amortBody = document.getElementById("amortTableBody");
        amortBody.innerHTML = "";
        const maxYears = 10;
        for (let year = 1; year <= maxYears; year++) {
          const startMonth = (year - 1) * 12;
          const endMonth = year * 12;
          let oPrin = 0,
            oInt = 0,
            nPrin = 0,
            nInt = 0;
          for (let m = startMonth; m < endMonth; m++) {
            if (orig.schedule[m]) {
              oPrin += orig.schedule[m].principal;
              oInt += orig.schedule[m].interest;
            }
            if (newSchedule[m]) {
              nPrin += newSchedule[m].principal;
              nInt += newSchedule[m].interest;
            }
          }
          if (oPrin === 0 && nPrin === 0 && year > 1) break;
          const lastNew =
            newSchedule[Math.min(endMonth - 1, newSchedule.length - 1)];
          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="border px-2 py-1">${year}</td>
        <td class="border px-2 py-1 text-right">${fmtMoney(oPrin)}</td>
        <td class="border px-2 py-1 text-right">${fmtMoney(oInt)}</td>
        <td class="border px-2 py-1 text-right">${fmtMoney(nPrin)}</td>
        <td class="border px-2 py-1 text-right">${fmtMoney(nInt)}</td>
        <td class="border px-2 py-1 text-right">${
          lastNew ? fmtMoney(lastNew.balance) : "—"
        }</td>
      `;
          amortBody.appendChild(tr);
        }

        document.getElementById("resultsSection").classList.remove("hidden");
      });

      // Mobile menu script (simple)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", () => {
          const menu = document.getElementById("mobile-menu");
          const expanded = menu.classList.contains("hidden") ? "true" : "false";
          menu.classList.toggle("hidden");
          document
            .getElementById("mobile-menu-toggle")
            .setAttribute("aria-expanded", expanded);
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
