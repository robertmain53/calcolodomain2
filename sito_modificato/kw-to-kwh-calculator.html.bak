    <script>
      // Simple kW -> kWh converter
      function updateSimpleKwh() {
        const kW = parseFloat(document.getElementById("powerKw").value);
        const hours = parseFloat(document.getElementById("timeHours").value);
        const resultBox = document.getElementById("simple-result");
        const kwhSpan = document.getElementById("simple-kwh");

        if (isNaN(kW) || isNaN(hours) || kW < 0 || hours < 0) {
          resultBox.classList.add("hidden");
          return;
        }
        const kwh = kW * hours;
        kwhSpan.textContent = kwh.toFixed(4).replace(/\.?0+$/, "");
        resultBox.classList.remove("hidden");
      }

      document
        .getElementById("btn-calc-simple")
        .addEventListener("click", updateSimpleKwh);
      document
        .getElementById("powerKw")
        .addEventListener("input", updateSimpleKwh);
      document
        .getElementById("timeHours")
        .addEventListener("input", updateSimpleKwh);

      document
        .getElementById("btn-reset-simple")
        .addEventListener("click", () => {
          document.getElementById("powerKw").value = "";
          document.getElementById("timeHours").value = "";
          document.getElementById("simple-result").classList.add("hidden");
        });

      // Multi-appliance calculator
      const applianceTbody = document.getElementById("appliance-rows");
      const totalKwhEl = document.getElementById("total-kwh");
      const totalCostEl = document.getElementById("total-cost");
      const costPerKwhEl = document.getElementById("costPerKwh");

      function createApplianceRow(defaults = {}) {
        const tr = document.createElement("tr");

        tr.innerHTML = `
      <td class="px-3 py-2">
        <input type="text" class="w-full border border-gray-300 rounded-md px-2 py-1 text-sm focus:ring-1 focus:ring-blue-500 focus:outline-none" placeholder="e.g. Space heater" value="${
          defaults.name || ""
        }">
      </td>
      <td class="px-3 py-2">
        <div class="flex items-center gap-2">
          <input type="number" min="0" step="0.001" class="w-full border border-gray-300 rounded-md px-2 py-1 text-sm focus:ring-1 focus:ring-blue-500 focus:outline-none appliance-power" value="${
            defaults.power || ""
          }">
          <select class="border border-gray-300 rounded-md px-1.5 py-1 text-xs appliance-power-unit">
            <option value="kW"${
              defaults.unit === "kW" || !defaults.unit ? " selected" : ""
            }>kW</option>
            <option value="W"${
              defaults.unit === "W" ? " selected" : ""
            }>W</option>
          </select>
        </div>
      </td>
      <td class="px-3 py-2">
        <input type="number" min="0" step="0.01" class="w-full border border-gray-300 rounded-md px-2 py-1 text-sm focus:ring-1 focus:ring-blue-500 focus:outline-none appliance-hours" value="${
          defaults.hours || ""
        }">
      </td>
      <td class="px-3 py-2">
        <input type="number" min="0" step="1" class="w-full border border-gray-300 rounded-md px-2 py-1 text-sm focus:ring-1 focus:ring-blue-500 focus:outline-none appliance-days" value="${
          defaults.days || "1"
        }">
      </td>
      <td class="px-3 py-2">
        <input type="number" min="0" max="100" step="1" class="w-full border border-gray-300 rounded-md px-2 py-1 text-sm focus:ring-1 focus:ring-blue-500 focus:outline-none appliance-duty" value="${
          defaults.duty != null ? defaults.duty : "100"
        }">
      </td>
      <td class="px-3 py-2">
        <span class="appliance-kwh font-semibold text-gray-800">0</span>
      </td>
      <td class="px-3 py-2 text-right">
        <button type="button" class="remove-row-btn text-xs text-red-600 border border-red-200 rounded-md px-2 py-1 hover:text-red-700">
          Remove
        </button>
      </td>
    `;

        applianceTbody.appendChild(tr);
      }

      function recalcAppliances() {
        let totalKwh = 0;

        applianceTbody.querySelectorAll("tr").forEach((row) => {
          const powerInput = row.querySelector(".appliance-power");
          const unitSelect = row.querySelector(".appliance-power-unit");
          const hoursInput = row.querySelector(".appliance-hours");
          const daysInput = row.querySelector(".appliance-days");
          const dutyInput = row.querySelector(".appliance-duty");
          const kwhSpan = row.querySelector(".appliance-kwh");

          let power = parseFloat(powerInput.value);
          const hours = parseFloat(hoursInput.value);
          const days = parseFloat(daysInput.value);
          const duty = parseFloat(dutyInput.value);

          if (
            isNaN(power) ||
            isNaN(hours) ||
            isNaN(days) ||
            isNaN(duty) ||
            power < 0 ||
            hours < 0 ||
            days < 0 ||
            duty < 0
          ) {
            kwhSpan.textContent = "0";
            return;
          }

          // Convert W to kW if needed
          if (unitSelect.value === "W") {
            power = power / 1000;
          }

          const dutyFactor = Math.min(Math.max(duty, 0), 100) / 100;
          const kwh = power * hours * days * dutyFactor;

          kwhSpan.textContent = kwh.toFixed(3).replace(/\.?0+$/, "");
          totalKwh += kwh;
        });

        totalKwhEl.textContent = totalKwh.toFixed(3).replace(/\.?0+$/, "");

        const rate = parseFloat(costPerKwhEl.value);
        if (!isNaN(rate) && rate >= 0) {
          const cost = totalKwh * rate;
          totalCostEl.textContent = cost.toFixed(2);
        } else {
          totalCostEl.textContent = "0.00";
        }
      }

      document.getElementById("btn-add-row").addEventListener("click", () => {
        createApplianceRow();
      });

      document
        .getElementById("btn-reset-rows")
        .addEventListener("click", () => {
          applianceTbody.innerHTML = "";
          totalKwhEl.textContent = "0";
          totalCostEl.textContent = "0.00";
          createDefaultRows();
        });

      applianceTbody.addEventListener("input", (e) => {
        if (e.target.matches("input") || e.target.matches("select")) {
          recalcAppliances();
        }
      });

      applianceTbody.addEventListener("click", (e) => {
        if (e.target.classList.contains("remove-row-btn")) {
          const row = e.target.closest("tr");
          row.remove();
          recalcAppliances();
        }
      });

      costPerKwhEl.addEventListener("input", recalcAppliances);

      function createDefaultRows() {
        createApplianceRow({
          name: "Space heater",
          power: 1.5,
          unit: "kW",
          hours: 4,
          days: 1,
          duty: 100,
        });
        createApplianceRow({
          name: "Fridge",
          power: 150,
          unit: "W",
          hours: 24,
          days: 1,
          duty: 35,
        });
        recalcAppliances();
      }

      // Initialize with two example rows
      createDefaultRows();
    </script>

