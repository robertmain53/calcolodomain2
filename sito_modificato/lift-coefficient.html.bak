    <script>
      // Utility: unit conversions
      const g = 9.80665; // m/s^2

      function toSIForce(value, unit) {
        if (value == null || isNaN(value)) return null;
        if (unit === "N") return value;
        if (unit === "lbf") return value * 4.4482216153;
        return null;
      }

      function fromSIForce(valueN, unit) {
        if (unit === "N") return valueN;
        if (unit === "lbf") return valueN / 4.4482216153;
        return valueN;
      }

      function toSIDensity(value, unit) {
        if (value == null || isNaN(value)) return null;
        if (unit === "kg_m3") return value;
        if (unit === "slug_ft3") return value * 515.378818; // 1 slug/ft^3 ≈ 515.3788 kg/m^3
        return null;
      }

      function fromSIDensity(valueKgM3, unit) {
        if (unit === "kg_m3") return valueKgM3;
        if (unit === "slug_ft3") return valueKgM3 / 515.378818;
        return valueKgM3;
      }

      function toSIArea(value, unit) {
        if (value == null || isNaN(value)) return null;
        if (unit === "m2") return value;
        if (unit === "ft2") return value * 0.09290304;
        return null;
      }

      function fromSIArea(valueM2, unit) {
        if (unit === "m2") return valueM2;
        if (unit === "ft2") return valueM2 / 0.09290304;
        return valueM2;
      }

      function toSISpeed(value, unit) {
        if (value == null || isNaN(value)) return null;
        switch (unit) {
          case "m_s":
            return value;
          case "ft_s":
            return value * 0.3048;
          case "kt":
            return value * 0.514444;
          case "km_h":
            return value * (1000 / 3600);
          case "mph":
            return value * 0.44704;
          default:
            return null;
        }
      }

      function fromSISpeed(valueMS, unit) {
        switch (unit) {
          case "m_s":
            return valueMS;
          case "ft_s":
            return valueMS / 0.3048;
          case "kt":
            return valueMS / 0.514444;
          case "km_h":
            return valueMS * 3.6;
          case "mph":
            return valueMS / 0.44704;
          default:
            return valueMS;
        }
      }

      function toSIDynamicPressure(value, unit) {
        if (value == null || isNaN(value)) return null;
        if (unit === "Pa") return value;
        if (unit === "psf") return value * 47.880258; // 1 lb/ft^2 ≈ 47.8803 Pa
        return null;
      }

      function fromSIDynamicPressure(valuePa, unit) {
        if (unit === "Pa") return valuePa;
        if (unit === "psf") return valuePa / 47.880258;
        return valuePa;
      }

      // UI state
      let currentMode = "cl"; // cl, lift, speed, density, area
      let currentSystem = "si"; // si, imperial

      const modeButtons = document.querySelectorAll("[data-mode]");
      const unitButtons = {
        si: document.getElementById("unit-si"),
        imperial: document.getElementById("unit-imperial"),
      };

      modeButtons.forEach((btn) => {
        btn.addEventListener("click", () => {
          currentMode = btn.getAttribute("data-mode");
          modeButtons.forEach((b) => {
            b.classList.remove("tab-active");
            b.classList.remove("text-blue-600");
          });
          btn.classList.add("tab-active");
          updatePlaceholders();
          clearError();
        });
      });

      unitButtons.si.addEventListener("click", () => setUnitSystem("si"));
      unitButtons.imperial.addEventListener("click", () =>
        setUnitSystem("imperial")
      );

      function setUnitSystem(system) {
        currentSystem = system;
        unitButtons.si.classList.toggle("tab-active", system === "si");
        unitButtons.imperial.classList.toggle(
          "tab-active",
          system === "imperial"
        );

        const rhoUnit = document.getElementById("rho-unit");
        const areaUnit = document.getElementById("area-unit");
        const liftUnit = document.getElementById("lift-unit");
        const speedUnit = document.getElementById("speed-unit");
        const qUnit = document.getElementById("q-unit");

        if (system === "si") {
          rhoUnit.value = "kg_m3";
          areaUnit.value = "m2";
          liftUnit.value = "N";
          speedUnit.value = "m_s";
          qUnit.value = "Pa";
        } else {
          rhoUnit.value = "slug_ft3";
          areaUnit.value = "ft2";
          liftUnit.value = "lbf";
          speedUnit.value = "ft_s";
          qUnit.value = "psf";
        }
      }

      function updatePlaceholders() {
        const clInput = document.getElementById("cl-input");
        const liftInput = document.getElementById("lift-input");
        const speedInput = document.getElementById("speed-input");
        const rhoInput = document.getElementById("rho-input");
        const areaInput = document.getElementById("area-input");

        if (currentMode === "cl") {
          clInput.placeholder = "Cl (result)";
        } else if (currentMode === "lift") {
          liftInput.placeholder = "Lift L (result)";
        } else if (currentMode === "speed") {
          speedInput.placeholder = "Speed V (result)";
        } else if (currentMode === "density") {
          rhoInput.placeholder = "Density ρ (result)";
        } else if (currentMode === "area") {
          areaInput.placeholder = "Area S (result)";
        }
      }

      function setStdAtmosphere() {
        const rhoInput = document.getElementById("rho-input");
        const rhoUnit = document.getElementById("rho-unit").value;
        if (rhoUnit === "kg_m3") {
          rhoInput.value = 1.225;
        } else {
          // convert 1.225 kg/m3 to slug/ft3
          rhoInput.value = (1.225 / 515.378818).toFixed(5);
        }
      }

      document
        .getElementById("btn-std-atm")
        .addEventListener("click", setStdAtmosphere);

      function showError(msg) {
        const el = document.getElementById("error-msg");
        el.textContent = msg;
      }

      function clearError() {
        document.getElementById("error-msg").textContent = "";
      }

      function parseInput(id) {
        const v = parseFloat(document.getElementById(id).value);
        return isNaN(v) ? null : v;
      }

      function calculate() {
        clearError();
        const L_val = parseInput("lift-input");
        const rho_val = parseInput("rho-input");
        const V_val = parseInput("speed-input");
        const S_val = parseInput("area-input");
        const Cl_val = parseInput("cl-input");
        const q_val = parseInput("q-input");

        const liftUnit = document.getElementById("lift-unit").value;
        const rhoUnit = document.getElementById("rho-unit").value;
        const speedUnit = document.getElementById("speed-unit").value;
        const areaUnit = document.getElementById("area-unit").value;
        const qUnit = document.getElementById("q-unit").value;

        let L_SI = L_val != null ? toSIForce(L_val, liftUnit) : null;
        let rho_SI = rho_val != null ? toSIDensity(rho_val, rhoUnit) : null;
        let V_SI = V_val != null ? toSISpeed(V_val, speedUnit) : null;
        let S_SI = S_val != null ? toSIArea(S_val, areaUnit) : null;
        let q_SI = q_val != null ? toSIDynamicPressure(q_val, qUnit) : null;

        // Determine which variable to solve for
        let resultText = "";
        let secondaryText = "";
        let steps = "";

        try {
          if (currentMode === "cl") {
            if (
              L_SI == null ||
              S_SI == null ||
              (rho_SI == null && V_SI == null && q_SI == null)
            ) {
              showError(
                "Please enter lift, density (or dynamic pressure), speed, and area to compute Cl."
              );
              return;
            }
            if (q_SI == null) {
              if (rho_SI == null || V_SI == null) {
                showError(
                  "Provide either dynamic pressure q or both density ρ and speed V."
                );
                return;
              }
              q_SI = 0.5 * rho_SI * V_SI * V_SI;
            }
            const Cl = L_SI / (q_SI * S_SI);
            if (!isFinite(Cl)) {
              showError(
                "Inputs lead to an invalid Cl (division by zero). Check S and q."
              );
              return;
            }
            document.getElementById("cl-input").value = Cl.toFixed(4);
            resultText = `Lift coefficient C\u2097 = ${Cl.toFixed(4)}`;
            secondaryText = `Using L = ${L_SI.toFixed(2)} N, q = ${q_SI.toFixed(
              2
            )} Pa, S = ${S_SI.toFixed(3)} m².`;
            steps =
              `1. Compute dynamic pressure q:\n` +
              (q_val != null
                ? `   q = ${q_val} ${qUnit} = ${q_SI.toFixed(
                    2
                  )} Pa (converted)\n`
                : `   q = 0.5 · ρ · V² = 0.5 · ${rho_SI.toFixed(
                    3
                  )} · (${V_SI.toFixed(2)})² = ${q_SI.toFixed(2)} Pa\n`) +
              `2. Apply C_L = L / (q · S):\n` +
              `   C_L = ${L_SI.toFixed(2)} / (${q_SI.toFixed(
                2
              )} · ${S_SI.toFixed(3)}) ≈ ${Cl.toFixed(4)}.`;
          } else if (currentMode === "lift") {
            if (
              Cl_val == null ||
              S_SI == null ||
              (rho_SI == null && V_SI == null && q_SI == null)
            ) {
              showError(
                "Please enter Cl, density (or dynamic pressure), speed, and area to compute lift."
              );
              return;
            }
            if (q_SI == null) {
              if (rho_SI == null || V_SI == null) {
                showError(
                  "Provide either dynamic pressure q or both density ρ and speed V."
                );
                return;
              }
              q_SI = 0.5 * rho_SI * V_SI * V_SI;
            }
            const L = q_SI * S_SI * Cl_val;
            if (!isFinite(L)) {
              showError(
                "Inputs lead to an invalid lift value. Check S, q, and Cl."
              );
              return;
            }
            const L_out = fromSIForce(L, liftUnit);
            document.getElementById("lift-input").value = L_out.toFixed(2);
            resultText = `Lift L = ${L_out.toFixed(2)} ${liftUnit}`;
            secondaryText = `Using C\u2097 = ${Cl_val}, q = ${q_SI.toFixed(
              2
            )} Pa, S = ${S_SI.toFixed(3)} m².`;
            steps =
              `1. Compute dynamic pressure q:\n` +
              (q_val != null
                ? `   q = ${q_val} ${qUnit} = ${q_SI.toFixed(
                    2
                  )} Pa (converted)\n`
                : `   q = 0.5 · ρ · V² = 0.5 · ${rho_SI.toFixed(
                    3
                  )} · (${V_SI.toFixed(2)})² = ${q_SI.toFixed(2)} Pa\n`) +
              `2. Apply L = q · S · C_L:\n` +
              `   L = ${q_SI.toFixed(2)} · ${S_SI.toFixed(
                3
              )} · ${Cl_val} = ${L.toFixed(2)} N\n` +
              `   Converted to ${liftUnit}: ${L_out.toFixed(2)} ${liftUnit}.`;
          } else if (currentMode === "speed") {
            if (
              Cl_val == null ||
              S_SI == null ||
              rho_SI == null ||
              L_SI == null
            ) {
              showError(
                "Please enter lift, density, area, and Cl to compute speed."
              );
              return;
            }
            const V = Math.sqrt((2 * L_SI) / (rho_SI * S_SI * Cl_val));
            if (!isFinite(V)) {
              showError(
                "Inputs lead to an invalid speed. Check that ρ, S, and Cl are positive."
              );
              return;
            }
            const V_out = fromSISpeed(V, speedUnit);
            document.getElementById("speed-input").value = V_out.toFixed(3);
            resultText = `True airspeed V = ${V_out.toFixed(3)} ${speedUnit}`;
            secondaryText = `Using L = ${L_SI.toFixed(
              2
            )} N, ρ = ${rho_SI.toFixed(3)} kg/m³, S = ${S_SI.toFixed(
              3
            )} m², C\u2097 = ${Cl_val}.`;
            steps =
              `1. Rearrange L = 0.5 · ρ · V² · S · C_L for V:\n` +
              `   V = sqrt(2L / (ρ · S · C_L))\n` +
              `2. Substitute values:\n` +
              `   V = sqrt(2 · ${L_SI.toFixed(2)} / (${rho_SI.toFixed(
                3
              )} · ${S_SI.toFixed(3)} · ${Cl_val})) ≈ ${V.toFixed(3)} m/s\n` +
              `   Converted to ${speedUnit}: ${V_out.toFixed(3)} ${speedUnit}.`;
          } else if (currentMode === "density") {
            if (
              Cl_val == null ||
              S_SI == null ||
              V_SI == null ||
              L_SI == null
            ) {
              showError(
                "Please enter lift, speed, area, and Cl to compute density."
              );
              return;
            }
            const rho = (2 * L_SI) / (V_SI * V_SI * S_SI * Cl_val);
            if (!isFinite(rho)) {
              showError(
                "Inputs lead to an invalid density. Check that V, S, and Cl are positive."
              );
              return;
            }
            const rho_out = fromSIDensity(rho, rhoUnit);
            document.getElementById("rho-input").value = rho_out.toFixed(4);
            resultText = `Air density ρ = ${rho_out.toFixed(4)} ${
              rhoUnit === "kg_m3" ? "kg/m³" : "slug/ft³"
            }`;
            secondaryText = `Using L = ${L_SI.toFixed(2)} N, V = ${V_SI.toFixed(
              3
            )} m/s, S = ${S_SI.toFixed(3)} m², C\u2097 = ${Cl_val}.`;
            steps =
              `1. Rearrange L = 0.5 · ρ · V² · S · C_L for ρ:\n` +
              `   ρ = 2L / (V² · S · C_L)\n` +
              `2. Substitute values:\n` +
              `   ρ = 2 · ${L_SI.toFixed(2)} / ((${V_SI.toFixed(
                3
              )})² · ${S_SI.toFixed(3)} · ${Cl_val}) ≈ ${rho.toFixed(
                4
              )} kg/m³\n` +
              `   Converted to ${
                rhoUnit === "kg_m3" ? "kg/m³" : "slug/ft³"
              }: ${rho_out.toFixed(4)}.`;
          } else if (currentMode === "area") {
            if (
              Cl_val == null ||
              rho_SI == null ||
              V_SI == null ||
              L_SI == null
            ) {
              showError(
                "Please enter lift, density, speed, and Cl to compute area."
              );
              return;
            }
            const S = (2 * L_SI) / (rho_SI * V_SI * V_SI * Cl_val);
            if (!isFinite(S)) {
              showError(
                "Inputs lead to an invalid area. Check that ρ, V, and Cl are positive."
              );
              return;
            }
            const S_out = fromSIArea(S, areaUnit);
            document.getElementById("area-input").value = S_out.toFixed(3);
            resultText = `Reference area S = ${S_out.toFixed(3)} ${
              areaUnit === "m2" ? "m²" : "ft²"
            }`;
            secondaryText = `Using L = ${L_SI.toFixed(
              2
            )} N, ρ = ${rho_SI.toFixed(3)} kg/m³, V = ${V_SI.toFixed(
              3
            )} m/s, C\u2097 = ${Cl_val}.`;
            steps =
              `1. Rearrange L = 0.5 · ρ · V² · S · C_L for S:\n` +
              `   S = 2L / (ρ · V² · C_L)\n` +
              `2. Substitute values:\n` +
              `   S = 2 · ${L_SI.toFixed(2)} / (${rho_SI.toFixed(
                3
              )} · (${V_SI.toFixed(3)})² · ${Cl_val}) ≈ ${S.toFixed(3)} m²\n` +
              `   Converted to ${
                areaUnit === "m2" ? "m²" : "ft²"
              }: ${S_out.toFixed(3)}.`;
          }

          const resContainer = document.getElementById("results-container");
          document.getElementById("primary-result").textContent = resultText;
          document.getElementById("secondary-result").textContent =
            secondaryText;
          document.getElementById("step-details").textContent = steps;
          resContainer.classList.remove("hidden");
        } catch (e) {
          console.error(e);
          showError("An unexpected error occurred. Please check your inputs.");
        }
      }

      document.getElementById("btn-calc").addEventListener("click", calculate);

      document.getElementById("btn-clear").addEventListener("click", () => {
        [
          "lift-input",
          "rho-input",
          "speed-input",
          "area-input",
          "cl-input",
          "q-input",
        ].forEach((id) => {
          document.getElementById(id).value = "";
        });
        document.getElementById("results-container").classList.add("hidden");
        clearError();
      });

      // Initialize defaults
      setUnitSystem("si");
      updatePlaceholders();
    </script>

