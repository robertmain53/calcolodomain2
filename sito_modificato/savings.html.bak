    <script>
      // Utility functions
      function formatCurrency(value) {
        if (!isFinite(value)) return "$0";
        return value.toLocaleString(undefined, {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: 2,
        });
      }
      function formatPercent(value) {
        if (!isFinite(value)) return "0%";
        return (
          value.toLocaleString(undefined, { maximumFractionDigits: 2 }) + "%"
        );
      }

      // Elements
      const initialAmountEl = document.getElementById("initialAmount");
      const monthlyContributionEl = document.getElementById(
        "monthlyContribution"
      );
      const contributionFrequencyEl = document.getElementById(
        "contributionFrequency"
      );
      const interestRateEl = document.getElementById("interestRate");
      const yearsEl = document.getElementById("years");
      const compoundingEl = document.getElementById("compounding");
      const timingToggleEl = document.getElementById("timingToggle");
      const timingLabelEl = document.getElementById("timingLabel");

      const finalBalanceEl = document.getElementById("finalBalance");
      const totalContributionsEl =
        document.getElementById("totalContributions");
      const totalInterestEl = document.getElementById("totalInterest");
      const effectiveRateEl = document.getElementById("effectiveRate");
      const barPrincipalEl = document.getElementById("barPrincipal");
      const barInterestEl = document.getElementById("barInterest");
      const scheduleBodyEl = document.getElementById("scheduleBody");

      const calculateBtn = document.getElementById("calculateBtn");

      const interestRateDisplayEl = document.getElementById(
        "interestRateDisplay"
      );
      const yearsDisplayEl = document.getElementById("yearsDisplay");

      const initialAmountLabelEl =
        document.getElementById("initialAmountLabel");
      const monthlyContributionLabelEl = document.getElementById(
        "monthlyContributionLabel"
      );
      const interestRateLabelEl = document.getElementById("interestRateLabel");
      const yearsLabelEl = document.getElementById("yearsLabel");

      // Goal-based elements
      const goalAmountEl = document.getElementById("goalAmount");
      const goalYearsEl = document.getElementById("goalYears");
      const goalRateEl = document.getElementById("goalRate");
      const goalFrequencyEl = document.getElementById("goalFrequency");
      const goalTimingToggleEl = document.getElementById("goalTimingToggle");
      const goalTimingLabelEl = document.getElementById("goalTimingLabel");

      const goalYearsDisplayEl = document.getElementById("goalYearsDisplay");
      const goalRateDisplayEl = document.getElementById("goalRateDisplay");
      const goalAmountLabelEl = document.getElementById("goalAmountLabel");
      const goalYearsLabelEl = document.getElementById("goalYearsLabel");
      const goalRateLabelEl = document.getElementById("goalRateLabel");

      const goalContributionEl = document.getElementById("goalContribution");
      const goalTotalPaidEl = document.getElementById("goalTotalPaid");
      const goalInterestPortionEl = document.getElementById(
        "goalInterestPortion"
      );
      const goalHorizonLabelEl = document.getElementById("goalHorizonLabel");
      const goalCalculateBtn = document.getElementById("goalCalculateBtn");

      // Tabs
      const tabGrowth = document.getElementById("tab-growth");
      const tabGoal = document.getElementById("tab-goal");
      const panelGrowth = document.getElementById("panel-growth");
      const panelGoal = document.getElementById("panel-goal");

      // Toggle state
      let contributeAtBeginning = false;
      let goalContributeAtBeginning = false;

      function updateSlidersDisplay() {
        interestRateDisplayEl.textContent = interestRateEl.value + "%";
        yearsDisplayEl.textContent =
          yearsEl.value + " year" + (yearsEl.value === "1" ? "" : "s");
        goalYearsDisplayEl.textContent =
          goalYearsEl.value + " year" + (goalYearsEl.value === "1" ? "" : "s");
        goalRateDisplayEl.textContent = goalRateEl.value + "%";

        initialAmountLabelEl.textContent = formatCurrency(
          Number(initialAmountEl.value || 0)
        );
        monthlyContributionLabelEl.textContent =
          formatCurrency(Number(monthlyContributionEl.value || 0)) +
          " per " +
          contributionFrequencyEl.options[
            contributionFrequencyEl.selectedIndex
          ].text.toLowerCase();
        interestRateLabelEl.textContent = interestRateEl.value + "% APY";
        yearsLabelEl.textContent = yearsEl.value + " years";

        goalAmountLabelEl.textContent = formatCurrency(
          Number(goalAmountEl.value || 0)
        );
        goalYearsLabelEl.textContent = goalYearsEl.value + " years";
        goalRateLabelEl.textContent = goalRateEl.value + "% APY";
      }

      function setToggleState(toggleEl, labelEl, isBeginning) {
        if (isBeginning) {
          toggleEl.classList.remove("bg-gray-200");
          toggleEl.classList.add("bg-blue-600");
          toggleEl.firstElementChild.style.transform = "translateX(1.1rem)";
          labelEl.textContent = "Beginning of period";
        } else {
          toggleEl.classList.add("bg-gray-200");
          toggleEl.classList.remove("bg-blue-600");
          toggleEl.firstElementChild.style.transform = "translateX(0)";
          labelEl.textContent = "End of period";
        }
      }

      function calculateSavings() {
        const P = Number(initialAmountEl.value) || 0;
        const contribPerPeriod = Number(monthlyContributionEl.value) || 0;
        const contribFreq = Number(contributionFrequencyEl.value) || 12;
        const r = (Number(interestRateEl.value) || 0) / 100;
        const years = Number(yearsEl.value) || 0;
        const m = Number(compoundingEl.value) || 1;

        const totalPeriods = years * m;
        const contribPerYear = contribPerPeriod * contribFreq;
        const contribPerCompPeriod = contribPerYear / m;

        const i = r / m;
        let FVPrincipal, FVContrib;

        if (r === 0 || m === 0) {
          FVPrincipal = P;
          FVContrib = contribPerCompPeriod * totalPeriods;
        } else {
          const factor = Math.pow(1 + i, totalPeriods);
          FVPrincipal = P * factor;
          FVContrib = (contribPerCompPeriod * (factor - 1)) / i;
          if (contributeAtBeginning) {
            FVContrib *= 1 + i;
          }
        }

        const finalBalance = FVPrincipal + FVContrib;
        const totalContributions = P + contribPerCompPeriod * totalPeriods;
        const totalInterest = finalBalance - totalContributions;

        finalBalanceEl.textContent = formatCurrency(finalBalance);
        totalContributionsEl.textContent = formatCurrency(totalContributions);
        totalInterestEl.textContent = formatCurrency(totalInterest);

        // Effective annual rate (EAR)
        const effectiveRate =
          r > 0 && m > 0 ? (Math.pow(1 + r / m, m) - 1) * 100 : 0;
        effectiveRateEl.textContent = formatPercent(effectiveRate);

        // Bars
        const principalPortion =
          totalContributions <= 0
            ? 0
            : (totalContributions / finalBalance) * 100;
        const interestPortion = 100 - principalPortion;
        barPrincipalEl.style.width = principalPortion + "%";
        barInterestEl.style.width = interestPortion + "%";

        // Schedule (yearly)
        scheduleBodyEl.innerHTML = "";
        let balance = P;
        const periodsPerYear = m;
        const contribPerPeriodSchedule = contribPerCompPeriod;
        for (let year = 1; year <= years; year++) {
          const startBalance = balance;
          let contribThisYear = 0;
          let interestThisYear = 0;

          for (let p = 0; p < periodsPerYear; p++) {
            if (contributeAtBeginning) {
              balance += contribPerPeriodSchedule;
              contribThisYear += contribPerPeriodSchedule;
            }
            const interest = balance * i;
            balance += interest;
            interestThisYear += interest;
            if (!contributeAtBeginning) {
              balance += contribPerPeriodSchedule;
              contribThisYear += contribPerPeriodSchedule;
            }
          }

          const row = document.createElement("tr");
          row.innerHTML = `
        <td class="text-left text-gray-700">${year}</td>
        <td class="text-right text-gray-700">${formatCurrency(
          startBalance
        )}</td>
        <td class="text-right text-gray-700">${formatCurrency(
          contribThisYear
        )}</td>
        <td class="text-right text-gray-700">${formatCurrency(
          interestThisYear
        )}</td>
        <td class="text-right text-gray-700">${formatCurrency(balance)}</td>
      `;
          scheduleBodyEl.appendChild(row);
        }
      }

      function calculateGoal() {
        const FV = Number(goalAmountEl.value) || 0;
        const years = Number(goalYearsEl.value) || 0;
        const r = (Number(goalRateEl.value) || 0) / 100;
        const freq = Number(goalFrequencyEl.value) || 12;

        const n = years * freq;
        const i = r / freq;

        let PMT;
        if (r === 0 || freq === 0) {
          PMT = n > 0 ? FV / n : 0;
        } else {
          const factor = Math.pow(1 + i, n);
          PMT = FV * (i / (factor - 1));
          if (goalContributeAtBeginning) {
            PMT / (1 + i);
          }
        }

        const totalPaid = PMT * n;
        const interestPortion = FV - totalPaid;

        let labelFreq = "month";
        if (freq === 26) labelFreq = "2 weeks";
        else if (freq === 52) labelFreq = "week";
        else if (freq === 1) labelFreq = "year";

        goalContributionEl.textContent =
          formatCurrency(PMT) + " / " + labelFreq;
        goalTotalPaidEl.textContent = formatCurrency(totalPaid);
        goalInterestPortionEl.textContent = formatCurrency(interestPortion);
        goalHorizonLabelEl.textContent =
          years + " year" + (years === 1 ? "" : "s");
      }

      // Event listeners
      interestRateEl.addEventListener("input", () => {
        updateSlidersDisplay();
        calculateSavings();
      });
      yearsEl.addEventListener("input", () => {
        updateSlidersDisplay();
        calculateSavings();
      });
      initialAmountEl.addEventListener("input", () => {
        updateSlidersDisplay();
        calculateSavings();
      });
      monthlyContributionEl.addEventListener("input", () => {
        updateSlidersDisplay();
        calculateSavings();
      });
      contributionFrequencyEl.addEventListener("change", () => {
        updateSlidersDisplay();
        calculateSavings();
      });
      compoundingEl.addEventListener("change", () => {
        updateSlidersDisplay();
        calculateSavings();
      });

      calculateBtn.addEventListener("click", calculateSavings);

      timingToggleEl.addEventListener("click", () => {
        contributeAtBeginning = !contributeAtBeginning;
        setToggleState(timingToggleEl, timingLabelEl, contributeAtBeginning);
        calculateSavings();
      });

      goalYearsEl.addEventListener("input", () => {
        updateSlidersDisplay();
        calculateGoal();
      });
      goalRateEl.addEventListener("input", () => {
        updateSlidersDisplay();
        calculateGoal();
      });
      goalAmountEl.addEventListener("input", () => {
        updateSlidersDisplay();
        calculateGoal();
      });
      goalFrequencyEl.addEventListener("change", () => {
        updateSlidersDisplay();
        calculateGoal();
      });

      goalTimingToggleEl.addEventListener("click", () => {
        goalContributeAtBeginning = !goalContributeAtBeginning;
        setToggleState(
          goalTimingToggleEl,
          goalTimingLabelEl,
          goalContributeAtBeginning
        );
        calculateGoal();
      });

      goalCalculateBtn.addEventListener("click", calculateGoal);

      // Tabs
      function activateTab(tab) {
        if (tab === "growth") {
          tabGrowth.classList.add("tab-button-active");
          tabGrowth.classList.remove("tab-button-inactive");
          tabGoal.classList.add("tab-button-inactive");
          tabGoal.classList.remove("tab-button-active");
          panelGrowth.classList.remove("hidden");
          panelGoal.classList.add("hidden");
        } else {
          tabGoal.classList.add("tab-button-active");
          tabGoal.classList.remove("tab-button-inactive");
          tabGrowth.classList.add("tab-button-inactive");
          tabGrowth.classList.remove("tab-button-active");
          panelGoal.classList.remove("hidden");
          panelGrowth.classList.add("hidden");
        }
      }

      tabGrowth.addEventListener("click", () => activateTab("growth"));
      tabGoal.addEventListener("click", () => activateTab("goal"));

      // Initial setup
      setToggleState(timingToggleEl, timingLabelEl, contributeAtBeginning);
      setToggleState(
        goalTimingToggleEl,
        goalTimingLabelEl,
        goalContributeAtBeginning
      );
      updateSlidersDisplay();
      calculateSavings();
      calculateGoal();
    </script>

