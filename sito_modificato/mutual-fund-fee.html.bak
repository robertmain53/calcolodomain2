    <script>
      // Utility functions
      function toDecimal(percent) {
        return isNaN(percent) ? 0 : percent / 100;
      }
      function fmtCurrency(val) {
        if (!isFinite(val)) return "$0";
        const abs = Math.abs(val);
        const opts = {
          style: "currency",
          currency: "USD",
          maximumFractionDigits: abs >= 100000 ? 0 : 2,
        };
        return new Intl.NumberFormat("en-US", opts).format(val);
      }
      function fmtPercent(val) {
        if (!isFinite(val)) return "0.00%";
        return (val * 100).toFixed(2) + "%";
      }

      function calculateScenario(params) {
        const {
          initialInvestment,
          monthlyContribution,
          years,
          grossReturn,
          expenseRatio,
          advisoryFee,
          frontLoad,
          backLoad,
          otherOneTime,
        } = params;

        const rGross = toDecimal(grossReturn);
        const fAnnual = toDecimal(expenseRatio) + toDecimal(advisoryFee);
        const rNet = rGross - fAnnual;

        // Initial after front load and one-time fees
        let balance = Math.max(initialInvestment - otherOneTime, 0);
        balance = balance * (1 - toDecimal(frontLoad));

        const monthly = monthlyContribution;
        const projection = [];

        for (let year = 1; year <= years; year++) {
          balance = (balance + monthly * 12) * (1 + rNet);
          if (year === years) {
            // apply back-end load at the end
            balance = balance * (1 - toDecimal(backLoad));
          }
          projection.push({ year, balance });
        }

        return {
          finalBalance: balance,
          projection,
          annualFeeRate: fAnnual,
        };
      }

      function calculateNoFee(
        initialInvestment,
        monthlyContribution,
        years,
        grossReturn
      ) {
        const rGross = toDecimal(grossReturn);
        let balance = initialInvestment;
        const monthly = monthlyContribution;
        const projection = [];

        for (let year = 1; year <= years; year++) {
          balance = (balance + monthly * 12) * (1 + rGross);
          projection.push({ year, balance });
        }

        return {
          finalBalance: balance,
          projection,
        };
      }

      function buildProjectionTable(withFees, noFees, tbodyEl) {
        tbodyEl.innerHTML = "";
        const len = withFees.length;
        const step = len <= 10 ? 1 : Math.round(len / 10);

        for (let i = 0; i < len; i += step) {
          const wf = withFees[i];
          const nf = noFees[i] || noFees[noFees.length - 1];
          const feeCost = nf.balance - wf.balance;

          const tr = document.createElement("tr");
          tr.innerHTML = `
        <td class="px-2 py-1 text-gray-700">${wf.year}</td>
        <td class="px-2 py-1 text-right text-gray-900">${fmtCurrency(
          wf.balance
        )}</td>
        <td class="px-2 py-1 text-right text-gray-900">${fmtCurrency(
          nf.balance
        )}</td>
        <td class="px-2 py-1 text-right text-gray-900">${fmtCurrency(
          feeCost
        )}</td>
      `;
          tbodyEl.appendChild(tr);
        }
      }

      document.addEventListener("DOMContentLoaded", function () {
        const tabSingle = document.getElementById("tab-single");
        const tabCompare = document.getElementById("tab-compare");
        const compareSection = document.getElementById("compare-section");
        const comparisonWrapper = document.getElementById("comparisonWrapper");

        function setTab(mode) {
          if (mode === "single") {
            tabSingle.classList.add("border-blue-500", "text-blue-600");
            tabSingle.classList.remove("border-transparent", "text-gray-500");
            tabCompare.classList.remove("border-blue-500", "text-blue-600");
            tabCompare.classList.add("border-transparent", "text-gray-500");
            compareSection.classList.add("hidden");
            comparisonWrapper.classList.add("hidden");
          } else {
            tabCompare.classList.add("border-blue-500", "text-blue-600");
            tabCompare.classList.remove("border-transparent", "text-gray-500");
            tabSingle.classList.remove("border-blue-500", "text-blue-600");
            tabSingle.classList.add("border-transparent", "text-gray-500");
            compareSection.classList.remove("hidden");
          }
        }

        tabSingle.addEventListener("click", () => setTab("single"));
        tabCompare.addEventListener("click", () => setTab("compare"));

        const calculateBtn = document.getElementById("calculateBtn");
        const resetBtn = document.getElementById("resetBtn");
        const resultsSection = document.getElementById("resultsSection");
        const projectionTableBody = document.getElementById(
          "projectionTableBody"
        );
        const resultNarrative = document.getElementById("resultNarrative");

        calculateBtn.addEventListener("click", () => {
          const initialInvestment =
            parseFloat(document.getElementById("initialInvestment").value) || 0;
          const monthlyContribution =
            parseFloat(document.getElementById("monthlyContribution").value) ||
            0;
          const years = Math.max(
            1,
            parseInt(document.getElementById("years").value, 10) || 1
          );
          const grossReturn =
            parseFloat(document.getElementById("grossReturn").value) || 0;

          const expenseRatio =
            parseFloat(document.getElementById("expenseRatio").value) || 0;
          const advisoryFee =
            parseFloat(document.getElementById("advisoryFee").value) || 0;
          const frontLoad =
            parseFloat(document.getElementById("frontLoad").value) || 0;
          const backLoad =
            parseFloat(document.getElementById("backLoad").value) || 0;
          const otherOneTime =
            parseFloat(document.getElementById("otherOneTime").value) || 0;

          const scenario = calculateScenario({
            initialInvestment,
            monthlyContribution,
            years,
            grossReturn,
            expenseRatio,
            advisoryFee,
            frontLoad,
            backLoad,
            otherOneTime,
          });

          const noFee = calculateNoFee(
            initialInvestment,
            monthlyContribution,
            years,
            grossReturn
          );

          const endingBalanceEl = document.getElementById("endingBalance");
          const endingNoFeeEl = document.getElementById("endingNoFee");
          const totalFeesEl = document.getElementById("totalFees");

          const feeCost = noFee.finalBalance - scenario.finalBalance;

          endingBalanceEl.textContent = fmtCurrency(scenario.finalBalance);
          endingNoFeeEl.textContent = fmtCurrency(noFee.finalBalance);
          totalFeesEl.textContent = fmtCurrency(feeCost);

          buildProjectionTable(
            scenario.projection,
            noFee.projection,
            projectionTableBody
          );

          const annualFeePct = scenario.annualFeeRate;
          const modeCompare = !compareSection.classList.contains("hidden");

          if (modeCompare) {
            const expenseRatio2 =
              parseFloat(document.getElementById("expenseRatio2").value) || 0;
            const advisoryFee2 =
              parseFloat(document.getElementById("advisoryFee2").value) || 0;
            const frontLoad2 =
              parseFloat(document.getElementById("frontLoad2").value) || 0;
            const backLoad2 =
              parseFloat(document.getElementById("backLoad2").value) || 0;

            const scenarioB = calculateScenario({
              initialInvestment,
              monthlyContribution,
              years,
              grossReturn,
              expenseRatio: expenseRatio2,
              advisoryFee: advisoryFee2,
              frontLoad: frontLoad2,
              backLoad: backLoad2,
              otherOneTime: 0,
            });

            const annualFeeRateB = scenarioB.annualFeeRate;

            document.getElementById("annualFeeA").textContent =
              fmtPercent(annualFeePct);
            document.getElementById("annualFeeB").textContent =
              fmtPercent(annualFeeRateB);
            document.getElementById("annualFeeDiff").textContent = fmtPercent(
              annualFeePct - annualFeeRateB
            );

            document.getElementById("endingA").textContent = fmtCurrency(
              scenario.finalBalance
            );
            document.getElementById("endingB").textContent = fmtCurrency(
              scenarioB.finalBalance
            );
            document.getElementById("endingDiff").textContent = fmtCurrency(
              scenarioB.finalBalance - scenario.finalBalance
            );

            const noFeeB = calculateNoFee(
              initialInvestment,
              monthlyContribution,
              years,
              grossReturn
            );
            const feeCostA = noFee.finalBalance - scenario.finalBalance;
            const feeCostB = noFeeB.finalBalance - scenarioB.finalBalance;

            document.getElementById("feesA").textContent =
              fmtCurrency(feeCostA);
            document.getElementById("feesB").textContent =
              fmtCurrency(feeCostB);
            document.getElementById("feesDiff").textContent = fmtCurrency(
              feeCostA - feeCostB
            );

            comparisonWrapper.classList.remove("hidden");

            resultNarrative.textContent =
              `Over ${years} years, Fund A’s total annual fee of ${fmtPercent(
                annualFeePct
              )} leaves you with ` +
              `${fmtCurrency(
                scenario.finalBalance
              )}, while a lower‑fee Fund B at ${fmtPercent(
                annualFeeRateB
              )} would grow to ` +
              `${fmtCurrency(
                scenarioB.finalBalance
              )}. That’s a difference of ${fmtCurrency(
                scenarioB.finalBalance - scenario.finalBalance
              )} ` +
              `in your favor with the low‑fee option.`;
          } else {
            comparisonWrapper.classList.add("hidden");
            resultNarrative.textContent =
              `With a total annual fee of ${fmtPercent(
                annualFeePct
              )}, your investment grows to ` +
              `${fmtCurrency(
                scenario.finalBalance
              )} over ${years} years. Without any fees, the same investment would reach ` +
              `${fmtCurrency(
                noFee.finalBalance
              )}, so fees reduce your ending balance by about ${fmtCurrency(
                feeCost
              )}.`;
          }

          resultsSection.classList.remove("hidden");
          resultsSection.scrollIntoView({ behavior: "smooth", block: "start" });
        });

        resetBtn.addEventListener("click", () => {
          document.getElementById("initialInvestment").value = 10000;
          document.getElementById("monthlyContribution").value = 0;
          document.getElementById("years").value = 30;
          document.getElementById("grossReturn").value = 7;
          document.getElementById("expenseRatio").value = 1;
          document.getElementById("advisoryFee").value = 0;
          document.getElementById("frontLoad").value = 0;
          document.getElementById("backLoad").value = 0;
          document.getElementById("otherOneTime").value = 0;
          document.getElementById("expenseRatio2").value = 0.1;
          document.getElementById("advisoryFee2").value = 0;
          document.getElementById("frontLoad2").value = 0;
          document.getElementById("backLoad2").value = 0;
          setTab("single");
          resultsSection.classList.add("hidden");
        });
      });

      // MathJax config
      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
