    <script>
      // Utility: parse numeric list from textarea
      function parseNumberList(text) {
        if (!text) return [];
        const tokens = text
          .replace(/[\r\n]+/g, " ")
          .split(/[\s,;]+/)
          .map((t) => t.trim())
          .filter((t) => t.length > 0);
        const nums = tokens.map(Number).filter((v) => Number.isFinite(v));
        return nums;
      }

      // Convert prices to log returns (%)
      function pricesToReturns(prices) {
        const rets = [];
        for (let i = 1; i < prices.length; i++) {
          const p0 = prices[i - 1];
          const p1 = prices[i];
          if (p0 > 0 && p1 > 0) {
            rets.push(Math.log(p1 / p0) * 100);
          }
        }
        return rets;
      }

      function statsFromReturns(returns) {
        const n = returns.length;
        if (!n) return null;
        const mean = returns.reduce((a, b) => a + b, 0) / n;
        const varSum = returns.reduce((a, b) => a + Math.pow(b - mean, 2), 0);
        const variance = n > 1 ? varSum / (n - 1) : 0;
        const std = Math.sqrt(variance);
        const min = Math.min(...returns);
        const max = Math.max(...returns);
        // Skewness (sample)
        let skew = 0;
        if (std > 0 && n > 2) {
          const m3 = returns.reduce((a, b) => a + Math.pow(b - mean, 3), 0) / n;
          skew = m3 / Math.pow(std, 3);
        }
        return { n, mean, std, min, max, skew };
      }

      function normalQuantile(p) {
        // Acklam's approximation
        if (p <= 0 || p >= 1) return NaN;
        const a = [
          -3.969683028665376e1, 2.209460984245205e2, -2.759285104469687e2,
          1.38357751867269e2, -3.066479806614716e1, 2.506628277459239,
        ];
        const b = [
          -5.447609879822406e1, 1.615858368580409e2, -1.556989798598866e2,
          6.680131188771972e1, -1.328068155288572e1,
        ];
        const c = [
          -7.784894002430293e-3, -3.223964580411365e-1, -2.400758277161838,
          -2.549732539343734, 4.374664141464968, 2.938163982698783,
        ];
        const d = [
          7.784695709041462e-3, 3.224671290700398e-1, 2.445134137142996,
          3.754408661907416,
        ];

        let q, r;
        if (p < 0.02425) {
          q = Math.sqrt(-2 * Math.log(p));
          return (
            (((((c[0] * q + c[1]) * q + c[2]) * q + c[3]) * q + c[4]) * q +
              c[5]) /
            ((((d[0] * q + d[1]) * q + d[2]) * q + d[3]) * q + 1)
          );
        } else if (p > 1 - 0.02425) {
          q = Math.sqrt(-2 * Math.log(1 - p));
          return (
            -(
              ((((c[0] * q + c[1]) * q + c[2]) * q + c[3]) * q + c[4]) * q +
              c[5]
            ) /
            ((((d[0] * q + d[1]) * q + d[2]) * q + d[3]) * q + 1)
          );
        } else {
          q = p - 0.5;
          r = q * q;
          return (
            ((((((a[0] * r + a[1]) * r + a[2]) * r + a[3]) * r + a[4]) * r +
              a[5]) *
              q) /
            (((((b[0] * r + b[1]) * r + b[2]) * r + b[3]) * r + b[4]) * r + 1)
          );
        }
      }

      function empiricalQuantile(arr, p) {
        if (!arr.length) return NaN;
        const sorted = [...arr].sort((a, b) => a - b);
        const idx = Math.min(
          sorted.length - 1,
          Math.max(0, Math.floor(p * sorted.length))
        );
        return sorted[idx];
      }

      function formatCurrency(x) {
        if (!Number.isFinite(x)) return "—";
        const abs = Math.abs(x);
        const decimals = abs >= 1000 ? 0 : 2;
        return (x < 0 ? "-" : "") + "$" + Math.abs(x).toFixed(decimals);
      }

      function formatPercent(x) {
        if (!Number.isFinite(x)) return "—";
        return x.toFixed(2) + "%";
      }

      document.addEventListener("DOMContentLoaded", () => {
        const methodEl = document.getElementById("method");
        const parametricInputs = document.getElementById("parametricInputs");
        const monteCarloInputs = document.getElementById("monteCarloInputs");
        const dataModeToggle = document.getElementById("toggleDataMode");
        const dataModeLabel = document.getElementById("dataModeLabel");
        const dataHintType = document.getElementById("dataHintType");
        const dataInput = document.getElementById("dataInput");
        const calcButton = document.getElementById("calcButton");
        const clearButton = document.getElementById("clearButton");
        const errorMessage = document.getElementById("errorMessage");

        let dataMode = "returns"; // or 'prices'

        function updateMethodVisibility() {
          const m = methodEl.value;
          parametricInputs.classList.toggle("hidden", m !== "parametric");
          monteCarloInputs.classList.toggle("hidden", m !== "montecarlo");
        }

        function toggleDataMode() {
          dataMode = dataMode === "returns" ? "prices" : "returns";
          dataModeLabel.textContent = "Mode: " + dataMode;
          dataHintType.textContent =
            dataMode === "returns" ? "returns in %" : "prices";
          dataInput.placeholder =
            dataMode === "returns"
              ? "-0.5\n1.2\n0.3\n-2.1\n..."
              : "100\n101.5\n100.8\n99.2\n...";
        }

        function showError(msg) {
          errorMessage.textContent = msg || "";
        }

        function updateStats(returns) {
          const statsBox = document.getElementById("statsContainer");
          const s = statsFromReturns(returns);
          if (!s) {
            statsBox.classList.add("hidden");
            return;
          }
          document.getElementById("statN").textContent = s.n;
          document.getElementById("statMean").textContent = formatPercent(
            s.mean
          );
          document.getElementById("statStd").textContent = formatPercent(s.std);
          document.getElementById("statMin").textContent = formatPercent(s.min);
          document.getElementById("statMax").textContent = formatPercent(s.max);
          document.getElementById("statSkew").textContent = s.skew.toFixed(2);
          statsBox.classList.remove("hidden");
        }

        function computeVaR() {
          showError("");
          const V = parseFloat(document.getElementById("portfolioValue").value);
          const alpha = parseFloat(
            document.getElementById("confidenceLevel").value
          );
          const T = parseInt(document.getElementById("horizonDays").value, 10);
          const method = methodEl.value;

          if (!Number.isFinite(V) || V <= 0) {
            showError("Please enter a valid positive portfolio value.");
            return;
          }
          if (!Number.isFinite(T) || T <= 0) {
            showError("Please enter a valid positive time horizon in days.");
            return;
          }

          let returns = [];
          const raw = parseNumberList(dataInput.value);
          if (raw.length) {
            if (dataMode === "prices") {
              if (raw.length < 2) {
                showError(
                  "At least two prices are required to compute returns."
                );
                return;
              }
              returns = pricesToReturns(raw);
            } else {
              returns = raw; // already in %
            }
          }

          let dailyMean = 0;
          let dailyStd = 0;

          if (returns.length) {
            const s = statsFromReturns(returns);
            dailyMean = s.mean;
            dailyStd = s.std;
            updateStats(returns);
          } else {
            updateStats([]);
          }

          let varLoss = NaN;
          let cvarLoss = NaN;
          const pTail = 1 - alpha;

          if (method === "historical") {
            if (!returns.length) {
              showError(
                "Historical VaR requires historical returns or prices."
              );
              return;
            }
            const losses = returns.map((r) => (-r * V) / 100); // loss = -return * V
            const var1d = empiricalQuantile(losses, alpha); // alpha quantile of loss
            const scale = Math.sqrt(T);
            varLoss = var1d * scale;

            const tailLosses = losses.filter((L) => L >= var1d);
            if (tailLosses.length) {
              const avgTail =
                tailLosses.reduce((a, b) => a + b, 0) / tailLosses.length;
              cvarLoss = avgTail * scale;
            }
          } else if (method === "parametric") {
            let mu = parseFloat(document.getElementById("meanReturn").value);
            let sigma = parseFloat(document.getElementById("volatility").value);
            if (!Number.isFinite(mu)) mu = 0;
            if (!Number.isFinite(sigma) || sigma <= 0) {
              if (dailyStd > 0) {
                sigma = dailyStd;
              } else {
                showError(
                  "Please enter a valid positive volatility or provide data to estimate it."
                );
                return;
              }
            }
            const z = normalQuantile(alpha);
            const muT = (mu * T) / 100;
            const sigmaT = (sigma * Math.sqrt(T)) / 100;
            const varReturn = z * sigmaT - muT; // as fraction
            varLoss = varReturn * V;

            // CVaR for normal distribution
            const pdf = (x) => Math.exp(-0.5 * x * x) / Math.sqrt(2 * Math.PI);
            const cvarReturn = (pdf(z) / pTail) * sigmaT - muT;
            cvarLoss = cvarReturn * V;
          } else if (method === "montecarlo") {
            let mu = parseFloat(document.getElementById("meanReturn").value);
            let sigma = parseFloat(document.getElementById("volatility").value);
            const sims = parseInt(
              document.getElementById("mcSimulations").value,
              10
            );
            const useData = document.getElementById("mcUseData").checked;

            if (useData && returns.length) {
              const s = statsFromReturns(returns);
              mu = s.mean;
              sigma = s.std;
            }

            if (!Number.isFinite(mu)) mu = 0;
            if (!Number.isFinite(sigma) || sigma <= 0) {
              if (dailyStd > 0) {
                sigma = dailyStd;
              } else {
                showError(
                  "Please enter a valid positive volatility or provide data to estimate it."
                );
                return;
              }
            }
            if (!Number.isFinite(sims) || sims < 100) {
              showError(
                "Please enter at least 100 simulations for Monte Carlo VaR."
              );
              return;
            }

            const muT = (mu * T) / 100;
            const sigmaT = (sigma * Math.sqrt(T)) / 100;
            const losses = [];
            for (let i = 0; i < sims; i++) {
              // Box-Muller
              const u1 = Math.random();
              const u2 = Math.random();
              const z =
                Math.sqrt(-2 * Math.log(u1)) * Math.cos(2 * Math.PI * u2);
              const rT = muT + sigmaT * z;
              const L = -rT * V;
              losses.push(L);
            }
            losses.sort((a, b) => a - b);
            const idx = Math.floor(alpha * losses.length);
            const var1d = losses[Math.min(Math.max(idx, 0), losses.length - 1)];
            varLoss = var1d;

            const tailLosses = losses.filter((L) => L >= var1d);
            if (tailLosses.length) {
              cvarLoss =
                tailLosses.reduce((a, b) => a + b, 0) / tailLosses.length;
            }
          }

          const varCurrencyEl = document.getElementById("varCurrency");
          const varPercentEl = document.getElementById("varPercent");
          const varSummaryLabel = document.getElementById("varSummaryLabel");
          const cvarCurrencyEl = document.getElementById("cvarCurrency");
          const cvarPercentEl = document.getElementById("cvarPercent");

          if (!Number.isFinite(varLoss)) {
            showError("Unable to compute VaR with the provided inputs.");
            return;
          }

          const varPct = (varLoss / V) * 100;
          varCurrencyEl.textContent = formatCurrency(varLoss);
          varPercentEl.textContent =
            "≈ " + formatPercent(varPct) + " of portfolio value";
          varSummaryLabel.textContent = `${(alpha * 100)
            .toFixed(1)
            .replace(/\.0$/, "")}% VaR over ${T} day${T > 1 ? "s" : ""}`;

          if (Number.isFinite(cvarLoss)) {
            const cvarPct = (cvarLoss / V) * 100;
            cvarCurrencyEl.textContent = formatCurrency(cvarLoss);
            cvarPercentEl.textContent =
              "≈ " + formatPercent(cvarPct) + " of portfolio value";
          } else {
            cvarCurrencyEl.textContent = "—";
            cvarPercentEl.textContent = "—";
          }
        }

        methodEl.addEventListener("change", updateMethodVisibility);
        dataModeToggle.addEventListener("click", toggleDataMode);
        calcButton.addEventListener("click", computeVaR);
        clearButton.addEventListener("click", () => {
          dataInput.value = "";
          updateStats([]);
          showError("");
        });

        updateMethodVisibility();
      });
    </script>

