    <script>
      // Standard OCPD sizes (A)
      const STANDARD_SIZES = [
        15, 20, 25, 30, 35, 40, 45, 50, 60, 70, 80, 90, 100, 110, 125, 150, 175,
        200, 225, 250, 300, 350, 400, 450, 500, 600, 800, 1000,
      ];

      function roundDownToStandard(amps) {
        let result = null;
        for (const size of STANDARD_SIZES) {
          if (size <= amps) result = size;
          else break;
        }
        return result ?? STANDARD_SIZES[0];
      }

      function getPercentages(scheme, deviceType, necRule) {
        // Very simplified interpretation of NEC 450.3(B) behavior
        if (necRule === "custom") {
          const custom =
            parseFloat(document.getElementById("custom-percent").value) || 125;
          return { priPct: custom, secPct: custom };
        }

        if (scheme === "primary-only") {
          // Primary-only protection: keep primary conservative, no secondary OCPD
          return { priPct: 125, secPct: 0 };
        } else {
          // Primary & secondary protection
          if (deviceType === "fuse") {
            return { priPct: 250, secPct: 125 };
          } else {
            // breaker
            return { priPct: 250, secPct: 125 };
          }
        }
      }

      function formatNumber(num, decimals = 1) {
        return Number.isFinite(num) ? num.toFixed(decimals) : "–";
      }

      document
        .getElementById("nec-rule")
        .addEventListener("change", function () {
          const customWrapper = document.getElementById(
            "custom-percent-wrapper"
          );
          customWrapper.classList.toggle("hidden", this.value !== "custom");
        });

      document
        .getElementById("calculate-btn")
        .addEventListener("click", function () {
          const phase = parseInt(document.getElementById("phase").value, 10);
          const kva = parseFloat(document.getElementById("kva").value);
          const vpri = parseFloat(document.getElementById("vpri").value);
          const vsec = parseFloat(document.getElementById("vsec").value);
          const scheme = document.getElementById("protection-scheme").value;
          const deviceType = document.getElementById("device-type").value;
          const necRule = document.getElementById("nec-rule").value;

          if (!kva || !vpri || !vsec || !phase) {
            alert("Please enter valid kVA and voltages.");
            return;
          }

          const base = kva * 1000;
          const priFlc =
            phase === 1 ? base / vpri : base / (Math.sqrt(3) * vpri);
          const secFlc =
            phase === 1 ? base / vsec : base / (Math.sqrt(3) * vsec);

          const { priPct, secPct } = getPercentages(
            scheme,
            deviceType,
            necRule
          );

          const priMaxAmps = priPct > 0 ? priFlc * (priPct / 100) : 0;
          const secMaxAmps = secPct > 0 ? secFlc * (secPct / 100) : 0;

          const priStd =
            priMaxAmps > 0 ? roundDownToStandard(priMaxAmps) : null;
          const secStd =
            secMaxAmps > 0 ? roundDownToStandard(secMaxAmps) : null;

          document.getElementById("pri-flc").textContent = formatNumber(
            priFlc,
            1
          );
          document.getElementById("sec-flc").textContent = formatNumber(
            secFlc,
            1
          );

          document.getElementById("pri-max-pct").textContent = priPct || "–";
          document.getElementById("sec-max-pct").textContent = secPct || "–";

          document.getElementById("pri-max-amps").textContent = priMaxAmps
            ? formatNumber(priMaxAmps, 1)
            : "–";
          document.getElementById("sec-max-amps").textContent = secMaxAmps
            ? formatNumber(secMaxAmps, 1)
            : "–";

          document.getElementById("pri-std-size").textContent = priStd
            ? priStd + " A"
            : "N/A";
          document.getElementById("sec-std-size").textContent = secStd
            ? secStd + " A"
            : "N/A";

          // Badges
          const priBadge = document.getElementById("primary-scheme-badge");
          const secBadge = document.getElementById("secondary-scheme-badge");

          if (scheme === "primary-only") {
            priBadge.textContent = "Primary-only";
            secBadge.textContent = "No secondary OCPD";
          } else {
            priBadge.textContent = "Primary OCPD";
            secBadge.textContent = "Secondary OCPD";
          }

          document.getElementById("results-section").classList.remove("hidden");
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
