    <script>
      // Mobile menu toggle
      document
        .getElementById("mobile-menu-toggle")
        ?.addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          if (!menu) return;
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          this.setAttribute("aria-expanded", String(isHidden));
        });

      // Reliability class helper text
      const reliabilitySelect = document.getElementById("reliability-class");
      const reliabilityNote = document.getElementById("reliability-note");
      const gammaGInput = document.getElementById("gamma-g");
      const gammaQInput = document.getElementById("gamma-q");

      function updateReliabilityInfo() {
        const rc = reliabilitySelect.value;
        if (rc === "RC1") {
          reliabilityNote.textContent =
            "RC1: low consequence structures (e.g. small farm buildings).";
          if (!gammaGInput.dataset.userEdited) gammaGInput.value = 1.2;
          if (!gammaQInput.dataset.userEdited) gammaQInput.value = 1.5;
        } else if (rc === "RC3") {
          reliabilityNote.textContent =
            "RC3: high consequence structures (e.g. large public buildings, critical infrastructure).";
          if (!gammaGInput.dataset.userEdited) gammaGInput.value = 1.35;
          if (!gammaQInput.dataset.userEdited) gammaQInput.value = 1.6;
        } else {
          reliabilityNote.textContent =
            "RC2: residential, office, standard bridges.";
          if (!gammaGInput.dataset.userEdited) gammaGInput.value = 1.35;
          if (!gammaQInput.dataset.userEdited) gammaQInput.value = 1.5;
        }
      }

      reliabilitySelect.addEventListener("change", updateReliabilityInfo);
      ["gamma-g", "gamma-q"].forEach((id) => {
        const el = document.getElementById(id);
        el.addEventListener("input", () => {
          el.dataset.userEdited = "true";
        });
      });

      // Actions table logic
      const actionsTbody = document.getElementById("actions-tbody");
      const addActionBtn = document.getElementById("add-action-btn");

      let actionCounter = 0;

      function createActionRow(preset) {
        actionCounter++;
        const row = document.createElement("tr");
        row.className = actionCounter % 2 === 0 ? "bg-white" : "bg-gray-50";

        const id = "action-" + actionCounter;
        const type = preset?.type || "G";
        const name = preset?.name || "";
        const fk = preset?.fk ?? "";
        const psi0 = preset?.psi0 ?? (type === "Q" ? 0.7 : 0.0);
        const psi1 = preset?.psi1 ?? (type === "Q" ? 0.5 : 0.0);
        const psi2 = preset?.psi2 ?? (type === "Q" ? 0.3 : 0.0);
        const leading = !!preset?.leading;

        row.innerHTML = `
      <td class="px-2 py-1">
        <select class="action-type border border-gray-300 rounded-md px-2 py-1 text-xs">
          <option value="G"${
            type === "G" ? " selected" : ""
          }>G – Permanent</option>
          <option value="Q"${
            type === "Q" ? " selected" : ""
          }>Q – Variable</option>
          <option value="A"${
            type === "A" ? " selected" : ""
          }>A – Accidental</option>
        </select>
      </td>
      <td class="px-2 py-1">
        <input type="text" class="action-name border border-gray-300 rounded-md px-2 py-1 text-xs w-32" value="${name}">
      </td>
      <td class="px-2 py-1 text-right">
        <input type="number" step="0.01" class="action-fk border border-gray-300 rounded-md px-2 py-1 text-xs w-24 text-right" value="${fk}">
      </td>
      <td class="px-2 py-1 text-right">
        <input type="number" step="0.01" class="action-psi0 border border-gray-300 rounded-md px-2 py-1 text-xs w-16 text-right" value="${psi0}">
      </td>
      <td class="px-2 py-1 text-right">
        <input type="number" step="0.01" class="action-psi1 border border-gray-300 rounded-md px-2 py-1 text-xs w-16 text-right" value="${psi1}">
      </td>
      <td class="px-2 py-1 text-right">
        <input type="number" step="0.01" class="action-psi2 border border-gray-300 rounded-md px-2 py-1 text-xs w-16 text-right" value="${psi2}">
      </td>
      <td class="px-2 py-1 text-center">
        <input type="radio" name="leading-action" class="action-leading"${
          leading ? " checked" : ""
        } aria-label="Leading variable action">
      </td>
      <td class="px-2 py-1 text-center">
        <button type="button" class="remove-action text-xs text-red-600 hover:underline">Remove</button>
      </td>
    `;

        actionsTbody.appendChild(row);
      }

      // Seed with typical example rows
      createActionRow({
        type: "G",
        name: "Self-weight + finishes",
        fk: 20,
        psi0: 1.0,
        psi1: 1.0,
        psi2: 1.0,
      });
      createActionRow({
        type: "Q",
        name: "Imposed load",
        fk: 10,
        psi0: 0.7,
        psi1: 0.5,
        psi2: 0.3,
        leading: true,
      });
      createActionRow({
        type: "Q",
        name: "Snow",
        fk: 5,
        psi0: 0.5,
        psi1: 0.2,
        psi2: 0.0,
      });

      addActionBtn.addEventListener("click", () => createActionRow());

      actionsTbody.addEventListener("click", (e) => {
        if (e.target.classList.contains("remove-action")) {
          const row = e.target.closest("tr");
          if (row) row.remove();
        }
      });

      // Calculation logic
      const calculateBtn = document.getElementById("calculate-btn");
      const resultPanel = document.getElementById("results-panel");
      const resultEd = document.getElementById("result-ed");
      const resultExpression = document.getElementById("result-expression");
      const designSituationSelect = document.getElementById("design-situation");
      const limitStateSelect = document.getElementById("limit-state");

      function parseActions() {
        const rows = Array.from(actionsTbody.querySelectorAll("tr"));
        const actions = [];
        rows.forEach((row) => {
          const type = row.querySelector(".action-type")?.value || "G";
          const name = row.querySelector(".action-name")?.value || "";
          const fk =
            parseFloat(row.querySelector(".action-fk")?.value || "0") || 0;
          const psi0 =
            parseFloat(row.querySelector(".action-psi0")?.value || "0") || 0;
          const psi1 =
            parseFloat(row.querySelector(".action-psi1")?.value || "0") || 0;
          const psi2 =
            parseFloat(row.querySelector(".action-psi2")?.value || "0") || 0;
          const leading =
            row.querySelector(".action-leading")?.checked || false;
          actions.push({ type, name, fk, psi0, psi1, psi2, leading });
        });
        return actions;
      }

      function calculateCombination() {
        const actions = parseActions().filter((a) => a.fk !== 0);
        if (!actions.length) {
          alert(
            "Please enter at least one action with a non-zero characteristic value."
          );
          return;
        }

        const gammaG = parseFloat(gammaGInput.value || "1") || 1;
        const gammaQ = parseFloat(gammaQInput.value || "1") || 1;
        const situation = designSituationSelect.value;
        const limitState = limitStateSelect.value;

        const G = actions.filter((a) => a.type === "G");
        const Q = actions.filter((a) => a.type === "Q");
        const A = actions.filter((a) => a.type === "A");

        const leadingQ = Q.find((a) => a.leading) || Q[0];

        let Ed = 0;
        let exprParts = [];

        const sumG = G.reduce((s, a) => s + a.fk, 0);
        if (limitState === "ULS") {
          if (situation === "persistent") {
            const termG = gammaG * sumG;
            Ed += termG;
            if (sumG !== 0)
              exprParts.push(`${gammaG.toFixed(2)}·ΣGk=${termG.toFixed(2)}`);

            if (leadingQ) {
              const leadingTerm = gammaQ * leadingQ.fk;
              Ed += leadingTerm;
              exprParts.push(
                `${gammaQ.toFixed(2)}·Qk,lead=${leadingTerm.toFixed(2)}`
              );
            }

            const others = Q.filter((a) => a !== leadingQ);
            if (others.length) {
              const sumOthers = others.reduce((s, a) => s + a.psi0 * a.fk, 0);
              const termOthers = gammaQ * sumOthers;
              Ed += termOthers;
              exprParts.push(
                `${gammaQ.toFixed(2)}·Σ(ψ0·Qk,others)=${termOthers.toFixed(2)}`
              );
            }
          } else if (situation === "accidental") {
            const termG = gammaG * sumG;
            Ed += termG;
            if (sumG !== 0)
              exprParts.push(`${gammaG.toFixed(2)}·ΣGk=${termG.toFixed(2)}`);

            const sumA = A.reduce((s, a) => s + a.fk, 0);
            Ed += sumA;
            if (sumA !== 0) exprParts.push(`ΣAk=${sumA.toFixed(2)}`);

            const sumQ = Q.reduce((s, a) => s + a.psi1 * a.fk, 0);
            Ed += sumQ;
            if (sumQ !== 0) exprParts.push(`Σ(ψ1·Qk)=${sumQ.toFixed(2)}`);
          }
        } else if (limitState === "SLS-char") {
          Ed += sumG;
          if (sumG !== 0) exprParts.push(`ΣGk=${sumG.toFixed(2)}`);

          if (leadingQ) {
            Ed += leadingQ.fk;
            exprParts.push(`Qk,lead=${leadingQ.fk.toFixed(2)}`);
          }

          const others = Q.filter((a) => a !== leadingQ);
          if (others.length) {
            const sumOthers = others.reduce((s, a) => s + a.psi0 * a.fk, 0);
            Ed += sumOthers;
            exprParts.push(`Σ(ψ0·Qk,others)=${sumOthers.toFixed(2)}`);
          }
        } else if (limitState === "SLS-freq") {
          Ed += sumG;
          if (sumG !== 0) exprParts.push(`ΣGk=${sumG.toFixed(2)}`);

          if (leadingQ) {
            const termLead = leadingQ.psi1 * leadingQ.fk;
            Ed += termLead;
            exprParts.push(`ψ1,lead·Qk,lead=${termLead.toFixed(2)}`);
          }

          const others = Q.filter((a) => a !== leadingQ);
          if (others.length) {
            const sumOthers = others.reduce((s, a) => s + a.psi2 * a.fk, 0);
            Ed += sumOthers;
            exprParts.push(`Σ(ψ2·Qk,others)=${sumOthers.toFixed(2)}`);
          }
        } else if (limitState === "SLS-quasi") {
          Ed += sumG;
          if (sumG !== 0) exprParts.push(`ΣGk=${sumG.toFixed(2)}`);

          const sumQqp = Q.reduce((s, a) => s + a.psi2 * a.fk, 0);
          Ed += sumQqp;
          if (sumQqp !== 0) exprParts.push(`Σ(ψ2·Qk)=${sumQqp.toFixed(2)}`);
        }

        resultEd.textContent = Ed.toFixed(2);
        resultExpression.textContent =
          exprParts.join(" + ") || "No non-zero actions defined.";
        resultPanel.classList.remove("hidden");
      }

      calculateBtn.addEventListener("click", calculateCombination);

      // Initialize reliability info
      updateReliabilityInfo();

      // MathJax config
      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
