    <script>
      // Simple normal inverse CDF approximation for Z from service level
      function approximateZ(p) {
        // p between 0.5 and 0.999
        if (p <= 0.5 || p >= 0.999) return NaN;
        // Abramowitz-Stegun approximation
        const a1 = -39.6968302866538,
          a2 = 220.946098424521,
          a3 = -275.928510446969;
        const a4 = 138.357751867269,
          a5 = -30.6647980661472,
          a6 = 2.50662827745924;
        const b1 = -54.4760987982241,
          b2 = 161.585836858041,
          b3 = -155.698979859887;
        const b4 = 66.8013118877197,
          b5 = -13.2806815528857;
        const c1 = -0.00778489400243029,
          c2 = -0.322396458041136,
          c3 = -2.40075827716184;
        const c4 = -2.54973253934373,
          c5 = 4.37466414146497,
          c6 = 2.93816398269878;
        const d1 = 0.00778469570904146,
          d2 = 0.32246712907004,
          d3 = 2.445134137143,
          d4 = 3.75440866190742;
        const plow = 0.02425,
          phigh = 1 - plow;
        let q, r;
        if (p < plow) {
          q = Math.sqrt(-2 * Math.log(p));
          return (
            (((((c1 * q + c2) * q + c3) * q + c4) * q + c5) * q + c6) /
            ((((d1 * q + d2) * q + d3) * q + d4) * q + 1)
          );
        }
        if (phigh < p) {
          q = Math.sqrt(-2 * Math.log(1 - p));
          return (
            -(((((c1 * q + c2) * q + c3) * q + c4) * q + c5) * q + c6) /
            ((((d1 * q + d2) * q + d3) * q + d4) * q + 1)
          );
        }
        q = p - 0.5;
        r = q * q;
        return (
          ((((((a1 * r + a2) * r + a3) * r + a4) * r + a5) * r + a6) * q) /
          (((((b1 * r + b2) * r + b3) * r + b4) * r + b5) * r + 1)
        );
      }

      document.addEventListener("DOMContentLoaded", function () {
        const modeBasicBtn = document.getElementById("mode-basic");
        const modeSimpleBtn = document.getElementById("mode-simple-ss");
        const modeAdvancedBtn = document.getElementById("mode-advanced");
        const simpleSection = document.getElementById("simple-ss-section");
        const advancedSection = document.getElementById("advanced-section");
        const errorMsg = document.getElementById("error-msg");
        const resultsCard = document.getElementById("results-card");
        const dltOutput = document.getElementById("dlt-output");
        const ssOutput = document.getElementById("ss-output");
        const ropOutput = document.getElementById("rop-output");
        const ropExplainer = document.getElementById("rop-explainer");

        let mode = "basic";

        function setMode(newMode) {
          mode = newMode;
          [modeBasicBtn, modeSimpleBtn, modeAdvancedBtn].forEach((btn) =>
            btn.classList.remove("toggle-btn-active")
          );
          if (mode === "basic") modeBasicBtn.classList.add("toggle-btn-active");
          if (mode === "simple")
            modeSimpleBtn.classList.add("toggle-btn-active");
          if (mode === "advanced")
            modeAdvancedBtn.classList.add("toggle-btn-active");

          simpleSection.classList.toggle("hidden", mode !== "simple");
          advancedSection.classList.toggle("hidden", mode !== "advanced");
          errorMsg.textContent = "";
          resultsCard.classList.add("hidden");
        }

        modeBasicBtn.addEventListener("click", () => setMode("basic"));
        modeSimpleBtn.addEventListener("click", () => setMode("simple"));
        modeAdvancedBtn.addEventListener("click", () => setMode("advanced"));

        document
          .getElementById("calculate-btn")
          .addEventListener("click", function () {
            errorMsg.textContent = "";
            resultsCard.classList.add("hidden");

            const d = parseFloat(document.getElementById("avg-demand").value);
            const periodType = document.getElementById("period-type").value;
            let L = parseFloat(document.getElementById("lead-time").value);
            const leadUnit = document.getElementById("lead-time-unit").value;

            if (isNaN(d) || d <= 0 || isNaN(L) || L <= 0) {
              errorMsg.textContent =
                "Please enter positive values for demand and lead time.";
              return;
            }

            // Convert to consistent units: use days as base
            let demandPerDay;
            if (periodType === "day") {
              demandPerDay = d;
            } else {
              demandPerDay = d / 7; // per week -> per day
            }

            let leadTimeDays = leadUnit === "day" ? L : L * 7;

            const demandDuringLeadTime = demandPerDay * leadTimeDays;

            let safetyStock = 0;
            let explanation = "";

            if (mode === "basic") {
              safetyStock = 0;
              explanation =
                "Basic ROP = demand during lead time (no safety stock).";
            } else if (mode === "simple") {
              const ssSimple = parseFloat(
                document.getElementById("safety-stock-simple").value
              );
              if (isNaN(ssSimple) || ssSimple < 0) {
                errorMsg.textContent =
                  "Enter a non-negative safety stock value.";
                return;
              }
              safetyStock = ssSimple;
              explanation =
                "ROP = demand during lead time + provided safety stock.";
            } else if (mode === "advanced") {
              const sigmaD = parseFloat(
                document.getElementById("demand-std").value
              );
              const sigmaLInput = parseFloat(
                document.getElementById("leadtime-std").value
              );
              const serviceLevel = parseFloat(
                document.getElementById("service-level").value
              );
              let z = parseFloat(document.getElementById("z-value").value);

              if (
                isNaN(sigmaD) ||
                sigmaD < 0 ||
                isNaN(sigmaLInput) ||
                sigmaLInput < 0
              ) {
                errorMsg.textContent =
                  "Enter non-negative standard deviations for demand and lead time.";
                return;
              }
              if (isNaN(z)) {
                if (
                  isNaN(serviceLevel) ||
                  serviceLevel <= 50 ||
                  serviceLevel >= 99.9
                ) {
                  errorMsg.textContent =
                    "Enter a service level between 50 and 99.9, or provide a Z-score.";
                  return;
                }
                const p = serviceLevel / 100;
                z = approximateZ(p);
                if (!isFinite(z)) {
                  errorMsg.textContent =
                    "Could not compute Z from service level. Try entering Z-score directly.";
                  return;
                }
              }

              // Convert lead time std dev to days
              const sigmaL = leadUnit === "day" ? sigmaLInput : sigmaLInput * 7;

              const sigmaLT = Math.sqrt(
                leadTimeDays *
                  Math.pow(demandPerDay, 2) *
                  Math.pow(sigmaD / d, 2) +
                  (Math.pow(demandPerDay, 2) * Math.pow(sigmaL, 2)) /
                    Math.pow(leadTimeDays, 2)
              );
              // Better: exact formula: L*σd^2 + d^2*σL^2
              const sigmaLTExact = Math.sqrt(
                leadTimeDays * Math.pow(sigmaD, 2) +
                  Math.pow(demandPerDay, 2) * Math.pow(sigmaL, 2)
              );
              safetyStock = z * sigmaLTExact;

              explanation =
                "Advanced ROP = demand during lead time + Z × σ(during lead time).";
            }

            const rop = demandDuringLeadTime + safetyStock;

            dltOutput.textContent = demandDuringLeadTime.toFixed(2) + " units";
            ssOutput.textContent = safetyStock.toFixed(2) + " units";
            ropOutput.textContent = rop.toFixed(2) + " units";
            ropExplainer.textContent = explanation;
            resultsCard.classList.remove("hidden");
          });

        document
          .getElementById("reset-btn")
          .addEventListener("click", function () {
            document.getElementById("rop-form").reset();
            errorMsg.textContent = "";
            resultsCard.classList.add("hidden");
            setMode("basic");
          });
      });
    </script>

