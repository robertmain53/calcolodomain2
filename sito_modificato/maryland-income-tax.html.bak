<script>
  function formatCurrency(value) {
    if (!isFinite(value)) return '$0';
    return value.toLocaleString('en-US', { style: 'currency', currency: 'USD', maximumFractionDigits: 0 });
  }

  function formatPercent(value) {
    if (!isFinite(value)) return '0%';
    return (value * 100).toFixed(1) + '%';
  }

  function getStandardDeduction(status, income) {
    // Very simplified mid-range estimates for MD standard deduction
    if (status === 'mfj' || status === 'hoh') {
      return Math.min(Math.max(income * 0.15, 5000), 10000);
    }
    return Math.min(Math.max(income * 0.15, 2500), 5000);
  }

  function getMdTaxableIncome(income, status, deductionType, itemized, exemptionsCount) {
    let deduction = 0;
    if (deductionType === 'standard') {
      deduction = getStandardDeduction(status, income);
    } else if (deductionType === 'itemized') {
      deduction = Math.max(0, itemized || 0);
    } else {
      deduction = 0;
    }

    const exemptionAmount = 3200; // simplified flat estimate
    const exemptionsTotal = Math.max(0, exemptionsCount) * exemptionAmount;

    let taxable = income - deduction - exemptionsTotal;
    return Math.max(0, taxable);
  }

  function getStateTax(taxable, status) {
    // Approximate MD brackets; adjust thresholds for MFJ/HOH
    let brackets;
    if (status === 'mfj' || status === 'hoh') {
      brackets = [
        { upTo: 1000 * 2, rate: 0.02 },
        { upTo: 2000 * 2, rate: 0.03 },
        { upTo: 3000 * 2, rate: 0.04 },
        { upTo: 100000 * 2, rate: 0.0475 },
        { upTo: 125000 * 2, rate: 0.05 },
        { upTo: 150000 * 2, rate: 0.0525 },
        { upTo: 250000 * 2, rate: 0.055 },
        { upTo: Infinity, rate: 0.0575 }
      ];
    } else {
      brackets = [
        { upTo: 1000, rate: 0.02 },
        { upTo: 2000, rate: 0.03 },
        { upTo: 3000, rate: 0.04 },
        { upTo: 100000, rate: 0.0475 },
        { upTo: 125000, rate: 0.05 },
        { upTo: 150000, rate: 0.0525 },
        { upTo: 250000, rate: 0.055 },
        { upTo: Infinity, rate: 0.0575 }
      ];
    }

    let remaining = taxable;
    let lastLimit = 0;
    let tax = 0;

    for (const bracket of brackets) {
      if (remaining <= 0) break;
      const taxableInBracket = Math.min(remaining, bracket.upTo - lastLimit);
      if (taxableInBracket > 0) {
        tax += taxableInBracket * bracket.rate;
        remaining -= taxableInBracket;
      }
      lastLimit = bracket.upTo;
    }
    return Math.max(0, tax);
  }

  function calculateMdTax() {
    const income = parseFloat(document.getElementById('income').value) || 0;
    const status = document.getElementById('filing-status').value;
    const countyRate = parseFloat(document.getElementById('county').value) || 0;
    const dependents = parseInt(document.getElementById('dependents').value, 10) || 0;
    const deductionType = document.getElementById('deduction-type').value;
    const itemized = parseFloat(document.getElementById('itemized').value) || 0;

    if (income <= 0) {
      document.getElementById('state-tax').textContent = '$0';
      document.getElementById('local-tax').textContent = '$0';
      document.getElementById('total-tax').textContent = '$0';
      document.getElementById('effective-rate').textContent = '0%';
      document.getElementById('taxable-income').textContent = '$0';
      document.getElementById('after-tax').textContent = '$0';
      document.getElementById('result-summary').textContent = 'Enter your income and details above to estimate your Maryland state and local income tax.';
      return;
    }

    const taxableIncome = getMdTaxableIncome(income, status, deductionType, itemized, dependents);
    const stateTax = getStateTax(taxableIncome, status);
    const localTax = taxableIncome * countyRate;
    const totalTax = stateTax + localTax;
    const afterTax = income - totalTax;
    const effectiveRate = income > 0 ? totalTax / income : 0;

    document.getElementById('state-tax').textContent = formatCurrency(stateTax);
    document.getElementById('local-tax').textContent = formatCurrency(localTax);
    document.getElementById('total-tax').textContent = formatCurrency(totalTax);
    document.getElementById('effective-rate').textContent = formatPercent(effectiveRate);
    document.getElementById('taxable-income').textContent = formatCurrency(taxableIncome);
    document.getElementById('after-tax').textContent = formatCurrency(afterTax);

    const statusLabel = {
      single: 'single',
      mfj: 'married filing jointly',
      mfs: 'married filing separately',
      hoh: 'head of household'
    }[status] || 'your';

    document.getElementById('result-summary').textContent =
      `Based on an income of ${formatCurrency(income)} as ${statusLabel}, your estimated Maryland state income tax is ` +
      `${formatCurrency(stateTax)} and your county/local tax is ${formatCurrency(localTax)}, for a total Maryland tax of ` +
      `${formatCurrency(totalTax)} (${formatPercent(effectiveRate)} of your income).`;
  }

  document.addEventListener('DOMContentLoaded', function () {
    const deductionTypeEl = document.getElementById('deduction-type');
    const itemizedWrapper = document.getElementById('itemized-wrapper');
    const calculateBtn = document.getElementById('calculate-btn');
    const resetBtn = document.getElementById('reset-btn');

    deductionTypeEl.addEventListener('change', function () {
      if (this.value === 'itemized') {
        itemizedWrapper.classList.remove('hidden');
      } else {
        itemizedWrapper.classList.add('hidden');
      }
    });

    calculateBtn.addEventListener('click', calculateMdTax);

    resetBtn.addEventListener('click', function () {
      document.getElementById('income').value = 60000;
      document.getElementById('filing-status').value = 'single';
      document.getElementById('county').selectedIndex = 0;
      document.getElementById('dependents').value = 1;
      document.getElementById('deduction-type').value = 'standard';
      document.getElementById('itemized').value = '';
      document.getElementById('itemized-wrapper').classList.add('hidden');
      calculateMdTax();
    });

    calculateMdTax();
  });
</script>

