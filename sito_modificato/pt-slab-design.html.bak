    <script>
      // Unit system handling
      const state = {
        unit: "metric", // 'metric' or 'imperial'
      };

      const els = {
        btnMetric: document.getElementById("btn-unit-metric"),
        btnImperial: document.getElementById("btn-unit-imperial"),
        unitNote: document.getElementById("unit-note"),
        spanUnit: document.getElementById("span-unit"),
        stripWidthUnit: document.getElementById("strip-width-unit"),
        thicknessUnit: document.getElementById("thickness-unit"),
        fcUnit: document.getElementById("fc-unit"),
        fpuUnit: document.getElementById("fpu-unit"),
        areaPerTendonUnit: document.getElementById("area-per-tendon-unit"),
        superDeadUnit: document.getElementById("superimposed-dead-unit"),
        liveLoadUnit: document.getElementById("live-load-unit"),
        precompressionUnit: document.getElementById("precompression-unit"),
        selfweight: document.getElementById("selfweight"),
        error: document.getElementById("calc-error"),
        resPtForce: document.getElementById("res-pt-force"),
        resPrecompression: document.getElementById("res-precompression"),
        resNumTendons: document.getElementById("res-num-tendons"),
        resSpacing: document.getElementById("res-spacing"),
        resStressTop: document.getElementById("res-stress-top"),
        resStressBottom: document.getElementById("res-stress-bottom"),
        resStatus: document.getElementById("res-status"),
        resThicknessCheck: document.getElementById("res-thickness-check"),
      };

      function setUnits(unit) {
        state.unit = unit;
        if (unit === "metric") {
          els.btnMetric.classList.add("bg-blue-600", "text-white");
          els.btnImperial.classList.remove("bg-blue-600", "text-white");
          els.btnImperial.classList.add("bg-white", "text-gray-700");
          els.unitNote.textContent = "Currently: Metric";

          els.spanUnit.textContent = "m";
          els.stripWidthUnit.textContent = "m";
          els.thicknessUnit.textContent = "mm";
          els.fcUnit.textContent = "MPa";
          els.fpuUnit.textContent = "MPa";
          els.areaPerTendonUnit.textContent = "mm²";
          els.superDeadUnit.textContent = "kN/m²";
          els.liveLoadUnit.textContent = "kN/m²";
          els.precompressionUnit.textContent = "MPa";
        } else {
          els.btnImperial.classList.add("bg-blue-600", "text-white");
          els.btnMetric.classList.remove("bg-blue-600", "text-white");
          els.btnMetric.classList.add("bg-white", "text-gray-700");
          els.unitNote.textContent = "Currently: US";

          els.spanUnit.textContent = "ft";
          els.stripWidthUnit.textContent = "ft";
          els.thicknessUnit.textContent = "in";
          els.fcUnit.textContent = "ksi";
          els.fpuUnit.textContent = "ksi";
          els.areaPerTendonUnit.textContent = "in²";
          els.superDeadUnit.textContent = "psf";
          els.liveLoadUnit.textContent = "psf";
          els.precompressionUnit.textContent = "ksi";
        }
      }

      els.btnMetric.addEventListener("click", () => setUnits("metric"));
      els.btnImperial.addEventListener("click", () => setUnits("imperial"));

      function resetForm() {
        document.getElementById("span").value = 8;
        document.getElementById("strip-width").value = 1;
        document.getElementById("thickness").value = 250;
        document.getElementById("fc").value = 35;
        document.getElementById("fpu").value = 1860;
        document.getElementById("area-per-tendon").value = 140;
        document.getElementById("superimposed-dead").value = 1.5;
        document.getElementById("live-load").value = 3;
        document.getElementById("precompression-min").value = 0.7;
        document.getElementById("precompression-max").value = 1.5;
        document.getElementById("loss-factor").value = 0.75;
        els.selfweight.value = "Computed from h, 24 kN/m³";
        els.error.textContent = "";
        els.resPtForce.textContent = "–";
        els.resPrecompression.textContent = "–";
        els.resNumTendons.textContent = "–";
        els.resSpacing.textContent = "–";
        els.resStressTop.textContent = "Top fiber: –";
        els.resStressBottom.textContent = "Bottom fiber: –";
        els.resStatus.textContent = "–";
        els.resThicknessCheck.textContent = "–";
        setUnits("metric");
      }

      document.getElementById("btn-reset").addEventListener("click", resetForm);

      function calcPtSlab() {
        els.error.textContent = "";

        const L = parseFloat(document.getElementById("span").value);
        const b = parseFloat(document.getElementById("strip-width").value);
        const hInput = parseFloat(document.getElementById("thickness").value);
        const fc = parseFloat(document.getElementById("fc").value);
        const fpu = parseFloat(document.getElementById("fpu").value);
        const ApsInput = parseFloat(
          document.getElementById("area-per-tendon").value
        );
        const gs = parseFloat(
          document.getElementById("superimposed-dead").value
        );
        const ql = parseFloat(document.getElementById("live-load").value);
        const fpcMin = parseFloat(
          document.getElementById("precompression-min").value
        );
        const fpcMax = parseFloat(
          document.getElementById("precompression-max").value
        );
        const kLoss = parseFloat(document.getElementById("loss-factor").value);

        if (
          [L, b, hInput, fc, fpu, ApsInput, gs, ql, fpcMin, fpcMax, kLoss].some(
            (v) => !isFinite(v) || v <= 0
          )
        ) {
          els.error.textContent =
            "Please enter positive numeric values for all inputs.";
          return;
        }
        if (fpcMax <= fpcMin) {
          els.error.textContent =
            "Precompression max must be greater than min.";
          return;
        }

        let h,
          Aps,
          unitWeight,
          loadFactorArea,
          stressConv,
          lengthConv,
          areaConv,
          momentConv,
          forceConv;
        let precompUnitLabel, forceUnitLabel, spacingUnitLabel, stressUnitLabel;

        if (state.unit === "metric") {
          h = hInput / 1000; // mm to m
          Aps = ApsInput / 1e6; // mm² to m²
          unitWeight = 24; // kN/m³
          loadFactorArea = 1; // kN/m² to kN/m²
          stressConv = 1; // MPa
          lengthConv = 1; // m
          areaConv = 1; // m²
          momentConv = 1; // kNm
          forceConv = 1; // kN
          precompUnitLabel = "MPa";
          forceUnitLabel = "kN";
          spacingUnitLabel = "m";
          stressUnitLabel = "MPa";
        } else {
          h = hInput / 12; // in to ft
          Aps = ApsInput / 144; // in² to ft²
          unitWeight = 150 / 1000; // kcf (150 pcf ≈ 0.15 kcf)
          loadFactorArea = 1 / 47.8803; // psf to kN/m² approx, but we will stay in kip/ft
          // For US, we will work in kip, ft, ksi internally
          // Convert everything to kip, ft, ksi
          unitWeight = 0.15; // kip/ft³
          stressConv = 1; // ksi
          precompUnitLabel = "ksi";
          forceUnitLabel = "kip";
          spacingUnitLabel = "ft";
          stressUnitLabel = "ksi";
        }

        // Self-weight line load
        let w_sw, w_g, w_l, w_s;
        if (state.unit === "metric") {
          const swAreaLoad = unitWeight * h; // kN/m²
          w_sw = swAreaLoad * b; // kN/m
          w_g = gs * b; // kN/m
          w_l = ql * b; // kN/m
          w_s = w_sw + w_g + w_l; // kN/m
          els.selfweight.value =
            swAreaLoad.toFixed(2) +
            " kN/m² (" +
            w_sw.toFixed(2) +
            " kN/m strip)";
        } else {
          const swAreaLoad = unitWeight * h; // kip/ft²
          w_sw = swAreaLoad * b; // kip/ft
          w_g = (gs / 1000) * b; // psf to kip/ft² then to kip/ft
          w_l = (ql / 1000) * b;
          w_s = w_sw + w_g + w_l; // kip/ft
          els.selfweight.value =
            (swAreaLoad * 1000).toFixed(1) +
            " psf (" +
            w_sw.toFixed(3) +
            " kip/ft strip)";
        }

        // Service moment
        let M_s;
        if (state.unit === "metric") {
          M_s = (w_s * L * L) / 8; // kNm
        } else {
          M_s = (w_s * L * L) / 8; // kip-ft
        }

        // Section properties
        let A, S;
        if (state.unit === "metric") {
          A = b * h; // m²
          S = (b * h * h) / 6; // m³
        } else {
          A = b * h; // ft²
          S = (b * h * h) / 6; // ft³
        }

        // Target precompression (mid-range)
        let fpcTarget = 0.5 * (fpcMin + fpcMax); // MPa or ksi
        // Convert to consistent units with forces and areas
        let fpcTargetBase, fpcMinBase, fpcMaxBase;
        if (state.unit === "metric") {
          // 1 MPa = 1 N/mm² = 1e6 N/m² = 1e3 kN/m²
          fpcTargetBase = fpcTarget * 1000; // kN/m²
          fpcMinBase = fpcMin * 1000;
          fpcMaxBase = fpcMax * 1000;
        } else {
          // 1 ksi = 1000 psi; 1 psi ≈ 0.006944 kip/ft²
          const psiToKipFt2 = 0.006944;
          fpcTargetBase = fpcTarget * 1000 * psiToKipFt2; // kip/ft²
          fpcMinBase = fpcMin * 1000 * psiToKipFt2;
          fpcMaxBase = fpcMax * 1000 * psiToKipFt2;
        }

        let P_eff = fpcTargetBase * A; // kN or kip
        let P_jack = P_eff / kLoss;

        // Tendon effective force
        let fpe;
        if (state.unit === "metric") {
          fpe = 0.7 * fpu; // MPa
          const fpeBase = fpe * 1000; // kN/m²
          const P_tendon = fpeBase * Aps; // kN
          let n = P_tendon > 0 ? P_eff / P_tendon : 0;
          if (n < 1) n = 1;
          const nRounded = Math.round(n);
          const P_eff_actual = nRounded * P_tendon; // kN
          const fpc_actual = P_eff_actual / A; // kN/m²
          const fpc_actual_MPa = fpc_actual / 1000;

          const spacing = b / nRounded; // m

          // Service stresses
          const fpc_base = fpc_actual; // kN/m²
          const fpc_MPa = fpc_base / 1000;
          const sigma_bend = M_s / S; // kN/m²
          const sigma_bend_MPa = sigma_bend / 1000;
          const f_top = fpc_MPa - sigma_bend_MPa;
          const f_bot = fpc_MPa + sigma_bend_MPa;

          // Status
          let statusMsg = "";
          let ok = true;
          if (f_top < -0.5) {
            ok = false;
            statusMsg += "Top fiber tension exceeds 0.5 MPa. ";
          }
          if (f_bot < -0.5) {
            ok = false;
            statusMsg += "Bottom fiber tension exceeds 0.5 MPa. ";
          }
          if (fpc_actual_MPa < fpcMin || fpc_actual_MPa > fpcMax) {
            ok = false;
            statusMsg += "Precompression outside target range. ";
          }
          if (ok)
            statusMsg =
              "Within typical precompression range and service tension limits (approximate).";

          // Thickness check
          const hRuleMin = L / 50;
          const hRuleMax = L / 45;
          let thickMsg = `Span L = ${L.toFixed(
            2
          )} m. Rule-of-thumb h ≈ L/45–L/50 → ${hRuleMin.toFixed(
            3
          )}–${hRuleMax.toFixed(3)} m. Actual h = ${h.toFixed(3)} m. `;
          if (h < hRuleMin)
            thickMsg += "Slab may be slender; check deflections carefully.";
          else if (h > hRuleMax)
            thickMsg += "Slab is relatively thick; may be conservative.";
          else thickMsg += "Slab thickness is within typical PT slab range.";

          // Populate results
          els.resPtForce.textContent =
            P_eff_actual.toFixed(1) + " " + forceUnitLabel + " (effective)";
          els.resPrecompression.textContent =
            "Precompression ≈ " +
            fpc_actual_MPa.toFixed(2) +
            " " +
            precompUnitLabel;
          els.resNumTendons.textContent = nRounded + " tendons across strip";
          els.resSpacing.textContent =
            "Average spacing ≈ " + spacing.toFixed(3) + " " + spacingUnitLabel;
          els.resStressTop.textContent =
            "Top fiber ≈ " + f_top.toFixed(2) + " " + stressUnitLabel;
          els.resStressBottom.textContent =
            "Bottom fiber ≈ " + f_bot.toFixed(2) + " " + stressUnitLabel;
          els.resStatus.textContent = statusMsg;
          els.resStatus.classList.toggle("text-green-700", ok);
          els.resStatus.classList.toggle("text-red-700", !ok);
          els.resThicknessCheck.textContent = thickMsg;
        } else {
          // US units
          fpe = 0.7 * fpu; // ksi
          const psiToKipFt2 = 0.006944;
          const fpeBase = fpe * 1000 * psiToKipFt2; // kip/ft²
          const P_tendon = fpeBase * Aps; // kip
          let n = P_tendon > 0 ? P_eff / P_tendon : 0;
          if (n < 1) n = 1;
          const nRounded = Math.round(n);
          const P_eff_actual = nRounded * P_tendon; // kip
          const fpc_actual = P_eff_actual / A; // kip/ft²
          const fpc_actual_ksi = fpc_actual / (1000 * psiToKipFt2);

          const spacing = b / nRounded; // ft

          // Service stresses
          const fpc_base = fpc_actual; // kip/ft²
          const fpc_ksi = fpc_base / (1000 * psiToKipFt2);
          const sigma_bend = M_s / S; // kip/ft²
          const sigma_bend_ksi = sigma_bend / (1000 * psiToKipFt2);
          const f_top = fpc_ksi - sigma_bend_ksi;
          const f_bot = fpc_ksi + sigma_bend_ksi;

          // Status
          let statusMsg = "";
          let ok = true;
          if (f_top < -0.07) {
            ok = false;
            statusMsg += "Top fiber tension exceeds 0.07 ksi (≈ 70 psi). ";
          }
          if (f_bot < -0.07) {
            ok = false;
            statusMsg += "Bottom fiber tension exceeds 0.07 ksi (≈ 70 psi). ";
          }
          if (fpc_actual_ksi < fpcMin || fpc_actual_ksi > fpcMax) {
            ok = false;
            statusMsg += "Precompression outside target range. ";
          }
          if (ok)
            statusMsg =
              "Within typical precompression range and service tension limits (approximate).";

          // Thickness check
          const hRuleMin = L / 50;
          const hRuleMax = L / 45;
          let thickMsg = `Span L = ${L.toFixed(
            2
          )} ft. Rule-of-thumb h ≈ L/45–L/50 → ${hRuleMin.toFixed(
            2
          )}–${hRuleMax.toFixed(2)} ft. Actual h = ${h.toFixed(2)} ft. `;
          if (h < hRuleMin)
            thickMsg += "Slab may be slender; check deflections carefully.";
          else if (h > hRuleMax)
            thickMsg += "Slab is relatively thick; may be conservative.";
          else thickMsg += "Slab thickness is within typical PT slab range.";

          els.resPtForce.textContent =
            P_eff_actual.toFixed(2) + " " + forceUnitLabel + " (effective)";
          els.resPrecompression.textContent =
            "Precompression ≈ " +
            fpc_actual_ksi.toFixed(3) +
            " " +
            precompUnitLabel;
          els.resNumTendons.textContent = nRounded + " tendons across strip";
          els.resSpacing.textContent =
            "Average spacing ≈ " + spacing.toFixed(2) + " " + spacingUnitLabel;
          els.resStressTop.textContent =
            "Top fiber ≈ " + f_top.toFixed(3) + " " + stressUnitLabel;
          els.resStressBottom.textContent =
            "Bottom fiber ≈ " + f_bot.toFixed(3) + " " + stressUnitLabel;
          els.resStatus.textContent = statusMsg;
          els.resStatus.classList.toggle("text-green-700", ok);
          els.resStatus.classList.toggle("text-red-700", !ok);
          els.resThicknessCheck.textContent = thickMsg;
        }
      }

      document.getElementById("btn-calc").addEventListener("click", calcPtSlab);

      // Initialize
      resetForm();

      // Mobile menu script (from template)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          const expanded = this.getAttribute("aria-expanded") === "true";
          this.setAttribute("aria-expanded", String(!expanded));
          menu.classList.toggle("hidden");
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
