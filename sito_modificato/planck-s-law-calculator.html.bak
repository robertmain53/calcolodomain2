    <script>
      // Physical constants (SI)
      const h = 6.62607015e-34; // Planck constant [J·s]
      const c = 2.99792458e8; // speed of light [m/s]
      const kB = 1.380649e-23; // Boltzmann constant [J/K]
      const sigma = 5.670374419e-8; // Stefan–Boltzmann constant [W·m^-2·K^-4]
      const bWien = 2.897771955e-3; // Wien displacement constant [m·K]

      function toKelvin(value, unit) {
        const v = Number(value);
        if (!isFinite(v)) return NaN;
        if (unit === "K") return v;
        if (unit === "C") return v + 273.15;
        if (unit === "F") return ((v - 32) * 5) / 9 + 273.15;
        return NaN;
      }

      function wavelengthToMeters(value, unit) {
        const v = Number(value);
        if (!isFinite(v)) return NaN;
        if (unit === "m") return v;
        if (unit === "nm") return v * 1e-9;
        if (unit === "um") return v * 1e-6;
        return NaN;
      }

      function frequencyToHz(value, unit) {
        const v = Number(value);
        if (!isFinite(v)) return NaN;
        if (unit === "Hz") return v;
        if (unit === "GHz") return v * 1e9;
        if (unit === "THz") return v * 1e12;
        return NaN;
      }

      function planckWavelength(lambda, T) {
        if (lambda <= 0 || T <= 0) return NaN;
        const a = 2 * h * c * c;
        const b = (h * c) / (lambda * kB * T);
        const denom = Math.exp(b) - 1;
        if (!isFinite(denom) || denom === 0) return 0;
        return a / (Math.pow(lambda, 5) * denom); // W·sr^-1·m^-3
      }

      function planckFrequency(nu, T) {
        if (nu <= 0 || T <= 0) return NaN;
        const a = (2 * h * Math.pow(nu, 3)) / (c * c);
        const b = (h * nu) / (kB * T);
        const denom = Math.exp(b) - 1;
        if (!isFinite(denom) || denom === 0) return 0;
        return a / denom; // W·sr^-1·m^-2·Hz^-1
      }

      function formatNumber(x, digits = 4) {
        if (!isFinite(x)) return "—";
        if (x === 0) return "0";
        const absx = Math.abs(x);
        if (absx >= 1e-3 && absx < 1e4) {
          return x.toFixed(digits);
        }
        return x.toExponential(digits);
      }

      function updateDomainVisibility() {
        const domain = document.querySelector(
          'input[name="domain"]:checked'
        ).value;
        document
          .getElementById("wavelength-group")
          .classList.toggle("hidden", domain !== "wavelength");
        document
          .getElementById("frequency-group")
          .classList.toggle("hidden", domain !== "frequency");
      }

      document.querySelectorAll('input[name="domain"]').forEach((r) => {
        r.addEventListener("change", updateDomainVisibility);
      });

      let spectrumChart = null;

      function computeRadiance() {
        const T = toKelvin(
          document.getElementById("tempValue").value,
          document.getElementById("tempUnit").value
        );
        if (!isFinite(T) || T <= 0) {
          alert("Please enter a valid temperature greater than 0 K.");
          return;
        }

        const domain = document.querySelector(
          'input[name="domain"]:checked'
        ).value;
        const radUnit = document.getElementById("radianceUnit").value;

        let B_SI = NaN;
        let label = "";

        if (domain === "wavelength") {
          const lambda = wavelengthToMeters(
            document.getElementById("lambdaValue").value,
            document.getElementById("lambdaUnit").value
          );
          if (!isFinite(lambda) || lambda <= 0) {
            alert("Please enter a valid wavelength.");
            return;
          }
          B_SI = planckWavelength(lambda, T); // W·sr^-1·m^-3
          if (radUnit === "SI") {
            label = "Bλ in W·sr⁻¹·m⁻³";
          } else if (radUnit === "per_nm") {
            B_SI *= 1e-9; // per nm
            label = "Bλ in W·sr⁻¹·m⁻²·nm⁻¹ (approx.)";
          } else if (radUnit === "per_um") {
            B_SI *= 1e-6; // per µm
            label = "Bλ in W·sr⁻¹·m⁻²·µm⁻¹ (approx.)";
          }
        } else {
          const nu = frequencyToHz(
            document.getElementById("freqValue").value,
            document.getElementById("freqUnit").value
          );
          if (!isFinite(nu) || nu <= 0) {
            alert("Please enter a valid frequency.");
            return;
          }
          B_SI = planckFrequency(nu, T); // W·sr^-1·m^-2·Hz^-1
          if (radUnit === "SI") {
            label = "Bν in W·sr⁻¹·m⁻²·Hz⁻¹";
          } else if (radUnit === "per_nm") {
            // approximate conversion via dν/dλ = -c/λ^2; for display only we keep SI
            label = "Bν in W·sr⁻¹·m⁻²·Hz⁻¹ (SI, per Hz)";
          } else if (radUnit === "per_um") {
            label = "Bν in W·sr⁻¹·m⁻²·Hz⁻¹ (SI, per Hz)";
          }
        }

        document.getElementById("radianceValue").textContent = formatNumber(
          B_SI,
          4
        );
        document.getElementById("radianceUnitsLabel").textContent = label;

        // Wien peak and Stefan–Boltzmann
        const lambdaMax = bWien / T; // m
        const lambdaMaxNm = lambdaMax * 1e9;
        document.getElementById("wienPeak").textContent =
          "Peak wavelength (Wien): " + formatNumber(lambdaMaxNm, 3) + " nm";

        const jStar = sigma * Math.pow(T, 4);
        document.getElementById("stefanPower").textContent =
          "Total emitted power σT⁴: " + formatNumber(jStar, 3) + " W·m⁻²";
      }

      function autoRangeWavelength(T) {
        const lambdaMax = bWien / T;
        const min = lambdaMax / 20;
        const max = lambdaMax * 20;
        return [min, max];
      }

      function autoRangeFrequency(T) {
        const lambdaMax = bWien / T;
        const nuMax = c / lambdaMax;
        const min = nuMax / 50;
        const max = nuMax * 5;
        return [min, max];
      }

      function plotSpectrum() {
        const T = toKelvin(
          document.getElementById("tempValue").value,
          document.getElementById("tempUnit").value
        );
        if (!isFinite(T) || T <= 0) {
          alert("Please enter a valid temperature greater than 0 K.");
          return;
        }
        const domain = document.querySelector(
          'input[name="domain"]:checked'
        ).value;

        const N = 200;
        const xs = [];
        const ys = [];

        if (domain === "wavelength") {
          const [lambdaMin, lambdaMax] = autoRangeWavelength(T);
          for (let i = 0; i < N; i++) {
            const t = i / (N - 1);
            const lambda = lambdaMin * Math.pow(lambdaMax / lambdaMin, t); // log spacing
            const B = planckWavelength(lambda, T);
            xs.push(lambda * 1e9); // nm
            ys.push(B);
          }
        } else {
          const [nuMin, nuMax] = autoRangeFrequency(T);
          for (let i = 0; i < N; i++) {
            const t = i / (N - 1);
            const nu = nuMin * Math.pow(nuMax / nuMin, t);
            const B = planckFrequency(nu, T);
            xs.push(nu * 1e-12); // THz
            ys.push(B);
          }
        }

        const canvas = document.getElementById("spectrumCanvas");
        const ctx = canvas.getContext("2d");
        ctx.clearRect(0, 0, canvas.width, canvas.height);

        // Simple manual plot (no external chart library)
        const w = (canvas.width = canvas.clientWidth || 600);
        const h = (canvas.height = 320);

        const minX = Math.min(...xs);
        const maxX = Math.max(...xs);
        const minY = 0;
        const maxY = Math.max(...ys) * 1.05;

        function xToPx(x) {
          return 40 + ((x - minX) / (maxX - minX)) * (w - 60);
        }
        function yToPx(y) {
          return h - 30 - ((y - minY) / (maxY - minY)) * (h - 60);
        }

        // Axes
        ctx.strokeStyle = "#4b5563";
        ctx.lineWidth = 1;
        ctx.beginPath();
        ctx.moveTo(40, 10);
        ctx.lineTo(40, h - 30);
        ctx.lineTo(w - 20, h - 30);
        ctx.stroke();

        ctx.fillStyle = "#374151";
        ctx.font = "11px Inter, system-ui, sans-serif";
        const xLabel =
          domain === "wavelength" ? "Wavelength (nm)" : "Frequency (THz)";
        ctx.fillText(xLabel, w / 2 - 40, h - 8);
        ctx.save();
        ctx.translate(14, h / 2 + 20);
        ctx.rotate(-Math.PI / 2);
        ctx.fillText("Spectral radiance (relative)", 0, 0);
        ctx.restore();

        // Normalize for nicer plotting
        const maxYPlot = Math.max(...ys);
        const ysNorm = ys.map((v) => v / maxYPlot);

        ctx.strokeStyle = "#2563eb";
        ctx.lineWidth = 1.5;
        ctx.beginPath();
        ysNorm.forEach((y, i) => {
          const px = xToPx(xs[i]);
          const py = yToPx(y * maxYPlot);
          if (i === 0) ctx.moveTo(px, py);
          else ctx.lineTo(px, py);
        });
        ctx.stroke();
      }

      function resetForm() {
        document.getElementById("tempValue").value = 5778;
        document.getElementById("tempUnit").value = "K";
        document.querySelector(
          'input[name="domain"][value="wavelength"]'
        ).checked = true;
        updateDomainVisibility();
        document.getElementById("lambdaValue").value = 500;
        document.getElementById("lambdaUnit").value = "nm";
        document.getElementById("freqValue").value = 600;
        document.getElementById("freqUnit").value = "THz";
        document.getElementById("radianceUnit").value = "SI";
        document.getElementById("radianceValue").textContent = "—";
        document.getElementById("radianceUnitsLabel").textContent =
          "Bλ or Bν at the specified point.";
        document.getElementById("wienPeak").textContent = "Peak wavelength: —";
        document.getElementById("stefanPower").textContent =
          "Total emitted power σT⁴: —";
        const canvas = document.getElementById("spectrumCanvas");
        const ctx = canvas.getContext("2d");
        ctx.clearRect(0, 0, canvas.width, canvas.height);
      }

      document
        .getElementById("calcButton")
        .addEventListener("click", computeRadiance);
      document
        .getElementById("plotButton")
        .addEventListener("click", plotSpectrum);
      document
        .getElementById("resetButton")
        .addEventListener("click", resetForm);

      // Initialize
      updateDomainVisibility();
      computeRadiance();
      plotSpectrum();
    </script>

