        document.getElementById('calculate').addEventListener('click', function () {
            const purchasePrice = parseFloat(document.getElementById('purchasePrice').value);
            const location = document.getElementById('location').value;
            let taxRate = 0;

            if (location === 'ontario') {
                taxRate = 0.02; // Example rate
            } else if (location === 'bc') {
                taxRate = 0.015; // Example rate
            }

            if (!isNaN(purchasePrice) && taxRate > 0) {
                const tax = purchasePrice * taxRate;
                document.getElementById('landTransferTaxValue').textContent = `$${tax.toFixed(2)}`;
            } else {
                alert('Please enter a valid purchase price and select a state/province.');
            }
        });
    </script>


