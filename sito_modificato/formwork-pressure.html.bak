    <script>
      // Mobile menu toggle
      document
        .getElementById("mobile-menu-toggle")
        ?.addEventListener("click", function () {
          const menu = document.getElementById("mobile-menu");
          if (!menu) return;
          const isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          this.setAttribute("aria-expanded", String(isHidden));
        });

      // Helper: format number
      function fmt(x, decimals = 1) {
        if (!isFinite(x)) return "–";
        return Number(x).toFixed(decimals);
      }

      const usBtn = document.getElementById("units-us");
      const metricBtn = document.getElementById("units-metric");
      const typeNormalBtn = document.getElementById("type-normal");
      const typeSccBtn = document.getElementById("type-scc");

      const heightInput = document.getElementById("height");
      const rateInput = document.getElementById("rate");
      const tempInput = document.getElementById("temp");
      const slumpInput = document.getElementById("slump");
      const gammaInput = document.getElementById("unitWeight");
      const vibrationSelect = document.getElementById("vibration");

      const heightUnitLabel = document.getElementById("height-unit-label");
      const rateUnitLabel = document.getElementById("rate-unit-label");
      const tempUnitLabel = document.getElementById("temp-unit-label");
      const slumpUnitLabel = document.getElementById("slump-unit-label");
      const gammaUnitLabel = document.getElementById("gamma-unit-label");

      const slumpWrapper = document.getElementById("slump-wrapper");
      const rateWrapper = document.getElementById("rate-wrapper");

      const warningText = document.getElementById("warning-text");

      const resultsBox = document.getElementById("results");
      const resultPressureMain = document.getElementById(
        "result-pressure-main"
      );
      const resultPressureAlt = document.getElementById("result-pressure-alt");
      const resultControl = document.getElementById("result-control");
      const resultAci = document.getElementById("result-aci");
      const resultHydro = document.getElementById("result-hydro");
      const resultFluid = document.getElementById("result-fluid");

      let unitSystem = "us"; // 'us' or 'metric'
      let concreteType = "normal"; // 'normal' or 'scc'

      function setUnitSystem(system) {
        unitSystem = system;
        if (system === "us") {
          usBtn.classList.add("active");
          metricBtn.classList.remove("active");
          heightUnitLabel.textContent = "(ft)";
          rateUnitLabel.textContent = "(ft/h)";
          tempUnitLabel.textContent = "(°F)";
          slumpUnitLabel.textContent = "(in)";
          gammaUnitLabel.textContent = "(pcf)";
          // Typical defaults
          if (!heightInput.value) heightInput.value = 12;
          if (!rateInput.value) rateInput.value = 5;
          if (!tempInput.value) tempInput.value = 70;
          if (!slumpInput.value) slumpInput.value = 5;
          if (!gammaInput.value) gammaInput.value = 150;
        } else {
          metricBtn.classList.add("active");
          usBtn.classList.remove("active");
          heightUnitLabel.textContent = "(m)";
          rateUnitLabel.textContent = "(m/h)";
          tempUnitLabel.textContent = "(°C)";
          slumpUnitLabel.textContent = "(mm)";
          gammaUnitLabel.textContent = "(kN/m³)";
          if (!heightInput.value) heightInput.value = 3.5;
          if (!rateInput.value) rateInput.value = 1.5;
          if (!tempInput.value) tempInput.value = 21;
          if (!slumpInput.value) slumpInput.value = 125;
          if (!gammaInput.value) gammaInput.value = 24;
        }
      }

      function setConcreteType(type) {
        concreteType = type;
        if (type === "normal") {
          typeNormalBtn.classList.add("active");
          typeSccBtn.classList.remove("active");
          slumpWrapper.classList.remove("hidden");
          rateWrapper.classList.remove("hidden");
          vibrationSelect.disabled = false;
        } else {
          typeSccBtn.classList.add("active");
          typeNormalBtn.classList.remove("active");
          // For SCC we still allow rate input but we ignore it in calc; keep visible for user context
          slumpWrapper.classList.add("hidden");
          vibrationSelect.disabled = true;
        }
      }

      usBtn.addEventListener("click", () => setUnitSystem("us"));
      metricBtn.addEventListener("click", () => setUnitSystem("metric"));
      typeNormalBtn.addEventListener("click", () => setConcreteType("normal"));
      typeSccBtn.addEventListener("click", () => setConcreteType("scc"));

      setUnitSystem("us");
      setConcreteType("normal");

      document.getElementById("calc-btn").addEventListener("click", () => {
        warningText.textContent = "";

        let h = parseFloat(heightInput.value);
        let R = parseFloat(rateInput.value);
        let T = parseFloat(tempInput.value);
        let slump = parseFloat(slumpInput.value);
        let gamma = parseFloat(gammaInput.value);

        if (!isFinite(h) || h <= 0 || !isFinite(gamma) || gamma <= 0) {
          warningText.textContent =
            "Enter a positive pour height and concrete unit weight.";
          resultsBox.classList.add("hidden");
          return;
        }

        // Convert to US internal units for ACI equation
        let h_ft, R_ftph, T_F, gamma_pcf;
        if (unitSystem === "us") {
          h_ft = h;
          R_ftph = isFinite(R) ? R : 0;
          T_F = T;
          gamma_pcf = gamma;
        } else {
          h_ft = h * 3.28084;
          R_ftph = isFinite(R) ? R * 3.28084 : 0;
          T_F = (T * 9) / 5 + 32;
          gamma_pcf = gamma * 6.36588; // 1 kN/m³ ≈ 6.36588 pcf
          slump = slump / 25.4; // mm to in
        }

        const pHydro_psf = gamma_pcf * h_ft;

        let pAci_psf = NaN;
        let control = "";

        if (concreteType === "scc") {
          // Conservative: full hydrostatic
          pAci_psf = NaN;
          control = "Hydrostatic (SCC – conservative)";
        } else {
          // ACI 347 empirical equation (US units)
          if (!isFinite(R_ftph) || R_ftph <= 0 || !isFinite(T_F)) {
            warningText.textContent =
              "Enter a valid rate of placement and temperature for ACI 347 calculation.";
          } else {
            pAci_psf = 150 + (900 * R_ftph) / (T_F + 70);
            control = "ACI 347 empirical equation";

            // Check ranges and warn if outside typical ACI range
            const warnings = [];
            if (slump < 4 || slump > 7) {
              warnings.push("slump outside 4–7 in");
            }
            if (T_F < 50 || T_F > 90) {
              warnings.push("temperature outside 50–90°F");
            }
            if (R_ftph < 2 || R_ftph > 20) {
              warnings.push("placement rate outside typical range");
            }
            if (warnings.length) {
              warningText.textContent =
                "Warning: " +
                warnings.join("; ") +
                ". ACI 347 equation may be less accurate.";
            }
          }
        }

        let pDesign_psf;
        if (concreteType === "scc") {
          pDesign_psf = pHydro_psf;
        } else {
          if (!isFinite(pAci_psf)) {
            pDesign_psf = pHydro_psf;
            control = "Hydrostatic (ACI input incomplete)";
          } else {
            pDesign_psf = Math.min(pAci_psf, pHydro_psf);
            if (pHydro_psf < pAci_psf) {
              control = "Hydrostatic head controls";
            } else {
              control = "ACI 347 empirical equation controls";
            }
          }
        }

        // Equivalent fluid pressure (psf/ft)
        const eqFluid_psf_per_ft = pDesign_psf / h_ft;

        // Convert outputs to display units
        let mainStr, altStr, aciStr, hydroStr, fluidStr;
        if (unitSystem === "us") {
          mainStr = fmt(pDesign_psf, 0) + " psf";
          altStr = "(" + fmt(pDesign_psf / 144, 2) + " psi)";
          aciStr = isFinite(pAci_psf)
            ? fmt(pAci_psf, 0) + " psf"
            : "Not used (SCC or incomplete input)";
          hydroStr = fmt(pHydro_psf, 0) + " psf";
          fluidStr = fmt(eqFluid_psf_per_ft, 1) + " psf/ft";
        } else {
          const pDesign_kPa = pDesign_psf * 0.0478803; // 1 psf ≈ 0.0478803 kPa
          const pAci_kPa = isFinite(pAci_psf) ? pAci_psf * 0.0478803 : NaN;
          const pHydro_kPa = pHydro_psf * 0.0478803;
          const eqFluid_kPa_per_m = eqFluid_psf_per_ft * 0.157087; // psf/ft to kPa/m

          mainStr = fmt(pDesign_kPa, 1) + " kPa";
          altStr = "(" + fmt(pDesign_kPa / 9.80665, 2) + " m of water approx.)";
          aciStr = isFinite(pAci_kPa)
            ? fmt(pAci_kPa, 1) + " kPa"
            : "Not used (SCC or incomplete input)";
          hydroStr = fmt(pHydro_kPa, 1) + " kPa";
          fluidStr = fmt(eqFluid_kPa_per_m, 2) + " kPa/m";
        }

        resultPressureMain.textContent = mainStr;
        resultPressureAlt.textContent = altStr;
        resultControl.textContent = control;
        resultAci.textContent = aciStr;
        resultHydro.textContent = hydroStr;
        resultFluid.textContent = fluidStr;

        resultsBox.classList.remove("hidden");
      });

      document.getElementById("reset-btn").addEventListener("click", () => {
        setUnitSystem("us");
        setConcreteType("normal");
        heightInput.value = 12;
        rateInput.value = 5;
        tempInput.value = 70;
        slumpInput.value = 5;
        gammaInput.value = 150;
        vibrationSelect.value = "internal";
        warningText.textContent = "";
        resultsBox.classList.add("hidden");
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
