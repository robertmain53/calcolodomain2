    <script>
      // --- Utility functions ---
      function parseDateInput(id) {
        const v = document.getElementById(id).value;
        if (!v) return null;
        const d = new Date(v + "T00:00:00");
        return isNaN(d.getTime()) ? null : d;
      }

      function formatDateISO(d) {
        return d.toISOString().slice(0, 10);
      }

      function cloneDate(d) {
        return new Date(d.getTime());
      }

      function addDays(date, n) {
        const d = new Date(date.getTime());
        d.setDate(d.getDate() + n);
        return d;
      }

      function dateDiffDays(a, b) {
        const ms = b.getTime() - a.getTime();
        return Math.round(ms / 86400000);
      }

      function isSameDate(a, b) {
        return (
          a.getFullYear() === b.getFullYear() &&
          a.getMonth() === b.getMonth() &&
          a.getDate() === b.getDate()
        );
      }

      // --- Workweek & holidays state ---
      const workweek = new Set([1, 2, 3, 4, 5]); // 0=Sun..6=Sat
      let regionHolidays = []; // {date: 'YYYY-MM-DD', label}
      let customHolidays = [];

      function getAllHolidaysSet() {
        const set = new Map();
        [...regionHolidays, ...customHolidays].forEach((h) => {
          if (!set.has(h.date)) set.set(h.date, h.label || "Holiday");
        });
        return set;
      }

      function isHoliday(date, holidaysMap) {
        return holidaysMap.has(formatDateISO(date));
      }

      function isWorkday(date, holidaysMap, countHolidaysAsWorkdays) {
        const dow = date.getDay();
        if (!workweek.has(dow)) return false;
        if (!countHolidaysAsWorkdays && isHoliday(date, holidaysMap))
          return false;
        return true;
      }

      // --- Region holiday presets (very simplified, year-agnostic patterns) ---
      function loadRegionHolidays(region, year) {
        const y = year;
        const holidays = [];

        function add(dateStr, label) {
          holidays.push({ date: dateStr, label });
        }

        function fixed(month, day, label) {
          const d = new Date(Date.UTC(y, month - 1, day));
          add(formatDateISO(d), label);
        }

        if (region === "us") {
          fixed(1, 1, "New Year's Day");
          fixed(7, 4, "Independence Day");
          fixed(11, 11, "Veterans Day");
          fixed(12, 25, "Christmas Day");
          // simple approximations for some floating holidays
          // Labor Day: first Monday of September
          let d = new Date(Date.UTC(y, 8, 1));
          while (d.getUTCDay() !== 1) d.setUTCDate(d.getUTCDate() + 1);
          add(formatDateISO(d), "Labor Day");
          // Thanksgiving: 4th Thursday of November
          d = new Date(Date.UTC(y, 10, 1));
          let countThu = 0;
          while (d.getUTCMonth() === 10) {
            if (d.getUTCDay() === 4) {
              countThu++;
              if (countThu === 4) {
                add(formatDateISO(d), "Thanksgiving");
                break;
              }
            }
            d.setUTCDate(d.getUTCDate() + 1);
          }
        } else if (region === "uk") {
          fixed(1, 1, "New Year's Day");
          fixed(12, 25, "Christmas Day");
          fixed(12, 26, "Boxing Day");
        } else if (region === "ca") {
          fixed(7, 1, "Canada Day");
          fixed(12, 25, "Christmas Day");
          fixed(12, 26, "Boxing Day");
        } else if (region === "eu") {
          fixed(1, 1, "New Year's Day");
          fixed(5, 1, "Labour Day");
          fixed(12, 25, "Christmas Day");
          fixed(12, 26, "Second Day of Christmas");
        } else if (region === "au") {
          fixed(1, 1, "New Year's Day");
          fixed(1, 26, "Australia Day");
          fixed(12, 25, "Christmas Day");
          fixed(12, 26, "Boxing Day");
        } else if (region === "in") {
          fixed(1, 26, "Republic Day");
          fixed(8, 15, "Independence Day");
          fixed(10, 2, "Gandhi Jayanti");
        }

        regionHolidays = holidays;
        renderHolidayList();
      }

      function renderHolidayList() {
        const container = document.getElementById("holiday-list");
        container.innerHTML = "";
        const all = [
          ...regionHolidays.map((h) => ({ ...h, type: "Region" })),
          ...customHolidays.map((h) => ({ ...h, type: "Custom" })),
        ];

        if (!all.length) {
          container.innerHTML =
            '<p class="text-gray-500">No holidays added yet. Select a region or add your own dates.</p>';
          return;
        }

        const ul = document.createElement("ul");
        ul.className = "space-y-1";
        all.sort((a, b) => a.date.localeCompare(b.date));
        all.forEach((h, idx) => {
          const li = document.createElement("li");
          li.className = "flex items-center justify-between gap-2";
          li.innerHTML = `
        <span>
          <span class="font-mono">${h.date}</span>
          <span class="ml-1">${h.label ? "– " + h.label : ""}</span>
          <span class="ml-2 text-[10px] uppercase tracking-wide text-gray-500">${
            h.type
          }</span>
        </span>
        ${
          h.type === "Custom"
            ? `<button data-idx="${idx}" class="remove-custom-holiday text-[10px] text-red-600 hover:underline">Remove</button>`
            : ""
        }
      `;
          ul.appendChild(li);
        });
        container.appendChild(ul);

        container.querySelectorAll(".remove-custom-holiday").forEach((btn) => {
          btn.addEventListener("click", () => {
            const idx = parseInt(btn.getAttribute("data-idx"), 10);
            if (!isNaN(idx)) {
              customHolidays.splice(idx, 1);
              renderHolidayList();
            }
          });
        });
      }

      // --- Workweek toggle UI ---
      document.querySelectorAll(".workday-toggle").forEach((btn) => {
        btn.addEventListener("click", () => {
          const day = parseInt(btn.getAttribute("data-day"), 10);
          if (workweek.has(day)) {
            workweek.delete(day);
            btn.classList.remove(
              "border-blue-500",
              "bg-blue-50",
              "text-blue-700",
              "font-medium"
            );
            btn.classList.add("border-gray-300", "bg-white", "text-gray-600");
          } else {
            workweek.add(day);
            btn.classList.add(
              "border-blue-500",
              "bg-blue-50",
              "text-blue-700",
              "font-medium"
            );
            btn.classList.remove(
              "border-gray-300",
              "bg-white",
              "text-gray-600"
            );
          }
        });
      });

      // --- Region select ---
      document
        .getElementById("region-select")
        .addEventListener("change", (e) => {
          const region = e.target.value;
          if (region === "none") {
            regionHolidays = [];
            renderHolidayList();
            return;
          }
          const today = new Date();
          loadRegionHolidays(region, today.getFullYear());
        });

      // --- Custom holidays ---
      document
        .getElementById("btn-add-holiday")
        .addEventListener("click", () => {
          const d = parseDateInput("holiday-date");
          if (!d) {
            alert("Please select a holiday date.");
            return;
          }
          const label = document.getElementById("holiday-label").value.trim();
          const iso = formatDateISO(d);
          if (!customHolidays.some((h) => h.date === iso)) {
            customHolidays.push({ date: iso, label });
            customHolidays.sort((a, b) => a.date.localeCompare(b.date));
            renderHolidayList();
          }
        });

      document
        .getElementById("btn-clear-holidays")
        .addEventListener("click", () => {
          if (
            confirm("Remove all custom holidays? Region holidays will remain.")
          ) {
            customHolidays = [];
            renderHolidayList();
          }
        });

      // --- Tabs ---
      document.querySelectorAll(".tab-btn").forEach((btn) => {
        btn.addEventListener("click", () => {
          const target = btn.getAttribute("data-target");
          document.querySelectorAll(".tab-btn").forEach((b) => {
            b.classList.remove("border-blue-500", "text-blue-600");
            b.classList.add("border-transparent", "text-gray-600");
          });
          btn.classList.add("border-blue-500", "text-blue-600");
          btn.classList.remove("text-gray-600");

          document
            .querySelectorAll(".calc-panel")
            .forEach((p) => p.classList.add("hidden"));
          document.getElementById(target).classList.remove("hidden");
        });
      });

      // --- Between dates calculation ---
      document
        .getElementById("btn-calc-between")
        .addEventListener("click", () => {
          let start = parseDateInput("between-start");
          let end = parseDateInput("between-end");
          if (!start || !end) {
            alert("Please select both start and end dates.");
            return;
          }

          // Normalize to midnight
          start = new Date(
            start.getFullYear(),
            start.getMonth(),
            start.getDate()
          );
          end = new Date(end.getFullYear(), end.getMonth(), end.getDate());

          if (end < start) {
            const tmp = start;
            start = end;
            end = tmp;
          }

          const includeStart =
            document.getElementById("include-start").value === "yes";
          const includeEnd =
            document.getElementById("include-end").value === "yes";
          const countHolidaysAsWorkdays =
            document.getElementById("count-holidays-between").value === "yes";

          const holidaysMap = getAllHolidaysSet();

          let rangeStart = start;
          let rangeEnd = end;

          if (!includeStart) rangeStart = addDays(rangeStart, 1);
          if (!includeEnd) rangeEnd = addDays(rangeEnd, -1);

          if (rangeEnd < rangeStart) {
            // No days in range
            document.getElementById("between-workdays").textContent = "0";
            document.getElementById("between-total-days").textContent = "0";
            document.getElementById("between-weekends").textContent = "0";
            document.getElementById("between-holidays").textContent = "0";
            document.getElementById("between-breakdown").textContent =
              "No days in the selected range.";
            document
              .getElementById("result-between")
              .classList.remove("hidden");
            return;
          }

          let totalDays = dateDiffDays(rangeStart, rangeEnd) + 1;
          let workdaysCount = 0;
          let weekendCount = 0;
          let holidayCount = 0;
          let breakdownLines = [];

          let current = cloneDate(rangeStart);
          while (current <= rangeEnd) {
            const iso = formatDateISO(current);
            const dow = current.getDay();
            const isWeekend = !workweek.has(dow);
            const isHol = holidaysMap.has(iso);
            let flag = "";

            if (isWeekend) {
              weekendCount++;
              flag = "Weekend";
            } else if (isHol && !countHolidaysAsWorkdays) {
              holidayCount++;
              flag = "Holiday";
            } else {
              workdaysCount++;
              flag = "Workday";
            }

            const label = holidaysMap.get(iso) || "";
            breakdownLines.push(
              `${iso}  ${
                ["Sun", "Mon", "Tue", "Wed", "Thu", "Fri", "Sat"][dow]
              }  ${flag}${label ? " – " + label : ""}`
            );
            current = addDays(current, 1);
          }

          document.getElementById("between-workdays").textContent =
            workdaysCount;
          document.getElementById("between-total-days").textContent = totalDays;
          document.getElementById("between-weekends").textContent =
            weekendCount;
          document.getElementById("between-holidays").textContent =
            holidayCount;
          document.getElementById("between-breakdown").textContent =
            breakdownLines.join("\n");
          document.getElementById("result-between").classList.remove("hidden");
        });

      document
        .getElementById("btn-swap-between")
        .addEventListener("click", () => {
          const s = document.getElementById("between-start").value;
          const e = document.getElementById("between-end").value;
          document.getElementById("between-start").value = e;
          document.getElementById("between-end").value = s;
        });

      document
        .getElementById("btn-today-range")
        .addEventListener("click", () => {
          const today = new Date();
          const day = today.getDay();
          const diffToMonday = day === 0 ? -6 : 1 - day;
          const monday = addDays(today, diffToMonday);
          const friday = addDays(monday, 4);
          document.getElementById("between-start").value =
            formatDateISO(monday);
          document.getElementById("between-end").value = formatDateISO(friday);
        });

      // --- Add/Subtract workdays ---
      document.getElementById("btn-calc-add").addEventListener("click", () => {
        let start = parseDateInput("add-start");
        if (!start) {
          alert("Please select a start date.");
          return;
        }
        start = new Date(
          start.getFullYear(),
          start.getMonth(),
          start.getDate()
        );

        let n = parseInt(document.getElementById("add-days").value, 10);
        if (isNaN(n) || n === 0) {
          alert("Please enter a non-zero number of workdays.");
          return;
        }

        const countHolidaysAsWorkdays =
          document.getElementById("count-holidays-add").value === "yes";
        const holidaysMap = getAllHolidaysSet();

        let current = cloneDate(start);
        let remaining = Math.abs(n);
        let weekendsSkipped = 0;
        let holidaysSkipped = 0;
        let breakdownLines = [];
        const direction = n > 0 ? 1 : -1;

        while (remaining > 0) {
          current = addDays(current, direction);
          const iso = formatDateISO(current);
          const dow = current.getDay();
          const isWeekend = !workweek.has(dow);
          const isHol = holidaysMap.has(iso);

          if (isWeekend) {
            weekendsSkipped++;
            breakdownLines.push(
              `${iso}  ${
                ["Sun", "Mon", "Tue", "Wed", "Thu", "Fri", "Sat"][dow]
              }  Weekend (skipped)`
            );
            continue;
          }
          if (isHol && !countHolidaysAsWorkdays) {
            holidaysSkipped++;
            breakdownLines.push(
              `${iso}  ${
                ["Sun", "Mon", "Tue", "Wed", "Thu", "Fri", "Sat"][dow]
              }  Holiday (skipped) – ${holidaysMap.get(iso) || ""}`
            );
            continue;
          }

          remaining--;
          breakdownLines.push(
            `${iso}  ${
              ["Sun", "Mon", "Tue", "Wed", "Thu", "Fri", "Sat"][dow]
            }  Workday #${Math.abs(n) - remaining}`
          );
        }

        document.getElementById("add-target-date").textContent =
          formatDateISO(current);
        document.getElementById("add-direction").textContent =
          n > 0 ? "Forward" : "Backward";
        document.getElementById("add-workdays-moved").textContent = Math.abs(n);
        document.getElementById("add-weekends-skipped").textContent =
          weekendsSkipped;
        document.getElementById("add-holidays-skipped").textContent =
          holidaysSkipped;
        document.getElementById("add-breakdown").textContent =
          breakdownLines.join("\n");
        document.getElementById("result-add").classList.remove("hidden");
      });

      document.getElementById("btn-add-today").addEventListener("click", () => {
        const today = new Date();
        document.getElementById("add-start").value = formatDateISO(today);
      });

      // --- Initialize defaults ---
      (function initDefaults() {
        const today = new Date();
        const nextWeek = addDays(today, 7);
        document.getElementById("between-start").value = formatDateISO(today);
        document.getElementById("between-end").value = formatDateISO(nextWeek);
        document.getElementById("add-start").value = formatDateISO(today);
      })();
    </script>

