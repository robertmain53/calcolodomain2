    <script>
      // Utility conversions
      function cmFromImperial(ft, inch) {
        return ft * 30.48 + inch * 2.54;
      }
      function kgFromLb(lb) {
        return lb * 0.45359237;
      }

      function updateHeightInputs() {
        const unit = document.getElementById("heightUnit").value;
        const metricWrapper = document.getElementById("heightMetricWrapper");
        const imperialWrapper = document.getElementById(
          "heightImperialWrapper"
        );
        if (unit === "metric") {
          metricWrapper.classList.remove("hidden");
          imperialWrapper.classList.add("hidden");
        } else {
          metricWrapper.classList.add("hidden");
          imperialWrapper.classList.remove("hidden");
        }
      }

      function syncCarbPercentUI() {
        const goal = document.getElementById("goal").value;
        const slider = document.getElementById("carbPercent");
        const label = document.getElementById("carbPercentLabel");
        const hint = document.getElementById("carbRangeHint");

        let min, max, def, text;
        if (goal === "loss") {
          min = 20;
          max = 45;
          def = 35;
          text = "Typical weight-loss range: 20–45% of calories from carbs.";
        } else if (goal === "performance") {
          min = 50;
          max = 65;
          def = 55;
          text = "Typical performance range: 50–65% of calories from carbs.";
        } else if (goal === "lowcarb") {
          min = 10;
          max = 35;
          def = 25;
          text = "Lower-carb range: 10–35% of calories from carbs.";
        } else {
          min = 40;
          max = 60;
          def = 50;
          text = "General health range: 40–60% of calories from carbs.";
        }

        slider.min = min;
        slider.max = max;
        if (
          parseInt(slider.value, 10) < min ||
          parseInt(slider.value, 10) > max
        ) {
          slider.value = def;
        }
        label.textContent = slider.value + "% of calories";
        hint.textContent = text;
      }

      document
        .getElementById("heightUnit")
        .addEventListener("change", updateHeightInputs);
      document
        .getElementById("goal")
        .addEventListener("change", syncCarbPercentUI);
      document
        .getElementById("carbPercent")
        .addEventListener("input", function () {
          document.getElementById("carbPercentLabel").textContent =
            this.value + "% of calories";
        });

      function calculateCarbs() {
        const sex = document.getElementById("sex").value;
        const age = parseFloat(document.getElementById("age").value);
        const heightUnit = document.getElementById("heightUnit").value;
        let heightCm;
        if (heightUnit === "metric") {
          heightCm = parseFloat(document.getElementById("heightCm").value);
        } else {
          const ft = parseFloat(document.getElementById("heightFt").value);
          const inch = parseFloat(document.getElementById("heightIn").value);
          heightCm = cmFromImperial(ft, inch);
        }

        const weightUnit = document.getElementById("weightUnit").value;
        let weight = parseFloat(document.getElementById("weight").value);
        let weightKg = weightUnit === "kg" ? weight : kgFromLb(weight);

        const activity = parseFloat(document.getElementById("activity").value);
        const carbPercent = parseFloat(
          document.getElementById("carbPercent").value
        );
        const calorieAdjust = parseFloat(
          document.getElementById("calorieAdjust").value
        );
        const proteinPerKg = parseFloat(
          document.getElementById("proteinPerKg").value
        );
        const fatPercentMin = parseFloat(
          document.getElementById("fatPercent").value
        );

        if (
          !age ||
          !heightCm ||
          !weightKg ||
          age <= 0 ||
          heightCm <= 0 ||
          weightKg <= 0
        ) {
          alert("Please enter valid age, height, and weight values.");
          return;
        }

        // BMR (Mifflin–St Jeor)
        let bmr;
        if (sex === "male") {
          bmr = 10 * weightKg + 6.25 * heightCm - 5 * age + 5;
        } else {
          bmr = 10 * weightKg + 6.25 * heightCm - 5 * age - 161;
        }

        const tdee = bmr * activity;
        let targetCalories = tdee + calorieAdjust;
        if (targetCalories < 1200) targetCalories = 1200; // safety floor

        // Protein
        const proteinGrams = proteinPerKg * weightKg;
        const proteinKcal = proteinGrams * 4;

        // Minimum fat
        const fatKcalMin = (fatPercentMin / 100) * targetCalories;
        const fatGrams = fatKcalMin / 9;

        // Carb calories from chosen percentage, but ensure total macros don't exceed calories
        let desiredCarbKcal = (carbPercent / 100) * targetCalories;
        let macroKcal = proteinKcal + fatKcalMin + desiredCarbKcal;

        if (macroKcal > targetCalories) {
          // Scale carbs down to fit remaining calories after protein and fat
          const remaining = Math.max(
            targetCalories - proteinKcal - fatKcalMin,
            0
          );
          desiredCarbKcal = remaining;
          macroKcal = proteinKcal + fatKcalMin + desiredCarbKcal;
        }

        const carbGrams = desiredCarbKcal / 4;

        const carbPctActual = (desiredCarbKcal / targetCalories) * 100;
        const proteinPctActual = (proteinKcal / targetCalories) * 100;
        const fatPctActual = (fatKcalMin / targetCalories) * 100;

        // Per-meal distribution (3 meals + 1 snack by default)
        const meals = 3;
        const snacks = 1;
        const carbPerMeal = (carbGrams * 0.8) / meals; // 80% to meals
        const carbPerSnack = (carbGrams * 0.2) / (snacks || 1); // 20% to snacks

        // Update UI
        document.getElementById("tdeeResult").textContent = Math.round(tdee);
        document.getElementById("targetCaloriesResult").textContent =
          Math.round(targetCalories);

        document.getElementById("carbGramsResult").textContent =
          Math.round(carbGrams);
        document.getElementById("carbKcalResult").textContent =
          Math.round(desiredCarbKcal);
        document.getElementById("carbPercentResult").textContent =
          carbPctActual.toFixed(1);

        document.getElementById("proteinGramsResult").textContent =
          Math.round(proteinGrams);
        document.getElementById("proteinPercentResult").textContent =
          proteinPctActual.toFixed(1);

        document.getElementById("fatGramsResult").textContent =
          Math.round(fatGrams);
        document.getElementById("fatPercentResult").textContent =
          fatPctActual.toFixed(1);

        document.getElementById("mealsPerDayLabel").textContent = meals;
        document.getElementById("snacksPerDayLabel").textContent = snacks;
        document.getElementById("carbPerMealResult").textContent =
          Math.round(carbPerMeal);
        document.getElementById("carbPerSnackResult").textContent =
          Math.round(carbPerSnack);

        document.getElementById("macroCarbPctSummary").textContent =
          carbPctActual.toFixed(0);
        document.getElementById("macroProteinPctSummary").textContent =
          proteinPctActual.toFixed(0);
        document.getElementById("macroFatPctSummary").textContent =
          fatPctActual.toFixed(0);

        document.getElementById("resultsSection").classList.remove("hidden");
      }

      document
        .getElementById("calculateBtn")
        .addEventListener("click", calculateCarbs);

      // Initialize UI on load
      updateHeightInputs();
      syncCarbPercentUI();
      document.getElementById("carbPercentLabel").textContent =
        document.getElementById("carbPercent").value + "% of calories";
    </script>

    <script>
      window.MathJax = {
        tex: {
          inlineMath: [
