    <script>
      // Tab switching
      const tabs = [
        { tab: "tab-ppv", panel: "panel-ppv" },
        { tab: "tab-charge", panel: "panel-charge" },
        { tab: "tab-distance", panel: "panel-distance" },
      ];
      tabs.forEach(({ tab, panel }) => {
        document.getElementById(tab).addEventListener("click", () => {
          tabs.forEach(({ tab: t, panel: p }) => {
            document.getElementById(t).classList.remove("tab-active");
            document.getElementById(p).classList.add("hidden");
          });
          document.getElementById(tab).classList.add("tab-active");
          document.getElementById(panel).classList.remove("hidden");
        });
      });

      // Unit conversion helpers
      function ftToM(ft) {
        return ft * 0.3048;
      }
      function mToFt(m) {
        return m / 0.3048;
      }
      function lbToKg(lb) {
        return lb * 0.45359237;
      }
      function kgToLb(kg) {
        return kg / 0.45359237;
      }
      function inPerSecToMmPerSec(v) {
        return v * 25.4;
      }
      function mmPerSecToInPerSec(v) {
        return v / 25.4;
      }

      function parsePositive(id) {
        const v = parseFloat(document.getElementById(id).value);
        return isFinite(v) && v > 0 ? v : null;
      }

      // 1) Predict PPV
      document.getElementById("btn-calc-ppv").addEventListener("click", () => {
        const D_in = parsePositive("ppv-distance");
        const W_in = parsePositive("ppv-charge");
        const K = parsePositive("ppv-k");
        const n = parseFloat(document.getElementById("ppv-n").value);
        const b = parseFloat(document.getElementById("ppv-b").value);

        if (!D_in || !W_in || !K || !isFinite(n) || !isFinite(b)) {
          alert(
            "Please enter valid positive values for distance, charge, K, n and b."
          );
          return;
        }

        let D_m = D_in;
        if (document.getElementById("ppv-distance-unit").value === "ft") {
          D_m = ftToM(D_in);
        }

        let W_kg = W_in;
        if (document.getElementById("ppv-charge-unit").value === "lb") {
          W_kg = lbToKg(W_in);
        }

        const SD = D_m / Math.pow(W_kg, n);
        const PPV_mm_s = K * Math.pow(SD, -b);

        const outUnit = document.getElementById("ppv-ppv-unit").value;
        let ppvDisplay = PPV_mm_s;
        let unitLabel = "mm/s";
        if (outUnit === "in_s") {
          ppvDisplay = mmPerSecToInPerSec(PPV_mm_s);
          unitLabel = "in/s";
        }

        document.getElementById("ppv-result-value").textContent =
          ppvDisplay.toFixed(2);
        document.getElementById("ppv-result-unit").textContent = unitLabel;
        document.getElementById("ppv-result-sd").textContent = SD.toFixed(2);
        document.getElementById("ppv-results").classList.remove("hidden");

        // Compliance check
        const critVal = parseFloat(
          document.getElementById("ppv-criterion").value
        );
        const critUnit = document.getElementById("ppv-criterion-unit").value;
        const complianceEl = document.getElementById("ppv-compliance");
        complianceEl.textContent = "";
        complianceEl.className = "mt-3 text-sm font-medium";

        if (isFinite(critVal) && critVal > 0) {
          let crit_mm_s = critVal;
          if (critUnit === "in_s") {
            crit_mm_s = inPerSecToMmPerSec(critVal);
          }
          if (PPV_mm_s <= crit_mm_s) {
            complianceEl.textContent =
              "Predicted PPV is below the specified limit.";
            complianceEl.classList.add("text-green-700");
          } else {
            complianceEl.textContent =
              "Predicted PPV exceeds the specified limit.";
            complianceEl.classList.add("text-red-700");
          }
        }
      });

      // 2) Max charge per delay
      document
        .getElementById("btn-calc-charge")
        .addEventListener("click", () => {
          const D_in = parsePositive("charge-distance");
          const PPV_limit_in = parsePositive("charge-ppv-limit");
          const K = parsePositive("charge-k");
          const n = parseFloat(document.getElementById("charge-n").value);
          const b = parseFloat(document.getElementById("charge-b").value);

          if (
            !D_in ||
            !PPV_limit_in ||
            !K ||
            !isFinite(n) ||
            !isFinite(b) ||
            b === 0
          ) {
            alert(
              "Please enter valid positive values for distance, PPV limit, K, n and b."
            );
            return;
          }

          let D_m = D_in;
          if (document.getElementById("charge-distance-unit").value === "ft") {
            D_m = ftToM(D_in);
          }

          let PPV_limit_mm_s = PPV_limit_in;
          if (
            document.getElementById("charge-ppv-limit-unit").value === "in_s"
          ) {
            PPV_limit_mm_s = inPerSecToMmPerSec(PPV_limit_in);
          }

          // From PPV = K * (D / W^n)^(-b)
          // => (D / W^n) = (PPV / K)^(-1/b)
          // => W^n = D / (PPV / K)^(-1/b)
          // => W = [ D / (PPV / K)^(-1/b) ]^(1/n)
          const ratio = PPV_limit_mm_s / K;
          const D_over_Wn = Math.pow(ratio, -1 / b);
          const Wn = D_m / D_over_Wn;
          if (Wn <= 0 || !isFinite(Wn)) {
            alert(
              "Calculation failed with the given parameters. Check that K, n, b and PPV limit are reasonable."
            );
            return;
          }
          const W_kg = Math.pow(Wn, 1 / n);

          const outUnit = document.getElementById("charge-output-unit").value;
          let W_display = W_kg;
          let unitLabel = "kg";
          if (outUnit === "lb") {
            W_display = kgToLb(W_kg);
            unitLabel = "lb";
          }

          document.getElementById("charge-result-value").textContent =
            W_display.toFixed(2);
          document.getElementById("charge-result-unit").textContent = unitLabel;
          document.getElementById("charge-results").classList.remove("hidden");
        });

      // 3) Required distance
      document
        .getElementById("btn-calc-distance")
        .addEventListener("click", () => {
          const W_in = parsePositive("dist-charge");
          const PPV_limit_in = parsePositive("dist-ppv-limit");
          const K = parsePositive("dist-k");
          const n = parseFloat(document.getElementById("dist-n").value);
          const b = parseFloat(document.getElementById("dist-b").value);

          if (
            !W_in ||
            !PPV_limit_in ||
            !K ||
            !isFinite(n) ||
            !isFinite(b) ||
            b === 0
          ) {
            alert(
              "Please enter valid positive values for charge, PPV limit, K, n and b."
            );
            return;
          }

          let W_kg = W_in;
          if (document.getElementById("dist-charge-unit").value === "lb") {
            W_kg = lbToKg(W_in);
          }

          let PPV_limit_mm_s = PPV_limit_in;
          if (document.getElementById("dist-ppv-limit-unit").value === "in_s") {
            PPV_limit_mm_s = inPerSecToMmPerSec(PPV_limit_in);
          }

          // From PPV = K * (D / W^n)^(-b)
          // => (D / W^n) = (PPV / K)^(-1/b)
          // => D = W^n * (PPV / K)^(-1/b)
          const ratio = PPV_limit_mm_s / K;
          const D_over_Wn = Math.pow(ratio, -1 / b);
          const D_m = Math.pow(W_kg, n) * D_over_Wn;

          if (!isFinite(D_m) || D_m <= 0) {
            alert(
              "Calculation failed with the given parameters. Check that K, n, b and PPV limit are reasonable."
            );
            return;
          }

          const outUnit = document.getElementById("dist-output-unit").value;
          let D_display = D_m;
          let unitLabel = "m";
          if (outUnit === "ft") {
            D_display = mToFt(D_m);
            unitLabel = "ft";
          }

          document.getElementById("distance-result-value").textContent =
            D_display.toFixed(1);
          document.getElementById("distance-result-unit").textContent =
            unitLabel;
          document
            .getElementById("distance-results")
            .classList.remove("hidden");
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
