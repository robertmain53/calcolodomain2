    <script>
      // Core roulette odds logic
      (function () {
        const wheelSelect = document.getElementById("wheelType");
        const betSelect = document.getElementById("betType");
        const stakeInput = document.getElementById("stake");
        const spinsInput = document.getElementById("spins");

        const numbersCoveredEl = document.getElementById("numbersCovered");
        const payoutEl = document.getElementById("payout");
        const winProbEl = document.getElementById("winProb");
        const loseProbEl = document.getElementById("loseProb");
        const houseEdgeEl = document.getElementById("houseEdge");
        const evPerSpinEl = document.getElementById("evPerSpin");
        const evSessionEl = document.getElementById("evSession");
        const totalWageredEl = document.getElementById("totalWagered");
        const returnRateEl = document.getElementById("returnRate");

        const betConfig = {
          straight: { k: 1, payout: 35 },
          split: { k: 2, payout: 17 },
          street: { k: 3, payout: 11 },
          corner: { k: 4, payout: 8 },
          line: { k: 6, payout: 5 },
          column: { k: 12, payout: 2 },
          dozen: { k: 12, payout: 2 },
          even: { k: 18, payout: 1 }, // even-money
        };

        function formatPercent(x) {
          return (x * 100).toFixed(2) + "%";
        }

        function formatMoney(x) {
          const sign = x < 0 ? "-" : "";
          const v = Math.abs(x);
          return sign + "$" + v.toFixed(2);
        }

        function update() {
          const wheel = wheelSelect.value;
          const bet = betSelect.value;
          const stake = parseFloat(stakeInput.value) || 0;
          const spins = Math.max(1, parseInt(spinsInput.value, 10) || 1);

          const cfg = betConfig[bet];
          if (!cfg) return;

          const N = wheel === "american" ? 38 : 37;
          const k = cfg.k;
          const b = cfg.payout;

          const pWin = k / N;
          const pLose = 1 - pWin;

          // Base EV per unit
          let evPerUnit = pWin * b - pLose * 1;

          // French La Partage / En Prison effect on even-money bets
          // Approximate: when zero hits (1/37), player loses only half instead of full stake
          if (wheel === "french" && bet === "even") {
            const pZero = 1 / 37;
            const pNonZero = 36 / 37;
            const pWinNonZero = 18 / 36;
            const pLoseNonZero = 18 / 36;

            const pWinEff = pNonZero * pWinNonZero;
            const pHalfLoss = pZero; // when zero hits
            const pFullLoss = pNonZero * pLoseNonZero;

            evPerUnit = pWinEff * b - pFullLoss * 1 - pHalfLoss * 0.5;
          }

          const houseEdge = -evPerUnit;
          const evPerSpin = evPerUnit * stake;
          const evSession = evPerSpin * spins;
          const totalWagered = stake * spins;
          const returnRate = 1 + evPerUnit; // player return

          numbersCoveredEl.textContent = k.toString();
          payoutEl.textContent = b + " : 1";
          winProbEl.textContent = formatPercent(pWin);
          loseProbEl.textContent = formatPercent(pLose);
          houseEdgeEl.textContent = formatPercent(houseEdge);

          evPerSpinEl.textContent = formatMoney(evPerSpin);
          evSessionEl.textContent = formatMoney(evSession);
          totalWageredEl.textContent = formatMoney(totalWagered);
          returnRateEl.textContent = formatPercent(returnRate);
        }

        wheelSelect.addEventListener("change", update);
        betSelect.addEventListener("change", update);
        stakeInput.addEventListener("input", update);
        spinsInput.addEventListener("input", update);

        update();
      })();

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
