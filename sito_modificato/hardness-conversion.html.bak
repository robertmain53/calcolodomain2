    <script>
      // Basic hardness reference table (approximate, steels)
      const hardnessData = [
        { HRC: 20, HRB: 96, HB: 235, HV: 245 },
        { HRC: 22, HRB: 99, HB: 245, HV: 255 },
        { HRC: 24, HRB: 101, HB: 255, HV: 265 },
        { HRC: 26, HRB: 103, HB: 270, HV: 280 },
        { HRC: 28, HRB: 105, HB: 285, HV: 295 },
        { HRC: 30, HRB: 107, HB: 300, HV: 310 },
        { HRC: 32, HRB: 109, HB: 320, HV: 330 },
        { HRC: 34, HRB: 111, HB: 340, HV: 350 },
        { HRC: 36, HRB: 113, HB: 360, HV: 370 },
        { HRC: 38, HRB: 115, HB: 380, HV: 390 },
        { HRC: 40, HRB: 118, HB: 400, HV: 410 },
        { HRC: 42, HRB: 120, HB: 420, HV: 430 },
        { HRC: 44, HRB: 122, HB: 445, HV: 455 },
        { HRC: 46, HRB: 124, HB: 470, HV: 480 },
        { HRC: 48, HRB: 126, HB: 495, HV: 505 },
        { HRC: 50, HRB: 128, HB: 520, HV: 530 },
        { HRC: 52, HRB: 130, HB: 545, HV: 555 },
        { HRC: 54, HRB: 132, HB: 570, HV: 580 },
        { HRC: 56, HRB: 134, HB: 595, HV: 605 },
        { HRC: 58, HRB: 136, HB: 620, HV: 630 },
        { HRC: 60, HRB: 138, HB: 645, HV: 655 },
        { HRC: 62, HRB: 140, HB: 670, HV: 680 },
        { HRC: 64, HRB: 142, HB: 695, HV: 705 },
        { HRC: 66, HRB: 144, HB: 720, HV: 730 },
        { HRC: 68, HRB: 146, HB: 745, HV: 755 },
        { HRC: 70, HRB: 148, HB: 770, HV: 780 },
      ];

      // Add UTS (MPa) using HB correlation
      hardnessData.forEach((row) => {
        row.UTS = Math.round(row.HB * 3.45);
      });

      const tbody = document.getElementById("hardness-tbody");
      const rangeNote = document.getElementById("range-note");
      const warningNote = document.getElementById("warning-note");

      function renderTable(data) {
        tbody.innerHTML = "";
        data.forEach((row, idx) => {
          const tr = document.createElement("tr");
          tr.className =
            idx % 2 === 0
              ? "bg-white hover:bg-blue-50 cursor-pointer"
              : "bg-gray-50 hover:bg-blue-50 cursor-pointer";
          tr.dataset.index = idx;
          tr.innerHTML = `
        <td class="px-2 py-1">${row.HRC}</td>
        <td class="px-2 py-1">${row.HRB}</td>
        <td class="px-2 py-1">${row.HB}</td>
        <td class="px-2 py-1">${row.HV}</td>
        <td class="px-2 py-1">${row.UTS}</td>
      `;
          tr.addEventListener("click", () => {
            document.getElementById("input-scale").value = "HRC";
            document.getElementById("input-value").value = row.HRC;
            performConversion();
            tbody
              .querySelectorAll("tr")
              .forEach((r) => r.classList.remove("ring-1", "ring-blue-400"));
            tr.classList.add("ring-1", "ring-blue-400");
          });
          tbody.appendChild(tr);
        });
      }

      renderTable(hardnessData);

      // Simple linear interpolation helper
      function interpolateByScale(scale, value) {
        const key = scale;
        const sorted = [...hardnessData].sort((a, b) => a[key] - b[key]);
        if (value <= sorted[0][key]) return sorted[0];
        if (value >= sorted[sorted.length - 1][key])
          return sorted[sorted.length - 1];

        for (let i = 0; i < sorted.length - 1; i++) {
          const a = sorted[i];
          const b = sorted[i + 1];
          if (value >= a[key] && value <= b[key]) {
            const t = (value - a[key]) / (b[key] - a[key]);
            return {
              HRC: a.HRC + t * (b.HRC - a.HRC),
              HRB: a.HRB + t * (b.HRB - a.HRB),
              HB: a.HB + t * (b.HB - a.HB),
              HV: a.HV + t * (b.HV - a.HV),
              UTS: a.UTS + t * (b.UTS - a.UTS),
            };
          }
        }
        return sorted[0];
      }

      function formatVal(v) {
        return isFinite(v) ? (Math.round(v * 10) / 10).toString() : "–";
      }

      function performConversion() {
        const scale = document.getElementById("input-scale").value;
        const val = parseFloat(document.getElementById("input-value").value);
        const outHRC = document.getElementById("out-hrc");
        const outHRB = document.getElementById("out-hrb");
        const outHB = document.getElementById("out-hb");
        const outHV = document.getElementById("out-hv");
        const outUTS = document.getElementById("out-uts");

        if (isNaN(val)) {
          outHRC.textContent =
            outHRB.textContent =
            outHB.textContent =
            outHV.textContent =
            outUTS.textContent =
              "–";
          rangeNote.textContent = "";
          warningNote.classList.add("hidden");
          return;
        }

        let approx;
        if (scale === "UTS") {
          // Invert UTS ≈ 3.45 * HB
          const hb = val / 3.45;
          approx = interpolateByScale("HB", hb);
        } else {
          approx = interpolateByScale(scale, val);
        }

        outHRC.textContent = formatVal(approx.HRC);
        outHRB.textContent = formatVal(approx.HRB);
        outHB.textContent = Math.round(approx.HB);
        outHV.textContent = Math.round(approx.HV);
        outUTS.textContent = Math.round(approx.UTS);

        // Range and warning
        let validRange = "";
        if (scale === "HRC")
          validRange = "Approximate valid range for steels: 20–70 HRC.";
        if (scale === "HRB")
          validRange = "Approximate valid range for steels: 40–100 HRB.";
        if (scale === "HB")
          validRange = "Approximate valid range for steels: 120–650 HB.";
        if (scale === "HV")
          validRange = "Approximate valid range for steels: 130–900 HV.";
        if (scale === "UTS")
          validRange =
            "Approximate correlation for carbon and low-alloy steels only.";

        rangeNote.textContent = validRange;
        warningNote.classList.remove("hidden");
      }

      document
        .getElementById("convert-btn")
        .addEventListener("click", performConversion);
      document
        .getElementById("input-value")
        .addEventListener("keydown", (e) => {
          if (e.key === "Enter") performConversion();
        });

      document.getElementById("reset-btn").addEventListener("click", () => {
        document.getElementById("input-scale").value = "HRC";
        document.getElementById("input-value").value = "";
        document.getElementById("out-hrc").textContent = "–";
        document.getElementById("out-hrb").textContent = "–";
        document.getElementById("out-hb").textContent = "–";
        document.getElementById("out-hv").textContent = "–";
        document.getElementById("out-uts").textContent = "–";
        rangeNote.textContent = "";
        warningNote.classList.add("hidden");
        tbody
          .querySelectorAll("tr")
          .forEach((r) => r.classList.remove("ring-1", "ring-blue-400"));
      });

      // Sorting
      let currentSortKey = "HRC";
      let currentSortDir = "asc";

      document.querySelectorAll("#hardness-table thead th").forEach((th) => {
        th.addEventListener("click", () => {
          const key = th.dataset.key;
          if (!key) return;
          if (currentSortKey === key) {
            currentSortDir = currentSortDir === "asc" ? "desc" : "asc";
          } else {
            currentSortKey = key;
            currentSortDir = "asc";
          }
          const sorted = [...hardnessData].sort((a, b) => {
            return currentSortDir === "asc" ? a[key] - b[key] : b[key] - a[key];
          });
          renderTable(sorted);
        });
      });

      // Highlight by scale (just bold column)
      document
        .getElementById("filter-scale")
        .addEventListener("change", (e) => {
          const scale = e.target.value;
          const headers = document.querySelectorAll("#hardness-table thead th");
          headers.forEach((th) => {
            th.classList.remove("text-blue-700");
          });
          if (scale) {
            document
              .querySelector(`#hardness-table thead th[data-key="${scale}"]`)
              ?.classList.add("text-blue-700");
          }
        });
    </script>

    <script>
      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
