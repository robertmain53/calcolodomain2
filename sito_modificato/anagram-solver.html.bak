  function normalizeLetters(str) {
    return (str || "")
      .toLowerCase()
      .replace(/[^a-z?]/g, "");
  }

  function countLetters(str) {
    const counts = {};
    for (const ch of str) {
      counts[ch] = (counts[ch] || 0) + 1;
    }
    return counts;
  }

  function canFormWord(word, poolCounts, wildcards, useAllLetters) {
    const wordCounts = countLetters(word);
    let neededWildcards = 0;

    for (const [ch, cnt] of Object.entries(wordCounts)) {
      const available = poolCounts[ch] || 0;
      if (cnt > available) {
        neededWildcards += cnt - available;
        if (neededWildcards > wildcards) return false;
      }
    }

    if (!useAllLetters) return true;

    // For exact anagram: total letters used must equal pool letters
    const totalWordLetters = word.length;
    const totalPoolLetters = Object.values(poolCounts).reduce((a, b) => a + b, 0) + wildcards;
    return totalWordLetters === totalPoolLetters;
  }

  function matchesPattern(word, pattern) {
    if (!pattern) return true;
    if (word.length !== pattern.length) return false;
    for (let i = 0; i < pattern.length; i++) {
      const p = pattern[i];
      if (p === "?") continue;
      if (p.toLowerCase() !== word[i]) return false;
    }
    return true;
  }

  function matchesFilters(word, filters) {
    const w = word.toLowerCase();
    if (filters.startsWith && !w.startsWith(filters.startsWith)) return false;
    if (filters.endsWith && !w.endsWith(filters.endsWith)) return false;

    if (filters.includeLetters) {
      for (const ch of filters.includeLetters) {
        if (!w.includes(ch)) return false;
      }
    }
    if (filters.excludeLetters) {
      for (const ch of filters.excludeLetters) {
        if (w.includes(ch)) return false;
      }
    }
    return true;
  }

  function sortWords(words, mode) {
    const arr = [...words];
    if (mode === "alpha") {
      arr.sort((a, b) => a.localeCompare(b));
    } else if (mode === "length-asc") {
      arr.sort((a, b) => a.length - b.length || a.localeCompare(b));
    } else {
      // length-desc
      arr.sort((a, b) => b.length - a.length || a.localeCompare(b));
    }
    return arr;
  }

  function findSingleWordAnagrams(input, options) {
    const letters = normalizeLetters(input);
    if (!letters) return [];

    const pattern = options.pattern ? options.pattern.toLowerCase() : "";
    const poolCounts = countLetters(letters.replace(/\?/g, ""));
    const wildcards = (letters.match(/\?/g) || []).length;

    let minLen = options.minLength || 2;
    let maxLen = options.maxLength || 15;

    if (pattern) {
      minLen = maxLen = pattern.length;
    }

    const filters = {
      startsWith: (options.startsWith || "").toLowerCase(),
      endsWith: (options.endsWith || "").toLowerCase(),
      includeLetters: normalizeLetters(options.includeLetters),
      excludeLetters: normalizeLetters(options.excludeLetters)
    };

    const results = [];
    for (const word of WORD_LIST) {
      if (word.length < minLen || word.length > maxLen) continue;
      if (!matchesPattern(word, pattern)) continue;
      if (!matchesFilters(word, filters)) continue;
      if (!canFormWord(word, poolCounts, wildcards, options.useAllLetters)) continue;
      results.push(word);
    }
    return sortWords([...new Set(results)], options.sortMode);
  }

  function findMultiWordAnagrams(input, options) {
    const letters = normalizeLetters(input);
    if (!letters) return [];

    // Limit length for performance
    if (letters.length > 12) return [];

    const poolCounts = countLetters(letters.replace(/\?/g, ""));
    const wildcards = (letters.match(/\?/g) || []).length;

    const usableWords = WORD_LIST.filter(w => w.length >= 2 && w.length <= letters.length - 1);
    const combos = new Set();

    function canFormSequence(words) {
      const combined = words.join("");
      const combinedCounts = countLetters(combined);
      let neededWildcards = 0;
      for (const [ch, cnt] of Object.entries(combinedCounts)) {
        const available = poolCounts[ch] || 0;
        if (cnt > available) {
          neededWildcards += cnt - available;
          if (neededWildcards > wildcards) return false;
        }
      }
      const totalPoolLetters = Object.values(poolCounts).reduce((a, b) => a + b, 0) + wildcards;
      return combined.length === totalPoolLetters;
    }

    // Simple 2-word combinations for performance
    for (let i = 0; i < usableWords.length; i++) {
      for (let j = i + 1; j < usableWords.length; j++) {
        const pair = [usableWords[i], usableWords[j]];
        if (canFormSequence(pair)) {
          const sortedPair = sortWords(pair, "alpha").join(" ");
          combos.add(sortedPair);
        }
      }
    }

    return sortWords([...combos], options.sortMode);
  }

  function renderWordBadges(container, words) {
    container.innerHTML = "";
    if (!words.length) {
      const span = document.createElement("span");
      span.className = "text-xs text-gray-400";
      span.textContent = "No matches found.";
      container.appendChild(span);
      return;
    }
    for (const w of words) {
      const span = document.createElement("span");
      span.className = "inline-flex items-center gap-1 px-2 py-1 bg-blue-50 text-blue-800 rounded-full text-xs md:text-sm result-word";
      span.textContent = w;
      const len = document.createElement("span");
      len.className = "text-[10px] text-gray-500";
      len.textContent = w.length;
      span.appendChild(len);
      container.appendChild(span);
    }
  }

  function renderMultiWordList(container, phrases) {
    container.innerHTML = "";
    if (!phrases.length) {
      const span = document.createElement("span");
      span.className = "text-xs text-gray-400";
      span.textContent = "No multi-word anagrams found for this input.";
      container.appendChild(span);
      return;
    }
    for (const phrase of phrases) {
      const div = document.createElement("div");
      div.className = "px-2 py-1 rounded bg-green-50 text-green-800";
      div.textContent = phrase;
      container.appendChild(div);
    }
  }

  document.addEventListener("DOMContentLoaded", () => {
    const lettersInput = document.getElementById("letters-input");
    const minLengthInput = document.getElementById("min-length");
    const maxLengthInput = document.getElementById("max-length");
    const patternInput = document.getElementById("pattern-input");
    const startsWithInput = document.getElementById("starts-with");
    const endsWithInput = document.getElementById("ends-with");
    const includeLettersInput = document.getElementById("include-letters");
    const excludeLettersInput = document.getElementById("exclude-letters");
    const multiwordToggle = document.getElementById("multiword-toggle");
    const useAllLettersToggle = document.getElementById("use-all-letters-toggle");
    const sortModeSelect = document.getElementById("sort-mode");

    const solveButton = document.getElementById("solve-button");
    const clearButton = document.getElementById("clear-button");
    const statusText = document.getElementById("status-text");

    const noResults = document.getElementById("no-results");
    const resultsContainer = document.getElementById("results-container");
    const singleResults = document.getElementById("single-results");
    const multiResults = document.getElementById("multi-results");
    const resultCount = document.getElementById("result-count");
    const singleCount = document.getElementById("single-count");
    const multiCount = document.getElementById("multi-count");

    function clearAll() {
      lettersInput.value = "";
      patternInput.value = "";
      startsWithInput.value = "";
      endsWithInput.value = "";
      includeLettersInput.value = "";
      excludeLettersInput.value = "";
      minLengthInput.value = 2;
      maxLengthInput.value = 15;
      multiwordToggle.checked = false;
      useAllLettersToggle.checked = true;
      sortModeSelect.value = "length-desc";
      statusText.textContent = "";
      resultCount.classList.add("hidden");
      singleCount.classList.add("hidden");
      multiCount.classList.add("hidden");
      noResults.classList.remove("hidden");
      resultsContainer.classList.add("hidden");
      singleResults.innerHTML = "";
      multiResults.innerHTML = "";
    }

    clearButton.addEventListener("click", clearAll);

    function solve() {
      const rawInput = lettersInput.value.trim();
      if (!rawInput) {
        statusText.textContent = "Please enter some letters or a phrase.";
        return;
      }

      statusText.textContent = "Searching…";
      const t0 = performance.now();

      const options = {
        minLength: parseInt(minLengthInput.value, 10) || 2,
        maxLength: parseInt(maxLengthInput.value, 10) || 15,
        pattern: patternInput.value.trim(),
        startsWith: startsWithInput.value.trim(),
        endsWith: endsWithInput.value.trim(),
        includeLetters: includeLettersInput.value.trim(),
        excludeLetters: excludeLettersInput.value.trim(),
        useAllLetters: useAllLettersToggle.checked,
        sortMode: sortModeSelect.value
      };

      const single = findSingleWordAnagrams(rawInput, options);
      renderWordBadges(singleResults, single);

      let multi = [];
      if (multiwordToggle.checked) {
        multi = findMultiWordAnagrams(rawInput, options);
      }
      renderMultiWordList(multiResults, multi);

      const t1 = performance.now();
      const elapsed = (t1 - t0).toFixed(0);

      noResults.classList.add("hidden");
      resultsContainer.classList.remove("hidden");

      const total = single.length + multi.length;
      resultCount.textContent = total ? `${total} result${total !== 1 ? "s" : ""} in ${elapsed} ms` : `No results in ${elapsed} ms`;
      resultCount.classList.remove("hidden");

      singleCount.textContent = single.length;
      singleCount.classList.remove("hidden");
      multiCount.textContent = multi.length;
      multiCount.classList.remove("hidden");

      statusText.textContent = "";
    }

    solveButton.addEventListener("click", solve);
    lettersInput.addEventListener("keydown", (e) => {
      if (e.key === "Enter" && !e.shiftKey) {
        e.preventDefault();
        solve();
      }
    });
  });
</script>

