    <script>
      // Regex tester logic
      (function () {
        const patternInput = document.getElementById("regex-pattern");
        const flagsInlineInput = document.getElementById("regex-flags-inline");
        const flagCheckboxes = {
          g: document.getElementById("flag-g"),
          i: document.getElementById("flag-i"),
          m: document.getElementById("flag-m"),
          s: document.getElementById("flag-s"),
          u: document.getElementById("flag-u"),
          y: document.getElementById("flag-y"),
        };
        const testTextarea = document.getElementById("test-string");
        const highlightedOutput = document.getElementById("highlighted-output");
        const statusText = document.getElementById("status-text");
        const noMatches = document.getElementById("no-matches");
        const matchesContainer = document.getElementById("matches-container");
        const optHighlight = document.getElementById("opt-highlight");
        const optLineNumbers = document.getElementById("opt-line-numbers");
        const btnRun = document.getElementById("btn-run");
        const btnCopyRegex = document.getElementById("btn-copy-regex");
        const btnSampleEmail = document.getElementById("btn-load-sample-email");
        const btnSampleLog = document.getElementById("btn-load-sample-log");
        const btnClearTest = document.getElementById("btn-clear-test");

        function getFlagsFromUI() {
          let flags = "";
          Object.keys(flagCheckboxes).forEach((f) => {
            if (flagCheckboxes[f].checked) flags += f;
          });
          return flags;
        }

        function syncFlagsToInline() {
          const flags = getFlagsFromUI();
          flagsInlineInput.value = flags;
        }

        function syncInlineToFlags() {
          const val = flagsInlineInput.value.replace(/[^gimsuy]/g, "");
          const unique = Array.from(new Set(val.split(""))).join("");
          flagsInlineInput.value = unique;
          Object.keys(flagCheckboxes).forEach((f) => {
            flagCheckboxes[f].checked = unique.includes(f);
          });
        }

        Object.values(flagCheckboxes).forEach((cb) => {
          cb.addEventListener("change", () => {
            syncFlagsToInline();
            runTest();
          });
        });

        flagsInlineInput.addEventListener("input", () => {
          syncInlineToFlags();
          runTest();
        });

        function buildRegex() {
          const pattern = patternInput.value;
          const flags = getFlagsFromUI();
          if (pattern === "") {
            return { regex: null, error: null };
          }
          try {
            const regex = new RegExp(pattern, flags);
            return { regex, error: null };
          } catch (e) {
            return { regex: null, error: e.message || String(e) };
          }
        }

        function escapeHtml(str) {
          return str.replace(
            /[&<>"']/g,
            (ch) =>
              ({
                "&": "&amp;",
                "<": "&lt;",
                ">": "&gt;",
                '"': "&quot;",
                "'": "&#39;",
              }[ch])
          );
        }

        function buildHighlightedHtml(text, matches) {
          if (!optHighlight.checked || !matches.length) {
            if (!optLineNumbers.checked) return escapeHtml(text);
            return addLineNumbers(escapeHtml(text));
          }

          const segments = [];
          let lastIndex = 0;

          matches.forEach((m, idx) => {
            const { index, match, groups } = m;
            if (index > lastIndex) {
              segments.push({
                type: "text",
                text: text.slice(lastIndex, index),
              });
            }

            if (!groups || groups.length === 0) {
              segments.push({ type: "match", text: match });
            } else {
              let relPos = 0;
              const groupSegments = [];
              groups.forEach((g, gi) => {
                if (g == null || g.index == null) return;
                const start = g.index - index;
                const end = start + g.value.length;
                if (start > relPos) {
                  groupSegments.push({
                    type: "match",
                    text: match.slice(relPos, start),
                  });
                }
                groupSegments.push({
                  type: "group",
                  groupIndex: gi + 1,
                  text: match.slice(start, end),
                });
                relPos = end;
              });
              if (relPos < match.length) {
                groupSegments.push({
                  type: "match",
                  text: match.slice(relPos),
                });
              }
              segments.push(...groupSegments);
            }

            lastIndex = index + match.length;
          });

          if (lastIndex < text.length) {
            segments.push({ type: "text", text: text.slice(lastIndex) });
          }

          let html = "";
          segments.forEach((seg) => {
            if (seg.type === "text") {
              html += escapeHtml(seg.text);
            } else if (seg.type === "match") {
              html +=
                '<span class="match-highlight">' +
                escapeHtml(seg.text) +
                "</span>";
            } else if (seg.type === "group") {
              const cls =
                seg.groupIndex === 1
                  ? "group-highlight-1"
                  : seg.groupIndex === 2
                  ? "group-highlight-2"
                  : "group-highlight-3";
              html +=
                '<span class="' + cls + '">' + escapeHtml(seg.text) + "</span>";
            }
          });

          if (optLineNumbers.checked) {
            html = addLineNumbers(html, true);
          }

          return html;
        }

        function addLineNumbers(html, alreadyEscaped) {
          const raw = alreadyEscaped ? html : escapeHtml(html);
          const lines = raw.split(/\r\n|\r|\n/);
          return lines
            .map((line, idx) => {
              const ln = idx + 1;
              return (
                '<span class="text-gray-400 select-none mr-2">' +
                ln.toString().padStart(3, " ") +
                "│</span>" +
                line
              );
            })
            .join("\n");
        }

        function runTest() {
          const text = testTextarea.value || "";
          const { regex, error } = buildRegex();

          matchesContainer.innerHTML = "";
          noMatches.classList.add("hidden");

          if (!patternInput.value) {
            statusText.textContent = "Enter a regex pattern to begin.";
            highlightedOutput.innerHTML = optLineNumbers.checked
              ? addLineNumbers(escapeHtml(text))
              : escapeHtml(text);
            return;
          }

          if (error) {
            statusText.innerHTML =
              '<span class="text-red-600 font-medium">Syntax error:</span> ' +
              escapeHtml(error);
            highlightedOutput.innerHTML = optLineNumbers.checked
              ? addLineNumbers(escapeHtml(text))
              : escapeHtml(text);
            return;
          }

          const matches = [];
          if (!regex.global) {
            const m = regex.exec(text);
            if (m) {
              matches.push(buildMatchObject(m, text));
            }
          } else {
            let m;
            regex.lastIndex = 0;
            while ((m = regex.exec(text)) !== null) {
              if (m[0] === "") {
                matches.push(buildMatchObject(m, text));
                regex.lastIndex++;
                if (regex.lastIndex > text.length) break;
              } else {
                matches.push(buildMatchObject(m, text));
              }
            }
          }

          if (!matches.length) {
            statusText.textContent = "No matches.";
            noMatches.classList.remove("hidden");
            highlightedOutput.innerHTML = optLineNumbers.checked
              ? addLineNumbers(escapeHtml(text))
              : escapeHtml(text);
            return;
          }

          statusText.textContent =
            matches.length +
            " match" +
            (matches.length > 1 ? "es" : "") +
            " found.";
          highlightedOutput.innerHTML = buildHighlightedHtml(text, matches);
          renderMatches(matches);
        }

        function buildMatchObject(m, text) {
          const fullMatch = m[0];
          const index = m.index;
          const groups = [];
          for (let i = 1; i < m.length; i++) {
            const val = m[i];
            if (val == null) {
              groups.push({ value: null, index: null });
            } else {
              const before = m[0].indexOf(val);
              const gIndex = before >= 0 ? index + before : null;
              groups.push({ value: val, index: gIndex });
            }
          }
          const named = m.groups || {};
          return {
            match: fullMatch,
            index,
            end: index + fullMatch.length,
            groups,
            named,
          };
        }

        function renderMatches(matches) {
          matchesContainer.innerHTML = "";
          matches.forEach((m, mi) => {
            const card = document.createElement("div");
            card.className = "border border-gray-200 rounded-md p-3";

            const header = document.createElement("div");
            header.className = "flex items-center justify-between mb-2";
            header.innerHTML =
              '<div class="text-xs text-gray-500">Match ' +
              (mi + 1) +
              " at index " +
              m.index +
              "–" +
              m.end +
              "</div>" +
              '<div class="font-mono text-xs bg-gray-100 px-2 py-0.5 rounded">' +
              escapeHtml(m.match) +
              "</div>";
            card.appendChild(header);

            if (m.groups.length || (m.named && Object.keys(m.named).length)) {
              const table = document.createElement("table");
              table.className = "w-full text-xs border-t border-gray-100";
              const tbody = document.createElement("tbody");

              m.groups.forEach((g, gi) => {
                if (g.value == null) return;
                const tr = document.createElement("tr");
                tr.className = "border-b border-gray-100";
                const labelTd = document.createElement("td");
                labelTd.className = "py-1 pr-2 text-gray-500 whitespace-nowrap";
                labelTd.textContent = "Group " + (gi + 1);
                const valTd = document.createElement("td");
                valTd.className = "py-1 font-mono break-all";
                valTd.textContent = g.value;
                const idxTd = document.createElement("td");
                idxTd.className =
                  "py-1 pl-2 text-gray-400 text-right whitespace-nowrap";
                idxTd.textContent = g.index != null ? "@ " + g.index : "";
                tr.appendChild(labelTd);
                tr.appendChild(valTd);
                tr.appendChild(idxTd);
                tbody.appendChild(tr);
              });

              if (m.named && Object.keys(m.named).length) {
                Object.keys(m.named).forEach((name) => {
                  const val = m.named[name];
                  const tr = document.createElement("tr");
                  tr.className = "border-b border-gray-100";
                  const labelTd = document.createElement("td");
                  labelTd.className =
                    "py-1 pr-2 text-gray-500 whitespace-nowrap";
                  labelTd.textContent = "Named " + name;
                  const valTd = document.createElement("td");
                  valTd.className = "py-1 font-mono break-all";
                  valTd.textContent = val;
                  const idxTd = document.createElement("td");
                  idxTd.className =
                    "py-1 pl-2 text-gray-400 text-right whitespace-nowrap";
                  idxTd.textContent = "";
                  tr.appendChild(labelTd);
                  tr.appendChild(valTd);
                  tr.appendChild(idxTd);
                  tbody.appendChild(tr);
                });
              }

              table.appendChild(tbody);
              card.appendChild(table);
            } else {
              const p = document.createElement("p");
              p.className = "text-xs text-gray-500";
              p.textContent = "No capturing groups in this match.";
              card.appendChild(p);
            }

            matchesContainer.appendChild(card);
          });
        }

        btnRun.addEventListener("click", runTest);
        patternInput.addEventListener("input", runTest);
        testTextarea.addEventListener("input", runTest);
        optHighlight.addEventListener("change", runTest);
        optLineNumbers.addEventListener("change", runTest);

        btnCopyRegex.addEventListener("click", () => {
          const pattern = patternInput.value;
          const flags = getFlagsFromUI();
          const full = "/" + pattern + "/" + flags;
          navigator.clipboard
            .writeText(full)
            .then(() => {
              statusText.textContent = "Copied " + full + " to clipboard.";
            })
            .catch(() => {
              statusText.textContent =
                "Unable to copy to clipboard in this browser.";
            });
        });

        btnSampleEmail.addEventListener("click", () => {
          patternInput.value =
            "\\b[A-Za-z0-9._%+-]+@[A-Za-z0-9.-]+\\.[A-Za-z]{2,}\\b";
          flagCheckboxes.g.checked = true;
          syncFlagsToInline();
          runTest();
        });

        btnSampleLog.addEventListener("click", () => {
          patternInput.value =
            "^(\\d{4}-\\d{2}-\\d{2}) (\\d{2}:\\d{2}:\\d{2}) \\[(INFO|WARN|ERROR)\\] (.*)$";
          flagCheckboxes.g.checked = true;
          flagCheckboxes.m.checked = true;
          syncFlagsToInline();
          testTextarea.value = `2025-01-01 12:00:00 [INFO] Server started on port 8080
2025-01-01 12:01:05 [WARN] High memory usage detected
2025-01-01 12:02:10 [ERROR] Unhandled exception in worker thread`;
          runTest();
        });

        btnClearTest.addEventListener("click", () => {
          testTextarea.value = "";
          runTest();
        });

        // Initial sync and run
        syncFlagsToInline();
        runTest();
      })();
    </script>

