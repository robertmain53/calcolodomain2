    <script>
      // Utility conversions
      function cmFromImperial(ft, inch) {
        const f = parseFloat(ft) || 0;
        const i = parseFloat(inch) || 0;
        return (f * 12 + i) * 2.54;
      }
      function kgFromLb(lb) {
        return (parseFloat(lb) || 0) * 0.45359237;
      }
      function lbFromKg(kg) {
        return kg * 2.2046226218;
      }
      function round(value, decimals) {
        const d = decimals ?? 1;
        const factor = Math.pow(10, d);
        return Math.round(value * factor) / factor;
      }

      // LBM formulas
      function lbmBoer(sex, weightKg, heightCm) {
        if (sex === "male") {
          return 0.407 * weightKg + 0.267 * heightCm - 19.2;
        } else {
          return 0.252 * weightKg + 0.473 * heightCm - 48.3;
        }
      }
      function lbmJames(sex, weightKg, heightCm) {
        const ratioSq = Math.pow(weightKg / heightCm, 2);
        if (sex === "male") {
          return 1.1 * weightKg - 128 * ratioSq;
        } else {
          return 1.07 * weightKg - 148 * ratioSq;
        }
      }
      function lbmHume(sex, weightKg, heightCm) {
        if (sex === "male") {
          return 0.3281 * weightKg + 0.33929 * heightCm - 29.5336;
        } else {
          return 0.29569 * weightKg + 0.41813 * heightCm - 43.2933;
        }
      }
      function lbmJanmahasatian(sex, weightKg, heightCm) {
        const heightM = heightCm / 100;
        const bmi = weightKg / (heightM * heightM);
        if (sex === "male") {
          return (9270 * weightKg) / (6680 + 216 * bmi);
        } else {
          return (9270 * weightKg) / (8780 + 244 * bmi);
        }
      }

      function calculateLBM() {
        const sex = document.getElementById("sex").value;
        const units = document.getElementById("units").value;
        const age = parseFloat(document.getElementById("age").value);
        let heightCm, weightKg;

        if (units === "metric") {
          heightCm = parseFloat(document.getElementById("height-cm").value);
          weightKg = parseFloat(document.getElementById("weight-kg").value);
        } else {
          const ft = document.getElementById("height-ft").value;
          const inch = document.getElementById("height-in").value;
          const lb = document.getElementById("weight-lb").value;
          heightCm = cmFromImperial(ft, inch);
          weightKg = kgFromLb(lb);
        }

        if (!heightCm || !weightKg || heightCm <= 0 || weightKg <= 0) {
          alert("Please enter a valid height and weight.");
          return;
        }

        // Compute LBM values
        const formulas = [
          { key: "boer", name: "Boer", fn: lbmBoer },
          { key: "james", name: "James", fn: lbmJames },
          { key: "hume", name: "Hume", fn: lbmHume },
          { key: "jan", name: "Janmahasatian", fn: lbmJanmahasatian },
        ];

        const results = [];
        formulas.forEach((f) => {
          let val = f.fn(sex, weightKg, heightCm);
          if (!isFinite(val)) return;
          // Clamp to sensible range
          if (val < 10) val = 10;
          if (val > weightKg) val = weightKg;
          const bf = (1 - val / weightKg) * 100;
          results.push({
            key: f.key,
            name: f.name,
            lbmKg: val,
            lbmLb: lbFromKg(val),
            bfPercent: bf,
          });
        });

        if (!results.length) {
          alert("Unable to compute lean body mass with the given inputs.");
          return;
        }

        // Primary = Boer if available, else first
        const primary = results.find((r) => r.key === "boer") || results[0];

        // Update summary
        document.getElementById("lbm-primary-kg").textContent = round(
          primary.lbmKg,
          1
        );
        document.getElementById("lbm-primary-lb").textContent = round(
          primary.lbmLb,
          1
        );

        const estBf = primary.bfPercent;
        document.getElementById("bf-est").textContent = round(estBf, 1);
        const fatMassKg = weightKg - primary.lbmKg;
        document.getElementById("fat-mass-kg").textContent = round(
          fatMassKg,
          1
        );
        document.getElementById("fat-mass-lb").textContent = round(
          lbFromKg(fatMassKg),
          1
        );

        // Table
        const tbody = document.getElementById("lbm-table-body");
        tbody.innerHTML = "";
        results.forEach((r) => {
          const tr = document.createElement("tr");
          const isPrimary = r.key === primary.key;
          tr.innerHTML = `
        <td class="px-3 py-2 whitespace-nowrap">
          <span class="${
            isPrimary ? "font-semibold text-blue-700" : "text-gray-800"
          }">${r.name}${isPrimary ? " (primary)" : ""}</span>
        </td>
        <td class="px-3 py-2 text-right">${round(r.lbmKg, 1)}</td>
        <td class="px-3 py-2 text-right">${round(r.lbmLb, 1)}</td>
        <td class="px-3 py-2 text-right">${round(r.bfPercent, 1)}%</td>
      `;
          tbody.appendChild(tr);
        });

        // Measured body fat (optional)
        const bfInput = parseFloat(document.getElementById("body-fat").value);
        const measuredRow = document.getElementById("measured-row");
        if (bfInput && bfInput > 0 && bfInput < 80) {
          const measuredFatMassKg = weightKg * (bfInput / 100);
          const measuredLbmKg = weightKg - measuredFatMassKg;
          document.getElementById("measured-lbm-kg").textContent = round(
            measuredLbmKg,
            1
          );
          document.getElementById("measured-lbm-lb").textContent = round(
            lbFromKg(measuredLbmKg),
            1
          );
          document.getElementById("measured-fat-mass-kg").textContent = round(
            measuredFatMassKg,
            1
          );
          document.getElementById("bf-source-label").textContent =
            "Based on Boer formula vs. total weight";
          measuredRow.classList.remove("hidden");
        } else {
          measuredRow.classList.add("hidden");
          document.getElementById("bf-source-label").textContent =
            "Based on Boer LBM vs. total weight";
        }

        document.getElementById("results-section").classList.remove("hidden");
      }

      document.addEventListener("DOMContentLoaded", function () {
        // Sex toggle
        document.querySelectorAll(".sex-toggle").forEach((btn) => {
          btn.addEventListener("click", () => {
            const sex = btn.getAttribute("data-sex");
            document.getElementById("sex").value = sex;
            document.querySelectorAll(".sex-toggle").forEach((b) => {
              if (b === btn) {
                b.classList.remove(
                  "border-gray-300",
                  "bg-white",
                  "text-gray-700"
                );
                b.classList.add(
                  "border-blue-600",
                  "bg-blue-50",
                  "text-blue-700"
                );
              } else {
                b.classList.remove(
                  "border-blue-600",
                  "bg-blue-50",
                  "text-blue-700"
                );
                b.classList.add("border-gray-300", "bg-white", "text-gray-700");
              }
            });
          });
        });

        // Unit toggle
        document.querySelectorAll(".unit-toggle").forEach((btn) => {
          btn.addEventListener("click", () => {
            const units = btn.getAttribute("data-units");
            document.getElementById("units").value = units;
            document.querySelectorAll(".unit-toggle").forEach((b) => {
              if (b === btn) {
                b.classList.remove(
                  "border-gray-300",
                  "bg-white",
                  "text-gray-700"
                );
                b.classList.add(
                  "border-blue-600",
                  "bg-blue-50",
                  "text-blue-700"
                );
              } else {
                b.classList.remove(
                  "border-blue-600",
                  "bg-blue-50",
                  "text-blue-700"
                );
                b.classList.add("border-gray-300", "bg-white", "text-gray-700");
              }
            });

            if (units === "metric") {
              // Convert imperial to metric if values exist
              const ft = parseFloat(document.getElementById("height-ft").value);
              const inch = parseFloat(
                document.getElementById("height-in").value
              );
              const lb = parseFloat(document.getElementById("weight-lb").value);
              if (ft || inch) {
                document.getElementById("height-cm").value = round(
                  cmFromImperial(ft || 0, inch || 0),
                  1
                );
              }
              if (lb) {
                document.getElementById("weight-kg").value = round(
                  kgFromLb(lb),
                  1
                );
              }
              document
                .getElementById("height-metric")
                .classList.remove("hidden");
              document
                .getElementById("weight-metric")
                .classList.remove("hidden");
              document
                .getElementById("height-imperial")
                .classList.add("hidden");
              document
                .getElementById("weight-imperial")
                .classList.add("hidden");
            } else {
              // Convert metric to imperial if values exist
              const cm = parseFloat(document.getElementById("height-cm").value);
              const kg = parseFloat(document.getElementById("weight-kg").value);
              if (cm) {
                const totalIn = cm / 2.54;
                const ftVal = Math.floor(totalIn / 12);
                const inVal = Math.round(totalIn - ftVal * 12);
                document.getElementById("height-ft").value = ftVal;
                document.getElementById("height-in").value = inVal;
              }
              if (kg) {
                document.getElementById("weight-lb").value = round(
                  lbFromKg(kg),
                  1
                );
              }
              document.getElementById("height-metric").classList.add("hidden");
              document.getElementById("weight-metric").classList.add("hidden");
              document
                .getElementById("height-imperial")
                .classList.remove("hidden");
              document
                .getElementById("weight-imperial")
                .classList.remove("hidden");
            }
          });
        });

        document
          .getElementById("calculate-btn")
          .addEventListener("click", calculateLBM);

        document.getElementById("reset-btn").addEventListener("click", () => {
          document.getElementById("sex").value = "male";
          document.getElementById("units").value = "metric";
          document.getElementById("age").value = 30;
          document.getElementById("height-cm").value = 175;
          document.getElementById("weight-kg").value = 75;
          document.getElementById("height-ft").value = 5;
          document.getElementById("height-in").value = 9;
          document.getElementById("weight-lb").value = 165;
          document.getElementById("body-fat").value = "";

          // Reset toggles
          document.querySelectorAll(".sex-toggle").forEach((b) => {
            if (b.getAttribute("data-sex") === "male") {
              b.classList.remove(
                "border-gray-300",
                "bg-white",
                "text-gray-700"
              );
              b.classList.add("border-blue-600", "bg-blue-50", "text-blue-700");
            } else {
              b.classList.remove(
                "border-blue-600",
                "bg-blue-50",
                "text-blue-700"
              );
              b.classList.add("border-gray-300", "bg-white", "text-gray-700");
            }
          });
          document.querySelectorAll(".unit-toggle").forEach((b) => {
            if (b.getAttribute("data-units") === "metric") {
              b.classList.remove(
                "border-gray-300",
                "bg-white",
                "text-gray-700"
              );
              b.classList.add("border-blue-600", "bg-blue-50", "text-blue-700");
            } else {
              b.classList.remove(
                "border-blue-600",
                "bg-blue-50",
                "text-blue-700"
              );
              b.classList.add("border-gray-300", "bg-white", "text-gray-700");
            }
          });

          document.getElementById("height-metric").classList.remove("hidden");
          document.getElementById("weight-metric").classList.remove("hidden");
          document.getElementById("height-imperial").classList.add("hidden");
          document.getElementById("weight-imperial").classList.add("hidden");

          document.getElementById("results-section").classList.add("hidden");
          document.getElementById("measured-row").classList.add("hidden");
        });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
