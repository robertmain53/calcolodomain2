    <script>
      // Utility: format currency and percent
      function formatCurrency(value, symbol) {
        if (!isFinite(value)) return "–";
        const abs = Math.abs(value);
        const decimals = abs >= 1000 ? 0 : 2;
        return (
          symbol +
          value.toLocaleString(undefined, {
            minimumFractionDigits: decimals,
            maximumFractionDigits: decimals,
          })
        );
      }

      function formatPercent(value) {
        if (!isFinite(value)) return "–";
        return value.toFixed(2) + "%";
      }

      // IRR via bisection method (robust for typical real estate cash flows)
      function npv(rate, cashflows) {
        let total = 0;
        for (let t = 0; t < cashflows.length; t++) {
          total += cashflows[t] / Math.pow(1 + rate, t);
        }
        return total;
      }

      function computeIRR(cashflows) {
        const hasPositive = cashflows.some((v) => v > 0);
        const hasNegative = cashflows.some((v) => v < 0);
        if (!hasPositive || !hasNegative) return null;

        let low = -0.9999; // -99.99%
        let high = 10; // 1000%+
        let npvLow = npv(low, cashflows);
        let npvHigh = npv(high, cashflows);

        // Expand bounds if needed
        let iterations = 0;
        while (npvLow * npvHigh > 0 && iterations < 50) {
          if (Math.abs(npvLow) < Math.abs(npvHigh)) {
            high *= 2;
            npvHigh = npv(high, cashflows);
          } else {
            low = low / 2;
            npvLow = npv(low, cashflows);
          }
          iterations++;
          if (!isFinite(npvLow) || !isFinite(npvHigh)) break;
        }

        if (npvLow * npvHigh > 0) return null; // no sign change -> no root in range

        let mid, npvMid;
        for (let i = 0; i < 100; i++) {
          mid = (low + high) / 2;
          npvMid = npv(mid, cashflows);
          if (Math.abs(npvMid) < 1e-6) break;
          if (npvLow * npvMid < 0) {
            high = mid;
            npvHigh = npvMid;
          } else {
            low = mid;
            npvLow = npvMid;
          }
        }
        return mid;
      }

      function computeEquityMultiple(cashflows) {
        let inflows = 0;
        let outflows = 0;
        for (const c of cashflows) {
          if (c > 0) inflows += c;
          if (c < 0) outflows += c;
        }
        if (outflows === 0) return null;
        return inflows / Math.abs(outflows);
      }

      function computeNPVAtRate(rate, cashflows) {
        if (!isFinite(rate)) return null;
        return npv(rate, cashflows);
      }

      // Cashflow table management
      const cashflowBody = document.getElementById("cashflow-body");
      const periodsInput = document.getElementById("periods-count");
      const applyPeriodsBtn = document.getElementById("apply-periods");
      const addRowBtn = document.getElementById("add-row");
      const loadExampleBtn = document.getElementById("load-example");
      const resetAllBtn = document.getElementById("reset-all");
      const timelineModeSelect = document.getElementById("timeline-mode");
      const discountRateInput = document.getElementById("discount-rate");
      const currencySymbolInput = document.getElementById("currency-symbol");

      const irrResultEl = document.getElementById("irr-result");
      const equityMultipleEl = document.getElementById(
        "equity-multiple-result"
      );
      const npvResultEl = document.getElementById("npv-result");
      const irrMessageEl = document.getElementById("irr-message");

      function createRow(periodIndex, cash = "", desc = "") {
        const tr = document.createElement("tr");
        tr.dataset.index = periodIndex;

        const label =
          timelineModeSelect.value === "months"
            ? `Month ${periodIndex}`
            : `Year ${periodIndex}`;

        tr.innerHTML = `
      <td class="px-3 py-2 border-t text-gray-700 whitespace-nowrap">${label}</td>
      <td class="px-3 py-2 border-t">
        <div class="flex items-center gap-1">
          <span class="text-gray-500 text-sm">${currencySymbolInput.value}</span>
          <input type="number" step="0.01" class="input-number w-full border border-gray-300 rounded-md px-2 py-1 text-sm cashflow-input" value="${cash}" />
        </div>
      </td>
      <td class="px-3 py-2 border-t">
        <input type="text" class="w-full border border-gray-300 rounded-md px-2 py-1 text-sm" placeholder="e.g. Purchase, NOI, Capex, Sale" value="${desc}" />
      </td>
      <td class="px-3 py-2 border-t text-center">
        <button type="button" class="text-red-500 hover:text-red-700 text-sm delete-row" title="Delete row">&times;</button>
      </td>
    `;
        return tr;
      }

      function rebuildTable(periods) {
        cashflowBody.innerHTML = "";
        for (let i = 0; i <= periods; i++) {
          cashflowBody.appendChild(
            createRow(i, i === 0 ? "" : "", i === 0 ? "Initial investment" : "")
          );
        }
        attachRowListeners();
        recalc();
      }

      function attachRowListeners() {
        cashflowBody.querySelectorAll("input").forEach((input) => {
          input.addEventListener("input", () => {
            recalc();
          });
        });
        cashflowBody.querySelectorAll(".delete-row").forEach((btn) => {
          btn.addEventListener("click", (e) => {
            const row = e.target.closest("tr");
            row.remove();
            recalc();
          });
        });
      }

      function getCashflows() {
        const rows = Array.from(cashflowBody.querySelectorAll("tr"));
        const cashflows = [];
        rows.forEach((row, idx) => {
          const input = row.querySelector(".cashflow-input");
          const val = parseFloat(input.value);
          if (isNaN(val)) {
            cashflows.push(0);
            row.classList.add("cashflow-row-invalid");
          } else {
            cashflows.push(val);
            row.classList.remove("cashflow-row-invalid");
          }
          row.dataset.index = idx;
          const labelCell = row.querySelector("td:first-child");
          if (labelCell) {
            const label =
              timelineModeSelect.value === "months"
                ? `Month ${idx}`
                : `Year ${idx}`;
            labelCell.textContent = label;
          }
        });
        return cashflows;
      }

      function recalc() {
        const cashflows = getCashflows();
        const symbol = currencySymbolInput.value || "$";

        if (cashflows.length === 0) {
          irrResultEl.textContent = "–";
          equityMultipleEl.textContent = "–";
          npvResultEl.textContent = "–";
          irrMessageEl.textContent = "";
          return;
        }

        const irr = computeIRR(cashflows);
        const eqMult = computeEquityMultiple(cashflows);
        const discountRate = parseFloat(discountRateInput.value) / 100;
        const npvVal = isNaN(discountRate)
          ? null
          : computeNPVAtRate(discountRate, cashflows);

        irrResultEl.textContent =
          irr === null ? "No valid IRR" : formatPercent(irr * 100);
        equityMultipleEl.textContent =
          eqMult === null ? "–" : eqMult.toFixed(2) + "×";
        npvResultEl.textContent =
          npvVal === null ? "–" : formatCurrency(npvVal, symbol);

        if (irr === null) {
          irrMessageEl.textContent =
            "IRR could not be found. Ensure you have at least one negative and one positive cash flow, and that cash flows are reasonable.";
        } else {
          irrMessageEl.textContent = "";
        }
      }

      applyPeriodsBtn.addEventListener("click", () => {
        let periods = parseInt(periodsInput.value, 10);
        if (isNaN(periods) || periods < 1) periods = 1;
        if (periods > 600) periods = 600;
        periodsInput.value = periods;
        rebuildTable(periods);
      });

      addRowBtn.addEventListener("click", () => {
        const currentRows = cashflowBody.querySelectorAll("tr").length;
        const newRow = createRow(currentRows);
        cashflowBody.appendChild(newRow);
        attachRowListeners();
        recalc();
      });

      loadExampleBtn.addEventListener("click", () => {
        timelineModeSelect.value = "years";
        periodsInput.value = 5;
        rebuildTable(5);
        const rows = cashflowBody.querySelectorAll("tr");
        const exampleValues = [-1000000, 80000, 90000, 100000, 110000, 1520000]; // Year 5 includes NOI + sale
        const exampleDescs = [
          "Purchase & closing costs",
          "Year 1 net cash flow",
          "Year 2 net cash flow",
          "Year 3 net cash flow",
          "Year 4 net cash flow",
          "Year 5 NOI + sale proceeds",
        ];
        rows.forEach((row, idx) => {
          const cashInput = row.querySelector(".cashflow-input");
          const descInput = row.querySelector('input[type="text"]');
          if (cashInput) cashInput.value = exampleValues[idx] ?? "";
          if (descInput) descInput.value = exampleDescs[idx] ?? "";
        });
        recalc();
      });

      resetAllBtn.addEventListener("click", () => {
        periodsInput.value = 10;
        discountRateInput.value = "";
        timelineModeSelect.value = "years";
        rebuildTable(10);
      });

      timelineModeSelect.addEventListener("change", () => {
        recalc();
      });

      currencySymbolInput.addEventListener("input", () => {
        const symbol = currencySymbolInput.value || "$";
        cashflowBody.querySelectorAll("tr").forEach((row) => {
          const span = row.querySelector("td:nth-child(2) span");
          if (span) span.textContent = symbol;
        });
        recalc();
      });

      discountRateInput.addEventListener("input", () => {
        recalc();
      });

      // Initialize default table
      rebuildTable(parseInt(periodsInput.value, 10) || 10);
    </script>

