    <script>
      // Utility: format currency and percent
      function fmtCurrency(value) {
        if (!isFinite(value)) return "–";
        return (
          "$" + value.toLocaleString(undefined, { maximumFractionDigits: 0 })
        );
      }
      function fmtCurrency2(value) {
        if (!isFinite(value)) return "–";
        return (
          "$" +
          value.toLocaleString(undefined, {
            minimumFractionDigits: 2,
            maximumFractionDigits: 2,
          })
        );
      }
      function fmtPercent(value) {
        if (!isFinite(value)) return "–";
        return value.toFixed(1) + "%";
      }

      // DOM elements
      const maxCltvInput = document.getElementById("maxCltv");
      const maxCltvValue = document.getElementById("maxCltvValue");
      const interestRateInput = document.getElementById("interestRate");
      const interestRateValue = document.getElementById("interestRateValue");

      const tabMaxLoan = document.getElementById("tab-max-loan");
      const tabPayment = document.getElementById("tab-payment");
      const panelMaxLoan = document.getElementById("panel-max-loan");
      const panelPayment = document.getElementById("panel-payment");

      const calculateBtn = document.getElementById("calculateBtn");

      const currentLtvOut = document.getElementById("currentLtvOut");
      const targetCltvOut = document.getElementById("targetCltvOut");
      const maxTotalDebtOut = document.getElementById("maxTotalDebtOut");
      const maxLoanOut = document.getElementById("maxLoanOut");

      const loanAmountUsedOut = document.getElementById("loanAmountUsedOut");
      const monthlyPaymentOut = document.getElementById("monthlyPaymentOut");
      const totalPaidOut = document.getElementById("totalPaidOut");
      const totalInterestOut = document.getElementById("totalInterestOut");

      const amortizationPreview = document.getElementById(
        "amortizationPreview"
      );
      const amortizationBody = document.getElementById("amortizationBody");
      const desiredLoanWarning = document.getElementById("desiredLoanWarning");

      function updateRangeLabels() {
        maxCltvValue.textContent = maxCltvInput.value + "%";
        interestRateValue.textContent =
          parseFloat(interestRateInput.value).toFixed(1) + "%";
      }

      maxCltvInput.addEventListener("input", updateRangeLabels);
      interestRateInput.addEventListener("input", updateRangeLabels);
      updateRangeLabels();

      // Tab switching
      function activateTab(tab) {
        if (tab === "maxLoan") {
          tabMaxLoan.classList.add(
            "bg-blue-600",
            "text-white",
            "border-blue-600"
          );
          tabMaxLoan.classList.remove(
            "border-gray-300",
            "text-gray-700",
            "hover:bg-gray-50"
          );
          tabPayment.classList.remove(
            "bg-blue-600",
            "text-white",
            "border-blue-600"
          );
          tabPayment.classList.add(
            "border-gray-300",
            "text-gray-700",
            "hover:bg-gray-50"
          );
          panelMaxLoan.classList.remove("hidden");
          panelPayment.classList.add("hidden");
        } else {
          tabPayment.classList.add(
            "bg-blue-600",
            "text-white",
            "border-blue-600"
          );
          tabPayment.classList.remove(
            "border-gray-300",
            "text-gray-700",
            "hover:bg-gray-50"
          );
          tabMaxLoan.classList.remove(
            "bg-blue-600",
            "text-white",
            "border-blue-600"
          );
          tabMaxLoan.classList.add(
            "border-gray-300",
            "text-gray-700",
            "hover:bg-gray-50"
          );
          panelPayment.classList.remove("hidden");
          panelMaxLoan.classList.add("hidden");
        }
      }

      tabMaxLoan.addEventListener("click", () => activateTab("maxLoan"));
      tabPayment.addEventListener("click", () => activateTab("payment"));

      // Core calculation
      function calculate() {
        const homeValue =
          parseFloat(document.getElementById("homeValue").value) || 0;
        const currentMortgage =
          parseFloat(document.getElementById("currentMortgage").value) || 0;
        const cltvMax = (parseFloat(maxCltvInput.value) || 0) / 100;
        const annualRate = (parseFloat(interestRateInput.value) || 0) / 100;

        if (homeValue <= 0) {
          currentLtvOut.textContent = "–";
          targetCltvOut.textContent = "–";
          maxTotalDebtOut.textContent = "–";
          maxLoanOut.textContent = "–";
          loanAmountUsedOut.textContent = "–";
          monthlyPaymentOut.textContent = "–";
          totalPaidOut.textContent = "–";
          totalInterestOut.textContent = "–";
          amortizationPreview.classList.add("hidden");
          return;
        }

        const currentLtv = (currentMortgage / homeValue) * 100;
        const maxTotalDebt = homeValue * cltvMax;
        const maxNewLoan = Math.max(0, maxTotalDebt - currentMortgage);

        currentLtvOut.textContent = fmtPercent(currentLtv);
        targetCltvOut.textContent = fmtPercent(cltvMax * 100);
        maxTotalDebtOut.textContent = fmtCurrency(maxTotalDebt);
        maxLoanOut.textContent = fmtCurrency(maxNewLoan);

        // Determine which tab is active
        const isMaxLoanTabActive = !panelMaxLoan.classList.contains("hidden");

        let loanAmount, termYears, repayType;
        if (isMaxLoanTabActive) {
          termYears =
            parseInt(document.getElementById("termYearsAmount").value, 10) || 0;
          const repayRadios = document.querySelectorAll(
            'input[name="repayTypeAmount"]'
          );
          repayType =
            Array.from(repayRadios).find((r) => r.checked)?.value ||
            "amortizing";
          loanAmount = maxNewLoan;
        } else {
          termYears =
            parseInt(document.getElementById("termYearsPayment").value, 10) ||
            0;
          const repayRadios = document.querySelectorAll(
            'input[name="repayTypePayment"]'
          );
          repayType =
            Array.from(repayRadios).find((r) => r.checked)?.value ||
            "amortizing";
          loanAmount =
            parseFloat(document.getElementById("desiredLoanAmount").value) || 0;

          // Warn if desired amount exceeds max
          if (loanAmount > maxNewLoan + 1) {
            desiredLoanWarning.classList.remove("hidden");
          } else {
            desiredLoanWarning.classList.add("hidden");
          }
        }

        loanAmountUsedOut.textContent = fmtCurrency(loanAmount);

        if (loanAmount <= 0 || annualRate < 0 || termYears <= 0) {
          monthlyPaymentOut.textContent = "–";
          totalPaidOut.textContent = "–";
          totalInterestOut.textContent = "–";
          amortizationPreview.classList.add("hidden");
          return;
        }

        const monthlyRate = annualRate / 12;
        const n = termYears * 12;
        let monthlyPayment, totalPaid, totalInterest;

        if (repayType === "interestOnly") {
          monthlyPayment = loanAmount * monthlyRate;
          totalPaid = monthlyPayment * n;
          totalInterest = totalPaid; // principal not repaid in this simple model
          amortizationPreview.classList.add("hidden");
        } else {
          if (monthlyRate === 0) {
            monthlyPayment = loanAmount / n;
          } else {
            const pow = Math.pow(1 + monthlyRate, n);
            monthlyPayment = (loanAmount * (monthlyRate * pow)) / (pow - 1);
          }
          totalPaid = monthlyPayment * n;
          totalInterest = totalPaid - loanAmount;
          buildAmortizationTable(loanAmount, monthlyRate, n, monthlyPayment);
        }

        monthlyPaymentOut.textContent = fmtCurrency2(monthlyPayment);
        totalPaidOut.textContent = fmtCurrency2(totalPaid);
        totalInterestOut.textContent = fmtCurrency2(totalInterest);
      }

      function buildAmortizationTable(principal, monthlyRate, n, payment) {
        amortizationBody.innerHTML = "";
        let balance = principal;
        const monthsToShow = Math.min(12, n);

        for (let i = 1; i <= monthsToShow; i++) {
          const interest = balance * monthlyRate;
          let principalPaid = payment - interest;
          if (i === n || principalPaid > balance) {
            principalPaid = balance;
          }
          balance = balance - principalPaid;

          const tr = document.createElement("tr");
          tr.className = i % 2 === 0 ? "bg-white" : "bg-gray-50";
          tr.innerHTML = `
        <td class="px-2 py-1 text-gray-700">${i}</td>
        <td class="px-2 py-1 text-right text-gray-700">${fmtCurrency2(
          payment
        )}</td>
        <td class="px-2 py-1 text-right text-gray-700">${fmtCurrency2(
          principalPaid
        )}</td>
        <td class="px-2 py-1 text-right text-gray-700">${fmtCurrency2(
          interest
        )}</td>
        <td class="px-2 py-1 text-right text-gray-700">${fmtCurrency2(
          Math.max(balance, 0)
        )}</td>
      `;
          amortizationBody.appendChild(tr);

          if (balance <= 0) break;
        }
        amortizationPreview.classList.remove("hidden");
      }

      calculateBtn.addEventListener("click", calculate);

      // Auto-calc on load
      window.addEventListener("load", calculate);
    </script>

