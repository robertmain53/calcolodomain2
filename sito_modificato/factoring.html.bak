    <script>
      // --- Utility: GCD ---
      function gcd(a, b) {
        a = Math.abs(a);
        b = Math.abs(b);
        while (b !== 0) {
          const t = b;
          b = a % b;
          a = t;
        }
        return a || 1;
      }

      // --- Integer factoring ---
      function factorInteger(n) {
        const steps = [];
        if (n === 0) {
          steps.push(
            "0 has infinitely many divisors; we usually do not define a prime factorization for 0."
          );
          return { result: "0", steps };
        }
        const sign = n < 0 ? -1 : 1;
        let m = Math.abs(n);
        const factors = {};
        steps.push("Start with |n| = " + m + ".");

        // factor 2
        let count = 0;
        while (m % 2 === 0) {
          m / 2;
          count++;
        }
        if (count > 0) {
          factors[2] = count;
          steps.push("Divide out factor 2: exponent " + count + ".");
        }

        let d = 3;
        while (d * d <= m) {
          let c = 0;
          while (m % d === 0) {
            m / d;
            c++;
          }
          if (c > 0) {
            factors[d] = c;
            steps.push("Divide out factor " + d + ": exponent " + c + ".");
          }
          d += 2;
        }
        if (m > 1) {
          factors[m] = (factors[m] || 0) + 1;
          steps.push("Remaining factor " + m + " is prime.");
        }

        const parts = [];
        if (sign === -1) parts.push("-1");
        Object.keys(factors)
          .sort((a, b) => a - b)
          .forEach((p) => {
            const e = factors[p];
            parts.push(e === 1 ? p : p + "^" + e);
          });
        const result = parts.join(" · ");
        steps.push("Combine all prime factors: " + result + ".");
        return { result, steps };
      }

      // --- Polynomial parsing (single variable x, integer coefficients) ---
      function parsePolynomial(input) {
        let s = input.replace(/\s+/g, "");
        if (!s) throw new Error("Please enter a polynomial.");
        // normalize unary +/-
        s = s.replace(/^\+/, "");
        s = s.replace(/-/g, "+-");
        if (s[0] === "+") s = s.slice(1);
        const rawTerms = s.split("+").filter((t) => t.length);
        const terms = {};
        for (let term of rawTerms) {
          let coef = 0;
          let exp = 0;
          const xIndex = term.indexOf("x");
          if (xIndex === -1) {
            // constant term
            coef = parseInt(term, 10);
            if (isNaN(coef))
              throw new Error("Could not parse constant term: " + term);
            exp = 0;
          } else {
            // something with x
            const coefPart = term.slice(0, xIndex);
            if (coefPart === "" || coefPart === "+") coef = 1;
            else if (coefPart === "-") coef = -1;
            else {
              coef = parseInt(coefPart, 10);
              if (isNaN(coef))
                throw new Error("Could not parse coefficient in term: " + term);
            }
            if (term.slice(xIndex + 1, xIndex + 2) === "^") {
              const expPart = term.slice(xIndex + 2);
              exp = parseInt(expPart, 10);
              if (isNaN(exp))
                throw new Error("Could not parse exponent in term: " + term);
            } else {
              exp = 1;
            }
          }
          terms[exp] = (terms[exp] || 0) + coef;
        }
        // remove zero coefficients
        Object.keys(terms).forEach((k) => {
          if (terms[k] === 0) delete terms[k];
        });
        if (Object.keys(terms).length === 0) {
          return { terms: { 0: 0 }, degree: 0 };
        }
        const degrees = Object.keys(terms).map((e) => parseInt(e, 10));
        const degree = Math.max(...degrees);
        return { terms, degree };
      }

      function polyToString(poly) {
        const { terms } = poly;
        const exps = Object.keys(terms)
          .map((e) => parseInt(e, 10))
          .sort((a, b) => b - a);
        if (exps.length === 0) return "0";
        let out = "";
        exps.forEach((e, idx) => {
          const c = terms[e];
          if (c === 0) return;
          const sign = c < 0 ? "-" : "+";
          const absC = Math.abs(c);
          if (idx === 0) {
            // first term
            if (sign === "-") out += "-";
          } else {
            out += " " + sign + " ";
          }
          if (e === 0) {
            out += absC;
          } else if (e === 1) {
            if (absC !== 1) out += absC;
            out += "x";
          } else {
            if (absC !== 1) out += absC;
            out += "x^" + e;
          }
        });
        return out || "0";
      }

      function extractGCF(poly, steps) {
        const { terms } = poly;
        const coeffs = Object.values(terms);
        let g = 0;
        coeffs.forEach((c) => {
          g = gcd(g, c);
        });
        if (g === 0) return { gcf: 0, reduced: poly };
        if (Math.abs(g) === 1) return { gcf: 1, reduced: poly };
        const reducedTerms = {};
        Object.keys(terms).forEach((e) => {
          reducedTerms[e] = terms[e] / g;
        });
        steps.push("Greatest common factor of coefficients is " + g + ".");
        return {
          gcf: g,
          reduced: { terms: reducedTerms, degree: poly.degree },
        };
      }

      function isDifferenceOfSquares(poly) {
        const { terms } = poly;
        const exps = Object.keys(terms)
          .map((e) => parseInt(e, 10))
          .sort((a, b) => b - a);
        if (exps.length !== 2) return null;
        const [e1, e2] = exps;
        const c1 = terms[e1],
          c2 = terms[e2];
        if (e1 !== 2 || e2 !== 0) return null;
        if (c1 <= 0 || c2 >= 0) return null;
        const a2 = c1,
          b2 = -c2;
        const a = Math.round(Math.sqrt(a2));
        const b = Math.round(Math.sqrt(b2));
        if (a * a !== a2 || b * b !== b2) return null;
        return { a, b };
      }

      function factorQuadratic(a, b, c, steps) {
        steps.push("Treat as quadratic: " + a + "x² + " + b + "x + " + c + ".");
        const A = a * c;
        steps.push("Compute A = a·c = " + A + ".");
        const pairs = [];
        for (let m = -Math.abs(A); m <= Math.abs(A); m++) {
          if (m === 0) continue;
          if (A % m === 0) {
            const n = A / m;
            if (m + n === b) {
              pairs.push([m, n]);
              break;
            }
          }
        }
        if (pairs.length === 0) {
          steps.push(
            "No integer pair (m, n) with m·n = " +
              A +
              " and m + n = " +
              b +
              " was found."
          );
          return null;
        }
        const [m, n] = pairs[0];
        steps.push(
          "Found m = " +
            m +
            ", n = " +
            n +
            " such that m·n = " +
            A +
            " and m + n = " +
            b +
            "."
        );
        // factor by grouping: ax^2 + mx + nx + c
        // group (ax^2 + mx) + (nx + c)
        const g1 = gcd(a, m);
        const g2 = gcd(n, c);
        const factor1 = { a: g1, b: a / g1, c: m / g1 }; // g1 * (b x + c)
        const factor2 = { a: g2, b: n / g2, c: c / g2 }; // g2 * (b x + c)
        // common binomial should match
        if (factor1.b !== factor2.b || factor1.c !== factor2.c) {
          // fallback: just present as (ax + m)(x + n/a) if possible
          steps.push(
            "Grouping did not produce a clean common binomial; quadratic may still be factorable but not by simple integer grouping."
          );
          return null;
        }
        const binom = { b: factor1.b, c: factor1.c };
        const outer1 = factor1.a;
        const outer2 = factor2.a;
        steps.push(
          "Factor by grouping to get (" +
            outer1 +
            "x + " +
            outer2 +
            ")(" +
            binom.b +
            "x + " +
            binom.c +
            ")."
        );
        return {
          factors: [
            { a: outer1, b: outer2 },
            { a: binom.b, b: binom.c },
          ],
        };
      }

      function factorPolynomial(poly) {
        const steps = [];
        const originalStr = polyToString(poly);
        steps.push("Start with polynomial: " + originalStr + ".");

        // GCF
        const { gcf, reduced } = extractGCF(poly, steps);
        let outerFactor = gcf;
        let workPoly = reduced;

        // difference of squares
        const dos = isDifferenceOfSquares(workPoly);
        if (dos) {
          const { a, b } = dos;
          steps.push(
            "Recognize a difference of squares: " +
              a +
              "x² - " +
              b +
              "² = (" +
              a +
              "x - " +
              b +
              ")(" +
              a +
              "x + " +
              b +
              ")."
          );
          let result = "";
          if (outerFactor && Math.abs(outerFactor) !== 1) {
            result += outerFactor + "·";
          }
          result += "(" + a + "x - " + b + ")(" + a + "x + " + b + ")";
          return { result, steps };
        }

        // degree-based handling
        if (workPoly.degree === 0) {
          const c = workPoly.terms[0] || 0;
          if (outerFactor && Math.abs(outerFactor) !== 1) {
            const val = c * outerFactor;
            steps.push(
              "Constant polynomial; nothing to factor beyond the GCF."
            );
            return { result: String(val), steps };
          }
          steps.push("Constant polynomial; nothing to factor.");
          return { result: String(c), steps };
        }

        if (workPoly.degree === 1) {
          steps.push("Linear polynomial; already in simplest factored form.");
          let result = "";
          if (outerFactor && Math.abs(outerFactor) !== 1) {
            result += outerFactor + "·";
          }
          result += "(" + polyToString(workPoly) + ")";
          return { result, steps };
        }

        if (workPoly.degree === 2) {
          const a = workPoly.terms[2] || 0;
          const b = workPoly.terms[1] || 0;
          const c = workPoly.terms[0] || 0;
          const quadFact = factorQuadratic(a, b, c, steps);
          if (quadFact) {
            const [f1, f2] = quadFact.factors;
            let result = "";
            if (outerFactor && Math.abs(outerFactor) !== 1) {
              result += outerFactor + "·";
            }
            result +=
              "(" +
              (f1.a === 1 ? "" : f1.a) +
              "x" +
              (f1.b >= 0 ? "+" + f1.b : f1.b) +
              ")";
            result +=
              "(" +
              (f2.a === 1 ? "" : f2.a) +
              "x" +
              (f2.b >= 0 ? "+" + f2.b : f2.b) +
              ")";
            return { result, steps };
          } else {
            steps.push("Quadratic is irreducible over the integers.");
            let result = "";
            if (outerFactor && Math.abs(outerFactor) !== 1) {
              result += outerFactor + "·";
            }
            result += "(" + polyToString(workPoly) + ")";
            return { result, steps };
          }
        }

        // Quadratic in disguise: even degrees only, like x^4 - 5x^2 + 4
        const exps = Object.keys(workPoly.terms).map((e) => parseInt(e, 10));
        const allEven = exps.every((e) => e % 2 === 0);
        if (allEven && workPoly.degree >= 4) {
          steps.push("All exponents are even; treat as a quadratic in u = x².");
          // build quadratic in u
          const a = workPoly.terms[workPoly.degree] || 0;
          const b = workPoly.terms[workPoly.degree - 2] || 0;
          const c = workPoly.terms[workPoly.degree - 4] || 0;
          const quadFact = factorQuadratic(a, b, c, steps);
          if (quadFact) {
            const [f1, f2] = quadFact.factors;
            // replace u with x^(degree/2)
            const k = workPoly.degree / 2;
            function binomToStr(f) {
              const aU = f.a;
              const bU = f.b;
              let s = "";
              if (aU !== 0) {
                if (aU === 1) s += "x^" + k;
                else if (aU === -1) s += "-x^" + k;
                else s += aU + "x^" + k;
              }
              if (bU !== 0) {
                if (bU > 0 && s) s += "+" + bU;
                else s += bU;
              }
              if (!s) s = "0";
              return "(" + s + ")";
            }
            let result = "";
            if (outerFactor && Math.abs(outerFactor) !== 1) {
              result += outerFactor + "·";
            }
            result += binomToStr(f1) + binomToStr(f2);
            steps.push(
              "Substitute back u = x² (or higher power) to get factors in x."
            );
            return { result, steps };
          }
        }

        // Fallback
        steps.push(
          "No additional integer factorization found. Polynomial appears irreducible over the integers with the methods used."
        );
        let result = "";
        if (outerFactor && Math.abs(outerFactor) !== 1) {
          result += outerFactor + "·";
        }
        result += "(" + polyToString(workPoly) + ")";
        return { result, steps };
      }

      // --- UI wiring ---
      document.addEventListener("DOMContentLoaded", function () {
        const modeEl = document.getElementById("mode");
        const polyGroup = document.getElementById("poly-input-group");
        const intGroup = document.getElementById("int-input-group");
        const polyInput = document.getElementById("poly-input");
        const intInput = document.getElementById("int-input");
        const factorBtn = document.getElementById("factor-btn");
        const clearBtn = document.getElementById("clear-btn");
        const statusMsg = document.getElementById("status-msg");
        const resultsContainer = document.getElementById("results-container");
        const resultMain = document.getElementById("result-main");
        const resultExtra = document.getElementById("result-extra");
        const stepsContainer = document.getElementById("steps-container");
        const stepsList = document.getElementById("steps-list");
        const toggleSteps = document.getElementById("toggle-steps");
        const errorContainer = document.getElementById("error-container");
        const errorMsg = document.getElementById("error-msg");

        modeEl.addEventListener("change", () => {
          if (modeEl.value === "poly") {
            polyGroup.classList.remove("hidden");
            intGroup.classList.add("hidden");
          } else {
            polyGroup.classList.add("hidden");
            intGroup.classList.remove("hidden");
          }
          resultsContainer.classList.add("hidden");
          errorContainer.classList.add("hidden");
          statusMsg.textContent = "";
        });

        function showError(msg) {
          errorMsg.textContent = msg;
          errorContainer.classList.remove("hidden");
          resultsContainer.classList.add("hidden");
          statusMsg.textContent = "";
        }

        function clearAll() {
          polyInput.value = "";
          intInput.value = "";
          resultsContainer.classList.add("hidden");
          errorContainer.classList.add("hidden");
          statusMsg.textContent = "";
        }

        clearBtn.addEventListener("click", clearAll);

        factorBtn.addEventListener("click", () => {
          errorContainer.classList.add("hidden");
          statusMsg.textContent = "Factoring...";
          try {
            if (modeEl.value === "int") {
              const val = intInput.value.trim();
              if (!val) throw new Error("Please enter an integer.");
              const n = parseInt(val, 10);
              if (isNaN(n))
                throw new Error("That does not look like a valid integer.");
              const { result, steps } = factorInteger(n);
              resultMain.textContent = result;
              resultExtra.textContent = "Prime factorization of " + n + ".";
              stepsList.innerHTML = "";
              steps.forEach((s) => {
                const li = document.createElement("li");
                li.textContent = s;
                stepsList.appendChild(li);
              });
              resultsContainer.classList.remove("hidden");
            } else {
              const expr = polyInput.value.trim();
              if (!expr) throw new Error("Please enter a polynomial in x.");
              const poly = parsePolynomial(expr);
              const { result, steps } = factorPolynomial(poly);
              resultMain.textContent = result;
              resultExtra.textContent =
                "Factored over the integers (if possible).";
              stepsList.innerHTML = "";
              steps.forEach((s) => {
                const li = document.createElement("li");
                li.textContent = s;
                stepsList.appendChild(li);
              });
              resultsContainer.classList.remove("hidden");
            }
            statusMsg.textContent = "Done.";
          } catch (e) {
            showError(e.message || "An error occurred while factoring.");
          }
        });

        // Enter key support
        polyInput.addEventListener("keydown", (e) => {
          if (e.key === "Enter") {
            e.preventDefault();
            factorBtn.click();
          }
        });
        intInput.addEventListener("keydown", (e) => {
          if (e.key === "Enter") {
            e.preventDefault();
            factorBtn.click();
          }
        });

        // Toggle steps
        toggleSteps.addEventListener("click", () => {
          if (stepsList.classList.contains("hidden")) {
            stepsList.classList.remove("hidden");
            toggleSteps.textContent = "Hide steps";
          } else {
            stepsList.classList.add("hidden");
            toggleSteps.textContent = "Show steps";
          }
        });

        document.querySelectorAll("#faq .faq-question").forEach((btn) => {
          btn.addEventListener("click", () => {
            const answer = btn.parentElement.querySelector(".faq-answer");
            const icon = btn.querySelector("span:last-child");
            const isHidden = answer.classList.contains("hidden");
            document
              .querySelectorAll("#faq .faq-answer")
              .forEach((a) => a.classList.add("hidden"));
            document
              .querySelectorAll("#faq .faq-question span:last-child")
              .forEach((i) => (i.textContent = "+"));
            if (isHidden) {
              answer.classList.remove("hidden");
              icon.textContent = "−";
            }
          });
        });
      });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
    <script>
  (function () {
    var loadMathJax = function () {
