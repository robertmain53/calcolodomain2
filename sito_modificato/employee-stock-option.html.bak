    <script>
      // Utility: standard normal CDF using Abramowitz-Stegun approximation
      function normCdf(x) {
        var a1 = 0.254829592,
          a2 = -0.284496736,
          a3 = 1.421413741,
          a4 = -1.453152027,
          a5 = 1.061405429,
          p = 0.3275911;
        var sign = x < 0 ? -1 : 1;
        x = Math.abs(x) / Math.sqrt(2.0);
        var t = 1.0 / (1.0 + p * x);
        var y =
          1.0 -
          ((((a5 * t + a4) * t + a3) * t + a2) * t + a1) * t * Math.exp(-x * x);
        return 0.5 * (1.0 + sign * y);
      }

      function formatCurrency(value) {
        if (!isFinite(value)) return "–";
        var abs = Math.abs(value);
        var decimals = abs >= 1000 ? 0 : 2;
        return (value < 0 ? "-" : "") + "$" + Math.abs(value).toFixed(decimals);
      }

      function formatNumber(value, decimals) {
        if (!isFinite(value)) return "–";
        return value.toFixed(decimals);
      }

      function calculateESO() {
        var errorEl = document.getElementById("calcError");
        errorEl.classList.add("hidden");
        errorEl.textContent = "";

        var numOptions = parseFloat(
          document.getElementById("numOptions").value
        );
        var S = parseFloat(document.getElementById("currentPrice").value);
        var K = parseFloat(document.getElementById("strikePrice").value);
        var T = parseFloat(document.getElementById("timeToExpiry").value);
        var sigmaPct = parseFloat(document.getElementById("volatility").value);
        var rPct = parseFloat(document.getElementById("riskFreeRate").value);
        var qPct = parseFloat(document.getElementById("dividendYield").value);
        var taxRatePct = parseFloat(document.getElementById("taxRate").value);
        var downScenarioPct = parseFloat(
          document.getElementById("downScenario").value
        );
        var upScenarioPct = parseFloat(
          document.getElementById("upScenario").value
        );

        if (
          !isFinite(numOptions) ||
          numOptions <= 0 ||
          !isFinite(S) ||
          S <= 0 ||
          !isFinite(K) ||
          K < 0 ||
          !isFinite(T) ||
          T <= 0 ||
          !isFinite(sigmaPct) ||
          sigmaPct <= 0
        ) {
          errorEl.textContent =
            "Please enter valid positive values for options, prices, time, and volatility.";
          errorEl.classList.remove("hidden");
          return;
        }

        var sigma = sigmaPct / 100.0;
        var r = isFinite(rPct) ? rPct / 100.0 : 0;
        var q = isFinite(qPct) ? qPct / 100.0 : 0;
        var taxRate =
          isFinite(taxRatePct) && taxRatePct >= 0 && taxRatePct < 100
            ? taxRatePct / 100.0
            : 0;

        // Black–Scholes with continuous dividend yield
        var d1 =
          (Math.log(S / K) + (r - q + 0.5 * sigma * sigma) * T) /
          (sigma * Math.sqrt(T));
        var d2 = d1 - sigma * Math.sqrt(T);
        var Nd1 = normCdf(d1);
        var Nd2 = normCdf(d2);

        var callValue = S * Math.exp(-q * T) * Nd1 - K * Math.exp(-r * T) * Nd2;
        if (!isFinite(callValue) || callValue < 0) callValue = 0;

        var intrinsicPer = Math.max(S - K, 0);
        var intrinsicTotal = intrinsicPer * numOptions;

        document.getElementById("bsPerOption").textContent =
          formatCurrency(callValue);
        document.getElementById("bsTotalValue").textContent = formatCurrency(
          callValue * numOptions
        );
        document.getElementById("intrinsicPerOption").textContent =
          formatCurrency(intrinsicPer);
        document.getElementById("intrinsicTotal").textContent =
          formatCurrency(intrinsicTotal);

        var exerciseCost = K * numOptions;
        document.getElementById("exerciseCost").textContent =
          formatCurrency(exerciseCost);

        // Break-even price approximation after tax
        var breakEvenPrice = null;
        if (taxRate > 0 && taxRate < 1) {
          breakEvenPrice = K * (1 + taxRate / (1 - taxRate));
        } else {
          breakEvenPrice = K;
        }
        document.getElementById("breakEvenPrice").textContent = isFinite(
          breakEvenPrice
        )
          ? formatCurrency(breakEvenPrice)
          : "–";

        // Scenario table
        var scenarios = [
          { name: "Downside", pct: downScenarioPct },
          { name: "Current price", pct: 0 },
          { name: "Upside", pct: upScenarioPct },
        ];

        var tbody = document.getElementById("scenarioTableBody");
        tbody.innerHTML = "";

        scenarios.forEach(function (sc) {
          if (!isFinite(sc.pct)) return;
          var factor = 1 + sc.pct / 100.0;
          var S_scn = S * factor;
          var gainPer = Math.max(S_scn - K, 0);
          var preTaxGain = gainPer * numOptions;
          var estTax = preTaxGain * taxRate;
          var afterTax = preTaxGain - estTax;

          var tr = document.createElement("tr");

          var tdName = document.createElement("td");
          tdName.className = "px-2 py-2 border-b";
          tdName.textContent = sc.name + " (" + formatNumber(sc.pct, 1) + "%)";
          tr.appendChild(tdName);

          var tdPrice = document.createElement("td");
          tdPrice.className = "px-2 py-2 border-b text-right";
          tdPrice.textContent = formatCurrency(S_scn);
          tr.appendChild(tdPrice);

          var tdPreTax = document.createElement("td");
          tdPreTax.className = "px-2 py-2 border-b text-right";
          tdPreTax.textContent = formatCurrency(preTaxGain);
          tr.appendChild(tdPreTax);

          var tdTax = document.createElement("td");
          tdTax.className = "px-2 py-2 border-b text-right";
          tdTax.textContent = formatCurrency(estTax);
          tr.appendChild(tdTax);

          var tdAfter = document.createElement("td");
          tdAfter.className = "px-2 py-2 border-b text-right";
          tdAfter.textContent = formatCurrency(afterTax);
          tr.appendChild(tdAfter);

          tbody.appendChild(tr);
        });

        // Leverage on upside scenario
        var leverageEl = document.getElementById("leverageUpside");
        if (isFinite(upScenarioPct) && upScenarioPct !== 0 && callValue > 0) {
          var S_up = S * (1 + upScenarioPct / 100.0);
          var intrinsicUp = Math.max(S_up - K, 0) * numOptions;
          var intrinsicNow = intrinsicTotal;
          var optionReturnPct =
            intrinsicNow > 0
              ? ((intrinsicUp - intrinsicNow) / intrinsicNow) * 100
              : intrinsicUp > 0
              ? (intrinsicUp / (callValue * numOptions)) * 100
              : 0;
          var leverage = optionReturnPct / upScenarioPct;
          leverageEl.textContent = isFinite(leverage)
            ? leverage.toFixed(1) + "×"
            : "–";
        } else {
          leverageEl.textContent = "–";
        }
      }

      document
        .getElementById("calculateBtn")
        .addEventListener("click", calculateESO);
      document
        .getElementById("resetBtn")
        .addEventListener("click", function () {
          document.getElementById("numOptions").value = 1000;
          document.getElementById("currentPrice").value = 25;
          document.getElementById("strikePrice").value = 15;
          document.getElementById("timeToExpiry").value = 4;
          document.getElementById("volatility").value = 40;
          document.getElementById("riskFreeRate").value = 3;
          document.getElementById("dividendYield").value = 0;
          document.getElementById("taxRate").value = 35;
          document.getElementById("downScenario").value = -30;
          document.getElementById("upScenario").value = 50;
          document.getElementById("calcError").classList.add("hidden");
          document.getElementById("scenarioTableBody").innerHTML =
            '<tr><td class="px-2 py-2 border-b text-gray-500" colspan="5">Run the calculation to see scenario results.</td></tr>';
          [
            "bsPerOption",
            "bsTotalValue",
            "intrinsicPerOption",
            "intrinsicTotal",
            "exerciseCost",
            "breakEvenPrice",
            "leverageUpside",
          ].forEach(function (id) {
            document.getElementById(id).textContent = "–";
          });
        });

      // Optional: auto-calc on first load with defaults
      window.addEventListener("DOMContentLoaded", calculateESO);

      // Mobile menu script (simple toggle)
      document
        .getElementById("mobile-menu-toggle")
        .addEventListener("click", function () {
          var menu = document.getElementById("mobile-menu");
          var isHidden = menu.classList.contains("hidden");
          menu.classList.toggle("hidden", !isHidden);
          this.setAttribute("aria-expanded", isHidden ? "true" : "false");
        });

      window.MathJax = {
        tex: {
          inlineMath: [
            ["\\(", "\\)"],
            ["$", "$"],
          ],
          displayMath: [
            ["$", "$"],
            ["\\[", "\\]"],
          ],
        },
        svg: { fontCache: "global" },
      };
    </script>
