    <script>
      function degToRad(d) {
        return (d * Math.PI) / 180;
      }

      function setBadge(el, ok, labelOk, labelNg) {
        if (!el) return;
        el.textContent = ok ? labelOk || "OK" : labelNg || "Check";
        el.className =
          "ml-1 px-2 py-0.5 rounded text-xs " +
          (ok ? "result-badge-ok" : "result-badge-ng");
      }

      document.getElementById("calcBtn").addEventListener("click", function () {
        const H = parseFloat(document.getElementById("H").value);
        const tStem = parseFloat(document.getElementById("tStem").value);
        const LHeel = parseFloat(document.getElementById("LHeel").value);
        const LToe = parseFloat(document.getElementById("LToe").value);
        const tBase = parseFloat(document.getElementById("tBase").value);
        const BWidth = parseFloat(document.getElementById("BWidth").value);

        const gammaSoil = parseFloat(
          document.getElementById("gammaSoil").value
        );
        const phi = parseFloat(document.getElementById("phi").value);
        const mu = parseFloat(document.getElementById("mu").value);
        const qAllow = parseFloat(document.getElementById("qAllow").value);
        const gammaConc = parseFloat(
          document.getElementById("gammaConc").value
        );
        const surcharge = parseFloat(
          document.getElementById("surcharge").value
        );

        const FSotReq = parseFloat(document.getElementById("FSotReq").value);
        const FSslReq = parseFloat(document.getElementById("FSslReq").value);

        if (
          [
            H,
            tStem,
            LHeel,
            LToe,
            tBase,
            BWidth,
            gammaSoil,
            phi,
            mu,
            qAllow,
            gammaConc,
          ].some((v) => !isFinite(v) || v <= 0)
        ) {
          alert(
            "Please enter positive numeric values for all required fields."
          );
          return;
        }

        const B = LHeel + tStem + LToe;

        // Rankine Ka
        const Ka = Math.tan(degToRad(45 - phi / 2)) ** 2;

        // Earth pressure components
        const Pa_tri = 0.5 * Ka * gammaSoil * H * H; // triangular
        const Pa_tri_z = H / 3;
        const Pa_rect = Ka * surcharge * H; // rectangular from surcharge
        const Pa_rect_z = H / 2;

        const Pa = Pa_tri + Pa_rect;
        const Pa_z = (Pa_tri * Pa_tri_z + Pa_rect * Pa_rect_z) / (Pa || 1); // avoid /0

        // Self-weight of concrete (stem + base) per unit length
        const Wstem = gammaConc * tStem * H * BWidth;
        const Wbase = gammaConc * B * tBase * BWidth;

        // Soil over heel (assuming up to grade)
        const WsoilHeel = gammaSoil * LHeel * H * BWidth;

        const W = Wstem + Wbase + WsoilHeel;

        // Locations from toe (x=0 at toe, positive toward heel)
        const xStem = LToe + tStem / 2;
        const xBase = B / 2;
        const xSoilHeel = LToe + tStem + LHeel / 2;

        const Mstabilizing =
          Wstem * xStem + Wbase * xBase + WsoilHeel * xSoilHeel;
        const Moverturn = Pa * Pa_z; // about toe

        const FSsliding = (mu * W) / (Pa || 1);
        const FSot = Mstabilizing / (Moverturn || 1);

        // Bearing
        const qAvg = W / (B * BWidth);
        const e = (Mstabilizing - Moverturn) / (W || 1); // eccentricity from centroid at B/2
        const qMax = qAvg * (1 + (6 * e) / B);
        const qMin = qAvg * (1 - (6 * e) / B);

        // Stem moment at base (cantilever)
        // Integrate triangular + rectangular components:
        // M_tri = Pa_tri * (H/3)
        // M_rect = Pa_rect * (H/2)
        const Mstem = Pa_tri * (H / 3) + Pa_rect * (H / 2);

        // Output
        document.getElementById("KaOut").textContent = Ka.toFixed(3);
        document.getElementById("PaOut").textContent = Pa.toFixed(2);
        document.getElementById("PaLeverArmOut").textContent = Pa_z.toFixed(2);

        document.getElementById("WOut").textContent = W.toFixed(2);
        document.getElementById("RslOut").textContent = (mu * W).toFixed(2);
        document.getElementById("FSslOut").textContent = FSsliding.toFixed(2);
        document.getElementById("FSotOut").textContent = FSot.toFixed(2);

        setBadge(
          document.getElementById("FSslBadge"),
          FSsliding >= FSslReq,
          "OK",
          "Low FS"
        );
        setBadge(
          document.getElementById("FSotBadge"),
          FSot >= FSotReq,
          "OK",
          "Low FS"
        );

        document.getElementById("qMaxOut").textContent = qMax.toFixed(2);
        document.getElementById("qMinOut").textContent = qMin.toFixed(2);
        document.getElementById("eOut").textContent = e.toFixed(3);

        const qOk = qMax <= qAllow && qMin >= 0;
        setBadge(
          document.getElementById("qAllowBadge"),
          qOk,
          "Within q_allow",
          "Exceeds q_allow"
        );
        document.getElementById("qAllowCheck").textContent = qOk
          ? "OK vs allowable"
          : "Check bearing";

        document.getElementById("MstemOut").textContent = Mstem.toFixed(2);
      });
    </script>

